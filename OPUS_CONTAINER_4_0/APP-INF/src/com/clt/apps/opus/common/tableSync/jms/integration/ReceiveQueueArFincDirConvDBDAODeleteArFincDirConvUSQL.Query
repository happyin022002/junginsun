<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueArFincDirConvDBDAODeleteArFincDirConvRSQL">
			<desc><![CDATA[DeleteArFincDirConv]]></desc>
			<sql><![CDATA[
UPDATE AR_FINC_DIR_CONV
   SET delt_flg     = 'Y'
 WHERE slan_cd      = @[slan_cd]
   AND sconti_cd    = @[sconti_cd]
   AND slan_dir_cd  = @[slan_dir_cd]
   AND rlane_dir_cd = @[rlane_dir_cd]
   AND eai_evnt_dt  <= TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="s_slan_cd" type="12" value="" out="N"/>
				<param name="s_sconti_cd" type="12" value="" out="N"/>
				<param name="s_slan_dir_cd" type="12" value="" out="N"/>
				<param name="s_rlane_dir_cd" type="12" value="" out="N"/>
				<param name="s_eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
