<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueApPeriodDBDAOModifyApPeriodUSQL">
			<desc><![CDATA[수정]]></desc>
			<sql><![CDATA[
UPDATE	AP_PERIOD SET           												
		CLZ_STS_CD		= @[clz_sts_cd]
		, SYS_DESC		= HJSEAI_PKG.h_decode(@[sys_desc], 'UTF8' ,'UTF8')
		, EAI_EVNT_DT	= TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss') 		
		, LST_UPD_DT	= TO_DATE(@[lst_upd_dt], 'yyyymmdd hh24:mi:ss')
WHERE	SYS_DIV_CD		= @[sys_div_cd]     	
AND		EFF_YRMON		= @[eff_yrmon]     	 
AND		OFC_CD			= @[ofc_cd]     	 
AND		AR_AP_DIV_CD	= @[ar_ap_div_cd]    
AND 	EAI_EVNT_DT 	<= TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="clz_sts_cd" type="12" value="" out="N"/>
				<param name="sys_desc" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="lst_upd_dt" type="12" value="" out="N"/>
				<param name="sys_div_cd" type="12" value="" out="N"/>
				<param name="eff_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="ar_ap_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
