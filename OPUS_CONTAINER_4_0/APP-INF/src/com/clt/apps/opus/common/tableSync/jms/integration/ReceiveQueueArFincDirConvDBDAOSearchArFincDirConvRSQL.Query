<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueArFincDirConvDBDAOSearchArFincDirConvRSQL">
			<desc><![CDATA[ModifyArFincDirConv]]></desc>
			<sql><![CDATA[
SELECT slan_cd
  FROM AR_FINC_DIR_CONV
 WHERE slan_cd      = @[slan_cd]
   AND sconti_cd    = @[sconti_cd]
   AND slan_dir_cd  = @[slan_dir_cd]
   AND rlane_dir_cd = @[rlane_dir_cd]			]]></sql>
			<params>
				<param name="s_slan_cd" type="12" value="" out="N"/>
				<param name="s_sconti_cd" type="12" value="" out="N"/>
				<param name="s_slan_dir_cd" type="12" value="" out="N"/>
				<param name="s_rlane_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
