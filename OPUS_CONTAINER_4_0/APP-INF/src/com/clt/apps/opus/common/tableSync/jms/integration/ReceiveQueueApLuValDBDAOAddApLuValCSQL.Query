<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueApLuValDBDAOAddApLuValCSQL">
			<desc><![CDATA[ap_lu_val insert]]></desc>
			<sql><![CDATA[
INSERT
INTO ap_lu_val 
( 
    lu_tp_ind_cd
    , lu_cd
    , lu_delt_flg
    , lu_ctnt
    , lu_desc
    , lu_st_act_dt
    , lu_end_act_dt
    , cre_dt
    , cre_usr_id
    , upd_dt
    , upd_usr_id
    , eai_evnt_dt
    , aval_flg 
)
VALUES 
( 
    HJSEAI_PKG.h_decode(@[lu_tp_ind_cd]		, 'UTF8', 'UTF8')
    , HJSEAI_PKG.h_decode(@[lu_cd]		 	, 'UTF8', 'UTF8')
    , @[lu_delt_flg]
    , HJSEAI_PKG.h_decode(@[lu_ctnt]		, 'UTF8', 'UTF8')
    , HJSEAI_PKG.h_decode(@[lu_desc]		, 'UTF8', 'UTF8')
    , to_date(@[lu_st_act_dt]				, 'yyyymmddhh24miss')
    , to_date(@[lu_end_act_dt]				, 'yyyymmddhh24miss')
    , to_date(@[cre_dt]						, 'yyyymmddhh24miss')
    , @[cre_usr_id]
    , to_date(@[upd_dt]						, 'yyyymmddhh24miss')
    , @[upd_usr_id]
    , to_date(@[eai_evnt_dt]				, 'yyyymmddhh24miss')
    , @[aval_flg]
)			]]></sql>
			<params>
				<param name="lu_tp_ind_cd" type="12" value="" out="N"/>
				<param name="lu_cd" type="12" value="" out="N"/>
				<param name="lu_delt_flg" type="12" value="" out="N"/>
				<param name="lu_ctnt" type="12" value="" out="N"/>
				<param name="lu_desc" type="12" value="" out="N"/>
				<param name="lu_st_act_dt" type="12" value="" out="N"/>
				<param name="lu_end_act_dt" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="aval_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
