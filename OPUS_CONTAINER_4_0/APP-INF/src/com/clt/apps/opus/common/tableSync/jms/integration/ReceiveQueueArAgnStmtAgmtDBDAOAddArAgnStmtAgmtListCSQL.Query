<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueArAgnStmtAgmtDBDAOAddArAgnStmtAgmtListCSQL">
			<desc><![CDATA[ar_agn_stmt_agmt 테이블에 저장]]></desc>
			<sql><![CDATA[
INSERT INTO	ar_agn_stmt_agmt (
    asa_rhq_cd,
    asa_no,
    asa_ofc_cd,
    asa_curr_cd,
    asa_prd_fm_dt,
    asa_prd_to_dt,
    asa_clz_dt,
    asa_prpr_usr_id,
    asa_apro_usr_id,
    asa_apro_dt,
    delt_flg,
    eai_evnt_dt,
    cre_dt,
	cre_usr_id,
	upd_usr_id,
	upd_dt
)
VALUES  (
    @[asa_rhq_cd],
    @[asa_no],
    @[asa_ofc_cd],
    @[asa_curr_cd],
    substr(@[asa_prd_fm_dt],1,8),
    substr(@[asa_prd_to_dt],1,8),
    substr(@[asa_clz_dt],1,8),
    @[asa_prpr_usr_id],
    @[asa_apro_usr_id],
    substr(@[asa_apro_dt],1,8),
    @[delt_flg],
    to_date(@[eai_evnt_dt],'yyyymmddhh24miss'),
    to_date(@[eai_evnt_dt],'yyyymmddhh24miss'),
	'MIG_USER',
	'MIG_USER',
	to_date(@[eai_evnt_dt],'yyyymmddhh24miss')
)			]]></sql>
			<params>
				<param name="asa_rhq_cd" type="12" value="" out="Y"/>
				<param name="asa_no" type="12" value="" out="Y"/>
				<param name="asa_ofc_cd" type="12" value="" out="Y"/>
				<param name="asa_curr_cd" type="12" value="" out="Y"/>
				<param name="asa_prd_fm_dt" type="12" value="" out="Y"/>
				<param name="asa_prd_to_dt" type="12" value="" out="Y"/>
				<param name="asa_clz_dt" type="12" value="" out="Y"/>
				<param name="asa_prpr_usr_id" type="12" value="" out="Y"/>
				<param name="asa_apro_usr_id" type="12" value="" out="Y"/>
				<param name="asa_apro_dt" type="12" value="" out="Y"/>
				<param name="delt_flg" type="12" value="" out="Y"/>
				<param name="eai_evnt_dt" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
