<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueApLuValDBDAORemoveApLuValUSQL">
			<desc><![CDATA[ap_lu_val 삭제 플래그 업데이트]]></desc>
			<sql><![CDATA[
UPDATE ap_lu_val 
 SET 												
 		lu_delt_flg  = 'Y'
 		, aval_flg    = 'N' 			  			
 WHERE 	lu_tp_ind_cd  = HJSEAI_PKG.h_decode(@[lu_tp_ind_cd], 'UTF8' ,'UTF8')
 AND	lu_cd    	  = HJSEAI_PKG.h_decode(@[lu_cd], 'UTF8' ,'UTF8')
 AND 	eai_evnt_dt <= to_date(@[eai_evnt_dt],'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="lu_tp_ind_cd" type="12" value="" out="N"/>
				<param name="lu_cd" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
