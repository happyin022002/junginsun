<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueApLuValDBDAOModifyApLuValUSQL">
			<desc><![CDATA[update ap_lu_val]]></desc>
			<sql><![CDATA[
UPDATE ap_lu_val 
 SET           											
	lu_delt_flg   	= @[lu_delt_flg]
	, lu_ctnt     	= HJSEAI_PKG.h_decode(@[lu_ctnt], 'UTF8' ,'UTF8')
	, lu_desc       = HJSEAI_PKG.h_decode(@[lu_desc], 'UTF8' ,'UTF8')						
	, lu_st_act_dt  = to_date(@[lu_st_act_dt],'yyyymmddhh24miss')                 			
	, lu_end_act_dt = to_date(@[lu_end_act_dt],'yyyymmddhh24miss')                 			
	, cre_dt        = to_date(@[cre_dt],'yyyymmddhh24miss')
	, cre_usr_id    = @[cre_usr_id]			
	, upd_dt        = to_date(@[upd_dt],'yyyymmddhh24miss')
	, upd_usr_id    = @[upd_usr_id]			
	, eai_evnt_dt   = to_date(@[eai_evnt_dt],'yyyymmddhh24miss')
	, aval_flg	    = @[aval_flg]			
 WHERE 	lu_tp_ind_cd = HJSEAI_PKG.h_decode(@[lu_tp_ind_cd], 'UTF8' ,'UTF8')					
 AND	lu_cd        = HJSEAI_PKG.h_decode(@[lu_cd], 'UTF8' ,'UTF8')					
 AND 	eai_evnt_dt <= to_date(@[eai_evnt_dt],'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="lu_delt_flg" type="12" value="" out="N"/>
				<param name="lu_ctnt" type="12" value="" out="N"/>
				<param name="lu_desc" type="12" value="" out="N"/>
				<param name="lu_st_act_dt" type="12" value="" out="N"/>
				<param name="lu_end_act_dt" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="aval_flg" type="12" value="" out="N"/>
				<param name="lu_tp_ind_cd" type="12" value="" out="N"/>
				<param name="lu_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
