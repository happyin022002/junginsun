<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueArFincDirConvDBDAOCreateArFincDirConvRSQL">
			<desc><![CDATA[CreateArFincDirConv]]></desc>
			<sql><![CDATA[
INSERT INTO AR_FINC_DIR_CONV (
    slan_cd,                sconti_cd,              slan_dir_cd,
    rlane_dir_cd,           dir_cng_cd,             delt_flg,
    upd_usr_id,             cre_dt,                 upd_dt,
    eai_evnt_dt,            rlane_cd
)
VALUES  (
    @[slan_cd],           @[sconti_cd],         @[slan_dir_cd],
    @[rlane_dir_cd],      @[dir_cng_cd],        @[delt_flg],
    @[upd_usr_id],        TO_DATE(@[cre_dt],'YYYYMMDDHH24MISS'),            TO_DATE(@[upd_dt],'YYYYMMDDHH24MISS'),
    TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS'),           @[rlane_cd]
)			]]></sql>
			<params>
				<param name="s_slan_cd" type="12" value="" out="N"/>
				<param name="s_sconti_cd" type="12" value="" out="N"/>
				<param name="s_slan_dir_cd" type="12" value="" out="N"/>
				<param name="s_rlane_dir_cd" type="12" value="" out="N"/>
				<param name="s_dir_cng_cd" type="12" value="" out="N"/>
				<param name="s_delt_flg" type="12" value="" out="N"/>
				<param name="s_upd_usr_id" type="12" value="" out="N"/>
				<param name="s_cre_dt" type="12" value="" out="N"/>
				<param name="s_upd_dt" type="12" value="" out="N"/>
				<param name="s_eai_evnt_dt" type="12" value="" out="N"/>
				<param name="s_rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
