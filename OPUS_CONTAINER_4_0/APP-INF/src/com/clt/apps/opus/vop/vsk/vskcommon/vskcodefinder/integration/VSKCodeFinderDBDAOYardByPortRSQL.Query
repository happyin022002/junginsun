<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOYardByPortRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 		LOC_CD
       , 	SUBSTR(YD_CD, 6) 	AS YD_KIND
       , 	YD_CD
       , 	YD_NM
FROM		MDM_YARD
WHERE		DELT_FLG      		= 'N'
AND			LOC_CD 				= @[loc_cd]
--- ::2014-07-23::---- AND		(NVL(YD_FCTY_TP_MRN_TML_FLG,' ') = 'Y' OR NVL(YD_FCTY_TP_BRG_RMP_FLG,' ') = 'Y' )
AND		YD_FCTY_TP_MRN_TML_FLG	= 'Y'
ORDER BY 3			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
