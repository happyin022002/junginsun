<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtDBDAOSearchTariffListWithCostCdRSQL">
			<desc><![CDATA[PSO_YD_CHG 테이블에서 PortCd와 CostCd를 가지는 Tariff List를 조회한다.
---------------------------------------------------------------------------------
CHM-201006949-01 박희동 신규]]></desc>
			<sql><![CDATA[
SELECT
--T1.YD_CHG_NO
--,T1.YD_CHG_VER_SEQ
--,T1.LGS_COST_CD
 DISTINCT T1.YD_CD
,(SELECT YD_NM FROM MDM_YARD WHERE YD_CD=T1.YD_CD) YD_NM
--,T1.VNDR_SEQ
--,T1.EFF_DT
--,T1.EXP_DT
--,T1.CURR_CD
--,T1.CPLS_FLG
--,T1.ORG_VNDR_NM
--,T1.RLT_AGMT_NO
--,T1.LST_FLG
--,T1.CRE_USR_ID
--,T1.CRE_DT
--,T1.UPD_USR_ID
--,T1.UPD_DT
FROM   PSO_YD_CHG T1
WHERE  1 = 1
AND    T1.YD_CD LIKE @[port_cd] || '%'
#if (${cost_cd} != '') 
AND    T1.LGS_COST_CD = @[cost_cd]
#end
AND    T1.LST_FLG='Y'			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
