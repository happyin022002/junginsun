<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnStwgCgoApprovalDBDAOScgStwgCgoVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
	  ,SPCL_CGO_APRO_RQST_SEQ
	  ,STWG_CGO_SEQ
	  ,STWG_CD
	  ,STWG_RMK
	  ,RQST_DT
	  ,RQST_GDT
	  ,RQST_USR_ID
	  ,SPCL_CGO_APRO_CD
	  ,APLY_NO
	  ,CRE_USR_ID
	  ,CRE_DT
	  ,UPD_USR_ID
	  ,UPD_DT
FROM SCG_STWG_CGO
WHERE	BKG_NO = @[bkg_no]
#if (${spcl_cgo_apro_rqst_seq} != '') 
AND	SPCL_CGO_APRO_RQST_SEQ = @[spcl_cgo_apro_rqst_seq]
#else
AND	SPCL_CGO_APRO_RQST_SEQ = (	SELECT MAX(SPCL_CGO_APRO_RQST_SEQ) FROM SCG_APRO_RQST WHERE SPCL_CGO_CATE_CD = 'SS' AND BKG_NO = @[bkg_no]	)
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
