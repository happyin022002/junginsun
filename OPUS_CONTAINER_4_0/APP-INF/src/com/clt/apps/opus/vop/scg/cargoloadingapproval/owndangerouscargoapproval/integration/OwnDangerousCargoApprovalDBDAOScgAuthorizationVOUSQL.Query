<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgAuthorizationVOUSQL">
			<desc><![CDATA[SCG_AUTHORIZATION 수정]]></desc>
			<sql><![CDATA[
UPDATE SCG_AUTHORIZATION SET 
	SPCL_CGO_AUTH_CD = @[spcl_cgo_auth_cd]
,	AUTH_OFC_CD = @[auth_ofc_cd]
,	AUTH_USR_ID = @[auth_usr_id]
,	AUTH_DT = NVL(GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),SYSDATE,BKG_COM_USER_LOC_FNC(@[upd_usr_id])), SYSDATE)
,	AUTH_GDT = NVL(GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),SYSDATE,'GMT'), SYSDATE)
,	SPCL_CGO_AUTH_NO = @[spcl_cgo_auth_no]
,	SPCL_CGO_AUTH_RJCT_CD = @[spcl_cgo_auth_rjct_cd]
,	APRO_REF_NO = @[apro_ref_no]
,	SPCL_CGO_AUTH_RMK = @[spcl_cgo_auth_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no]
AND	SPCL_CGO_APRO_RQST_SEQ = @[spcl_cgo_apro_rqst_seq]
AND	VSL_PRE_PST_CD = @[vsl_pre_pst_cd]
AND	VSL_SEQ = @[vsl_seq]
AND	SPCL_CGO_AUTH_SEQ = @[spcl_cgo_auth_seq]			]]></sql>
			<params>
				<param name="spcl_cgo_auth_cd" type="12" value="" out="N"/>
				<param name="auth_ofc_cd" type="12" value="" out="N"/>
				<param name="auth_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="spcl_cgo_auth_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_auth_rjct_cd" type="12" value="" out="N"/>
				<param name="apro_ref_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_auth_rmk" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="2" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="2" value="" out="N"/>
				<param name="spcl_cgo_auth_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
