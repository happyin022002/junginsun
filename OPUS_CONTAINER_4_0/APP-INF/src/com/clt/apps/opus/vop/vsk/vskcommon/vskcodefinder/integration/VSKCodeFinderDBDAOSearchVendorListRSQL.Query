<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOSearchVendorListRSQL">
			<desc><![CDATA[searchVendorList]]></desc>
			<sql><![CDATA[
SELECT 	
		TO_CHAR(VNDR_SEQ,'000000') AS VNDR_SEQ,
		VNDR_CNT_CD,
		VNDR_LGL_ENG_NM,
		OFC_CD
FROM    MDM_VENDOR
WHERE   DELT_FLG = 'N'
#if (${vndr_seq} != '')
AND     VNDR_SEQ = @[vndr_seq]
#end			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
