<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OperationNPerformMasterDataMgtDBDAORstwgRsnCdRSQL">
			<desc><![CDATA[RstwgRsnCd Select Query]]></desc>
			<sql><![CDATA[
SELECT RSTWG_CD_TP_CD, 
       RSTWG_RSN_CD, 
       RSTWG_RSN_CD_FULL_DESC, 
       DELT_FLG
 FROM OPF_RSTWG_RSN_CD
WHERE DELT_FLG = @[delt_flg]
  AND RSTWG_CD_TP_CD = @[rstwg_cd_tp_cd]
  AND RSTWG_RSN_CD = @[rstwg_rsn_cd]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="rstwg_cd_tp_cd" type="12" value="" out="N"/>
				<param name="rstwg_rsn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
