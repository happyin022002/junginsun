<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOsearchSCGMailAdrsListRSQL">
			<desc><![CDATA[searchSCGMailAdrsList]]></desc>
			<sql><![CDATA[
SELECT CNTC_PSON_EML_CTNT
  FROM SCG_CNTC_PNT
 WHERE RGN_SHP_OPR_CD = @[rgn_shp_opr_cd]
   AND CRR_CD = @[crr_cd]
   AND SLAN_CD = @[slan_cd]
   AND SPCL_CGO_CATE_CD = @[spcl_cgo_cate_cd]--'DG'
   AND CNTC_PSON_EML_CTNT IS NOT NULL
   AND DELT_FLG != 'Y'              
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_cate_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
