<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveOwnBkgCancelRequestMgtDBDAOModifyScgVvdDgCgoCxlRqstCSQL">
			<desc><![CDATA[AddScgVvdDgCgoCxlRqst]]></desc>
			<sql><![CDATA[
 UPDATE SCG_VVD_DG_CGO_CXL_RQST
	SET RQST_VRFY_RSLT_CD  = @[rqst_vrfy_rslt_cd]
      , RQST_VRFY_RSLT_RMK = @[rqst_vrfy_rslt_rmk]
      , UPD_USR_ID         = @[upd_usr_id]
      , UPD_DT             = SYSDATE
  WHERE BKG_NO             = @[bkg_no]
    AND DCGO_CXL_RQST_SEQ  = @[dcgo_cxl_rqst_seq]			]]></sql>
			<params>
				<param name="rqst_vrfy_rslt_cd" type="12" value="" out="N"/>
				<param name="rqst_vrfy_rslt_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dcgo_cxl_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
