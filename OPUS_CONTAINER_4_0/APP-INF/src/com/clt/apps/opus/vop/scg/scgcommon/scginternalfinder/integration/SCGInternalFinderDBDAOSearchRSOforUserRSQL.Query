<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGInternalFinderDBDAOSearchRSOforUserRSQL">
			<desc><![CDATA[Retrieving RSO for Specific Port Code]]></desc>
			<sql><![CDATA[
SELECT X.RGN_SHP_OPR_CD
  FROM SCG_RGN_SHP_OPR_PORT X ,
       COM_USER Y ,
       MDM_ORGANIZATION Z
 WHERE Y.OFC_CD = Z.OFC_CD
   AND X.LOC_CD = Z.LOC_CD
   AND Y.USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
