<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOsearchPsoYdChgRSQL">
			<desc><![CDATA[searchPsoYdChg]]></desc>
			<sql><![CDATA[
SELECT YD_CHG_NO
     , YD_CHG_VER_SEQ
     , CURR_CD
  FROM PSO_YD_CHG
 WHERE LGS_COST_CD = @[lgs_cost_cd]
   AND YD_CD = @[yd_cd]
   AND VNDR_SEQ = @[vndr_seq]
   AND TO_DATE(REPLACE(@[iss_dt],'-'), 'YYYYMMDD') BETWEEN EFF_DT AND EXP_DT
   AND LST_FLG = 'Y'
			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="iss_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
