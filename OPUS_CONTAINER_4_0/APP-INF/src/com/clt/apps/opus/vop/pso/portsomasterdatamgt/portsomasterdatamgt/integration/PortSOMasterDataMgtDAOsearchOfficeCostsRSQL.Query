<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortSOMasterDataMgtDAOsearchOfficeCostsRSQL">
			<desc><![CDATA[searchOfficeCosts
[2015.10.13]962111 OWNERS ACCOUNT INTER-OFFICE ACCT 주석처리.]]></desc>
			<sql><![CDATA[
SELECT T1.ACCT_CD
     , T2.LGS_COST_CD
     , T1.LGS_COST_FULL_NM
     , T3.ACCT_ENG_NM
     , T1.LGS_COST_DTL_CD
     , T1.LGS_COST_OPT_NO
  FROM TES_LGS_COST T1
     , PSO_INV_OFC_COST T2
     , MDM_ACCOUNT T3
WHERE 1=1
  AND T1.LGS_COST_CD_CLSS_LVL = 'A'
  AND T1.LGS_COST_CD = T2.LGS_COST_CD
  AND T2.OFC_CD = @[ofc_cd]
  AND T1.ACCT_CD = T3.ACCT_CD
#if (${cnl_anag_flg} == 'Y') 
  AND T1.LGS_COST_SUBJ_CD = 'CN'
#else 
  AND T1.LGS_COST_SUBJ_CD = 'PT'
#end
 ORDER BY T1.LGS_COST_DTL_CD , T1.LGS_COST_OPT_NO			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
