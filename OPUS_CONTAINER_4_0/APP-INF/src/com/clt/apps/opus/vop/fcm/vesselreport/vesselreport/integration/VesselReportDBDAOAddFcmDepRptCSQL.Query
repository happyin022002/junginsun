<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselReportDBDAOAddFcmDepRptCSQL">
			<desc><![CDATA[Create departure report.
=======================]]></desc>
			<sql><![CDATA[
MERGE INTO FCM_DEP_RPT
USING DUAL ON(
    VSL_CD       = @[vsl_cd]
AND SKD_VOY_NO   = SUBSTR(@[voy_dir_cd], 1, 4)
AND SKD_DIR_CD   = SUBSTR(@[voy_dir_cd], 5, 1)
AND DEP_PORT_CD  = @[dep_port_cd]
AND CLPT_IND_SEQ = DECODE(@[clpt_ind_seq], 'F', '1', 'S', '2', 'T', '3')
)

WHEN MATCHED THEN
UPDATE SET 
REF_NO                 = @[ref_no]                ,
VSL_SLAN_CD            = @[vsl_slan_cd]           ,
GMT_TD_HRS             = @[gmt_td_hrs]            ,
NXT_PORT_CD            = @[nxt_port_cd]           ,
DEP_STS_CD             = @[dep_sts_cd]            ,
RMN_DIST               = @[rmn_dist]              ,
RMN_AVG_SPD            = @[rmn_avg_spd]           ,
NXT_PORT_ETA_DT        = (CASE WHEN LENGTH(@[nxt_port_eta_dt]      )<=12 THEN TO_DATE(RPAD(@[nxt_port_eta_dt]      , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[nxt_port_eta_dt]      , 1, 12), 'YYYYMMDDHH24MI') END) ,
SB_ENG_DT              = (CASE WHEN LENGTH(@[sb_eng_dt]            )<=12 THEN TO_DATE(RPAD(@[sb_eng_dt]            , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[sb_eng_dt]            , 1, 12), 'YYYYMMDDHH24MI') END) ,
PLT_IN_DT              = (CASE WHEN LENGTH(@[plt_in_dt]            )<=12 THEN TO_DATE(RPAD(@[plt_in_dt]            , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[plt_in_dt]            , 1, 12), 'YYYYMMDDHH24MI') END) ,
BFR_BRTH_ANK_DRP_DT    = (CASE WHEN LENGTH(@[bfr_brth_ank_drp_dt]  )<=12 THEN TO_DATE(RPAD(@[bfr_brth_ank_drp_dt]  , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[bfr_brth_ank_drp_dt]  , 1, 12), 'YYYYMMDDHH24MI') END) ,
BFR_BRTH_ANK_OFF_DT    = (CASE WHEN LENGTH(@[bfr_brth_ank_off_dt]  )<=12 THEN TO_DATE(RPAD(@[bfr_brth_ank_off_dt]  , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[bfr_brth_ank_off_dt]  , 1, 12), 'YYYYMMDDHH24MI') END) ,
VPS_ETB_DT             = (CASE WHEN LENGTH(@[vps_etb_dt]           )<=12 THEN TO_DATE(RPAD(@[vps_etb_dt]           , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[vps_etb_dt]           , 1, 12), 'YYYYMMDDHH24MI') END) ,
CGO_WRK_ST_DT          = (CASE WHEN LENGTH(@[cgo_wrk_st_dt]        )<=12 THEN TO_DATE(RPAD(@[cgo_wrk_st_dt]        , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[cgo_wrk_st_dt]        , 1, 12), 'YYYYMMDDHH24MI') END) ,
CGO_WRK_END_DT         = (CASE WHEN LENGTH(@[cgo_wrk_end_dt]       )<=12 THEN TO_DATE(RPAD(@[cgo_wrk_end_dt]       , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[cgo_wrk_end_dt]       , 1, 12), 'YYYYMMDDHH24MI') END) ,
VPS_ETD_DT             = (CASE WHEN LENGTH(@[vps_etd_dt]           )<=12 THEN TO_DATE(RPAD(@[vps_etd_dt]           , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[vps_etd_dt]           , 1, 12), 'YYYYMMDDHH24MI') END) ,
AFT_UNBRTH_ANK_DRP_DT  = (CASE WHEN LENGTH(@[aft_unbrth_ank_drp_dt])<=12 THEN TO_DATE(RPAD(@[aft_unbrth_ank_drp_dt], 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[aft_unbrth_ank_drp_dt], 1, 12), 'YYYYMMDDHH24MI') END) ,
AFT_UNBRTH_ANK_OFF_DT  = (CASE WHEN LENGTH(@[aft_unbrth_ank_off_dt])<=12 THEN TO_DATE(RPAD(@[aft_unbrth_ank_off_dt], 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[aft_unbrth_ank_off_dt], 1, 12), 'YYYYMMDDHH24MI') END) ,
PLT_OUT_DT             = (CASE WHEN LENGTH(@[plt_out_dt]           )<=12 THEN TO_DATE(RPAD(@[plt_out_dt]           , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[plt_out_dt]           , 1, 12), 'YYYYMMDDHH24MI') END) ,
RUP_DT                 = (CASE WHEN LENGTH(@[rup_dt]               )<=12 THEN TO_DATE(RPAD(@[rup_dt]               , 12, '0'), 'YYYYMMDDHH24MI') ELSE TO_DATE(SUBSTR(@[rup_dt]               , 1, 12), 'YYYYMMDDHH24MI') END) ,
MNVR_IN_ML_DIST        = @[mnvr_in_ml_dist]       ,
MNVR_OUT_ML_DIST       = @[mnvr_out_ml_dist]      ,
NVGT_ML_DIST           = @[nvgt_ml_dist]          ,
ENG_ML_DIST            = @[eng_ml_dist]           ,
AVG_SPD                = @[avg_spd]               ,
AVG_RPM_PWR            = @[avg_rpm_pwr]           ,
RUN_HRS_IN_HV_WE       = @[run_hrs_in_hv_we]      ,
SEA_DET_RSN_CD1                 = SUBSTR(SUBSTR(@[det_rsn_ctnt], 1,                                INSTR(@[det_rsn_ctnt], '|', 1, 1) - 1), 1, 1)                                      ,
SEA_DET_RSN_HRS1                = SUBSTR(SUBSTR(@[det_rsn_ctnt], 1,                                INSTR(@[det_rsn_ctnt], '|', 1, 1) - 1), 2)                                         ,
SEA_DET_RSN_CD2                 = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 1)+1, INSTR(@[det_rsn_ctnt], '|', 1, 2) - INSTR(@[det_rsn_ctnt], '|', 1, 1)-1), 1, 1) ,
SEA_DET_RSN_HRS2                = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 1)+1, INSTR(@[det_rsn_ctnt], '|', 1, 2) - INSTR(@[det_rsn_ctnt], '|', 1, 1)-1), 2)    ,
SEA_DET_RSN_CD3                 = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 2)+1, INSTR(@[det_rsn_ctnt], '|', 1, 3) - INSTR(@[det_rsn_ctnt], '|', 1, 2)-1), 1, 1) ,
SEA_DET_RSN_HRS3                = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 2)+1, INSTR(@[det_rsn_ctnt], '|', 1, 3) - INSTR(@[det_rsn_ctnt], '|', 1, 2)-1), 2)    ,
PORT_DET_RSN_CD1                = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 3)+1, INSTR(@[det_rsn_ctnt], '|', 1, 4) - INSTR(@[det_rsn_ctnt], '|', 1, 3)-1), 1, 1) ,
PORT_DET_RSN_HRS1               = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 3)+1, INSTR(@[det_rsn_ctnt], '|', 1, 4) - INSTR(@[det_rsn_ctnt], '|', 1, 3)-1), 2)    ,
PORT_DET_RSN_CD2                = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 4)+1, INSTR(@[det_rsn_ctnt], '|', 1, 5) - INSTR(@[det_rsn_ctnt], '|', 1, 4)-1), 1, 1) ,
PORT_DET_RSN_HRS2               = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 4)+1, INSTR(@[det_rsn_ctnt], '|', 1, 5) - INSTR(@[det_rsn_ctnt], '|', 1, 4)-1), 2)    ,
PORT_DET_RSN_CD3                = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 5)+1, INSTR(@[det_rsn_ctnt], '|', 1, 6) - INSTR(@[det_rsn_ctnt], '|', 1, 5)-1), 1, 1) ,
PORT_DET_RSN_HRS3               = SUBSTR(SUBSTR(@[det_rsn_ctnt], INSTR(@[det_rsn_ctnt], '|', 1, 5)+1, INSTR(@[det_rsn_ctnt], '|', 1, 6) - INSTR(@[det_rsn_ctnt], '|', 1, 5)-1), 2)    ,
ARR_FWDDR_HGT                   = SUBSTR(@[arr_drft_ctnt],                                 1, INSTR(@[arr_drft_ctnt], '|', 1, 1) -                                 1)                 ,
ARR_MID_DRFT_HGT                = SUBSTR(@[arr_drft_ctnt], INSTR(@[arr_drft_ctnt], '|', 1, 1)+1, INSTR(@[arr_drft_ctnt], '|', 1, 2) - INSTR(@[arr_drft_ctnt], '|', 1, 1)-1)           ,
ARR_AFTDR_HGT                   = SUBSTR(@[arr_drft_ctnt], INSTR(@[arr_drft_ctnt], '|', 1, 2)+1, INSTR(@[arr_drft_ctnt], '|', 1, 3) - INSTR(@[arr_drft_ctnt], '|', 1, 2)-1)           ,
ARR_GM_HGT                      = SUBSTR(@[arr_drft_ctnt], INSTR(@[arr_drft_ctnt], '|', 1, 3)+1, INSTR(@[arr_drft_ctnt], '|', 1, 4) - INSTR(@[arr_drft_ctnt], '|', 1, 3)-1)           ,
ARR_FOIL_WGT                    = SUBSTR(@[arr_rob_ctnt],                                1, INSTR(@[arr_rob_ctnt], '|', 1, 1) -                                1)                     ,
ARR_DOIL_WGT                    = SUBSTR(@[arr_rob_ctnt], INSTR(@[arr_rob_ctnt], '|', 1, 1)+1, INSTR(@[arr_rob_ctnt], '|', 1, 2) - INSTR(@[arr_rob_ctnt], '|', 1, 1)-1)               ,
ARR_FRSH_WTR_WGT                = SUBSTR(@[arr_rob_ctnt], INSTR(@[arr_rob_ctnt], '|', 1, 2)+1, INSTR(@[arr_rob_ctnt], '|', 1, 3) - INSTR(@[arr_rob_ctnt], '|', 1, 2)-1)               ,
ARR_BLST_WGT                    = SUBSTR(@[arr_rob_ctnt], INSTR(@[arr_rob_ctnt], '|', 1, 3)+1, INSTR(@[arr_rob_ctnt], '|', 1, 4) - INSTR(@[arr_rob_ctnt], '|', 1, 3)-1)               ,
ARR_LOW_SULP_FOIL_WGT           = SUBSTR(@[arr_rob_ctnt], INSTR(@[arr_rob_ctnt], '|', 1, 4)+1, INSTR(@[arr_rob_ctnt], '|', 1, 5) - INSTR(@[arr_rob_ctnt], '|', 1, 4)-1)               ,
ARR_LOW_SULP_DOIL_WGT           = SUBSTR(@[arr_rob_ctnt], INSTR(@[arr_rob_ctnt], '|', 1, 5)+1, INSTR(@[arr_rob_ctnt], '|', 1, 6) - INSTR(@[arr_rob_ctnt], '|', 1, 5)-1)               ,
DEP_FWDDR_HGT                   = SUBSTR(@[dep_drft_ctnt],                                 1, INSTR(@[dep_drft_ctnt], '|', 1, 1) -                                 1)                 ,
DEP_MID_DRFT_HGT                = SUBSTR(@[dep_drft_ctnt], INSTR(@[dep_drft_ctnt], '|', 1, 1)+1, INSTR(@[dep_drft_ctnt], '|', 1, 2) - INSTR(@[dep_drft_ctnt], '|', 1, 1)-1)           ,
DEP_AFTDR_HGT                   = SUBSTR(@[dep_drft_ctnt], INSTR(@[dep_drft_ctnt], '|', 1, 2)+1, INSTR(@[dep_drft_ctnt], '|', 1, 3) - INSTR(@[dep_drft_ctnt], '|', 1, 2)-1)           ,
DEP_GM_HGT                      = SUBSTR(@[dep_drft_ctnt], INSTR(@[dep_drft_ctnt], '|', 1, 3)+1, INSTR(@[dep_drft_ctnt], '|', 1, 4) - INSTR(@[dep_drft_ctnt], '|', 1, 3)-1)           ,
DEP_FOIL_WGT                    = SUBSTR(@[dep_rob_ctnt],                                1, INSTR(@[dep_rob_ctnt], '|', 1, 1) -                                1)                     ,
DEP_DOIL_WGT                    = SUBSTR(@[dep_rob_ctnt], INSTR(@[dep_rob_ctnt], '|', 1, 1)+1, INSTR(@[dep_rob_ctnt], '|', 1, 2) - INSTR(@[dep_rob_ctnt], '|', 1, 1)-1)               ,
DEP_FRSH_WTR_WGT                = SUBSTR(@[dep_rob_ctnt], INSTR(@[dep_rob_ctnt], '|', 1, 2)+1, INSTR(@[dep_rob_ctnt], '|', 1, 3) - INSTR(@[dep_rob_ctnt], '|', 1, 2)-1)               ,
DEP_BLST_WGT                    = SUBSTR(@[dep_rob_ctnt], INSTR(@[dep_rob_ctnt], '|', 1, 3)+1, INSTR(@[dep_rob_ctnt], '|', 1, 4) - INSTR(@[dep_rob_ctnt], '|', 1, 3)-1)               ,
DEP_LOW_SULP_FOIL_WGT           = SUBSTR(@[dep_rob_ctnt], INSTR(@[dep_rob_ctnt], '|', 1, 4)+1, INSTR(@[dep_rob_ctnt], '|', 1, 5) - INSTR(@[dep_rob_ctnt], '|', 1, 4)-1)               ,
DEP_LOW_SULP_DOIL_WGT           = SUBSTR(@[dep_rob_ctnt], INSTR(@[dep_rob_ctnt], '|', 1, 5)+1, INSTR(@[dep_rob_ctnt], '|', 1, 6) - INSTR(@[dep_rob_ctnt], '|', 1, 5)-1)               ,
NXT_FWDDR_HGT                   = SUBSTR(@[nxt_drpt_ctnt],                                 1, INSTR(@[nxt_drpt_ctnt], '|', 1, 1) -                                 1)                 ,
NXT_MID_DRFT_HGT                = SUBSTR(@[nxt_drpt_ctnt], INSTR(@[nxt_drpt_ctnt], '|', 1, 1)+1, INSTR(@[nxt_drpt_ctnt], '|', 1, 2) - INSTR(@[nxt_drpt_ctnt], '|', 1, 1)-1)           ,
NXT_AFTDR_HGT                   = SUBSTR(@[nxt_drpt_ctnt], INSTR(@[nxt_drpt_ctnt], '|', 1, 2)+1, INSTR(@[nxt_drpt_ctnt], '|', 1, 3) - INSTR(@[nxt_drpt_ctnt], '|', 1, 2)-1)           ,
NXT_GM_HGT                      = SUBSTR(@[nxt_drpt_ctnt], INSTR(@[nxt_drpt_ctnt], '|', 1, 3)+1, INSTR(@[nxt_drpt_ctnt], '|', 1, 4) - INSTR(@[nxt_drpt_ctnt], '|', 1, 3)-1)           ,
NXT_FOIL_WGT                    = SUBSTR(@[nxt_rob_ctnt],                                1, INSTR(@[nxt_rob_ctnt], '|', 1, 1) -                                1)                     ,
