<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneExpenseRatioMgtDAOsearchPsoPortExpenseDivisionRSQL">
			<desc><![CDATA[PSO PORT EXPENSE DIVISION 테이블 Select
[2015.03.13]Order by RLANE_DIR_CD 제거]]></desc>
			<sql><![CDATA[
SELECT  RLANE_CD
       ,REV_DIR_CD RLANE_DIR_CD
       ,SKD_DIR_CD
       ,LOC_CD
       ,IB_RTO
       ,OB_RTO
       ,SLAN_CD
       ,PORT_SEQ
       ,RLANE_CD RLANE_CD_B
       ,rev_dir_cd/*2009.11.23. rlane_dir_cd*/ RLANE_DIR_CD_B
       ,SKD_DIR_CD SKD_DIR_CD_B
       ,LOC_CD LOC_CD_B
FROM    PSO_PORT_EXPN_DIV T1, MDM_VSL_SVC_LANE_DIR T2
WHERE   T1.SLAN_CD      = T2.VSL_SLAN_CD
AND     T1.SKD_DIR_CD   = T2.VSL_SLAN_DIR_CD
AND     T1.SLAN_CD      = @[vsl_slan_cd]
--ORDER BY T2.VSL_SLAN_DIR_SEQ, T1.PORT_SEQ
--ORDER BY RLANE_CD, SKD_DIR_CD, LOC_CD
--ORDER BY PORT_SEQ
ORDER BY T2.VSL_SLAN_DIR_SEQ, PORT_SEQ			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="FEX" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
