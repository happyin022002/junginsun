<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EdiTransmitHistoryDBDAOSearchFlatFileRSQL">
			<desc><![CDATA[FlatFile Contents 조회]]></desc>
			<sql><![CDATA[
SELECT TRSM_BND_CD, FLT_FILE_DAT_CTNT 
FROM SCG_PRNR_SPCL_CGO_FLT_FILE
WHERE BKG_REF_NO = @[bkg_ref_no]
AND PRNR_SPCL_CGO_SEQ = @[prnr_spcl_cgo_seq]			]]></sql>
			<params>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="prnr_spcl_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
