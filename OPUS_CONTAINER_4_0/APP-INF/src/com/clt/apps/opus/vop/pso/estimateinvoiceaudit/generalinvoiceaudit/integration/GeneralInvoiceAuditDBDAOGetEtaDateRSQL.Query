<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOGetEtaDateRSQL">
			<desc><![CDATA[case 157: ETA Date 
[2015.07.21]Virtual Add Calling 처리. VSK_VSL_PORT_SKD.NVL(VT_ADD_CALL_FLG, 'N') = 'N']]></desc>
			<sql><![CDATA[
SELECT 'TO_DATE('''||TO_CHAR (A.VPS_ETA_DT, 'YYYYMMDD')||''',''YYYYMMDD'')'
  FROM VSK_VSL_PORT_SKD A
 WHERE 1=1
   AND A.VSL_CD         = SUBSTR(@[vvd], 1, 4)
   AND A.SKD_VOY_NO     = SUBSTR(@[vvd], 5, 4)
   AND A.SKD_DIR_CD     = SUBSTR(@[vvd], 9)
   AND A.VPS_PORT_CD    = SUBSTR(@[yd_cd], 1, 5)
   AND A.YD_CD          = @[yd_cd]
   AND A.CLPT_IND_SEQ   = @[clpt_ind_seq]
   AND NVL(A.VT_ADD_CALL_FLG, 'N') = 'N' /*2015.07.21 Add*/
			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
