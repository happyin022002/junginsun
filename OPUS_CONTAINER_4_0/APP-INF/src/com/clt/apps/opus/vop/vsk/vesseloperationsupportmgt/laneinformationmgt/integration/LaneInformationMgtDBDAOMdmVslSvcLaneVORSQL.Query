<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneInformationMgtDBDAOMdmVslSvcLaneVORSQL">
			<desc><![CDATA[Lane Group]]></desc>
			<sql><![CDATA[
SELECT VSL_SLAN_CD,	
	   VSL_SLAN_NM, 
	   VSKD_FLET_GRP_CD, 
       UPD_USR_ID, 
       TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI') AS UPD_DT,
	   VSL_SVC_TP_CD
FROM MDM_VSL_SVC_LANE
#if (${vskd_flet_grp_cd} != '') 
WHERE	VSKD_FLET_GRP_CD = @[vskd_flet_grp_cd]
#else
WHERE	VSKD_FLET_GRP_CD IS NULL
#end

AND DELT_FLG = 'N'
#if (${vsl_svc_tp_cd} != '') 
	#if (${vsl_svc_tp_cd} == '1') 
AND 	NVL(VSL_SVC_TP_CD, '-')  != 'O'
	#else
AND 	VSL_SVC_TP_CD  = 'O'
	#end
#end

ORDER BY VSL_SLAN_CD			]]></sql>
			<params>
				<param name="vskd_flet_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
