<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConsortiumVoyageMgtDBDAOUpdateConsortiumVoyageUSQL">
			<desc><![CDATA[Update Cssm]]></desc>
			<sql><![CDATA[
UPDATE 		VSK_VSL_PORT_SKD				X
SET 		X.IB_CSSM_VOY_NO 				= @[ib_cssm_voy_no]
		,	X.OB_CSSM_VOY_NO 				= CASE WHEN TURN_PORT_IND_CD NOT IN ('D','V','F') THEN @[ob_cssm_voy_no] ELSE '' END 
		,	X.UPD_USR_ID     				= @[upd_usr_id]
		,	X.UPD_DT         				= SYSDATE
			
			--::ADD:2015-05-15:by TOP::--
		,	X.CSSM_VOY_INIT_CRE_FLG  		= 'N'
			-----------------------------
WHERE		X.VSL_CD          				= @[vsl_cd]
AND 		X.SKD_VOY_NO	  				= @[skd_voy_no]
AND 		X.SKD_DIR_CD    				= @[skd_dir_cd]
AND 		X.VPS_PORT_CD   				= @[vps_port_cd]
AND			X.CLPT_IND_SEQ					= @[clpt_ind_seq]
			]]></sql>
			<params>
				<param name="ib_cssm_voy_no" type="12" value="" out="N"/>
				<param name="ob_cssm_voy_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
