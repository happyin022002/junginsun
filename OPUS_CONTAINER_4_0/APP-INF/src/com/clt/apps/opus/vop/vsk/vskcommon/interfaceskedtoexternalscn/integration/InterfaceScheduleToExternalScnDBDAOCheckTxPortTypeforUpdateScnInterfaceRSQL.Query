<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToExternalScnDBDAOCheckTxPortTypeforUpdateScnInterfaceRSQL">
			<desc><![CDATA[TX PORT]]></desc>
			<sql><![CDATA[
SELECT  F.VSL_CD
      , F.SKD_VOY_NO
      , F.SKD_DIR_CD
      , F.VPS_PORT_CD
      , F.CSSM_VOY_NO_IF_SEQ 

	  , (SELECT   IB_CSSM_VOY_NO
         FROM     (
                  SELECT   RANK() OVER (PARTITION BY PS.VSL_CD,PS.SKD_VOY_NO,PS.SKD_DIR_CD,PS.VPS_PORT_CD ORDER BY DECODE(PS.SKD_CNG_STS_CD,'S',9,1) ASC, PS.CLPT_IND_SEQ ASC) RNK
                        ,  PS.IB_CSSM_VOY_NO
                  FROM     VSK_VSL_PORT_SKD        		PS
                  WHERE    PS.VSL_CD               		= @[vsl_cd]
                  AND      PS.SKD_VOY_NO           		= @[skd_voy_no]
                  AND      PS.SKD_DIR_CD           		= @[skd_dir_cd]
                  AND      PS.VPS_PORT_CD          		= @[vps_port_cd]
                  AND      NVL(PS.VT_ADD_CALL_FLG,'N')	= 'N'
                  )
         WHERE     RNK      = 1
		 )						AS IB_CSSM_VOY_NO
	  , (SELECT   OB_CSSM_VOY_NO
         FROM     (
                  SELECT   RANK() OVER (PARTITION BY PS.VSL_CD,PS.SKD_VOY_NO,PS.SKD_DIR_CD,PS.VPS_PORT_CD ORDER BY DECODE(PS.SKD_CNG_STS_CD,'S',9,1) ASC, PS.CLPT_IND_SEQ ASC) RNK
                        ,  PS.OB_CSSM_VOY_NO
                  FROM     VSK_VSL_PORT_SKD        		PS
                  WHERE    PS.VSL_CD               		= @[vsl_cd]
                  AND      PS.SKD_VOY_NO           		= @[skd_voy_no]
                  AND      PS.SKD_DIR_CD           		= @[skd_dir_cd]
                  AND      PS.VPS_PORT_CD          		= @[vps_port_cd]
                  AND      NVL(PS.VT_ADD_CALL_FLG,'N')	= 'N'
                  )
         WHERE     RNK      = 1
		 )						AS OB_CSSM_VOY_NO
       
      , F.INSF_CNQE_VAL         -- 'S':SUCCESS, NULL:INITIAL --
      , F.INSF_DV_CD            -- 'I':INSERT, 'U':UPDATE, 'D':DELETE --
        
      , CASE WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'I' THEN 'U'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'U' THEN 'U'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'D' THEN 'I'        
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'I' THEN 'I'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'U' THEN 'U'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'D' THEN 'U'
        END                     NEW_INSF_DV_CD      
      , CASE WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'I' THEN 'I'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'U' THEN 'I'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'D' THEN 'I'        
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'I' THEN 'U'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'U' THEN 'U'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'D' THEN 'U'
        END                     NEW_TX_TP_CD      
        
FROM    VSK_VSL_CSSM_VOY_NO_IF  F
WHERE   F.VSL_CD                = @[vsl_cd]
AND     F.SKD_VOY_NO            = @[skd_voy_no]
AND     F.SKD_DIR_CD            = @[skd_dir_cd]
AND     F.VPS_PORT_CD           = @[vps_port_cd]
AND     F.CSSM_VOY_NO_IF_SEQ    = (SELECT   MAX(FF.CSSM_VOY_NO_IF_SEQ)
                                   FROM     VSK_VSL_CSSM_VOY_NO_IF  FF
                                   WHERE    FF.VSL_CD               = F.VSL_CD
                                   AND      FF.SKD_VOY_NO           = F.SKD_VOY_NO
                                   AND      FF.SKD_DIR_CD           = F.SKD_DIR_CD
                                   AND      FF.VPS_PORT_CD          = F.VPS_PORT_CD
                                   )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
