<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToIBISDBDAOVskPortNworkCSQL">
			<desc><![CDATA[..]]></desc>
			<sql><![CDATA[
#if (${if_mnpl_cd} == '') 
INSERT INTO VSK_PORT_NWORK_IBIS_IF
(
      LOC_CD
    , HOL_SEQ
    , IBIS_IF_SEQ
    , IF_MNPL_CD
    , HOL_ST_DT
    , HOL_END_DT
    , HOL_NM
    , HOL_RMK
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
    , IF_SND_CD 
) VALUES (
      @[loc_cd]
    , '1'
    , VSK_PORT_NWORK_IBIS_IF_SEQ.NEXTVAL
    , 'D'
    , ''
    , ''
    , ''
    , ''
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
    , NVL((SELECT /*+ INDEX_DESC(A XPKVSK_PORT_NWORK_IBIS_IF) */ 'N'
            FROM VSK_PORT_NWORK_IBIS_IF A WHERE LOC_CD=@[loc_cd] AND IF_SND_CD='Y' AND ROWNUM=1) ,'X')
)
#else
INSERT INTO VSK_PORT_NWORK_IBIS_IF
(
      LOC_CD
    , HOL_SEQ
    , IBIS_IF_SEQ
    , IF_MNPL_CD
    , HOL_ST_DT
    , HOL_END_DT
    , HOL_NM
    , HOL_RMK
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
    , IF_SND_CD 
)
SELECT 
      LOC_CD
    , HOL_SEQ
    , VSK_PORT_NWORK_IBIS_IF_SEQ.NEXTVAL
    , 'I'
    , HOL_ST_DT
    , HOL_END_DT
    , HOL_NM
    , HOL_RMK
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
    , 'N'
FROM  VSK_PORT_NWORK
WHERE LOC_CD = @[loc_cd]

#end			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
