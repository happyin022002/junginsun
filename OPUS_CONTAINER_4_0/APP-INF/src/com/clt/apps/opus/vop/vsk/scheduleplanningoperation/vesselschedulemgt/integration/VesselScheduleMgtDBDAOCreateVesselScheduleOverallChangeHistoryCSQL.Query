<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOCreateVesselScheduleOverallChangeHistoryCSQL">
			<desc><![CDATA[CreateVesselScheduleOverallChangeHistory]]></desc>
			<sql><![CDATA[
INSERT INTO VSK_VSL_SKD_CNG_HIS
      	(
        	VSL_CD
      	,  	SKD_VOY_NO
      	,  	SKD_DIR_CD
      	,  	HIS_VVD_SEQ
      	,  	VSKD_CNG_TP_CD
      	,  	VSL_SLAN_CD
      	,  	PF_SKD_TP_CD
      	,  	ACT_CRR_CD
      	--,	HIS_DTL_DELT_DT--
      	,  	HIS_DELT_PROHI_FLG
      	,  	VVD_CRE_USR_ID
      	,  	VVD_CRE_DT
      	,  	VVD_UPD_USR_ID
      	,  	VVD_UPD_DT
      	,  	SKD_CNG_ID
      	,  	SKD_CNG_DESC
      	,  	CRE_USR_ID
      	,  	CRE_DT
      	,  	UPD_USR_ID
      	,  	UPD_DT
      )
SELECT   	VS.VSL_CD
      	,  	VS.SKD_VOY_NO
      	,  	VS.SKD_DIR_CD
      	,  	@[his_vvd_seq]
      	,  	@[vskd_cng_tp_cd]
      	,  	VS.VSL_SLAN_CD
      	,  	VS.PF_SKD_TP_CD
      	,  	VS.ACT_CRR_CD
      	--,	''	--
      	,  	NVL(@[his_delt_prohi_flg]	,'N')
      	,  	VS.CRE_USR_ID
      	,  	VS.CRE_DT
      	,  	VS.UPD_USR_ID
      	,  	VS.UPD_DT
		,	@[skd_cng_id]
		,	@[skd_cng_desc]
      	,  	NVL(@[upd_usr_id],'NO-ACCOUNT')
      	,  	SYSDATE
      	,  	NVL(@[upd_usr_id],'NO-ACCOUNT')
      	,  	SYSDATE      
FROM     	VSK_VSL_SKD      				VS  
WHERE    	VS.VSL_CD        				= @[vsl_cd]
AND      	VS.SKD_VOY_NO    				= @[skd_voy_no]
AND      	VS.SKD_DIR_CD    				= @[skd_dir_cd]
 			]]></sql>
			<params>
				<param name="his_vvd_seq" type="12" value="" out="N"/>
				<param name="vskd_cng_tp_cd" type="12" value="" out="N"/>
				<param name="his_delt_prohi_flg" type="12" value="" out="N"/>
				<param name="skd_cng_id" type="12" value="" out="N"/>
				<param name="skd_cng_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
