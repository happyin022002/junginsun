<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchCostCodeListRSQL">
			<desc><![CDATA[cost/account 코드 조회]]></desc>
			<sql><![CDATA[
SELECT T2.* 
  FROM (
         SELECT T2.ACCT_CD
             , T3.ACCT_ENG_NM
             , T2.LGS_COST_CD
             , T2.LGS_COST_FULL_NM
             , T2.LGS_COST_SUBJ_CD
             , DECODE(T2.LGS_COST_SUBJ_CD,'PT','001','002') ORD
             , T2.LGS_COST_OPT_NO
          FROM PSO_INV_OFC_COST T1
             , TES_LGS_COST T2
             , MDM_ACCOUNT T3
         WHERE T1.LGS_COST_CD = T2.LGS_COST_CD
           AND T2.LGS_COST_SUBJ_CD IN ('PT', 'CN')
           AND T2.LGS_COST_CD_CLSS_LVL = 'A'
           AND T2.ACCT_CD = T3.ACCT_CD
           AND T1.OFC_CD = @[ofc_cd]
       ) T2
 ORDER BY T2.ORD, T2.LGS_COST_OPT_NO			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
