<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgClssCdVORSQL">
			<desc><![CDATA[Definition of Class (Creation) 조회]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_CLSS_CD
,	IMDG_CLSS_CD_DESC
,	NVL(DELT_FLG,'N') AS DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_IMDG_CLSS_CD
WHERE	1 = 1
#if (${delt_flg} != '') 
AND NVL(DELT_FLG,'N') = @[delt_flg]
#end
#if (${imdg_clss_cd} != '') 
AND IMDG_CLSS_CD = @[imdg_clss_cd]
#end
ORDER BY IMDG_CLSS_CD			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
