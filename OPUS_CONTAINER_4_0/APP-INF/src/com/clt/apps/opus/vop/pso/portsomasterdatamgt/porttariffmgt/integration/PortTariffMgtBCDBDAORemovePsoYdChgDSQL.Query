<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAORemovePsoYdChgDSQL">
			<desc><![CDATA[PSO_YD_CHG <delete>]]></desc>
			<sql><![CDATA[
DELETE PSO_YD_CHG X
WHERE  1=1  
AND    X.YD_CHG_NO      = @[yd_chg_no]
AND    X.YD_CHG_VER_SEQ = @[yd_chg_ver_seq] 			]]></sql>
			<params>
				<param name="yd_chg_no" type="12" value="" out="N"/>
				<param name="yd_chg_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
