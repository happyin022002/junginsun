<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToExternalDBDAORemoveVipsIfDtlOldPortListDSQL">
			<desc><![CDATA[VIPS I/F DETAIL OLD PORT LIST 전체삭제]]></desc>
			<sql><![CDATA[
DELETE FROM		VSK_VSL_SKD_VIPS_IF_DTL	D
WHERE     		1 = 1
AND       		D.VSL_CD				= @[vsl_cd]
AND       		D.SKD_VOY_NO			= @[skd_voy_no]
AND       		D.SKD_DIR_CD			= @[skd_dir_cd]     
AND       		D.VIPS_IF_SEQ			= @[vips_if_seq]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vips_if_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
