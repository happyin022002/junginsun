<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetRhVolRSQL">
			<desc><![CDATA[Tariff 비용 계산을 위해 Rehandling Volume을 구한다.
[2015.09.09]TON > TEU(D2외 사이즈 카운터 *2)]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(DECODE(CNTR_SIZE,'2',1,2)),0)
  FROM TDR_CNTR_DETAIL
 WHERE VSL_CD   = SUBSTR(@[vvd], 1, 4)
   AND VOY_NO   = SUBSTR(@[vvd], 5, 4)
   AND DIR_CD   = SUBSTR(@[vvd], 9, 1)
   AND PORT_CD  = SUBSTR(@[yd_cd], 1, 5)
   AND CALL_IND = @[clpt_ind_seq]
   AND PRECELL IS NOT NULL
   AND STATUS   = 'ST'			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
