<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToExternalScnDBDAOCreateAllPortScnInterfaceCSQL">
			<desc><![CDATA[SCN I/F VVD 데이터 생성]]></desc>
			<sql><![CDATA[
INSERT INTO  VSK_VSL_CSSM_VOY_NO_IF F
             (   VSL_CD
              ,  SKD_VOY_NO
              ,  SKD_DIR_CD
              ,  VPS_PORT_CD
              ,  CSSM_VOY_NO_IF_SEQ
              ,  VSL_SLAN_CD
              ,  IB_CSSM_VOY_NO
              ,  OB_CSSM_VOY_NO
              ,  INSF_ID
              ,  INSF_PRS_ID
              ,  INSF_DTTM
              ,  INSF_CNQE_VAL
              ,  INSF_DV_CD
              ,	INSF_CNQE_CONT
              ,	CRE_USR_ID
              ,	CRE_DT
              ,	UPD_USR_ID
              ,	UPD_DT
             )
           
SELECT   PS.VSL_CD
       , PS.SKD_VOY_NO
       , PS.SKD_DIR_CD
       , PS.VPS_PORT_CD
       , VSK_VSL_CSSM_VOY_NO_IF_SEQ.NEXTVAL
       , PS.SLAN_CD
       , PS.IB_CSSM_VOY_NO
       , PS.OB_CSSM_VOY_NO
       , 'OPEDI002'
       , ''
       , ''
       , ''
       , 'I'
       , ''
       , PS.UPD_USR_ID
       , SYSDATE
       , PS.UPD_USR_ID
       , SYSDATE
FROM     (
          SELECT   RANK() OVER (PARTITION BY PS.VSL_CD,PS.SKD_VOY_NO,PS.SKD_DIR_CD,PS.VPS_PORT_CD ORDER BY DECODE(PS.SKD_CNG_STS_CD,'S',9,1) ASC, PS.CLPT_IND_SEQ ASC) RNK
                ,  PS.* 
          FROM     VSK_VSL_PORT_SKD        PS
          WHERE    PS.VSL_CD               = @[vsl_cd]
          AND      PS.SKD_VOY_NO           = @[skd_voy_no]
          AND      PS.SKD_DIR_CD           = @[skd_dir_cd]
		  AND	   NVL(PS.VT_ADD_CALL_FLG,'N') = 'N'
          )        PS
WHERE     RNK      = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
