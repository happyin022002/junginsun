<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOGetLoadingPortRsoRSQL">
			<desc><![CDATA[get Loading Port Rso]]></desc>
			<sql><![CDATA[
SELECT B.LOC_CD, B.LOC_NM,
        B.RGN_CD rgn_shp_opr_cd,
        (SELECT cnt_nm FROM MDM_COUNTRY A WHERE A.cnt_cd=B.cnt_cd )cnt_nm
  FROM MDM_LOCATION B
WHERE  B.loc_cd =  @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
