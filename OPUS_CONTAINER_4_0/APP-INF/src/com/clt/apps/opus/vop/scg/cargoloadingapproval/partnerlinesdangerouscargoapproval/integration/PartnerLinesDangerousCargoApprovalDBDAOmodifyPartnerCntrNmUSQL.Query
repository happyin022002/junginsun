<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerLinesDangerousCargoApprovalDBDAOmodifyPartnerCntrNmUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
--update container no.
UPDATE SCG_PRNR_APRO_RQST_CGO 
   SET CNTR_REF_NO = @[cntr_ref_no],
       UPD_DT      = SYSDATE, 
       UPD_USR_ID  = @[upd_usr_id]
 WHERE CRR_CD            = @[crr_cd]
   AND BKG_REF_NO        = @[bkg_ref_no] 
   AND SPCL_CGO_RQST_SEQ = @[spcl_cgo_rqst_seq] 
   AND SPCL_CNTR_SEQ     = @[spcl_cntr_seq]
   AND CGO_OPR_CD        = @[cgo_opr_cd]
   AND PRNR_CGO_RQST_SEQ = @[prnr_cgo_rqst_seq] 			]]></sql>
			<params>
				<param name="cntr_ref_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_rqst_seq" type="12" value="" out="N"/>
				<param name="spcl_cntr_seq" type="12" value="" out="N"/>
				<param name="cgo_opr_cd" type="12" value="" out="N"/>
				<param name="prnr_cgo_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
