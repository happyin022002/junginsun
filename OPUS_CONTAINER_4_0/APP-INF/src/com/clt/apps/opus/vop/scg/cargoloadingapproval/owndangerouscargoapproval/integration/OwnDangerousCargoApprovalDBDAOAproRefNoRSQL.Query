<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOAproRefNoRSQL">
			<desc><![CDATA[AproRefNo를 생성하는 화면]]></desc>
			<sql><![CDATA[
SELECT      'COM'
         || 'POL'
         || TO_CHAR(SYSDATE, 'YYMMDD')
         || LPAD('1', 3, '0')  AS APRO_REF_NO 
FROM     DUAL  			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
