<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOUpdateVVDAproRqstForEdiAutoCancelUSQL">
			<desc><![CDATA[updateVVDAproRqstForEdiAutoCancel]]></desc>
			<sql><![CDATA[
UPDATE    SCG_VVD_APRO_RQST         YY
SET       YY.EDI_AUTO_CXL_FLG       = 'Y'
		, YY.MAPG_EDI_TRSM_STS_CD   = 'SX'
WHERE     YY.BKG_NO                 = @[bkg_no]
AND       YY.SPCL_CGO_APRO_RQST_SEQ = @[spcl_cgo_apro_rqst_seq]
AND       YY.VSL_PRE_PST_CD         = @[vsl_pre_pst_cd]
AND       YY.VSL_SEQ                = @[vsl_seq]
AND       YY.MAPG_EDI_TRSM_STS_CD   = 'S'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
