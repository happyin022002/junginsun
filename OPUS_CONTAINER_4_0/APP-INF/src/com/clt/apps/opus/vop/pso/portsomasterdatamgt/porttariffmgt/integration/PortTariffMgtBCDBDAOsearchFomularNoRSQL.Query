<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchFomularNoRSQL">
			<desc><![CDATA[저장되어있는 formula의 no를 가져온다.]]></desc>
			<sql><![CDATA[
select nvl(max(foml_no), -1) foml_no from pso_formula
where 1=1
and foml_desc = @[foml_desc]			]]></sql>
			<params>
				<param name="foml_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
