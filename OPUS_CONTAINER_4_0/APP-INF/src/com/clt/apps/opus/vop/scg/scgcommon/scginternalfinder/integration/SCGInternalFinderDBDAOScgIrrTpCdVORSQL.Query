<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGInternalFinderDBDAOScgIrrTpCdVORSQL">
			<desc><![CDATA[Irregular Type 조회      ]]></desc>
			<sql><![CDATA[
SELECT 
	spcl_cgo_irr_tp_cd
,	spcl_cgo_irr_tp_nm
,	spcl_cgo_irr_tp_desc
,	dg_flg
,	awk_flg
,	delt_flg
,	cre_usr_id
,	cre_dt
,	upd_usr_id
,	upd_dt
FROM scg_irr_tp_cd
WHERE	delt_flg = 'N'
#if (${dg_flg} != '') 
AND dg_flg = @[dg_flg]
#end
#if (${awk_flg} != '') 
AND	awk_flg = @[awk_flg]
#end
ORDER BY spcl_cgo_irr_tp_cd			]]></sql>
			<params>
				<param name="dg_flg" type="12" value="" out="N"/>
				<param name="awk_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
