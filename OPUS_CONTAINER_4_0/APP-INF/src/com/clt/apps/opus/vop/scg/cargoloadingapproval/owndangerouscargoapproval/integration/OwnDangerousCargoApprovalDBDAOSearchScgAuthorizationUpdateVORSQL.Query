<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOSearchScgAuthorizationUpdateVORSQL">
			<desc><![CDATA[SCG_AUTHORIZATION에 기존에 같은 조건으로 Update된 Data가 있는지 체크한다.]]></desc>
			<sql><![CDATA[
SELECT 
	BKG_NO
,	SPCL_CGO_APRO_RQST_SEQ
,	VSL_PRE_PST_CD
,	VSL_SEQ
,	SPCL_CGO_AUTH_SEQ
,	SPCL_CGO_CATE_CD
,	DCGO_SEQ
,	RC_SEQ
,	BB_CGO_SEQ
,	AWK_CGO_SEQ
,   STWG_CGO_SEQ
,	RGN_SHP_OPR_CD
,	SPCL_CGO_AUTH_CD
,	AUTH_OFC_CD
,	AUTH_USR_ID
,	AUTH_DT
,	SPCL_CGO_AUTH_NO
,	SPCL_CGO_AUTH_RJCT_CD
,	APRO_REF_NO
,	SPCL_CGO_AUTH_RMK
,	CRE_USR_ID
,	TO_CHAR(CRE_DT, 'YYYYMMDDHH24MI') AS CRE_DT
,	UPD_USR_ID
,	TO_CHAR(UPD_DT, 'YYYYMMDDHH24MI') AS UPD_DT
FROM SCG_AUTHORIZATION
WHERE	BKG_NO = @[bkg_no]
AND	SPCL_CGO_APRO_RQST_SEQ = @[spcl_cgo_apro_rqst_seq]
AND	VSL_PRE_PST_CD = @[vsl_pre_pst_cd]
AND	VSL_SEQ = @[vsl_seq]
AND SPCL_CGO_CATE_CD = @[spcl_cgo_cate_cd]
AND(  
	DCGO_SEQ = @[dcgo_seq]
	OR AWK_CGO_SEQ = @[awk_cgo_seq]
	OR BB_CGO_SEQ = @[bb_cgo_seq]
	OR RC_SEQ = @[rc_seq]
	OR STWG_CGO_SEQ = @[stwg_cgo_seq]
	)
--AND SPCL_CGO_AUTH_CD IS NOT NULL
AND SPCL_CGO_AUTH_CD IN('Y', 'N')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
				<param name="spcl_cgo_cate_cd" type="12" value="" out="N"/>
				<param name="dcgo_seq" type="12" value="" out="N"/>
				<param name="awk_cgo_seq" type="12" value="" out="N"/>
				<param name="bb_cgo_seq" type="12" value="" out="N"/>
				<param name="rc_seq" type="12" value="" out="N"/>
				<param name="stwg_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
