<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChangeOfDestinationMgtDBDAOCODDiversionChargeCalcRSQL">
			<desc><![CDATA[Search DVC]]></desc>
			<sql><![CDATA[
SELECT 'DVC' AS CHG_CD
     , 'USD' AS CURR_CD
     , NVL(O.DVS_FEE_AMT,0) AS CHG_UT_AMT
  FROM MDM_LOCATION M
    ,  OPF_COD_DVS_FEE O
 WHERE 1=1
   AND M.LOC_CD = SUBSTR( @[cod_rhnd_port_cd], 1, 5 )
   AND M.CONTI_CD = O.CONTI_CD
   AND O.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="cod_rhnd_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
