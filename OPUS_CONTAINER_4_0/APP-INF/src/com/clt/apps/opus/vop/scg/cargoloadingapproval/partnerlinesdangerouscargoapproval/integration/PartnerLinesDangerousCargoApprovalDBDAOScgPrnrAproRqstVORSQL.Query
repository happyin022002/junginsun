<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerLinesDangerousCargoApprovalDBDAOScgPrnrAproRqstVORSQL">
			<desc><![CDATA[Dangerous CGO Application Details for Partner Lines 의 Booking 조회]]></desc>
			<sql><![CDATA[
SELECT 
	BKG.CRR_CD
,	BKG.BKG_REF_NO
,	BKG.SPCL_CGO_RQST_SEQ
,	BKG.VSL_CD
,	BKG.SKD_VOY_NO
,	BKG.SKD_DIR_CD
,	BKG.SLAN_CD
,	BKG.RGN_SHP_OPR_CD
,	(BKG.POL_CD||NVL(BKG.POL_CLPT_IND_SEQ,1)) AS POL_CD
,	(BKG.POD_CD||NVL(BKG.POD_CLPT_IND_SEQ,1)) AS POD_CD
--,BKG.POL_CD AS POL_CD
--,BKG.POD_CD AS POD_CD
,	TO_CHAR(BKG.ETA_DT,'YYYY-MM-DD HH24:MI:SS') ETA_DT
,	BKG.DG_FLG
,	BKG.AWK_FLG
,	BKG.CRE_USR_ID
,	BKG.CRE_DT
,	BKG.UPD_USR_ID
,	BKG.UPD_DT
,   '' CGO_OPR_CD
,   BKG.PRNR_CGO_RQST_SEQ
,   (SELECT LISTAGG (EDI_UNMAP_DTL_CD, ',') WITHIN GROUP (ORDER BY BKG_REF_NO)
       FROM SCG_PRNR_RQST_UNMAP
      WHERE CRR_CD = BKG.CRR_CD
        AND BKG_REF_NO = BKG.BKG_REF_NO
        AND SPCL_CGO_RQST_SEQ = BKG.SPCL_CGO_RQST_SEQ
        AND PRNR_CGO_RQST_SEQ = BKG.PRNR_CGO_RQST_SEQ
        AND EDI_UNMAP_CORR_RSLT_CD = 'N'
    ) EDI_UNMAP_DTL_CD
,   TRH.POL_CD AS UNMAP_POL_CD
,   TRH.POD_CD AS UNMAP_POD_CD
FROM SCG_PRNR_APRO_RQST BKG, SCG_PRNR_SPCL_CGO_TRSM_HDR TRH
WHERE 1=1
AND BKG.MAPG_TRSM_BND_CD = TRH.TRSM_BND_CD(+)
AND BKG.MAPG_TRSM_DT = TRH.TRSM_DT(+)
AND BKG.MAPG_TRSM_SPCL_CGO_CATE_CD = TRH.SPCL_CGO_CATE_CD(+)
AND BKG.MAPG_PRNR_SPCL_CGO_SEQ = TRH.PRNR_SPCL_CGO_SEQ(+)
#if (${crr_cd} != '')
AND	BKG.CRR_CD         = COM_ConstantMgr_PKG.COM_getCompanyCode_FNC()
#end
#if (${bkg_ref_no} != '')
AND	BKG.BKG_REF_NO     = @[bkg_ref_no]
#end
#if (${prnr_cgo_rqst_seq} != '')
AND BKG.PRNR_CGO_RQST_SEQ = @[prnr_cgo_rqst_seq]
#end
#if (${spcl_cgo_rqst_seq} != '')
AND	BKG.SPCL_CGO_RQST_SEQ = @[spcl_cgo_rqst_seq]
#else

#if (${rgn_shp_opr_cd} != '')
AND BKG.RGN_SHP_OPR_CD = @[rgn_shp_opr_cd]
#end
#if (${vsl_cd} != '')
AND	BKG.VSL_CD         = @[vsl_cd]
#end
#if (${skd_voy_no} != '')
AND	BKG.SKD_VOY_NO     = @[skd_voy_no]
#end
#if (${skd_dir_cd} != '')
AND	BKG.SKD_DIR_CD     = @[skd_dir_cd]
#end
#if (${pol_cd} != '')
AND	BKG.POL_CD||BKG.POL_CLPT_IND_SEQ = @[pol_cd]
#end
#if (${pod_cd} != '')
AND	BKG.POD_CD||BKG.POD_CLPT_IND_SEQ = @[pod_cd]
#end
AND EXISTS (
	SELECT 'X'
      FROM SCG_PRNR_APRO_RQST_CGO CGO
     WHERE CGO.CRR_CD = BKG.CRR_CD
       AND CGO.BKG_REF_NO = BKG.BKG_REF_NO
	   AND CGO.SPCL_CGO_RQST_SEQ = BKG.SPCL_CGO_RQST_SEQ
#if (${crr_cd} != '')
       AND CGO.CRR_CD = COM_ConstantMgr_PKG.COM_getCompanyCode_FNC()
#end
#if (${bkg_ref_no} != '')
       AND CGO.BKG_REF_NO     = @[bkg_ref_no]
#end
#if (${spcl_cgo_rqst_seq} != '')
       AND CGO.SPCL_CGO_RQST_SEQ = @[spcl_cgo_rqst_seq]
#end
#if (${prnr_cgo_rqst_seq} != '')
       AND CGO.PRNR_CGO_RQST_SEQ = @[prnr_cgo_rqst_seq]
#end
#if (${cgo_opr_cd} != '')
       AND CGO.CGO_OPR_CD = @[cgo_opr_cd]
#end
)
#end			]]></sql>
			<params>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="prnr_cgo_rqst_seq" type="12" value="" out="N"/>
				<param name="spcl_cgo_rqst_seq" type="2" value="" out="N"/>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="cgo_opr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
