<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAORemoveVskNoonRptDSQL">
			<desc><![CDATA[VSK_NOON_RPT <delete By PK>]]></desc>
			<sql><![CDATA[
DELETE 
FROM   VSK_NOON_RPT A
WHERE  1=1
AND    A.VSL_CD      = @[vsl_cd]	
AND    A.SKD_VOY_NO  = @[skd_voy_no]
AND    A.SKD_DIR_CD  = @[skd_dir_cd]	
AND    A.NOON_RPT_DT = TO_DATE(@[noon_rpt_dt], 'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="noon_rpt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
