<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchBkgBdrLogRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT	VSL_CD				AS VSL_CD
		, SKD_VOY_NO		AS SKD_VOY_NO
		, SKD_DIR_CD		AS SKD_DIR_CD
		, VPS_PORT_CD		AS VPS_PORT_CD
		, CLPT_IND_SEQ		AS CLPT_IND_SEQ
		, PORT_SKD_STS_CD	AS PORT_SKD_STS_CD
		, 'N'				AS DEL_FLAG
FROM	VSK_VSL_PORT_SKD T1
WHERE	1 = 1
AND		VSL_CD		= @[vsl_cd]
AND		SKD_VOY_NO	= @[skd_voy_no]
AND		SKD_DIR_CD	= @[skd_dir_cd]
AND		1 <
(
		SELECT 	COUNT(*)
		FROM	VSK_VSL_PORT_SKD
		WHERE	1 = 1
		AND		VSL_CD		= @[vsl_cd]
		AND		SKD_VOY_NO	= @[skd_voy_no]
		AND		SKD_DIR_CD	= @[skd_dir_cd]
		AND		'S'				<> NVL(SKD_CNG_STS_CD, ' ')
		AND		TURN_PORT_IND_CD	NOT IN ('D', 'V', 'F')
)
UNION	ALL
SELECT	@[vsl_cd]
		, @[skd_voy_no]
		, @[skd_dir_cd]
		, NULL
		, NULL
		, NULL	
		, 'Y'
FROM	DUAL
WHERE	1 >=
(
	SELECT 	COUNT(*)
	FROM	VSK_VSL_PORT_SKD
	WHERE	1 = 1
	AND		VSL_CD		= @[vsl_cd]
	AND		SKD_VOY_NO	= @[skd_voy_no]
	AND		SKD_DIR_CD	= @[skd_dir_cd]
	AND		'S'				<> NVL(SKD_CNG_STS_CD, ' ')
	AND		TURN_PORT_IND_CD	NOT IN ('D', 'V', 'F')
)
			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
