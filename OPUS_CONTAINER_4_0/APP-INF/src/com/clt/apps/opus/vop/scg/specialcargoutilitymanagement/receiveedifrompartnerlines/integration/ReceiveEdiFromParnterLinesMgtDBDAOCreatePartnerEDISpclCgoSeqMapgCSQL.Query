<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOCreatePartnerEDISpclCgoSeqMapgCSQL">
			<desc><![CDATA[EDI DG Item SEQ와 Container SEQ와의 매핑정보를 저정한다.]]></desc>
			<sql><![CDATA[
INSERT INTO 	SCG_PRNR_SPCL_CGO_SEQ_MAPG
				(
					PRNR_SPCL_CGO_SEQ
				,	CGO_MAPG_SEQ
				,	CNTR_SEQ
				,	CGO_SEQ
				,	CNTR_TPSZ_CD
				,	CNTR_REF_NO
				,	EDI_ITM_SEQ
				,	DCGO_REF_NO
				,	CRE_USR_ID
				,	CRE_DT
				,	UPD_USR_ID
				,	UPD_DT
				)
VALUES			(
					@[prnr_spcl_cgo_seq]			/* PRNR_SPCL_CGO_SEQ    */
				,	@[cgo_mapg_seq]					/* CGO_MAPG_SEQ         */
				,	@[cntr_seq]         			/* CNTR_SEQ             */
				,	@[cgo_seq]          			/* CGO_SEQ              */
				,	@[cntr_tpsz_cd]     			/* CNTR_TPSZ_CD         */
				,	@[cntr_ref_no]      			/* CNTR_REF_NO          */
				,	@[edi_itm_seq]      			/* EDI_ITM_SEQ          */
				,	@[dcgo_ref_no]      			/* DCGO_REF_NO          */
				,	'EDI'       					/* CRE_USR_ID           */
				,	SYSDATE      					/* CRE_DT               */
				,	'EDI'       					/* UPD_USR_ID           */
				,	SYSDATE           				/* UPD_DT				*/
				)			]]></sql>
			<params>
				<param name="prnr_spcl_cgo_seq" type="12" value="" out="N"/>
				<param name="cgo_mapg_seq" type="12" value="" out="N"/>
				<param name="cntr_seq" type="12" value="" out="N"/>
				<param name="cgo_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_ref_no" type="12" value="" out="N"/>
				<param name="edi_itm_seq" type="12" value="" out="N"/>
				<param name="dcgo_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
