<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToIBISDBDAOVskPfSkdUSQL">
			<desc><![CDATA[VskPfSkd update]]></desc>
			<sql><![CDATA[
UPDATE VSK_PF_SKD_IBIS_IF
   SET IF_SND_CD = 'X'
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT = SYSDATE
 WHERE 1=1
   AND VSL_SLAN_CD = @[vsl_slan_cd]
   AND PF_SVC_TP_CD = @[pf_svc_tp_cd]
   AND IF_SND_CD = 'N'
			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="pf_svc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
