<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StevedoreDamageMgtDBDAOOpfStvDmgAtchFileVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE OPF_STV_DMG_ATCH_FILE SET 
	STV_DMG_NO = @[stv_dmg_no]
,	STV_DMG_PROC_CD = @[stv_dmg_proc_cd]
,	STV_DMG_ATCH_FILE_TP_CD = @[stv_dmg_atch_file_tp_cd]
,	STV_DMG_ATCH_FILE_SEQ = @[stv_dmg_atch_file_seq]
,	VSL_CD = @[vsl_cd]
,	FILE_NM = @[file_nm]
,	FILE_SAV_ID = @[file_sav_id]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE 
WHERE	STV_DMG_NO = @[stv_dmg_no]
AND	STV_DMG_PROC_CD = @[stv_dmg_proc_cd]
AND	STV_DMG_ATCH_FILE_TP_CD = @[stv_dmg_atch_file_tp_cd]
AND STV_DMG_ATCH_FILE_SEQ = @[stv_dmg_atch_file_seq]			]]></sql>
			<params>
				<param name="stv_dmg_no" type="12" value="" out="N"/>
				<param name="stv_dmg_proc_cd" type="12" value="" out="N"/>
				<param name="stv_dmg_atch_file_tp_cd" type="12" value="" out="N"/>
				<param name="stv_dmg_atch_file_seq" type="2" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
