<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAOSearchRdrAllocationRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT   A.VSL_CD
  FROM   RDR_ALLOCATION A
 WHERE   A.VSL_CD =  @[vsl_cd]
   AND   A.VOY_NO =  @[voy_no]
   AND   A.DIR_CD =  @[dir_cd]
   AND   A.REGION =  @[region]
   AND   A.OPR_CD =  @[opr_cd]
   AND   A.SEGMENT = @[segment]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
				<param name="segment" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
