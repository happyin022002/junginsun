<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgSegrGrpVORSQL">
			<desc><![CDATA[Segregation Groups (Creation) 의 Segregation Group 조회   ]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_SEGR_GRP_NO
,	IMDG_SEGR_GRP_NM
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_IMDG_SEGR_GRP
WHERE 1 = 1
#if (${imdg_segr_grp_no} != '')
  AND IMDG_SEGR_GRP_NO = @[imdg_segr_grp_no]
#end
ORDER BY
    IMDG_SEGR_GRP_NO			]]></sql>
			<params>
				<param name="imdg_segr_grp_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
