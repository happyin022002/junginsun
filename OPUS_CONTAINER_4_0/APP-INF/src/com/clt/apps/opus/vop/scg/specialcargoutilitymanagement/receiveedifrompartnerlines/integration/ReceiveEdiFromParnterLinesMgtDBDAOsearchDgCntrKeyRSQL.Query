<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOsearchDgCntrKeyRSQL">
			<desc><![CDATA[searchDgCgoCnt]]></desc>
			<sql><![CDATA[
SELECT CRR_CD
     , BKG_REF_NO
     , SPCL_CGO_RQST_SEQ
  FROM SCG_PRNR_APRO_RQST
 WHERE MAPG_TRSM_BND_CD           = @[mapg_trsm_bnd_cd]
   AND MAPG_TRSM_DT               = TO_DATE(@[mapg_trsm_dt],'yyyy-MM-dd')
   AND MAPG_PRNR_SPCL_CGO_SEQ     = @[mapg_prnr_spcl_cgo_seq]
   AND MAPG_TRSM_SPCL_CGO_CATE_CD = @[mapg_trsm_cgo_cate_cd]			]]></sql>
			<params>
				<param name="mapg_trsm_bnd_cd" type="12" value="" out="N"/>
				<param name="mapg_trsm_dt" type="12" value="" out="N"/>
				<param name="mapg_prnr_spcl_cgo_seq" type="12" value="" out="N"/>
				<param name="mapg_trsm_cgo_cate_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
