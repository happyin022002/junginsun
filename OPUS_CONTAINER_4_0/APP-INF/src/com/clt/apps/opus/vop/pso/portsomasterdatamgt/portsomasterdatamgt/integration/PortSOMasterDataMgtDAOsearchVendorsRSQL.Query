<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortSOMasterDataMgtDAOsearchVendorsRSQL">
			<desc><![CDATA[vendor 조회]]></desc>
			<sql><![CDATA[
SELECT  VNDR_SEQ, VNDR_LGL_ENG_NM ,ofc_cd
FROM    MDM_VENDOR 
WHERE   1 = 1
  AND     VNDR_SEQ    = @[vndr_seq]
ORDER BY VNDR_SEQ			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
