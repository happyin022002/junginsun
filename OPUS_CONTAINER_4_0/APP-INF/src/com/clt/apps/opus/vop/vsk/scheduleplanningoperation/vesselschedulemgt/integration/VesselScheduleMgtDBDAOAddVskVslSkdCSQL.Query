<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOAddVskVslSkdCSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
INSERT INTO VSK_VSL_SKD 
			(
			VSL_CD
		,	SKD_VOY_NO
		,	SKD_DIR_CD
		,	VSL_SLAN_CD
		,	SKD_STS_CD
		,	SKD_VOY_TP_CD
		,	SKD_USD_IND_CD
		,	PF_SKD_TP_CD
		,	ST_PORT_CD
		,	N1ST_PORT_BRTH_DT
		,	PSDO_VVD_CD
		,	CO_CD
		,	SKD_RMK
		,	CRE_USR_ID
		,	CRE_DT
		,	UPD_USR_ID
		,	UPD_DT
			) 

SELECT 		@[vsl_cd]
	   	, 	@[skd_voy_no]
	   	, 	@[skd_dir_cd]
	   	, 	@[vsl_slan_cd]
	   	, 	@[skd_sts_cd]

	   	--::2015-06-30::--, 	T3.INTG_CD_VAL_DP_DESC AS SKD_VOY_TP_CD
		,	(SELECT 	CD.INTG_CD_VAL_DP_DESC 
			 FROM 		COM_INTG_CD_DTL 	CD
			 WHERE 		CD.INTG_CD_ID			= 'CD20069'
			 AND		CD.INTG_CD_VAL_CTNT	= T1.VSL_SVC_TP_CD || DECODE(T2.VSL_OWN_IND_CD,'O','O','C') || DECODE(T2.CRR_CD,COM_CONSTANTMGR_PKG.COM_getCompanyCode_FNC(),'O','X') 
			)			AS SKD_VOY_TP_CD
	   	, 	@[skd_usd_ind_cd]
	   	, 	@[pf_skd_tp_cd]
	   	, 	@[st_port_cd]
	   	, 	TO_DATE(@[n1st_port_brth_dt], 'YYYYMMDDHH24MI')
	   	, 	@[psdo_vvd_cd]
	   	, 	@[co_cd]
	   	, 	@[skd_rmk]
	   	, 	@[cre_usr_id]
	   	, 	SYSDATE
       	, 	@[upd_usr_id]
	   	, 	SYSDATE
FROM   		MDM_VSL_SVC_LANE 				T1
		,	MDM_VSL_CNTR 					T2
WHERE  		1 = 1
AND    		T1.VSL_SLAN_CD   				= @[vsl_slan_cd]
AND    		T2.VSL_CD        				= @[vsl_cd]
AND    		T1.VSL_TP_CD     				= 'C' 				/* Container */
 			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="skd_sts_cd" type="12" value="" out="N"/>
				<param name="skd_usd_ind_cd" type="12" value="" out="N"/>
				<param name="pf_skd_tp_cd" type="12" value="" out="N"/>
				<param name="st_port_cd" type="12" value="" out="N"/>
				<param name="n1st_port_brth_dt" type="12" value="" out="N"/>
				<param name="psdo_vvd_cd" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
				<param name="skd_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
