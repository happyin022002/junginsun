<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchSimNoRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 
	TO_CHAR(SYSDATE, 'YYYYMMDD') AS SIM_DT
,	(NVL(MAX(TO_NUMBER(SIM_NO)), 0) + 1) AS SIM_NO
FROM VSK_SWAP_CST_SIM
WHERE	SIM_DT = TRUNC(SYSDATE)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
