<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetRegularValueByObjectNoRSQL">
			<desc><![CDATA[Object No of Reqular Value Search]]></desc>
			<sql><![CDATA[
SELECT T2.OBJ_LIST_NO
     , TO_CHAR(T1.DFLT_VAL) AS DFLT_VAL
  FROM PSO_YD_CHG_OBJ_LIST T1
     , PSO_OBJ_LIST T2
 WHERE T1.OBJ_LIST_NO       (+)= T2.OBJ_LIST_NO
   AND T1.YD_CHG_NO         (+)= @[yd_chg_no]
   AND T1.YD_CHG_VER_SEQ    (+)= @[yd_chg_ver_seq]
   AND T2.PSO_MEAS_UT_CD    <> 12
   AND T2.OBJ_LIST_NO IN (
#foreach($key IN ${obj_list_no}) #if($velocityCount < $obj_list_no.size()) '$key', #else '$key' #end #end
       )
 UNION
SELECT T2.OBJ_LIST_NO
     , ''''||NVL(T1.DFLT_FLG,'N')||'''' AS DFLT_VAL
  FROM PSO_YD_CHG_OBJ_LIST T1
     , PSO_OBJ_LIST T2
 WHERE T1.OBJ_LIST_NO       (+)= T2.OBJ_LIST_NO
   AND T1.YD_CHG_NO         (+)= @[yd_chg_no]
   AND T1.YD_CHG_VER_SEQ    (+)= @[yd_chg_ver_seq]
   AND T2.PSO_MEAS_UT_CD     = 12
   AND T2.OBJ_LIST_NO IN (
#foreach($key IN ${obj_list_no2}) #if($velocityCount < $obj_list_no2.size()) '$key', #else '$key' #end #end
       )			]]></sql>
			<params>
				<param name="yd_chg_no" type="12" value="" out="N"/>
				<param name="yd_chg_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
