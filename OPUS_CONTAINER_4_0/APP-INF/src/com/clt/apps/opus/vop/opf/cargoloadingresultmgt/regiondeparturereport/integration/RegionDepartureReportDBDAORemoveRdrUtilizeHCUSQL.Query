<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAORemoveRdrUtilizeHCUSQL">
			<desc><![CDATA[DESC Enter..
]]></desc>
			<sql><![CDATA[
UPDATE RDR_UTILIZE
  SET  
       SLOT_QTY   = NULL
,      UPDATE_USER  = @[update_user]
,      UPDATE_TIME  = SYSDATE
,      SLOT_ADD     = NULL
WHERE
      VSL_CD =   @[vsl_cd]
  AND VOY_NO =   @[voy_no]
  AND DIR_CD =   @[dir_cd]
  AND REGION =   @[region]
  AND OPR_CD =   @[opr_cd]
  AND TYPE  IN ('3','H','L')			]]></sql>
			<params>
				<param name="update_user" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
