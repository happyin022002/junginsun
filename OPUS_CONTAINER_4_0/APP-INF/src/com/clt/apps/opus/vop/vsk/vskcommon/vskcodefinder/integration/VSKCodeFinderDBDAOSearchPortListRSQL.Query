<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOSearchPortListRSQL">
			<desc><![CDATA[VVD로 PORT조회]]></desc>
			<sql><![CDATA[
SELECT VSL_CD         AS VSL_CD
     , SKD_VOY_NO     AS SKD_VOY_NO
     , SKD_DIR_CD     AS SKD_DIR_CD
     , SLAN_CD        AS VSL_SLAN_CD
     , VPS_PORT_CD    AS VPS_PORT_CD
     , IB_CSSM_VOY_NO AS IB_CSSM_VOY_NO
     , OB_CSSM_VOY_NO AS OB_CSSM_VOY_NO
  FROM VSK_VSL_PORT_SKD
 WHERE VSL_CD     	 = @[vsl_cd]
   AND SKD_VOY_NO 	 = @[skd_voy_no]
   AND SKD_DIR_CD 	 = @[skd_dir_cd]
   #if (${vps_port_cd} != '') 
   AND VPS_PORT_CD = @[vps_port_cd]
   #end
   AND CLPT_IND_SEQ  = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
