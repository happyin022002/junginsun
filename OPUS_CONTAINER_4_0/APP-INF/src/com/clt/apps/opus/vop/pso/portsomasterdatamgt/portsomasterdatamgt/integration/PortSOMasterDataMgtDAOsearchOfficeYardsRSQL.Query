<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortSOMasterDataMgtDAOsearchOfficeYardsRSQL">
			<desc><![CDATA[searchOfficeYards]]></desc>
			<sql><![CDATA[
/* PsoInvOfcYd VO*/
SELECT T1.YD_CD YD_CD1
     ,T2.YD_NM YD_CD2
     ,T4.AP_OFC_CD OFC_CD
     ,T4.AR_CURR_CD CURR_CD
     ,T1.CRE_USR_ID
     ,DECODE(T5.PORT_CD,NULL,'N','Y') AS CANAL_FLAG
  FROM PSO_INV_OFC_YD T1
     , MDM_YARD T2
     , MDM_ORGANIZATION T4
     ,(SELECT INTG_CD_VAL_CTNT PORT_CD
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID='CD20074') T5
 WHERE T1.OFC_CD = @[ofc_cd]
   AND T1.YD_CD = T2.YD_CD
   AND T2.OFC_CD = T4.OFC_CD
   AND T4.DELT_FLG = 'N'
   AND T2.DELT_FLG = 'N'
   AND SUBSTR(T1.YD_CD, 1,5) = T5.PORT_CD(+)			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
