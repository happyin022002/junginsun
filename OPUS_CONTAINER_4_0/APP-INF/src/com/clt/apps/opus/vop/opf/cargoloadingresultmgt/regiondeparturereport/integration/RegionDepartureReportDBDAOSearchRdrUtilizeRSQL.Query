<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAOSearchRdrUtilizeRSQL">
			<desc><![CDATA[RDR UTILIZE 존재여부 확인]]></desc>
			<sql><![CDATA[
SELECT    U.VSL_CD
  FROM    RDR_UTILIZE  U
 WHERE    U.VSL_CD  = @[vsl_cd]
  AND     U.VOY_NO  = @[voy_no]
  AND     U.DIR_CD  = @[dir_cd]
  AND     U.REGION  = @[region]
  AND     U.OPR_CD  = @[opr_cd]
  AND     ROWNUM =1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
