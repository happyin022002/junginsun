<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOCheckDGPackagingEffectivenessRSQL">
			<desc><![CDATA[DG Packaging Code 유효성 체크]]></desc>
			<sql><![CDATA[
SELECT 		IMDG_PCK_CD
		,	IMDG_PCK_DESC
		,	IMDG_PCK_TP_CD
		,	DELT_FLG
		,	CRE_USR_ID
		,	CRE_DT
		,	UPD_USR_ID
		,	UPD_DT
FROM 		SCG_IMDG_PCK_CD
WHERE 		IMDG_PCK_CD 			= @[imdg_pck_cd]

#if (${imdg_pck_tp_cd} =='O' )
AND 		IMDG_PCK_TP_CD 	= 'O'
#elseif(${imdg_pck_tp_cd} =='M' )
AND 		IMDG_PCK_TP_CD 	= 'M'
#else
AND 		IMDG_PCK_TP_CD 	= 'I'
#end

AND 		DELT_FLG 		= 'N'
ORDER BY 	IMDG_PCK_CD			]]></sql>
			<params>
				<param name="imdg_pck_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
