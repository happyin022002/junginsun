<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToIBISDBDAOVskPfSkdCSQL">
			<desc><![CDATA[..]]></desc>
			<sql><![CDATA[
#if (${if_mnpl_cd} == '') 
INSERT INTO VSK_PF_SKD_IBIS_IF
(
      VSL_SLAN_CD
    , PF_SVC_TP_CD
    , IBIS_IF_SEQ
    , IF_SND_CD
    , IF_MNPL_CD
    , SLAN_STND_FLG
    , SVC_DUR_DYS
    , STND_SVC_SPD
    , BRTH_ITVAL_DYS
    , MML_USD_FLG
    , SIM_DT
    , SIM_NO
    , N1ST_VSL_CLSS_CD
    , N1ST_VSL_CLSS_KNT
    , N2ND_VSL_CLSS_CD
    , N2ND_VSL_CLSS_KNT
    , N3RD_VSL_CLSS_CD
    , N3RD_VSL_CLSS_KNT
    , CLPT_KNT
    , TTL_DIST
    , MAX_SPD
    , AVG_SPD
    , DELT_FLG
    , PF_SKD_RMK
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
) VALUES (
      @[vsl_slan_cd]
    , @[pf_svc_tp_cd]
    , @[ibis_if_seq]
    , NVL((SELECT /*+ INDEX_DESC(A XPKVSK_PF_SKD_IBIS_IF) */ 'N'
            FROM VSK_PF_SKD_IBIS_IF A WHERE VSL_SLAN_CD=@[vsl_slan_cd] AND PF_SVC_TP_CD=@[pf_svc_tp_cd] AND IF_SND_CD='Y' AND ROWNUM=1) ,'X')
    , 'D'
    , @[slan_stnd_flg]
    , @[svc_dur_dys]
    , @[stnd_svc_spd]
    , @[brth_itval_dys]
    , @[mml_usd_flg]
    , @[sim_dt]
    , @[sim_no]
    , @[n1st_vsl_clss_cd]
    , @[n1st_vsl_clss_knt]
    , @[n2nd_vsl_clss_cd]
    , @[n2nd_vsl_clss_knt]
    , @[n3rd_vsl_clss_cd]
    , @[n3rd_vsl_clss_knt]
    , @[clpt_knt]
    , @[ttl_dist]
    , @[max_spd]
    , @[avg_spd]
    , @[delt_flg]
    , @[pf_skd_rmk]
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
)
#else
INSERT INTO VSK_PF_SKD_IBIS_IF
(
      VSL_SLAN_CD
    , PF_SVC_TP_CD
    , IBIS_IF_SEQ
    , IF_SND_CD
    , IF_MNPL_CD
    , SLAN_STND_FLG
    , SVC_DUR_DYS
    , STND_SVC_SPD
    , BRTH_ITVAL_DYS
    , MML_USD_FLG
    , SIM_DT
    , SIM_NO
    , N1ST_VSL_CLSS_CD
    , N1ST_VSL_CLSS_KNT
    , N2ND_VSL_CLSS_CD
    , N2ND_VSL_CLSS_KNT
    , N3RD_VSL_CLSS_CD
    , N3RD_VSL_CLSS_KNT
    , CLPT_KNT
    , TTL_DIST
    , MAX_SPD
    , AVG_SPD
    , DELT_FLG
    , PF_SKD_RMK
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
) 
SELECT
      VSL_SLAN_CD
    , PF_SVC_TP_CD
    , @[ibis_if_seq]
    , 'N'
    , 'I'
    , SLAN_STND_FLG
    , SVC_DUR_DYS
    , STND_SVC_SPD
    , BRTH_ITVAL_DYS
    , MML_USD_FLG
    , SIM_DT
    , SIM_NO
    , N1ST_VSL_CLSS_CD
    , N1ST_VSL_CLSS_KNT
    , N2ND_VSL_CLSS_CD
    , N2ND_VSL_CLSS_KNT
    , N3RD_VSL_CLSS_CD
    , N3RD_VSL_CLSS_KNT
    , CLPT_KNT
    , TTL_DIST
    , MAX_SPD
    , AVG_SPD
    , DELT_FLG
    , PF_SKD_RMK
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
 FROM VSK_PF_SKD
WHERE 1=1
  AND VSL_SLAN_CD = @[vsl_slan_cd]
  AND PF_SVC_TP_CD = @[pf_svc_tp_cd]
#end			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="pf_svc_tp_cd" type="12" value="" out="N"/>
				<param name="ibis_if_seq" type="12" value="" out="N"/>
				<param name="slan_stnd_flg" type="12" value="" out="N"/>
				<param name="svc_dur_dys" type="12" value="" out="N"/>
				<param name="stnd_svc_spd" type="12" value="" out="N"/>
				<param name="brth_itval_dys" type="12" value="" out="N"/>
				<param name="mml_usd_flg" type="12" value="" out="N"/>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="n1st_vsl_clss_cd" type="12" value="" out="N"/>
				<param name="n1st_vsl_clss_knt" type="12" value="" out="N"/>
				<param name="n2nd_vsl_clss_cd" type="12" value="" out="N"/>
				<param name="n2nd_vsl_clss_knt" type="12" value="" out="N"/>
				<param name="n3rd_vsl_clss_cd" type="12" value="" out="N"/>
				<param name="n3rd_vsl_clss_knt" type="12" value="" out="N"/>
				<param name="clpt_knt" type="12" value="" out="N"/>
				<param name="ttl_dist" type="12" value="" out="N"/>
				<param name="max_spd" type="12" value="" out="N"/>
				<param name="avg_spd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="pf_skd_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
