<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveOwnBkgCancelRequestMgtDBDAOModifyScgVvdAproRqstCSQL">
			<desc><![CDATA[AddScgVvdDgCgoCxlRqst]]></desc>
			<sql><![CDATA[
UPDATE SCG_VVD_APRO_RQST
	SET DCGO_CXL_RQST_SEQ       = @[dcgo_cxl_rqst_seq]
      , CXL_CGO_KND_CD          = @[cxl_cgo_knd_cd]
      , CXL_CGO_RQST_DT         = DECODE(@[cxl_cgo_rqst_dt], NULL, SYSDATE , TO_DATE(@[cxl_cgo_rqst_dt],'YYYYMMDD HH24MISS'))
      , CXL_CGO_RSN             = @[cxl_cgo_rsn]
      , UPD_USR_ID              = @[upd_usr_id]
      , UPD_DT                  = SYSDATE
  WHERE BKG_NO                  = @[bkg_no]
    AND SPCL_CGO_APRO_RQST_SEQ  = @[spcl_cgo_apro_rqst_seq]
    AND VSL_PRE_PST_CD          = @[vsl_pre_pst_cd]
    AND VSL_SEQ                 = @[vsl_seq]
	AND	MAPG_EDI_TRSM_STS_CD	= 'S'	-- ::EDI SEND SUCCESS:: --			]]></sql>
			<params>
				<param name="dcgo_cxl_rqst_seq" type="12" value="" out="N"/>
				<param name="cxl_cgo_knd_cd" type="12" value="" out="N"/>
				<param name="cxl_cgo_rqst_dt" type="12" value="" out="N"/>
				<param name="cxl_cgo_rsn" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
