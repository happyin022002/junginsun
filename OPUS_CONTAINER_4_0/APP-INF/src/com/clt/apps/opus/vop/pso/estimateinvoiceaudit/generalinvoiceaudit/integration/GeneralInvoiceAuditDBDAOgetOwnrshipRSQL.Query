<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetOwnrshipRSQL">
			<desc><![CDATA[해당 Vessel의 Owner가 자사이면 Y, Charter이면 N을 리턴한다.]]></desc>
			<sql><![CDATA[
SELECT distinct ''''||DECODE(VSL_OWN_IND_CD,'O','Y','N')||''''   -- UOM 이 Flag 이면 ' ' 를 붙여 준다
FROM MDM_VSL_CNTR
WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
