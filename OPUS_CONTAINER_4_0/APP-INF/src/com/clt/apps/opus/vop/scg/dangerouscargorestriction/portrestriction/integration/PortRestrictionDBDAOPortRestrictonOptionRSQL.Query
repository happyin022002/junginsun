<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortRestrictionDBDAOPortRestrictonOptionRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 
        '' OPTCLASS,
        A.PORT_CD                       ,
        (SELECT   E.LOC_NM
          FROM    MDM_LOCATION E WHERE E.LOC_CD = A.PORT_CD )PORT_CD_NM,
        A.IMDG_PORT_RSTR_SEQ            ,
        A.IMDG_PORT_RSTR_EXPT_FLG       ,
        A.IMDG_UN_NO                    ,
        A.IMDG_UN_NO_SEQ                ,
        A.IMDG_CLSS_CD                  ,
        A.PROHI_LOD_FLG,
        A.PROHI_DCHG_FLG                ,
        A.PROHI_TS_FLG                  ,
        A.PROHI_PASS_FLG                ,
        A.PROHI_DY_TM_OP_FLG            ,
        A.PROHI_DY_TM_INLND_TZ_FLG      ,
        A.PROHI_PORT_FLG                ,
        A.PROHI_PINSP_FLG               ,
        A.XTRA_HNDL_CHG_FLG             ,
        A.SFT_GAD_FLG                   ,
        A.KEP_SFT_DIST_IHB_FLG          ,
        A.KEP_SFT_DIST_IHB_DIST         ,
        A.RSTR_RMK                      ,
        A.PROHI_NGT_FLG                 ,
        A.DIR_LOD_FLG                   ,
        A.DIR_DCHG_FLG                  ,
        A.DIR_TS_FLG                    ,
        A.CRE_USR_ID                    ,
        A.CRE_DT                        ,
        A.UPD_USR_ID                    ,
        A.UPD_DT  ,
        TO_CHAR( A.UPD_DT,'YYYY-MM-DD HH24:MI') UPD_DT_F,
        '' STO_DYS   ,
        '' LOAD_IMDG_CMPTN_AUTH_CD,
        '' DIS_IMDG_CMPTN_AUTH_CD,
        ''  TS_IMDG_CMPTN_AUTH_CD,
        ''  PASS_IMDG_CMPTN_AUTH_CD,
	    '' LOAD_STO_DYS ,
        '' DIS_STO_DYS ,
        '' TS_STO_DYS  ,
        '' LOAD_DYS_STO_FLG , 
        '' DIS_DYS_STO_FLG  ,
        '' TS_DYS_STO_FLG  ,
        '1'     sav_type_class_flag,
        'Class' sav_type_class_label,
        '1'     sav_type_unno_flag,
        'UN No.' sav_type_unno_label,
        ''imdg_clss_cd_txt
FROM    SCG_IMDG_PORT_RSTR A
WHERE
#if (${port_cd} != '' ) 
     A.PORT_CD             =  @[port_cd]
#end
#if ( ${imdg_clss_cd} != ''  ) 
  AND   A.IMDG_CLSS_CD  =  @[imdg_clss_cd]
#end
#if ( ${imdg_un_no} != ''  ) 
  AND   IMDG_UN_NO  =  @[imdg_un_no]
#end
#if ( ${imdg_un_no_seq} != ''  ) 
  AND   IMDG_UN_NO_SEQ  =  @[imdg_un_no_seq]
#end			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
