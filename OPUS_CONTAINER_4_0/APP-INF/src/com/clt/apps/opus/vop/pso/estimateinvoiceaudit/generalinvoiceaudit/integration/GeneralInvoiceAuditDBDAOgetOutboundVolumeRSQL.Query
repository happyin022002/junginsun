<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetOutboundVolumeRSQL">
			<desc><![CDATA[getOutboundVolume
[2015.07.21]Virtual Add Calling 처리. VSK_VSL_PORT_SKD.NVL(VT_ADD_CALL_FLG, 'N') = 'N'
[2015.09.09]TON > TEU(D2외 사이즈 카운터 *2) ]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(DECODE(S.CNTR_SIZE,'2', S.QTY, S.QTY*2)),0)
  FROM VSK_VSL_PORT_SKD V
     , TDR_HEADER H
     , TDR_SUMMARY S
 WHERE V.VSL_CD                     = SUBSTR(@[vvd], 1, 4)
   AND V.SKD_VOY_NO                 = SUBSTR(@[vvd], 5, 4)
   AND V.SKD_DIR_CD                 = SUBSTR(@[vvd], 9)
   AND V.YD_CD                      = @[yd_cd]
   AND V.CLPT_IND_SEQ               = @[clpt_ind_seq]
   AND NVL(V.VT_ADD_CALL_FLG, 'N')  = 'N' /*2015.07.21 Add*/
   AND V.VSL_CD                     = H.VSL_CD
   AND V.SKD_VOY_NO                 = H.VOY_NO
   AND V.SKD_DIR_CD                 = H.DIR_CD
   AND V.VPS_PORT_CD                = H.PORT_CD
   AND V.CLPT_IND_SEQ               = H.CALL_IND
   AND H.VSL_CD                     = S.VSL_CD
   AND H.VOY_NO                     = S.VOY_NO
   AND H.DIR_CD                     = S.DIR_CD
   AND H.PORT_CD                    = S.PORT_CD
   AND H.CALL_IND                   = S.CALL_IND
   AND S.STATUS IN ('LM','OT') -- Only Ocean Volume			]]></sql>
			<params>
				<param name="vvd" type="12" value="ACKT0024S" out="N"/>
				<param name="yd_cd" type="12" value="KRPUS01" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
