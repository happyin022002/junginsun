<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSOCodeFinderDBDAOsearchAccountListRSQL">
			<desc><![CDATA[account 조회
[2015.10.13]962111 OWNERS ACCOUNT INTER-OFFICE ACCT 주석처리.]]></desc>
			<sql><![CDATA[

#if (${ofc_cd} != '') 
SELECT DISTINCT T1.ACCT_CD
     , T2.ACCT_ENG_NM
     --, T1.LGS_COST_SUBJ_CD
  FROM TES_LGS_COST T1
     , MDM_ACCOUNT T2
     , PSO_INV_OFC_COST T3
 WHERE 1=1
   AND T1.LGS_COST_SUBJ_CD IN ('PT', 'CN')
   AND T1.LGS_COST_CD_CLSS_LVL = 'A'
   AND T1.ACCT_CD = T2.ACCT_CD
   AND T2.DELT_FLG = 'N'
   AND T1.LGS_COST_CD = T3.LGS_COST_CD
   AND T1.ACCT_CD NOT IN ( '110911')
#else 
SELECT DISTINCT T1.ACCT_CD
     , T2.ACCT_ENG_NM
     --, T1.LGS_COST_SUBJ_CD
  FROM TES_LGS_COST T1
     , MDM_ACCOUNT T2
 WHERE T1.LGS_COST_SUBJ_CD IN ('PT', 'CN')
   AND T1.LGS_COST_CD_CLSS_LVL = 'A'
   AND T2.DELT_FLG = 'N'
   AND T1.ACCT_CD = T2.ACCT_CD
   AND T1.ACCT_CD NOT IN ( '110911')
#end
 ORDER BY T1.ACCT_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
