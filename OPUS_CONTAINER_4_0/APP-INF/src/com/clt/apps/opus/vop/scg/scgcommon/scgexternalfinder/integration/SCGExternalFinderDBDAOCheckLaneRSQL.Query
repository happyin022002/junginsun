<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGExternalFinderDBDAOCheckLaneRSQL">
			<desc><![CDATA[checkLane 조회]]></desc>
			<sql><![CDATA[
SELECT 
	VSL_SLAN_CD
,	VSL_SLAN_NM
,	VSL_SVC_TP_CD
,	VSL_TP_CD
,	ST_EFF_DT
,	END_EFF_DT
,	VSL_SLAN_SKD_TP_CD
,	OFC_CD
,	CO_CD
,	FDR_DIV_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	DELT_FLG
,	EAI_EVNT_DT
,	CNL_AGN_VNDR_SEQ
,	VSKD_FLET_GRP_CD
,	SPCL_CGO_RQST_TGT_LANE_FLG
,	TML_PROD_RPT_FLG
,	PNDLM_FLG
FROM MDM_VSL_SVC_LANE
WHERE 1=1
#if (${vsl_slan_cd} != '') 
 AND  VSL_SLAN_CD = @[vsl_slan_cd]
#end
 AND DELT_FLG = 'N'
ORDER BY VSL_SLAN_CD			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="aa" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
