<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToExternalScnDBDAOCheckTxTypeforUpdateScnInterfaceRSQL">
			<desc><![CDATA[SCN I/F TX TYPE]]></desc>
			<sql><![CDATA[
SELECT  F.VSL_CD
      , F.SKD_VOY_NO
      , F.SKD_DIR_CD
      , F.VPS_PORT_CD
      , F.CSSM_VOY_NO_IF_SEQ 
       
      , F.INSF_CNQE_VAL         -- 'S':SUCCESS, NULL:INITIAL --
      , F.INSF_DV_CD            -- 'I':INSERT, 'U':UPDATE, 'D':DELETE --
      
      , CASE WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'I' THEN NULL
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'U' THEN NULL
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'D' THEN 'X'        
             WHEN F.INSF_CNQE_VAL IS NULL                        THEN 'ROW_DELETE'
        END                     NEW_INSF_CNQE_VAL
      , CASE WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'I' THEN 'D'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'U' THEN 'D'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'D' THEN 'X'        
             WHEN F.INSF_CNQE_VAL IS NULL                        THEN 'ROW_DELETE'
        END                     NEW_INSF_DV_CD      
        
FROM    (
        SELECT   *
        FROM     (
                  SELECT   RANK() OVER (PARTITION BY PS.VSL_CD,PS.SKD_VOY_NO,PS.SKD_DIR_CD,PS.VPS_PORT_CD ORDER BY DECODE(PS.SKD_CNG_STS_CD,'S',9,1) ASC, PS.CLPT_IND_SEQ ASC) RNK
                        ,  PS.* 
                  FROM     VSK_VSL_PORT_SKD        PS
                  WHERE    PS.VSL_CD               = @[vsl_cd]
                  AND      PS.SKD_VOY_NO           = @[skd_voy_no]
                  AND      PS.SKD_DIR_CD           = @[skd_dir_cd]
				  AND	   NVL(PS.VT_ADD_CALL_FLG,'N') = 'N'
                  )
        WHERE     RNK      = 1
        )         PS
     ,  VSK_VSL_CSSM_VOY_NO_IF  F

WHERE   1 = 1
AND     PS.VSL_CD             	= F.VSL_CD
AND     PS.SKD_VOY_NO           = F.SKD_VOY_NO
AND     PS.SKD_DIR_CD           = F.SKD_DIR_CD
AND     PS.VPS_PORT_CD          = F.VPS_PORT_CD
AND     PS.VSL_CD               = @[vsl_cd]
AND     PS.SKD_VOY_NO           = @[skd_voy_no]
AND     PS.SKD_DIR_CD           = @[skd_dir_cd]
AND     F.CSSM_VOY_NO_IF_SEQ    = (SELECT   MAX(FF.CSSM_VOY_NO_IF_SEQ)
                                   FROM     VSK_VSL_CSSM_VOY_NO_IF  FF
                                   WHERE    FF.VSL_CD               = F.VSL_CD
                                   AND      FF.SKD_VOY_NO           = F.SKD_VOY_NO
                                   AND      FF.SKD_DIR_CD           = F.SKD_DIR_CD
                                   AND      FF.VPS_PORT_CD          = F.VPS_PORT_CD
                                   )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
