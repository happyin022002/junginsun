<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOSearchContiCdforBKGRSQL">
			<desc><![CDATA[특정BKG의 POL에 대한 Continent 추출]]></desc>
			<sql><![CDATA[
SELECT   CASE WHEN ML.CONTI_CD IN ('F','E') THEN 'E'
              ELSE ML.CONTI_CD
         END  AS CONTI_CD
FROM     BKG_BOOKING   		B
      ,  MDM_LOCATION  		ML
WHERE    B.POL_CD      		= ML.LOC_CD
AND      B.BKG_NO      		= @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
