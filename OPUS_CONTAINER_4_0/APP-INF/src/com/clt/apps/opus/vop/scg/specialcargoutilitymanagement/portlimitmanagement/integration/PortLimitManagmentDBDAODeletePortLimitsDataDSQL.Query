<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortLimitManagmentDBDAODeletePortLimitsDataDSQL">
			<desc><![CDATA[선택한 Port & Lane && IMDG 정보로 해당 Data를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM SCG_IMDG_PORT_LMT_MST
WHERE PORT_CD = @[port_cd]
#if (${lmt_wgt_tp_cd} != '')
  AND LMT_WGT_TP_CD = @[lmt_wgt_tp_cd]
#else
  AND LMT_WGT_TP_CD IS NULL
#end
  AND PORT_LMT_SEQ = @[port_lmt_seq]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="lmt_wgt_tp_cd" type="12" value="" out="N"/>
				<param name="port_lmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
