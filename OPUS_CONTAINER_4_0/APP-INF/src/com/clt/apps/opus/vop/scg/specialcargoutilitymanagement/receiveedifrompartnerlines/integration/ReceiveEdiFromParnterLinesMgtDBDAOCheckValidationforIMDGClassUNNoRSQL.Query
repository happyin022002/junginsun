<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOCheckValidationforIMDGClassUNNoRSQL">
			<desc><![CDATA[IMDG CLASS와 UN NO 유효성체크]]></desc>
			<sql><![CDATA[
SELECT 	  	X.IMDG_CLSS_CD
FROM      	SCG_IMDG_UN_NO    						X
WHERE     	CASE 	WHEN @[cfr_flg] = 'Y' THEN '*'
					ELSE X.IMDG_AMDT_NO
			END										= CASE	WHEN @[cfr_flg] = 'Y' THEN '*'
															ELSE @[imdg_amdt_no]
													  END
#if(${cfr_flg} == 'Y')

AND       X.IMDG_UN_NO      = @[cfr_no]
AND		  X.CFR_FLG			= 'Y'

#else

AND       X.IMDG_CLSS_CD    = @[imdg_clss_cd_ctnt]
AND       X.IMDG_UN_NO      = @[imdg_un_no_ctnt]

#end

			]]></sql>
			<params>
				<param name="cfr_flg" type="12" value="" out="N"/>
				<param name="imdg_amdt_no" type="12" value="" out="N"/>
				<param name="cfr_no" type="12" value="" out="N"/>
				<param name="imdg_clss_cd_ctnt" type="12" value="" out="N"/>
				<param name="imdg_un_no_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
