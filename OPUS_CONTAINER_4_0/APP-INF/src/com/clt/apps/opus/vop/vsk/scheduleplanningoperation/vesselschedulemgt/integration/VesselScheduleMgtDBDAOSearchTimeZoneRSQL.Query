<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchTimeZoneRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(GMT_HRS/60.0, '999,990.0') AS TIME_DIFF
FROM MDM_LOCATION
WHERE LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
