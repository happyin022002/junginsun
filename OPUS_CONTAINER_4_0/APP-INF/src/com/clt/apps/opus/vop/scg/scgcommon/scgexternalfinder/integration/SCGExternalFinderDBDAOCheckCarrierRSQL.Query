<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGExternalFinderDBDAOCheckCarrierRSQL">
			<desc><![CDATA[checkCarrier 체크]]></desc>
			<sql><![CDATA[
SELECT 
	CRR_CD
,	CRR_NM
,	BKG_EDI_CD
,	MAPG_CRR_CD
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	EAI_EVNT_DT
FROM MDM_CARRIER
WHERE	CRR_CD = @[crr_cd]
AND	DELT_FLG = 'N'
			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
