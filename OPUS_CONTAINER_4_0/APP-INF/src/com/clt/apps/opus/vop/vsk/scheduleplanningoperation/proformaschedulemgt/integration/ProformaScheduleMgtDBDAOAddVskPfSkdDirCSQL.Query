<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOAddVskPfSkdDirCSQL">
			<desc><![CDATA[VSK_PF_SKD_DIR 테이블에 데이터 생성]]></desc>
			<sql><![CDATA[
INSERT INTO	VSK_PF_SKD_DIR
          	(	VSL_SLAN_CD
            ,  	PF_SVC_TP_CD
            ,  	VSL_SLAN_DIR_CD
            ,  	VSL_SLAN_DIR_SEQ
            ,  	DELT_FLG
            ,  	CRE_USR_ID
            ,  	CRE_DT
            ,	UPD_USR_ID
            ,	UPD_DT 
            )
SELECT      	VSL_SLAN_CD
            ,	@[pf_svc_tp_cd]			AS PF_SVC_TP_CD
            ,	VSL_SLAN_DIR_CD
            ,	VSL_SLAN_DIR_SEQ
            ,	DELT_FLG
            ,	UPD_USR_ID				AS CRE_USR_ID
            ,	UPD_DT					AS CRE_DT
            ,	'SYSTEM'
            ,	SYSDATE
FROM       		MDM_VSL_SVC_LANE_DIR 	Y
WHERE      		Y.VSL_SLAN_CD        	= @[vsl_slan_cd]			]]></sql>
			<params>
				<param name="pf_svc_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
