<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetLoaRSQL">
			<desc><![CDATA[getLoa]]></desc>
			<sql><![CDATA[
SELECT LOA_LEN 
FROM MDM_VSL_CNTR 
WHERE VSL_CD = substr(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
