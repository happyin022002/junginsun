<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOremoveFormulaByObjectDSQL">
			<desc><![CDATA[Object List 삭제시 Formula Delete]]></desc>
			<sql><![CDATA[
DELETE
  FROM PSO_FORMULA
 WHERE FOML_SYS_DESC LIKE '%'||(SELECT OBJ_LIST_NO
                                  FROM PSO_OBJ_LIST
                                 WHERE OBJ_LIST_NO = @[obj_list_no]) || '%'			]]></sql>
			<params>
				<param name="obj_list_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
