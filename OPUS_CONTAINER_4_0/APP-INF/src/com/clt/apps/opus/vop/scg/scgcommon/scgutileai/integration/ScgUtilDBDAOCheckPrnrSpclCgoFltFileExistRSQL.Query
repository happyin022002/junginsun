<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScgUtilDBDAOCheckPrnrSpclCgoFltFileExistRSQL">
			<desc><![CDATA[Flat File Log 존재유무체크]]></desc>
			<sql><![CDATA[
SELECT  '*'
FROM	SCG_PRNR_SPCL_CGO_FLT_FILE 		XX
WHERE	1 = 1
AND		XX.TRSM_BND_CD					= @[trsm_bnd_cd]        	
AND		XX.PRNR_SPCL_CGO_SEQ			= @[prnr_spcl_cgo_seq]			]]></sql>
			<params>
				<param name="trsm_bnd_cd" type="12" value="" out="N"/>
				<param name="prnr_spcl_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
