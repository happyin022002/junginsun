<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOsearchCarrierCodeRSQL">
			<desc><![CDATA[checkCarrier 체크]]></desc>
			<sql><![CDATA[
#if (${crr_cd} == 'COM') 
SELECT CASE WHEN CRR_CD IS NULL THEN 'COM' ELSE CRR_CD END CRR_CD
     , CASE WHEN CRR_NM IS NULL THEN ''    ELSE CRR_NM END CRR_NM
  FROM (SELECT MAX(CRR_CD) CRR_CD
             , MAX(CRR_NM) CRR_NM 
             , COUNT(CRR_CD) CRR_CNT
          FROM MDM_CARRIER
         WHERE CRR_CD = @[crr_cd]
           AND DELT_FLG = 'N'
       )
#else
SELECT CRR_CD
     , CRR_NM
  FROM MDM_CARRIER
 WHERE CRR_CD = @[crr_cd]
   AND DELT_FLG = 'N'
#end			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
