<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOModifyYardCallSeqUSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
UPDATE 	VSK_VSL_PORT_SKD T1
SET  	CALL_YD_IND_SEQ =  (
        					SELECT 	SEQ_YD
        					FROM 	(
           							SELECT 	ROWID, CLPT_SEQ
											, ROW_NUMBER() OVER (PARTITION BY YD_CD ORDER BY CLPT_SEQ) AS SEQ_YD
           							FROM 	VSK_VSL_PORT_SKD
           							WHERE 	VSL_CD  = @[vsl_cd]
           							AND  	SKD_VOY_NO = @[skd_voy_no]
           							AND  	SKD_DIR_CD = @[skd_dir_cd]
           							ORDER BY CLPT_SEQ
          							) S
        					WHERE T1.ROWID = S.ROWID
       						)
WHERE 	VSL_CD  = @[vsl_cd]
AND  	SKD_VOY_NO = @[skd_voy_no]
AND  	SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
