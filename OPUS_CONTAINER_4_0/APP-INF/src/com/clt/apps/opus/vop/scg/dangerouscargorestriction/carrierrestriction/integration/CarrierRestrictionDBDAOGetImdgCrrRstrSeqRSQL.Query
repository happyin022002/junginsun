<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierRestrictionDBDAOGetImdgCrrRstrSeqRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT   NVL(MAX( A.IMDG_CRR_RSTR_SEQ ),0)+1 IMDG_CRR_RSTR_SEQ
  FROM   SCG_IMDG_CRR_RSTR A
 WHERE   A.VSL_OPR_TP_CD = @[vsl_opr_tp_cd]
 			]]></sql>
			<params>
				<param name="vsl_opr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
