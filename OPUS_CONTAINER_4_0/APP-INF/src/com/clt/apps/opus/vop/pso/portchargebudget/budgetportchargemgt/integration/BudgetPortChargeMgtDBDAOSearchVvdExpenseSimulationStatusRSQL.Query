<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BudgetPortChargeMgtDBDAOSearchVvdExpenseSimulationStatusRSQL">
			<desc><![CDATA[주어진 BIZ TYPE CODE을 가지는 VVD 정보가 존재하는지 확인한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) FROM PSO_TGT_VVD
WHERE 1=1
AND PSO_BZTP_CD=@[pso_bztp_cd]
AND VSL_CD=@[vsl_cd]
AND SKD_VOY_NO=@[skd_voy_no]
AND SKD_DIR_CD=@[skd_dir_cd]			]]></sql>
			<params>
				<param name="pso_bztp_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
