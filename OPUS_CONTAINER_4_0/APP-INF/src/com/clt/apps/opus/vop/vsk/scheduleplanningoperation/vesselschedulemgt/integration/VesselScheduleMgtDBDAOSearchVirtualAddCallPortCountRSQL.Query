<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchVirtualAddCallPortCountRSQL">
			<desc><![CDATA[Counting Add Calling Port Only]]></desc>
			<sql><![CDATA[
SELECT		COUNT(1)			KNT
FROM		VSK_VSL_PORT_SKD	PS
WHERE		1 = 1
AND			PS.VSL_CD			= @[vsl_cd]
AND			PS.SKD_VOY_NO		= @[skd_voy_no]
AND			PS.SKD_DIR_CD		= @[skd_dir_cd]
AND			PS.VT_ADD_CALL_FLG	= 'Y'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
