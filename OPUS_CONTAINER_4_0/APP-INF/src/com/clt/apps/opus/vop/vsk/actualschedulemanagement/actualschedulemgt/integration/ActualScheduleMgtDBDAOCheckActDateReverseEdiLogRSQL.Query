<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualScheduleMgtDBDAOCheckActDateReverseEdiLogRSQL">
			<desc><![CDATA[Checking Actual Dates are reversed]]></desc>
			<sql><![CDATA[
SELECT  CASE WHEN TO_DATE(@[act_arr_dt], 'YYYYMMDDHH24MI') >= TO_DATE(@[act_brth_dt],'YYYYMMDDHH24MI') 
	   	 	 OR  TO_DATE(@[act_arr_dt], 'YYYYMMDDHH24MI')  >= TO_DATE(@[act_dep_dt],'YYYYMMDDHH24MI') 
			 OR  TO_DATE(@[act_brth_dt],'YYYYMMDDHH24MI')  >= TO_DATE(@[act_dep_dt],'YYYYMMDDHH24MI') 
             THEN 'N'
             ELSE 'Y'
        END AS REV_CHK_FLG 
FROM   DUAL 			]]></sql>
			<params>
				<param name="act_arr_dt" type="12" value="" out="N"/>
				<param name="act_brth_dt" type="12" value="" out="N"/>
				<param name="act_dep_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
