<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BudgetPortChargeMgtDBDAOSearchActivityDateRSQL">
			<desc><![CDATA[Activity Date Search
[2015.07.21]Virtual Add Calling 처리. VSK_VSL_PORT_SKD.NVL(VT_ADD_CALL_FLG, 'N') = 'N']]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(T5.VPS_ETD_DT,'YYYYMMDD') AS VPS_ETD_DT
  FROM VSK_VSL_SKD T1
     , VSK_VSL_PORT_SKD T5
 WHERE 1=1
   AND T1.VSL_CD = @[vsl_cd]
   AND T1.SKD_VOY_NO = @[skd_voy_no]
   AND T1.SKD_DIR_CD = @[skd_dir_cd]
   AND T1.VSL_CD = T5.VSL_CD
   AND T1.SKD_VOY_NO = T5.SKD_VOY_NO
   AND T1.SKD_DIR_CD = T5.SKD_DIR_CD
   AND T5.CLPT_IND_SEQ = NVL(@[clpt_ind_seq],(  SELECT MIN(CLPT_IND_SEQ)
                              FROM VSK_VSL_PORT_SKD S
                             WHERE S.VSL_CD = T5.VSL_CD
                               AND S.SKD_VOY_NO = T5.SKD_VOY_NO
                               AND S.SKD_DIR_CD = T5.SKD_DIR_CD
                               AND S.YD_CD = T5.YD_CD
                               AND 'S' != NVL(S.SKD_CNG_STS_CD, ' ')
							   AND NVL(S.VT_ADD_CALL_FLG, 'N') = 'N' /*2015.07.21 Add*/ ))
   AND NVL(T5.SKD_CNG_STS_CD, ' ') != 'S'
   AND NVL(T5.VT_ADD_CALL_FLG, 'N') = 'N' /*2015.07.21 Add*/
   AND T5.YD_CD = @[yd_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="ACKT" out="N"/>
				<param name="skd_voy_no" type="12" value="0024" out="N"/>
				<param name="skd_dir_cd" type="12" value="S" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="KRPUS01" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
