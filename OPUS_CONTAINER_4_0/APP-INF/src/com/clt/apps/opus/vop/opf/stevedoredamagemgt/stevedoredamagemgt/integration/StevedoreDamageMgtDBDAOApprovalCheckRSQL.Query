<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StevedoreDamageMgtDBDAOApprovalCheckRSQL">
			<desc><![CDATA[Approval Check SQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT 
FROM   COM_USER
WHERE  1=1
AND    OFC_CD IN 
	   ( 

		SELECT   MO.OFC_CD
		FROM     MDM_ORGANIZATION MO
		WHERE    MO.OFC_KND_CD    = '2'
		AND      MO.OFC_TP_CD     = 'HQ'
		AND      MO.DELT_FLG      = 'N'

	     --SELECT OFC_CD 
	     --FROM TABLE( COM_OfficeCodeMgr_PKG.COM_getOfficeCodeList_FNC( '000001', 'OPF'))
	   )
AND    PSN_ENG_NM = 'Team Executive'
AND    USR_ID     = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
