<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OpfUtilDBDAOMstContainerChkRSQL">
			<desc><![CDATA[MST_CONTAINER 테이블에서 Container No의 Validation을 Check한다.]]></desc>
			<sql><![CDATA[
SELECT 	C.CNTR_NO AS VAL
FROM 	MST_CONTAINER C
WHERE 	C.CNTR_NO = @[cntr_no]
			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
