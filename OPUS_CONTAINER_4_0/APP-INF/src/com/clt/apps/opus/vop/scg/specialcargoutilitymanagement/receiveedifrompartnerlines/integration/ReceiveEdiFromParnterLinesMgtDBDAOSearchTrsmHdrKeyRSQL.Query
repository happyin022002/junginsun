<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOSearchTrsmHdrKeyRSQL">
			<desc><![CDATA[SearchTrsmHdrKey]]></desc>
			<sql><![CDATA[
SELECT 		H.TRSM_BND_CD
     	, 	TO_CHAR(H.TRSM_DT, 'YYYYMMDD') TRSM_DT
     	, 	H.SPCL_CGO_CATE_CD
     	, 	H.PRNR_SPCL_CGO_SEQ
     	, 	H.BKG_REF_NO
FROM 		SCG_PRNR_SPCL_CGO_TRSM_HDR		H
WHERE 		1 = 1
AND   		H.TRSM_MZD_CD              		= 'EDI'										--::EDI, EML::--
AND   		H.EDI_MSG_REF_NO 				= @[org_msg_key]

--:2016-03-11:--AND		H.PRNR_SPCL_CGO_SEQ	= (	SELECT	MAX(HH.PRNR_SPCL_CGO_SEQ)
--:2016-03-11:--								FROM	SCG_PRNR_SPCL_CGO_TRSM_HDR		HH
--:2016-03-11:--								WHERE	HH.EDI_MSG_REF_NO				= H.EDI_MSG_REF_NO
--:2016-03-11:--								AND		HH.TRSM_MZD_CD					= H.TRSM_MZD_CD
--:2016-03-11:--							  )			]]></sql>
			<params>
				<param name="org_msg_key" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
