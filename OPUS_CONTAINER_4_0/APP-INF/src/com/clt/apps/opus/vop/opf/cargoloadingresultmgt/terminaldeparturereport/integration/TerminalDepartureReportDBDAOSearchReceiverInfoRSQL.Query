<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOSearchReceiverInfoRSQL">
			<desc><![CDATA[get Send Email Address]]></desc>
			<sql><![CDATA[
SELECT X.CNTC_PSON_EML_CTNT
FROM   SCG_CNTC_PNT_ADD     X
     , SCG_RGN_SHP_OPR_PORT Y
     , MDM_VSL_CNTR         VC
     , VSK_VSL_SKD          VS
WHERE  1 = 1 
AND    X.RGN_SHP_OPR_CD     = Y.RGN_SHP_OPR_CD
AND    X.SLAN_CD            = VS.VSL_SLAN_CD
AND    VS.VSL_CD            = VC.VSL_CD
AND    Y.LOC_CD             = @[port_cd]   			-- <<Binding Var : Departure Port >> --
AND    X.CNTC_CATE_CD       = @[spcl_cgo_cate_cd]   -- <<Fixed Value : 'TD':TDR, 'RD':RDR >> --
AND    X.CRR_CD             = VC.CRR_CD
AND    VS.VSL_CD            = @[vsl_cd]    			-- <<Binding Var : Vessel Code >> --
AND    VS.SKD_VOY_NO        = @[voy_no]    			-- <<Binding Var : SKD Voyage No >> --
AND    VS.SKD_DIR_CD        = @[dir_cd]       		-- <<Binding Var : SKD Direction Code >> --			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_cate_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
