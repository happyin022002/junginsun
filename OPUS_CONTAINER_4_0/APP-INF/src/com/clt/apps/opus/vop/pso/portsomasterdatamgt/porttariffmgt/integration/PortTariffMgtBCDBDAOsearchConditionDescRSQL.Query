<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchConditionDescRSQL">
			<desc><![CDATA[condition의 수식을 가져온다.]]></desc>
			<sql><![CDATA[
select 
cond_desc,cond_sys_desc 
from pso_condition
where cond_no = @[cond_no]
			]]></sql>
			<params>
				<param name="cond_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
