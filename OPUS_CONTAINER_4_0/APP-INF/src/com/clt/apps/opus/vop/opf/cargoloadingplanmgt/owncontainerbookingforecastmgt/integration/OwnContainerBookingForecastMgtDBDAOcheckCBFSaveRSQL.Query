<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOcheckCBFSaveRSQL">
			<desc><![CDATA[checkCBFSave]]></desc>
			<sql><![CDATA[
SELECT 1 CBFCOUNT  
  FROM OPF_CGO_BKG_FCAST_CNTR A, 
       OPF_CGO_BKG_FCAST      B
 WHERE A.VSL_CD                    = @[vsl_cd]
   AND A.SKD_VOY_NO                = @[skd_voy_no]
   AND A.SKD_DIR_CD                = @[skd_dir_cd]
   AND A.BKG_SHPR_OWNR_FLG         = @[bkg_shpr_ownr_flg]
   AND A.CRR_CD                    = @[crr_cd]
   AND A.YD_CD||A.POL_CLPT_IND_SEQ = @[yd_cd]
   AND B.VSL_CD                    = A.VSL_CD                                                    
   AND B.SKD_VOY_NO                = A.SKD_VOY_NO                                                    
   AND B.SKD_DIR_CD                = A.SKD_DIR_CD                                                    
   AND B.BKG_SHPR_OWNR_FLG         = A.BKG_SHPR_OWNR_FLG
   AND B.CRR_CD                    = A.CRR_CD
   AND B.YD_CD                     = A.YD_CD
   AND B.POL_CLPT_IND_SEQ          = A.POL_CLPT_IND_SEQ
   AND A.CBF_DP_CD                 = 'V'
   AND ROWNUM                      = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="COCN" out="N"/>
				<param name="skd_voy_no" type="12" value="0001" out="N"/>
				<param name="skd_dir_cd" type="12" value="E" out="N"/>
				<param name="bkg_shpr_ownr_flg" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="GBFXTK1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
