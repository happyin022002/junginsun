<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOUpdateAproRqstForVVDChangeUSQL">
			<desc><![CDATA[updateAproRqstForVVDChange]]></desc>
			<sql><![CDATA[
UPDATE    	SCG_APRO_RQST             XX
SET       	XX.VVD_CNG_FLG            = 'Y'
WHERE     	1 = 1
AND       	XX.SPCL_CGO_CATE_CD       = 'DG'
AND       	XX.LST_RQST_DAT_FLG       = 'Y'
AND			XX.BKG_NO                 = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
