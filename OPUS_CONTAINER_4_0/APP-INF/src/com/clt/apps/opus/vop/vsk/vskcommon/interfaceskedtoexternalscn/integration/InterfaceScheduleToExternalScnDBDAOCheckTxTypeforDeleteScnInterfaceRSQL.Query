<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToExternalScnDBDAOCheckTxTypeforDeleteScnInterfaceRSQL">
			<desc><![CDATA[SCN I/F TX TYPE]]></desc>
			<sql><![CDATA[
SELECT  F.VSL_CD
      , LPAD(F.SKD_VOY_NO,4,'0')	AS SKD_VOY_NO
      , F.SKD_DIR_CD
      , F.VPS_PORT_CD
      , F.CSSM_VOY_NO_IF_SEQ 
       
      , F.INSF_CNQE_VAL         	-- 'S':SUCCESS, NULL:INITIAL --
      , F.INSF_DV_CD            	-- 'I':INSERT, 'U':UPDATE, 'D':DELETE --
      
      , CASE WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'I' THEN NULL
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'U' THEN NULL
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'D' THEN 'X'   
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'I' THEN 'ROW_DELETE'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'U' THEN NULL
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'D' THEN 'X'
        END                     NEW_INSF_CNQE_VAL
      , CASE WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'I' THEN 'D'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'U' THEN 'D'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'D' THEN 'X'    
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'I' THEN 'ROW_DELETE'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'U' THEN 'D'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'D' THEN 'X'               
        END                     NEW_INSF_DV_CD  
        
      , CASE WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'I' THEN 'I'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'U' THEN 'I'
             WHEN F.INSF_CNQE_VAL = 'S'   AND F.INSF_DV_CD = 'D' THEN 'X'        
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'I' THEN 'ROW_DELETE'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'U' THEN 'U'
             WHEN F.INSF_CNQE_VAL IS NULL AND F.INSF_DV_CD = 'D' THEN 'X'
        END                     NEW_TX_TP_CD      
        
FROM    VSK_VSL_CSSM_VOY_NO_IF  F
WHERE   F.VSL_CD                = @[vsl_cd]
AND     F.SKD_VOY_NO            = @[skd_voy_no]
AND     F.SKD_DIR_CD            = @[skd_dir_cd]
AND     F.CSSM_VOY_NO_IF_SEQ    = (SELECT   MAX(FF.CSSM_VOY_NO_IF_SEQ)
                                   FROM     VSK_VSL_CSSM_VOY_NO_IF  FF
                                   WHERE    FF.VSL_CD               = F.VSL_CD
                                   AND      FF.SKD_VOY_NO           = F.SKD_VOY_NO
                                   AND      FF.SKD_DIR_CD           = F.SKD_DIR_CD
                                   AND      FF.VPS_PORT_CD          = F.VPS_PORT_CD
                                   )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
