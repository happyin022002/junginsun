<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OpfUtilDBDAOSearchPortInfoRSQL">
			<desc><![CDATA[MDM_LOCATION과 MDM_COUNTRY
에서 Port 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  T1.LOC_CD AS  VAL
,       T1.LOC_NM AS  NAME
FROM MDM_LOCATION T1, MDM_COUNTRY T2
WHERE 1=1
AND T1.CNT_CD=T2.CNT_CD
AND T1.LOC_CD = UPPER(@[port_cd])
AND T1.DELT_FLG = 'N'
AND T2.DELT_FLG = 'N'
AND T1.CALL_PORT_FLG='Y'			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
