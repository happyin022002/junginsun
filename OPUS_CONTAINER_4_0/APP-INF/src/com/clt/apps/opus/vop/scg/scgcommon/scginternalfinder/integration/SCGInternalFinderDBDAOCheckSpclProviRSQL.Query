<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGInternalFinderDBDAOCheckSpclProviRSQL">
			<desc><![CDATA[CheckSpclProvi 체크]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_SPCL_PROVI_NO
,	IMDG_SPCL_PROVI_DESC
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_IMDG_SPCL_PROVI
WHERE	IMDG_SPCL_PROVI_NO = @[imdg_spcl_provi_no]
AND	DELT_FLG = 'N'
			]]></sql>
			<params>
				<param name="imdg_spcl_provi_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
