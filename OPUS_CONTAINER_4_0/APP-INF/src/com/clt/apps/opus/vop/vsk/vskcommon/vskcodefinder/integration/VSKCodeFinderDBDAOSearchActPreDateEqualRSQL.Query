<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOSearchActPreDateEqualRSQL">
			<desc><![CDATA[ACTUAL SKD 입력시 GMT 기준으로 이전포트의 ETD와 입력포트 ATA역전관계 체크]]></desc>
			<sql><![CDATA[
SELECT  CASE SIGN(GLOBALDATE_PKG.TIME_CONV_FNC(@[vps_port_cd],TO_DATE(@[act_arr_dt],'YYYY-MM-DD HH24:MI'),'GBFXT') - GLOBALDATE_PKG.TIME_CONV_FNC(@[pre_port_cd],TO_DATE(@[pre_etd_dt],'YYYY-MM-DD HH24:MI'),'GBFXT')) WHEN 1 THEN 'NORMAL'
             ELSE 'REVERSE'
         END ACT_SKD_REVERSE_INDICATOR
   FROM DUAL			]]></sql>
			<params>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="act_arr_dt" type="12" value="" out="N"/>
				<param name="pre_port_cd" type="12" value="" out="N"/>
				<param name="pre_etd_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
