<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchConditionNoRSQL">
			<desc><![CDATA[저장된 condition의 no를 가져온다.]]></desc>
			<sql><![CDATA[
select nvl(max(cond_no), -1) cond_no from pso_condition
where 1=1
and cond_desc = @[cond_desc]			]]></sql>
			<params>
				<param name="cond_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
