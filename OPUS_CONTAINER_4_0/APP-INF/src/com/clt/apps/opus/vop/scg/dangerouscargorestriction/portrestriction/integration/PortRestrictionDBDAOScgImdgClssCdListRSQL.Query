<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortRestrictionDBDAOScgImdgClssCdListRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT IMDG_CLSS_CD, IMDG_CLSS_CD_DESC 
  FROM SCG_IMDG_CLSS_CD
#if (${class_cd} != '' ) 
 WHERE  IMDG_CLSS_CD = @[class_cd]
#else
 WHERE   
        DELT_FLG = 'N'
#end
ORDER BY imdg_clss_cd			]]></sql>
			<params>
				<param name="class_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
