<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierRestrictionDBDAOCheckImdgTecNmRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  'Y'  
  FROM SCG_IMDG_UN_NO_ORG_RACT F 
 WHERE 
       F.IMDG_TEC_NM IS NOT NULL
   AND ROWNUM = 1
#if (${imdg_un_no} != '') 
   AND F.IMDG_UN_NO     = @[imdg_un_no]
#end
#if (${imdg_un_no} != '') 
   AND F.IMDG_UN_NO_SEQ =  @[imdg_un_no_seq]
#end			]]></sql>
			<params>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
