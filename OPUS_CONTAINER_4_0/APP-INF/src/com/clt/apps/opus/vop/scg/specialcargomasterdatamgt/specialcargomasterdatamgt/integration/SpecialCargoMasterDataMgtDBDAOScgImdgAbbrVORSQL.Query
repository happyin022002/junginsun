<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgAbbrVORSQL">
			<desc><![CDATA[DG Abbreviation (Inquiry) 조회   ]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_ABBR_CD
,	IMDG_ABBR_FULL_DESC
,	NVL(DELT_FLG,'N') AS DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_IMDG_ABBR
WHERE	1 = 1
#if (${delt_flg} != '') 
AND NVL(DELT_FLG,'N') = @[delt_flg]
#end
#if (${imdg_abbr_cd} != '')
AND IMDG_ABBR_CD LIKE @[imdg_abbr_cd]||'%'
#end
ORDER BY IMDG_ABBR_CD			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="imdg_abbr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
