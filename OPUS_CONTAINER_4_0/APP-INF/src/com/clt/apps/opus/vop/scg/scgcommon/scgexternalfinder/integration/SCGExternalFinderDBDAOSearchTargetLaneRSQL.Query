<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGExternalFinderDBDAOSearchTargetLaneRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT H.VSL_SLAN_CD
       ,(SELECT A2.VSL_SLAN_NM  FROM MDM_VSL_SVC_LANE A2 WHERE A2.VSL_SLAN_CD= H.VSL_SLAN_CD AND ROWNUM=1)VSL_SLAN_NM
FROM   VSK_PF_SKD H, VSK_PF_CALL_PORT F, VSK_PF_CALL_PORT T
WHERE  ( H.SLAN_STND_FLG = 'Y' OR H.MML_USD_FLG = 'Y' )
AND    H.VSL_SLAN_CD   = F.VSL_SLAN_CD
AND    H.PF_SVC_TP_CD  = F.PF_SVC_TP_CD
AND    F.VSL_SLAN_CD   = T.VSL_SLAN_CD
AND    F.PF_SVC_TP_CD  = T.PF_SVC_TP_CD
AND    F.SKD_DIR_CD    = T.SKD_DIR_CD
AND    F.PORT_ROTN_SEQ < T.PORT_ROTN_SEQ
AND    F.PORT_CD       = @[pol]          --:pol_cd
AND    T.PORT_CD       = @[pod]          --:pod_cd
ORDER BY H.VSL_SLAN_CD			]]></sql>
			<params>
				<param name="pol" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
