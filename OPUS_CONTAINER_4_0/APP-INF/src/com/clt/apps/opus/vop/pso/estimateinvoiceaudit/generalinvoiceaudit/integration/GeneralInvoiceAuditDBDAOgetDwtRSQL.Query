<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetDwtRSQL">
			<desc><![CDATA[Tariff 비용 계산을 위해 DWT를 구한다.]]></desc>
			<sql><![CDATA[
select NVL(DWT_WGT,0)
from mdm_vsl_cntr
where vsl_cd = substr(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
