<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOremoveFormulaDetailByObjectDSQL">
			<desc><![CDATA[Object List Delete 시 삭제되는 Formula Detail]]></desc>
			<sql><![CDATA[
DELETE
  FROM PSO_FOML_DTL
 WHERE FOML_NO IN (
                    SELECT DISTINCT FOML_NO
                      FROM PSO_FOML_DTL
                     WHERE RT_OBJ_LIST_NO IN (SELECT OBJ_LIST_NO
                                                FROM PSO_OBJ_LIST
                                               WHERE OBJ_LIST_NO = @[obj_list_no] )
                     UNION
                    SELECT DISTINCT FOML_NO
                      FROM PSO_FOML_DTL
                     WHERE OBJ_LIST_NO IN ( SELECT OBJ_LIST_NO
                                              FROM PSO_OBJ_LIST
                                             WHERE OBJ_LIST_NO = @[obj_list_no] )
                 )			]]></sql>
			<params>
				<param name="obj_list_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
