<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSOCodeFinderDBDAOsearchVendorListRSQL">
			<desc><![CDATA[searchVendorList]]></desc>
			<sql><![CDATA[
SELECT 	
		VNDR_SEQ,
		VNDR_CNT_CD,
		VNDR_LGL_ENG_NM,
		VNDR_LOCL_LANG_NM,
		VNDR_ABBR_NM,
		LGS_FLG,
		PROCU_FLG,
		TEAM_FLG,
		FINC_FLG,
		BLK_FLG,
		INTER_CO_FLG,
		LOC_CD,
		OFC_CD,
		CEO_NM,
		RGST_NO,
		TAX_ID,
		PRNT_CNT_CD,
		PRNT_VNDR_SEQ,
		DCGO_HNDL_FLG,
		SVC_SCP_CD_NM,
		SVC_PRD_TP_NM,
		SVC_PRD_RMK,
		BZCT_NM,
		BZTP_NM,
		GEN_PAY_TERM_CD,
		--ENG_ADD,
		--RLOCL_LANG_ADDR,
		ZIP_CD,
		CNTC_PSON_NM,
		INV_CURR_CD,
		PAY_CURR_CD,
		PAY_MZD_CD,
		USA_EDI_CD,
		WO_ATCH_FILE_FLG,
		WO_EDI_USE_FLG,
		INV_EDI_USE_FLG,
		MTY_RRO_EDI_USE_FLG,
		BLK_VNDR_SVC_CD,
		--SUBS_CO_CDOTR_FLG,
		VNDR_OFC_CD,
		MODI_VNDR_SEQ,
		CRE_USR_ID,
		CRE_DT,
		UPD_USR_ID,
		UPD_DT,
		DELT_FLG,
		EAI_EVNT_DT,
		RFND_PSDO_CUST_CD,
		PAY_TERM_TP_CD,
		CHK_DE_ADDR1,
		CHK_DE_ADDR2,
		CHK_DE_ADDR3,
		CHK_DE_CTY_NM,
		CHK_DE_STE_CD,
		CHK_DE_ZIP_CD,
		CHK_DE_CNT_CD,
		LU_DELT_FLG,
		FLET_MGMT_OWNR_VNDR_SEQ,
		CNL_AGN_FLG,
		CNL_AGN_BANK_DESC--,
		--MNR_DFLT_TRSM_CD
FROM    MDM_VENDOR
WHERE   DELT_FLG = 'N'
#if(${cnl_agn_flg}!='')
AND     CNL_AGN_FLG = 'Y'
#end 
#if (${vndr_seq} != '')
AND     VNDR_SEQ = @[vndr_seq]
#end
#if (${loc_cd} != '')
AND     VNDR_CNT_CD = DECODE(SUBSTR(@[loc_cd], 1,5), 'PAPCA', 'PA', 'EGSCA', 'EG', 'All', VNDR_CNT_CD)
#end
ORDER BY VNDR_SEQ			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
