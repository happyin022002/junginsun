<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetDoubleCallRSQL">
			<desc><![CDATA[177 Double Call]]></desc>
			<sql><![CDATA[
SELECT ''''||DBL_CALL_FLG||''''
  FROM (
        SELECT CASE WHEN COUNT(A.CLPT_IND_SEQ) > 1 THEN 'Y'
                    ELSE 'N'
               END DBL_CALL_FLG
          FROM VSK_VSL_PORT_SKD A
         WHERE 1=1
           AND A.VSL_CD        = SUBSTR(@[vvd] , 1, 4)
           AND A.SKD_VOY_NO    = SUBSTR(@[vvd] , 5, 4)
           AND A.SKD_DIR_CD    = SUBSTR(@[vvd] , 9)
           AND A.VPS_PORT_CD   = SUBSTR(@[yd_cd]   , 1, 5)
           --AND A.CLPT_IND_SEQ  = '1' /*2016.03.23 Add*/
           AND NVL(A.SKD_CNG_STS_CD,' ') <> 'S'
           AND NVL(A.VT_ADD_CALL_FLG, 'N') = 'N' /*2015.07.21 Add*/
           AND A.TURN_PORT_IND_CD NOT IN ('D','V','F')
         ORDER BY A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD, A.CLPT_SEQ
       ) 			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
