<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOGetLoadingPortRsoDupChkRSQL">
			<desc><![CDATA[get Loading Port Rso]]></desc>
			<sql><![CDATA[
SELECT LOC_CD
  FROM SCG_RGN_SHP_OPR_PORT
 WHERE LOC_CD = @[loc_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
