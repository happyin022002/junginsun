<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetBerthingDateRSQL">
			<desc><![CDATA[Tariff 비용 계산을 위해 Berthing Date를 구한다.
=====================================================================
2014.06.27 이윤정 [CHM-201429888] New Object - Berthing Date]]></desc>
			<sql><![CDATA[
SELECT (TRUNC(A.VPS_ETD_DT)-TRUNC(A.VPS_ETB_DT)) +1
  FROM VSK_VSL_PORT_SKD A
 WHERE 1=1
   AND A.VSL_CD         = SUBSTR(@[vvd], 1, 4)
   AND A.SKD_VOY_NO     = SUBSTR(@[vvd], 5, 4)
   AND A.SKD_DIR_CD     = SUBSTR(@[vvd], 9)
   AND A.VPS_PORT_CD    = SUBSTR(@[yd_cd], 1, 5)
   AND A.YD_CD          = @[yd_cd]
   AND A.CLPT_IND_SEQ   = @[clpt_ind_seq]
   AND NVL(A.VT_ADD_CALL_FLG, 'N') = 'N' /*2015.07.21 Add*/			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
