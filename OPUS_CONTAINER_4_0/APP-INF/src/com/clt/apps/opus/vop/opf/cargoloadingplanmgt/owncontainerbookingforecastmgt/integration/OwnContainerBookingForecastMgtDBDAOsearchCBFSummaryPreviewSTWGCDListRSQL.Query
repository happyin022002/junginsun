<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOsearchCBFSummaryPreviewSTWGCDListRSQL">
			<desc><![CDATA[searchCBFSummaryPreviewSTWGCDList 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.CRR_CD, A.STWG_CD, B.INTG_CD_VAL_DESC STWG_NM
  FROM OPF_CGO_BKG_FCAST_CNTR A
     , COM_INTG_CD_DTL        B
 WHERE A.VSL_CD                    = @[vsl_cd]
   AND A.SKD_VOY_NO                = @[skd_voy_no]
   AND A.SKD_DIR_CD                = @[skd_dir_cd]
   AND A.YD_CD||A.POL_CLPT_IND_SEQ = @[yd_cd]
#if (${crr_cd} != '' && ${crr_cd} != 'ALL')
   AND A.CRR_CD       = @[crr_cd]
#elseif (${qty1} != '')
   AND A.CRR_CD IN ( @[qty1], @[qty2], @[qty3], @[qty4], @[qty5] )
#end
   AND A.CBF_DP_CD    = 'S'
   AND A.STWG_CGO_FLG = 'Y'
   AND A.STWG_CD IS NOT NULL
   AND A.STWG_CD      = B.INTG_CD_VAL_CTNT(+)
   AND B.INTG_CD_ID(+) = 'CD02146'
 ORDER BY DECODE(A.CRR_CD,COM_ConstantMgr_PKG.COM_getCompanyCode_FNC(),'1',A.CRR_CD), A.STWG_CD			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="HJPO" out="N"/>
				<param name="skd_voy_no" type="12" value="0012" out="N"/>
				<param name="skd_dir_cd" type="12" value="W" out="N"/>
				<param name="yd_cd" type="12" value="KRPUSYG" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="qty1" type="12" value="" out="N"/>
				<param name="qty2" type="12" value="" out="N"/>
				<param name="qty3" type="12" value="" out="N"/>
				<param name="qty4" type="12" value="" out="N"/>
				<param name="qty5" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
