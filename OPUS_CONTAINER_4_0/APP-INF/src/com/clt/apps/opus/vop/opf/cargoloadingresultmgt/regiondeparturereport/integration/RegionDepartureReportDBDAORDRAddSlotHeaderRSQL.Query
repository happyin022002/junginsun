<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAORDRAddSlotHeaderRSQL">
			<desc><![CDATA[RDRAddSlotHeader Select Query]]></desc>
			<sql><![CDATA[
SELECT DISTINCT D.OPR_CD
FROM   RDR_HEADER H, RDR_CNTR_DETAIL D
WHERE  H.VSL_CD  = @[vsl_cd]
AND    H.VOY_NO  = @[voy_no]
AND    H.DIR_CD  = @[dir_cd]
AND    H.REGION  = @[region] 
#if (${opr_cd} != '' && ${opr_cd} != 'All') 
AND    D.OPR_CD  = @[opr_cd] 
#end
#if (${port_cd} != '')
AND    H.PORT_CD    = @[port_cd] 
#end
AND    H.VSL_CD  = D.VSL_CD
AND    H.VOY_NO  = D.VOY_NO
AND    H.DIR_CD  = D.DIR_CD
AND    H.REGION  = D.REGION
AND    D.CARGO_TYPE IN ('AK','BB')
ORDER	BY D.OPR_CD			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
