<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAORDRNextPortVORSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 	NEXT_PORT  , TO_CHAR(ETA,  'YYYY-MM-DD HH24:MI') ETA
,       NEXT_CANAL
,       TO_CHAR(ETA_CANAL,  'YYYY-MM-DD HH24:MI') ETA_CANAL
FROM	RDR_HEADER
WHERE  	VSL_CD  = @[vsl_cd] 
AND    	VOY_NO  = @[voy_no] 
AND    	DIR_CD  = @[dir_cd] 
AND    	REGION  = @[region]
#if (${port_cd} != '')
AND     PORT_CD    = @[port_cd] 
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
