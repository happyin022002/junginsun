<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToIBISDBDAOVskVslSkdRSQL">
			<desc><![CDATA[VskVslSkd select]]></desc>
			<sql><![CDATA[
SELECT VSL_CD AS IF_MNPL_CD
  FROM VSK_VSL_SKD
 WHERE 1=1
   AND VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]



			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
