     , NVL(@[cntr_grs_wgt],0)
     , @[cgo_grs_wgt]
     , @[cntr_tpsz_cd]
     , @[imdg_clss_cd]
     , @[imdg_un_no]
     , @[prp_shp_nm]
     , @[hzd_desc]
     , @[imdg_mrn_polut_cd]
     , DECODE(@[pck_grp_cd],'I','1','II','2','III','3',@[pck_grp_cd])
     , @[lmt_qty_flg]
     , @[expt_qty_flg]
     , NVL(@[fdo_temp],0)
     , NVL(@[cdo_temp],0)
     , @[dim_len]
     , @[dim_wdt]
     , @[dim_hgt]
     , NVL(@[ovr_fwd],0)
     , NVL(@[ovr_aft],0)
     , NVL(@[ovr_hgt],0)
     , NVL(@[ovr_lft],0)
     , NVL(@[ovr_rgt],0)
     , @[crn_pst_sts_cd]
     , DECODE(@[stwg_cd],'+','',@[stwg_cd])
     , @[stwg_n1st_rmk]
     , @[stwg_n2nd_rmk]
     , NVL(@[spcl_cgo_auth_flg],'N')
     , DECODE(@[apro_ref_no],'+','',@[apro_ref_no])
     , @[cbf_rmk]
     , NVL(@[cbf_cmdt_nm],'')
     , @[cre_usr_id]
     , TO_DATE(REPLACE(REPLACE(@[cre_dt],'-',''),':',''),'YYYYMMDD HH24MISS')
     , @[cre_usr_id]
     , TO_DATE(REPLACE(REPLACE(@[cre_dt],'-',''),':',''),'YYYYMMDD HH24MISS')
