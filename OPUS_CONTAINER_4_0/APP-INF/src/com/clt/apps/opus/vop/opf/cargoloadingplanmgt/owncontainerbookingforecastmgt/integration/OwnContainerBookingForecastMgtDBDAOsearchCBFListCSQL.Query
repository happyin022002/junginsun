     , @[pol_clpt_ind_seq]
     , @[pod_clpt_ind_seq]
     , @[cbf_smry_seq]
     , @[bkg_no]
     , @[prnr_bkg_ref_no]
     , @[cntr_seq]
     , NVL(@[cgo_seq],0)
     , NVL(@[spcl_cgo_seq],0)
     , DECODE(@[cntr_no],'+','',@[cntr_no])
     , DECODE(@[prnr_cntr_ref_no],'+','',@[prnr_cntr_ref_no])
