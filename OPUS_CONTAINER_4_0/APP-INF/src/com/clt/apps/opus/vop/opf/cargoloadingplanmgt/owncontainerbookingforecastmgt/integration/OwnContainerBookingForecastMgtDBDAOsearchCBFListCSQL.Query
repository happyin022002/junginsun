     --, DECODE(ABS(NVL([rc_flg],0)),1,'Y','N')
     --, DECODE(ABS(NVL([awk_cgo_flg],0)),1,'Y','N')
     --, DECODE(ABS(NVL([bb_cgo_flg],0)),1,'Y','N')
     , DECODE(NVL(@[dcgo_flg],'N'),'N','N','Y')
     , DECODE(NVL(@[rc_flg],'N'),'N','N','Y')
     , DECODE(NVL(@[awk_cgo_flg],'N'),'N','N','Y')
     , DECODE(NVL(@[bb_cgo_flg],'N'),'N','N','Y')
     , @[cbf_dp_cd]
     , @[prct_flg]
     , @[bkg_sts_cd]
     --, DECODE(ABS(NVL([stwg_cgo_flg],0)),1,'Y','N')
	 , DECODE(NVL(@[stwg_cgo_flg],'N'),'N','N','Y')
	 , @[bkg_ref_no_ctnt]
)			]]></sql>
