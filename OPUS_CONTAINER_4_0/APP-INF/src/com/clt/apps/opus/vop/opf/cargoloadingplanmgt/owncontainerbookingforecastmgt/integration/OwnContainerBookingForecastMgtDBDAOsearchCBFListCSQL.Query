     , IMDG_UN_NO
     , PRP_SHP_NM
     , HZD_DESC
     , IMDG_MRN_POLUT_CD
     , PCK_GRP_CD
     , LMT_QTY_FLG
     , EXPT_QTY_FLG
     , FDO_TEMP
     , CDO_TEMP
