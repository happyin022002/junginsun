<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselInformationMgtDBDAOMdmVslCntrExcelVORSQL">
			<desc><![CDATA[dummy]]></desc>
			<sql><![CDATA[
SELECT 
       case rownum 
         when 1  then a.vsl_eng_nm
         when 2  then 'VESSEL CODE : '||a.vsl_cd
         when 3  then 'BUILDER                   '
         when 4  then 'KEEL LAID                 '
         when 5  then 'LAUNCHED DATE             '
         when 6  then 'DELIVERED DATE            '
         when 7  then 'REGISTERED DATE           '
         when 8  then 'CALL SIGN                 '
         when 9  then 'OFFICIAL NO               '
         when 10 then 'IMO NO                   '
         when 11 then 'CLASS                    '
         when 12 then 'CLASS NO                  '
         when 13 then 'PORT OF REGISTRY          '
         when 14 then 'FLAG (NATIONALITY)        '
         when 15 then 'OWNER                     '
         when 16 then 'HULL NO                   '
         when 17 then 'P & I CLUB                '
         when 18 then 'CREW                      '
         when 19 then 'TELEPHONE NO              '
         when 20 then 'FAX NO                    '
         when 21 then 'TELEX NO                  '
         when 22 then 'E-MAIL ADDRESS            '
         when 23 then 'DESIGN CAPACITY (TEU)     '
         when 24 then 'OPERATION CAPACITY (TEU)  '
         when 25 then 'PANAMA CAPACITY (TEU)     '
         when 26 then 'VESSEL CLASS              '
         when 27 then 'R/F RECEPTACLE - MAX      '
         when 28 then 'R/F RECEPTACLE - OPERATION'
         when 29 then 'MIN SPEED (KNOTS)    '
         when 30 then 'SERVICE SPEED (KNOTS)     '
         when 31 then 'MAX SPEED (KNOTS)         '
         when 32 then 'L.O.A (M)                 '
         when 33 then 'L.B.P (M)                 '
         when 34 then 'BREADTH (M)               '
         when 35 then 'DEPTH (M)                 '
         when 36 then 'HEIGHT (M)                '
         when 37 then 'SUMMER DRAFT (M)          '
         when 38 then 'FREEBOARD (M)             '
         else '' 
       end as name1
,      case rownum 
         when 1  then a.vsl_eng_nm
         when 2  then 'VESSEL CODE : '||a.vsl_cd         
         when 3  then a.vsl_bldr_nm       
         when 4  then a.vsl_kel_ly_dt      
         when 5  then a.vsl_lnch_dt   
         when 6  then a.vsl_de_dt  
         when 7  then a.rgst_dt     
         when 8  then a.call_sgn_no
         when 9  then a.rgst_no 
         when 10 then a.lloyd_no      
         when 11 then a.clss_no_rgst_area_nm         
         when 12 then a.vsl_clss_no      
         when 13 then a.rgst_port_cd
         when 14 then a.cnt_nm       
         when 15 then case a.vsl_own_ind_cd when 'O' then COM_CONSTANTMGR_PKG.COM_getCompanyName_FNC()
                                            when 'C' then 'CHARTER'
                      end    -- OWNER 재작업 필요    
         when 16 then a.vsl_hl_no       
         when 17 then a.piclb_desc             
         when 18 then DECODE(a.crw_knt, 0, '', to_char(a.crw_knt))
         when 19 then a.phn_no     
         when 20 then a.fax_no
         when 21 then a.tlx_no
         when 22 then a.vsl_eml   
         --when 5  then trim(to_char(a.lgt_shp_tong_wgt,'999,999,999.99'))          
         when 23 then DECODE(a.cntr_dzn_capa,0,'',trim(to_char(a.cntr_dzn_capa,'999,999,999,999.99')))
         when 24 then DECODE(a.cntr_op_capa,0,'',trim(to_char(a.cntr_op_capa,'999,999,999,999.99')))
         when 25 then DECODE(a.cntr_pnm_capa,0,'',trim(to_char(a.cntr_pnm_capa,'999,999,999,999.99')))
         when 26 then DECODE(a.cntr_vsl_clss_capa,0,'',trim(to_char(a.cntr_vsl_clss_capa,'999,999,999,999.99')))
         when 27 then DECODE(a.rf_rcpt_knt,0,'',trim(to_char(a.rf_rcpt_knt,'99,999')))
         when 28 then DECODE(a.rf_rcpt_max_knt,0,'',trim(to_char(a.rf_rcpt_max_knt,'99,999')))
         when 29 then DECODE(a.ecn_spd,0,'',trim(to_char(a.ecn_spd,'99,999.99')))       
         when 30 then DECODE(a.vsl_svc_spd,0,'',trim(to_char(a.vsl_svc_spd,'99,999.99')))
         when 31 then DECODE(a.max_spd,0,'',trim(to_char(a.max_spd,'99,999.99')))
         when 32 then DECODE(a.loa_len,0,'',trim(to_char(a.loa_len,'9,999,999.99')))
         when 33 then DECODE(a.lbp_len,0,'',trim(to_char(a.lbp_len,'9,999,999.99')))
         when 34 then DECODE(a.vsl_wdt,0,'',trim(to_char(a.vsl_wdt,'9,999,999.99')))
         when 35 then DECODE(a.vsl_dpth,0,'',trim(to_char(a.vsl_dpth,'999,999.99')))         
         when 36 then DECODE(a.vsl_hgt,0,'',trim(to_char(a.vsl_hgt,'99,999,999.99')))        
         when 37 then DECODE(a.smr_drft_hgt,0,'',trim(to_char(a.smr_drft_hgt,'99,999,999.99')))         
         when 38 then DECODE(a.fbd_capa,0,'',trim(to_char(a.fbd_capa,'999,999,999,999.99')))  
         else '' 
       end as val1
,      case rownum 
         when 1  then a.vsl_eng_nm
         when 2  then 'ISSUE DATE : '||to_char(sysdate,'yyyy.mm.dd')
         when 3  then 'DISPLACEMENT (MT)            '
         when 4  then 'DEAD WEIGHT (MT)             '
         when 5  then 'LIGHT SHIP (MT)              '
         when 6  then 'INTERNATIONAL GROSS TON (MT) '
         when 7  then 'INTERNATIONAL NET TON (MT)   '
         when 8  then 'PANAMA GROSS TON (MT)        '
         when 9  then 'PANAMA NET TON (MT)          '
         when 10  then 'SUEZ GROSS TON (MT)          '
         when 11  then 'SUEZ NET TON (MT)            '
         when 12 then 'ITC (Y/N)                    '
         when 13 then 'F.O TANK CAPACITY - 85% (MT) '
         when 14 then 'D.O TANK CAPACITY - 85% (MT) '
         when 15 then 'F.W TANK CAPACITY - 100% (MT)'
         when 16 then 'B.W TANK CAPACITY - 100% (MT)'
         when 17 then 'F.O CONSUMPTION (DAY)        '
         when 18 then 'D.O CONSUMPTION (DAY)        '
         when 19 then 'F.W CONSUMPTION (DAY)        '
         when 20 then 'M/E MAKER                    '
         when 21 then 'M/E TYPE                     '
         when 22 then 'M/E BHP                      '
         when 23 then 'M/E RPM                      '
         when 24 then 'G/E MAKER                    '
         when 25 then 'G/E TYPE                     '
         when 26 then 'G/E BHP                      '
         when 27 then 'G/E RPM                      '
         when 28 then 'BOW THRUST MAKER             '
         when 29 then 'BOW THRUST TYPE              '
         when 30 then 'BOW THRUST BHP               '
         when 31 then 'BOW THRUST RPM               '
         when 32 then ''
         when 33 then ''
         when 34 then ''
         when 35 then ''
         when 36 then ''
         when 37 then ''
         when 38 then ''
         else '' 
       end as name2
,      case rownum 
         when 1  then a.vsl_eng_nm
         when 2  then 'ISSUE DATE : '||to_char(sysdate,'yyyy.mm.dd')
         when 3  then DECODE(a.dpl_capa,0,'',trim(to_char(a.dpl_capa,'999,999,999,999.99')))
         when 4  then DECODE(a.dwt_wgt,0,'',trim(to_char(a.dwt_wgt,'999,999,999.99')))
         when 5  then DECODE(a.lgt_shp_tong_wgt,0,'',trim(to_char(a.lgt_shp_tong_wgt,'999,999,999.99')))
         when 6  then DECODE(a.grs_rgst_tong_wgt,0,'',trim(to_char(a.grs_rgst_tong_wgt,'999,999,999.99')))
         when 7  then DECODE(a.net_rgst_tong_wgt,0,'',trim(to_char(a.net_rgst_tong_wgt,'999,999,999.99')))
         when 8  then DECODE(a.pnm_gt_wgt,0,'',trim(to_char(a.pnm_gt_wgt,'999,999,999.99')))
         when 9  then DECODE(a.pnm_net_tong_wgt,0,'',trim(to_char(a.pnm_net_tong_wgt,'999,999,999.99')))
         when 10 then DECODE(a.suz_gt_wgt,0,'',trim(to_char(a.suz_gt_wgt,'999,999,999.99')))
         when 11 then DECODE(a.suz_net_tong_wgt,0,'',trim(to_char(a.suz_net_tong_wgt,'999,999,999.99')))
         when 12 then a.intl_tong_certi_flg
         when 13 then DECODE(a.foil_capa,0,'',trim(to_char(a.foil_capa,'999,999,999,999.99')))
         when 14 then DECODE(a.doil_capa,0,'',trim(to_char(a.doil_capa,'999,999,999,999.99')))
         when 15 then DECODE(a.frsh_wtr_capa,0,'',trim(to_char(a.frsh_wtr_capa,'999,999,999,999.99')))
         when 16 then DECODE(a.blst_tnk_capa,0,'',trim(to_char(a.blst_tnk_capa,'999,999,999,999.99')))
         when 17 then DECODE(a.foil_csm,0,'',trim(to_char(a.foil_csm,'99,999,999.99')))
         when 18 then DECODE(a.doil_csm,0,'',trim(to_char(a.doil_csm,'99,999,999.99')))
         when 19 then DECODE(a.frsh_wtr_csm,0,'',trim(to_char(a.frsh_wtr_csm,'99,999,999.99')))
         when 20 then a.mn_eng_mkr_nm
         when 21 then a.mn_eng_tp_desc
         when 22 then DECODE(a.mn_eng_bhp_pwr,0,'',trim(to_char(a.mn_eng_bhp_pwr,'99,999,999')))
         when 23 then DECODE(a.mn_eng_rpm_pwr,0,'',trim(to_char(a.mn_eng_rpm_pwr,'99,999,999')))
         when 24 then a.gnr_mkr_nm
         when 25 then a.gnr_tp_desc
         when 26 then DECODE(a.gnr_bhp_pwr,0,'',trim(to_char(a.gnr_bhp_pwr,'99,999,999')))
         when 27 then DECODE(a.gnr_rpm_pwr,0,'',trim(to_char(a.gnr_rpm_pwr,'99,999,999')))
         when 28 then a.bwthst_mkr_nm
         when 29 then a.bwthst_tp_desc
         when 30 then DECODE(a.bwthst_bhp_pwr,0,'',trim(to_char(a.bwthst_bhp_pwr,'99,999,999')))
         when 31 then DECODE(a.bwthst_rpm_pwr,0,'',trim(to_char(a.bwthst_rpm_pwr,'99,999,999')))
         when 32 then ''
         when 33 then ''
         when 34 then ''
         when 35 then ''
         when 36 then ''
         when 37 then ''
         when 38 then ''
         else '' 
       end as val2              
 FROM ( 
        SELECT mvc.cntr_vsl_clss_capa                                             
             , to_char(mvc.vsl_kel_ly_dt,'ddmonyy', 'nls_date_language=american') vsl_kel_ly_dt
             , to_char(mvc.vsl_lnch_dt,'ddmonyy', 'nls_date_language=american') vsl_lnch_dt    
             , to_char(mvc.vsl_de_dt,'ddmonyy', 'nls_date_language=american') vsl_de_dt         
             , to_char(mvc.rgst_dt,'ddmonyy', 'nls_date_language=american') rgst_dt                          
             , mvc.rf_rcpt_knt                                                    
             , mvc.rf_rcpt_max_knt                                                             
             , mvc.fbd_capa                                                                    
             , mvc.dpl_capa                                                                    
             , mvc.blst_tnk_capa                                                               
             , mvc.foil_csm                                                                    
             , mvc.doil_csm                                                                    
             , mvc.frsh_wtr_csm                                                                
             , mvc.mn_eng_rpm_pwr                                                              
             , mvc.gnr_rpm_pwr                                                                 
             , mvc.vsl_hgt                                                                     
             , mvc.vsl_edi_nm                                                                  
             , mvc.co_cd                                                                       
             , mvc.vsl_clz_dt                                                                  
             , mvc.vsl_cre_ofc_cd                                                              
             , mvc.vsl_delt_ofc_cd                                                             
             , mvc.vsl_bld_area_nm                                                             
             , mvc.gnr_mkr_nm                                                                  
             , mvc.gnr_tp_desc                                                                 
             , mvc.gnr_bhp_pwr                                                                 
             , mvc.bwthst_mkr_nm                                                               
             , mvc.bwthst_tp_desc                                                              
             , mvc.bwthst_bhp_pwr                                                              
             , mvc.bwthst_rpm_pwr                                                              
             , mvc.lloyd_no                                                                    
             , mvc.vsl_hl_no                                                                   
             , mvc.ttl_teu_knt                                                                 
             , mvc.vsl_htch_knt                                                                
             , mvc.vsl_hld_knt                                                                 
             , mvc.vsl_rmk                                                                     
             , mvc.intl_tong_certi_flg                                                         
             , mvc.vsl_sft_cstru_certi_exp_dt                                                  
             , mvc.vsl_sft_rdo_certi_exp_dt                                                    
             , mvc.vsl_sft_eq_certi_exp_dt                                                     
             , mvc.vsl_lod_line_certi_exp_dt                                                   
             , mvc.vsl_derat_certi_exp_dt                                                      
             , mvc.cre_usr_id                                                                  
             , mvc.cre_dt                                                                      
             , mvc.upd_usr_id                                                                  
             , to_char(mvc.upd_dt,'yyyy-mm-dd hh24:mi') upd_dt                                 
             , mvc.delt_flg                                                                    
             , mvc.eai_evnt_dt                                                                 
             , mvc.vsl_cd                                                                      
             , '' vsl_clss_cd                                                                 
             , mvc.vsl_eng_nm                                                                  
             , mvc.foil_capa                                                                   
             , mvc.doil_capa                                                                   
             , mvc.frsh_wtr_capa                                                               
             , mvc.call_sgn_no                                                                 
             , mvc.rgst_no                                                                     
             , mvc.phn_no                                                                      
             , mvc.fax_no                                                                      
             , mvc.tlx_no                                                                      
             , mvc.vsl_eml                                                                     
             , mvc.piclb_desc                                                                  
             --, mvc.rgst_port_cd 
             , ml.loc_nm||', '||mc2.cnt_nm rgst_port_cd                                                               
             , mvc.clss_no_rgst_area_nm                                                        
             , mvc.vsl_clss_no                                                                 
             , mvc.vsl_bldr_nm                                                                 
             , mvc.loa_len                                                                     
             , mvc.lbp_len                                                                     
             , mvc.vsl_wdt                                                                     
             , mvc.vsl_dpth                                                                    
             , mvc.smr_drft_hgt                                                                
             , mvc.dwt_wgt                                                                     
             , mvc.lgt_shp_tong_wgt                                                            
             , mvc.grs_rgst_tong_wgt                                                           
             , mvc.net_rgst_tong_wgt                                                           
             , mvc.pnm_gt_wgt                                                                  
             , mvc.pnm_net_tong_wgt                                                            
             , mvc.suz_gt_wgt                                                                  
             , mvc.suz_net_tong_wgt                                                            
             , mvc.mn_eng_mkr_nm                                                               
             , mvc.mn_eng_tp_desc                                                              
             , mvc.mn_eng_bhp_pwr                                                              
             , mvc.vsl_own_ind_cd                                                              
             , mvc.vsl_rgst_cnt_cd                                                             
             , mvc.vsl_bld_cd                                                                  
             , mvc.crr_cd                                                                      
             , mvc.fdr_div_cd                                                                  
             , mvc.vsl_svc_spd                                                                 
             , mvc.max_spd                                                                     
             , mvc.ecn_spd                                                                     
             , mvc.crw_knt                                                                     
             , mvc.cntr_dzn_capa                                                               
             , mvc.cntr_op_capa                                                                
             , mvc.cntr_pnm_capa                                                               
             , vc.crr_nm                                                                  
             , mc1.cnt_cd||'-'||mc1.cnt_nm as cnt_nm                                                            
         FROM  mdm_vsl_cntr mvc                                                           
             , mdm_carrier  vc                                                            
             , mdm_country  mc1
             , mdm_country  mc2
             , mdm_location ml                                                            
        WHERE mvc.crr_cd = vc.crr_cd(+)
          AND mvc.vsl_rgst_cnt_cd = mc1.cnt_cd(+)
          AND substr(mvc.rgst_port_cd,1,2) = mc2.cnt_cd(+)
          AND mvc.rgst_port_cd = ml.loc_cd(+)
          AND mvc.vsl_cd = @[vsl_cd]                                                         
      ) a
    , (SELECT rownum
         FROM dual
       CONNECT BY 1=1
          AND rownum < 39
       ) b			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
