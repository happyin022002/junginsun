<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOSelectMatchedPodClptIndSeqforEDIRSQL">
			<desc><![CDATA[적합한 pod calling indicator 추출]]></desc>
			<sql><![CDATA[
SELECT     	NVL(MIN(Y.CLPT_IND_SEQ),-1)
FROM       	VSK_VSL_PORT_SKD       		X
       ,   	VSK_VSL_PORT_SKD       		Y
WHERE      	X.VSL_CD               		= Y.VSL_CD
AND        	X.SKD_VOY_NO           		= Y.SKD_VOY_NO
AND        	X.SKD_DIR_CD           		= Y.SKD_DIR_CD    
AND        	NVL(X.SKD_CNG_STS_CD,'*')  	<> 'S'
AND        	NVL(Y.SKD_CNG_STS_CD,'*')  	<> 'S'
AND    		X.TURN_PORT_IND_CD         	IN ('Y','N')
AND			X.VT_ADD_CALL_FLG			IS NULL
AND        	X.VSL_CD               		= @[vsl_cd]
AND        	X.SKD_VOY_NO           		= @[skd_voy_no]
AND        	X.SKD_DIR_CD           		= @[skd_dir_cd]
AND        	X.VPS_PORT_CD          		= @[pol_cd]
AND        	X.CLPT_IND_SEQ         		= @[pol_clpt_ind_seq]
AND        	X.CLPT_SEQ             		< Y.CLPT_SEQ
AND        	Y.VPS_PORT_CD          		= @[pod_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
