<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOAddPsoYdChgCSQL">
			<desc><![CDATA[PSO_YD_CHG <Insert>]]></desc>
			<sql><![CDATA[
INSERT INTO PSO_YD_CHG (
	YD_CHG_NO
,	YD_CHG_VER_SEQ
,	LGS_COST_CD
,	YD_CD
,	VNDR_SEQ
,	EFF_DT
,	EXP_DT
,	CURR_CD
,	CPLS_FLG
,	ORG_VNDR_NM
,	RLT_AGMT_NO
,	LST_FLG
,	CRE_USR_ID
,	UPD_USR_ID
) VALUES( 
	@[yd_chg_no]
,	@[yd_chg_ver_seq]
,	@[lgs_cost_cd]
,	@[yd_cd]
,	@[vndr_seq]
,	TO_DATE(@[eff_dt],'YYYYMMDD')
,	TO_DATE(@[exp_dt],'YYYYMMDD')
,	@[curr_cd]
,	NVL(@[cpls_flg], 'N')
,	@[org_vndr_nm]
,	@[rlt_agmt_no]
,	@[lst_flg]
,	@[cre_usr_id]
,	@[cre_usr_id]
)			]]></sql>
			<params>
				<param name="yd_chg_no" type="12" value="" out="N"/>
				<param name="yd_chg_ver_seq" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="cpls_flg" type="12" value="" out="N"/>
				<param name="org_vndr_nm" type="12" value="" out="N"/>
				<param name="rlt_agmt_no" type="12" value="" out="N"/>
				<param name="lst_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
