<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OperationNPerformMasterDataMgtDBDAOSearchContiCdUseYnRSQL">
			<desc><![CDATA[SearchContiCdUseYn Query Search]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CONTI_CD FROM MDM_CONTINENT
WHERE CONTI_CD = @[conti_cd]
AND DELT_FLG='N'			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
