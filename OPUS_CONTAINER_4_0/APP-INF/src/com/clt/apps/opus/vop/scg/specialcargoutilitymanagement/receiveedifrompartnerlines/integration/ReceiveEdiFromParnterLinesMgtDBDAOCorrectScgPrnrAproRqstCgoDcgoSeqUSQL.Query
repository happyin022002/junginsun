<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOCorrectScgPrnrAproRqstCgoDcgoSeqUSQL">
			<desc><![CDATA[DCGO_SEQ 일괄생성 및 보정]]></desc>
			<sql><![CDATA[
MERGE INTO  SCG_PRNR_APRO_RQST_CGO  XX
USING       (
            --=========================================================
            --=========================================================
            SELECT   X.CRR_CD
                  ,  X.BKG_REF_NO
                  ,  X.SPCL_CGO_RQST_SEQ
                  ,  X.PRNR_CGO_RQST_SEQ
                  ,  X.SPCL_CGO_CATE_CD
                  ,  X.CGO_OPR_CD
                  ,  X.VSL_CD
                  ,  X.SKD_VOY_NO
                  ,  X.SKD_DIR_CD
                  ,  X.POL_CD
                  ,  X.POD_CD
                                      
                  ,  X.CNTR_REF_NO
                  ,  X.DCGO_REF_NO
                  ,  X.DCGO_SEQ
                  ,  X.SPCL_CNTR_SEQ
                  ,  X.SPCL_CGO_SEQ
                  
                  ,  X.DCGO_EXIST_YN
				  ,	 X.PRE_DCGO_SEQ
                  ,  X.PRE_MAX_DCGO_SEQ
                  ,  CASE WHEN X.DCGO_EXIST_YN = 'Y' THEN X.PRE_DCGO_SEQ
                          WHEN X.DCGO_EXIST_YN = 'N' THEN X.PRE_MAX_DCGO_SEQ + ROWNUM
                     END  AS NEW_DCGO_SEQ
                     
                  ,  ROWNUM
            FROM     (
                      --===============================================================================================
                      SELECT   XX.*
							,  YY.PRE_DCGO_SEQ
                            ,  CASE --:2015-11-09:--WHEN XX.CNTR_REF_NO = YY.CNTR_REF_NO AND XX.DCGO_REF_NO = YY.DCGO_REF_NO THEN 'Y' 
                                    WHEN XX.DCGO_REF_NO = YY.DCGO_REF_NO THEN 'Y' 
                                    WHEN YY.CNTR_REF_NO IS NULL          THEN 'N'
                               END       AS DCGO_EXIST_YN 
                             
                            ,  (SELECT  MAX(T2.DCGO_SEQ)
                                FROM    SCG_PRNR_APRO_RQST     T1
                                      , SCG_PRNR_APRO_RQST_CGO T2
                                WHERE   T1.CRR_CD              = T2.CRR_CD
                                AND     T1.BKG_REF_NO          = T2.BKG_REF_NO
                                AND     T1.SPCL_CGO_RQST_SEQ   = T2.SPCL_CGO_RQST_SEQ
                                AND     T1.PRNR_CGO_RQST_SEQ   = T2.PRNR_CGO_RQST_SEQ
                                AND     T1.CRR_CD              = XX.CRR_CD
                                AND     T1.BKG_REF_NO          = XX.BKG_REF_NO                    
                                AND     T1.LST_RQST_DAT_FLG    = 'N'
                                AND     T2.CGO_OPR_CD          = XX.CGO_OPR_CD
                                AND     T1.VSL_CD              = XX.VSL_CD
                                AND     T1.SKD_VOY_NO          = XX.SKD_VOY_NO
                                AND     T1.SKD_DIR_CD          = XX.SKD_DIR_CD
                                AND     T1.POL_CD              = XX.POL_CD
                                AND     T1.POD_CD              = XX.POD_CD
                                AND     T1.SPCL_CGO_RQST_SEQ   = (SELECT   MAX(T1.SPCL_CGO_RQST_SEQ)     
                                                                    FROM     SCG_PRNR_APRO_RQST      T11
                                                                          ,  SCG_PRNR_APRO_RQST_CGO  T21
                                                                    WHERE    T11.CRR_CD               = T21.CRR_CD
                                                                    AND      T11.BKG_REF_NO           = T21.BKG_REF_NO
                                                                    AND      T11.SPCL_CGO_RQST_SEQ    = T21.SPCL_CGO_RQST_SEQ
                                                                    AND      T11.PRNR_CGO_RQST_SEQ    = T21.PRNR_CGO_RQST_SEQ
                                                                    AND      T21.SPCL_CGO_CATE_CD     = T2.SPCL_CGO_CATE_CD
                                                                    AND      T11.CRR_CD               = T1.CRR_CD
                                                                    AND      T11.BKG_REF_NO           = T1.BKG_REF_NO
                                                                    AND      T21.CGO_OPR_CD           = T2.CGO_OPR_CD 
                                                                    AND      T11.VSL_CD               = T1.VSL_CD
                                                                    AND      T11.SKD_VOY_NO           = T1.SKD_VOY_NO
                                                                    AND      T11.SKD_DIR_CD           = T1.SKD_DIR_CD
                                                                    AND      T11.POL_CD               = T1.POL_CD
                                                                    AND      T11.POD_CD               = T1.POD_CD
                                                                    AND      T11.LST_RQST_DAT_FLG     = T1.LST_RQST_DAT_FLG
                                                                   )
                                )       AS PRE_MAX_DCGO_SEQ                
                            
                      FROM     (
                               --------------- <CURRENT DG DATA> -------------------------------
                                SELECT   X.CRR_CD
                                      ,  X.BKG_REF_NO
                                      ,  X.SPCL_CGO_RQST_SEQ
                                      ,  X.PRNR_CGO_RQST_SEQ
                                      ,  Y.SPCL_CGO_CATE_CD
                                      ,  Y.CGO_OPR_CD
                                      ,  X.VSL_CD
                                      ,  X.SKD_VOY_NO
                                      ,  X.SKD_DIR_CD
                                      ,  X.POL_CD
                                      ,  X.POD_CD
                                      
                                      ,  Y.CNTR_REF_NO
                                      ,  Y.DCGO_REF_NO
                                      ,  Y.DCGO_SEQ	
			
                                      ,  Y.SPCL_CNTR_SEQ
                                      ,  Y.SPCL_CGO_SEQ

                                FROM     SCG_PRNR_APRO_RQST      X
                                      ,  SCG_PRNR_APRO_RQST_CGO  Y
                                WHERE    1 = 1
                                AND      X.CRR_CD                = Y.CRR_CD
                                AND      X.BKG_REF_NO            = Y.BKG_REF_NO
                                AND      X.SPCL_CGO_RQST_SEQ     = Y.SPCL_CGO_RQST_SEQ
                                AND      X.PRNR_CGO_RQST_SEQ     = Y.PRNR_CGO_RQST_SEQ
                                AND      Y.SPCL_CGO_CATE_CD      = 'DG'
                                AND      X.LST_RQST_DAT_FLG      = 'Y'
                                AND      X.CRR_CD                = @[crr_cd]
                                AND      X.BKG_REF_NO            = @[bkg_ref_no]
                                AND      Y.CGO_OPR_CD            = @[cgo_opr_cd]
                                AND      X.VSL_CD                = @[vsl_cd]
                                AND      X.SKD_VOY_NO            = @[skd_voy_no]
                                AND      X.SKD_DIR_CD            = @[skd_dir_cd]
                                AND      X.POL_CD                = @[pol_cd]
                                AND      X.POD_CD                = @[pod_cd]
                               --------------- <CURRENT DG DATA> --------------------------------
                               ) XX
                            ,  (
                               --------------- <PREVIOUS DG DATA> -------------------------------
                                SELECT   X.CRR_CD
                                      ,  X.BKG_REF_NO
                                      ,  X.SPCL_CGO_RQST_SEQ
                                      ,  X.PRNR_CGO_RQST_SEQ
                                      ,  Y.SPCL_CGO_CATE_CD                
                                      ,  Y.CGO_OPR_CD
                                      ,  X.VSL_CD
                                      ,  X.SKD_VOY_NO
                                      ,  X.SKD_DIR_CD
                                      ,  X.POL_CD
                                      ,  X.POD_CD
                                                      
                                      ,  Y.CNTR_REF_NO
                                      ,  Y.DCGO_REF_NO
                                      ,  Y.DCGO_SEQ         	 AS PRE_DCGO_SEQ

                                      ,  Y.SPCL_CNTR_SEQ
                                      ,  Y.SPCL_CGO_SEQ

                                FROM     SCG_PRNR_APRO_RQST      X
                                      ,  SCG_PRNR_APRO_RQST_CGO  Y
                                WHERE    1 = 1
                                AND      X.CRR_CD                = Y.CRR_CD
                                AND      X.BKG_REF_NO            = Y.BKG_REF_NO
                                AND      X.SPCL_CGO_RQST_SEQ     = Y.SPCL_CGO_RQST_SEQ
                                AND      X.PRNR_CGO_RQST_SEQ     = Y.PRNR_CGO_RQST_SEQ
                                AND      Y.SPCL_CGO_CATE_CD      = 'DG'
                                AND      X.LST_RQST_DAT_FLG      = 'N'
                                AND      X.CRR_CD                = @[crr_cd]
                                AND      X.BKG_REF_NO            = @[bkg_ref_no]
                                AND      Y.CGO_OPR_CD            = @[cgo_opr_cd]
                                AND      X.VSL_CD                = @[vsl_cd]
                                AND      X.SKD_VOY_NO            = @[skd_voy_no]
                                AND      X.SKD_DIR_CD            = @[skd_dir_cd]
                                AND      X.POL_CD                = @[pol_cd]
                                AND      X.POD_CD                = @[pod_cd]
                                AND      X.SPCL_CGO_RQST_SEQ     = (SELECT   MAX(T1.SPCL_CGO_RQST_SEQ)     
                                                                    FROM     SCG_PRNR_APRO_RQST      T1
                                                                          ,  SCG_PRNR_APRO_RQST_CGO  T2
                                                                    WHERE    T1.CRR_CD               = T2.CRR_CD
                                                                    AND      T1.BKG_REF_NO           = T2.BKG_REF_NO
                                                                    AND      T1.SPCL_CGO_RQST_SEQ    = T2.SPCL_CGO_RQST_SEQ
                                                                    AND      T1.PRNR_CGO_RQST_SEQ    = T2.PRNR_CGO_RQST_SEQ
                                                                    AND      T2.SPCL_CGO_CATE_CD     = Y.SPCL_CGO_CATE_CD
                                                                    AND      T1.CRR_CD               = X.CRR_CD
                                                                    AND      T1.BKG_REF_NO           = X.BKG_REF_NO
                                                                    AND      T2.CGO_OPR_CD           = Y.CGO_OPR_CD 
                                                                    AND      T1.VSL_CD               = X.VSL_CD
                                                                    AND      T1.SKD_VOY_NO           = X.SKD_VOY_NO
                                                                    AND      T1.SKD_DIR_CD           = X.SKD_DIR_CD
                                                                    AND      T1.POL_CD               = X.POL_CD
                                                                    AND      T1.POD_CD               = X.POD_CD
                                                                    AND      T1.LST_RQST_DAT_FLG     = X.LST_RQST_DAT_FLG
																	
																	/* ADDING 2015-11-26 */
																	AND      T2.AUTH_STS_CD          IN ('Y','N')
                                                                   )
                               --------------- <PREVIOUS DG DATA> -------------------------------
                               ) YY
                      WHERE    XX.CRR_CD                         = YY.CRR_CD           (+)
                      AND      XX.BKG_REF_NO                     = YY.BKG_REF_NO       (+)
                      AND      XX.CGO_OPR_CD                     = YY.CGO_OPR_CD       (+)
                      AND      XX.SPCL_CGO_CATE_CD               = YY.SPCL_CGO_CATE_CD (+)
                      AND      XX.VSL_CD                         = YY.VSL_CD           (+)
                      AND      XX.SKD_VOY_NO                     = YY.SKD_VOY_NO       (+)
                      AND      XX.SKD_DIR_CD                     = YY.SKD_DIR_CD       (+)
                      AND      XX.POL_CD                         = YY.POL_CD           (+)
                      AND      XX.POD_CD                         = YY.POD_CD           (+)

                      --:2015-11-09:--AND      XX.CNTR_REF_NO                    = YY.CNTR_REF_NO      (+)
                      AND      XX.DCGO_REF_NO                    = YY.DCGO_REF_NO      (+)

                      AND      XX.SPCL_CNTR_SEQ                  = YY.SPCL_CNTR_SEQ    (+)
                      AND      XX.SPCL_CGO_SEQ                   = YY.SPCL_CGO_SEQ     (+)
                      
                      ORDER BY CASE --:2015-11-09:--WHEN XX.CNTR_REF_NO = YY.CNTR_REF_NO AND XX.DCGO_REF_NO = YY.DCGO_REF_NO THEN 'Y' 
                                    WHEN XX.DCGO_REF_NO = YY.DCGO_REF_NO THEN 'Y' 
                                    WHEN YY.CNTR_REF_NO IS NULL          THEN 'N'
                               END  ASC
                      --===============================================================================================
                      ) X
            --=========================================================
            --=========================================================
            ) YY
ON          (
            XX.CRR_CD               = YY.CRR_CD
   AND      XX.BKG_REF_NO           = YY.BKG_REF_NO
   AND      XX.SPCL_CGO_RQST_SEQ    = YY.SPCL_CGO_RQST_SEQ
   AND      XX.PRNR_CGO_RQST_SEQ    = YY.PRNR_CGO_RQST_SEQ
   AND      XX.SPCL_CNTR_SEQ        = YY.SPCL_CNTR_SEQ
   AND      XX.SPCL_CGO_SEQ         = YY.SPCL_CGO_SEQ
            )      

WHEN MATCHED THEN
   UPDATE   SET 
            XX.DCGO_SEQ             = YY.NEW_DCGO_SEQ			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="cgo_opr_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
