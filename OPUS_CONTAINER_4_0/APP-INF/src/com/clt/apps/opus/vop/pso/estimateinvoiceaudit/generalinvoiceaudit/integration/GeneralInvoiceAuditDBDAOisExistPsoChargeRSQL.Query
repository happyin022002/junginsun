<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOisExistPsoChargeRSQL">
			<desc><![CDATA[isExistPsoCharge]]></desc>
			<sql><![CDATA[
SELECT  A.ISS_CTY_CD
      , A.SO_SEQ
      , A.INV_NO
      ,(SELECT DECODE(MAX(DTL.SO_DTL_SEQ),NULL,'N','Y') FROM PSO_CHG_DTL DTL WHERE DTL.ISS_CTY_CD = A.ISS_CTY_CD AND DTL.SO_SEQ = A.SO_SEQ ) AS EXIST_DTL_YN
  FROM PSO_CHARGE A
 WHERE 1=1
-- vndr_seq, inv_no = unique
   AND A.VNDR_SEQ = @[vndr_seq]
   AND A.INV_NO   = @[inv_no]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
