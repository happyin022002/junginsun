<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToIBISDBDAOVskVslSkdCSQL">
			<desc><![CDATA[..]]></desc>
			<sql><![CDATA[
#if (${if_mnpl_cd} == '')
INSERT INTO VSK_VSL_SKD_IBIS_IF
(
      VSL_CD
    , SKD_VOY_NO
    , SKD_DIR_CD
    , IBIS_IF_SEQ
    , IF_SND_CD
    , IF_MNPL_CD
    , VSL_SLAN_CD
    , SKD_STS_CD
    , SKD_STS_MNL_FLG
    , SKD_VOY_TP_CD
    , PF_SKD_TP_CD
    , ST_PORT_CD
    , N1ST_PORT_BRTH_DT
    , ACT_CRR_CD
    , SKD_RMK
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
) VALUES (
      @[vsl_cd]
    , @[skd_voy_no]
    , @[skd_dir_cd]
    , @[ibis_if_seq]
    , NVL((SELECT /*+ INDEX_DESC(A XPKVSK_VSL_SKD_IBIS_IF) */ 'N'
             FROM VSK_VSL_SKD_IBIS_IF A WHERE VSL_CD=@[vsl_cd] AND SKD_VOY_NO=@[skd_voy_no] AND SKD_DIR_CD=@[skd_dir_cd] AND IF_SND_CD='Y' AND ROWNUM=1) ,'X')
    , 'D'
    , @[vsl_slan_cd]
    , @[skd_sts_cd]
    , @[skd_sts_mnl_flg]
    , @[skd_voy_tp_cd]
    , @[pf_skd_tp_cd]
    , @[st_port_cd]
    , TO_DATE(@[n1st_port_brth_dt] , 'YYYYMMDDHH24MI')
    , @[act_crr_cd]
    , @[skd_rmk]
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
)
#else
INSERT INTO VSK_VSL_SKD_IBIS_IF
(
      VSL_CD
    , SKD_VOY_NO
    , SKD_DIR_CD
    , IBIS_IF_SEQ
    , IF_SND_CD
    , IF_MNPL_CD
    , VSL_SLAN_CD
    , SKD_STS_CD
    , SKD_STS_MNL_FLG
    , SKD_VOY_TP_CD
    , PF_SKD_TP_CD
    , ST_PORT_CD
    , N1ST_PORT_BRTH_DT
    , ACT_CRR_CD
    , SKD_RMK
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
) 
SELECT 
      VSL_CD
    , SKD_VOY_NO
    , SKD_DIR_CD
    , @[ibis_if_seq]
    , 'N'
    , 'I'
    , VSL_SLAN_CD
    , SKD_STS_CD
    , SKD_STS_MNL_FLG
    , SKD_VOY_TP_CD
    , PF_SKD_TP_CD
    , ST_PORT_CD
    , N1ST_PORT_BRTH_DT
    , ACT_CRR_CD
    , SKD_RMK
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
 FROM VSK_VSL_SKD
WHERE 1=1
  AND VSL_CD     = @[vsl_cd]
  AND SKD_VOY_NO = @[skd_voy_no]
  AND SKD_DIR_CD = @[skd_dir_cd]
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="ibis_if_seq" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="skd_sts_cd" type="12" value="" out="N"/>
				<param name="skd_sts_mnl_flg" type="12" value="" out="N"/>
				<param name="skd_voy_tp_cd" type="12" value="" out="N"/>
				<param name="pf_skd_tp_cd" type="12" value="" out="N"/>
				<param name="st_port_cd" type="12" value="" out="N"/>
				<param name="n1st_port_brth_dt" type="12" value="" out="N"/>
				<param name="act_crr_cd" type="12" value="" out="N"/>
				<param name="skd_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
