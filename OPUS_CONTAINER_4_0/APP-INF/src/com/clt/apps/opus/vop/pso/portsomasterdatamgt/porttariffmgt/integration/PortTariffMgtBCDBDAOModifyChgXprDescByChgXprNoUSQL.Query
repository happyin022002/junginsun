<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOModifyChgXprDescByChgXprNoUSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
UPDATE PSO_CHG_XPR A SET 
       A.XPR_DESC              = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 1)     
      ,A.DFLT_XPR_DESC         = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 2)  
      ,A.SYS_XPR_DESC          = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 3)  
      ,A.SYS_XPR_USR_DESC      = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 4)
      ,A.SYS_XPR_VAL_DESC      = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 5)
      ,A.DFLT_SYS_XPR_DESC     = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 6) 
      ,A.DFLT_SYS_XPR_USR_DESC = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 7)
      ,A.DFLT_SYS_XPR_VAL_DESC = PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 8)
      ,A.UPD_DT                = SYSDATE
WHERE  1=1
AND    A.CHG_XPR_NO = @[chg_xpr_no]
AND    PSO_STMT_CHK_FNC(PSO_GET_XPR_DESC_FNC(@[chg_xpr_no], 3), 3) = 'Y'	--XPR_DESC Validation			]]></sql>
			<params>
				<param name="chg_xpr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
