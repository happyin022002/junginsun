<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOAddVskCustEdiLogToKlnetCSQL">
			<desc><![CDATA[AddVskCustEdiLogToKlnet]]></desc>
			<sql><![CDATA[
INSERT INTO VSK_CUST_EDI_LOG
(
    ROUT_RCV_DT	, CUST_TRD_PRNR_ID	, ROUT_SEQ          , VSKD_EDI_SND_ID   ,
    N1ST_VSL_CD , N1ST_SKD_VOY_NO   , N1ST_SKD_DIR_CD   ,
    CRE_USR_ID  , CRE_DT            , UPD_USR_ID, UPD_DT,
    DIFF_RMK    
)
SELECT	TO_CHAR(SYSDATE, 'YYYYMMDD')	AS ROUT_RCV_DT
	,@[port_pos]					AS CUST_TRD_PRNR_ID
	,(	SELECT	NVL(MAX(ROUT_SEQ), 0) + 1
		FROM	VSK_CUST_EDI_LOG
		WHERE	ROUT_RCV_DT	= TO_CHAR(SYSDATE, 'YYYYMMDD')
		AND		VSKD_EDI_SND_ID = @[header_seq]
	)						AS ROUT_SEQ
        ,@[header_seq]
        ,@[vsl_cd]
        ,@[skd_voy_no]
        ,@[skd_dir_cd]
        ,@[upd_usr_id]
        ,SYSDATE
        ,@[upd_usr_id]
        ,SYSDATE
		,@[diff_rmk]
FROM	DUAL			]]></sql>
			<params>
				<param name="port_pos" type="12" value="" out="N"/>
				<param name="header_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
