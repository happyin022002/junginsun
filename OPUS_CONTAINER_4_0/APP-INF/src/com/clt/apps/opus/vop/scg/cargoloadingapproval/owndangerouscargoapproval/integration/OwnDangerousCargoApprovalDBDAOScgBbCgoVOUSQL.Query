<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgBbCgoVOUSQL">
			<desc><![CDATA[BKG에서 CANCEL 및 DELETE한 CARGO에 대해 SCG_BB_CGO에 수정 반영 한다.]]></desc>
			<sql><![CDATA[
UPDATE SCG_BB_CGO SET 
	SPCL_CGO_APRO_CD = @[spcl_cgo_apro_cd]
WHERE	BKG_NO = @[bkg_no]
AND	SPCL_CGO_APRO_RQST_SEQ = (	SELECT MAX(SPCL_CGO_APRO_RQST_SEQ) FROM SCG_APRO_RQST WHERE SPCL_CGO_CATE_CD = 'BB' AND BKG_NO = @[bkg_no]	)
AND	BB_CGO_SEQ = @[bb_cgo_seq]			]]></sql>
			<params>
				<param name="spcl_cgo_apro_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bb_cgo_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
