<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneExpenseRatioMgtDAOsearchRevenueDirectionByRevLaneRSQL">
			<desc><![CDATA[Rev Lane 에 연결된 Revenue direction 조회]]></desc>
			<sql><![CDATA[
SELECT RLANE_DIR_CD
  FROM AR_FINC_DIR_CONV
 WHERE SLAN_CD = @[slan_cd]
   AND RLANE_CD = @[rlane_cd]
   AND SLAN_DIR_CD = @[skd_dir_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
