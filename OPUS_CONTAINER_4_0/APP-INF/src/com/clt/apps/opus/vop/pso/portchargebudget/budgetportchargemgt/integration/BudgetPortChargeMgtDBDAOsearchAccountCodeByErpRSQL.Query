<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BudgetPortChargeMgtDBDAOsearchAccountCodeByErpRSQL">
			<desc><![CDATA[ERP Account Code Search]]></desc>
			<sql><![CDATA[
SELECT DISTINCT M.ACCT_CD
     , M.ACCT_ENG_NM
  FROM GL_ESTM_IF_ERP G
     , MDM_ACCOUNT M
 WHERE 1=1
   AND G.SYS_SRC_ID = 'PSO'
   AND G.EXE_YRMON = REPLACE(@[exe_yrmon], '-', '') 
#if( ${sdt} != '' )
   AND SUBSTR(G.ACT_DT,1,6) BETWEEN REPLACE(@[sdt], '-', '') AND REPLACE(@[edt], '-', '') 
#end 
   AND G.ACCT_CD = M.ACCT_CD
 ORDER BY 1			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
				<param name="sdt" type="12" value="" out="N"/>
				<param name="edt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
