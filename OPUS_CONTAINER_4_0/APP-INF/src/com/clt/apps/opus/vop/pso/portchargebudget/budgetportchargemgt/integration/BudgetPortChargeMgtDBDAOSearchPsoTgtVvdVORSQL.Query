<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BudgetPortChargeMgtDBDAOSearchPsoTgtVvdVORSQL">
			<desc><![CDATA[PSO_TGT_VVD 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT * FROM PSO_TGT_VVD
WHERE 1=1
AND PSO_BZTP_CD='2'
AND VSL_CD=@[vsl_cd]
AND SKD_VOY_NO=@[skd_voy_no]
AND SKD_DIR_CD=@[skd_dir_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
