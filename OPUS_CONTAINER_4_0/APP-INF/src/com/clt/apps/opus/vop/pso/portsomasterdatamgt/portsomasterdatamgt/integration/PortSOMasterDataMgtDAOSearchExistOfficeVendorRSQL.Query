<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortSOMasterDataMgtDAOSearchExistOfficeVendorRSQL">
			<desc><![CDATA[Search Exist Office Vendor]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*), 0, 'N','Y') AS EXIST_YN
  FROM PSO_INV_OFC_VNDR
 WHERE OFC_CD = @[ofc_cd]
   AND VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
