<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOTdrCntrDetailVOCSQL">
			<desc><![CDATA[Cntr
]]></desc>
			<sql><![CDATA[
INSERT INTO TDR_CNTR_DETAIL (
	VSL_CD
,	VOY_NO
,	DIR_CD
,	PORT_CD
,	CALL_IND
,	STATUS
,	CNTR_NO
,	CNTR_TYPE
,	CNTR_SIZE
,	CARGO_TYPE
,	OPR_CD
,	POR
,	POL
,	POD
,	POD2
,	POD_ISO
,	POD2_ISO
,	FPOD
----------------------------
,	SHIFT_TYPE
,	SHIFT_RSN
,	PARTY
,	ACCOUNT
----------------------------
,	DML
,	DMB
,	DMH
,	UNIT
,	POSITION
,	DELV_CD
,	OVF
,	OVA
,	OVP
,	OVS
,	OVH
,	SLOT
,	WEIGHT
,	CRANE
,	COMMENCE
,	COMPLETE
,	COMMODITY
,	GROSS_WORK
,	NET_WORK
,	LOST_HR
,	GROSS_GANG
,	NET_GANG
,	WIDTH
,	RF_CHK
,	DG_CHK
,	HC_CHK
,	MISHANDLE_CHK
,	DM_CHK
,	SHIFT_CHK
,	COD_CHK
,	TEMP_CHK
,	OPTION_CHK
,	TEMP
,	IMDG
,	UNNO
,	PSA_CLASS
,	PAGE_NO
,	PRECELL
,	DM_CODE

,	PRE_POD
,	COD_RSN
,	REMARK
,	SZTP
,	UPDATE_USER
,	UPDATE_TIME

) VALUES( 
	@[vsl_cd]
,	@[voy_no]
,	@[dir_cd]
,	@[port_cd]
,	@[call_ind]
,	@[status]
,	@[cntr_no]
,	@[cntr_type]
,	@[cntr_size]
,	@[cargo_type]
,	@[opr_cd]
,	@[por]
,	@[pol]
,	@[pod]
,	@[pod2]
,	NVL((SELECT   Y.INTER_CD_CTNT
     FROM     OPF_XTER_CD_CONV_MST   X
           ,  OPF_XTER_CD_CONV_DTL   Y
     WHERE    X.XTER_CD_KND_CTNT     = Y.XTER_CD_KND_CTNT
     AND      X.XTER_CD_KND_CTNT     = 'BLOCK_STOWAGE_OPTION_CGO'    
     AND      X.DELT_FLG             = 'N'
     AND      Y.DELT_FLG             = 'N'  
     AND      Y.XTER_CD_CTNT         = @[pod]), @[pod]) 
,	@[pod2]
,	@[fpod]
----------------------------
,	@[shift_type]
,	@[shift_rsn]
,	@[party]
,	@[account]
----------------------------
,	@[dml]
,	@[dmb]
,	@[dmh]
,	@[unit]
,	@[position]
,	@[delv_cd]
,	@[ovf]
,	@[ova]
,	@[ovp]
,	@[ovs]
,	@[ovh]
,	@[slot]
,	@[weight]
,	@[crane]
,	TO_DATE(@[commence],'YYYYMMDDHH24MI')
,	TO_DATE(@[complete],'YYYYMMDDHH24MI')
,	@[commodity]
,	@[gross_work]
,	@[net_work]
,	@[lost_hr]
,	@[gross_gang]
,	@[net_gang]
,	@[width]
,	@[rf_chk]
,	@[dg_chk]
,	@[hc_chk]
,	@[mishandle_chk]
,	@[dm_chk]
,	@[shift_chk]
,	@[cod_chk]
,	@[temp_chk]
,	@[option_chk]
,	@[temp]
,	@[imdg]
,	@[unno]
,	@[psa_class]
,	@[page_no]
,	@[precell]
,	@[dm_code]

,	@[pre_pod]
,	@[cod_rsn]
,	@[remark]
,	@[sztp]
,	@[update_user]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
