<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgIrrTpCdDupChkVORSQL">
			<desc><![CDATA[SPCL CGO Irregular Type (Creation) 조회   ]]></desc>
			<sql><![CDATA[
SELECT 
	SPCL_CGO_IRR_TP_CD
,	SPCL_CGO_IRR_TP_NM
,	SPCL_CGO_IRR_TP_DESC
,	DG_FLG
,	AWK_FLG
,	NVL(DELT_FLG,'N') AS DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_IRR_TP_CD
WHERE	1 = 1	
AND SPCL_CGO_IRR_TP_CD = @[spcl_cgo_irr_tp_cd]			]]></sql>
			<params>
				<param name="spcl_cgo_irr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
