<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOCheckVslSkdByRowIDListRSQL">
			<desc><![CDATA[CheckVslSkdByRowIDList]]></desc>
			<sql><![CDATA[
SELECT		T1.VSL_CD
		,	T1.SKD_VOY_NO
		,	T1.SKD_DIR_CD
		,	T2.VSL_SLAN_CD		AS SLAN_CD
		, 	T1.VPS_PORT_CD
		, 	T1.YD_CD
		, 	T1.SKD_CNG_STS_CD
		, 	TO_CHAR(T1.VPS_ETA_DT, 'YYYYMMDDHH24MI') AS VPS_ETA_DT
		, 	TO_CHAR(T1.VPS_ETB_DT, 'YYYYMMDDHH24MI') AS VPS_ETB_DT
		, 	TO_CHAR(T1.VPS_ETD_DT, 'YYYYMMDDHH24MI') AS VPS_ETD_DT

		,	'VIRTUAL'			AS BKG_VVD_KND
FROM		VSK_VSL_PORT_SKD 	T1
		, 	VSK_VSL_SKD 		T2
WHERE		1 = 1
AND			T1.VSL_CD        	= T2.VSL_CD
AND			T1.SKD_VOY_NO   	= T2.SKD_VOY_NO
AND			T1.SKD_DIR_CD    	= T2.SKD_DIR_CD
AND			T1.VSL_CD        	= @[vsl_cd]    		/* 변경 전 VSL_CD           */
AND			T1.TURN_SKD_VOY_NO  = @[skd_voy_no]    	/* 변경 전 SKD_VOY_NO       */
AND			T1.TURN_SKD_DIR_CD  = @[skd_dir_cd]    	/* 변경 전 SKD_DIR_CD       */

#if (${vps_port_cd} != '') 
AND			T1.VPS_PORT_CD  	 = @[vps_port_cd]   /* 변경 전 VPS_PORT_CD      */
#end

#if (${clpt_ind_seq} != '') 
AND			T1.TURN_CLPT_IND_SEQ = @[clpt_ind_seq]  /* 변경 전 CLPT_IND_SEQ     */
#end

AND	(
        EXISTS	(   SELECT  	'X'
                    FROM    	BKG_VVD 			S
							, 	BKG_BOOKING 		B
                    WHERE   	1 = 1
                    AND     	S.BKG_NO           	= B.BKG_NO
                    AND     	B.BKG_STS_CD      	!= 'X'
                    AND     	S.VSL_CD           	= T1.VSL_CD
                    AND     	S.SKD_VOY_NO       	= T1.SKD_VOY_NO
                    AND     	S.SKD_DIR_CD       	= T1.SKD_DIR_CD
                    AND     	S.POD_CD      		= T1.VPS_PORT_CD 
					AND			S.POD_CLPT_IND_SEQ	= T1.CLPT_IND_SEQ
                )
	)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
