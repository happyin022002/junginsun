<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOOwnApprovalRequestVORSQL">
			<desc><![CDATA[Mail Preview 항목들을 조회한다.]]></desc>
			<sql><![CDATA[
#if (${send_type} != '' && ${send_type} == 'P0')
--2016-01-28 FNC 분리 Partner Mail Contents 
SELECT
   M.FROM_PSN
 , M.TO_PSN
 , M.CC_PSN
 , M.SUBJECT
 , M.ATTACH_FILE
 , M.BODY_HEADER
 , M.BODY_FOOTER
 , M.BODY_CONTS
 , '' CRR_CD
 , '' BKG_REF_NO
 , '' SPCL_CGO_RQST_SEQ
 --, '' BKG_NO
 --, '' SPCL_CGO_APRO_RQST_SEQ
 , '' PRNR_CGO_RQST_SEQ
 --, '' VSL_PRE_PST_CD
 --, '' VSL_SEQ
 , '' RGN_SHP_OPR_CD
 , '' VSL_CD
 , '' SKD_VOY_NO
 , '' SKD_DIR_CD
 , '' POL_CD
 , '' POD_CD
 , '' SCG_FLG
 , '' SEND_TYPE
 , '' USER_ID
FROM TABLE(SCG_PRNR_RQST_MAIL_INFO_FNC(@[crr_cd],@[bkg_ref_no],@[spcl_cgo_rqst_seq],@[prnr_cgo_rqst_seq],@[rgn_shp_opr_cd],@[vsl_cd],@[skd_voy_no],@[skd_dir_cd],SUBSTR(@[pol_cd],0,5),SUBSTR(@[pod_cd],0,5),@[scg_flg],@[send_type],@[user_id])) M
#else 
--2016-01-28 FNC 분리 OWN Mail Contents 
SELECT
   M.FROM_PSN
 , M.TO_PSN
 , M.CC_PSN
 , M.SUBJECT
 , M.ATTACH_FILE
 , M.BODY_HEADER
 , M.BODY_FOOTER
 , M.BODY_CONTS
 , '' CRR_CD
 , '' BKG_REF_NO
 , '' SPCL_CGO_RQST_SEQ
 , '' BKG_NO
 , '' SPCL_CGO_APRO_RQST_SEQ
 , '' VSL_PRE_PST_CD
 , '' VSL_SEQ
 , '' RGN_SHP_OPR_CD
 , '' SCG_FLG
 , '' SEND_TYPE
 , '' USER_ID
FROM TABLE(SCG_OWN_MAIL_INFO_FNC(@[crr_cd],@[bkg_ref_no],@[spcl_cgo_rqst_seq],@[bkg_no],@[spcl_cgo_apro_rqst_seq],@[vsl_pre_pst_cd],@[vsl_seq],@[rgn_shp_opr_cd],@[scg_flg],@[send_type],@[user_id])) M
#end			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_rqst_seq" type="12" value="" out="N"/>
				<param name="prnr_cgo_rqst_seq" type="12" value="" out="N"/>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="scg_flg" type="12" value="" out="N"/>
				<param name="send_type" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
