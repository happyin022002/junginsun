<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SendEdiFromParnterLinesMgtDBDAOScgPrnrAproRqstUSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
UPDATE	SCG_VVD_APRO_RQST
   SET	MAPG_TRSM_BND_CD             	= @[mapg_trsm_bnd_cd]
     ,	MAPG_TRSM_DT                 	= TO_DATE(@[mapg_trsm_dt],'yyyy-MM-dd')
     ,	MAPG_TRSM_SPCL_CGO_CATE_CD   	= @[mapg_trsm_spcl_cgo_cate_cd]
     ,	MAPG_PRNR_SPCL_CGO_SEQ       	= @[mapg_prnr_spcl_cgo_seq]
     ,	MAPG_EDI_TRSM_STS_CD         	= @[mapg_edi_trsm_sts_cd]
#if (${mapg_edi_trsm_sts_cd} == 'SX') 
     ,	CXL_EDI_SND_OFC_CD           	= NVL(@[cxl_edi_snd_ofc_cd],'AUTO_CXL')     
     ,	CXL_EDI_SND_DT               	= SYSDATE
#end

#if (${edi_status} == 'EDI_AUTO_CXL')
	,	EDI_AUTO_CXL_FLG       			= 'Y'
#end

WHERE 	BKG_NO                       	= @[bkg_no]
AND 	SPCL_CGO_APRO_RQST_SEQ       	= @[spcl_cgo_apro_rqst_seq]
AND 	VSL_PRE_PST_CD               	= @[vsl_pre_pst_cd]
AND 	VSL_SEQ                      	= @[vsl_seq]			]]></sql>
			<params>
				<param name="mapg_trsm_bnd_cd" type="12" value="" out="N"/>
				<param name="mapg_trsm_dt" type="12" value="" out="N"/>
				<param name="mapg_trsm_spcl_cgo_cate_cd" type="12" value="" out="N"/>
				<param name="mapg_prnr_spcl_cgo_seq" type="12" value="" out="N"/>
				<param name="mapg_edi_trsm_sts_cd" type="12" value="" out="N"/>
				<param name="cxl_edi_snd_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
