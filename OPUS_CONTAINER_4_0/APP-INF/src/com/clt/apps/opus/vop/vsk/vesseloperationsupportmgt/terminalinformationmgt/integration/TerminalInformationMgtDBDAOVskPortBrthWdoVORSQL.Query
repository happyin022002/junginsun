<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalInformationMgtDBDAOVskPortBrthWdoVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	A.YD_CD
,	B.YD_NM
,	CASE WHEN C.VSL_SLAN_CD IS NULL THEN A.REF_SLAN_NM ELSE C.VSL_SLAN_CD END AS REF_SLAN_NM
,	A.SKD_DIR_CD
,	A.CRR_CD
,	A.LOC_CD
,	A.ETB_DY_CD
,	A.ETB_TM_HRMNT
,	A.ETD_DY_CD
,	A.ETD_TM_HRMNT
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,   to_char(A.UPD_DT, 'YYYY-MM-DD HH24:MI') AS UPD_DT
,   A.BRTH_SEQ
FROM 	VSK_PORT_BRTH_WDO	A,
		MDM_YARD			B,
		MDM_VSL_SVC_LANE    C
WHERE	A.YD_CD	=	B.YD_CD
AND     A.REF_SLAN_NM  = C.VSL_SLAN_NM  (+)
AND		A.LOC_CD = @[loc_cd]
#if (${flt_type} != '') 
AND	nvl(c.vskd_flet_grp_cd, '-') like @[flt_type]
#end
ORDER BY A.YD_CD, C.VSL_SLAN_CD, A.SKD_DIR_CD, A.CRR_CD			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="flt_type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
