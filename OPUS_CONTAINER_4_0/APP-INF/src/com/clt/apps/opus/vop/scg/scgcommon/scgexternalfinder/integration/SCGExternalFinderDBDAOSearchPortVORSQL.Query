<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGExternalFinderDBDAOSearchPortVORSQL">
			<desc><![CDATA[VVD내의 Port 목록 조회]]></desc>
			<sql><![CDATA[
SELECT 
       RSO.RGN_SHP_OPR_CD
     , RSO.DELT_FLG     
     , ML.LOC_CD
     , ML.LOC_NM
     , ML.SCONTI_CD
     , PS.VSL_CD
     , PS.SKD_VOY_NO
     , PS.SKD_DIR_CD
     , PS.CLPT_SEQ
     , PS.CLPT_IND_SEQ
     , TO_CHAR(PS.VPS_ETA_DT,'YYYY-MM-DD HH24:MI') VPS_ETA_DT
     , PS.TURN_PORT_IND_CD
     , PS.YD_CD
     , PS.SKD_CNG_STS_CD
     , PS.VT_ADD_CALL_FLG
  FROM MDM_LOCATION 		ML
     , VSK_VSL_PORT_SKD 	PS
     , SCG_RGN_SHP_OPR_PORT RSO
 WHERE ML.LOC_CD 			= PS.VPS_PORT_CD
	AND ML.LOC_CD           = RSO.LOC_CD
    AND RSO.DELT_FLG        = 'N'   

#if (${vsl_cd} != '') 
   AND PS.VSL_CD 			= @[vsl_cd]
#end
#if (${skd_voy_no} != '') 
   AND PS.SKD_VOY_NO 		= @[skd_voy_no]
#end
#if (${skd_dir_cd} != '') 
   AND PS.SKD_DIR_CD 		= @[skd_dir_cd]
#end  
#if (${loc_cd} != '') 
   AND ML.LOC_CD 			= @[loc_cd]
#end
#if (${clpt_ind_seq} != '') 
   AND PS.CLPT_IND_SEQ		= @[clpt_ind_seq]
#end
 GROUP BY
      RSO.RGN_SHP_OPR_CD
     , RSO.DELT_FLG     
     , ML.LOC_CD
     , ML.LOC_NM
     , ML.SCONTI_CD
     , PS.VSL_CD
     , PS.SKD_VOY_NO
     , PS.SKD_DIR_CD
     , PS.CLPT_SEQ
     , PS.CLPT_IND_SEQ
     , PS.VPS_ETA_DT
     , PS.TURN_PORT_IND_CD
     , PS.YD_CD
     , PS.SKD_CNG_STS_CD
     , PS.VT_ADD_CALL_FLG
 ORDER BY
       PS.CLPT_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
