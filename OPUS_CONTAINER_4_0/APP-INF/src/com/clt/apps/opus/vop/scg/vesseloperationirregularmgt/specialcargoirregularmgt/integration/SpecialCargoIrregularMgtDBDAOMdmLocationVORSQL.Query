<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoIrregularMgtDBDAOMdmLocationVORSQL">
			<desc><![CDATA[SPCL CGO Irregular List의 Location Combo 조회]]></desc>
			<sql><![CDATA[
SELECT
    MLT.LOC_CD
FROM MDM_LOCATION MLT
   , SCG_IRR_CNTR SIC
   , BKG_BOOKING   BBK
WHERE 1=1
#if (${loc_tp_cd} == 'POR') 
       AND MLT.LOC_CD = BBK.POR_CD
#end
#if (${loc_tp_cd} == 'POL') 
       AND MLT.LOC_CD = BBK.POL_CD
#end
#if (${loc_tp_cd} == 'POD') 
       AND MLT.LOC_CD = BBK.POD_CD
#end
#if (${loc_tp_cd} == 'DEL') 
       AND MLT.LOC_CD = BBK.DEL_CD
#end
AND BBK.BKG_NO = SIC.BKG_NO
GROUP BY MLT.LOC_CD
ORDER BY MLT.LOC_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
