<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgAproRqstVOUSQL">
			<desc><![CDATA[Booking로부터 SCG_APRO_RQST를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE SCG_APRO_RQST SET 
	SPCL_BKG_RQST_FLG = @[spcl_bkg_rqst_flg]
,	SPCL_CGO_CATE_CD = @[spcl_cgo_cate_cd]
,	POR_CD = @[por_cd]
,	DEL_CD = @[del_cd]
,	BKG_NO = @[bkg_no]
,	AWK_CGO_QTY = @[awk_cgo_qty]
,	BB_CGO_QTY = @[bb_cgo_qty]
,	DCGO_QTY = @[dcgo_qty]
,	RC_QTY = @[rc_qty]
,	BKG_RCV_TERM_CD = @[bkg_rcv_term_cd]
,	BKG_DE_TERM_CD = @[bkg_de_term_cd]
,	RQST_USR_ID = @[rqst_usr_id]
,	RQST_OFC_CD = @[rqst_ofc_cd]
,	RQST_DT = NVL(GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),SYSDATE,BKG_COM_USER_LOC_FNC(@[upd_usr_id])), SYSDATE)
,	RQST_GDT = NVL(GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),SYSDATE,'GMT'), SYSDATE)
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no]
AND	SPCL_CGO_APRO_RQST_SEQ = (	SELECT MAX(SPCL_CGO_APRO_RQST_SEQ) FROM SCG_APRO_RQST WHERE BKG_NO = @[bkg_no]	)			]]></sql>
			<params>
				<param name="spcl_bkg_rqst_flg" type="12" value="" out="N"/>
				<param name="spcl_cgo_cate_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="awk_cgo_qty" type="6" value="" out="N"/>
				<param name="bb_cgo_qty" type="6" value="" out="N"/>
				<param name="dcgo_qty" type="6" value="" out="N"/>
				<param name="rc_qty" type="6" value="" out="N"/>
				<param name="bkg_rcv_term_cd" type="12" value="" out="N"/>
				<param name="bkg_de_term_cd" type="12" value="" out="N"/>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
