<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortSOMasterDataMgtDAOSearchExistExchangeRateRSQL">
			<desc><![CDATA[GL_MON_XCH_RT 존재여부]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(ACCT_XCH_RT_YRMON) > 0 THEN 'Y' ELSE 'N' END EXIST_YN
  FROM GL_MON_XCH_RT
 WHERE ACCT_XCH_RT_LVL = '1'
   AND CURR_CD = @[curr_cd]
   AND ACCT_XCH_RT_YRMON = SUBSTR(REPLACE(@[iss_dt], '-', ''), 1, 6)			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="iss_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
