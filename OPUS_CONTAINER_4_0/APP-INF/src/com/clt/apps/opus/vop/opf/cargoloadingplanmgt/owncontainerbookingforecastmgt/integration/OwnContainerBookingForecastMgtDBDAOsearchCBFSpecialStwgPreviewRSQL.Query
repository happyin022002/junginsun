                                     SUM(DECODE(STWG_CD,@[st_8],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'5',1,0),0)) ST8_4H,
                                     SUM(DECODE(STWG_CD,@[st_8],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'7',1,0),0)) ST8_45,
                                     SUM(DECODE(STWG_CD,@[st_9],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'2',1,0),0))+SUM(DECODE(STWG_CD,@[st_9],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'A',1,0),0)) ST9_20,
                                     SUM(DECODE(STWG_CD,@[st_9],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'3',1,0),0)) ST9_2H,
                                     SUM(DECODE(STWG_CD,@[st_9],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'4',1,0),0))+SUM(DECODE(STWG_CD,@[st_9],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'B',1,0),0)) ST9_40,
                                     SUM(DECODE(STWG_CD,@[st_9],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'5',1,0),0)) ST9_4H,
