                                     SUM(DECODE(STWG_CD,@[st_12],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'3',1,0),0)) ST12_2H,
                                     SUM(DECODE(STWG_CD,@[st_12],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'4',1,0),0))+SUM(DECODE(STWG_CD,@[st_12],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'B',1,0),0)) ST12_40,
                                     SUM(DECODE(STWG_CD,@[st_12],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'5',1,0),0)) ST12_4H,
                                     SUM(DECODE(STWG_CD,@[st_12],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'7',1,0),0)) ST12_45,
                                     SUM(DECODE(STWG_CD,@[st_13],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'2',1,0),0))+SUM(DECODE(STWG_CD,@[st_13],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'A',1,0),0)) ST13_20,
                                     SUM(DECODE(STWG_CD,@[st_13],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'3',1,0),0)) ST13_2H,
                                     SUM(DECODE(STWG_CD,@[st_13],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'4',1,0),0))+SUM(DECODE(STWG_CD,@[st_13],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'B',1,0),0)) ST13_40,
                                     SUM(DECODE(STWG_CD,@[st_13],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'5',1,0),0)) ST13_4H,
