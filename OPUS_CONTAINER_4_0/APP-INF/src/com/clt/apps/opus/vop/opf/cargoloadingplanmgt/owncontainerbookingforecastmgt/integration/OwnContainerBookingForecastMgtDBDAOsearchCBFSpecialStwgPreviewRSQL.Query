                                     SUM(DECODE(STWG_CD,@[st_15],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'2',1,0),0))+SUM(DECODE(STWG_CD,@[st_15],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'A',1,0),0)) ST15_20,
                                     SUM(DECODE(STWG_CD,@[st_15],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'3',1,0),0)) ST15_2H,
                                     SUM(DECODE(STWG_CD,@[st_15],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'4',1,0),0))+SUM(DECODE(STWG_CD,@[st_15],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'B',1,0),0)) ST15_40,
                                     SUM(DECODE(STWG_CD,@[st_15],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'5',1,0),0)) ST15_4H,
                                     SUM(DECODE(STWG_CD,@[st_15],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'7',1,0),0)) ST15_45,

                                     MIN(ROW_NUM) ROW_NUM
                              FROM ( SELECT DISTINCT 
                                            POD,
                                            POD_CLPT_IND_SEQ,
                                            OPR,
                                            MLB,
                                            BKG_NO,
                                            CNTR_NO,
                                            STWG_CD,
                                            CNTR_TPSZ_CD,
                                            ROW_NUM
                                     FROM   ( SELECT D.POD_CD   POD,
                                                     D.POD_CLPT_IND_SEQ,
                                                     D.CRR_CD   OPR,
                                                     D.MLB_CD   MLB,
                                                     DECODE(H.BKG_SHPR_OWNR_FLG, 'Y', D.BKG_NO, D.PRNR_BKG_REF_NO) BKG_NO,
                                                     DECODE(H.BKG_SHPR_OWNR_FLG, 'Y', D.CNTR_NO, D.PRNR_CNTR_REF_NO) CNTR_NO,
                                                     D.STWG_CD,
                                                     D.CNTR_TPSZ_CD,
                                                     ( CASE WHEN D.CRR_CD = @[qty1] THEN 1
                                                            WHEN D.CRR_CD = @[qty2] THEN 2                                                                                              
                                                            WHEN D.CRR_CD = @[qty3] THEN 3                                                                                              
                                                            WHEN D.CRR_CD = @[qty4] THEN 4                                                                                              
                                                            WHEN D.CRR_CD = @[qty5] THEN 5                                                                                              
                                                       END) ROW_NUM,
                                                     DECODE(DECODE(H.BKG_SHPR_OWNR_FLG, 'Y', D.CNTR_NO, D.PRNR_CNTR_REF_NO),NULL,1,ROW_NUMBER() OVER (PARTITION BY D.CRR_CD, DECODE(H.BKG_SHPR_OWNR_FLG, 'Y', D.CNTR_NO, D.PRNR_CNTR_REF_NO), D.CNTR_SEQ, D.STWG_CD ORDER BY DECODE(H.BKG_SHPR_OWNR_FLG, 'Y', D.BKG_NO, D.PRNR_BKG_REF_NO))) CNTR_RN
                                              FROM   OPF_CGO_BKG_FCAST H, OPF_CGO_BKG_FCAST_CNTR D
