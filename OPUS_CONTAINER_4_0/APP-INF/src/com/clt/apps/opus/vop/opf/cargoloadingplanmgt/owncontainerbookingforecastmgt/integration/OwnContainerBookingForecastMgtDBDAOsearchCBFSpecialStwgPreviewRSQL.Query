                                     SUM(DECODE(STWG_CD,@[st_10],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'3',1,0),0)) ST10_2H,
                                     SUM(DECODE(STWG_CD,@[st_10],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'4',1,0),0))+SUM(DECODE(STWG_CD,@[st_10],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'B',1,0),0)) ST10_40,
                                     SUM(DECODE(STWG_CD,@[st_10],DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'5',1,0),0)) ST10_4H,
