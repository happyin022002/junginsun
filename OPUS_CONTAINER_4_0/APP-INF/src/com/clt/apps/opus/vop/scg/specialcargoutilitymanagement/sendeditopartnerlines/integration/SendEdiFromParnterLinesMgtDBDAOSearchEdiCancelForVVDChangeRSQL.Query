<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SendEdiFromParnterLinesMgtDBDAOSearchEdiCancelForVVDChangeRSQL">
			<desc><![CDATA[SearchEdiCancelForVVDChange]]></desc>
			<sql><![CDATA[
SELECT    H.BKG_REF_NO                						AS BKG_NO
       ,  TO_CHAR(H.BKG_CRE_LOCL_DT, 'YYYYMMDDHH24MI')     	AS BKG_DT
       ,  H.CGO_OPR_CD
       ,  H.SLAN_CD
       
       ,  H.VSL_CD
       ,  H.VSL_ENG_FULL_NM                                	AS VSL_ENG_NM
       ,  H.CALL_SGN_NO
       ,  H.LLOYD_NO       
       ,  H.SKD_VOY_NO
       ,  H.SKD_DIR_CD
       
       ,  H.POR_CD
       ,  H.POR_NM
       ,  H.POR_YD_CD
       
       ,  H.POL_CD
       ,  H.POL_NM
       ,  H.POL_YD_CD                                      	AS POL_NOD_CD
       ,  H.POL_CLPT_IND_SEQ
       
       ,  H.POD_CD
       ,  H.POD_NM
       ,  H.POD_YD_CD                                      	AS POD_NOD_CD
       ,  H.POD_CLPT_IND_SEQ
       
       ,  H.DEL_CD
       ,  H.DEL_NM
       ,  H.DEL_YD_CD
       
       ,  H.OUT_BND_CSSM_VOY_NO								AS OB_CSSM_VOY_NO
       
FROM      SCG_PRNR_SPCL_CGO_TRSM_HDR  H
WHERE     (H.TRSM_BND_CD,H.TRSM_DT,H.SPCL_CGO_CATE_CD,H.PRNR_SPCL_CGO_SEQ)
          IN
          (
          -----------------------------------------------------
          SELECT     YY.MAPG_TRSM_BND_CD
                  ,  YY.MAPG_TRSM_DT
                  ,  YY.MAPG_TRSM_SPCL_CGO_CATE_CD
                  ,  YY.MAPG_PRNR_SPCL_CGO_SEQ
          FROM       SCG_VVD_APRO_RQST          YY
          WHERE      YY.BKG_NO                  = @[bkg_no]
          AND        YY.SPCL_CGO_APRO_RQST_SEQ  = @[spcl_cgo_apro_rqst_seq]
          AND        YY.VSL_CD                  = @[vsl_cd]
          AND        YY.SKD_VOY_NO              = @[skd_voy_no]
          AND        YY.SKD_DIR_CD              = @[skd_dir_cd]
          AND        YY.MAPG_EDI_TRSM_STS_CD    = 'S'
          -----------------------------------------------------
          )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
