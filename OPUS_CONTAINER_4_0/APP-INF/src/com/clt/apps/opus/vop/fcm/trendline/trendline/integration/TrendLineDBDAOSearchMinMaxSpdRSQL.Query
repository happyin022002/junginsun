<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrendLineDBDAOSearchMinMaxSpdRSQL">
			<desc><![CDATA[VSL에 해당하는 MIN, MAX SPD를 구한다.(VSL이 여러 종류인 경우, 가장 작은 MAX & 가장 큰 MIN)]]></desc>
			<sql><![CDATA[
SELECT MAX(OP_MIN_SPD) MIN_SPD, MIN(OP_MAX_SPD) MAX_SPD FROM FCM_VSL_CNTR_RGST
WHERE 1=1
#if( ${arr_vsl_cd}!='')
AND VSL_CD IN (#foreach( $key IN ${arr_vsl_cd}) 
	      	     #if($velocityCount < $arr_vsl_cd.size())
  	                '$key',
	             #else
	                '$key'
	             #end
	           #end)
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
