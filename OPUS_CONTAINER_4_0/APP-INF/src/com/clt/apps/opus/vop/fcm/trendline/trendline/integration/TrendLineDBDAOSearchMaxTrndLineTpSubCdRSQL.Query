<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrendLineDBDAOSearchMaxTrndLineTpSubCdRSQL">
			<desc><![CDATA[새로 생성할 Trnd Line의 TRND_LINE_TP_SUB_CD를 구한다]]></desc>
			<sql><![CDATA[
SELECT LPAD(NVL(MAX(TRND_LINE_TP_SUB_CD),0)+1,3,0) TRND_LINE_TP_SUB_CD
FROM FCM_TRND_LINE
WHERE TRND_LINE_USE_TP_CD=@[trnd_line_use_tp_cd]
AND TRND_LINE_TP_CD=@[trnd_line_tp_cd]
#if (${trnd_line_tp_cd} == '1' || ${trnd_line_tp_cd} == '2') 
AND VSL_CLSS_CD=@[vsl_clss_cd]
AND VSL_SLAN_CD=@[vsl_slan_cd]
#if (${vsl_clss_sub_cd} != '')
AND VSL_CLSS_SUB_CD=@[vsl_clss_sub_cd]
#end
#elseif (${trnd_line_tp_cd} == '3' || ${trnd_line_tp_cd} == '4') 
AND VSL_CD=@[vsl_cd]
AND SKD_DIR_CD=@[skd_dir_cd]
#end
AND TO_CHAR(CRE_DT,'YYMM')=TO_CHAR(SYSDATE,'YYMM')			]]></sql>
			<params>
				<param name="trnd_line_use_tp_cd" type="12" value="" out="N"/>
				<param name="trnd_line_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_sub_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
