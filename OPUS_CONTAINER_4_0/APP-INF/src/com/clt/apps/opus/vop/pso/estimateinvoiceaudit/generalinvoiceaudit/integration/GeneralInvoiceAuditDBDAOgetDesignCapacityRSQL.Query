<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetDesignCapacityRSQL">
			<desc><![CDATA[getDesignCapacity]]></desc>
			<sql><![CDATA[
SELECT   CNTR_DZN_CAPA
  FROM   mdm_vsl_cntr
 WHERE   VSL_CD = substr(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
