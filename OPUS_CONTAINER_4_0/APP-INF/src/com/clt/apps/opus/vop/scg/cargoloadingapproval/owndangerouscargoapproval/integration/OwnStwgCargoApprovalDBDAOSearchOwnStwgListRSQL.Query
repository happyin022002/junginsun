				#end
				#if (${auth_flg} == 'Y') 
					AND G.SPCL_CGO_AUTH_CD 			= 'Y'
				#end
				#if (${auth_flg} == 'N') 
					AND G.SPCL_CGO_AUTH_CD 			= 'N'
				#end
				#if (${auth_flg} == 'U') 
					AND NVL(G.SPCL_CGO_AUTH_CD,'R') IN ( 'R','P')
				#end
				
				#if (${auth_flg} == 'YN') 
					AND G.SPCL_CGO_AUTH_CD 			IN ('Y','N')
				#end
				
				#if (${apro_ref_no} != '') 
					AND G.APRO_REF_NO 				= @[apro_ref_no]
				#end

            ) A
 WHERE 1 = 1
    #if (${scg_flg} == 'SS') 
        AND     A.RQST_DT                     > SYSTIMESTAMP - 300
        AND     A.VPS_ETA_DT                 > SYSTIMESTAMP - 300
        AND     A.SPCL_CGO_APRO_CD             NOT IN ('C','D','N')
        AND     A.BKG_STS_CD                 != 'X'
    #else
        AND     A.BKG_STS_CD                 != 'X'
        --AND     A.SPCL_CGO_APRO_RQST_SEQ     = A.SPCL_CGO_APRO_RQST_SEQ(+)
        AND     A.SPCL_CGO_APRO_CD             NOT IN ('C','D')    --@@
    #end
    
    #if (${rgn_shp_opr_cd} != '') 
