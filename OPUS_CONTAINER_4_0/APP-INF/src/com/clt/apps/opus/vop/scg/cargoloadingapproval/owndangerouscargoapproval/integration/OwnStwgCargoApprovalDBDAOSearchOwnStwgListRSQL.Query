    #end

    #if (${imdg_clss_cd} != '') 
        AND     A.IMDG_CLSS_CD         = @[imdg_clss_cd]
    #end

    #if (${skd_voy_no} != '') 
        AND        A.SKD_VOY_NO         IN ( @[skd_voy_no] )
    #end

    #if (${skd_dir_cd} != '') 
        AND     A.SKD_DIR_CD         IN ( @[skd_dir_cd] )
    #end
    
    #if (${pol_cd} != '') 
