<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScgUtilEAIDAOSearchEdiSendHeaderRSQL">
			<desc><![CDATA[ScgUtilEAIDAOSearchEdiSendHeaderR]]></desc>
			<sql><![CDATA[
SELECT '$$$MSGSTART:' AS HEADER
     , RPAD(NVL(COM_ConstantMgr_PKG.COM_getCompanyCode_FNC(),' '),20,' ') AS SENDER_ID
     , RPAD(NVL(CASE WHEN @[crr_cd] IN ('HLC','HSD') THEN 
                     (SELECT 'HLC'|| DECODE(CONTI_CD, 'A', 'AS', 'E', 'EU', 'M', 'US') 
                        FROM MDM_LOCATION 
                       WHERE LOC_CD = @[loc_cd] 
                         AND NVL(DELT_FLG,'N') <> 'Y'
                     ) 
                ELSE @[crr_cd] END ,' '),20,' ') AS RECEIVER_ID
     , RPAD(NVL('IFTMBF',' '),10,' ')            AS MESSAGE_TYPE
  FROM DUAL			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
