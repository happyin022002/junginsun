<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgAproRqstIfRmkUSQL">
			<desc><![CDATA[ScgAproRqstIfRmk]]></desc>
			<sql><![CDATA[
UPDATE	SCG_APRO_RQST 				RQ
SET		RQ.IF_RMK					= SUBSTR(RQ.IF_RMK,1,3000)||' <<<>>> '
									  ||CHR(10)||'-------------------------------------------------------------'
									  ||CHR(10)||' > '||'I/F TIME : ['||TO_CHAR(SYSTIMESTAMP,'YYYY-MM-DD HH24:MI:SS FF6')||']'
									  ||CHR(10)||' > '||NVL(@[if_rmk]	,'Update Interfaced Result by System')
									  ||CHR(10)||'============================================================='
									  ||CHR(10)
WHERE	RQ.BKG_NO           		= @[bkg_no]
AND		RQ.SPCL_CGO_APRO_RQST_SEQ 	= @[spcl_cgo_apro_rqst_seq]			]]></sql>
			<params>
				<param name="if_rmk" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
