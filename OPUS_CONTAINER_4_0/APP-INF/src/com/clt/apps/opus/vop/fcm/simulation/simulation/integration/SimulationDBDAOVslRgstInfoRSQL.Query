<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SimulationDBDAOVslRgstInfoRSQL">
			<desc><![CDATA[Simulation에 필요한 Vessel Registration 정보를 조회한다.]]></desc>
			<sql><![CDATA[
#if (${trnd_line_tp_cd} == '1' || ${trnd_line_tp_cd} == '2') 
SELECT
MAX(OP_MIN_SPD) OP_MIN_SPD,
MAX(OP_MAX_SPD) OP_MAX_SPD,
MAX(OP_GNR_SPD) OP_GNR_SPD,
MAX(MNVR_CSM_WGT) MNVR_CSM_WGT,
MAX(GNR_CSM_WGT) GNR_CSM_WGT
FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
WHERE T1.VSL_CD=T2.VSL_CD
AND T1.CNTR_DZN_CAPA=@[cntr_dzn_capa]
#elseif (${trnd_line_tp_cd} == '3' || ${trnd_line_tp_cd} == '4') 
SELECT OP_MIN_SPD, OP_MAX_SPD, OP_GNR_SPD, MNVR_CSM_WGT, GNR_CSM_WGT
FROM FCM_VSL_CNTR_RGST
WHERE 1=1
AND VSL_CD=@[vsl_cd]
#else 
SELECT 0 OP_MIN_SPD, 0 OP_MAX_SPD, 0 OP_GNR_SPD, 0 MNVR_CSM_WGT, 0 GNR_CSM_WGT
FROM DUAL
#end			]]></sql>
			<params>
				<param name="cntr_dzn_capa" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
