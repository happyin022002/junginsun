<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OperationNPerformMasterDataMgtDBDAOMdmLocationVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT     DISTINCT
           ML.LOC_CD
      ,    NVL(ML.VOP_PORT_RHQ_CD,MO.AR_HD_QTR_OFC_CD)  AS VOP_PORT_RHQ_CD
      ,    'N'     										AS VOP_PORT_FLG
FROM       MDM_LOCATION  		ML
      ,    MDM_ORGANIZATION 	MO
WHERE      1 = 1
AND        ML.LOC_CD        	= MO.LOC_CD
AND        ML.DELT_FLG   		= 'N'
AND        ML.CALL_PORT_FLG   	= 'Y' 
AND        MO.AR_HD_QTR_OFC_CD 	<> 'SINHO'   
--AND        ML.VOP_PORT_RHQ_CD IS NOT NULL
ORDER BY   ML.LOC_CD			ASC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
