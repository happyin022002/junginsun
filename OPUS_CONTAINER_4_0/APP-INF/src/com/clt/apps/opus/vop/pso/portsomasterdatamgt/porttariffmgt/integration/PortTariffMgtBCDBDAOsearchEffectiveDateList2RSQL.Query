<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchEffectiveDateList2RSQL">
			<desc><![CDATA[traff 일자의 version을 조회]]></desc>
			<sql><![CDATA[
select      t1. yd_chg_no  ,  
decode(  upd_mnu_no , '1' ,   'TL_'  ||  t1.yd_chg_ver_seq ,  'SF_'  ||  t1.yd_chg_ver_seq  )  ver  ,
 to_char(  eff_dt, 'YYYY-mm-dd') eff_dt ,  
to_char(  exp_dt , 'YYYY-mm-dd')  exp_dt 
from pso_yd_chg t1,  pso_yd_chg_xpr t2 , pso_chg_xpr t3 ,
(
    select  t2.acct_cd, t2.lgs_cost_cd, lgs_cost_full_nm
    from    pso_inv_ofc_cost t1, tes_lgs_cost t2
    where   t1.lgs_cost_cd = t2.lgs_cost_cd
    and     t1.ofc_cd      = @[ofc_cd]
    and     t2.acct_cd          <> '999999'
    and length(t2.LGS_COST_CD) > 4
    order by 1
) T4
where  t1.yd_chg_no = t2.yd_chg_no
           and  t1.yd_chg_ver_seq = t2.yd_chg_ver_seq
           and t2.chg_xpr_no = t3.chg_xpr_no 
AND t1.LGS_COST_CD      = T4.lgs_cost_cd
AND  YD_CD            = @[combo1]
AND  VNDR_SEQ         = @[vndr_seq]
AND  T4.acct_cd       = @[acct_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="combo1" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
