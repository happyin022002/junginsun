<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetPsoTrfTpCdRSQL">
			<desc><![CDATA[getPsoTrfTpCd]]></desc>
