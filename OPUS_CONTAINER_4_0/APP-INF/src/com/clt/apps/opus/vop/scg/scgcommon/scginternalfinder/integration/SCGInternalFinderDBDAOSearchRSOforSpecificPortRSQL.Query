<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGInternalFinderDBDAOSearchRSOforSpecificPortRSQL">
			<desc><![CDATA[Retrieving RSO for Specific Port Code]]></desc>
			<sql><![CDATA[
SELECT    X.RGN_SHP_OPR_CD
       ,  X.LOC_CD
FROM      SCG_RGN_SHP_OPR_PORT  X
WHERE     X.LOC_CD              = @[loc_cd]
AND       X.DELT_FLG            = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
