<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceScheduleToExternalDBDAOCreateVskVipsIfHdrCSQL">
			<desc><![CDATA[VIPS I/F HEADER 생성]]></desc>
			<sql><![CDATA[
INSERT INTO VSK_VSL_SKD_VIPS_IF_HDR
	(
	 VSL_CD
	,SKD_VOY_NO
	,VIPS_IF_SEQ

	 --::COLS FOR EAI I/F::--
	,INSF_ID
	,INSF_PRS_ID
	----,INSF_DTTMIN
	,INSF_CNQE_VAL
	,INSF_DV_CD
	,VIPS_RUN_UT_NO
	 --::COLS FOR EAI I/F::--

	,VIPS_IF_TGT_FLG
	,VIPS_IF_RMK

	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
	)

VALUES

     ( @[vsl_cd]
     , @[skd_voy_no]
     , @[vips_if_seq]

	 --::COLS FOR EAI I/F::--
	 , 'OPVIPS01'
     , ''
     ----, SYSDATE --[insf_dttm]
     , @[insf_cnqe_val]			--: 'S':SUCCESS, 'E':ERROR :--
	 , @[insf_dv_cd]			--: 'I':INSERT , 'U':UPDATE, 'D':DELETE :--
	 , @[vips_run_ut_no]		--: Run Unit Number :--
	 --::COLS FOR EAI I/F::--

     , @[vips_if_tgt_flg]		--: VIPS I/F TARGET INDICATOR :--
	 , @[vips_if_rmk]

     , 'VIPS_IF'
     , SYSDATE
     , 'VIPS_IF'
     , SYSDATE	
	)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="vips_if_seq" type="12" value="" out="N"/>
				<param name="insf_cnqe_val" type="12" value="" out="N"/>
				<param name="insf_dv_cd" type="12" value="" out="N"/>
				<param name="vips_run_ut_no" type="12" value="" out="N"/>
				<param name="vips_if_tgt_flg" type="12" value="" out="N"/>
				<param name="vips_if_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
