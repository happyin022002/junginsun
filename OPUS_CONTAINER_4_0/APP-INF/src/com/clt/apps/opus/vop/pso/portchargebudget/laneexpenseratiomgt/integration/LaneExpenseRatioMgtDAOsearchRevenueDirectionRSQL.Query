<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneExpenseRatioMgtDAOsearchRevenueDirectionRSQL">
			<desc><![CDATA[Revenue direction Search]]></desc>
			<sql><![CDATA[
SELECT SLAN_CD||'|'||RLANE_CD||'|'||SLAN_DIR_CD||'|'||RLANE_DIR_CD AS DATA
  FROM AR_FINC_DIR_CONV
 WHERE SLAN_CD = @[slan_cd]
   AND RLANE_CD = @[rlane_cd]
   AND SLAN_DIR_CD = @[skd_dir_cd]
   AND RLANE_DIR_CD = @[rlane_dir_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rlane_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
