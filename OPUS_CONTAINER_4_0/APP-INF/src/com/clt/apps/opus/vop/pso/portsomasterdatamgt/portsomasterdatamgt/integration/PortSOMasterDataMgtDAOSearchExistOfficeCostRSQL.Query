<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortSOMasterDataMgtDAOSearchExistOfficeCostRSQL">
			<desc><![CDATA[Search Exist Office Cost]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*), 0, 'N','Y') AS EXIST_YN
  FROM PSO_INV_OFC_COST
 WHERE OFC_CD = @[ofc_cd]
   AND LGS_COST_CD = @[lgs_cost_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
