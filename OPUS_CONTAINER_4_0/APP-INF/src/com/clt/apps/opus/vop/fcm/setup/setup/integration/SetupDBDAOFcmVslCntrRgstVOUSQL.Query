<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SetupDBDAOFcmVslCntrRgstVOUSQL">
			<desc><![CDATA[Vessel Registration 정보를 변경한다.

History
2012.04.04 진마리아 CHM-201216636-01 [FCM] ALPS 모델 및 DB 구조 불일치 복구]]></desc>
			<sql><![CDATA[
MERGE INTO FCM_VSL_CNTR_RGST
USING DUAL ON(VSL_CD = @[vsl_cd])

WHEN MATCHED THEN
	UPDATE SET
     VSL_CLSS_SUB_CD           = @[vsl_clss_sub_cd]
	,MN_ENG_MKR_NM             = @[mn_eng_mkr_nm]
	,MN_ENG_TP_DESC            = @[mn_eng_tp_desc]
	,MN_ENG_RPM_PWR            = @[mn_eng_rpm_pwr]
	,MN_ENG_BHP_PWR            = @[mn_eng_bhp_pwr]
	,TBCGR_NO                  = @[tbcgr_no]
	,TBCGR_MKR_NM              = @[tbcgr_mkr_nm]
	,TBCGR_TP_DESC             = @[tbcgr_tp_desc]
	,TBCGR_RPM_PWR             = @[tbcgr_rpm_pwr]
	,TBCGR_COFF_FLG            = @[tbcgr_coff_flg]
	,SPR_AUX_BLW_MKR_NM        = @[spr_aux_blw_mkr_nm]
	,SPR_AUX_BLW_TP_DESC       = @[spr_aux_blw_tp_desc]
	,SPR_AUX_BLW_NO            = @[spr_aux_blw_no]
	,FOIL_ADTV_CD              = @[foil_adtv_cd]
    ,HL_PNT_CD                 = @[hl_pnt_cd]
	,GNR_CSM_WGT               = @[gnr_csm_wgt]
	,FOIL_TNK_CBM_CAPA         = @[foil_tnk_cbm_capa]
	,FOIL_TNK_MT_CAPA          = @[foil_tnk_mt_capa]
	,FOIL_STL_SVC_TNK_MT_CAPA  = @[foil_stl_svc_tnk_mt_capa]
	,OP_MAX_SPD               = @[op_max_spd]
	,OP_MIN_SPD               = @[op_min_spd]
	,OP_GNR_SPD               = @[op_gnr_spd]
    ,TRND_LINE_USE_FLG         = @[trnd_line_use_flg]
    ,MNVR_CSM_WGT              = @[mnvr_csm_wgt]
	,UPD_USR_ID                = @[upd_usr_id]
	,UPD_DT                    = SYSDATE
	WHERE VSL_CD=@[vsl_cd]

WHEN NOT MATCHED THEN
	INSERT(
		VSL_CD
        ,VSL_CLSS_SUB_CD
		,MN_ENG_MKR_NM
		,MN_ENG_TP_DESC
		,MN_ENG_RPM_PWR
		,MN_ENG_BHP_PWR
		,TBCGR_NO
		,TBCGR_MKR_NM
		,TBCGR_TP_DESC
		,TBCGR_RPM_PWR
		,TBCGR_COFF_FLG
		,SPR_AUX_BLW_MKR_NM
		,SPR_AUX_BLW_TP_DESC
		,SPR_AUX_BLW_NO
		,FOIL_ADTV_CD
        ,HL_PNT_CD
		,GNR_CSM_WGT
		,FOIL_TNK_CBM_CAPA
		,FOIL_TNK_MT_CAPA
		,FOIL_STL_SVC_TNK_MT_CAPA
		,OP_MAX_SPD
		,OP_MIN_SPD
		,OP_GNR_SPD
        ,TRND_LINE_USE_FLG
        ,MNVR_CSM_WGT
		,CRE_USR_ID
		,CRE_DT
		,UPD_USR_ID
		,UPD_DT
	)VALUES(
		@[vsl_cd]
        ,@[vsl_clss_sub_cd]
		,@[mn_eng_mkr_nm]
		,@[mn_eng_tp_desc]
		,@[mn_eng_rpm_pwr]
		,@[mn_eng_bhp_pwr]
		,@[tbcgr_no]
		,@[tbcgr_mkr_nm]
		,@[tbcgr_tp_desc]
		,@[tbcgr_rpm_pwr]
		,@[tbcgr_coff_flg]
		,@[spr_aux_blw_mkr_nm]
		,@[spr_aux_blw_tp_desc]
		,@[spr_aux_blw_no]
		,@[foil_adtv_cd]
        ,@[hl_pnt_cd]
		,@[gnr_csm_wgt]
		,@[foil_tnk_cbm_capa]
		,@[foil_tnk_mt_capa]
		,@[foil_stl_svc_tnk_mt_capa]
		,@[op_max_spd]
		,@[op_min_spd]
		,@[op_gnr_spd]
        ,@[trnd_line_use_flg]
        ,@[mnvr_csm_wgt]
		,@[cre_usr_id]
		,SYSDATE
		,@[upd_usr_id]
		,SYSDATE
	)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_sub_cd" type="12" value="" out="N"/>
				<param name="mn_eng_mkr_nm" type="12" value="" out="N"/>
				<param name="mn_eng_tp_desc" type="12" value="" out="N"/>
				<param name="mn_eng_rpm_pwr" type="12" value="" out="N"/>
				<param name="mn_eng_bhp_pwr" type="12" value="" out="N"/>
				<param name="tbcgr_no" type="12" value="" out="N"/>
				<param name="tbcgr_mkr_nm" type="12" value="" out="N"/>
				<param name="tbcgr_tp_desc" type="12" value="" out="N"/>
				<param name="tbcgr_rpm_pwr" type="12" value="" out="N"/>
				<param name="tbcgr_coff_flg" type="12" value="" out="N"/>
				<param name="spr_aux_blw_mkr_nm" type="12" value="" out="N"/>
				<param name="spr_aux_blw_tp_desc" type="12" value="" out="N"/>
				<param name="spr_aux_blw_no" type="12" value="" out="N"/>
				<param name="foil_adtv_cd" type="12" value="" out="N"/>
				<param name="hl_pnt_cd" type="12" value="" out="N"/>
				<param name="gnr_csm_wgt" type="12" value="" out="N"/>
				<param name="foil_tnk_cbm_capa" type="12" value="" out="N"/>
				<param name="foil_tnk_mt_capa" type="12" value="" out="N"/>
				<param name="foil_stl_svc_tnk_mt_capa" type="12" value="" out="N"/>
				<param name="op_max_spd" type="12" value="" out="N"/>
				<param name="op_min_spd" type="12" value="" out="N"/>
				<param name="op_gnr_spd" type="12" value="" out="N"/>
				<param name="trnd_line_use_flg" type="12" value="" out="N"/>
				<param name="mnvr_csm_wgt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
