<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOLoadVolSGTdrVORSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 	D.OPR_CD,
       	D.POD,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'DG', DECODE(D.CNTR_SIZE, '2', 1, '3', 1, 'A', 1, 0), 0), 0))                   				 	DG_20_QTY,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'DG', DECODE(D.CNTR_SIZE, '2', D.WEIGHT, '3', D.WEIGHT, 'A', D.WEIGHT, 0), 0), 0))     				DG_20_WGT,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'DG', DECODE(D.CNTR_SIZE, '4', 1, 'H', 1, 'L', 1, 'B', 1, 0), 0), 0))           				 	DG_40_QTY,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'DG', DECODE(D.CNTR_SIZE, '4', D.WEIGHT, 'H', D.WEIGHT, 'L', D.WEIGHT, 'B', D.WEIGHT, 0), 0), 0))   DG_40_WGT,

       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.TEMP, NULL, 0, DECODE(D.CNTR_SIZE, '2', 1, '3', 1, 'A', 1, 0)), 0))                                       		RF_20_QTY,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.TEMP, NULL, 0, DECODE(D.CNTR_SIZE, '2', D.WEIGHT, '3', 'A', D.WEIGHT, D.WEIGHT, 0)), 0))                        RF_20_WGT,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.TEMP, NULL, 0, DECODE(D.CNTR_SIZE, '4', 1, 'H', 1, 'L', 1, 'B', 1, 0)), 0))                               		RF_40_QTY,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.TEMP, NULL, 0, DECODE(D.CNTR_SIZE, '4', D.WEIGHT, 'H', D.WEIGHT, 'L', D.WEIGHT, 'B', D.WEIGHT, 0)), 0))         RF_40_WGT,

       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'AK', DECODE(D.CNTR_SIZE, '2', 1, '3', 1, 'A', 1, 0), 0), 0))                                 		AK_20_QTY,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'AK', DECODE(D.CNTR_SIZE, '2', D.WEIGHT, '3', 'A', D.WEIGHT, D.WEIGHT, 0), 0), 0))                  AK_20_WGT,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'AK', DECODE(D.CNTR_SIZE, '4', 1, 'H', 1,'L', 1, 'B', 1, 0), 0), 0))                          		AK_40_QTY,
       	SUM(DECODE(D.STATUS,@[sc_status1], DECODE(D.CARGO_TYPE, 'AK', DECODE(D.CNTR_SIZE, '4', D.WEIGHT, 'H', D.WEIGHT, 'L', D.WEIGHT, 'B', D.WEIGHT, 0), 0), 0))   AK_40_WGT
FROM   	VSK_VSL_PORT_SKD 	V
	, 	TDR_HEADER 			H
	, 	TDR_CNTR_DETAIL 	D
WHERE  	V.VSL_CD       		= @[vsl_cd]     	--:vsl_cd
AND    	V.SKD_VOY_NO   		= @[voy_no]     	--:skd_voy_no
AND    	V.SKD_DIR_CD   		= @[dir_cd]     	--:skd_dir_cd
AND    	V.YD_CD        		= @[yd_cd]  	  	--:port_cd
AND    	V.CLPT_IND_SEQ 		= @[clpt_ind_seq]	--:clpt_ind_seq
AND    	V.VSL_CD       		= H.VSL_CD
AND    	V.SKD_VOY_NO   		= H.VOY_NO
AND    	V.SKD_DIR_CD   		= H.DIR_CD
AND    	V.VPS_PORT_CD  		= H.PORT_CD
AND    	V.CLPT_IND_SEQ 		= H.CALL_IND
AND    	H.VSL_CD       		= D.VSL_CD
AND    	H.VOY_NO       		= D.VOY_NO
AND    	H.DIR_CD       		= D.DIR_CD
AND    	H.PORT_CD      		= D.PORT_CD
AND    	H.CALL_IND     		= D.CALL_IND
AND    	D.STATUS       		IN (@[sc_status1], @[sc_status2])
AND    	(D.CARGO_TYPE IN ('DG','AK') OR D.TEMP IS NOT NULL)
GROUP BY OPR_CD, POD
ORDER BY OPR_CD, POD			]]></sql>
			<params>
				<param name="sc_status1" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="sc_status2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
