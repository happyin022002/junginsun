<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveEdiFromParnterLinesMgtDBDAOAddScgPrnrSpclCgoDtlErrCSQL">
			<desc><![CDATA[AddScgPrnrSpclCgoDtlErr]]></desc>
			<sql><![CDATA[
INSERT INTO
       SCG_PRNR_SPCL_CGO_DTL_ERR
	(
	   TRSM_BND_CD
     , TRSM_DT
     , SPCL_CGO_CATE_CD
     , PRNR_SPCL_CGO_SEQ
     , PRNR_SPCL_CGO_SUB_SEQ
     , ERR_DTL_SEQ
     , ERR_KND_CD
     , ERR_DTL_CD
     , ERR_DTL_RMK
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
	)
  SELECT @[trsm_bnd_cd]
	   , TO_DATE(@[trsm_dt],'yyyy-MM-dd')
	   , @[spcl_cgo_cate_cd]
	   , @[prnr_spcl_cgo_seq]
       , @[prnr_spcl_cgo_sub_seq]
	   , (SELECT NVL(MAX(ERR_DTL_SEQ),0) + 1 
            FROM SCG_PRNR_SPCL_CGO_DTL_ERR
           WHERE TRSM_BND_CD       = @[trsm_bnd_cd]
             AND TRSM_DT           = TO_DATE(@[trsm_dt],'yyyy-MM-dd')
             AND SPCL_CGO_CATE_CD  = @[spcl_cgo_cate_cd]
             AND PRNR_SPCL_CGO_SEQ = @[prnr_spcl_cgo_seq]
         )
	   , @[err_knd_cd]
	   , @[err_dtl_cd]
	   , @[err_dtl_rmk]
	   , @[cre_usr_id]
	   , SYSDATE
	   , @[upd_usr_id]
	   , SYSDATE
   FROM DUAL			]]></sql>
			<params>
				<param name="trsm_bnd_cd" type="12" value="" out="N"/>
				<param name="trsm_dt" type="12" value="" out="N"/>
				<param name="spcl_cgo_cate_cd" type="12" value="" out="N"/>
				<param name="prnr_spcl_cgo_seq" type="12" value="" out="N"/>
				<param name="prnr_spcl_cgo_sub_seq" type="12" value="" out="N"/>
				<param name="err_knd_cd" type="12" value="" out="N"/>
				<param name="err_dtl_cd" type="12" value="" out="N"/>
				<param name="err_dtl_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
