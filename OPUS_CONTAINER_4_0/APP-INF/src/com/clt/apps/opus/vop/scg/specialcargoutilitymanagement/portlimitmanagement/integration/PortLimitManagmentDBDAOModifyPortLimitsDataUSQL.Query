<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortLimitManagmentDBDAOModifyPortLimitsDataUSQL">
			<desc><![CDATA[Port별 Lane & Entry Typr & IMDG & Weight & Property 정보를 변경 후 Data를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE SCG_IMDG_PORT_LMT_MST
    SET      LMT_WGT_TP_CD 		  = @[lmt_wgt_tp_cd]
			,PIER_TP_CD		      = NVL(@[pier_tp_cd],'C1')
			,PORT_LMT_REP_DESC    = @[port_lmt_rep_desc]
			--,IMDG_SUBS_RSK_LBL_CD = imdg_subs_rsk_lbl_cd
			,IMDG_PCK_GRP_CD      = @[imdg_pck_grp_cd]
			,FLSH_PNT_TEMP        = @[flsh_pnt_temp]
			,PORT_LMT_SUB_PPT_CD  = @[port_lmt_sub_ppt_cd]
			,CNTR_TP_CD           = @[cntr_tp_cd]
			,CMDT_CD              = @[cmdt_cd]
			,ARR_DEP_PROHI_FLG    = DECODE(@[arr_dep_prohi_flg], '0', 'N', 1, 'Y', @[arr_dep_prohi_flg])
			,LDIS_APLY_TGT_FLG    = DECODE(@[ldis_aply_tgt_flg], '0', 'N', 1, 'Y', @[ldis_aply_tgt_flg])
			,ARR_MAX_QTY          = @[arr_max_qty]
			,LOD_MAX_QTY          = @[lod_max_qty]
			,DCHG_MAX_QTY         = @[dchg_max_qty]
			,DEP_MAX_QTY          = @[dep_max_qty]
			,PPT_EXPLO_FLG        = DECODE(@[ppt_explo_flg], '0', 'N', 1, 'Y', @[ppt_explo_flg])
			,PPT_PROHI_FLG        = DECODE(@[ppt_prohi_flg], '0', 'N', 1, 'Y', @[ppt_prohi_flg])
			,DP_SEQ               = @[dp_seq]
			,CRE_OFC_CD           = @[cre_ofc_cd]
			,UPD_OFC_CD           = @[upd_ofc_cd]
			,UPD_USR_ID           = @[upd_usr_id]
			,UPD_DT               = SYSDATE
            ,YD_CD                = @[yd_cd]
	        ,LOD_MAX_TEU_QTY      = @[lod_max_teu_qty]
	        ,DCHG_MAX_TEU_QTY     = @[dchg_max_teu_qty]
WHERE PORT_CD = @[port_cd]
  AND PORT_LMT_SEQ = @[port_lmt_seq]			]]></sql>
			<params>
				<param name="lmt_wgt_tp_cd" type="12" value="" out="N"/>
				<param name="pier_tp_cd" type="12" value="" out="N"/>
				<param name="port_lmt_rep_desc" type="12" value="" out="N"/>
				<param name="imdg_pck_grp_cd" type="12" value="" out="N"/>
				<param name="flsh_pnt_temp" type="12" value="" out="N"/>
				<param name="port_lmt_sub_ppt_cd" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="arr_dep_prohi_flg" type="12" value="" out="N"/>
				<param name="ldis_aply_tgt_flg" type="12" value="" out="N"/>
				<param name="arr_max_qty" type="12" value="" out="N"/>
				<param name="lod_max_qty" type="12" value="" out="N"/>
				<param name="dchg_max_qty" type="12" value="" out="N"/>
				<param name="dep_max_qty" type="12" value="" out="N"/>
				<param name="ppt_explo_flg" type="12" value="" out="N"/>
				<param name="ppt_prohi_flg" type="12" value="" out="N"/>
				<param name="dp_seq" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="lod_max_teu_qty" type="12" value="" out="N"/>
				<param name="dchg_max_teu_qty" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="port_lmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
