<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOSearchPsoConditionBySysDescRSQL">
			<desc><![CDATA[COND_SYS_DESC로 Condition조회하기]]></desc>
			<sql><![CDATA[
/**** COND_SYS_DESC로 Condition조회하기 ****/
SELECT A.* 
FROM   PSO_CONDITION A
WHERE  1=1
AND    A.COND_SYS_DESC = @[cond_sys_desc]			]]></sql>
			<params>
				<param name="cond_sys_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
