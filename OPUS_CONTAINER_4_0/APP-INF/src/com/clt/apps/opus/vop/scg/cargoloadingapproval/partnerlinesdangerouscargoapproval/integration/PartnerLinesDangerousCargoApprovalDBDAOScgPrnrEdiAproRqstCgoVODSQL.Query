<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerLinesDangerousCargoApprovalDBDAOScgPrnrEdiAproRqstCgoVODSQL">
			<desc><![CDATA[SPCL CGO APVL for Partner Lines 의 Request Cago 삭제]]></desc>
			<sql><![CDATA[
UPDATE SCG_PRNR_APRO_RQST_CGO SET 
	   AUTH_STS_CD       = 'X'
     , UPD_USR_ID        = @[upd_usr_id]
     , UPD_DT            = SYSDATE
 WHERE CRR_CD            = COM_ConstantMgr_PKG.COM_getCompanyCode_FNC()
   AND BKG_REF_NO        = @[bkg_ref_no]
   AND SPCL_CGO_RQST_SEQ = @[spcl_cgo_rqst_seq]
   AND PRNR_CGO_RQST_SEQ = @[prnr_cgo_rqst_seq]

   --::2015-05-30::--AND REF_NO            = @[ref_no]
   AND DCGO_REF_NO       = @[dcgo_ref_no]

   AND AUTH_STS_CD       <> 'X'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_rqst_seq" type="2" value="" out="N"/>
				<param name="prnr_cgo_rqst_seq" type="12" value="" out="N"/>
				<param name="ref_no" type="12" value="" out="N"/>
				<param name="dcgo_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
