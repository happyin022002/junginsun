<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerLinesDangerousCargoApprovalDBDAOScgApprovalMailUSQL">
			<desc><![CDATA[mail send no 갱신]]></desc>
			<sql><![CDATA[
UPDATE SCG_PRNR_APRO_RQST SET 
EML_SND_NO  = @[eml_snd_no]
, UPD_USR_ID       = @[upd_usr_id]
, UPD_DT           = SYSDATE
WHERE BKG_REF_NO   = @[bkg_ref_no]
AND SPCL_CGO_RQST_SEQ = @[spcl_cgo_rqst_seq]
AND POD_CD = SUBSTR(@[pod_cd], 0, 5)
AND POL_CD = SUBSTR(@[pol_cd], 0, 5)			]]></sql>
			<params>
				<param name="eml_snd_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_rqst_seq" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
