<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOmodiyYardChargeObjectListUSQL">
			<desc><![CDATA[pso_yd_chg_obj_list 수정]]></desc>
			<sql><![CDATA[
UPDATE PSO_YD_CHG_OBJ_LIST SET 
	DFLT_CTNT = @[dflt_ctnt]
,	DFLT_VAL = @[dflt_val]
,	DFLT_FLG = @[dflt_flg]
,	UPD_USR_ID = @[cre_usr_id]
,	UPD_DT = sysdate
WHERE	YD_CHG_NO = @[yd_chg_no]
AND	YD_CHG_VER_SEQ = @[yd_chg_ver_seq]
AND	OBJ_LIST_NO = @[obj_list_no]			]]></sql>
			<params>
				<param name="dflt_ctnt" type="12" value="" out="N"/>
				<param name="dflt_val" type="6" value="" out="N"/>
				<param name="dflt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="yd_chg_no" type="2" value="" out="N"/>
				<param name="yd_chg_ver_seq" type="2" value="" out="N"/>
				<param name="obj_list_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
