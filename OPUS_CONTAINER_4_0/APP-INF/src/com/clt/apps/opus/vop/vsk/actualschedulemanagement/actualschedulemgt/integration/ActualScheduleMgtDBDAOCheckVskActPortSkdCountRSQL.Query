<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualScheduleMgtDBDAOCheckVskActPortSkdCountRSQL">
			<desc><![CDATA[Actual Port SKD Count 추출]]></desc>
			<sql><![CDATA[
SELECT	 COUNT(H.VSL_CD)	KNT
FROM     VSK_ACT_PORT_SKD	H
WHERE    1 = 1
AND      H.VSL_CD         	= @[vsl_cd]
AND      H.SKD_VOY_NO     	= @[skd_voy_no]              
AND      H.SKD_DIR_CD     	= @[skd_dir_cd]              
AND      H.VPS_PORT_CD    	= @[vps_port_cd]             
AND      H.CLPT_IND_SEQ   	= @[clpt_ind_seq]
			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
