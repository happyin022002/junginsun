<sqls>
	<querys>
		<query name="StevedoreDamageMgtDBDAOOpfStvDmgStlVORSQL">
			<desc><![CDATA[OpfStvDmgStlVO Select Query]]></desc>
			<sql><![CDATA[
SELECT 
	STV_DMG_NO
,	STV_DMG_STL_PROC_STS_CD
,	SHP_OWNR_CO_NM
,	USTL_ACCT_NO
,	RUN_RPR_ACCT_NO
,	BIL_INV_NO
,	TO_CHAR(PAY_DT, 'YYYY-MM-DD') AS PAY_DT
,	PAY_CURR_CD
,	PAY_LOCL_AMT
,	PAY_USD_AMT
,	PAY_ACCT_NO
,	STL_INV_ATCH_FLG
,	STL_DOC_ATCH_FLG
,	STL_RMK
,	STL_APRO_DT
,	STL_APRO_FLG
,	STL_APRO_USR_ID
,	STL_EML_SND_NO
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,   TO_CHAR(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',UPD_DT,'GMT'), UPD_DT), 'YYYY-MM-DD') AS UPD_DT
,   STL_INV_ATCH_KNT
,   STL_DOC_ATCH_KNT
FROM OPF_STV_DMG_STL
WHERE	STV_DMG_NO = @[stv_dmg_no]			]]></sql>
			<params>
				<param name="stv_dmg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
