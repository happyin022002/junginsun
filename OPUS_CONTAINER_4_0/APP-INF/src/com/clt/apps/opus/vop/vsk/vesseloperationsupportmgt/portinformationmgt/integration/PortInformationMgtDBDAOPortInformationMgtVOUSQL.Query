<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortInformationMgtDBDAOPortInformationMgtVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE VSK_PORT_MNVR SET 
	ALSD_CD = @[alsd_cd]
,	CHNL_DPTH = @[chnl_dpth]
,	BRTH_DPTH = @[brth_dpth]
,	AT_DRFT_DPTH = @[at_drft_dpth]
,	MAX_DRFT_DPTH = @[max_drft_dpth]
,	AD_LEN = @[ad_len]
,	BRTH_LEN = @[brth_len]
,	TURN_BSN_LEN = @[turn_bsn_len]
,	PLT_MNVR_DIST = @[plt_mnvr_dist]
,	PLT_MNVR_TM_HRS = @[plt_mnvr_tm_hrs]
,	MNVR_RMK = @[mnvr_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	YD_CD = @[temp_yd_cd]			]]></sql>
			<params>
				<param name="alsd_cd" type="12" value="" out="N"/>
				<param name="chnl_dpth" type="6" value="" out="N"/>
				<param name="brth_dpth" type="6" value="" out="N"/>
				<param name="at_drft_dpth" type="6" value="" out="N"/>
				<param name="max_drft_dpth" type="6" value="" out="N"/>
				<param name="ad_len" type="6" value="" out="N"/>
				<param name="brth_len" type="6" value="" out="N"/>
				<param name="turn_bsn_len" type="6" value="" out="N"/>
				<param name="plt_mnvr_dist" type="6" value="" out="N"/>
				<param name="plt_mnvr_tm_hrs" type="6" value="" out="N"/>
				<param name="mnvr_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="temp_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
