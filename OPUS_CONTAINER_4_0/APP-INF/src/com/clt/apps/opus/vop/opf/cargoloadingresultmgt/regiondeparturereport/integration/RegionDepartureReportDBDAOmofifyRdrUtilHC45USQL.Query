<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAOmofifyRdrUtilHC45USQL">
			<desc><![CDATA[DESC Enter..
]]></desc>
			<sql><![CDATA[
UPDATE RDR_UTILIZE  /*HIGH CUBIC UPDATING시, 처리.*/
  SET  SLOT_HC20    =  CASE WHEN TYPE = 'F'  THEN @[load_20] ELSE TO_CHAR(SLOT_HC20)  END
,      SLOT_HC      =  CASE WHEN TYPE = 'F'  THEN @[load_40] ELSE TO_CHAR(SLOT_HC) END
,      SLOT_45      =  CASE WHEN TYPE = 'F'  THEN @[load_45] ELSE TO_CHAR(SLOT_45) END

,      SLOT_QTY     = CASE WHEN TYPE IN ('3','H','L')  THEN @[slot_qty] ELSE   TO_CHAR(SLOT_QTY) END

,      UPDATE_USER  = @[update_user]
,      UPDATE_TIME  = SYSDATE

WHERE
      VSL_CD =   @[vsl_cd]
  AND VOY_NO =   @[voy_no]
  AND DIR_CD =   @[dir_cd]
  AND REGION =   @[region]
  AND OPR_CD =   @[opr_cd]
  AND TYPE   =   @[type]
  AND TYPE  IN ('F', '3', 'H', 'L')			]]></sql>
			<params>
				<param name="load_20" type="12" value="" out="N"/>
				<param name="load_40" type="12" value="" out="N"/>
				<param name="load_45" type="12" value="" out="N"/>
				<param name="slot_qty" type="12" value="" out="N"/>
				<param name="update_user" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
				<param name="type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
