<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOTdrAllocationVOUSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
UPDATE 	TDR_ALLOCATION 
SET		BSA_SLOT 		= @[bsa_slot]
	,	BSA_WGT 		= @[bsa_wgt]
	,	RELEASE_SLOT 	= @[release_slot]
	,	RELEASE_WGT 	= @[release_wgt]
	,	SWAP_SLOT 		= @[swap_slot]
	,	SWAP_WGT 		= @[swap_wgt]
	,	BSA_TYPE 		= @[bsa_type]
	,	RATIO_TYPE 		= @[ratio_type]
	,	UPDATE_USER 	= @[update_user]
	,	UPDATE_TIME 	= SYSDATE
WHERE	VSL_CD 			= @[vsl_cd]
AND		VOY_NO 			= @[voy_no]
AND		DIR_CD 			= @[dir_cd]
AND		PORT_CD 		= @[port_cd]
AND		CALL_IND 		= @[call_ind]
AND		OPR_CD 			= @[opr_cd]
 			]]></sql>
			<params>
				<param name="bsa_slot" type="6" value="" out="N"/>
				<param name="bsa_wgt" type="6" value="" out="N"/>
				<param name="release_slot" type="6" value="" out="N"/>
				<param name="release_wgt" type="6" value="" out="N"/>
				<param name="swap_slot" type="6" value="" out="N"/>
				<param name="swap_wgt" type="6" value="" out="N"/>
				<param name="bsa_type" type="12" value="" out="N"/>
				<param name="ratio_type" type="12" value="" out="N"/>
				<param name="update_user" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="call_ind" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
