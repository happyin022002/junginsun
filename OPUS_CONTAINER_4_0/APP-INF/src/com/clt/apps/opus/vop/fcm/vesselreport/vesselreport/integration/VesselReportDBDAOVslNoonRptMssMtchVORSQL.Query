<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselReportDBDAOVslNoonRptMssMtchVORSQL">
			<desc><![CDATA[주어진 조건에 대한 Lane별 Noon Report Mismatched 현황을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
VSL_SLAN_CD,
VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD,
NXT_PORT_CD,
TO_CHAR(NOON_RPT_DT, 'YYYY/MM/DD') RPT_DT,
REF_NO
FROM FCM_NOON_RPT
WHERE 1=1
AND NOT EXISTS (SELECT 'X' FROM VSK_VSL_PORT_SKD T1
                WHERE 1=1
                AND T1.VSL_CD=VSL_CD
                AND T1.SKD_VOY_NO=SKD_VOY_NO
                AND T1.SKD_DIR_CD=SKD_DIR_CD
                AND T1.VPS_PORT_CD=NXT_PORT_CD)
#if (${fm_dt} != '') 
AND NOON_RPT_DT BETWEEN TO_DATE(REPLACE(@[fm_dt], '-', ''), 'YYYYMMDD')     -- 화면조건 : FM_DT
                AND TO_DATE(REPLACE(@[to_dt], '-', ''), 'YYYYMMDD')+0.99999 -- 화면조건 : TO_DT
#end
#if( ${vsl_slan_cd} != '' )								  
	-- 화면조건 : VSL_SLAN_CD
    AND VSL_SLAN_CD IN (                     
		#foreach($sVslSlanCd in ${vel_vsl_slan_cd})  
			'$sVslSlanCd',  
			#end  
			'') 
#end
#if( ${vsl_cd} != '' )
	-- 화면조건 : VSL_CD
    AND VSL_CD IN (                     
		#foreach($sVslCd in ${vel_vsl_cd})  
			'$sVslCd',  
			#end  
			'') 
#end
#if (${skd_dir_cd} != '') 
    AND SKD_DIR_CD=@[skd_dir_cd]                                        -- 화면조건 : SKD_DIR_CD
#end
ORDER BY VSL_SLAN_CD			]]></sql>
			<params>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
