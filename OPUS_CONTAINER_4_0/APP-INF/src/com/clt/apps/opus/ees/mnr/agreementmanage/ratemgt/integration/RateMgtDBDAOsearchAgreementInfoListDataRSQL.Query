<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RateMgtDBDAOsearchAgreementInfoListDataRSQL">
			<desc><![CDATA[searchAgreementInfoListData]]></desc>
			<sql><![CDATA[
SELECT 
       MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(MAX(A.AGMT_OFC_CD)) RHQ_OFC,
       MNR_COMMON_PKG.MNR_CONV_AGMT_NO_FNC(A.AGMT_OFC_CTY_CD, A.AGMT_SEQ) AGMT_NO,
	   MAX(A.DELT_FLG) DELT_FLG,
       MAX(A.AGMT_OFC_CD) AGMT_OFC_CD,
       MAX(D.MNR_CD_DP_DESC) MNR_CD_DP_DESC,
       MAX(A.AGMT_VER_NO) AGMT_VER_NO,
       MAX(A.AGMT_REF_NO) REF_NO,
	   MAX(MNR_COMMON_PKG.MNR_CONV_PARTNER_CD_FNC(A.VNDR_SEQ)) VNDR_SEQ,
       MAX(A.TRF_NO) TRF_NO,
	   MAX(E.VNDR_LGL_ENG_NM) VNDR_LGL_ENG_NM,
       TO_CHAR(MAX(A.EFF_DT), 'yyyy-mm-dd')||'~'||TO_CHAR(MAX(A.EXP_DT), 'yyyy-mm-dd') EFF_DT,
       MAX(DECODE(F.DP_SEQ, 1, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK1,
       MAX(DECODE(F.DP_SEQ, 2, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK2,
       MAX(DECODE(F.DP_SEQ, 3, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK3,
       MAX(DECODE(F.DP_SEQ, 4, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK4,
       MAX(DECODE(F.DP_SEQ, 5, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK5,
       MAX(DECODE(F.DP_SEQ, 6, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK6,
       MAX(DECODE(F.DP_SEQ, 7, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK7,
       MAX(DECODE(F.DP_SEQ, 8, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK8,
       MAX(DECODE(F.DP_SEQ, 9, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK9,
       MAX(DECODE(F.DP_SEQ, 10, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK10,
       MAX(DECODE(F.DP_SEQ, 11, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK11,
       MAX(DECODE(F.DP_SEQ, 12, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK12,
       MAX(DECODE(F.DP_SEQ, 13, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK13,
       MAX(DECODE(F.DP_SEQ, 14, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK14,
       MAX(DECODE(F.DP_SEQ, 15, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK15,
       MAX(DECODE(F.DP_SEQ, 16, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK16,
       MAX(DECODE(F.DP_SEQ, 17, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK17,
       MAX(DECODE(F.DP_SEQ, 18, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK18,
       MAX(DECODE(F.DP_SEQ, 19, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK19,
       MAX(DECODE(F.DP_SEQ, 20, DECODE(SIGN(C.AGMT_RT_AMT), 1, 'Y', ''))) CHK20,
       MAX(A.CRE_USR_ID) CRE_USR_ID,
       MAX(TO_CHAR(A.CRE_DT, 'YYYY-MM-DD HH24:MI:SS')) CRE_DT,
       MAX(A.UPD_USR_ID) UPD_USR_ID,
       MAX(TO_CHAR(A.UPD_DT, 'YYYY-MM-DD HH24:MI:SS')) UPD_DT
FROM  MNR_AGMT_HDR A, MNR_AGMT_APLY_OFC B, MNR_AGMT_RT C, MNR_GEN_CD D, MDM_VENDOR E, 
	(SELECT DECODE(EQ_TP, 1, DECODE(COST_CD, 'RC', 'URC', COST_CD), 2, DECODE(COST_CD, 'RC', 'ZRC', COST_CD), 3, DECODE(COST_CD, 'RC', 'GRC', COST_CD)) COST_CD,
       TT AS COST_CD_NM,
       ROW_NUMBER() OVER (ORDER BY EQ_TP, TP) DP_SEQ
  FROM (SELECT DISTINCT DECODE(SUBSTR(A.COST_CD, 5, 2), 'RC', 'RC', A.COST_CD) COST_CD,
                       CASE WHEN A.COST_CD LIKE '%RC' THEN 'Repair' ELSE (SELECT DISTINCT MNR_CD_DESC
                                                                            FROM MNR_GEN_CD
                                                                           WHERE MNR_CD_ID = A.COST_CD
                                                                             AND PRNT_CD_ID LIKE '_G') END TT ,
               DECODE((SELECT MNR_ORD_TP_CD
                         FROM MNR_GEN_CD
                        WHERE MNR_CD_ID = A.COST_CD
                          AND PRNT_CD_ID LIKE '_G'), 'LB', 1, 'TS', 2, 'QT', 3, 1) TP,
               DECODE((SELECT PRNT_CD_ID
                         FROM MNR_GEN_CD
                        WHERE MNR_CD_ID = A.COST_CD
                          AND PRNT_CD_ID LIKE '_G'), 'UG', 1, 'ZG', 2, 'GG', 3, 1) EQ_TP
          FROM MNR_AGMT_RT A, MNR_AGMT_HDR B
         WHERE A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD
           AND A.AGMT_SEQ = B.AGMT_SEQ
           AND A.AGMT_VER_NO = B.AGMT_VER_NO
#if(${agmt_eq_type} != 'A' && ${agmt_eq_type} != '')           
           AND B.EQ_KND_CD = @[agmt_eq_type]
#end
          )) F
WHERE A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD
AND   A.AGMT_SEQ        = B.AGMT_SEQ
AND   A.AGMT_VER_NO = B.AGMT_VER_NO
AND   A.VNDR_SEQ = E.VNDR_SEQ
AND   B.AGMT_OFC_CTY_CD = C.AGMT_OFC_CTY_CD
AND   B.AGMT_SEQ        = C.AGMT_SEQ
AND   B.AGMT_VER_NO     = C.AGMT_VER_NO
AND   A.EQ_KND_CD       = DECODE(@[agmt_eq_type], 'A', A.EQ_KND_CD, @[agmt_eq_type])
AND   ((A.EFF_DT BETWEEN TO_DATE(@[agmt_fm_dt], 'yyyy-mm-dd') AND TO_DATE(@[agmt_to_dt], 'yyyy-mm-dd')) OR (A.EXP_DT BETWEEN TO_DATE(@[agmt_fm_dt], 'yyyy-mm-dd') AND TO_DATE(@[agmt_to_dt], 'yyyy-mm-dd')))
AND   A.AGMT_LST_VER_FLG = 'Y'
AND   D.PRNT_CD_ID = 'CD00002'
AND   D.MNR_CD_ID = A.EQ_KND_CD 
AND   DECODE(SUBSTR(C.COST_CD, 5, 2), 'RC', A.EQ_KND_CD||'RC', C.COST_CD) = F.COST_CD
#if (${vndr_seq} != '')
AND   A.VNDR_SEQ        = TO_NUMBER(@[vndr_seq])
#end
#if (${cost_ofc_cd} != '')
AND   B.AGMT_OFC_TP_CD = 'COST'
AND   B.APLY_OFC_CD LIKE @[cost_ofc_cd]||'%%' 
#end
#if (${ar_hd_qtr_cd} != '')
AND   A.AGMT_OFC_CD IN ( select OFC_CD
                         from mdm_organization
                         where AR_HD_QTR_OFC_CD = @[ar_hd_qtr_cd]
                         and ofc_cd LIKE @[agmt_ofc_cd]||'%%'
                         and delt_flg = 'N'
                       )   
#end
#if (${agmt_ofc_cd} != '')
AND   A.AGMT_OFC_CD IN ( SELECT D.OFC_CD
                         FROM   MNR_OFC_GEN_INFO D
                         WHERE  D.UPPR_OFC_CD  =  @[agmt_ofc_cd]
                         AND    D.MNR_GRP_TP_CD = 'OFC'
                         AND    D.COST_CD       = 'MR'
                         UNION ALL
                         SELECT @[agmt_ofc_cd]
                         FROM DUAL
                       )
#end
#if (${old_agmt_no} != '')
AND   A.OLD_AGMT_NO = @[old_agmt_no]
#end
GROUP BY MNR_COMMON_PKG.MNR_CONV_AGMT_NO_FNC(A.AGMT_OFC_CTY_CD, A.AGMT_SEQ)			]]></sql>
			<params>
				<param name="agmt_eq_type" type="12" value="" out="Y"/>
				<param name="agmt_fm_dt" type="12" value="" out="Y"/>
				<param name="agmt_to_dt" type="12" value="" out="Y"/>
				<param name="vndr_seq" type="12" value="" out="Y"/>
				<param name="cost_ofc_cd" type="12" value="" out="Y"/>
				<param name="ar_hd_qtr_cd" type="12" value="" out="Y"/>
				<param name="agmt_ofc_cd" type="12" value="" out="Y"/>
				<param name="old_agmt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
