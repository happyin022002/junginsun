<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOInvoiceDetailListVORSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT	D.SYS_AREA_GRP_ID AS SVR_ID,
        D.CNTR_NO,	
        D.INV_DTL_SEQ,
		D.CNTR_CYC_NO,	
		D.DMDT_TRF_CD,
		D.DMDT_CHG_LOC_DIV_CD,
		D.CHG_SEQ,	
		D.ORG_CHG_AMT,
		D.SC_RFA_EXPT_AMT,	
		D.AFT_EXPT_DC_AMT,
		D.BIL_AMT,	
		M.INV_CURR_CD
FROM	DMT_INV_MN	M,	
        DMT_INV_DTL	D
WHERE	M.DMDT_INV_NO	=	D.DMDT_INV_NO
AND		D.DMDT_INV_NO	=	@[dmdt_inv_no]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
