<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncomeMgtDBDAOsearchReceivableInvoiceNoDataRSQL">
			<desc><![CDATA[IncomeMgtDBDAOsearchReceivableInvoiceNoDataRSQL]]></desc>
			<sql><![CDATA[
SELECT
      SUBSTR(@[user_ofc_cd], 1, 3)||TO_CHAR(sysdate, 'yyyymm')||LTRIM(TO_CHAR(NVL(SUBSTR(MAX(MRIW.INV_NO), 10), '0')+1, '000')) INV_NO
FROM MNR_RCV_INV_WRK MRIW
WHERE MRIW.INV_NO LIKE SUBSTR(@[user_ofc_cd], 1, 3)||TO_CHAR(sysdate, 'yyyymm')||'%'			]]></sql>
			<params>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
