<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOMstCntrSpecVOCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO MST_CNTR_SPEC (
	CNTR_SPEC_NO
,	OWN_CNTR_FLG
,   CNTR_SPEC_TP_CD
,	SPEC_YR
,	CNTR_TPSZ_CD
,	LOD_CAPA
,	CNTR_GRS_WGT
,	TARE_WGT
,	INTER_LEN
,	INTER_WDT
,	INTER_HGT
,	XTER_LEN
,	XTER_WDT
,	XTER_HGT
,	OPN_DOR_WDT
,	OPN_DOR_HGT
,	RC_LDB_CAPA
,	RC_LDB_HGT
,	TNK_CAPA
,	DIFF_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	FRACK_CLPS_CTNT
,	FRACK_BED_TIK_CTNT
,	OPNTP_ROOF_OPN_CTNT 
,	OPNTP_INTR_HGT_CTNT
,	OPNTP_REAR_HDR_OPN_CTNT
,	VNDR_SEQ
,	CNTR_MTRL_CD
) VALUES( 
	@[cntr_spec_no]
,	DECODE(@[own_cntr_flg], 'O', 'Y', 'N')
,   @[own_cntr_flg]
,	@[spec_yr]
,	@[cntr_tpsz_cd]
,	NVL(@[lod_capa],0)
,	TO_NUMBER(NVL(@[cntr_grs_wgt],0))
,	TO_NUMBER(NVL(@[tare_wgt],0))
,	NVL(@[inter_len],0)
,	NVL(@[inter_wdt],0)
,	NVL(@[inter_hgt],0)   
,	NVL(@[xter_len],0)
,	NVL(@[xter_wdt],0)
,	NVL(@[xter_hgt],0)
,	NVL(@[opn_dor_wdt],0)
,	NVL(@[opn_dor_hgt],0)
,	NVL(@[rc_ldb_capa],0)
,	NVL(@[rc_ldb_hgt],0)
,	NVL(@[tnk_capa],0)
,	@[diff_rmk]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	@[frack_clps_ctnt]
,	@[frack_bed_tik_ctnt] 
,	@[opntp_roof_opn_ctnt]
,	@[opntp_intr_hgt_ctnt]
,	@[opntp_rear_hdr_opn_ctnt]
#if(${own_cntr_flg} == 'S')
,	''
,	''
#else
,	@[vndr_seq2]
,	@[cntr_mtrl_cd]
#end
)			]]></sql>
			<params>
				<param name="cntr_spec_no" type="12" value="    111" out="N"/>
				<param name="own_cntr_flg" type="12" value="1" out="N"/>
				<param name="spec_yr" type="12" value="11" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="1" out="N"/>
				<param name="lod_capa" type="6" value="1" out="N"/>
				<param name="cntr_grs_wgt" type="12" value="" out="N"/>
				<param name="tare_wgt" type="12" value="" out="N"/>
				<param name="inter_len" type="2" value="156.111" out="N"/>
				<param name="inter_wdt" type="2" value="" out="N"/>
				<param name="inter_hgt" type="2" value="" out="N"/>
				<param name="xter_len" type="2" value="" out="N"/>
				<param name="xter_wdt" type="2" value="" out="N"/>
				<param name="xter_hgt" type="2" value="" out="N"/>
				<param name="opn_dor_wdt" type="2" value="" out="N"/>
				<param name="opn_dor_hgt" type="2" value="" out="N"/>
				<param name="rc_ldb_capa" type="6" value="" out="N"/>
				<param name="rc_ldb_hgt" type="2" value="" out="N"/>
				<param name="tnk_capa" type="6" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="frack_clps_ctnt" type="12" value="" out="N"/>
				<param name="frack_bed_tik_ctnt" type="12" value="" out="N"/>
				<param name="opntp_roof_opn_ctnt" type="12" value="" out="N"/>
				<param name="opntp_intr_hgt_ctnt" type="12" value="" out="N"/>
				<param name="opntp_rear_hdr_opn_ctnt" type="12" value="" out="N"/>
				<param name="vndr_seq2" type="12" value="" out="N"/>
				<param name="cntr_mtrl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
