<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffMgtDBDAOaddRepairTariffHeaderDataCSQL">
			<desc><![CDATA[입력]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_RPR_TRF_HDR (
	CURR_CD
,	EFF_DT
,	MNR_TRF_RMK
,	RQST_OFC_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	TRF_NO
,	EQ_KND_CD
,	MNR_TRF_KND_CD
,	MNR_INP_TP_CD
,	VNDR_SEQ
,	APRO_OFC_CD
,	MNR_TRF_STS_CD
,	MNR_TRF_STS_DT
,	STS_REF_NO
,	MNR_MEAS_UT_CD
) VALUES( 
	@[curr_cd]
,	TO_DATE(@[eff_dt],'YYYY-MM-DD')
,	@[mnr_trf_rmk]
,	@[rqst_ofc_cd]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	@[trf_no]
,	@[eq_knd_cd]
,	@[mnr_trf_knd_cd]
,	@[mnr_inp_tp_cd]
,	TO_NUMBER(@[vndr_seq])
,	@[apro_ofc_cd]
,	@[mnr_trf_sts_cd]
,	SYSDATE
,	@[sts_ref_no]
,	@[mnr_meas_ut_cd]
)			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="mnr_trf_rmk" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="mnr_trf_knd_cd" type="12" value="" out="N"/>
				<param name="mnr_inp_tp_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="mnr_trf_sts_cd" type="12" value="" out="N"/>
				<param name="sts_ref_no" type="2" value="" out="N"/>
				<param name="mnr_meas_ut_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
