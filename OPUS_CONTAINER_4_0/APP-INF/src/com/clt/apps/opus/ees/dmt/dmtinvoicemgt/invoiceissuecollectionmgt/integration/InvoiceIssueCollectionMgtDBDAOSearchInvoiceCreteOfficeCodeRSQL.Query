<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchInvoiceCreteOfficeCodeRSQL">
			<desc><![CDATA[Invoice No. 를 생성한 사용자의 Office Code 를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	CRE_OFC_CD
FROM	DMT_INV_MN
WHERE	DMDT_INV_NO = @[dmdt_inv_no]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
