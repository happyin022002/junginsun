<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TotalLossMgtDBDAOaddTotalLossDetailDataCSQL">
			<desc><![CDATA[Total Loss Request 의 Detail  입력저장]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_TTL_LSS_RQST_DTL
(
  TTL_LSS_NO
 ,TTL_LSS_DTL_SEQ
 ,MNR_INV_TP_CD
 ,TTL_LSS_DTL_STS_CD
 ,EQ_KND_CD
 ,RQST_EQ_NO
 ,EQ_TPSZ_CD
 ,DPC_VAL_AMT
 ,TTL_LSS_N3PTY_TP_CD
 ,MNR_PRNR_CNT_CD
 ,MNR_PRNR_SEQ
 ,CNT_CD
 ,VNDR_CUST_SEQ
 ,TTL_LSS_PLC_NM
 ,INV_NO
 ,CURR_CD
 ,TTL_LSS_BIL_AMT
 ,TTL_LSS_BIL_DT
 ,TTL_LSS_EXPN_AMT
 ,TTL_LSS_INCM_AMT
 ,AR_CHK_NO
 ,N3PTY_NO
 ,CSR_NO
 ,EQ_OWNR_FLG
 ,TTL_LSS_CFM_FLG
 ,PAY_INV_SEQ
 ,IF_TRC_SEQ
 ,DTL_RMK
 ,CRE_USR_ID
 ,CRE_DT
 ,UPD_USR_ID
 ,UPD_DT
 ,BANK_NM
 ,BANK_ACCT_NO
 ,RESPB_OFC_CD
 ,TTL_LSS_YD_CD
 ,MNR_SWIFT_NO
 ,CR_END_DT
)
VALUES
( 
  @[ttl_lss_no]
 ,(SELECT NVL(MAX(TTL_LSS_DTL_SEQ),0)+ @[temp_seq]
   FROM MNR_TTL_LSS_RQST_DTL
   WHERE TTL_LSS_NO=@[ttl_lss_no]
   )
 ,@[mnr_inv_tp_cd]
 ,@[ttl_lss_dtl_sts_cd]
 ,@[eq_knd_cd]
 ,@[rqst_eq_no]
 ,@[eq_tpsz_cd]
 ,@[dpc_val_amt]
 ,@[ttl_lss_n3pty_tp_cd]
 ,@[mnr_prnr_cnt_cd]
 ,@[mnr_prnr_seq]
 ,@[cnt_cd]
 ,@[vndr_cust_seq]
 ,@[ttl_lss_plc_nm]
 ,@[inv_no]
 ,@[curr_cd]
 ,@[ttl_lss_bil_amt]
 ,TO_DATE(@[ttl_lss_bil_dt], 'yyyy-mm-dd')
 ,@[ttl_lss_expn_amt]
 ,@[ttl_lss_incm_amt]
 ,@[ar_chk_no]
 ,@[n3pty_no]
 ,@[csr_no]
 ,@[eq_ownr_flg]
 ,@[ttl_lss_cfm_flg]
 ,@[pay_inv_seq]
 ,@[if_trc_seq]
 ,@[dtl_rmk]
 ,@[cre_usr_id]
 ,SYSDATE
 ,@[upd_usr_id]
 ,SYSDATE
 ,@[bank_nm]
 ,@[bank_acct_no]
 ,@[respb_ofc_cd]
 ,@[ttl_lss_yd_cd]
 ,@[mnr_swift_no]
 ,TO_DATE(@[cr_end_dt], 'yyyy-mm-dd')
)			]]></sql>
			<params>
				<param name="ttl_lss_no" type="12" value="" out="N"/>
				<param name="temp_seq" type="12" value="" out="N"/>
				<param name="mnr_inv_tp_cd" type="12" value="" out="N"/>
				<param name="ttl_lss_dtl_sts_cd" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="dpc_val_amt" type="12" value="" out="N"/>
				<param name="ttl_lss_n3pty_tp_cd" type="12" value="" out="N"/>
				<param name="mnr_prnr_cnt_cd" type="12" value="" out="N"/>
				<param name="mnr_prnr_seq" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_cust_seq" type="12" value="" out="N"/>
				<param name="ttl_lss_plc_nm" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="ttl_lss_bil_amt" type="12" value="" out="N"/>
				<param name="ttl_lss_bil_dt" type="12" value="" out="N"/>
				<param name="ttl_lss_expn_amt" type="12" value="" out="N"/>
				<param name="ttl_lss_incm_amt" type="12" value="" out="N"/>
				<param name="ar_chk_no" type="12" value="" out="N"/>
				<param name="n3pty_no" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
				<param name="eq_ownr_flg" type="12" value="" out="N"/>
				<param name="ttl_lss_cfm_flg" type="12" value="" out="N"/>
				<param name="pay_inv_seq" type="12" value="" out="N"/>
				<param name="if_trc_seq" type="12" value="" out="N"/>
				<param name="dtl_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bank_nm" type="12" value="" out="N"/>
				<param name="bank_acct_no" type="12" value="" out="N"/>
				<param name="respb_ofc_cd" type="12" value="" out="N"/>
				<param name="ttl_lss_yd_cd" type="12" value="" out="N"/>
				<param name="mnr_swift_no" type="12" value="" out="N"/>
				<param name="cr_end_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
