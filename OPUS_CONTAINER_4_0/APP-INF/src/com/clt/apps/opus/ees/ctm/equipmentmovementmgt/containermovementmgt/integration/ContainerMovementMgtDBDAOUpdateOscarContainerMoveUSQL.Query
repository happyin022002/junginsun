<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOUpdateOscarContainerMoveUSQL">
			<desc><![CDATA[Update.]]></desc>
			<sql><![CDATA[
UPDATE  CTM_BKG_CNTR    A
SET     
       A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
      ,A.CNMV_YR      = @[cnmv_yr]
      ,A.CNMV_ID_NO   = @[cnmv_id_no]
      ,A.CNMV_CYC_NO  = @[cnmv_cyc_no]
      ,A.CNMV_STS_CD  = @[mvmt_sts_cd]
      ,A.CNMV_EVNT_DT = TO_DATE(@[cnmv_evnt_dt], 'YYYYMMDDHH24MISS')
      ,A.UPD_USR_ID   = @[cre_usr_id]
      ,A.UPD_DT = SYSDATE
WHERE   A.BKG_NO = @[bkg_no]
AND     A.CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
				<param name="cnmv_cyc_no" type="12" value="" out="N"/>
				<param name="mvmt_sts_cd" type="12" value="" out="N"/>
				<param name="cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
