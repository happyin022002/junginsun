<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSelectEDICtmBkgCntrRSQL">
			<desc><![CDATA[SelectEDICtmBkgCntr]]></desc>
			<sql><![CDATA[
select count(*) CNT from CTM_BKG_CNTR
where 1=1
and BKG_NO = @[bkg_no]
and CNTR_NO = @[cntr_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
