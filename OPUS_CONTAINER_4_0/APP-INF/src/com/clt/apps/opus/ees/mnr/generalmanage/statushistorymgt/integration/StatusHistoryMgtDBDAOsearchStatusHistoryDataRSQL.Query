<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusHistoryMgtDBDAOsearchStatusHistoryDataRSQL">
			<desc><![CDATA[Status History 조회]]></desc>
			<sql><![CDATA[
SELECT A.MNR_STS_REF_NO
      ,A.MNR_STS_DTL_SEQ
      ,A.MNR_GRP_TP_CD
      ,A.MNR_HIS_STS_CD
      ,A.MNR_INP_TP_CD
      ,A.RQST_OFC_CD
      ,TO_CHAR(A.RQST_DT, 'yyyy-mm-dd') RQST_DT
      ,A.RQST_USR_ID
      ,A.APRO_OFC_CD
      ,TO_CHAR(A.APRO_DT, 'yyyy-mm-dd') APRO_DT
      ,A.APRO_USR_ID
      ,A.MNR_STS_RMK
      ,TO_CHAR(A.MNR_STS_DT, 'yyyy-mm-dd') MNR_STS_DT
      ,A.CRE_USR_ID
      ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
      ,A.UPD_USR_ID
      ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
  FROM MNR_STS_HIS A
 WHERE A.MNR_STS_REF_NO = @[mnr_sts_ref_no]			]]></sql>
			<params>
				<param name="mnr_sts_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
