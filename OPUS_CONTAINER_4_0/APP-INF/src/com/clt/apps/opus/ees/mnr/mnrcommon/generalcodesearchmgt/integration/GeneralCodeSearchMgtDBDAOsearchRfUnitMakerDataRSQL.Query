<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchRfUnitMakerDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 
       A.DISP_FLG AS DSP_FLAG
       , (SELECT NVL(VNDR_ABBR_NM, VNDR_LGL_ENG_NM) 
        FROM MDM_VENDOR 
        WHERE VNDR_SEQ = CASE WHEN A.LSTM_CD= 'LT' OR A.LSTM_CD = 'ST' THEN A.RF_MKR_SEQ ELSE L.RF_MKR_SEQ END
    ) AS MKR_NM
FROM 
    MST_CONTAINER A
    , MST_CNTR_LOT L
WHERE 
    SUBSTR(A.CNTR_NO,0,10) BETWEEN L.LOT_CNTR_PFX_CD(+)||L.FM_SER_NO(+) AND L.LOT_CNTR_PFX_CD(+)||L.TO_SER_NO(+)
    AND A.CNTR_NO = @[eq_no]
    AND ROWNUM = 1			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
