<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmValidationDBDAOsearchCNTRMasterDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(b XAK1CGM_EQ_ATCH_DTCH_HIS) */
       A.CNTR_NO
      ,A.CNTR_TPSZ_CD 
      ,DECODE(TO_CHAR(B.DTCH_DT,'YYYYMMDD'),NULL,'DT','88881231','AT','DT') disp_flg
      ,B.EQ_NO
      ,B.EQ_KND_CD
#if ( ${onh_yd_cd} != '' )
      ,( SELECT B.DTCH_YD_CD FROM CGM_EQ_ATCH_DTCH_HIS B WHERE CNTR_NO= A.CNTR_NO
     AND DTCH_YD_CD IS  NULL AND ROWNUM = 1) ONH_YD_CD   
#end 
FROM MST_CONTAINER A ,CGM_EQ_ATCH_DTCH_HIS B
WHERE A.CNTR_NO = B.CNTR_no(+)
  AND A.CNTR_NO =  @[cntr_no]
  AND ROWNUM =1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
