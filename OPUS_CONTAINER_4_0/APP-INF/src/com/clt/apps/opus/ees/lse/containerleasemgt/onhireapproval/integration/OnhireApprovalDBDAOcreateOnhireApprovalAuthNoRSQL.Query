<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOcreateOnhireApprovalAuthNoRSQL">
			<desc><![CDATA[create auth_no]]></desc>
			<sql><![CDATA[
SELECT 
   TO_CHAR(SYSDATE, 'YYMMDD')||@[cntr_onh_auth_no]||TRIM(TO_CHAR(@[agmt_seq], '00000'))|| '-' || SUBSTR(TO_CHAR(( COUNT(*) + 1 ),'000'),2,3) CNTR_ONH_AUTH_NO
FROM LSE_ONH_APRO A 
WHERE A.ONH_LOC_CD = @[onh_loc_cd]
AND   A.LSTM_CD    = @[lstm_cd]
AND   TO_CHAR(A.CRE_DT,'YYYYMMDD') = TO_CHAR( SYSDATE , 'YYYYMMDD')			]]></sql>
			<params>
				<param name="cntr_onh_auth_no" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="onh_loc_cd" type="12" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
