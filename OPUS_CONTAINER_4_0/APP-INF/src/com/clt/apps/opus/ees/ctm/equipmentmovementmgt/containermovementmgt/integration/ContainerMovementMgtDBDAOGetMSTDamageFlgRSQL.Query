<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetMSTDamageFlgRSQL">
			<desc><![CDATA[MST DamageFlg]]></desc>
			<sql><![CDATA[
SELECT CNMV_ID_NO, DMG_FLG
FROM MST_CONTAINER
WHERE 1=1
AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
