<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisHistoryDBDAOaddPoolMovementDataCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO 
CGM_POOL_MVMT_HIS(
     CHSS_NO
    ,MVMT_DT
    ,CHSS_OWNR_CO_CD
    ,VNDR_NM
    ,CHSS_POOL_CD
    ,CHSS_TPSZ_CD
    ,EQ_TPSZ_REP_CD
    ,YD_CD
    ,YD_COM_FLG
    ,LOC_NM
    ,CNTR_NO
    ,CNTR_OWNR_CO_CD
    ,POOL_CO_GATE_IO_CD
    ,CNTR_TPSZ_CD
    ,MGST_NO
    ,CHSS_USE_CO_NM
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
    ,POOL_CHSS_CO_CD
	,TRKR_NM
	,MVMT_STS_NM
	,CHSS_MVMT_ACT_CD
) VALUES  (
    @[chss_no]
   ,TO_DATE(@[mvmt_dt],'RRRRMMDDHH24MI')
   ,NVL(( SELECT CASE WHEN CHSS_OWNR_CO_CD = 'H' AND ACIAC_DIV_CD = 'A' THEN 'H' 
				 ELSE 'O' END 
		FROM CGM_EQUIPMENT
		 WHERE EQ_NO= @[chss_no]
		  ), 'O') 
   , @[vndr_nm] 
   , @[chss_pool_cd]
   , @[chss_tpsz_cd]
   , ''
   , @[yd_cd]
   , ''
   , @[loc_nm]
   , @[cntr_no]
   ,  decode(@[cntr_no],null,'','','',nvl((select  CASE WHEN  A.ACIAC_DIV_CD = 'A' OR  A.CNTR_STS_CD='LST' THEN 'H' 
               														 ELSE 'O'
            													 END
           														  from  MST_CONTAINER A
        													 WHERE CNTR_NO =@[cntr_no]
     														  ),'O'))
   , @[pool_co_gate_io_cd]
   , @[cntr_tpsz_cd]
   , @[mgst_no]
   , @[chss_use_co_nm]
   , 'EDI_322'
   , SYSDATE
   , 'EDI_322'
   , SYSDATE
   , @[pool_chss_co_cd]
   , @[trkr_nm]
   , @[mvmt_sts_nm]
   , @[act_cd]
)			]]></sql>
			<params>
				<param name="chss_no" type="12" value="" out="N"/>
				<param name="mvmt_dt" type="12" value="" out="N"/>
				<param name="vndr_nm" type="12" value="" out="N"/>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
				<param name="chss_tpsz_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="loc_nm" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="pool_co_gate_io_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="mgst_no" type="12" value="" out="N"/>
				<param name="chss_use_co_nm" type="12" value="" out="N"/>
				<param name="pool_chss_co_cd" type="12" value="" out="N"/>
				<param name="trkr_nm" type="12" value="" out="N"/>
				<param name="mvmt_sts_nm" type="12" value="" out="N"/>
				<param name="act_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
