<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAOAddDmtTrfRtCSQL">
			<desc><![CDATA[Basic Tariff Creation - Group]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_TRF_RT (
	SYS_AREA_GRP_ID
,	DMDT_TRF_CD
,	TRF_SEQ
,	TRF_GRP_SEQ
,	TRF_RT_SEQ
,	FT_OVR_DYS
,	FT_UND_DYS
,	CNTR_20FT_RT_AMT
,	CNTR_40FT_RT_AMT
,	CNTR_HC_RT_AMT
,	CNTR_45FT_RT_AMT
,	CRE_USR_ID
,	CRE_DT
,	CRE_OFC_CD
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
) VALUES( 
@[svr_id]
,	@[dmdt_trf_cd]
,	@[trf_seq]
,	@[trf_grp_seq]
,	(SELECT NVL(MAX(TRF_RT_SEQ),0)+1 FROM DMT_TRF_RT
     WHERE SYS_AREA_GRP_ID = @[svr_id] 
     AND DMDT_TRF_CD = @[dmdt_trf_cd]
     AND TRF_SEQ = @[trf_seq]
	 AND TRF_GRP_SEQ = @[trf_grp_seq])
,	@[ft_ovr_dys]
,	@[ft_und_dys]
,	@[cntr_20ft_rt_amt]
,	@[cntr_40ft_rt_amt]
,	@[cntr_hc_rt_amt]
,	@[cntr_45ft_rt_amt]
,	@[usr_id]
,	sysdate
,	@[ofc_cd]
,	@[usr_id]
,	sysdate
,	@[ofc_cd]
)			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="trf_seq" type="2" value="" out="N"/>
				<param name="trf_grp_seq" type="2" value="" out="N"/>
				<param name="ft_ovr_dys" type="2" value="" out="N"/>
				<param name="ft_und_dys" type="2" value="" out="N"/>
				<param name="cntr_20ft_rt_amt" type="6" value="" out="N"/>
				<param name="cntr_40ft_rt_amt" type="6" value="" out="N"/>
				<param name="cntr_hc_rt_amt" type="6" value="" out="N"/>
				<param name="cntr_45ft_rt_amt" type="6" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
