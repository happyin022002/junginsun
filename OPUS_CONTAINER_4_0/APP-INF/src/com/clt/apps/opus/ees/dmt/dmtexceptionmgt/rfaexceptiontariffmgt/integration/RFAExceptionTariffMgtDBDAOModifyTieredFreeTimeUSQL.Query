<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOModifyTieredFreeTimeUSQL">
			<desc><![CDATA[RFA Tiered Free Time Update]]></desc>
			<sql><![CDATA[
UPDATE DMT_RFA_EXPT_FREE_TM
   SET CNTR_FM_QTY          = @[cntr_fm_qty]
     , CNTR_TO_QTY          = @[cntr_to_qty]
     , FT_DYS               = @[ft_dys]
     , UPD_USR_ID           = @[upd_usr_id]
     , UPD_DT               = SYSDATE
 WHERE 1=1
   AND RFA_EXPT_DAR_NO      = @[rfa_expt_dar_no]
   AND RFA_EXPT_MAPG_SEQ    = @[rfa_expt_mapg_seq]
   AND RFA_EXPT_VER_SEQ     = @[rfa_expt_ver_seq]
   AND RFA_RQST_DTL_SEQ     = @[rfa_rqst_dtl_seq]
   AND CVRG_CMB_SEQ         = @[cvrg_cmb_seq]
   AND FT_SEQ               = @[ft_seq]			]]></sql>
			<params>
				<param name="cntr_fm_qty" type="12" value="" out="N"/>
				<param name="cntr_to_qty" type="12" value="" out="N"/>
				<param name="ft_dys" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
				<param name="rfa_rqst_dtl_seq" type="12" value="" out="N"/>
				<param name="cvrg_cmb_seq" type="12" value="" out="N"/>
				<param name="ft_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
