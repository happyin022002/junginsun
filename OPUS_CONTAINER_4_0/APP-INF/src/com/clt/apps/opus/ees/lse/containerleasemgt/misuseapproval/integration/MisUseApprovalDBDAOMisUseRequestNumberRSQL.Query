<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MisUseApprovalDBDAOMisUseRequestNumberRSQL">
			<desc><![CDATA[Miss Use 최대 Request No.를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  RQST_FIX||TRIM(TO_CHAR(LSE_MU_SEQ.NEXTVAL,'000000')) RQST_NO
FROM   (SELECT  @[ofc_cd]||TO_CHAR(SYSDATE, 'YYMMDD')||'-' AS RQST_FIX
	    FROM    DUAL)			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
