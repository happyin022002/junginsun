<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOMakeInvoiceDtlSeqRSQL">
			<desc><![CDATA[MakeInvoiceDtlSeq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(INV_DTL_SEQ),0)+1 AS MAX_SEQ
FROM DMT_INV_DTL
WHERE DMDT_INV_NO = @[dmdt_inv_no]
  AND CRE_OFC_CD = @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
