<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetLocalTimeForGateNewRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR (GLOBALDATE_PKG.TIME_CONV_FNC (COM_CONSTANTMGR_PKG.COM_GETBASELOCATIONCODE_FNC(), SYSDATE, SUBSTR (@[event_yard], 0, 5)), 'YYYYMMDDHH24MI')
  FROM DUAL			]]></sql>
			<params>
				<param name="event_yard" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
