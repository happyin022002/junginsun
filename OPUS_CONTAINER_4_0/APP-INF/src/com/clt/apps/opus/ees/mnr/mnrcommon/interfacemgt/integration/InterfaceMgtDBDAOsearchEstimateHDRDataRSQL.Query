<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchEstimateHDRDataRSQL">
			<desc><![CDATA[InterfaceMgtDBDAOsearchEstimateHDRDataRSQL]]></desc>
			<sql><![CDATA[
SELECT
      RPAD(MAX((SELECT DECODE(CSAG.SYS_AREA_GRP_ID, 'CHN', 'CNSHAHJCA', 
                                                       'SWA', 'SGSINHJCA',
                                                       'EUR', 'DEHAMHJCA',
                                                       'USA', 'USEWRHJCB',
                                                       'KRSELHJCA')
            FROM MDM_ORGANIZATION MO, COM_SYS_AREA_GRP_ID CSAG
            WHERE A.COST_OFC_CD = MO.OFC_CD
            AND   SUBSTR(MO.LOC_CD, 1, 2)     = CSAG.CNT_CD
            AND   CSAG.CO_IND_CD = 'H'
            AND   ROWNUM =1
       )), 20, ' ')||
       RPAD(@[edi_id], 20, ' ')||
       'WORDER    '||
       RPAD(MAX((SELECT CSAG.SYS_AREA_GRP_ID
                 FROM MDM_ORGANIZATION MO, COM_SYS_AREA_GRP_ID CSAG
                 WHERE A.COST_OFC_CD = MO.OFC_CD
                 AND   SUBSTR(MO.LOC_CD, 1, 2)     = CSAG.CNT_CD
                 AND   CSAG.CO_IND_CD = 'H'
                 AND   ROWNUM =1
          ))||
       TO_CHAR(SYSDATE, 'yymmdd')||ROUND(dbms_random.value(1111, 9999), 0), 20, ' ') MSG_ID,
       MAX(A.MNR_ORD_OFC_CTY_CD||A.MNR_ORD_SEQ) ORD_NO, 
       A.RPR_RQST_VER_NO MSG_RVS_NO, 
       MAX(A.RQST_REF_NO) REF_NO,
       MAX(A.EQ_KND_CD) EQ_TYPE,
       MAX(SUBSTR(A.RQST_EQ_NO, 1, 4)) EQ_PREFIX,
       MAX(SUBSTR(A.RQST_EQ_NO, 5)) EQ_NO,
       MAX(A.COST_OFC_CD) COM_OFC_CD,
       @[user_nm] AUTH_SENDOR,
      @[edi_id]  VENDOR_EDI_ADDR,
       '' SPCF_RECPT,
       TO_CHAR(sysdate, 'yyyymmdd') ACT_TRANS_DT,
       MAX(TO_CHAR(A.RQST_DT, 'yyyymmdd'))  ORD_DT,
       MAX(MNR_COMMON_PKG.MNR_CONV_TPSZ_FNC(A.EQ_KND_CD, A.EQ_TPSZ_CD)) EQ_TPSZ,      
       MAX(A.CURR_CD) CUR_CD,
       SUM(B.LBR_COST_AMT) LAB_TOT,
       SUM(B.MTRL_COST_AMT) MAT_TOT,
       0 HAN_TOT,
       0 TAX,
       MAX(A.MNR_WRK_AMT) TOT_INV_AMT,
       @[edi_id] EDI_ID,
       REPLACE(REPLACE(MAX(A.MNR_RPR_RMK), CHR(13)||CHR(10), ' '),CHR(12), ' ') REMARK,
       MAX(A.MNR_WRK_AMT) ORD_GRND_TOT,
       '' DPP_CUR,
       '' DPP       
FROM MNR_RPR_RQST_HDR A, MNR_RPR_RQST_DTL B
WHERE A.RQST_EQ_NO = B.RQST_EQ_NO
AND   A.RPR_RQST_SEQ = B.RPR_RQST_SEQ
AND   A.RPR_RQST_VER_NO = B.RPR_RQST_VER_NO
AND   A.RPR_RQST_LST_VER_FLG = 'Y'
AND   A.MNR_ORD_OFC_CTY_CD = @[mnr_ord_ofc_cty_cd]
AND   A.MNR_ORD_SEQ = @[mnr_ord_seq]
GROUP BY A.RQST_EQ_NO, A.RPR_RQST_SEQ, A.RPR_RQST_VER_NO			]]></sql>
			<params>
				<param name="edi_id" type="12" value="" out="N"/>
				<param name="user_nm" type="12" value="" out="N"/>
				<param name="mnr_ord_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="mnr_ord_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
