<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MSTCommonDBDAOSearchManufacturePlaceListDataRSQL">
			<desc><![CDATA[SearchManufacturePlaceListDataRSQL]]></desc>
			<sql><![CDATA[
WITH PARAM
AS (
SELECT @[loc_cd] CODE
FROM DUAL
)
SELECT A.MNR_CD_ID  CODE,A.MNR_CD_DP_DESC CODE_NM
FROM  MNR_GEN_CD A, PARAM P
WHERE 1 = 1 
#if(${loc_cd} != '')
AND A.MNR_CD_ID = P.CODE
#end
AND A.PRNT_CD_ID ='POM'
ORDER BY A.MNR_CD_ID			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
