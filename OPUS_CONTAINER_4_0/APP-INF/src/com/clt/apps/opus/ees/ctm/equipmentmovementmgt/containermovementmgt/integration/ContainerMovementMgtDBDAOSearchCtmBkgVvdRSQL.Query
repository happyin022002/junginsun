<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchCtmBkgVvdRSQL">
			<desc><![CDATA[ContainerMovementMgtDBDAOSearchCtmBkgVvdRSQL]]></desc>
			<sql><![CDATA[
SELECT 'Y' AS VVD_YN 
  FROM CTM_BKG_VVD BV
 WHERE 1 = 1
   AND BV.VSL_CD = SUBSTR(@[p_vvd], 0,4)
   AND BV.SKD_VOY_NO = SUBSTR(@[p_vvd], 5,4)
   AND BV.SKD_DIR_CD = SUBSTR(@[p_vvd], 9,1)
   AND ROWNUM <= 1			]]></sql>
			<params>
				<param name="p_vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
