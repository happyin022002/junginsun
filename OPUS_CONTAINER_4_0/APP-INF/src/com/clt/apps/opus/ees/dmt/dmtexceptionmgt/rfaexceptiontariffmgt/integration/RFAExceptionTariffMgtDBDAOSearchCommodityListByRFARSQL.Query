<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchCommodityListByRFARSQL">
			<desc><![CDATA[Commodity Search]]></desc>
			<sql><![CDATA[
SELECT A.RFA_EXPT_DAR_NO
     , A.RFA_EXPT_MAPG_SEQ
     , A.RFA_EXPT_VER_SEQ
     , A.RFA_RQST_DTL_SEQ
     , A.CVRG_CMB_SEQ
     , A.CMDT_CD
     , B.CMDT_NM
  FROM DMT_RFA_EXPT_CMDT A
     , MDM_COMMODITY B
 WHERE 1=1
   AND A.RFA_EXPT_DAR_NO   = @[rfa_expt_dar_no] 
   AND A.RFA_EXPT_MAPG_SEQ = @[rfa_expt_mapg_seq]
   AND A.RFA_EXPT_VER_SEQ  = @[rfa_expt_ver_seq]
   AND A.RFA_RQST_DTL_SEQ  = @[rfa_rqst_dtl_seq]
   AND A.CVRG_CMB_SEQ      = @[cvrg_cmb_seq]
   AND A.CMDT_CD           = B.CMDT_CD
 ORDER BY A.RFA_EXPT_DAR_NO
        , A.RFA_EXPT_MAPG_SEQ
        , A.RFA_EXPT_VER_SEQ
        , A.RFA_RQST_DTL_SEQ
        , A.CVRG_CMB_SEQ
        , A.CMDT_CD			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
				<param name="rfa_rqst_dtl_seq" type="12" value="" out="N"/>
				<param name="cvrg_cmb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
