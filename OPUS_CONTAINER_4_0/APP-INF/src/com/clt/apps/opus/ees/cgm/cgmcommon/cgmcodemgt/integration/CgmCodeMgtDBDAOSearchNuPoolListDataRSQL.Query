<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOSearchNuPoolListDataRSQL">
			<desc><![CDATA[CgmCodeMgtDB.SearchMovementStatusListData]]></desc>
			<sql><![CDATA[
SELECT  AGMT_OFC_CTY_CD||LPAD(A.AGMT_SEQ,6,'0')  AS CODE1
      , A.AGMT_REF_NO   || ' ' ||   B.VNDR_LGL_ENG_NM AS DESC1 
 FROM CGM_AGREEMENT A
    , MDM_VENDOR  B
WHERE A.AGMT_LSTM_CD='NP'
AND A.VNDR_SEQ = B.VNDR_SEQ
AND A.LST_VER_FLG ='Y'
AND A.DELT_FLG = 'N'			]]></sql>
			<params>
