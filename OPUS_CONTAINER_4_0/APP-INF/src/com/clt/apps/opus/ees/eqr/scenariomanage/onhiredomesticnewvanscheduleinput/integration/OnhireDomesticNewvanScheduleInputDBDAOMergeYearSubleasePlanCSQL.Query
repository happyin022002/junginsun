<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireDomesticNewvanScheduleInputDBDAOMergeYearSubleasePlanCSQL">
			<desc><![CDATA[EQR_SCNR_SLSE 테이블의 데이터 입력/수정]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_SCNR_SLSE I                       				
USING                                                       	
(                                                           	
    SELECT 
        @[scnr_id] AS SCNR_ID                                  		
        ,@[pln_yrwk] AS PLN_YRWK                               		
        ,@[fm_ecc_cd] AS FM_ECC_CD                             		
        ,@[to_ecc_cd] AS TO_ECC_CD                               		
        ,@[cntr_tpsz_cd] AS CNTR_TPSZ_CD                               	
    FROM DUAL                                               	
) M                                                         	
ON                                                          	
(                                                           	
	    I.SCNR_ID    	= M.SCNR_ID                       		 
	AND I.PLN_YRWK 		= M.PLN_YRWK                           	
	AND I.FM_ECC_CD		= M.FM_ECC_CD                    		
	AND I.TO_ECC_CD 	= M.TO_ECC_CD                     		
	AND I.CNTR_TPSZ_CD  = M.CNTR_TPSZ_CD                      	
)                                                           	
WHEN MATCHED THEN                                      		
    UPDATE SET 
        I.CNTR_VOL_QTY    = @[cntr_vol_qty] ,                    			
    	I.UPD_USR_ID      = @[user_id] ,                     			
    	I.UPD_DT    	  = SYSDATE		           			  
WHEN NOT MATCHED THEN                                      	
    INSERT (                                                       	
        I.SCNR_ID                                        		      
       ,I.PLN_YRWK                                               
       ,I.FM_ECC_CD                                      		   
       ,I.TO_ECC_CD                                      		   
       ,I.CNTR_TPSZ_CD                                      	   
       ,I.CNTR_VOL_QTY                                       	   
       ,I.CRE_USR_ID                                        	   
       ,I.CRE_DT                                            	   
       ,I.UPD_USR_ID                                        	   
       ,I.UPD_DT                                            	   
    ) VALUES (
        @[scnr_id]
        , @[pln_yrwk]
        , @[fm_ecc_cd]
        , @[to_ecc_cd]
        , @[cntr_tpsz_cd]
        , @[cntr_vol_qty]
        , @[user_id]
        , SYSDATE
        , @[user_id]
        , SYSDATE
    ) 					]]></sql>
			<params>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_vol_qty" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
