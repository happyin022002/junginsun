<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOSearchRepoExePlanTypeSizeRSQL">
			<desc><![CDATA[등록된 Type/Size]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD type_size
FROM EQR_REPO_EXE_PLN_QTY 
WHERE REPO_PLN_ID=@[repoPlanID]
      AND CNTR_QTY>0 GROUP BY CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="repoPlanID" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
