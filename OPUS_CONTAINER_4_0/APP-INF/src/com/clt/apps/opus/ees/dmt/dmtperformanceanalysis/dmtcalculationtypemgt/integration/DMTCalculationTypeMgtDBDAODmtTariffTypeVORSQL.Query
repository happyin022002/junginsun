<sqls>
	<querys>
		<query name="DMTCalculationTypeMgtDBDAODmtTariffTypeVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT
	DMDT_TRF_CD,
	DMDT_TRF_NM,
	IO_BND_CD,
	DECODE(DMDT_CALC_TP_CD, 'C', 'COMBINED',
                            'D', 'DUAL') DMDT_CALC_TP_CD,

    DECODE(DMDT_TRF_DIV_CD, 'DEM', 'M',
                            'DET', 'T') DMDT_TRF_DIV_CD,
    DMDT_CALC_TP_CD MGMT_CALC_TP_CD,
    DMDT_TRF_DIV_CD MGMT_TRF_DIV_CD,
    CRE_USR_ID,
    TO_CHAR(CRE_DT, 'YYYY-MM-DD') CRE_DT,
    UPD_USR_ID,
    TO_CHAR(UPD_DT, 'YYYY-MM-DD') UPD_DT
FROM DMT_TRF_TP
ORDER BY DMDT_CALC_TP_CD DESC, IO_BND_CD ASC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
