<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrForecastPrecisionManageDBDAOSearchMtyRailArrivalDetailRSQL">
			<desc><![CDATA[해당 조건의 Mty Rail Cntr No.를 조회한다

2011.06.13 나상보 [CHM-201111518-01] [EQR]  MTY Rail Trans. 화면의 CNTR List 조회 기능 추가]]></desc>
			<sql><![CDATA[
SELECT GUBUN
      ,EQ_NO, MT
      ,(SELECT CM.CNMV_STS_CD 
 		  FROM MST_CONTAINER CM
         WHERE CM.CNTR_NO = AA.EQ_NO
       ) MVMT_STS_CD
  FROM(	
	SELECT
			#if ( ${gubun} == 'A' ) 
				TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd') || NVL(C.LOC_CD,'ZZZZZ') || NVL(A.TO_NOD_CD, 'ZZZZZZZ') || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#elseif ( ${gubun} == 'B' ) 
				TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd') || NVL(A.TO_NOD_CD,'ZZZZZ') || NVL(A.FM_NOD_CD, 'ZZZZZZZ') || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#end 
				,A.EQ_NO
			    ,(SELECT DECODE(COUNT(*),0,0,1)
        			FROM SCE_CLM CLM, CTM_MOVEMENT CM
			       WHERE A.TRSP_SO_OFC_CTY_CD = CLM.TRSP_SO_OFC_CTY_CD
			         AND A.TRSP_SO_SEQ = CLM.TRSP_SO_SEQ
			         AND CM.CNTR_NO = CLM.CNTR_NO
			         AND CM.CNMV_YR = CLM.CNMV_YR
			         AND CM.CNMV_ID_NO = CLM.CNMV_ID_NO  
			         AND CM.MVMT_STS_CD = 'MT'
					 AND CM.ORG_YD_CD = A.TO_NOD_CD
			       GROUP BY CM.CNTR_NO ) MT
		FROM	SCE_RAIL_TZ_RPT A 
				,MDM_YARD B 
				,MDM_YARD C
	#if ( ${rail_company} != '' ) 
				,TRS_TRSP_RAIL_BIL_VNDR_SET D
    #end  
		WHERE	A.FM_NOD_CD	= B.YD_CD 
			AND	A.TO_NOD_CD	= C.YD_CD 
			AND	A.RAIL_DEST_N2ND_ETA_DT BETWEEN  TO_DATE('${fm_dt} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
										AND  TO_DATE('${to_dt} 23:59:59', 'YYYY-MM-DD HH24:MI:SS') 
			AND	A.CGO_TP_CD	= 'M'			-- MTY 
			AND	A.DELT_FLG	= 'N'			-- S/O CANCEL 
		#if ( ${org_loc} != '' ) 
			AND B.LOC_CD = @[org_loc] 
		#end 
		#if ( ${dest_loc} != '' ) 
			AND C.LOC_CD = @[dest_loc] 
		#end 
		#if ( ${tpszText} != '' )
			AND	A.EQ_TPSZ_CD IN ( ${tpszText} )
		#end
		#if ( ${rail_company} != '' ) 
         	AND A.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD
			AND A.TRSP_SO_SEQ = D.TRSP_SO_SEQ
			AND D.SUB_RAIL_SEQ	= '1'
			AND D.VNDR_SEQ		= @[rail_company]
		#end 
		UNION ALL 
		SELECT	
				TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd') || 'ZZZZZ'||	'ZZZZZZZ' ||A.EQ_TPSZ_CD GUBUN 
				,A.EQ_NO
			    ,(SELECT DECODE(COUNT(*),0,0,1)
        			FROM SCE_CLM CLM, CTM_MOVEMENT CM
			       WHERE A.TRSP_SO_OFC_CTY_CD = CLM.TRSP_SO_OFC_CTY_CD
			         AND A.TRSP_SO_SEQ = CLM.TRSP_SO_SEQ
			         AND CM.CNTR_NO = CLM.CNTR_NO
			         AND CM.CNMV_YR = CLM.CNMV_YR
			         AND CM.CNMV_ID_NO = CLM.CNMV_ID_NO  
			         AND CM.MVMT_STS_CD = 'MT'
					 AND CM.ORG_YD_CD = A.TO_NOD_CD
			       GROUP BY CM.CNTR_NO ) MT
		FROM	SCE_RAIL_TZ_RPT A 
				,MDM_YARD B 
				,MDM_YARD C 
	#if ( ${rail_company} != '' ) 
				,TRS_TRSP_RAIL_BIL_VNDR_SET D
    #end  
		WHERE	A.FM_NOD_CD	= B.YD_CD 
			AND	A.TO_NOD_CD	= C.YD_CD 
			AND	A.RAIL_DEST_N2ND_ETA_DT BETWEEN  TO_DATE('${fm_dt} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
										AND  TO_DATE('${to_dt} 23:59:59', 'YYYY-MM-DD HH24:MI:SS') 
			AND	A.CGO_TP_CD	= 'M'				-- MTY 
			AND	A.DELT_FLG	= 'N'			-- S/O CANCEL 
		#if ( ${org_loc} != '' ) 
			AND B.LOC_CD = @[org_loc] 
		#end 
		#if ( ${dest_loc} != '' ) 
			AND C.LOC_CD = @[dest_loc] 
		#end 
		#if ( ${tpszText} != '' )
			AND	A.EQ_TPSZ_CD IN ( ${tpszText})
		#end
		#if ( ${rail_company} != '' ) 
	  	    AND A.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD
			AND A.TRSP_SO_SEQ = D.TRSP_SO_SEQ
			AND D.SUB_RAIL_SEQ	= '1'
			AND D.VNDR_SEQ		= @[rail_company]
		#end 
		UNION ALL 
		SELECT	
			#if ( ${gubun} == 'A' ) 
				TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd') || NVL(C.LOC_CD,'ZZZZZ') || 'ZZZZZZZ' || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#elseif ( ${gubun} == 'B' ) 
				TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd') || NVL(A.TO_NOD_CD,'ZZZZZ') || 'ZZZZZZZ' || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#end
				,A.EQ_NO
			    ,(SELECT DECODE(COUNT(*),0,0,1)
        			FROM SCE_CLM CLM, CTM_MOVEMENT CM
			       WHERE A.TRSP_SO_OFC_CTY_CD = CLM.TRSP_SO_OFC_CTY_CD
			         AND A.TRSP_SO_SEQ = CLM.TRSP_SO_SEQ
			         AND CM.CNTR_NO = CLM.CNTR_NO
			         AND CM.CNMV_YR = CLM.CNMV_YR
			         AND CM.CNMV_ID_NO = CLM.CNMV_ID_NO  
			         AND CM.MVMT_STS_CD = 'MT'
					 AND CM.ORG_YD_CD = A.TO_NOD_CD
			       GROUP BY CM.CNTR_NO ) MT 
		FROM	SCE_RAIL_TZ_RPT A 
				,MDM_YARD B 
				,MDM_YARD C
	#if ( ${rail_company} != '' ) 
				,TRS_TRSP_RAIL_BIL_VNDR_SET D
    #end  
		WHERE	A.FM_NOD_CD	= B.YD_CD 
			AND	A.TO_NOD_CD	= C.YD_CD 
			AND	A.RAIL_DEST_N2ND_ETA_DT BETWEEN  TO_DATE('${fm_dt} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
										AND  TO_DATE('${to_dt} 23:59:59', 'YYYY-MM-DD HH24:MI:SS') 
			AND	A.CGO_TP_CD	= 'M'				-- MTY 
			AND	A.DELT_FLG	= 'N'			-- S/O CANCEL 
		#if ( ${org_loc} != '' ) 
			AND B.LOC_CD = @[org_loc] 
		#end 
		#if ( ${dest_loc} != '' ) 
			AND C.LOC_CD = @[dest_loc] 
		#end 
		#if ( ${tpszText} != '' )
			AND	A.EQ_TPSZ_CD IN ( ${tpszText})
		#end
		#if ( ${rail_company} != '' ) 
	      	AND A.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD
			AND A.TRSP_SO_SEQ = D.TRSP_SO_SEQ
			AND D.SUB_RAIL_SEQ	= '1'
			AND D.VNDR_SEQ		= @[rail_company]
		#end 
		)AA
ORDER BY GUBUN, MT			]]></sql>
			<params>
				<param name="org_loc" type="12" value="" out="N"/>
				<param name="dest_loc" type="12" value="" out="N"/>
				<param name="rail_company" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
