<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MSTCommonDBDAOSearchUserInfoDataRSQL">
			<desc><![CDATA[유저 패스워드 검색]]></desc>
			<sql><![CDATA[
SELECT 
	USR_PWD 
FROM COM_USER 
WHERE 1=1 
#if (${input_user} != "") 
   AND USR_ID = @[input_user]
#end			]]></sql>
			<params>
				<param name="input_user" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
