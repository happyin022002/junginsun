<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOSearchCntrStatusCntCheckRSQL">
			<desc><![CDATA[Container Status 삭제전 데이타 유무 확인]]></desc>
			<sql><![CDATA[
SELECT 'X'
FROM BKG_BOOKING BB
       , BKG_CONTAINER BC
WHERE BB.BKG_NO = BC.BKG_NO
AND    BC.CNTR_NO = @[cntr_no]
AND    BB.BKG_STS_CD != 'X'
AND    EXISTS ( SELECT 'X'
                     FROM MST_CNTR_STS_HIS HIS
                     WHERE BC.CNTR_NO = HIS.CNTR_NO
                     GROUP BY CNTR_NO
                     HAVING COUNT(*) < 2)
AND   ROWNUM = 1 			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="11" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
