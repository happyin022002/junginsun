<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQFlagMgtDBDAOmodifyEQFlagStatusDataUSQL">
			<desc><![CDATA[modifyEQFlagStatusData]]></desc>
			<sql><![CDATA[
UPDATE MNR_EQ_STS A
	SET  A.MNR_DMG_FLG     = DECODE(@[cost_dtl_cd], 'DMG', DECODE(@[mnr_dmg_flg],'0','N','Y'), A.MNR_DMG_FLG),
	     A.MNR_TTL_LSS_FLG = DECODE(@[cost_dtl_cd], 'TLL', DECODE(@[mnr_dmg_flg],'0','N','Y'), A.MNR_TTL_LSS_FLG),
	     A.MNR_DISP_FLG    = DECODE(@[cost_dtl_cd], 'DSP', DECODE(@[mnr_dmg_flg],'0','N','Y'), A.MNR_DISP_FLG),
	     A.MNR_SCRP_FLG    = DECODE(@[cost_dtl_cd], 'SCR', DECODE(@[mnr_dmg_flg],'0','N','Y'), A.MNR_SCRP_FLG),
	     A.MNR_DONA_FLG    = DECODE(@[cost_dtl_cd], 'DON', DECODE(@[mnr_dmg_flg],'0','N','Y'), A.MNR_DONA_FLG),
		 A.UPD_USR_ID = @[cre_usr_id],
		 A.UPD_DT = SYSDATE,
	  	 A.MNR_DMG_FLG_YD_CD     = DECODE(@[cost_dtl_cd], 'DMG', @[mnr_dmg_flg_yd_cd], A.MNR_DMG_FLG_YD_CD),
	     A.MNR_TTL_LSS_FLG_YD_CD = DECODE(@[cost_dtl_cd], 'TLL', @[mnr_dmg_flg_yd_cd], A.MNR_TTL_LSS_FLG_YD_CD),
	     A.MNR_DISP_FLG_YD_CD    = DECODE(@[cost_dtl_cd], 'DSP', @[mnr_dmg_flg_yd_cd], A.MNR_DISP_FLG_YD_CD),
	     A.MNR_SCRP_FLG_YD_CD    = DECODE(@[cost_dtl_cd], 'SCR', @[mnr_dmg_flg_yd_cd], A.MNR_SCRP_FLG_YD_CD),
	     A.MNR_DONA_FLG_YD_CD    = DECODE(@[cost_dtl_cd], 'DON', @[mnr_dmg_flg_yd_cd], A.MNR_DONA_FLG_YD_CD),
	  	 A.MNR_DMG_FLG_DT     = DECODE(@[cost_dtl_cd], 'DMG', TO_DATE(@[mnr_dmg_flg_dt],'YYYY-MM-DD HH24:MI'), A.MNR_DMG_FLG_DT),
	     A.MNR_TTL_LSS_FLG_DT = DECODE(@[cost_dtl_cd], 'TLL', TO_DATE(@[mnr_dmg_flg_dt],'YYYY-MM-DD'), A.MNR_TTL_LSS_FLG_DT),
	     A.MNR_DISP_FLG_DT    = DECODE(@[cost_dtl_cd], 'DSP', TO_DATE(@[mnr_dmg_flg_dt],'YYYY-MM-DD'), A.MNR_DISP_FLG_DT),
	     A.MNR_SCRP_FLG_DT    = DECODE(@[cost_dtl_cd], 'SCR', TO_DATE(@[mnr_dmg_flg_dt],'YYYY-MM-DD'), A.MNR_SCRP_FLG_DT),
	     A.MNR_DONA_FLG_DT    = DECODE(@[cost_dtl_cd], 'DON', TO_DATE(@[mnr_dmg_flg_dt],'YYYY-MM-DD'), A.MNR_DONA_FLG_DT),
		 A.MNR_STS_RMK = @[mnr_sts_rmk]
WHERE A.EQ_NO = @[eq_no]			]]></sql>
			<params>
				<param name="cost_dtl_cd" type="12" value="" out="N"/>
				<param name="mnr_dmg_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="mnr_dmg_flg_yd_cd" type="12" value="" out="N"/>
				<param name="mnr_dmg_flg_dt" type="12" value="" out="N"/>
				<param name="mnr_sts_rmk" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
