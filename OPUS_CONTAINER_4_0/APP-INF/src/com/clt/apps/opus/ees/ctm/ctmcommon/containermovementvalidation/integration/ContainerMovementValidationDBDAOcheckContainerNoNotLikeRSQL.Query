<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOcheckContainerNoNotLikeRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO,
       CNMV_STS_CD,
       CNTR_TPSZ_CD,
       CRNT_YD_CD,
       ACIAC_DIV_CD,
       CO_CRE_FLG,
       IMDT_EXT_FLG,
	   CNMV_DT,
       FULL_FLG
  FROM (SELECT CNTR_NO,
               CNMV_STS_CD,
               CNTR_TPSZ_CD,
               CRNT_YD_CD,
               ACIAC_DIV_CD,
               CO_CRE_FLG,
               IMDT_EXT_FLG,
			   TO_CHAR(CNMV_DT, 'YYYY-MM-DD HH24:MI:SS') AS CNMV_DT,
               FULL_FLG
          FROM MST_CONTAINER
#if (${cntr_no} != '')
         WHERE CNTR_NO = @[cntr_no]
#end
        ORDER BY CNMV_DT DESC)
 WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
