<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOSearchFATargetListDataRSQL">
			<desc><![CDATA[SearchFATargetListData]]></desc>
			<sql><![CDATA[
#if (${hid_type} == '0') 
SELECT
   A.LOT_PLN_YR||'-'||A.LOT_LOC_CD||'-'||A.CNTR_TPSZ_CD||'-'||LTRIM(TO_CHAR(A.LOT_SEQ, '000')) LOT_NO,
   A.CNTR_TPSZ_CD,
   A.LOT_CNTR_PFX_CD||A.FM_SER_NO||'~'||A.TO_SER_NO SER_RANGE,
   A.TO_SER_NO - A.FM_SER_NO + 1 CNTR_QTY,
   A.CNTR_AQZ_AMT,
   A.ACCT_QTY_MZD_CD,
   A.CNTR_INVST_NO,
   NVL(A.CNTR_CURR_CD, 'USD') CNTR_CURR_CD,
   A.CNTR_SPEC_NO,
   MST_COMMON_PKG.MST_AGMT_NO_CONV_FNC(A.AGMT_CTY_CD,A.AGMT_SEQ)  AS AGMT_NO,
   A.AGMT_CTY_CD,
   A.AGMT_SEQ,
   C.VNDR_ABBR_NM,
   A.LOT_LOC_CD,
   TO_CHAR(A.CRE_DT, 'YYYY-MM-DD') CRE_DT,
   DECODE(A.FA_IF_GRP_STS_CD, NULL,'Not Interface','E','Error','S','Sending','C','Completed') FA_IF_GRP_STS_CD,
   A.FA_IF_DT,
   A.LOT_PLN_YR,
   A.LOT_SEQ,
   (SELECT COUNT(*)
      FROM MST_CONTAINER MC 
     WHERE 1=1
       AND A.LOT_PLN_YR = MC.LOT_PLN_YR
       AND A.LOT_LOC_CD = MC.LOT_LOC_CD
       AND A.CNTR_TPSZ_CD = MC.CNTR_TPSZ_CD
       AND A.LOT_SEQ      = MC.LOT_SEQ
       AND MC.ACIAC_DIV_CD = 'A'
       AND MC.LSTM_CD IN ('LP', 'OW', 'OL')) AS ACT_QTY,
   (SELECT MV.VNDR_SEQ FROM MDM_VENDOR MV WHERE 1=1 AND MV.VNDR_SEQ = D.VNDR_SEQ ) AS ORG_VNDR_SEQ,
   (SELECT MV.VNDR_LGL_ENG_NM FROM MDM_VENDOR MV WHERE 1=1 AND MV.VNDR_SEQ = D.VNDR_SEQ ) AS ORG_VNDR_LGL_ENG_NM
FROM
   MST_CNTR_LOT A,
   MDM_VENDOR C,
   LSE_AGREEMENT D
WHERE 1 = 1
AND A.DE_YRMON = REPLACE(@[de_yrmon],'-','')
#if (${if_cd} == '1') 
AND NVL(A.FA_IF_GRP_STS_CD,'0') = '0'
#end
#if (${if_cd} == '2') 
AND NVL(A.FA_IF_GRP_STS_CD,'0') != '0'
#end
AND C.VNDR_SEQ        = A.MFT_VNDR_SEQ
AND D.AGMT_CTY_CD  = A.AGMT_CTY_CD
AND D.AGMT_SEQ     = A.AGMT_SEQ
AND D.LSTM_CD = 'OW'
AND A.OWN_CNTR_FLG = 'Y'
#end
#if (${hid_type} == '1') 
SELECT 
   A.TERM_CNG_SEQ,
   A.CNTR_TPSZ_CD,
   A.CNTR_QTY,
   A.CNTR_AQZ_AMT,
   A.ACCT_QTY_MZD_CD,
   A.CNTR_INVST_NO,
   NVL(A.CNTR_CURR_CD, 'USD') CNTR_CURR_CD,
   MST_COMMON_PKG.MST_AGMT_NO_CONV_FNC(A.AGMT_CTY_CD,A.AGMT_SEQ)  AS AGMT_NO,
   A.AGMT_CTY_CD,
   A.AGMT_SEQ,
   TO_CHAR(A.CRE_DT, 'YYYY-MM-DD') CRE_DT,
   A.CRE_USR_ID,
   DECODE(A.FA_IF_GRP_STS_CD, NULL,'Not Interface','E','Error','S','Sending','C','Completed') FA_IF_GRP_STS_CD,
   A.FA_IF_DT,
   A.TERM_CNG_SEQ,
   (SELECT COUNT(*)
      FROM MST_CONTAINER MC
     WHERE 1=1
       AND A.TERM_CNG_SEQ  = MC.TERM_CNG_SEQ
       AND MC.ACIAC_DIV_CD = 'A') AS ACT_QTY,
   MST_COMMON_PKG.MST_AGMT_NO_CONV_FNC(A.FM_AGMT_CTY_CD, A.FM_AGMT_SEQ) AS ORG_AGMT_NO,
   (SELECT MV.VNDR_SEQ 
      FROM MDM_VENDOR MV, LSE_AGREEMENT LA
     WHERE 1=1 
       AND MV.VNDR_SEQ    = LA.VNDR_SEQ 
       AND LA.AGMT_CTY_CD = A.FM_AGMT_CTY_CD
       AND LA.AGMT_SEQ    = A.FM_AGMT_SEQ) AS ORG_VNDR_SEQ,
   (SELECT MV.VNDR_LGL_ENG_NM 
      FROM MDM_VENDOR MV, LSE_AGREEMENT LA
      WHERE 1=1 
       AND MV.VNDR_SEQ    = LA.VNDR_SEQ 
       AND LA.AGMT_CTY_CD = A.FM_AGMT_CTY_CD
       AND LA.AGMT_SEQ    = A.FM_AGMT_SEQ) AS ORG_VNDR_LGL_ENG_NM,
   (SELECT MV.VNDR_SEQ 
      FROM MDM_VENDOR MV, LSE_AGREEMENT LA
      WHERE 1=1 
       AND MV.VNDR_SEQ    = LA.VNDR_SEQ 
       AND LA.AGMT_CTY_CD = A.AGMT_CTY_CD
       AND LA.AGMT_SEQ    = A.AGMT_SEQ) AS VNDR_SEQ,
   (SELECT MV.VNDR_LGL_ENG_NM 
      FROM MDM_VENDOR MV, LSE_AGREEMENT LA
     WHERE 1=1 
       AND MV.VNDR_SEQ    = LA.VNDR_SEQ 
       AND LA.AGMT_CTY_CD = A.AGMT_CTY_CD
       AND LA.AGMT_SEQ    = A.AGMT_SEQ) AS VNDR_LGL_ENG_NM,
   (SELECT MCL.LOT_CNTR_PFX_CD||MCL.FM_SER_NO||'~'||MCL.TO_SER_NO
      FROM MST_CNTR_LOT MCL, MST_CONTAINER MC
     WHERE 1=1
       AND MC.TERM_CNG_SEQ  = A.TERM_CNG_SEQ
		AND MC.LOT_PLN_YR    = MCL.LOT_PLN_YR
  		AND MC.LOT_LOC_CD    = MCL.LOT_LOC_CD
  		AND MC.CNTR_TPSZ_CD  = MCL.CNTR_TPSZ_CD
  		AND MC.LOT_SEQ       = MCL.LOT_SEQ
       AND ROWNUM = 1) AS SN_RNG
FROM 
   MST_CNTR_TERM_CNG A
WHERE 1 = 1
AND A.AGMT_FM_DT BETWEEN TO_DATE(REPLACE(@[de_yrmon],'-','')||'01','YYYYMMDD') AND LAST_DAY(TO_DATE(REPLACE(@[de_yrmon],'-','')||'01','YYYYMMDD'))
#if (${if_cd} == '1') 
AND NVL(A.FA_IF_GRP_STS_CD,'0') = '0'
#end
#if (${if_cd} == '2') 
AND NVL(A.FA_IF_GRP_STS_CD,'0') != '0'
#end
ORDER BY A.TERM_CNG_SEQ
#end			]]></sql>
			<params>
				<param name="de_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
