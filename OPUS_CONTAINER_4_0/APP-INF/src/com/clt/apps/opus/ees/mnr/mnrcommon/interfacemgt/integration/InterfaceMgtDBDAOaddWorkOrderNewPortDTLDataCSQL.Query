<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOaddWorkOrderNewPortDTLDataCSQL">
			<desc><![CDATA[addWorkOrderNewPortDTLData]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_ORD_DTL(
         MNR_ORD_OFC_CTY_CD
        ,MNR_ORD_SEQ
        ,ORD_DTL_SEQ
        ,COST_CD
        ,ACCT_CD
        ,COST_DTL_CD
        ,RPR_OFFH_FLG
        ,MNR_RT_TP_CD
        ,MNR_EXPN_DTL_NM
        ,EQ_NO
        ,EQ_TPSZ_CD
        ,RQST_REF_NO
        ,YD_CD
        ,RPR_RSLT_DT
        ,RPR_QTY
        ,SPR_PRT_UC_AMT
        ,BZC_AMT
        ,COST_AMT
        ,N3PTY_FLG
        ,N3PTY_BIL_TTL_AMT
		,INV_AMT
        ,INV_NO
        ,PAY_INV_SEQ
        ,MNR_INP_TP_CD
        ,MNR_VRFY_TP_CD
        ,BKG_NO
        ,TRD_CD
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
        ,VSL_CD
        ,SKD_VOY_NO
        ,SKD_DIR_CD
        ,REV_DIR_CD
        ,SLAN_CD
)
VALUES (
            @[mnr_ord_ofc_cty_cd]
           ,@[mnr_ord_seq]
           ,@[ord_dtl_seq] 
		   ,@[cost_cd]
           , MNR_COMMON_PKG.MNR_GET_ACCT_CD_FNC(@[eq_knd_cd], NVL(@[cost_cd], DECODE(@[eq_knd_cd], 'U', 'MRDRRC', 'Z', 'MRZSRC', 'G', 'MRGSRC')), @[eq_tpsz_cd], 'N', @[cost_dtl_cd]) 
           ,NVL(@[cost_dtl_cd], 'NR')
           ,'N'
           ,NVL(@[cost_dtl_cd], 'NR')
           ,''
           ,@[eq_no]
           ,@[eq_tpsz_cd] -- eq_type
		   ,@[rqst_ref_no]
		   ,@[yd_cd] -- DETAIL.EVENT_LOC (Need to convert code)
		   ,TO_DATE(@[rpr_rslt_dt], 'yyyymmdd') -- detail EVENT DT
		   ,@[rpr_qty]
		   ,0
		   ,0
		   ,@[inv_amt]
           ,'N'
		   ,''
		   ,@[inv_amt]
		   ,@[inv_no]
		   ,@[pay_inv_seq] --MNR_PAY_INV_WRK.PAY_INV_SEQ
           ,@[mnr_inp_tp_cd]
           ,@[mnr_vrfy_tp_cd]
           ,@[bkg_no]
           ,@[trd_cd]
           ,@[cre_usr_id]
           ,SYSDATE
           ,@[upd_usr_id]
           ,SYSDATE
           ,@[vsl_cd]
           ,@[skd_voy_no]
           ,@[skd_dir_cd]
           ,@[rev_dir_cd]
           ,@[slan_cd]
)			]]></sql>
			<params>
				<param name="mnr_ord_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="mnr_ord_seq" type="12" value="" out="N"/>
				<param name="ord_dtl_seq" type="12" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="cost_dtl_cd" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="rqst_ref_no" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="rpr_rslt_dt" type="12" value="" out="N"/>
				<param name="rpr_qty" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="pay_inv_seq" type="12" value="" out="N"/>
				<param name="mnr_inp_tp_cd" type="12" value="" out="N"/>
				<param name="mnr_vrfy_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
