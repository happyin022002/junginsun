<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyInvoiceRateUSQL">
			<desc><![CDATA[Manual Invoice Creation & Issue 화면에서 변경된 Rate 정보를 수정하기 위한 쿼리]]></desc>
			<sql><![CDATA[
UPDATE DMT_INV_RT SET	
		FT_OVR_DYS = @[ft_ovr_dys]
	,	FT_UND_DYS = @[ft_und_dys]
	,	INV_RT_AMT = @[inv_rt_amt]
	,	RT_OVR_DYS = @[rt_ovr_dys]
	,	RT_OVR_CHG_AMT = @[rt_ovr_chg_amt]
	,	BZC_CURR_CD = @[bzc_curr_cd]
	,	UPD_USR_ID = @[upd_usr_id]
	,	UPD_DT = SYSDATE
	, 	UPD_OFC_CD = @[upd_ofc_cd]
WHERE	DMDT_INV_NO = @[dmdt_inv_no]
	AND	CRE_OFC_CD = @[cre_ofc_cd]
	AND INV_DTL_SEQ = @[inv_dtl_seq]
	AND INV_RT_SEQ = @[inv_rt_seq]			]]></sql>
			<params>
				<param name="ft_ovr_dys" type="12" value="" out="N"/>
				<param name="ft_und_dys" type="12" value="" out="N"/>
				<param name="inv_rt_amt" type="12" value="" out="N"/>
				<param name="rt_ovr_dys" type="12" value="" out="N"/>
				<param name="rt_ovr_chg_amt" type="12" value="" out="N"/>
				<param name="bzc_curr_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="inv_dtl_seq" type="12" value="" out="N"/>
				<param name="inv_rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
