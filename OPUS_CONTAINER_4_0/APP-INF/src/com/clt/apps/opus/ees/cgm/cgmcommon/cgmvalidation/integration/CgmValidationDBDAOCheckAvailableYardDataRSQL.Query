<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmValidationDBDAOCheckAvailableYardDataRSQL">
			<desc><![CDATA[CgmValidationDBDAOCheckAvailableYardDataRSQL]]></desc>
			<sql><![CDATA[
SELECT * FROM (
SELECT A.YD_CD FROM MDM_YARD A
WHERE YD_CD = @[yd_cd]
AND DELT_FLG ='N'
 UNION ALL
SELECT LSE_CO_YD_CD FROM MDM_LSE_CO_YD A
WHERE LSE_CO_YD_CD = @[yd_cd]
AND DELT_FLG ='N'
)			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
