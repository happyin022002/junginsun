<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchMvmtCntSameDayDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT COUNT(DISTINCT ORG_YD_CD) CNT
  FROM CTM_MOVEMENT
 WHERE CNTR_NO = @[cntr_no]
   AND TO_CHAR(CNMV_EVNT_DT, 'YYYYMMDD') = @[rpr_rslt_dt]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="rpr_rslt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
