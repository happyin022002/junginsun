<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchCountryByRhqOfcCdDataRSQL">
			<desc><![CDATA[searchCountryByRhqOfcCdData]]></desc>
			<sql><![CDATA[
SELECT DISTINCT MDC.CNT_CD AS CD_ID,MDC.CNT_NM AS CD_DESC FROM MDM_LOCATION MDL,MDM_COUNTRY MDC 
WHERE 1 = 1
#if(${searchcon} != 'A')
AND MDL.LOC_CD  IN (
  SELECT DISTINCT LOC_CD
  FROM  MDM_ORGANIZATION  A
  WHERE A.AR_HD_QTR_OFC_CD LIKE '%'|| NVL(@[searchcon],'') ||'%'
)
#end
AND MDL.CNT_CD = MDC.CNT_CD
ORDER BY MDC.CNT_CD
			]]></sql>
			<params>
				<param name="searchcon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
