<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DisposalMgtDBDAOsearchDisposalSeqDataRSQL">
			<desc><![CDATA[searchDisposalSeqData]]></desc>
			<sql><![CDATA[
SELECT P2.PRE_DSP_NO||
       DECODE(LENGTH(P2.PRE_DSP_SEQ),
              1,'00'||P2.PRE_DSP_SEQ,
              2,'0'||P2.PRE_DSP_SEQ,
              P2.PRE_DSP_SEQ)  DISP_NO
FROM (         
        SELECT @[ofc_cd] || '-' || TO_CHAR(SYSDATE,'YYMM') || '-' AS PRE_DSP_NO
              ,TO_CHAR(NVL(MAX(TO_NUMBER(SUBSTR(A.DISP_NO,LENGTH(A.DISP_NO)-2))),0)+1) PRE_DSP_SEQ
          FROM MNR_DISP_HDR A
         WHERE A.DISP_NO LIKE @[ofc_cd] || '-' || TO_CHAR(SYSDATE,'YYMM') || '-' ||'%'
     ) P2			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
