<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchComIntgCodeDataRSQL">
			<desc><![CDATA[Common INTG Code를 조회하는 Operation]]></desc>
			<sql><![CDATA[
SELECT CD_ID, CD_DESC
FROM (
SELECT 
  INTG_CD_VAL_CTNT AS CD_ID,
  INTG_CD_VAL_DP_DESC AS CD_DESC
FROM COM_INTG_CD_DTL 
WHERE INTG_CD_ID = @[searchkey]
ORDER BY INTG_CD_VAL_DP_SEQ
)	
			]]></sql>
			<params>
				<param name="searchkey" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
