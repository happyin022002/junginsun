<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOSearchCHSAgreementLseTrRtDataRSQL">
			<desc><![CDATA[ChassisMgsetAgreementDB.SearchCHSAgreementLseTrRtData]]></desc>
			<sql><![CDATA[
#if (${eq_rntl_tp_cd} == 'F') 
	SELECT
NVL(SUM(EQ_TPSZ_CD1),0) AS EQ_TPSZ_CD1,
NVL(SUM(EQ_TPSZ_CD2),0) AS EQ_TPSZ_CD2,
NVL(SUM(EQ_TPSZ_CD3),0) AS EQ_TPSZ_CD3,
NVL(SUM(EQ_TPSZ_CD4),0) AS EQ_TPSZ_CD4,
NVL(SUM(EQ_TPSZ_CD5),0) AS EQ_TPSZ_CD5,
NVL(SUM(EQ_TPSZ_CD6),0) AS EQ_TPSZ_CD6,
NVL(SUM(EQ_TPSZ_CD7),0) AS EQ_TPSZ_CD7,
NVL(SUM(EQ_TPSZ_CD8),0) AS EQ_TPSZ_CD8,
NVL(SUM(EQ_TPSZ_CD9),0) AS EQ_TPSZ_CD9,
NVL(SUM(EQ_TPSZ_CD10),0) AS EQ_TPSZ_CD10,
NVL(SUM(EQ_TPSZ_CD11),0) AS EQ_TPSZ_CD11,
NVL(SUM(EQ_TPSZ_CD12),0) AS EQ_TPSZ_CD12,
NVL(SUM(EQ_TPSZ_CD13),0) AS EQ_TPSZ_CD13,
NVL(SUM(EQ_TPSZ_CD14),0) AS EQ_TPSZ_CD14,
NVL(SUM(EQ_TPSZ_CD15),0) AS EQ_TPSZ_CD15,
NVL(SUM(EQ_TPSZ_CD16),0) AS EQ_TPSZ_CD16,
NVL(SUM(EQ_TPSZ_CD17),0) AS EQ_TPSZ_CD17,
NVL(SUM(EQ_TPSZ_CD18),0) AS EQ_TPSZ_CD18,
NVL(SUM(EQ_TPSZ_CD19),0) AS EQ_TPSZ_CD19,
NVL(SUM(EQ_TPSZ_CD20),0) AS EQ_TPSZ_CD20
FROM (
SELECT
CASE WHEN T1.DP_SEQ = '1' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD1,
CASE WHEN T1.DP_SEQ = '2' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD2,
CASE WHEN T1.DP_SEQ = '3' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD3,
CASE WHEN T1.DP_SEQ = '4' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD4,
CASE WHEN T1.DP_SEQ = '5' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD5,
CASE WHEN T1.DP_SEQ = '6' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD6,
CASE WHEN T1.DP_SEQ = '7' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD7,
CASE WHEN T1.DP_SEQ = '8' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD8,
CASE WHEN T1.DP_SEQ = '9' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD9,
CASE WHEN T1.DP_SEQ = '10' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD10,
CASE WHEN T1.DP_SEQ = '11' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD11,
CASE WHEN T1.DP_SEQ = '12' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD12,
CASE WHEN T1.DP_SEQ = '13' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD13,
CASE WHEN T1.DP_SEQ = '14' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD14,
CASE WHEN T1.DP_SEQ = '15' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD15,
CASE WHEN T1.DP_SEQ = '16' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD16,
CASE WHEN T1.DP_SEQ = '17' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD17,
CASE WHEN T1.DP_SEQ = '18' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD18,
CASE WHEN T1.DP_SEQ = '19' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD19,
CASE WHEN T1.DP_SEQ = '20' THEN T1.CHSS_LSE_RT_AMT END AS EQ_TPSZ_CD20
    	FROM
        	(
        	SELECT  B.EQ_TPSZ_CD, 
                	B.CHSS_LSE_RT_AMT,
					G.DP_SEQ
			FROM    CGM_AGREEMENT A, CGM_AGMT_LSE_RT B , 
		           (SELECT EQ_TPSZ_CD,ROW_NUMBER() OVER (ORDER BY EQ_TPSZ_REP_CD,EQ_TPSZ_CD) DP_SEQ 
  		              FROM CGM_EQ_TP_SZ WHERE EQ_KND_CD = 'Z' GROUP BY EQ_TPSZ_REP_CD,EQ_TPSZ_CD) G
        	WHERE   A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD
                	AND A.AGMT_SEQ = B.AGMT_SEQ
                	AND A.AGMT_VER_NO = B.AGMT_VER_NO
                	AND A.EQ_KND_CD = @[eq_knd_cd]
                	AND A.AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
                	AND A.AGMT_SEQ = @[agmt_seq]
					AND A.AGMT_VER_NO = @[agmt_ver_no]
					AND A.EQ_RNTL_TP_CD = @[eq_rntl_tp_cd]
					AND B.EQ_TPSZ_CD = G.EQ_TPSZ_CD
        	) T1
	)
#else 
	SELECT
    	RNTL_FM_TR_VAL,
    	RNTL_TO_TR_VAL,
NVL(SUM(EQ_TPSZ_CD1),0) AS EQ_TPSZ_CD1,
NVL(SUM(EQ_TPSZ_CD2),0) AS EQ_TPSZ_CD2,
NVL(SUM(EQ_TPSZ_CD3),0) AS EQ_TPSZ_CD3,
NVL(SUM(EQ_TPSZ_CD4),0) AS EQ_TPSZ_CD4,
NVL(SUM(EQ_TPSZ_CD5),0) AS EQ_TPSZ_CD5,
NVL(SUM(EQ_TPSZ_CD6),0) AS EQ_TPSZ_CD6,
NVL(SUM(EQ_TPSZ_CD7),0) AS EQ_TPSZ_CD7,
NVL(SUM(EQ_TPSZ_CD8),0) AS EQ_TPSZ_CD8,
NVL(SUM(EQ_TPSZ_CD9),0) AS EQ_TPSZ_CD9,
NVL(SUM(EQ_TPSZ_CD10),0) AS EQ_TPSZ_CD10,
NVL(SUM(EQ_TPSZ_CD11),0) AS EQ_TPSZ_CD11,
NVL(SUM(EQ_TPSZ_CD12),0) AS EQ_TPSZ_CD12,
NVL(SUM(EQ_TPSZ_CD13),0) AS EQ_TPSZ_CD13,
NVL(SUM(EQ_TPSZ_CD14),0) AS EQ_TPSZ_CD14,
NVL(SUM(EQ_TPSZ_CD15),0) AS EQ_TPSZ_CD15,
NVL(SUM(EQ_TPSZ_CD16),0) AS EQ_TPSZ_CD16,
NVL(SUM(EQ_TPSZ_CD17),0) AS EQ_TPSZ_CD17,
NVL(SUM(EQ_TPSZ_CD18),0) AS EQ_TPSZ_CD18,
NVL(SUM(EQ_TPSZ_CD19),0) AS EQ_TPSZ_CD19,
NVL(SUM(EQ_TPSZ_CD20),0) AS EQ_TPSZ_CD20
	FROM
    	(
    	SELECT
        	T1.RNTL_FM_TR_VAL, 
        	T1.RNTL_TO_TR_VAL,
			CASE WHEN T1.DP_SEQ = '1' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD1,
			CASE WHEN T1.DP_SEQ = '2' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD2,
			CASE WHEN T1.DP_SEQ = '3' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD3,
			CASE WHEN T1.DP_SEQ = '4' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD4,
			CASE WHEN T1.DP_SEQ = '5' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD5,
			CASE WHEN T1.DP_SEQ = '6' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD6,
			CASE WHEN T1.DP_SEQ = '7' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD7,
			CASE WHEN T1.DP_SEQ = '8' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD8,
			CASE WHEN T1.DP_SEQ = '9' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD9,
			CASE WHEN T1.DP_SEQ = '10' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD10,
			CASE WHEN T1.DP_SEQ = '11' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD11,
			CASE WHEN T1.DP_SEQ = '12' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD12,
			CASE WHEN T1.DP_SEQ = '13' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD13,
			CASE WHEN T1.DP_SEQ = '14' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD14,
			CASE WHEN T1.DP_SEQ = '15' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD15,
			CASE WHEN T1.DP_SEQ = '16' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD16,
			CASE WHEN T1.DP_SEQ = '17' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD17,
			CASE WHEN T1.DP_SEQ = '18' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD18,
			CASE WHEN T1.DP_SEQ = '19' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD19,
			CASE WHEN T1.DP_SEQ = '20' THEN SUM(T1.TR_RT_AMT) END AS EQ_TPSZ_CD20
    	FROM
        	(
        	SELECT  B.EQ_TPSZ_CD,
                	B.RNTL_FM_TR_VAL,
                	B.RNTL_TO_TR_VAL,
                	B.TR_RT_AMT,
					G.DP_SEQ
			FROM    CGM_AGREEMENT A, CGM_AGMT_LSE_TR_RT B ,  
					(SELECT EQ_TPSZ_CD,ROW_NUMBER() OVER (ORDER BY EQ_TPSZ_REP_CD,EQ_TPSZ_CD) DP_SEQ 
  					   FROM CGM_EQ_TP_SZ WHERE EQ_KND_CD = 'Z' GROUP BY EQ_TPSZ_REP_CD,EQ_TPSZ_CD) G
        	WHERE   A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD
                	AND A.AGMT_SEQ = B.AGMT_SEQ
                	AND A.AGMT_VER_NO = B.AGMT_VER_NO
                	AND A.EQ_KND_CD = @[eq_knd_cd]
                	AND A.AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
                	AND A.AGMT_SEQ = @[agmt_seq] 
                	AND A.AGMT_VER_NO = @[agmt_ver_no]
                	AND A.EQ_RNTL_TP_CD = @[eq_rntl_tp_cd]
					AND B.EQ_TPSZ_CD = G.EQ_TPSZ_CD
        	) T1
    	GROUP BY T1.RNTL_FM_TR_VAL, T1.RNTL_TO_TR_VAL, T1.EQ_TPSZ_CD,T1.DP_SEQ
	)
	GROUP BY RNTL_FM_TR_VAL, RNTL_TO_TR_VAL
	ORDER BY RNTL_FM_TR_VAL, RNTL_TO_TR_VAL
#end			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="agmt_ver_no" type="2" value="" out="N"/>
				<param name="eq_rntl_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
