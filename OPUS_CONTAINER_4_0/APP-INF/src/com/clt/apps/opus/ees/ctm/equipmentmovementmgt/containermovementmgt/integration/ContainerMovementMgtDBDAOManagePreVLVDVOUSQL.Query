<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOManagePreVLVDVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE CTM_MVMT_RSV
   SET CNMV_EVNT_DT = TO_DATE (@[event_dt], 'YYYY-MM-DD HH24:MI'),
#if (${error_status} == 'Y')
       CNMV_RMK = '',
       PRE_STS_FLG = 'N',
#else
       CNMV_RMK = REPLACE(@[remark], '^#^', CHR(39)),
#end
       GMT_DT = GLOBALDATE_PKG.TIME_CONV_FNC (SUBSTR (@[org_yard], 0, 5), TO_DATE (@[event_dt], 'YYYY-MM-DD HH24:MI'), 'GMT'),
       UPD_LOCL_DT = GLOBALDATE_PKG.TIME_LOCAL_FNC(SUBSTR (@[org_yard], 0, 5 )),
       UPD_USR_ID = @[user_id],
       UPD_DT = SYSDATE
 WHERE CNTR_NO = @[cntr_no]
   AND CNMV_STS_CD = @[status]
   AND CNMV_EVNT_DT = TO_DATE (@[org_event_dt], 'YYYY-MM-DD HH24:MI')			]]></sql>
			<params>
				<param name="event_dt" type="12" value="" out="N"/>
				<param name="remark" type="12" value="" out="N"/>
				<param name="org_yard" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="status" type="12" value="" out="N"/>
				<param name="org_event_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
