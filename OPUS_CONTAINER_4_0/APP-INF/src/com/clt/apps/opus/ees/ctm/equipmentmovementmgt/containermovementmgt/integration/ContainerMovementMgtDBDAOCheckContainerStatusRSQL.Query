<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOCheckContainerStatusRSQL">
			<desc><![CDATA[CheckContainerStatus]]></desc>
			<sql><![CDATA[
SELECT 1 AS RESULT
  FROM MST_CONTAINER
 WHERE 1=1
   AND ((FULL_FLG = 'N' AND CNMV_STS_CD IN ('MT', 'EN', 'TN'))
    OR CNMV_STS_CD = 'ID')
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
