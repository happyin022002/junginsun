<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchOfcComboCodeListDataRSQL">
			<desc><![CDATA[searchOfcComboCodeListData]]></desc>
			<sql><![CDATA[
#if (${searchcon} != 'DISP')
SELECT A.OFC_CD AS CD_ID,A.OFC_CD AS CD_DESC
FROM MNR_OFC_GEN_INFO A
WHERE A.UPPR_OFC_CD  = @[searchkey]
	  AND A.MNR_GRP_TP_CD = 'OFC'
      AND A.COST_CD       = 'RPROFC'
UNION ALL
#end
SELECT @[searchkey] AS CD_ID,@[searchkey] AS CD_DESC FROM DUAL			]]></sql>
			<params>
				<param name="searchkey" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
