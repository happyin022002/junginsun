<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchTypeSizeListDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
#if (${knd_cd} == 'U') 
SELECT A.CNTR_TPSZ_CD    CD_ID,
       A.CNTR_TPSZ_DESC  CD_DESC
  FROM MDM_CNTR_TP_SZ A
 WHERE @[knd_cd] = 'U'
 #if (${type_size_search_ind} == 'A')
 AND ACIAC_DIV_CD = 'A'	
 #end 
 #if(${order_by_col_nm} == 'Y')
 ORDER BY A.CNTR_TPSZ_CD
 #else
 ORDER BY RPT_DP_SEQ
 #end
#else 
SELECT A.EQ_TPSZ_CD    CD_ID,
       A.DIFF_DESC     CD_DESC
  FROM CGM_EQ_TP_SZ A 
 WHERE A.EQ_KND_CD = @[knd_cd]

 #if(${order_by_col_nm} == 'Y') 
 ORDER BY A.EQ_TPSZ_CD
 #else
 ORDER BY DP_SEQ
 #end
#end			]]></sql>
			<params>
				<param name="knd_cd" type="12" value="U" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
