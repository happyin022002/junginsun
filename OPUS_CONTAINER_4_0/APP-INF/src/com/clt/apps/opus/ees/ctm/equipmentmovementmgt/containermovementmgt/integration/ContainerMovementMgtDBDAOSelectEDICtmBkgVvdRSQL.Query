<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSelectEDICtmBkgVvdRSQL">
			<desc><![CDATA[SelectEDICtmBkgVvd]]></desc>
			<sql><![CDATA[
select count(*) CNT from CTM_BKG_VVD
where 1=1
and BKG_NO = @[bkg_no]
and VSL_PRE_PST_CD = @[vsl_pre_pst_cd]
and VSL_SEQ = @[vsl_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
