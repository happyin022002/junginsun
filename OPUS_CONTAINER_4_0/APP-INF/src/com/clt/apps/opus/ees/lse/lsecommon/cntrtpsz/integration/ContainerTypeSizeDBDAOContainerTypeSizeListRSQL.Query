		<query name="ContainerTypeSizeDBDAOContainerTypeSizeListRSQL">
			<desc><![CDATA[Container Type/Size Search]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD,
       CNTR_SZ_CD,
       CNTR_TP_CD,
       CNTR_TPSZ_DESC,
       CNTR_TPSZ_RMK,
       CNTR_TPSZ_ISO_CD,
       CNTR_TPSZ_GRP_CD
FROM MDM_CNTR_TP_SZ
WHERE DELT_FLG='N'
AND ACIAC_DIV_CD='A'
ORDER BY RPT_DP_SEQ			]]></sql>
