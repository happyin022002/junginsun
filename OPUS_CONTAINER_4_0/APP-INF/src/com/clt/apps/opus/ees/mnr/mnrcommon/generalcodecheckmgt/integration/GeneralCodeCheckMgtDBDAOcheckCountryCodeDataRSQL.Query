<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOcheckCountryCodeDataRSQL">
			<desc><![CDATA[조회 - Country 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT A.CNT_CD   AS CD_ID
     , A.CNT_NM   AS COD_DESC
  FROM MDM_COUNTRY A
 WHERE 1=1
   AND NVL(A.DELT_FLG, 'N') <> 'Y'
   AND A.CNT_CD = @[check_value]			]]></sql>
			<params>
				<param name="check_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
