<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOaddOnhireApprovalNumberQtyHistoryCSQL">
			<desc><![CDATA[LSE_ONH_APRO_QTY_HIS를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_ONH_APRO_QTY_HIS (
    CNTR_ONH_AUTH_NO
  , AGMT_CTY_CD
  , AGMT_SEQ
  , CNTR_TPSZ_CD
  , NEW_VAN_TP_CD
  , ONH_APRO_SEQ
  , ONH_QTY
  , LFT_CHG_AMT
  , CRE_USR_ID
  , CRE_DT
  , UPD_USR_ID
  , UPD_DT
)
SELECT CNTR_ONH_AUTH_NO
     , AGMT_CTY_CD
     , AGMT_SEQ
     , CNTR_TPSZ_CD
     , NEW_VAN_TP_CD
     , @[onh_apro_seq]
     , ONH_QTY
     , LFT_CHG_AMT
     , @[usr_id]
     , SYSDATE
     , @[usr_id]
     , SYSDATE
  FROM LSE_ONH_APRO_QTY
 WHERE CNTR_ONH_AUTH_NO = @[cntr_onh_auth_no]			]]></sql>
			<params>
				<param name="onh_apro_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cntr_onh_auth_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
