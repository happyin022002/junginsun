<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOSearchVVDCodeRSQL">
			<desc><![CDATA[Booking Table의 Trunk VVD를 조회한다]]></desc>
			<sql><![CDATA[
SELECT	 VSL_CD || SKD_VOY_NO || SKD_DIR_CD AS VVD_CD
FROM	BKG_BOOKING
WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
