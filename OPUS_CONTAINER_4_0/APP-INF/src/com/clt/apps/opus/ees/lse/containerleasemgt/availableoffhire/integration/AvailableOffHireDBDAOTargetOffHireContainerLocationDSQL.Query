   LSE_AGMT_OFFH_PLC WHERE 1=1
AND AGMT_CTY_CD  = @[agmt_cty_cd]
AND AGMT_SEQ     = @[agmt_seq]
AND CNTR_TPSZ_CD = @[cntr_tpsz_cd] 			]]></sql>
