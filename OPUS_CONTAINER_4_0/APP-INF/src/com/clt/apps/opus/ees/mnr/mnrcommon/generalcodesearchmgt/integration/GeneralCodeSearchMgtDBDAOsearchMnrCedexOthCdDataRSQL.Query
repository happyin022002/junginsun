<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchMnrCedexOthCdDataRSQL">
			<desc><![CDATA[Cedex Other Code 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
         A.EQ_CEDEX_OTR_CD AS CD_ID
        ,A.EQ_CEDEX_OTR_CD_NM AS CD_DESC
FROM MNR_CEDEX_OTR_CD A
#if (${searchcon} == 'COMMON') 
 	WHERE A.EQ_CEDEX_OTR_TP_CD = @[searchkey]
#end
ORDER BY A.EQ_CEDEX_OTR_NUM_CD			]]></sql>
			<params>
				<param name="searchkey" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
