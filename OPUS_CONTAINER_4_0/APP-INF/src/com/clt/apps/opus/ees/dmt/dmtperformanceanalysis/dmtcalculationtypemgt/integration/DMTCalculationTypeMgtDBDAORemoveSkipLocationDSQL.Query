<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationTypeMgtDBDAORemoveSkipLocationDSQL">
			<desc><![CDATA[Skip Location 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE
  FROM DMT_SKP_LOC
 WHERE KEY_LOC_CD = @[key_loc_cd]
   AND SKP_LOC_CD = @[skp_loc_cd]			]]></sql>
			<params>
				<param name="key_loc_cd" type="12" value="" out="N"/>
				<param name="skp_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
