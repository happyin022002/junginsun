<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOLocationCd2RSQL">
			<desc><![CDATA[rhq]]></desc>
			<sql><![CDATA[
SELECT VOP_PORT_RHQ_CD AS LOC_CD
FROM MDM_LOCATION
WHERE LOC_CD = @[loc_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
