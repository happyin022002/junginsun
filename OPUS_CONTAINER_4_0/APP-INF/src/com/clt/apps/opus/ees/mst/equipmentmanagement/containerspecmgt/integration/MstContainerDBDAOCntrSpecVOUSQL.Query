<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstContainerDBDAOCntrSpecVOUSQL">
			<desc><![CDATA[Container Specification Creation 화면에서 Year 수정시 Spec No 업데이트되는 쿼리]]></desc>
			<sql><![CDATA[
UPDATE MST_CONTAINER SET 
    CNTR_SPEC_NO = @[cntr_spec_no]
WHERE
    CNTR_SPEC_NO = @[his_cntr_spec_no]			]]></sql>
			<params>
				<param name="cntr_spec_no" type="12" value="" out="N"/>
				<param name="his_cntr_spec_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
