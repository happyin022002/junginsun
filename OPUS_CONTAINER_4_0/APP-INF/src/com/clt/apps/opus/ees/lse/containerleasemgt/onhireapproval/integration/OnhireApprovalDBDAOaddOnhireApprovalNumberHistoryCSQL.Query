<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOaddOnhireApprovalNumberHistoryCSQL">
			<desc><![CDATA[LSE_ONH_APRO_HIS를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_ONH_APRO_HIS (
    AGMT_CTY_CD
  , AGMT_SEQ
  , CNTR_ONH_AUTH_NO
  , ONH_APRO_SEQ
  , LSEE_VNDR_SEQ
  , LSTM_CD
  , ONH_LOC_CD
  , PKUP_FM_DT
  , PKUP_DUE_DT
  , MFT_YR
  , FREE_DYS
  , PKUP_CHG_AMT
  , PKUP_CHG_CR_AMT
  , DPP_CHG_AMT
  , MIN_ONH_DYS
  , AUTH_DPP_FREE_DYS
  , APRO_RMK
  , DELT_FLG
  , CRE_USR_ID
  , CRE_DT
  , UPD_USR_ID
  , UPD_DT
  , EQ_LOC_TP_CD
  , LOC_CD
)
SELECT AGMT_CTY_CD
     , AGMT_SEQ
     , CNTR_ONH_AUTH_NO
     , @[onh_apro_seq]
     , LSEE_VNDR_SEQ
     , LSTM_CD
     , ONH_LOC_CD
     , PKUP_FM_DT
     , PKUP_DUE_DT
     , MFT_YR
     , FREE_DYS
     , PKUP_CHG_AMT
     , PKUP_CHG_CR_AMT
     , DPP_CHG_AMT
     , MIN_ONH_DYS
     , AUTH_DPP_FREE_DYS
     , APRO_RMK
     , DELT_FLG
     , @[usr_id]
     , SYSDATE
     , @[usr_id]
     , SYSDATE
     , EQ_LOC_TP_CD
     , LOC_CD
  FROM LSE_ONH_APRO
 WHERE CNTR_ONH_AUTH_NO = @[cntr_onh_auth_no]			]]></sql>
			<params>
				<param name="onh_apro_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cntr_onh_auth_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
