<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOCountryRSQL">
			<desc><![CDATA[Country 정보를 조회하기 위한 쿼리]]></desc>
			<sql><![CDATA[
SELECT 	CNT_CD, CNT_NM
FROM	MDM_COUNTRY
WHERE	DELT_FLG = 'N'
ORDER BY CNT_CD ASC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
