<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOcheckComponentCodeDataRSQL">
			<desc><![CDATA[checkComponentCodeData]]></desc>
			<sql><![CDATA[
SELECT
A.EQ_CMPO_CD AS CD_ID
,A.EQ_CMPO_NM AS CD_DESC
FROM MNR_EQ_CMPO_CD A
WHERE A.EQ_CMPO_GRP_TP_CD = '3'
AND A.EQ_CMPO_CD = @[check_value]			]]></sql>
			<params>
				<param name="check_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
