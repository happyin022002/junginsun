<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSubContinentRSQL">
			<desc><![CDATA[Sub Continent Combo List]]></desc>
			<sql><![CDATA[
SELECT ms.sconti_cd fm_conti_cd,
	ms.sconti_nm,
	m.conti_cd,
	m.conti_nm,
	(CASE 
		WHEN 'Y' = ms.delt_flg THEN 'Delete' 
		ELSE 'Active' 
	END) AS delt_flg
FROM mdm_subcontinent ms left outer join mdm_continent m ON ms.conti_cd = m.conti_cd
WHERE 1 = 1 ORDER BY ms.sconti_cd			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
