<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetVvdRSQL">
			<desc><![CDATA[GetVvd]]></desc>
			<sql><![CDATA[
SELECT MAP.VSL_CD||'0'||SUBSTR(TRIM(@[p_vvd]),4,4) chgd_vvd
FROM  MDM_VSL_CNTR MAP 
WHERE 1=1
and (MAP.MODI_VSL_CD LIKE SUBSTR(TRIM(@[p_vvd]),1,3)||'%' 
OR MAP.VSL_CD LIKE SUBSTR(TRIM(@[p_vvd]),1,3)||'%')
AND DELT_FLG != 'Y'
AND ROWNUM <= 1			]]></sql>
			<params>
				<param name="p_vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
