<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOAddBeforeExceptionTieredFreeTimeOfHistVersionCSQL">
			<desc><![CDATA[RFA Tiered Free Time of hist verson insert]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_RFA_EXPT_FREE_TM ( 
        RFA_EXPT_DAR_NO 
      , RFA_EXPT_MAPG_SEQ 
      , RFA_EXPT_VER_SEQ 
      , RFA_RQST_DTL_SEQ 
      , CVRG_CMB_SEQ 
      , FT_SEQ 
      , CNTR_FM_QTY 
      , CNTR_TO_QTY 
      , FT_DYS 
      , CRE_USR_ID 
      , CRE_DT 
      , UPD_USR_ID 
      , UPD_DT 
    )   
SELECT @[rfa_expt_dar_no]
	 , @[rfa_expt_mapg_seq]
	 , @[rfa_expt_ver_seq]
     , RFA_RQST_DTL_SEQ
     , CVRG_CMB_SEQ
     , FT_SEQ
     , CNTR_FM_QTY
     , CNTR_TO_QTY
     , FT_DYS
     , @[cre_usr_id]
     , SYSDATE
     , @[cre_usr_id]
     , SYSDATE
  FROM DMT_RFA_EXPT_FREE_TM
 WHERE 1=1
   AND RFA_EXPT_DAR_NO    = @[rfa_expt_hist_dar_no]
   AND RFA_EXPT_MAPG_SEQ  = @[rfa_expt_hist_mapg_seq]
   AND RFA_EXPT_VER_SEQ   = @[rfa_expt_hist_ver_seq]			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="rfa_expt_hist_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_hist_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_hist_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
