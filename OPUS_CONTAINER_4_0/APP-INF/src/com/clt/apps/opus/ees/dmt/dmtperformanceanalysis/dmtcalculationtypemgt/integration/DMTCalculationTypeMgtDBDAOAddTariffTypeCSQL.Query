<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationTypeMgtDBDAOAddTariffTypeCSQL">
			<desc><![CDATA[Tariff Type 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_TRF_TP (
    DMDT_TRF_CD, 
    DMDT_TRF_NM, 
    IO_BND_CD, 
    DMDT_CALC_TP_CD, 
    DMDT_TRF_DIV_CD,  
    CRE_USR_ID, 
    CRE_DT, 
    UPD_USR_ID, 
    UPD_DT
) VALUES (
    @[dmdt_trf_cd],
    @[dmdt_trf_nm],
    @[io_bnd_cd],
    @[mgmt_calc_tp_cd],
    @[mgmt_trf_div_cd],
    @[cre_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_trf_nm" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="mgmt_calc_tp_cd" type="12" value="" out="N"/>
				<param name="mgmt_trf_div_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
