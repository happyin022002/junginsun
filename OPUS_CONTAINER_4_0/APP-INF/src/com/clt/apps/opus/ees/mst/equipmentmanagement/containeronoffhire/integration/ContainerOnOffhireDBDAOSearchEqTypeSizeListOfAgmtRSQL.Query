<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOSearchEqTypeSizeListOfAgmtRSQL">
			<desc><![CDATA[장비 Lease시 발생한 AGMT No.에 등록된 장비의 Type & Size를 체크하여 조회한다.]]></desc>
			<sql><![CDATA[
SELECT AA.CNTR_TPSZ_CD AS CODE, AA.CNTR_TPSZ_DESC AS CODE_NM
  FROM MDM_CNTR_TP_SZ AA, 
      (SELECT DISTINCT CNTR_TPSZ_CD
        FROM LSE_AGMT_RT
       WHERE 1=1
         AND AGMT_CTY_CD = @[agmt_cty_cd]
         AND AGMT_SEQ    = @[agmt_seq]
         AND CNTR_RNTL_CHG_TP_CD = 'GENV'
       ) BB 
  WHERE 1=1
    AND AA.CNTR_TPSZ_CD = BB.CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
