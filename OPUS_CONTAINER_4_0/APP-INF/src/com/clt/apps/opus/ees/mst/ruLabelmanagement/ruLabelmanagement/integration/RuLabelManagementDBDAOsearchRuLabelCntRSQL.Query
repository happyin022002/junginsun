<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RuLabelManagementDBDAOsearchRuLabelCntRSQL">
			<desc><![CDATA[RU LABEL 등록여부]]></desc>
			<sql><![CDATA[
SELECT
	COUNT(RSTR_USG_CD_SEQ) RSTR_CNT		
FROM 
	MST_RSTR_USG_CD 
WHERE 1=1
AND DELT_FLG = 'N'
#if (${ru_label_type} != '') 
	AND RSTR_USG_TP_CD = @[ru_label_type]
#end

#if (${ru_label_value} != '')
	AND RSTR_USG_LBL_NM = @[ru_label_value]
#end
			]]></sql>
			<params>
				<param name="ru_label_type" type="12" value="" out="N"/>
				<param name="ru_label_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
