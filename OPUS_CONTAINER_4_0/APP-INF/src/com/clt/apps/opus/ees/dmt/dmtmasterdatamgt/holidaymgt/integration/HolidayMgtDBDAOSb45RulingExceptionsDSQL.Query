<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HolidayMgtDBDAOSb45RulingExceptionsDSQL">
			<desc><![CDATA[각 Country, Location, Yard, Bound 별로 등록된 SB45 Ruling Exception 정보 삭제용 쿼리]]></desc>
			<sql><![CDATA[
DELETE 
  FROM DMT_OVR_DY_EXCLU
 WHERE 1=1
   AND CNT_CD        = @[cnt_cd]
   AND LOC_CD        = NVL(@[loc_cd],' ')
   AND YD_CD         = NVL(@[yd_cd],' ')
   AND IO_BND_CD     = @[io_bnd_cd]
   AND DEM_DET_TP_CD = @[dem_det_tp_cd]
   AND EXCLU_SEQ     = @[exclu_seq]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dem_det_tp_cd" type="12" value="" out="N"/>
				<param name="exclu_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
