<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchRepCustSeqRSQL">
			<desc><![CDATA[오피스별 대표고객코드 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(REP_CUST_CNT_CD,'') ACT_CUST_CNT_CD,
       NVL(REP_CUST_SEQ,'') ACT_CUST_SEQ
  FROM MDM_ORGANIZATION
 WHERE OFC_CD = (SELECT AR_OFC_CD FROM MDM_ORGANIZATION WHERE OFC_CD = @[ofc_cd])
   AND NVL(DELT_FLG,'N') = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
