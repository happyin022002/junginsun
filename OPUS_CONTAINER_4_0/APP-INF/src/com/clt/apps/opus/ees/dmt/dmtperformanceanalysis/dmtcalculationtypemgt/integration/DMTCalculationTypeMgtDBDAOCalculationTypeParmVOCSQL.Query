<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationTypeMgtDBDAOCalculationTypeParmVOCSQL">
			<desc><![CDATA[CalculationTypeParmVO Insert]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_CALC_TP(
		CNT_CD,
        RGN_CD,
        STE_CD,
        LOC_CD,
        IO_BND_CD,
        CALC_TP_SEQ,
        DMDT_CALC_TP_CD,
        EFF_DT,
        EXP_DT,
        CRE_USR_ID,
        CRE_DT,
        CRE_OFC_CD,
        UPD_USR_ID,
        UPD_DT,
        UPD_OFC_CD
) VALUES (
		NVL(@[cnt_cd],' ')
	,	NVL(@[rgn_cd],' ')
	,   NVL(@[ste_cd],' ')
	,   NVL(@[loc_cd],' ')
	,   NVL(@[io_bnd_cd],' ')
	,   (
            SELECT NVL(CALC_TP_SEQ, 1) CALC_TP_SEQ
              FROM (
                    SELECT MAX(CALC_TP_SEQ) + 1 AS CALC_TP_SEQ
                      FROM  DMT_CALC_TP
                     WHERE CNT_CD	 =	NVL(@[cnt_cd], ' ')
                       AND RGN_CD	 =	NVL(@[rgn_cd], ' ')
                       AND STE_CD	 =	NVL(@[ste_cd], ' ')
                       AND LOC_CD	 =	NVL(@[loc_cd], ' ')
                       AND IO_BND_CD =	NVL(@[io_bnd_cd], ' ')
                   )
        )
    ,   NVL(@[dmdt_calc_tp_cd],' ')
    ,   TO_DATE(@[eff_dt], 'YYYYMMDD')
	,	TO_DATE(@[exp_dt], 'YYYYMMDD')
	,	@[cre_usr_id]
	,	SYSDATE
	,	@[cre_ofc_cd]
	,	@[cre_usr_id]
	,	SYSDATE
	,	@[cre_ofc_cd]
)			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dmdt_calc_tp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
