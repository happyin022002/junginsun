<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchRegionListByCountryRSQL">
			<desc><![CDATA[Country 에 해당되는 모든 Region 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT	RGN_CD, RGN_NM
FROM	MDM_REGION
WHERE	DELT_FLG = 'N'
	AND	CNT_CD = @[cnt_cd]
ORDER BY RGN_CD ASC			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
