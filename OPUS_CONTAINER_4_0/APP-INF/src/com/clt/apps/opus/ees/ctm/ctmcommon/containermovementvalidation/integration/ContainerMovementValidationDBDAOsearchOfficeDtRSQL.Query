<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOsearchOfficeDtRSQL">
			<desc><![CDATA[Office Date]]></desc>
			<sql><![CDATA[
