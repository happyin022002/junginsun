<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAORemoveCntrMasterByUpdateDataDSQL">
			<desc><![CDATA[RemoveCntrMasterByUpdateData]]></desc>
			<sql><![CDATA[
DELETE MST_CONTAINER MC
WHERE MC.CNTR_NO = @[cntr_no]
AND 'Y' = @[init_flg]
AND NOT EXISTS (SELECT 'X'
                FROM MST_CNTR_STS_HIS HIS
                WHERE MC.CNTR_NO = HIS.CNTR_NO)			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="init_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
