<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CEDEXCodeMgtDBDAOsearchDivisionCodeListDataRSQL">
			<desc><![CDATA[searchDivisionCodeListData]]></desc>
			<sql><![CDATA[
SELECT
         A.EQ_CEDEX_RLT_TP_CD
        ,A.FM_RLT_CD
        ,A.TO_RLT_CD
        ,A.COST_GRP_CD
        ,A.MNR_RLT_CD_DESC
        ,A.CRE_USR_ID
		,A.RPR_LBR_HRS
		,SUBSTR(A.FM_RLT_CD,0,LENGTH(A.FM_RLT_CD)-2) AS EQ_CMPO_CD
		,SUBSTR(A.FM_RLT_CD,LENGTH(A.FM_RLT_CD)-1,LENGTH(A.FM_RLT_CD)) AS EQ_RPR_CD
        ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
        ,A.UPD_USR_ID
        ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
FROM MNR_CD_RLT A
WHERE A.EQ_CEDEX_RLT_TP_CD  = 'CTV'
#if (${in_cost_grp_cd} != 'ALL')	
	  AND A.COST_GRP_CD = @[in_cost_grp_cd]	
#end
#if (${in_eq_cmpo_cd} != '')	
	  AND A.FM_RLT_CD LIKE @[in_eq_cmpo_cd] || '%'
#end			]]></sql>
			<params>
				<param name="in_cost_grp_cd" type="12" value="" out="N"/>
				<param name="in_eq_cmpo_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
