<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyPayableInvoiceFileListByWODataUSQL">
			<desc><![CDATA[GeneralCodeCheckMgtDBDAOmodifyPayableInvoiceFileListByWODataUSQL]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
SET INP_MSG4 = 'NW'
WHERE TMP_SEQ = @[tmp_seq] 
AND NOT EXISTS 
( 
	SELECT B.MNR_ORD_SEQ
	FROM MNR_ORD_HDR B
	WHERE SUBSTR(A.INP_MSG1,1,3) = B.MNR_ORD_OFC_CTY_CD  
		AND SUBSTR(A.INP_MSG1,4) = B.MNR_ORD_SEQ	
)
			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
