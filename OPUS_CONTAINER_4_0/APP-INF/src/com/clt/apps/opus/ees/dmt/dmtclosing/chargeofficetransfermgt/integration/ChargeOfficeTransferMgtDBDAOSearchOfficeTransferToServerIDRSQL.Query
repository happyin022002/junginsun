<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeOfficeTransferMgtDBDAOSearchOfficeTransferToServerIDRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT	'N' AS RHQ_CHK_FLG
FROM    MDM_ORGANIZATION
WHERE   OFC_CD = @[fm_ofc_cd]  -- FROM 
AND     AR_HD_QTR_OFC_CD = (SELECT  AR_HD_QTR_OFC_CD 
                            FROM    MDM_ORGANIZATION 
                            WHERE   OFC_CD = @[to_ofc_cd] ) -- TO			]]></sql>
			<params>
				<param name="fm_ofc_cd" type="12" value="" out="N"/>
				<param name="to_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
