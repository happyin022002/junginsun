<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOcheckVVDExistsRSQL">
			<desc><![CDATA[VL VD시 VVD번호가 VSK_VSL_SKD에 존재하는지 체크]]></desc>
			<sql><![CDATA[
SELECT 'Y' AS VVD_EXIST
  FROM VSK_VSL_SKD
 WHERE 1 = 1
   AND VSL_CD     = SUBSTR(@[p_vvd], 0,4)
   AND SKD_VOY_NO = SUBSTR(@[p_vvd], 5,4)
   AND SKD_DIR_CD = SUBSTR(@[p_vvd], 9,1)			]]></sql>
			<params>
				<param name="p_vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
