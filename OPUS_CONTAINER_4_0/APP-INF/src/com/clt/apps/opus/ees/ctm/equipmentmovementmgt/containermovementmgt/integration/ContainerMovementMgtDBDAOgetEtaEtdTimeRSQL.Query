<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOgetEtaEtdTimeRSQL">
			<desc><![CDATA[SELECT VPS_ETD_DT, VPS_ETA_DT
  FROM VSK_VSL_PORT_SKD
 WHERE 1 = 1
   AND VSL_CD = 'HNAT'
   AND SKD_VOY_NO = '0062'
   AND SKD_DIR_CD = 'W'
   AND VPS_PORT_CD = 'KRKAN']]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(VPS_ETD_DT, 'YYYY-MM-DD HH24:MI') VPS_ETD_DT, TO_CHAR(VPS_ETA_DT, 'YYYY-MM-DD HH24:MI') VPS_ETA_DT
  FROM VSK_VSL_PORT_SKD
 WHERE 1 = 1
   AND VSL_CD     = SUBSTR(@[p_vvd], 0,4)
   AND SKD_VOY_NO = SUBSTR(@[p_vvd], 5,4)
   AND SKD_DIR_CD = SUBSTR(@[p_vvd], 9,1)
#if (${p_pol} != '')
   AND VPS_PORT_CD = @[p_pol]
#end
#if (${p_pod} != '')
   AND VPS_PORT_CD = @[p_pod]
#end			]]></sql>
			<params>
				<param name="p_vvd" type="12" value="" out="N"/>
				<param name="p_pol" type="12" value="" out="N"/>
				<param name="p_pod" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
