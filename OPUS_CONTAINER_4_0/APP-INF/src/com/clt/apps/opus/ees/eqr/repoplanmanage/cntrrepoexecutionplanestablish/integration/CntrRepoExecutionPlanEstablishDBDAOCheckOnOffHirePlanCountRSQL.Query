<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOCheckOnOffHirePlanCountRSQL">
			<desc><![CDATA[EQR_ONF_HIR_PLN 테이블에서 특정 repo plan id, ecc cd 의 count가 몇개인지 확인]]></desc>
			<sql><![CDATA[
SELECT 
    COUNT(*) COUN					
FROM 
    EQR_ONF_HIR_PLN					
WHERE 
    REPO_PLN_ID         = @[repo_pln_id] 				
    AND   PLN_YRWK   	= @[pln_yrwk]					
    AND   ONF_HIR_DIV_CD= @[onf_hir_div_cd]					
    AND   ECC_CD   		= @[ecc_cd]
	AND   CNTR_LSTM_CD  = @[lease_term]			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="onf_hir_div_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="lease_term" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
