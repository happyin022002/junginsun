<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchOfficByCountryRSQL">
			<desc><![CDATA[SearchOfficByCountry]]></desc>
			<sql><![CDATA[
SELECT OFC_N2ND_LVL_CD AS SVR_ID FROM DMT_OFC_LVL_V
WHERE OFC_N8TH_LVL_CD = (
    SELECT DMDT_OFC_CD FROM MDM_YARD
    WHERE YD_CD LIKE @[cnt_cd] || '%'
    AND ROWNUM = 1
    )			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
