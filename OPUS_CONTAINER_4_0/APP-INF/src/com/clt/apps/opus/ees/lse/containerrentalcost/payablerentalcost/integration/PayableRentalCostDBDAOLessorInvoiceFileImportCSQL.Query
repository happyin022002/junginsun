<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOLessorInvoiceFileImportCSQL">
			<desc><![CDATA[Payable Rental - Lessor Invoice File Import Save]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_PAY_RNTL_CHG_CO (
       PAY_IMP_SEQ
     , CNTR_NO
     , LSE_PAY_CHG_TP_CD
     , DTL_SEQ
     , AGMT_CTY_CD
     , AGMT_SEQ
     , LSE_CTRT_NO
     , CO_COST_YRMON
     , INV_NO
     , CNTR_TPSZ_CD
     , ONH_DT
     , OFFH_DT
     , ONH_LOC_CD
     , OFFH_LOC_CD
     , CHG_FREE_DYS
     , PD_RT_AMT
     , TTL_COST_AMT
     , COST_DYS
     , BIL_DYS
     , LSE_PAY_CHG_TP_NM
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
)
VALUES
(      @[pay_imp_seq]
     , @[cntr_no]
     , @[lse_pay_chg_tp_cd]
     , @[dtl_seq]
     , @[agmt_cty_cd]
     , @[agmt_seq]
     , @[lse_ctrt_no]
     , @[co_cost_yrmon]
     , @[inv_no]
     , @[cntr_tpsz_cd]
     , TO_DATE(@[onh_dt],  'DDMMYYYY')
     , TO_DATE(@[offh_dt], 'DDMMYYYY')
     , @[onh_loc_cd]
     , @[offh_loc_cd]
     , @[chg_free_dys]
     , @[pd_rt_amt]
     , @[ttl_cost_amt]
     , @[cost_dys]
     , @[bil_dys]
     , @[lse_pay_chg_tp_nm]
     , @[cre_usr_id]
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
)			]]></sql>
			<params>
				<param name="pay_imp_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="lse_pay_chg_tp_cd" type="12" value="" out="N"/>
				<param name="dtl_seq" type="2" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="lse_ctrt_no" type="12" value="" out="N"/>
				<param name="co_cost_yrmon" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="onh_dt" type="12" value="" out="N"/>
				<param name="offh_dt" type="12" value="" out="N"/>
				<param name="onh_loc_cd" type="12" value="" out="N"/>
				<param name="offh_loc_cd" type="12" value="" out="N"/>
				<param name="chg_free_dys" type="2" value="" out="N"/>
				<param name="pd_rt_amt" type="6" value="" out="N"/>
				<param name="ttl_cost_amt" type="6" value="" out="N"/>
				<param name="cost_dys" type="2" value="" out="N"/>
				<param name="bil_dys" type="2" value="" out="N"/>
				<param name="lse_pay_chg_tp_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
