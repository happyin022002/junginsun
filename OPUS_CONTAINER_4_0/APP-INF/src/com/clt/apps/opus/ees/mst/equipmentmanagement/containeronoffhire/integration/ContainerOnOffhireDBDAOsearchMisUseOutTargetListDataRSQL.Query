<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOsearchMisUseOutTargetListDataRSQL">
			<desc><![CDATA[searchMisUseOutTargetListData]]></desc>
			<sql><![CDATA[
SELECT                                                                                                
	AA.EEFLG,                                                                                             
	AA.CNTR_NO,                                                                                           
	'' OFFH_STS_CD,                                                                                       
	'' OFFH_DUE_DT,                                                                                       
	AA.CNTR_TPSZ_CD,                                                                                      
	AA.LSTM_CD,                                                                                           
	AA.AGMT_CTY_CD,                                                                                       
	AA.AGMT_NO,                                                                                           
	AA.REF_NO,                                                                                            
	AA.VNDR_SEQ,                                                                                          
	AA.VNDR_LGL_ENG_NM,                                                                                   
	AA.CNTR_STS_CD,                                                                                       
	AA.CNTR_STS_EVNT_DT,                                                                                  
	AA.FULL_FLG,                                                                                          
	AA.CNMV_STS_CD,                                                                                       
	AA.CRNT_YD_CD,                                                                                        
	AA.CNMV_DT,                                                                                           
	AA.CNTR_DRFF_AMT,                                                                                     
	'' CNTR_DRFF_CR_AMT,                                                                                  
	AA.CNTR_LFT_CHG_AMT,                                                                                  
	AA.CNTR_LFT_CHG_CUR,                                                                                  
	'' RNTL_CHG_FREE_DYS,                                                                                 
	'' CNTR_OLD_VAN_FLG,                                                                                  
	'' CNTR_PKUP_CHG_AMT,                                                                                 
	'' CNTR_PKUP_CR_CHG_AMT,                                                                              
	'' CNTR_RMK,
	AA.EFF_DT,
    AA.EXP_DT                                                                                               
FROM                                                                                                  
(                                                                                                     
	SELECT                                                                                                
		'' EEFLG,  
		B.CNTR_NO,                                                                                            
		B.CNTR_TPSZ_CD,                                                                                       
		B.LSTM_CD,                                                                                            
		B.AGMT_CTY_CD,                                                                                        
		MST_COMMON_PKG.MST_AGMT_NO_CONV_FNC(B.AGMT_CTY_CD,B.AGMT_SEQ)  AS AGMT_NO,                            
		C.REF_NO,                                                                                             
		B.VNDR_SEQ,                                                                                           
		D.VNDR_LGL_ENG_NM,                                                                                    
		B.CNTR_STS_CD,                                                                                        
		(SELECT /*+ INDEX_DESC(H XAK1MST_CNTR_STS_HIS) */ TO_CHAR(H.CNTR_STS_EVNT_DT,'YYYY-MM-DD')             
		FROM MST_CNTR_STS_HIS H                                                                               
		WHERE H.CNTR_NO = B.CNTR_NO                                                                           
        AND   H.CNTR_STS_SEQ = B.LST_STS_SEQ                                                            
		AND   ROWNUM = 1                                                                                      
		) CNTR_STS_EVNT_DT,                                                                                   
		DECODE(B.FULL_FLG, 'Y', 'F', 'M') FULL_FLG,                                                           
		B.CNMV_STS_CD,                                                                                        
		B.CRNT_YD_CD,                                                                                         
		TO_CHAR(B.CNMV_DT, 'YYYY-MM-DD') CNMV_DT,                                                             
		(SELECT N1ST_CHG_AMT                                                                                  
		FROM LSE_AGMT_RT                                                                                      
		WHERE 1 = 1                                                                                           
		AND   (AGMT_CTY_CD, AGMT_SEQ) =                                                                       
		(SELECT /*+ INDEX_DESC(H XAK1MST_CNTR_STS_HIS) */ H.AGMT_CTY_CD, H.AGMT_SEQ                            
		FROM MST_CNTR_STS_HIS H                                                                               
		WHERE H.CNTR_NO = B.CNTR_NO 
        AND   H.CNTR_STS_CD = 'MUO'                                                                             
		AND   ROWNUM = 1)                                                                                     
		AND   CNTR_RNTL_CHG_TP_CD ='DOCV'                                                                     
		AND   LOC_CD       = (SELECT  SCC_CD                                                                  
		FROM MDM_LOCATION                                                                                     
		WHERE LOC_CD = SUBSTR(@[sts_evnt_yd_cd], 1, 5))                                                       
		AND   CNTR_TPSZ_CD = B.CNTR_TPSZ_CD                                                                   
		) CNTR_DRFF_AMT,                                                                                      
		(SELECT N2ND_CHG_AMT                                                                                  
		FROM LSE_AGMT_RT                                                                                      
		WHERE 1 = 1                                                                                           
		AND   (AGMT_CTY_CD, AGMT_SEQ) =                                                                       
		(SELECT /*+ INDEX_DESC(H XAK1MST_CNTR_STS_HIS) */ H.AGMT_CTY_CD, H.AGMT_SEQ                            
		FROM MST_CNTR_STS_HIS H                                                                               
		WHERE H.CNTR_NO = B.CNTR_NO 
        AND   H.CNTR_STS_CD = 'MUO'                                                                            
		AND   ROWNUM = 1)                                                                                     
		AND   CNTR_RNTL_CHG_TP_CD ='LIFV'                                                                     
		AND   LOC_CD       = (SELECT  SCC_CD                                                                  
		FROM MDM_LOCATION                                                                                     
		WHERE LOC_CD = SUBSTR(@[sts_evnt_yd_cd], 1, 5))                                                       
		AND   CNTR_TPSZ_CD = B.CNTR_TPSZ_CD                                                                   
		) CNTR_LFT_CHG_AMT,                                                                                   
		(SELECT CURR_CD                                                                                       
		FROM LSE_AGREEMENT                                                                                    
		WHERE AGMT_CTY_CD = B.AGMT_CTY_CD                                                                     
		AND   AGMT_SEQ    = B.AGMT_SEQ                                                                        
		) CNTR_LFT_CHG_CUR,              
        TO_CHAR(C.LST_EFF_DT,'YYYY-MM-DD') AS EFF_DT ,
        TO_CHAR(C.LST_EXP_DT,'YYYY-MM-DD') AS EXP_DT                                                        
	FROM                                                                                                  
		MST_CONTAINER  B,                                                                                     
		LSE_AGREEMENT C,                                                                                      
		MDM_VENDOR D                                                                                          
	WHERE 1 = 1                                                                                           
		--AND  B.CNTR_STS_CD = 'MUO'   
		AND  B.CNTR_NO        = @[hid_cntr_no]                                                                         
		AND  C.AGMT_CTY_CD    = B.AGMT_CTY_CD                                                                 
		AND  C.AGMT_SEQ       = B.AGMT_SEQ                                                                    
		AND  D.VNDR_SEQ       = B.VNDR_SEQ                                                                    
) AA			]]></sql>
			<params>
				<param name="sts_evnt_yd_cd" type="12" value="" out="N"/>
				<param name="hid_cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
