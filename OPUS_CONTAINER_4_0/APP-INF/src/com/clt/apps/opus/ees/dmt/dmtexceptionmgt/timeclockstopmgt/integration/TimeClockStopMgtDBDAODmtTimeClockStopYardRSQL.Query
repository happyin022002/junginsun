<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TimeClockStopMgtDBDAODmtTimeClockStopYardRSQL">
			<desc><![CDATA[Time Clock Stop Yard select]]></desc>
			<sql><![CDATA[
SELECT A.YD_CD, B.YD_NM
FROM DMT_TM_CLK_STOP_YD A, MDM_YARD B
WHERE A.CLK_STOP_NO = @[clk_stop_no]
AND A.YD_CD = B.YD_CD			]]></sql>
			<params>
				<param name="clk_stop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
