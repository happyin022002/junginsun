<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MgsetMovementHistoryDBDAOcheckMGSBareMvmtByopocDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(B XPKCGM_MGST_MVMT_HIS ) */
  A.MGST_NO MGST_NO 
, TO_CHAR(A.MVMT_DT,'YYYYMMDDHH24MISS') MVMT_DT_1
, TO_CHAR(B.MVMT_DT,'YYYYMMDDHH24MISS') MVMT_DT_2
 FROM (
	SELECT /*+ INDEX_DESC(A XAK1CGM_MGST_MVMT_HIS) */ 
     	  A.MGST_NO
    	 ,A.GATE_IO_CD
   		 ,A.MVMT_DT      
	FROM   CGM_MGST_MVMT_HIS A, MDM_LOCATION B , MDM_EQ_ORZ_CHT C  
	WHERE  A.CNTR_NO  = @[cntr_no]
	AND    B.LOC_CD   = SUBSTR(A.YD_CD, 1, 5) 
	AND    C.SCC_CD = B.SCC_CD
	AND    C.LCC_CD = ( SELECT  B.LCC_CD FROM MDM_LOCATION A , MDM_EQ_ORZ_CHT B  
                     WHERE A.SCC_CD = B.SCC_CD
                     AND A.LOC_CD = substr(@[org_yd_cd],0,5)
                     AND ROWNUM=1 )
	AND    A.MGST_NO <> @[mgst_no]
	AND    A.MVMT_STS_CD= 'MT'
	AND    ROWNUM      = 1
	) A, CGM_MGST_MVMT_HIS B, CGM_EQUIPMENT C
WHERE B.MGST_NO =A.MGST_NO
  AND B.MGST_NO = C.EQ_NO
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="org_yd_cd" type="12" value="" out="N"/>
				<param name="mgst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
