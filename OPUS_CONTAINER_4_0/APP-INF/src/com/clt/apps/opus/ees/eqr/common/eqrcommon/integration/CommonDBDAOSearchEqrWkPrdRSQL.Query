<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchEqrWkPrdRSQL">
			<desc><![CDATA[EES_EQR_0008 조회 쿼리]]></desc>
			<sql><![CDATA[
SELECT PLN_YR
      ,PLN_WK
      ,PLN_MON
      ,WK_ST_DT
      ,WK_END_DT
  FROM EQR_WK_PRD
 WHERE 1=1
#if (${pln_yr} != '') 
AND PLN_YR = @[pln_yr] -- 변수처리, 4번 항목 조건 값 필수
#end
 ORDER BY PLN_YR, PLN_WK			]]></sql>
			<params>
				<param name="pln_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
