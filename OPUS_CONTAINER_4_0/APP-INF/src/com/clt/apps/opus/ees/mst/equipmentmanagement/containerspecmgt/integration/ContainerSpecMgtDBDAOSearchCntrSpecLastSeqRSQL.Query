<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOSearchCntrSpecLastSeqRSQL">
			<desc><![CDATA[SearchCntrSpecLastSeq]]></desc>
			<sql><![CDATA[
SELECT @[spec_yr] || '-' || @[cntr_tpsz_cd] || '-' ||
       DECODE(SUBSTR(MAX(A.CNTR_SPEC_NO),1,7)
       ,@[spec_yr] || '-' || @[cntr_tpsz_cd]
       ,LPAD(SUBSTR(MAX(A.CNTR_SPEC_NO),9,3)+1,3,'0') 
       ,'001') as CNTR_SPEC_NO
FROM MST_CNTR_SPEC A
WHERE A.SPEC_YR = @[spec_yr]
AND   A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="spec_yr" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
