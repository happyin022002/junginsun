<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrForecastPrecisionManageDBDAOSearchMtyRailArrivalRSQL">
			<desc><![CDATA[SCE_RAIL_TZ_RPT 데이터 조회

2011.06.13 나상보 [CHM-201111518-01] [EQR]  MTY Rail Trans. 화면의 CNTR List 조회 기능 추가]]></desc>
			<sql><![CDATA[
SELECT	 NVL(DEST1,'G.Total') AS DEST1 
		,CASE WHEN COLOR=2 THEN DEST1 ELSE NVL(DEST2,'G.Total') END DEST2 
		,DECODE(COLOR,1,'Total', 1.5, DECODE(DEST2, '','G.Total',NVL(TPSZ,'Total')),NVL(TPSZ,'Total')) TPSZ 
	#foreach ( $key in ${arrday} ) 
		,DAY_${key} 
		,DAY_${key}_MT
	#end 
		,COLOR 
		,GUBUN 
FROM	( 
		#if ( ${gubun} == 'A' ) 
		SELECT  GROUPING(C.LOC_CD) + GROUPING(A.TO_NOD_CD) + GROUPING(A.EQ_TPSZ_CD) COLOR
				,C.LOC_CD DEST1 
				,A.TO_NOD_CD DEST2 
		#elseif ( ${gubun} == 'B' ) 
		SELECT  GROUPING(A.FM_NOD_CD) + GROUPING(A.TO_NOD_CD) + GROUPING(A.EQ_TPSZ_CD) COLOR
				,A.TO_NOD_CD DEST1
				,A.FM_NOD_CD DEST2
		#end 
				,A.EQ_TPSZ_CD AS TPSZ 
			#if ( ${gubun} == 'A' ) 
				,NVL(C.LOC_CD,'ZZZZZ') || NVL(A.TO_NOD_CD, 'ZZZZZZZ') || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#elseif ( ${gubun} == 'B' ) 
				,NVL(A.TO_NOD_CD,'ZZZZZ') || NVL(A.FM_NOD_CD, 'ZZZZZZZ') || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#end 
			#foreach ( $key IN ${arrday} ) 
				,COUNT(DECODE(TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd'),TO_CHAR(TO_DATE(@[fm_dt],'yyyy-MM-dd')+$key,'yyyyMMdd'),1)) AS DAY_${key} 
				,NVL(SUM(DECODE(TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd'),TO_CHAR(TO_DATE(@[fm_dt],'yyyy-MM-dd')+$key,'yyyyMMdd'),
		        ( SELECT DECODE(COUNT(*),0,0,1)
        			FROM SCE_CLM CLM, CTM_MOVEMENT CM
			        WHERE A.TRSP_SO_OFC_CTY_CD = CLM.TRSP_SO_OFC_CTY_CD
			          AND A.TRSP_SO_SEQ = CLM.TRSP_SO_SEQ
			          AND CM.CNTR_NO = CLM.CNTR_NO
			          AND CM.CNMV_YR = CLM.CNMV_YR
			          AND CM.CNMV_ID_NO = CLM.CNMV_ID_NO  
			          AND CM.MVMT_STS_CD = 'MT'
					  AND CM.ORG_YD_CD = A.TO_NOD_CD
			        GROUP BY CM.CNTR_NO ))),0) AS DAY_${key}_MT
			#end 
		FROM	SCE_RAIL_TZ_RPT A 
				,MDM_YARD B 
				,MDM_YARD C
	#if ( ${rail_company} != '' ) 
				,TRS_TRSP_RAIL_BIL_VNDR_SET D
    #end  
		WHERE	A.FM_NOD_CD	= B.YD_CD 
			AND	A.TO_NOD_CD	= C.YD_CD 
			AND	A.RAIL_DEST_N2ND_ETA_DT BETWEEN  TO_DATE('${fm_dt} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
										AND  TO_DATE('${to_dt} 23:59:59', 'YYYY-MM-DD HH24:MI:SS') 
			AND	A.CGO_TP_CD	= 'M'			-- MTY 
			AND	A.DELT_FLG	= 'N'			-- S/O CANCEL ??????????.
		#if ( ${org_loc} != '' ) 
			AND B.LOC_CD = @[org_loc] 
		#end 
		#if ( ${dest_loc} != '' ) 
			AND C.LOC_CD = @[dest_loc] 
		#end 
		#if ( ${tpszText} != '' )
			AND	A.EQ_TPSZ_CD IN ( ${tpszText} )
		#end
		#if ( ${rail_company} != '' ) 
         	AND A.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD
			AND A.TRSP_SO_SEQ = D.TRSP_SO_SEQ
			AND D.SUB_RAIL_SEQ	= '1'
			AND D.VNDR_SEQ		= @[rail_company]
		#end 
		#if ( ${gubun} == 'A' ) 
		HAVING GROUPING(C.LOC_CD) + GROUPING(A.TO_NOD_CD) + GROUPING(a.eq_tpsz_cd) < 2 
		GROUP BY ROLLUP(C.LOC_CD,A.TO_NOD_CD,A.EQ_TPSZ_CD) 
		#elseif ( ${gubun} == 'B' ) 
		HAVING GROUPING(A.FM_NOD_CD) + GROUPING(A.TO_NOD_CD) + GROUPING(A.EQ_TPSZ_CD) < 2 
		GROUP BY ROLLUP(A.FM_NOD_CD,A.TO_NOD_CD,A.EQ_TPSZ_CD) 
		#end 
		UNION ALL 
		SELECT	2 COLOR
				,'Total' 
				,'Total' 
				,A.EQ_TPSZ_CD 
				,'ZZZZZ'||	'ZZZZZZZ' ||A.EQ_TPSZ_CD GUBUN 
			#foreach ( $key in ${arrday} ) 
				,COUNT(DECODE(TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd'),TO_CHAR(TO_DATE(@[fm_dt],'yyyy-MM-dd')+$key,'yyyyMMdd'),1)) AS DAY_${key} 
				,NVL(SUM(DECODE(TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd'),TO_CHAR(TO_DATE(@[fm_dt],'yyyy-MM-dd')+$key,'yyyyMMdd'),
		        ( SELECT DECODE(COUNT(*),0,0,1)
        			FROM SCE_CLM CLM, CTM_MOVEMENT CM
			        WHERE A.TRSP_SO_OFC_CTY_CD = CLM.TRSP_SO_OFC_CTY_CD
			          AND A.TRSP_SO_SEQ = CLM.TRSP_SO_SEQ
			          AND CM.CNTR_NO = CLM.CNTR_NO
			          AND CM.CNMV_YR = CLM.CNMV_YR
			          AND CM.CNMV_ID_NO = CLM.CNMV_ID_NO  
			          AND CM.MVMT_STS_CD = 'MT'
					  AND CM.ORG_YD_CD = A.TO_NOD_CD
			        GROUP BY CM.CNTR_NO ))),0) AS DAY_${key}_MT
			#end 
		FROM	SCE_RAIL_TZ_RPT A 
				,MDM_YARD B 
				,MDM_YARD C 
	#if ( ${rail_company} != '' ) 
				,TRS_TRSP_RAIL_BIL_VNDR_SET D
    #end  
		WHERE	A.FM_NOD_CD	= B.YD_CD 
			AND	A.TO_NOD_CD	= C.YD_CD 
			AND	A.RAIL_DEST_N2ND_ETA_DT BETWEEN  TO_DATE('${fm_dt} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
										AND  TO_DATE('${to_dt} 23:59:59', 'YYYY-MM-DD HH24:MI:SS') 
			AND	A.CGO_TP_CD	= 'M'				-- MTY 
			AND	A.DELT_FLG	= 'N'			-- S/O CANCEL ??????????.
		#if ( ${org_loc} != '' ) 
			AND B.LOC_CD = @[org_loc] 
		#end 
		#if ( ${dest_loc} != '' ) 
			AND C.LOC_CD = @[dest_loc] 
		#end 
		#if ( ${tpszText} != '' )
			AND	A.EQ_TPSZ_CD IN ( ${tpszText})
		#end
		#if ( ${rail_company} != '' ) 
	  	    AND A.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD
			AND A.TRSP_SO_SEQ = D.TRSP_SO_SEQ
			AND D.SUB_RAIL_SEQ	= '1'
			AND D.VNDR_SEQ		= @[rail_company]
		#end 
		GROUP BY  A.EQ_TPSZ_CD 
		UNION ALL 
		SELECT	1.5 COLOR
			#if ( ${gubun} == 'A' ) 
				,C.LOC_CD		DEST1 
				,DECODE(C.LOC_CD,NULL,'','Total') DEST2 
			#elseif ( ${gubun} == 'B' ) 
				,A.TO_NOD_CD	DEST1 
				,DECODE(A.TO_NOD_CD,NULL,'','Total') DEST2 
			#end 
				,A.EQ_TPSZ_CD AS TPSZ 
			#if ( ${gubun} == 'A' ) 
				,NVL(C.LOC_CD,'ZZZZZ') || 'ZZZZZZZ' || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#elseif ( ${gubun} == 'B' ) 
				,NVL(A.TO_NOD_CD,'ZZZZZ') || 'ZZZZZZZ' || NVL(A.EQ_TPSZ_CD,'ZZ') AS GUBUN 
			#end 
			#foreach ( $key in ${arrday} ) 
				,COUNT(DECODE(TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd'),TO_CHAR(TO_DATE(@[fm_dt],'yyyy-MM-dd')+$key,'yyyyMMdd'),1)) AS DAY_${key} 
				,NVL(SUM(DECODE(TO_CHAR(A.RAIL_DEST_N2ND_ETA_DT,'yyyyMMdd'),TO_CHAR(TO_DATE(@[fm_dt],'yyyy-MM-dd')+$key,'yyyyMMdd'),
		        ( SELECT DECODE(COUNT(*),0,0,1)
        			FROM SCE_CLM CLM, CTM_MOVEMENT CM
			        WHERE A.TRSP_SO_OFC_CTY_CD = CLM.TRSP_SO_OFC_CTY_CD
			          AND A.TRSP_SO_SEQ = CLM.TRSP_SO_SEQ
			          AND CM.CNTR_NO = CLM.CNTR_NO
			          AND CM.CNMV_YR = CLM.CNMV_YR
			          AND CM.CNMV_ID_NO = CLM.CNMV_ID_NO  
			          AND CM.MVMT_STS_CD = 'MT'
					  AND CM.ORG_YD_CD = A.TO_NOD_CD
			        GROUP BY CM.CNTR_NO ))),0) AS DAY_${key}_MT
			#end 
		FROM	SCE_RAIL_TZ_RPT A 
				,MDM_YARD B 
				,MDM_YARD C
	#if ( ${rail_company} != '' ) 
				,TRS_TRSP_RAIL_BIL_VNDR_SET D
    #end  
		WHERE	A.FM_NOD_CD	= B.YD_CD 
			AND	A.TO_NOD_CD	= C.YD_CD 
			AND	A.RAIL_DEST_N2ND_ETA_DT BETWEEN  TO_DATE('${fm_dt} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
										AND  TO_DATE('${to_dt} 23:59:59', 'YYYY-MM-DD HH24:MI:SS') 
			AND	A.CGO_TP_CD	= 'M'				-- MTY 
			AND	A.DELT_FLG	= 'N'			-- S/O CANCEL ??????????.
		#if ( ${org_loc} != '' ) 
			AND B.LOC_CD = @[org_loc] 
		#end 
		#if ( ${dest_loc} != '' ) 
			AND C.LOC_CD = @[dest_loc] 
		#end 
		#if ( ${tpszText} != '' )
			AND	A.EQ_TPSZ_CD IN ( ${tpszText})
		#end
		#if ( ${rail_company} != '' ) 
	      	AND A.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD
			AND A.TRSP_SO_SEQ = D.TRSP_SO_SEQ
			AND D.SUB_RAIL_SEQ	= '1'
			AND D.VNDR_SEQ		= @[rail_company]
		#end 
	#if ( ${gubun} == 'A' )
		GROUP BY  ROLLUP(C.LOC_CD,A.EQ_TPSZ_CD) 
	#elseif ( ${gubun} == 'B' )
		GROUP BY  ROLLUP(A.TO_NOD_CD,A.EQ_TPSZ_CD) 
	#end
		) 
ORDER BY GUBUN,COLOR			]]></sql>
			<params>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="org_loc" type="12" value="" out="N"/>
				<param name="dest_loc" type="12" value="" out="N"/>
				<param name="rail_company" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
