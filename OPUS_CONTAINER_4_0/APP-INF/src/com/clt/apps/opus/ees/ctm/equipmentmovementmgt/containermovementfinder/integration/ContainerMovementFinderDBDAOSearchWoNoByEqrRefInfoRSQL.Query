<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOSearchWoNoByEqrRefInfoRSQL">
			<desc><![CDATA[SearchWoNoByEqrRefInfo]]></desc>
			<sql><![CDATA[
SELECT DISTINCT (T.TRSP_WO_OFC_CTY_CD||T.TRSP_WO_SEQ) AS WO_NO
FROM EQR_PLAN_V V,
    TRS_TRSP_SVC_ORD T
WHERE V.REF_ID = T.REF_ID(+)
AND V.TP_CD IN ('T.VVD EXE','TRW EXE','ONH EXE','OFH EXE','LCC')
AND T.TRSP_WO_OFC_CTY_CD||T.TRSP_WO_SEQ IS NOT NULL
AND V.REF_ID = @[mty_pln_no]			]]></sql>
			<params>
				<param name="mty_pln_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
