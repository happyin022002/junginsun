<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CIMCommonDBDAOModifyBookingCycDataUSQL">
			<desc><![CDATA[CYC Change Modify]]></desc>
			<sql><![CDATA[
#if (${app_cd} == 'OSCAR')
	UPDATE CTM_BKG_CNTR SET 
		#if (${cnmv_cyc_no_chg} == 'F')
			CNMV_CYC_NO = 0
		#elseif (${cnmv_cyc_no_chg} == 'T')
			CNMV_CYC_NO = 9999
		#elseif (${cnmv_cyc_no_chg} == 'H')
			CNMV_CYC_NO = 9998
		#end
	WHERE 
		BKG_NO = @[bkg_no]
	AND CNTR_NO  = @[cntr_no]

#elseif (${app_cd} == 'OPUS')

	UPDATE BKG_CONTAINER SET 
		#if (${cnmv_cyc_no_chg} == 'F')
			CNMV_CYC_NO = 0
		#elseif (${cnmv_cyc_no_chg} == 'T')
			CNMV_CYC_NO = 9999
		#elseif (${cnmv_cyc_no_chg} == 'H')
			CNMV_CYC_NO = 9998
		#end 
	WHERE 
		BKG_NO = @[bkg_no]
	AND CNTR_NO  = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
