<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RateMgtDBDAOmodifyAGMTLastVersionUnFlagDataRSQL">
			<desc><![CDATA[라스트 버젼 플레그를 다 'N'으로 바꾼다.]]></desc>
			<sql><![CDATA[
UPDATE MNR_AGMT_HDR A
     SET
         A.AGMT_LST_VER_FLG = 'N'
        ,A.UPD_USR_ID = @[upd_usr_id]
        ,A.UPD_DT = sysdate
WHERE A.AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
AND   A.AGMT_SEQ = @[agmt_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
