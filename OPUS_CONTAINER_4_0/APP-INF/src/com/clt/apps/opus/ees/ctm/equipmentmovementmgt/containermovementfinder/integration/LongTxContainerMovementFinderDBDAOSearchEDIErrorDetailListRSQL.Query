<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongTxContainerMovementFinderDBDAOSearchEDIErrorDetailListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT E.CNTR_NO
       ,C.CNTR_TPSZ_CD
       ,E.EDI_MVMT_STS_CD
       ,E.EDI_GATE_IO_CD
       ,E.EVNT_YD_CD
       ,TO_CHAR (E.EVNT_DT, 'YYYY-MM-DD HH24:MI') AS EVNT_DT
       ,TO_CHAR (E.CRE_LOCL_DT, 'YYYY-MM-DD HH24:MI') AS CRE_LOCL_DT
#if (${divide} == 'corr_err' || ${divide} == 'corr_ignr' || ${divide} == 'corr_ok' || ${divide} == 'corr_ttl')
       ,DECODE (E.MVMT_EDI_RMK, NULL, 'There is no error MSG history', E.MVMT_EDI_RMK) AS MVMT_EDI_RMK
#else
       ,DECODE (R.EDI_RMK, NULL, 'There is no error MSG history', R.EDI_RMK) AS MVMT_EDI_RMK
#end
  FROM CTM_MVMT_EDI_MSG E
       ,CTM_EDI_RSLT_RMK R
       ,MST_CONTAINER C
       ,MDM_LOCATION ML
       ,MDM_EQ_ORZ_CHT ME
 WHERE 1=1
#if (${slvd_cnt_dt} == 'S' && (${divide} == 'slvd_err' || ${divide} == 'slvd_ignr' || ${divide} == 'slvd_ttl')) 
   AND TO_CHAR(E.UPD_LOCL_DT, 'YYYYMMDD') BETWEEN REPLACE (@[p_date1], '-', '') AND REPLACE (@[p_date2], '-', '')
#else
   AND E.IDX_CRE_LOCL_DT BETWEEN REPLACE (@[p_date1], '-', '') AND REPLACE (@[p_date2], '-', '')
#end
#if (${rcc} == 'Total')
 #if (${rcc_cd} == '' || ${rcc_cd} == 'ALL')
   AND E.MVMT_EDI_MSG_AREA_CD IN ('USA', 'SWA', 'KOR', 'EUR', 'CHN')
 #else
   AND E.MVMT_EDI_MSG_AREA_CD = (SELECT SYS_AREA_GRP_ID AS SVR_ID
                                   FROM COM_SYS_AREA_GRP_ID
                                  WHERE CNT_CD = SUBSTR (@[rcc_cd], 0, 2))
   AND ME.RCC_CD = @[rcc_cd]
  #if (${lcc_cd} != '')
   AND ME.LCC_CD = @[lcc_cd]
  #end
  #if (${p_yard2} != '')
   AND E.EVNT_YD_CD = @[p_yard1]||@[p_yard2]
  #elseif (${p_yard1} != '')
   AND E.EVNT_YD_CD LIKE @[p_yard1]||'%'
  #end
 #end
#elseif (${rcc} == '')
   AND ME.RCC_CD IS NULL
   #if (${data_radio} == 'CN')
    #if (${cn} == '')
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) IS NULL
	#else
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) = @[cn]
	#end
   #elseif (${data_radio} == 'LCC')
	#if (${cn} == '')
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) IS NULL
	#else
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) = @[cn]
	#end
   AND ME.LCC_CD IS NULL
   #elseif (${data_radio} == 'LOC')
    #if (${cn} == '')
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) IS NULL
	#else
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) = @[cn]
	#end
   AND ME.LCC_CD IS NULL
    #if (${loc} == '')
   AND SUBSTR (E.EVNT_YD_CD, 0, 5) IS NULL
	#else
   AND SUBSTR (E.EVNT_YD_CD, 0, 5) = @[loc]
	#end
   #elseif (${data_radio} == 'Yard')
    #if (${cn} == '')
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) IS NULL
	#else
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) = @[cn]
	#end
   AND ME.LCC_CD IS NULL
    #if (${yard} == '')
   AND E.EVNT_YD_CD IS NULL
	#else
   AND E.EVNT_YD_CD = @[yard]
	#end
   #end
#else
   AND E.MVMT_EDI_MSG_AREA_CD = (SELECT SYS_AREA_GRP_ID AS SVR_ID
                                   FROM COM_SYS_AREA_GRP_ID
                                  WHERE CNT_CD = SUBSTR (@[rcc], 0, 2))
   #if (${data_radio} == 'RCC')
   AND ME.RCC_CD = @[rcc]
   #elseif (${data_radio} == 'CN')
   AND SUBSTR (E.EVNT_YD_CD, 0, 2) = @[cn]
   #elseif (${data_radio} == 'LCC')
   AND ME.LCC_CD = @[lcc]
   #elseif (${data_radio} == 'LOC')
   AND SUBSTR (E.EVNT_YD_CD, 0, 5) = @[loc]
   #elseif (${data_radio} == 'Yard')
   AND E.EVNT_YD_CD = @[yard]
   #end
#end
   AND E.CNTR_NO = C.CNTR_NO(+)
   AND E.MVMT_EDI_TP_CD = R.MVMT_EDI_TP_CD(+)
   AND E.MVMT_EDI_MSG_TP_ID = R.MVMT_EDI_MSG_TP_ID(+)
   AND E.MVMT_EDI_MSG_AREA_CD = R.MVMT_EDI_MSG_AREA_CD(+)
   AND E.MVMT_EDI_MSG_YRMONDY = R.MVMT_EDI_MSG_YRMONDY(+)
   AND E.MVMT_EDI_MSG_SEQ = R.MVMT_EDI_MSG_SEQ(+)
   AND R.RTY_KNT(+) = 0
   AND ME.SCC_CD(+) = ML.SCC_CD
   AND ML.LOC_CD(+) = SUBSTR (E.EVNT_YD_CD, 0, 5)
#if (${divide} == 'corr_err')
   AND E.MVMT_EDI_RSLT_CD = 'N'
#elseif (${divide} == 'corr_ignr')
   AND E.MVMT_EDI_RSLT_CD IN ('X','I')
#elseif (${divide} == 'corr_ok')
   AND E.MVMT_EDI_RSLT_CD = 'Y'
#elseif (${divide} == 'corr_ttl')
   AND E.MVMT_EDI_RSLT_CD != 'D'
#elseif (${divide} == 'slvd_err')
   AND E.MVMT_EDI_RSLT_CD IN ('D', 'Y')
   AND R.MVMT_EDI_RSLT_CD = 'N'
#elseif (${divide} == 'slvd_ignr')
   AND E.MVMT_EDI_RSLT_CD IN ('D', 'Y')
   AND R.MVMT_EDI_RSLT_CD IN ('X', 'I')
#elseif (${divide} == 'slvd_ttl')
   AND E.MVMT_EDI_RSLT_CD IN ('D', 'Y') AND E.RTY_KNT > 0
#elseif (${divide} == 'init_err')
   AND R.MVMT_EDI_RSLT_CD = ('N')
#elseif (${divide} == 'init_ok')
   AND E.RTY_KNT = 0 AND E.MVMT_EDI_RSLT_CD = 'Y'
#elseif (${divide} == 'init_ignr')
   AND R.MVMT_EDI_RSLT_CD IN ('X', 'I')
#end			]]></sql>
			<params>
				<param name="p_date1" type="12" value="" out="N"/>
				<param name="p_date2" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="p_yard1" type="12" value="" out="N"/>
				<param name="p_yard2" type="12" value="" out="N"/>
				<param name="cn" type="12" value="" out="N"/>
				<param name="loc" type="12" value="" out="N"/>
				<param name="yard" type="12" value="" out="N"/>
				<param name="rcc" type="12" value="" out="N"/>
				<param name="lcc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
