<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffMgtDBDAOaddRepairTariffDetailDataCSQL">
			<desc><![CDATA[입력]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_RPR_TRF_DTL (
	TRF_NO
,	TRF_DTL_SEQ
,	COST_GRP_CD
,	EQ_CMPO_CD
,	EQ_RPR_CD
,	TRF_DIV_CD
,	TRF_OPT_CD
,	DTL_DESC
,	FM_RNG_SZ_NO
,	TO_RNG_SZ_NO
,	MNR_RNG_TP_CD
,	VOL_TP_CD
,	RPR_QTY
,	RPR_SZ_NO
,	RPR_LBR_HRS
,	MTRL_COST_AMT
,	XCH_MTRL_COST_AMT
,	MTRL_RECO_AMT
,	MNR_VRFY_TP_CD
,	MAPG_RSLT_CD
,	DTL_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[trf_no]
,	DECODE(@[temp_seq], 0, @[trf_dtl_seq], (SELECT NVL(MAX(TRF_DTL_SEQ),0)+ @[temp_seq] FROM MNR_RPR_TRF_DTL WHERE TRF_NO = @[trf_no]))
,	@[cost_grp_cd]
,	@[eq_cmpo_cd]
,	@[eq_rpr_cd]
,	@[trf_div_cd]
,	@[trf_opt_cd]
,	@[dtl_desc]
,	NVL(@[fm_rng_sz_no],0)
,	NVL(@[to_rng_sz_no],0)
,	@[mnr_rng_tp_cd]
,	@[vol_tp_cd]
,	@[rpr_qty]
,	@[rpr_sz_no]
,	NVL(@[rpr_lbr_hrs],0)
,	@[mtrl_cost_amt]
,	NVL(@[xch_mtrl_cost_amt],0)
,	NVL(@[mtrl_reco_amt],0)
,	@[mnr_vrfy_tp_cd]
,	@[mapg_rslt_cd]
,	@[dtl_rmk]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="trf_no" type="12" value="11" out="N"/>
				<param name="temp_seq" type="12" value="11" out="N"/>
				<param name="trf_dtl_seq" type="12" value="1" out="N"/>
				<param name="cost_grp_cd" type="12" value="11" out="N"/>
				<param name="eq_cmpo_cd" type="12" value="11" out="N"/>
				<param name="eq_rpr_cd" type="12" value="11" out="N"/>
				<param name="trf_div_cd" type="12" value="11" out="N"/>
				<param name="trf_opt_cd" type="12" value="1" out="N"/>
				<param name="dtl_desc" type="12" value="11" out="N"/>
				<param name="fm_rng_sz_no" type="2" value="11" out="N"/>
				<param name="to_rng_sz_no" type="2" value="11" out="N"/>
				<param name="mnr_rng_tp_cd" type="12" value="11" out="N"/>
				<param name="vol_tp_cd" type="12" value="1" out="N"/>
				<param name="rpr_qty" type="2" value="11" out="N"/>
				<param name="rpr_sz_no" type="2" value="11" out="N"/>
				<param name="rpr_lbr_hrs" type="6" value="11" out="N"/>
				<param name="mtrl_cost_amt" type="6" value="11" out="N"/>
				<param name="xch_mtrl_cost_amt" type="6" value="11" out="N"/>
				<param name="mtrl_reco_amt" type="6" value="11" out="N"/>
				<param name="mnr_vrfy_tp_cd" type="12" value="11" out="N"/>
				<param name="mapg_rslt_cd" type="12" value="1" out="N"/>
				<param name="dtl_rmk" type="12" value="11" out="N"/>
				<param name="cre_usr_id" type="12" value="11" out="N"/>
				<param name="upd_usr_id" type="12" value="11" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
