<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchMGSOnHireStatusChkDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT A.EQ_TPSZ_CD
      ,TO_CHAR(A.MFT_DT,'YYYYMMDD') MFT_DT
      ,B.AGMT_OFC_CTY_CD||B.AGMT_SEQ AS AGREEMENT_NO
      ,A.AGMT_VER_NO
      ,B.VNDR_SEQ
      ,A.EQ_SPEC_NO
      ,A.MGST_MCHN_SER_NO
      ,A.MGST_VLTG_CAPA
      ,A.MGST_FUEL_CAPA
      ,A.CRE_USR_ID
      ,B.AGMT_LSTM_CD
 FROM CGM_EQUIPMENT A
     ,CGM_AGREEMENT B
 WHERE A.EQ_KND_CD = @[eq_knd_cd]
   AND A.EQ_NO     = @[eq_no]
   AND A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD
   AND A.AGMT_SEQ = B.AGMT_SEQ
   AND A.AGMT_VER_NO = B.AGMT_VER_NO
   AND A.EQ_KND_CD = B.EQ_KND_CD
   AND B.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
