	COST_OFC_CD, 
	REV_VSL_CD,
	REV_SKD_VOY_NO,
	REV_SKD_DIR_CD,
	REV_DIR_CD,
	AGMT_OFC_CTY_CD, 
	AGMT_SEQ, 
	AGMT_VER_NO, 
	POOL_MAX_RT_AMT,
	POOL_MIN_RT_AMT,
	CHG_SMRY_AMT,
	INV_SMRY_AMT,
	INV_DT,
	INV_USR_ID,
	DIFF_RMK,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT,
    ISS_OFC_CD
