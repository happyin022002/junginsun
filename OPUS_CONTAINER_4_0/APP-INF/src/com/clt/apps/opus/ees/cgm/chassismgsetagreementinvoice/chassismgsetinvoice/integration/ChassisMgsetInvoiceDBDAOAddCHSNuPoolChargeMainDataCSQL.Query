				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
				<param name="pool_max_rt_amt" type="12" value="" out="N"/>
				<param name="pool_min_rt_amt" type="12" value="" out="N"/>
