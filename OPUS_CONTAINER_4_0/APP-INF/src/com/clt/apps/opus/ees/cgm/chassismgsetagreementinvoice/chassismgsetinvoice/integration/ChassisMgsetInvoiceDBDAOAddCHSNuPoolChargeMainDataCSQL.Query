	@[pay_inv_seq],
	@[inv_no],
	'Z',
	@[chss_mgst_inv_knd_cd],
	@[chss_mgst_inv_sts_cd],
	@[vndr_seq],
	@[curr_cd],
	@[cost_yrmon],
	@[cost_ofc_cd],
	'CNTC',
	SUBSTR(@[cost_yrmon],3,6),
	'M',
	'M',
	@[agmt_ofc_cty_cd],
	@[agmt_seq],
	@[agmt_ver_no],
	@[pool_max_rt_amt],
	@[pool_min_rt_amt],
	@[chg_smry_amt],
	@[inv_smry_amt],
	TO_DATE(TO_CHAR(SYSDATE,'YYYYMMDD'),'YYYY/MM/DD'),
	@[inv_usr_id],
	@[diff_rmk],
	@[cre_usr_id],
	SYSDATE,
	@[upd_usr_id],
	SYSDATE,
    @[cost_ofc_cd]
)			]]></sql>
			<params>
				<param name="pay_inv_seq" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="chss_mgst_inv_knd_cd" type="12" value="" out="N"/>
				<param name="chss_mgst_inv_sts_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
