<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetOscaBkgFlgRSQL">
			<desc><![CDATA[getOscaBkgFlg]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
FROM CTM_BKG_CNTR
WHERE CNTR_NO=@[cntr_no]
AND CNMV_CYC_NO=@[max_cycle_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="max_cycle_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
