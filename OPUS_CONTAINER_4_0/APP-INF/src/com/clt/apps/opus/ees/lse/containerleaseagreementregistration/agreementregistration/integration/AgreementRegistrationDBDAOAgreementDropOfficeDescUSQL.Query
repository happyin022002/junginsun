<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRegistrationDBDAOAgreementDropOfficeDescUSQL">
			<desc><![CDATA[Lease Agreement Drop Office Description Modify]]></desc>
			<sql><![CDATA[
UPDATE LSE_DRP_OFF_DESC SET
    DPT_DESC = @[dpt_desc]
  , ADDR_DESC = @[addr_desc]
  , CTRT_PSON_DESC = @[ctrt_pson_desc]
  , CTRT_NO_DESC = @[ctrt_no_desc]
  , TURN_REF_NO_DESC = @[turn_ref_no_desc]
  , LSE_PSON_CTRT_EML = @[lse_pson_ctrt_eml]
  , UPD_USR_ID = @[upd_usr_id]
  , UPD_DT =  SYSDATE
WHERE AGMT_CTY_CD = @[agmt_cty_cd]
AND   AGMT_SEQ    = @[agmt_seq]
AND   LOC_CD      = @[loc_cd]			]]></sql>
			<params>
				<param name="dpt_desc" type="12" value="" out="N"/>
				<param name="addr_desc" type="12" value="" out="N"/>
				<param name="ctrt_pson_desc" type="12" value="" out="N"/>
				<param name="ctrt_no_desc" type="12" value="" out="N"/>
				<param name="turn_ref_no_desc" type="12" value="" out="N"/>
				<param name="lse_pson_ctrt_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
