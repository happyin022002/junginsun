<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmValidationDBDAOsearchCGMMasterDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT  A.EQ_NO
       ,A.EQ_TPSZ_CD
       ,A.CRNT_YD_CD
       ,A.ACIAC_DIV_CD
       ,TO_CHAR(A.CHSS_MVMT_DT,'YYYY-MM-DD HH24:MI') CHSS_MVMT_DT
FROM	CGM_EQUIPMENT A 
WHERE 	@[eq_no]     = A.EQ_NO
  AND   @[eq_knd_cd] = A.EQ_KND_CD			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
