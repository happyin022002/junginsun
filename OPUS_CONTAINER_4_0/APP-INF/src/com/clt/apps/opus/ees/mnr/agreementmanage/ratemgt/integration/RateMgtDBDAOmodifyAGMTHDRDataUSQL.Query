<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RateMgtDBDAOmodifyAGMTHDRDataUSQL">
			<desc><![CDATA[modify AGMT HDR]]></desc>
			<sql><![CDATA[
UPDATE MNR_AGMT_HDR SET 
	VNDR_SEQ = TO_NUMBER(@[vndr_seq])
,	PAY_TERM_DYS = @[pay_term_dys]
,	AGMT_REF_NO = @[agmt_ref_no]
,	AGMT_DT = TO_DATE(@[agmt_dt],'YYYY-MM-DD')
,	EFF_DT = TO_DATE(@[eff_dt],'YYYY-MM-DD')
,	EXP_DT = TO_DATE(@[exp_dt],'YYYY-MM-DD')
,	CURR_CD = @[curr_cd]
,	AGMT_RMK = @[agmt_rmk]
,	AGMT_OFC_CD = @[agmt_ofc_cd]
,	TRF_NO = @[trf_no]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
AND	AGMT_SEQ = @[agmt_seq]
AND	AGMT_VER_NO = @[agmt_ver_no]			]]></sql>
			<params>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="pay_term_dys" type="2" value="" out="N"/>
				<param name="agmt_ref_no" type="12" value="" out="N"/>
				<param name="agmt_dt" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="agmt_rmk" type="12" value="" out="N"/>
				<param name="agmt_ofc_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="agmt_ver_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
