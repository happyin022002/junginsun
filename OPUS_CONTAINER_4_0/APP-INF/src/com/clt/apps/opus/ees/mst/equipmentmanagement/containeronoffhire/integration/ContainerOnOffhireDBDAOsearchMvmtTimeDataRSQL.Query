<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOsearchMvmtTimeDataRSQL">
			<desc><![CDATA[searchMvmtTimeData]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC( A XFN1CTM_MOVEMENT ) */
NVL(MAX(TO_CHAR(DECODE(TRUNC(CNMV_EVNT_DT),TO_DATE(REPLACE(@[cnmv_evnt_dt],'-'),'YYYYMMDD HH24:MI'),CNMV_EVNT_DT+0.0007,TO_DATE(REPLACE(@[cnmv_evnt_dt],'-'),'YYYYMMDD HH24:MI')),'YYYYMMDD HH24:MI')),TO_CHAR(TO_DATE(REPLACE(@[cnmv_evnt_dt],'-'),'YYYYMMDD HH24:MI'),'YYYY-MM-DD HH24:MI')) DT
FROM CTM_MOVEMENT A
WHERE CNTR_NO = @[cntr_no]
AND   ROWNUM = 1 			]]></sql>
			<params>
				<param name="cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
