<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOCheckTPBCustomerByVendorRSQL">
			<desc><![CDATA[CheckTPBCustomerByVendor]]></desc>
			<sql><![CDATA[
SELECT CUST_SEQ 
FROM MDM_CUSTOMER 
WHERE CUST_CNT_CD = 'TB' 
AND VNDR_SEQ = @[s_cust_cd]			]]></sql>
			<params>
				<param name="s_cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
