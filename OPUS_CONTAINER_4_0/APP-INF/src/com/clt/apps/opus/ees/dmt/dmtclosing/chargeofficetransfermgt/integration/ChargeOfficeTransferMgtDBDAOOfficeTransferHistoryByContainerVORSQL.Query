<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeOfficeTransferMgtDBDAOOfficeTransferHistoryByContainerVORSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT	 FM_OFC_CD
        ,TO_OFC_CD
        ,TRNS_RSN
        ,CRE_DT
        ,CRE_OFC_CD
        ,USR_NM
FROM (
	SELECT	DISTINCT
			 OT.FM_OFC_CD
			,OT.TO_OFC_CD
			,OT.TRNS_RSN
			,TO_CHAR(OT.CRE_DT, 'YYYYMMDD') CRE_DT
			,OT.CRE_OFC_CD
			,(	SELECT CU.USR_NM
	         	FROM COM_USER CU
	         	WHERE CU.USR_ID = OT.CRE_USR_ID
		 	) AS USR_NM
	FROM	DMT_OFC_TRNS_HIS OT        
	WHERE	OT.CNTR_NO				= @[cntr_no]
	AND     OT.CNTR_CYC_NO			= @[cntr_cyc_no]
	AND     OT.DMDT_TRF_CD			= @[dmdt_trf_cd]
	AND     OT.DMDT_CHG_LOC_DIV_CD	= @[dmdt_chg_loc_div_cd]
	AND     OT.CHG_SEQ				= @[chg_seq]			
)
ORDER BY CRE_DT			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
