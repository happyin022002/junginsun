<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchCHSOnHireDupChkDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[

SELECT A.EQ_NO,
       A.EQ_TPSZ_CD,
       A.CHSS_VEH_ID_NO,
       A.CHSS_TIT_NO,
       A.CHSS_ALS_NO,
       A.N2ND_CHSS_ALS_NO
  FROM CGM_EQUIPMENT A 
 WHERE A.EQ_KND_CD = @[eq_knd_cd]
  AND ( 1=0 
#if (${chss_veh_id_no_tmp} != ''  ) 
   OR A.CHSS_VEH_ID_NO = @[chss_veh_id_no_tmp]
#end
#if (${chss_tit_no_tmp} != '') 
   OR A.CHSS_TIT_NO = @[chss_tit_no_tmp]
#end
#if (${chss_als_no_tmp} != '') 
   OR A.CHSS_ALS_NO       = @[chss_als_no_tmp]
   OR A.N2ND_CHSS_ALS_NO  = @[chss_als_no_tmp]
#end
#if (${n2nd_chss_als_no_tmp} != '') 
   OR A.CHSS_ALS_NO      = @[n2nd_chss_als_no_tmp]
   OR A.N2ND_CHSS_ALS_NO = @[n2nd_chss_als_no_tmp]
#end
)
   AND A.EQ_NO <> @[eq_no]
 ORDER BY EQ_NO			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="chss_veh_id_no_tmp" type="12" value="" out="N"/>
				<param name="chss_tit_no_tmp" type="12" value="" out="N"/>
				<param name="chss_als_no_tmp" type="12" value="" out="N"/>
				<param name="n2nd_chss_als_no_tmp" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
