<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusHistoryMgtDBDAOmodifyStatusHistoryDataUSQL">
			<desc><![CDATA[Status History 수정]]></desc>
			<sql><![CDATA[
UPDATE MNR_STS_HIS A
   SET A.MNR_GRP_TP_CD = @[mnr_grp_tp_cd]
      ,A.MNR_HIS_STS_CD = @[mnr_his_sts_cd]
      ,A.MNR_INP_TP_CD = @[mnr_inp_tp_cd]
      ,A.RQST_OFC_CD = @[rqst_ofc_cd]
      ,A.RQST_DT = TO_DATE(@[rqst_dt], 'yyyy-mm-dd')
      ,A.RQST_USR_ID = @[rqst_usr_id]
      ,A.APRO_OFC_CD = @[apro_ofc_cd]
      ,A.APRO_DT = TO_DATE(@[apro_dt], 'yyyy-mm-dd')
      ,A.APRO_USR_ID = @[apro_usr_id]
      ,A.MNR_STS_RMK = @[mnr_sts_rmk]
      ,A.MNR_STS_DT = TO_DATE(@[mnr_sts_dt], 'yyyy-mm-dd')
      ,A.UPD_USR_ID = @[upd_usr_id]
      ,A.UPD_DT = SYSDATE
 WHERE A.MNR_STS_REF_NO = @[mnr_sts_ref_no]
   AND A.MNR_STS_DTL_SEQ = @[mnr_sts_dtl_seq]			]]></sql>
			<params>
				<param name="mnr_grp_tp_cd" type="12" value="" out="N"/>
				<param name="mnr_his_sts_cd" type="12" value="" out="N"/>
				<param name="mnr_inp_tp_cd" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="rqst_dt" type="12" value="" out="N"/>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="apro_dt" type="12" value="" out="N"/>
				<param name="apro_usr_id" type="12" value="" out="N"/>
				<param name="mnr_sts_rmk" type="12" value="" out="N"/>
				<param name="mnr_sts_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mnr_sts_ref_no" type="12" value="" out="N"/>
				<param name="mnr_sts_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
