<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOSearchMgsetNoFindDataRSQL">
			<desc><![CDATA[checkLocation]]></desc>
			<sql><![CDATA[
SELECT 
ROWNUM AS DESC1,
A.DESC2,
A.DESC3
FROM
( 
SELECT EQ_SPEC_NO,MIN(EQ_NO),MAX(EQ_NO),  GR
    ,MIN(EQ_NO)||'-'|| MAX(EQ_NO) AS DESC2
    ,COUNT(*) AS DESC3
 FROM (
 
SELECT EQ_NO ,EQ_SPEC_NO
,TO_NUMBER( SUBSTR( EQ_NO , 5) ) A 
,TO_NUMBER( SUBSTR( EQ_NO , 5) ) -ROWNUM GR

FROM (
SELECT
EQ_NO 
,EQ_SPEC_NO
,LAG(EQ_NO,1) OVER ( ORDER BY EQ_NO  ) EQ_LAG

  FROM
CGM_EQUIPMENT
WHERE EQ_SPEC_NO= @[eq_spec_no]
    AND EQ_KND_CD = 'G'
ORDER BY EQ_NO    
)

)
GROUP BY 
EQ_SPEC_NO,GR
ORDER BY DESC2
) A


			]]></sql>
			<params>
				<param name="eq_spec_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
