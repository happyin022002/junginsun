<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchNTdateRSQL">
			<desc><![CDATA[DMTCalculationDBDAOSearchNTdateRSQL]]></desc>
			<sql><![CDATA[
SELECT  TO_CHAR(E.EVNT_DT, 'YYYYMMDDHH24MI') EVNT_DT
  FROM    EDI_322_MSG E,
          MDM_YARD    Y
  WHERE E.EQ_NO = @[cntr_no]
  AND E.EDI_322_STS_CD = 'NT'
  AND E.EVNT_YD_CD = @[yd_cd]
  AND (E.BL_EDI_322_NO = @[bkg_no] or E.BKG_EDI_322_NO = @[bkg_no])
  AND E.EVNT_YD_CD = Y.YD_CD
  AND Y.RAIL_ARR_NTFC_FLG = 'Y'			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
