<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOcheckWorkOrderDtlDataRSQL">
			<desc><![CDATA[checkWorkOrderDtlData]]></desc>
			<sql><![CDATA[
SELECT
     A.MNR_ORD_OFC_CTY_CD AS CD_ID,
     A.MNR_ORD_SEQ AS CD_DESC
FROM MNR_ORD_HDR A,MNR_ORD_DTL B
WHERE 
     A.MNR_ORD_OFC_CTY_CD = B.MNR_ORD_OFC_CTY_CD
     AND A.MNR_ORD_SEQ = B.MNR_ORD_SEQ
     AND A.MNR_ORD_OFC_CTY_CD = @[mnr_ord_ofc_cty_cd]
     AND A.MNR_ORD_SEQ = @[mnr_ord_seq]
     AND ROWNUM = 1			]]></sql>
			<params>
				<param name="mnr_ord_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="mnr_ord_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
