<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MgsetMovementHistoryDBDAOsearchCgmEqAtchDtchHisDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT A.EQ_NO
     , A.CNTR_NO
     , A.ATCH_YD_CD
     , TO_CHAR(A.ATCH_DT,'YYYYMMDDHH24MISS' ) ATCH_DT
     , A.DTCH_YD_CD
     , TO_CHAR(A.DTCH_DT,'YYYYMMDDHH24MISS' ) DTCH_DT
     , LAG(A.DTCH_YD_CD) OVER (ORDER BY A.EQ_NO, TO_CHAR(A.ATCH_DT,'YYYYMMDDHH24MISS' )) LAG_DT_YD
     , LAG(TO_CHAR(A.DTCH_DT,'YYYYMMDDHH24MISS' )) OVER (ORDER BY A.EQ_NO, TO_CHAR(A.ATCH_DT,'YYYYMMDDHH24MISS' )) LAG_DT
     , LEAD(A.ATCH_YD_CD) OVER (ORDER BY A.EQ_NO, TO_CHAR(A.ATCH_DT,'YYYYMMDDHH24MISS' )) LEAD_DT_YD 
     , LEAD(TO_CHAR(A.ATCH_DT,'YYYYMMDDHH24MISS' )) OVER (ORDER BY A.EQ_NO, TO_CHAR(A.ATCH_DT,'YYYYMMDDHH24MISS' )) LEAD_DT
FROM CGM_EQ_ATCH_DTCH_HIS A
WHERE A.EQ_NO = @[eq_no]
 AND  A.ATCH_DT  = TO_DATE(@[atch_dt] ,'YYYYMMDDHH24MISS')
ORDER BY A.ATCH_DT			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="atch_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
