<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOgetRFAFreeTimeRSQL">
			<desc><![CDATA[RFA Free Time Search]]></desc>
			<sql><![CDATA[
SELECT FT_DYS FT_DAY
  FROM DMT_RFA_EXPT_FREE_TM
 WHERE RFA_EXPT_DAR_NO 		= @[dar_no]
   AND RFA_EXPT_MAPG_SEQ 	= @[mapg_seq]
   AND RFA_EXPT_VER_SEQ 	= @[ver_seq]
   AND RFA_RQST_DTL_SEQ 	= @[dtl_seq]
   AND CVRG_CMB_SEQ 		= @[cmb_seq]
   AND ( ( CNTR_FM_QTY <= @[qty] AND CNTR_TO_QTY = 0 )
      OR ( CNTR_FM_QTY <= @[qty] AND CNTR_TO_QTY IS NULL )
      OR ( CNTR_FM_QTY <= @[qty] AND CNTR_TO_QTY >= @[qty] ) )			]]></sql>
			<params>
				<param name="dar_no" type="12" value="" out="N"/>
				<param name="mapg_seq" type="12" value="" out="N"/>
				<param name="ver_seq" type="12" value="" out="N"/>
				<param name="dtl_seq" type="12" value="" out="N"/>
				<param name="cmb_seq" type="12" value="" out="N"/>
				<param name="qty" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
