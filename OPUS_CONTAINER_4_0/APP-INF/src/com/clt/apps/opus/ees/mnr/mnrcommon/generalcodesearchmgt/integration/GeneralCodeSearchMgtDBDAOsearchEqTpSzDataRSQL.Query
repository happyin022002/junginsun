<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchEqTpSzDataRSQL">
			<desc><![CDATA[EQ Type size 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CD_ID, CD_DESC
FROM 
(	SELECT
		 A.CNTR_TPSZ_CD AS CD_ID
		,A.CNTR_TPSZ_DESC AS CD_DESC
		,A.RPT_DP_SEQ AS DP_SEQ
	FROM MDM_CNTR_TP_SZ A
	WHERE A.DELT_FLG = 'N'
    AND   'U'  = @[searchkey]
	UNION ALL
	SELECT A.EQ_TPSZ_CD AS CD_ID
	       , A.DIFF_DESC  AS CD_DESC
	       , A.DP_SEQ DP_SEQ
	FROM CGM_EQ_TP_SZ A
	WHERE A.EQ_KND_CD = @[searchkey]
)
ORDER BY DP_SEQ			]]></sql>
			<params>
				<param name="searchkey" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
