<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyPayerInfomationUSQL">
			<desc><![CDATA[DEM/DET Payer Info & Fax/E-mail]]></desc>
			<sql><![CDATA[
UPDATE DMT_PAYR_INFO 
SET 
	DMDT_PAYR_NM 			= @[dmdt_payr_nm]
,	DMDT_PAYR_CNTC_PNT_NM 	= @[dmdt_payr_cntc_pnt_nm]
,	DMDT_PAYR_ADDR 			= @[dmdt_payr_addr]
,	DMDT_PAYR_ZIP_CTNT 		= @[dmdt_payr_zip_ctnt]
,	DMDT_PAYR_PHN_NO 		= @[dmdt_payr_phn_no]
,	DMDT_PAYR_FAX_NO 		= @[dmdt_payr_fax_no]
,	DMDT_PAYR_N1ST_EML 		= @[dmdt_payr_n1st_eml]
,	UPD_USR_ID 				= @[upd_usr_id]
,	UPD_DT 					= SYSDATE
,	UPD_OFC_CD 				= @[upd_ofc_cd]
WHERE	SYS_AREA_GRP_ID = @[svr_id]
AND CUST_CNT_CD 		= @[cust_cnt_cd]
AND	CUST_SEQ 			= @[cust_seq]			]]></sql>
			<params>
				<param name="dmdt_payr_nm" type="12" value="" out="N"/>
				<param name="dmdt_payr_cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="dmdt_payr_addr" type="12" value="" out="N"/>
				<param name="dmdt_payr_zip_ctnt" type="12" value="" out="N"/>
				<param name="dmdt_payr_phn_no" type="12" value="" out="N"/>
				<param name="dmdt_payr_fax_no" type="12" value="" out="N"/>
				<param name="dmdt_payr_n1st_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
