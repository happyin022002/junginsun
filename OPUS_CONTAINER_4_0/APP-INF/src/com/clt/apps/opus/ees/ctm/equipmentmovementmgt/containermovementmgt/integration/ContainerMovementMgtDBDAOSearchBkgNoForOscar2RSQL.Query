<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchBkgNoForOscar2RSQL">
			<desc><![CDATA[SearchBkgNoForOscar2]]></desc>
			<sql><![CDATA[
SELECT BKG_NO 
FROM CTM_BKG_CNTR
WHERE BKG_NO LIKE '%'||@[bkg_no]||'%'
AND CNTR_NO = @[cntr_no]
AND ROWNUM =1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
