<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOsearchNextWeekRSQL">
			<desc><![CDATA[SEARCH NEXT WEEK]]></desc>
			<sql><![CDATA[
SELECT PLN_YR||PLN_WK                           					
FROM EQR_WK_PRD                               					
WHERE TO_CHAR(SYSDATE+7,'YYYYMMDD') BETWEEN WK_ST_DT AND WK_END_DT			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
