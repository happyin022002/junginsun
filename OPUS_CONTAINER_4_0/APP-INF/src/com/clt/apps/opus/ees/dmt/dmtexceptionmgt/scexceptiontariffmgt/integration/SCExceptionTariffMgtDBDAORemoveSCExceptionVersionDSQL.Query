<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAORemoveSCExceptionVersionDSQL">
			<desc><![CDATA[Version 에 해당되는 모든 Group 정보가 삭제되었다면, Version 정보도 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM DMT_SC_EXPT_VER
WHERE  	PROP_NO 		= @[prop_no]
	AND	SC_EXPT_VER_SEQ = @[sc_expt_ver_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
