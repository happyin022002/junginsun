<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongstayingUnclaimEQFlaggingDBDAOaddFlaggingCSQL">
			<desc><![CDATA[장기체화장비(L/Staying) 및 Unclaim 장비 Flag 와 해소 방안을 등록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO CIM_UCLM_LONG_STAY_HIS(
     CNTR_NO
    ,UCLM_LS_SEQ
    ,CNTR_TPSZ_CD
    ,BKG_NO
    ,BL_NO
    ,CNMV_YR
    ,CNMV_ID_NO
    ,CNMV_STS_CD
    ,CNMV_DT
    ,CNMV_GDT
    ,YD_CD
    ,FULL_FLG
    ,CNTR_DISP_FLG
    ,CNTR_DMG_FLG
    ,STAY_DYS
    ,UCLM_LS_DIV_CD
    ,UCLM_DT
    ,UCLM_FREE_DYS
    ,UCLM_END_DT
    ,UCLM_ACT_STAY_DYS
    ,UCLM_RSN
    ,UCLM_PLN_RMK
    ,UCLM_CNTC_PNT_NM
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT

)
SELECT 
     @[cntr_no]               
    ,(SELECT NVL(MAX(UCLM_LS_SEQ),0)+1 FROM CIM_UCLM_LONG_STAY_HIS WHERE  CNTR_NO=@[cntr_no] )
    ,@[cntr_tpsz_cd]          
    ,@[bkg_no]                
    ,@[bl_no]                 
    ,@[cnmv_yr]               
    ,@[cnmv_id_no]            
    ,@[cnmv_sts_cd]           
    ,TO_DATE(@[cnmv_dt],'YYYY-MM-DD HH24:MI:SS')
    ,TO_DATE(@[cnmv_gmt_dt],'YYYY-MM-DD HH24:MI:SS')
    ,@[crnt_yd_cd]                 
    ,@[full_flg]              
    ,@[disp_flg]         
    ,@[dmg_flg]          
    ,@[stay_days]              
    ,@[uclm_ls_div_cd]        
    ,TO_DATE(@[uclm_dt],'YYYYMMDD')                   
    ,@[ft_dys]         
    ,TO_DATE(@[ft_end_dt],'YYYYMMDD')           
    ,@[act_dys]     
    ,@[uclm_rsn]              
    ,@[uclm_pln_rmk]          
    ,@[uclm_cntc_pnt_nm]      
    ,@[cre_usr_id]            
    ,sysdate                
    ,@[upd_usr_id]            
    ,sysdate                
FROM DUAL			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
				<param name="cnmv_sts_cd" type="12" value="" out="N"/>
				<param name="cnmv_dt" type="12" value="" out="N"/>
				<param name="cnmv_gmt_dt" type="12" value="" out="N"/>
				<param name="crnt_yd_cd" type="12" value="" out="N"/>
				<param name="full_flg" type="12" value="" out="N"/>
				<param name="disp_flg" type="12" value="" out="N"/>
				<param name="dmg_flg" type="12" value="" out="N"/>
				<param name="stay_days" type="12" value="" out="N"/>
				<param name="uclm_ls_div_cd" type="12" value="" out="N"/>
				<param name="uclm_dt" type="12" value="" out="N"/>
				<param name="ft_dys" type="12" value="" out="N"/>
				<param name="ft_end_dt" type="12" value="" out="N"/>
				<param name="act_dys" type="12" value="" out="N"/>
				<param name="uclm_rsn" type="12" value="" out="N"/>
				<param name="uclm_pln_rmk" type="12" value="" out="N"/>
				<param name="uclm_cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
