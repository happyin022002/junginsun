<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyInvoiceMainUSQL">
			<desc><![CDATA[Invoice Cancel Reason Entry]]></desc>
			<sql><![CDATA[
UPDATE DMT_INV_MN
SET CR_INV_NO = @[cr_inv_no]
,DMDT_INV_STS_CD = @[dmdt_inv_sts_cd]
,DMDT_CXL_RSN_CD = @[dmdt_cxl_rsn_cd]
,CXL_RMK = @[cxl_rmk]
,UPD_USR_ID = @[upd_usr_id]
,UPD_DT = SYSDATE
,UPD_OFC_CD = @[upd_ofc_cd]
WHERE DMDT_INV_NO = @[old_dmdt_inv_no]
AND CRE_OFC_CD = @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="cr_inv_no" type="12" value="" out="N"/>
				<param name="dmdt_inv_sts_cd" type="12" value="" out="N"/>
				<param name="dmdt_cxl_rsn_cd" type="12" value="" out="N"/>
				<param name="cxl_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="old_dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
