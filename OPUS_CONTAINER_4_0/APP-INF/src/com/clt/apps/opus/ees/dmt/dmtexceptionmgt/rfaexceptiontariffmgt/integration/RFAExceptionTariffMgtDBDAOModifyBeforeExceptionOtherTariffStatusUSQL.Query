<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOModifyBeforeExceptionOtherTariffStatusUSQL">
			<desc><![CDATA[생성된 상위 Version 의 Status 가 Approved 로 변경시에 기존의 Approved 의 STS 를 가지는 해당 DAR 의 다른 Version 의 STS 를
Cancelled 로 변경하는 쿼리]]></desc>
			<sql><![CDATA[
UPDATE	DMT_RFA_EXPT_TRF 
SET		DMDT_EXPT_RQST_STS_CD = 'C'
	,	RFA_EXPT_APRO_NO = ''
	,	UPD_USR_ID = @[upd_usr_id]
	,	UPD_DT = SYSDATE
	,	UPD_OFC_CD = @[upd_ofc_cd]
WHERE	RFA_EXPT_DAR_NO = @[rfa_expt_dar_no]
	AND	RFA_EXPT_MAPG_SEQ = @[rfa_expt_mapg_seq]
	AND RFA_EXPT_VER_SEQ < @[rfa_expt_ver_seq]
	AND DMDT_EXPT_RQST_STS_CD = 'A'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
