<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchBeforeVERListRSQL">
			<desc><![CDATA[DAR No. 에 해당되는 모든 VER 정보를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT 	DISTINCT
		LPAD(A.RFA_EXPT_VER_SEQ, 3, '0') AS RFA_EXPT_VER_SEQ
	, 	A.DMDT_EXPT_RQST_STS_CD || ':' || B.INTG_CD_VAL_DP_DESC  || ':' || A.RQST_OFC_CD AS DMDT_EXPT_RQST_STS_CD

FROM	DMT_RFA_EXPT_TRF A, COM_INTG_CD_DTL B

WHERE	A.RFA_EXPT_DAR_NO = @[rfa_expt_dar_no]
#if(${is_temp} == 'N')
	AND A.DMDT_EXPT_RQST_STS_CD != 'T'
#end
    AND A.DMDT_EXPT_RQST_STS_CD = B.INTG_CD_VAL_CTNT
    AND B.INTG_CD_ID = 'CD02069'

ORDER BY RFA_EXPT_VER_SEQ DESC			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
