<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOsearchRFSpareWOHeaderDataRSQL">
			<desc><![CDATA[searchRFSpareWOHeaderData select]]></desc>
			<sql><![CDATA[
SELECT
         A.MNR_ORD_OFC_CTY_CD
        ,A.MNR_ORD_SEQ
        ,A.EQ_KND_CD
        ,A.MNR_GRP_TP_CD
        ,A.MNR_WO_TP_CD
        ,A.COST_CD
        ,A.TRSM_MOD_CD
        ,A.AGMT_OFC_CTY_CD
        ,A.AGMT_SEQ
        ,A.AGMT_VER_NO
        ,A.CURR_CD
        ,A.MNR_AGMT_AMT
        ,A.MNR_WRK_AMT
        ,A.INV_AMT
        ,A.ORD_ISS_OFC_CD
        ,TO_CHAR(A.MNR_ORD_SND_DT, 'yyyy-mm-dd') MNR_ORD_SND_DT
        ,A.COST_OFC_CD
        ,A.VNDR_SEQ
        ,A.SPR_PRT_SPL_TP_CD
        ,A.VSL_CD
        ,A.SKD_VOY_NO
        ,A.SKD_DIR_CD
        ,TO_CHAR(A.SPR_PRT_BRTH_DT, 'yyyy-mm-dd') SPR_PRT_BRTH_DT
        ,A.SPR_PRT_SPL_YD_CD
        ,TO_CHAR(A.SPR_PRT_SPL_DT, 'yyyy-mm-dd') SPR_PRT_SPL_DT
        ,A.ORD_HDR_RMK
		,A.FILE_SEQ
        ,TO_CHAR(A.MNR_INP_DT, 'yyyy-mm-dd') MNR_INP_DT
        ,A.CRE_USR_ID
        ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
       	,(SELECT 
			   VNDR_LGL_ENG_NM
		  FROM mdm_vendor
		  WHERE VNDR_SEQ=A.VNDR_SEQ
        -- AND OFC_CD =B.COST_OFC_CD
            AND NVL(DELT_FLG, 'N') <> 'Y'
		) AS PIG_ENG_NM 
FROM MNR_ORD_HDR A
WHERE A.MNR_ORD_OFC_CTY_CD = @[mnr_ord_ofc_cty_cd]
AND   A.MNR_ORD_SEQ = @[mnr_ord_seq]
AND   A.MNR_WO_TP_CD = @[mnr_wo_tp_cd]
AND   A.COST_OFC_CD = @[cost_of_cd]			]]></sql>
			<params>
				<param name="mnr_ord_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="mnr_ord_seq" type="12" value="" out="N"/>
				<param name="mnr_wo_tp_cd" type="12" value="" out="N"/>
				<param name="cost_of_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
