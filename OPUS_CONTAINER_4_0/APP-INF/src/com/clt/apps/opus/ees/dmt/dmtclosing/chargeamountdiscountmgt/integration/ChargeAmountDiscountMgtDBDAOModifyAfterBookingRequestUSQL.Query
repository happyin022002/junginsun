<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAOModifyAfterBookingRequestUSQL">
			<desc><![CDATA[After Booking Request 정보를 수정하기 위한 쿼리]]></desc>
			<sql><![CDATA[
UPDATE	DMT_AFT_BKG_ADJ_RQST SET
	DMDT_EXPT_RQST_STS_CD = @[dmdt_expt_rqst_sts_cd]
,	APRO_OFC_CD = @[apro_ofc_cd]
#if(${aft_bkg_apro_no} != '')
,	AFT_BKG_APRO_NO = @[aft_bkg_apro_no]
,	APRO_USR_ID = @[apro_usr_id]
,	APRO_DT = SYSDATE
#end
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,	UPD_OFC_CD = @[upd_ofc_cd]
WHERE	AFT_EXPT_DAR_NO = @[aft_expt_dar_no]			]]></sql>
			<params>
				<param name="dmdt_expt_rqst_sts_cd" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="aft_bkg_apro_no" type="12" value="" out="N"/>
				<param name="apro_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
