<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAOmodifyTariffFreeTimeUSQL">
			<desc><![CDATA[Basic Tariff Creation - Group]]></desc>
			<sql><![CDATA[
UPDATE DMT_TRF_FREE_TM
SET FT_FM_QTY = @[ft_fm_qty]
, FT_TO_QTY = @[ft_to_qty]
, FT_DYS = @[ft_dys]
, UPD_USR_ID = @[usr_id]
, UPD_DT = sysdate
, UPD_OFC_CD = @[ofc_cd]
WHERE SYS_AREA_GRP_ID = @[svr_id]
AND DMDT_TRF_CD = @[dmdt_trf_cd]
AND TRF_SEQ = @[trf_seq]
AND TRF_GRP_SEQ = @[trf_grp_seq]
AND TRF_FT_SEQ = @[trf_ft_seq]			]]></sql>
			<params>
				<param name="ft_fm_qty" type="12" value="" out="N"/>
				<param name="ft_to_qty" type="12" value="" out="N"/>
				<param name="ft_dys" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="trf_seq" type="12" value="" out="N"/>
				<param name="trf_grp_seq" type="12" value="" out="N"/>
				<param name="trf_ft_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
