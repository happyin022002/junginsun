<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOSearchMovementListByCntrTpszCdVORSQL">
			<desc><![CDATA[EES_CTM_0409
-- HTML - VOLUME항목 데이터 가져오는 쿼리      ]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD,
       OP_CNTR_QTY
  FROM BKG_QUANTITY
 WHERE BKG_NO = @[bkg_no]
 ORDER BY OP_CNTR_QTY DESC			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
