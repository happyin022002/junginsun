<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmValidationDBDAOsearchMDMCurrencyDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.CURR_CD
      ,A.CURR_NM
      ,A.CURR_DESC
      ,A.CNT_CD
      ,A.FM_EFF_DT
      ,A.TO_EFF_DT
      ,A.DP_PRCS_KNT
      ,A.XTD_PRCS_KNT
 FROM MDM_CURRENCY A
WHERE A.DELT_FLG = 'N'
 AND  A.CURR_CD =  @[curr_cd]			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
