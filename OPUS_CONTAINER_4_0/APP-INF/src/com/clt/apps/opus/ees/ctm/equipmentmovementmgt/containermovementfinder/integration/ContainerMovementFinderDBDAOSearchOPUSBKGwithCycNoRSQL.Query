<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOSearchOPUSBKGwithCycNoRSQL">
			<desc><![CDATA[SearchOPUSBKGwithCycNo]]></desc>
			<sql><![CDATA[
SELECT BC.BKG_NO
  FROM BKG_CONTAINER BC, BKG_BOOKING BB
 WHERE 1=1
   AND BC.CNTR_NO = @[cntr_no]
   AND BC.CNMV_CYC_NO = @[cnmv_cyc_no]
#if (${cnmv_cyc_no} != '9999') 
   AND BC.CNMV_STS_CD NOT IN ('ID', 'MT')
#end
   AND BC.BKG_NO = BB.BKG_NO
   AND BB.BKG_STS_CD != 'X'
   AND BB.BKG_STS_CD != 'S'			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_cyc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
