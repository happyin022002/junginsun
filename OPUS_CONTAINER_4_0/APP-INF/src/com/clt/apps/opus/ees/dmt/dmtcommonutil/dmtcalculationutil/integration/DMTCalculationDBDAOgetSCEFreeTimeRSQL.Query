<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOgetSCEFreeTimeRSQL">
			<desc><![CDATA[getSCEFreeTime]]></desc>
			<sql><![CDATA[
SELECT  FT_DYS ft_day
  FROM  DMT_SC_EXPT_FREE_TM
 WHERE  PROP_NO        			=    @[sc_no]
   AND  SC_EXPT_VER_SEQ        	=    @[ver_seq]
   AND  SC_EXPT_GRP_SEQ        	=    @[grp_seq]
   AND  (
          ( CNTR_FM_QTY <= @[qty] AND CNTR_TO_QTY = 0 )
           OR
          ( CNTR_FM_QTY <= @[qty] AND CNTR_TO_QTY IS NULL )
           OR
          ( CNTR_FM_QTY <= @[qty] AND CNTR_TO_QTY >= @[qty] )
    	)			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="ver_seq" type="12" value="" out="N"/>
				<param name="grp_seq" type="12" value="" out="N"/>
				<param name="qty" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
