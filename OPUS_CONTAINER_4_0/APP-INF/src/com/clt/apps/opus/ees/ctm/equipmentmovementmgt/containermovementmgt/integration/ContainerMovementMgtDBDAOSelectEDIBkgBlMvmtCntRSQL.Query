<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSelectEDIBkgBlMvmtCntRSQL">
			<desc><![CDATA[SelectEDIBkgBlMvmtCnt]]></desc>
			<sql><![CDATA[
select count(*) CNT from CTM_BKG_BL_CMB
where 1=1
and BKG_NO = @[bkg_no]
and BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
