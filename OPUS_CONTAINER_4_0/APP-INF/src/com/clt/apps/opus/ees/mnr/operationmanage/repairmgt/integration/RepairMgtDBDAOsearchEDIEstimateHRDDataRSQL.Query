<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOsearchEDIEstimateHRDDataRSQL">
			<desc><![CDATA[searchEDIEstimateHRDData]]></desc>
			<sql><![CDATA[
SELECT
         A.RQST_EQ_NO
	    ,A.RPR_RQST_TMP_SEQ AS RPR_RQST_SEQ 
	    ,A.RPR_RQST_TMP_VER_NO AS RPR_RQST_VER_NO 
        ,A.RPR_RQST_LST_VER_FLG
        ,A.EQ_KND_CD
        ,A.EQ_TPSZ_CD
        ,A.VNDR_SEQ
        ,A.RPR_STS_CD
        ,A.RPR_DTL_STS_CD
        ,A.RQST_REF_NO
        ,A.MNR_INP_TP_CD
        ,A.COST_OFC_CD
        ,TO_CHAR(A.RQST_DT, 'yyyy-mm-dd') RQST_DT
        ,A.RQST_USR_ID
		,MNR_COMMON_PKG.MNR_GET_USR_NM_FNC(A.RQST_USR_ID) AS RQST_USR_NM
        ,A.MNR_MEAS_UT_CD
        ,A.APRO_OFC_CD
        ,TO_CHAR(A.APRO_DT, 'yyyy-mm-dd') APRO_DT
        ,A.APRO_USR_ID
        ,A.RPR_OFFH_FLG
        ,TO_CHAR(A.RPR_RSLT_DT, 'yyyy-mm-dd') RPR_RSLT_DT
        ,A.CURR_CD
        ,A.RPR_YD_CD
        ,TO_CHAR(A.EQ_DMG_DT, 'yyyy-mm-dd') EQ_DMG_DT
        ,A.EQ_DMG_TP_CD
        ,A.RPR_WRK_TP_CD
        ,A.MNR_EDI_NM
        ,TO_CHAR(A.MNR_ORD_ISS_DT, 'yyyy-mm-dd') MNR_ORD_ISS_DT
        ,TO_CHAR(A.MNR_ORD_SND_DT, 'yyyy-mm-dd') MNR_ORD_SND_DT
        ,A.N3PTY_FLG
        ,A.INV_OFC_CD
        ,A.INV_NO
        ,A.IF_TRC_SEQ
        ,A.MNR_LBR_AMT AS MNR_AGMT_AMT
        ,(A.MNR_LBR_AMT + A.MNR_MTRL_AMT) AS MNR_WRK_AMT
        ,A.INV_AMT
        ,A.MNR_TTL_AMT AS N3PTY_BIL_TTL_AMT
        ,A.DISP_FLG
        ,A.DISP_NO
        ,A.DISP_DTL_SEQ
        ,A.FILE_SEQ
        ,A.MNR_RPR_RMK
        ,A.EDI_ID
        ,A.MNR_ORD_OFC_CTY_CD
        ,A.MNR_ORD_SEQ
        ,A.AGMT_OFC_CTY_CD
        ,A.AGMT_SEQ
        ,A.AGMT_VER_NO
        ,A.RPR_RQST_TMP_SEQ
        ,A.CRE_USR_ID
        ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
        ,A.UPD_USR_ID
        ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
FROM MNR_RPR_RQST_TMP_HDR A
WHERE A.RQST_EQ_NO = @[rqst_eq_no]
AND   A.RPR_RQST_TMP_SEQ = @[rpr_rqst_seq]
AND   A.RPR_RQST_TMP_VER_NO = @[rpr_rqst_ver_no]			]]></sql>
			<params>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
				<param name="rpr_rqst_seq" type="12" value="" out="N"/>
				<param name="rpr_rqst_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
