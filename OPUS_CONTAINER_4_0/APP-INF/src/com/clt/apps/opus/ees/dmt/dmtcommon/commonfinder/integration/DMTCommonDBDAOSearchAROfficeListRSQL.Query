<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchAROfficeListRSQL">
			<desc><![CDATA[SearchAROfficeList]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT A.AR_OFC_CD  AS OFC_CD
        ,A.OFC_ENG_NM AS OFC_ENG_NM
    FROM MDM_ORGANIZATION A
        , (SELECT AR_HD_QTR_OFC_CD
                 ,AR_OFC_CD
                 ,LOC_CD
             FROM MDM_ORGANIZATION
#if(${rhq_ofc} != ${head_office})
            WHERE OFC_CD = @[ofc_cd]
#end
		  ) B
        ,COM_SYS_AREA_GRP_ID C
        ,INV_AR_STUP_OFC D
        ,MDM_CURRENCY E
   WHERE A.AR_HD_QTR_OFC_CD = B.AR_HD_QTR_OFC_CD
     AND A.OFC_CD = A.AR_OFC_CD
     AND A.OFC_CD = D.AR_OFC_CD(+)
     AND SUBSTR (A.LOC_CD
                ,1
                ,2
                ) = C.CNT_CD
     AND C.CO_IND_CD = 'H'
     AND A.AR_CURR_CD = E.CURR_CD
ORDER BY A.AR_OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
