<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffMgtDBDAOsearchRepairTariffHeaderDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT   A.TRF_NO
        ,A.EQ_KND_CD
        ,A.MNR_TRF_KND_CD
        ,A.MNR_INP_TP_CD
        --,A.VNDR_SEQ
        ,MNR_COMMON_PKG.MNR_CONV_PARTNER_CD_FNC(A.VNDR_SEQ) AS VNDR_SEQ
        ,A.APRO_OFC_CD
        ,A.MNR_TRF_STS_CD
        ,TO_CHAR(A.MNR_TRF_STS_DT, 'yyyy-mm-dd') MNR_TRF_STS_DT
        ,A.STS_REF_NO
        ,A.MNR_MEAS_UT_CD
        ,A.CURR_CD
        ,TO_CHAR(A.EFF_DT, 'yyyy-mm-dd') EFF_DT
        ,A.MNR_TRF_RMK
        ,A.RQST_OFC_CD
        ,A.CRE_USR_ID
        ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.CRE_DT, @[ofc_cd]), 'yyyy-mm-dd') CRE_DT
        ,A.UPD_USR_ID
        ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
FROM MNR_RPR_TRF_HDR A
WHERE A.TRF_NO = @[search_trf_no]
#if (${access_system} != 'SPP') 
AND A.MNR_TRF_STS_CD NOT IN ('SD','HD','SS')
#else 
AND A.MNR_TRF_STS_CD NOT IN ('SD','HD')
#end			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="1" out="N"/>
				<param name="search_trf_no" type="12" value="dfdfdfd" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
