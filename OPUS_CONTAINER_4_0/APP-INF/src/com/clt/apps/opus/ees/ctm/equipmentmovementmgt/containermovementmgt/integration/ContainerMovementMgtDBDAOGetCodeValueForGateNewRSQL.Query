<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetCodeValueForGateNewRSQL">
			<desc><![CDATA[Value, Column, Table을 Param으로 받아서 Value값을 return]]></desc>
			<sql><![CDATA[
SELECT ${return_nm} AS RETURN_NM
  FROM ${table_nm}
 WHERE ${column_nm} = @[code_value]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="code_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
