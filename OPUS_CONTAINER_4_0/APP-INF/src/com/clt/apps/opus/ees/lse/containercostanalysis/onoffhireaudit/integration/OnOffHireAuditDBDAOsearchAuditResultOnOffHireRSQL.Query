<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnOffHireAuditDBDAOsearchAuditResultOnOffHireRSQL">
			<desc><![CDATA[Lessor 와 자사를 비교하여 Concidence Discrepancy 자사 Lessor 데이타를 추출한다.]]></desc>
			<sql><![CDATA[
SELECT
  H.AGMT_CTY_CD|| LTRIM(TO_CHAR(H.AGMT_SEQ,'000000')) AGMT_NO ,
  H.AGMT_CTY_CD                   AGMT_CTY_CD,
  H.AGMT_SEQ                      AGMT_SEQ,
  H.LSTM_CD                       LSTM_CD ,
  H.CNTR_NO                       CNTR_NO ,
  H.CNTR_TPSZ_CD                  CNTR_TPSZ_CD ,
  TO_CHAR(H.ONH_DT , 'YYYYMMDD')  ONH_DT ,
  H.ONH_LOC_CD                    ONH_LOC_CD ,
  TO_CHAR(H.OFFH_DT ,'YYYYMMDD')  OFFH_DT ,
  H.OFFH_LOC_CD                   OFFH_LOC_CD ,
  TO_CHAR(L.LR_ONH_DT, 'YYYYMMDD')   LR_ONH_DT ,
  L.LR_ONH_LOC_CD                    LR_ONH_LOC_CD ,
  TO_CHAR(L.LR_OFFH_DT,'YYYYMMDD')   LR_OFFH_DT ,
  L.LR_OFFH_LOC_CD                   LR_OFFH_LOC_CD ,
  CASE WHEN H.AGMT_CTY_CD  = L.AGMT_CTY_CD
       AND  H.AGMT_SEQ     = L.AGMT_SEQ
       AND  H.LSTM_CD      = L.LSTM_CD
       AND  H.CNTR_NO      = L.CNTR_NO
       AND  H.CNTR_TPSZ_CD = L.CNTR_TPSZ_CD
       AND  H.REF_NO       = L.REF_NO
#if (${agmt_seq} != '' ) 
       AND H.AGMT_CTY_CD   = @[agmt_cty_cd]
       AND H.AGMT_SEQ      = @[agmt_seq]
#end
#if (${audit_tp} == 'N' ) 
       AND  H.ONH_DT       = L.LR_ONH_DT
       AND  H.ONH_LOC_CD   = L.LR_ONH_LOC_CD
#elseif (${audit_tp} == 'F' ) 
       AND  H.OFFH_DT      = L.LR_OFFH_DT 
       AND  H.OFFH_LOC_CD  = L.LR_OFFH_LOC_CD
#elseif (${audit_tp} == 'A' )        
       AND  DECODE( H.ONH_DT     , ''  , '1' , H.ONH_DT)     = DECODE( L.LR_ONH_DT      , '' , '1' , L.LR_ONH_DT )   
       AND  DECODE( H.ONH_LOC_CD , ''  , '1' , H.ONH_LOC_CD) = DECODE( L.LR_ONH_LOC_CD  , '' , '1' , L.LR_ONH_LOC_CD )   
       AND  DECODE( H.OFFH_DT     , '' , '1' , H.OFFH_DT)    = DECODE( L.LR_OFFH_DT     , '' , '1' , L.LR_OFFH_DT )   
       AND  DECODE( H.OFFH_LOC_CD , '' , '1' , H.OFFH_LOC_CD)= DECODE( L.LR_OFFH_LOC_CD , '' , '1' , L.LR_OFFH_LOC_CD ) 
#end   
       then 'C'
      WHEN H.AGMT_CTY_CD   = L.AGMT_CTY_CD
       AND  H.AGMT_SEQ     = L.AGMT_SEQ
       AND  H.LSTM_CD      = L.LSTM_CD
       AND  H.CNTR_NO      = L.CNTR_NO
       AND  H.CNTR_TPSZ_CD = L.CNTR_TPSZ_CD
       AND  H.REF_NO       = L.REF_NO
#if (${audit_tp} == 'N' ) 
       AND( H.ONH_DT  <> L.LR_ONH_DT  OR H.ONH_LOC_CD <> L.LR_ONH_LOC_CD )
#elseif (${audit_tp} == 'F' ) 
       AND( H.OFFH_DT <> L.LR_OFFH_DT OR H.OFFH_LOC_CD <> L.LR_OFFH_LOC_CD )
#elseif (${audit_tp} == 'A' ) 
       AND(( H.ONH_DT  <> L.LR_ONH_DT  OR H.ONH_LOC_CD <> L.LR_ONH_LOC_CD ) OR( H.OFFH_DT <> L.LR_OFFH_DT OR H.OFFH_LOC_CD <> L.LR_OFFH_LOC_CD ))
#end
       then 'D'
      WHEN  L.CNTR_NO IS NULL  then 'H' else 'D' end  LSE_AUD_TP_CD ,
  H.REF_NO REF_NO ,
  L.ONF_HIR_STS_CD , 
  L.AUDIT_TYPE , 
  @[vndr_seq] VNDR_SEQ
FROM (SELECT
      A.AGMT_CTY_CD,
      A.AGMT_SEQ,
      A.LSTM_CD,
      A.CNTR_NO,
      A.CNTR_TPSZ_CD,
      A.ONH_DT,
      A.ONH_LOC_CD,
      A.OFFH_DT,
      A.OFFH_LOC_CD,
      A.LR_ONH_DT,
      A.LR_ONH_LOC_CD,
      A.LR_OFFH_DT,
      A.LR_OFFH_LOC_CD,
      A.ONF_HIR_STS_CD,
      DECODE(A.ONF_HIR_STS_CD, 'A', 'ALL', 'F', 'OFF-HIRE', 'N', 'ON-HIRE') AUDIT_TYPE ,
      A.LSE_AUD_TP_CD ,
      A.REF_NO ,
      A.VNDR_SEQ
    FROM LSE_ONF_HIR_AUD  A 
    WHERE A.VNDR_SEQ       = @[vndr_seq]
      AND A.AUD_VER_SEQ    = @[aud_ver_seq]
      AND (A.LSE_AUD_TP_CD in('C' , 'D' , 'L' ) OR A.LSE_AUD_TP_CD IS NULL )) L ,
#if (${audit_tp} == 'F' )        
       --=COM-DATA====================================OFF-HIRE=START
       ( SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C 
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD') -- 1.OFF-Hire 일때 조건
                 AND     B.LSE_CTRT_NO = @[ref_no]
                 ) D
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ     = @[vndr_seq]      
        AND     A.CNTR_NO      = D.CNTR_NO
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ ) H    
#elseif (${audit_tp} == 'N' ) 
       --=COM-DATA====================================ON-HIRE=START
       (SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C 
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND B.LSE_CTRT_NO     = @[ref_no]
                 ) D 
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ     = @[vndr_seq]
        AND     A.CNTR_NO      = D.CNTR_NO(+) 
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ(+) 
        AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD')) H
#elseif (${audit_tp} == 'A' ) 
       --=COM-DATA====================================ALL=START
       ( SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD') -- 1.OFF-Hire 일때 조건
                 AND     B.LSE_CTRT_NO = @[ref_no]
                 ) D
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ     = @[vndr_seq]      
        AND     A.CNTR_NO      = D.CNTR_NO  -- 1.OFF-Hire 일때 조건
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ  -- 1.OFF-Hire 일때 조건
      UNION 
      SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND B.LSE_CTRT_NO     = @[ref_no]
                 ) D
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ    = @[vndr_seq]
        AND     A.CNTR_NO = D.CNTR_NO(+)
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ(+)
        AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD')) H
 #end  
WHERE H.AGMT_CTY_CD  = L.AGMT_CTY_CD(+)
  AND H.AGMT_SEQ     = L.AGMT_SEQ(+)
  AND H.LSTM_CD      = L.LSTM_CD(+)
  AND H.CNTR_NO      = L.CNTR_NO(+)
  AND H.CNTR_TPSZ_CD = L.CNTR_TPSZ_CD(+)
  AND H.REF_NO       = L.REF_NO(+)
UNION ALL
SELECT
  L.AGMT_CTY_CD|| LTRIM(TO_CHAR(L.AGMT_SEQ,'000000')) AGMT_NO ,
  L.AGMT_CTY_CD                   AGMT_CTY_CD,
  L.AGMT_SEQ                      AGMT_SEQ,
  L.LSTM_CD                       LSTM_CD ,
  L.CNTR_NO                       CNTR_NO ,
  L.CNTR_TPSZ_CD                  CNTR_TPSZ_CD ,
  TO_CHAR(H.ONH_DT , 'YYYYMMDD')  ONH_DT ,
  H.ONH_LOC_CD                    ONH_LOC_CD ,
  TO_CHAR(H.OFFH_DT ,'YYYYMMDD')  OFFH_DT ,
  H.OFFH_LOC_CD                   OFFH_LOC_CD ,
  TO_CHAR(L.LR_ONH_DT, 'YYYYMMDD')   LR_ONH_DT ,
  L.LR_ONH_LOC_CD                    LR_ONH_LOC_CD ,
  TO_CHAR(L.LR_OFFH_DT,'YYYYMMDD')   LR_OFFH_DT ,
  L.LR_OFFH_LOC_CD                   LR_OFFH_LOC_CD ,
  'L' LSE_AUD_TP_CD ,
  L.REF_NO REF_NO ,
  L.ONF_HIR_STS_CD , 
  L.AUDIT_TYPE , 
  @[vndr_seq] VNDR_SEQ
FROM (SELECT
      A.AGMT_CTY_CD,
      A.AGMT_SEQ,
      A.LSTM_CD,
      A.CNTR_NO,
      A.CNTR_TPSZ_CD,
      A.ONH_DT,
      A.ONH_LOC_CD,
      A.OFFH_DT,
      A.OFFH_LOC_CD,
      A.LR_ONH_DT,
      A.LR_ONH_LOC_CD,
      A.LR_OFFH_DT,
      A.LR_OFFH_LOC_CD,
      A.ONF_HIR_STS_CD,
      DECODE(A.ONF_HIR_STS_CD, 'A', 'ALL', 'F', 'OFF-HIRE', 'N', 'ON-HIRE') AUDIT_TYPE ,
      A.LSE_AUD_TP_CD ,
      A.REF_NO ,
      A.VNDR_SEQ
    FROM LSE_ONF_HIR_AUD  A 
    WHERE A.VNDR_SEQ       = @[vndr_seq]
      AND A.AUD_VER_SEQ    = @[aud_ver_seq]
      AND (A.LSE_AUD_TP_CD in('C' , 'D' , 'L' ) OR A.LSE_AUD_TP_CD IS NULL )) L ,
#if (${audit_tp} == 'F' )        
       --=COM-DATA====================================OFF-HIRE=START
       ( SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C 
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD') -- 1.OFF-Hire 일때 조건
                 AND     B.LSE_CTRT_NO = @[ref_no]
                 ) D
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ     = @[vndr_seq]      
        AND     A.CNTR_NO      = D.CNTR_NO
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ ) H    
#elseif (${audit_tp} == 'N' ) 
       --=COM-DATA====================================ON-HIRE=START
       (SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C 
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND B.LSE_CTRT_NO     = @[ref_no]
                 ) D 
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ     = @[vndr_seq]
        AND     A.CNTR_NO      = D.CNTR_NO(+) 
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ(+) 
        AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD')) H
#elseif (${audit_tp} == 'A' ) 
       --=COM-DATA====================================ALL=START
       ( SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD') -- 1.OFF-Hire 일때 조건
                 AND     B.LSE_CTRT_NO = @[ref_no]
                 ) D
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ     = @[vndr_seq]      
        AND     A.CNTR_NO      = D.CNTR_NO  -- 1.OFF-Hire 일때 조건
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ  -- 1.OFF-Hire 일때 조건
      UNION 
      SELECT
                A.AGMT_CTY_CD,
                A.AGMT_SEQ,
                B.LSTM_CD,
                A.CNTR_NO,
                C.CNTR_TPSZ_CD,
                DECODE(A.CNTR_STS_CD, 'LSI', A.CNTR_STS_EVNT_DT, 'DII', A.CNTR_STS_EVNT_DT, NULL) ONH_DT,
                DECODE(A.CNTR_STS_CD, 'LSI', A.SCC_CD, 'DII', A.SCC_CD, NULL) ONH_LOC_CD,
                D.OFFH_DT,
                D.OFFH_LOC_CD,
                NULL LR_ONH_DT,
                NULL LR_ONH_LOC_CD,
                NULL LR_OFFH_DT,
                NULL LR_OFFH_LOC_CD,
                NULL ONF_HIR_STS_CD ,
                NULL AUDIT_TYPE ,
                NULL LSE_AUD_TP_CD ,
                B.LSE_CTRT_NO REF_NO , 
                NULL VNDR_SEQ
        FROM    MST_CNTR_STS_HIS A,
                LSE_AGREEMENT B,
                (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER A ) C,
                (SELECT A.CNTR_NO, A.PRNR_STS_SEQ, A.CNTR_STS_CD,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.CNTR_STS_EVNT_DT, 'DIO', A.CNTR_STS_EVNT_DT,'SRO', A.CNTR_STS_EVNT_DT, 'TLL', A.CNTR_STS_EVNT_DT, NULL) OFFH_DT,
                 DECODE(A.CNTR_STS_CD, 'LSO', A.SCC_CD, 'DIO', A.SCC_CD,'SRO', A.SCC_CD, 'TLL', A.SCC_CD, NULL) OFFH_LOC_CD
                 FROM MST_CNTR_STS_HIS A,
                      LSE_AGREEMENT B,
                      (SELECT CNTR_NO , CNTR_TPSZ_CD FROM MST_CONTAINER ) C
                 WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
                 AND     A.AGMT_SEQ    = B.AGMT_SEQ
                 AND     A.CNTR_NO     = C.CNTR_NO
                 AND     A.CNTR_STS_CD IN ('LSO','DIO','SRO','TLL')
                 AND     B.VNDR_SEQ    = @[vndr_seq]
                 AND B.LSE_CTRT_NO     = @[ref_no]
                 ) D
        WHERE   A.AGMT_CTY_CD = B.AGMT_CTY_CD
        AND     A.AGMT_SEQ    = B.AGMT_SEQ
   #if (${agmt_seq} != '' )
        AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
        AND     A.AGMT_SEQ    = @[agmt_seq]
   #end
        AND     A.CNTR_NO = C.CNTR_NO
        AND     A.CNTR_STS_CD IN ('LSI','DII')
        AND     B.VNDR_SEQ     = @[vndr_seq]
        AND     A.CNTR_NO      = D.CNTR_NO(+)
        AND     A.CNTR_STS_SEQ = D.PRNR_STS_SEQ(+)
        AND     A.CNTR_STS_EVNT_DT BETWEEN TO_DATE(@[search_stdt], 'RRRRMMDD') AND TO_DATE(@[search_enddt], 'RRRRMMDD')) H
 #end  
WHERE L.AGMT_CTY_CD  = H.AGMT_CTY_CD(+)
  AND L.AGMT_SEQ     = H.AGMT_SEQ(+)
  AND L.LSTM_CD      = H.LSTM_CD(+)
  AND L.CNTR_NO      = H.CNTR_NO(+)
  AND L.CNTR_TPSZ_CD = H.CNTR_TPSZ_CD(+)
  AND L.REF_NO       = H.REF_NO(+)
  AND H.CNTR_NO IS NULL			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="aud_ver_seq" type="12" value="" out="N"/>
				<param name="search_stdt" type="12" value="" out="N"/>
				<param name="search_enddt" type="12" value="" out="N"/>
				<param name="ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
