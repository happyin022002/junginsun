<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOmodifyEmptyCostShipmentDataUSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
UPDATE MNR_ORD_DTL
   SET BKG_NO = @[bkg_no],
       TRD_CD = @[trd_cd],
       VSL_CD = @[vsl_cd],
       SKD_VOY_NO = @[skd_voy_no],
       SKD_DIR_CD = @[skd_dir_cd],
       REV_DIR_CD = @[rev_dir_cd],
       SLAN_CD = @[slan_cd],
       UPD_DT = SYSDATE,
       UPD_USR_ID = @[upd_usr_id]
 WHERE 1=1
   AND MNR_ORD_OFC_CTY_CD = @[mnr_ord_ofc_cty_cd]
   AND MNR_ORD_SEQ = @[mnr_ord_seq]
   AND ORD_DTL_SEQ = @[ord_dtl_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mnr_ord_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="mnr_ord_seq" type="12" value="" out="N"/>
				<param name="ord_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
