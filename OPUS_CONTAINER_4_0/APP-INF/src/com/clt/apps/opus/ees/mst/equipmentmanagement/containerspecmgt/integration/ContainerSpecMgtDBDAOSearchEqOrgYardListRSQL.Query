<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOSearchEqOrgYardListRSQL">
			<desc><![CDATA[SearchEqOrgYardList]]></desc>
			<sql><![CDATA[
SELECT /*+ index_asc(b XPKMDM_YARD) */
    b.YD_CD
FROM mdm_location a, mdm_yard b
WHERE 
    a.loc_cd = b.loc_cd
    AND a.scc_cd = @[scc_cd]
    AND b.delt_flg = 'N'			]]></sql>
			<params>
				<param name="scc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
