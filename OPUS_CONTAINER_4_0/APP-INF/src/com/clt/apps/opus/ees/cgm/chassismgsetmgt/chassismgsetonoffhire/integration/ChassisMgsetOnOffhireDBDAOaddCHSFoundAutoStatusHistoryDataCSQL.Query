<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOaddCHSFoundAutoStatusHistoryDataCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO CGM_EQ_STS_HIS
            ( EQ_NO	   
            ,EQ_STS_SEQ	 
            ,EQ_KND_CD 
            ,AGMT_OFC_CTY_CD 
            ,AGMT_SEQ	 
            ,AGMT_VER_NO	 
            ,EQ_ASET_STS_CD	 	
            ,STS_EVNT_YD_CD	 	
            ,STS_EVNT_LOC_CD 
            ,STS_EVNT_DT
            ,STS_EVNT_OFC_CD
            ,CRE_USR_ID	 
            ,CRE_DT	 	 
            ,UPD_USR_ID	 
            ,UPD_DT	
            )
         select 
              @[eq_no]
              ,(select max(EQ_STS_SEQ)+1 from CGM_EQ_STS_HIS)
              ,EQ_KND_CD 
              ,AGMT_OFC_CTY_CD 
              ,AGMT_SEQ	 
              ,AGMT_VER_NO
               ,'FND'	 	
             ,@[sts_evnt_yd_cd] 	
             ,SUBSTR(@[sts_evnt_yd_cd],0,5)
              ,TO_DATE(@[mvmts_dt] ,'YYYY-MM-DD HH24:MI:SS')
              ,@[sts_evnt_ofc_cd]
             ,@[cre_usr_id]  
             , SYSDATE
             ,@[cre_usr_id]   
             , SYSDATE
             from CGM_EQUIPMENT 
             where eq_no= @[eq_no]			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="sts_evnt_yd_cd" type="12" value="" out="N"/>
				<param name="mvmts_dt" type="12" value="" out="N"/>
				<param name="sts_evnt_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
