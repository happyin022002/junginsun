<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAODeleteInlandSoDataDSQL">
			<desc><![CDATA[Inland EQR_REPO_EXE_SO_IF       삭제   ( 해당 REF_ID )]]></desc>
			<sql><![CDATA[
DELETE FROM 
    EQR_REPO_EXE_SO_IF    							
WHERE 
    REPO_PLN_ID	= @[repo_pln_id]                               		
    AND   PLN_YRWK   	= @[pln_yrwk]     
	AND   PLN_SEQ		= @[pln_seq]                           		
    AND   REF_ID        = @[ref_id]                                					]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="pln_seq" type="12" value="" out="N"/>
				<param name="ref_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
