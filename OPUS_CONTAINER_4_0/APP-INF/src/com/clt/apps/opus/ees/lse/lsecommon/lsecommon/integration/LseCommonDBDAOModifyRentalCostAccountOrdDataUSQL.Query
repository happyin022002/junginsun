<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LseCommonDBDAOModifyRentalCostAccountOrdDataUSQL">
			<desc><![CDATA[modify]]></desc>
			<sql><![CDATA[
UPDATE LSE_RNTL_COST_ACCT_ORD
   SET ACCT_CD    = @[acct_cd],
       COST_CD    = @[cost_cd],
   	   LGS_COST_FULL_NM = @[lgs_cost_full_nm],
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT     = SYSDATE
 WHERE LSE_RCV_CHG_TP_CD = @[lse_rcv_chg_tp_cd]
   AND LSTM_CD = @[lstm_cd]			]]></sql>
			<params>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_full_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="lse_rcv_chg_tp_cd" type="12" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
