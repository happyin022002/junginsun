<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LseCommonDBDAOSearchCdNmListRSQL">
			<desc><![CDATA[EDM 코드를 이용하여 코드와 이름을 가져온다]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_ID EDM_CD, INTG_CD_VAL_CTNT CD, INTG_CD_VAL_DP_DESC NM 
  FROM COM_INTG_CD_DTL 
 WHERE INTG_CD_ID=@[edm_cd]
 ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="edm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
