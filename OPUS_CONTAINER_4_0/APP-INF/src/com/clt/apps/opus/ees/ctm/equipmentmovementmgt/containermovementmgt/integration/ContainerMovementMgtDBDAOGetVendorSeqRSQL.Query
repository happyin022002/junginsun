<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetVendorSeqRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ
  FROM MDM_VENDOR
 WHERE USA_EDI_CD = SUBSTR (@[usa_edi_cd], 1, 4)
   AND VNDR_CNT_CD = 'US'
   AND DELT_FLG <> 'Y'			]]></sql>
			<params>
				<param name="usa_edi_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
