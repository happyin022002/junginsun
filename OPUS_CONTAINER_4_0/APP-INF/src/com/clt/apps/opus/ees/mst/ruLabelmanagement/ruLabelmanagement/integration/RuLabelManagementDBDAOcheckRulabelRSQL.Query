<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RuLabelManagementDBDAOcheckRulabelRSQL">
			<desc><![CDATA[RU Label Check :
RU Label Attachment Detachment 화면에서 Cntr별 RU LAbel 등록시에 Container No와 RU Label Type에 대한 
Duplication Error를 처리하기 위해  Cntr의 RU 존재여부를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT 'X' AS FLAG
   FROM MST_CONTAINER A 
  WHERE 1=1
    AND A.CNTR_NO = @[cntr_no]
    AND NVL(@[ru_label_type], 'X') = NVL(@[ru_label_type], 'Y')      
#if (${ru_label_type} == 'FLOW')
    AND A.RSTR_USG_TP_LBL_NM1 IS NOT NULL
#elseif (${ru_label_type} == 'OWFU')
    AND A.RSTR_USG_TP_LBL_NM2 IS NOT NULL
#elseif (${ru_label_type} == 'OFHR')
    AND A.RSTR_USG_TP_LBL_NM3 IS NOT NULL
#elseif (${ru_label_type} == 'DOME')
    AND A.RSTR_USG_TP_LBL_NM4 IS NOT NULL
#elseif (${ru_label_type} == 'SALE')
    AND A.RSTR_USG_TP_LBL_NM5 IS NOT NULL
#elseif (${ru_label_type} == 'GOHH')
    AND A.RSTR_USG_TP_LBL_NM6 IS NOT NULL
#elseif (${ru_label_type} == 'REFR')
    AND A.RSTR_USG_TP_LBL_NM7 IS NOT NULL
#elseif (${ru_label_type} == 'ASST')
    AND A.RSTR_USG_TP_LBL_NM8 IS NOT NULL
#elseif (${ru_label_type} == 'OTR1')
    AND A.RSTR_USG_TP_LBL_NM9 IS NOT NULL
#elseif (${ru_label_type} == 'OTR2')
    AND A.RSTR_USG_TP_LBL_NM10 IS NOT NULL
#elseif (${ru_label_type} == 'OTR3')
    AND A.RSTR_USG_TP_LBL_NM11 IS NOT NULL
#end			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="ru_label_type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
