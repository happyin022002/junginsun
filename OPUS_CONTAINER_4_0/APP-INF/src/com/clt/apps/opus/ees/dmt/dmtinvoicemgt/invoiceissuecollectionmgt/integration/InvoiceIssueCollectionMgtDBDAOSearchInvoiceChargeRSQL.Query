<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchInvoiceChargeRSQL">
			<desc><![CDATA[Invoice Cancel Reason Entry]]></desc>
			<sql><![CDATA[
SELECT SYS_AREA_GRP_ID AS SVR_ID
	,CNTR_NO
	,CNTR_CYC_NO
	,DMDT_TRF_CD
	,DMDT_CHG_LOC_DIV_CD
	,CHG_SEQ
	,DMDT_INV_NO
	,CRE_OFC_CD
	,NVL(FX_FT_OVR_DYS, 0) AS FX_FT_OVR_DYS
FROM DMT_INV_DTL
WHERE DMDT_INV_NO 	= @[dmdt_inv_no]
AND CRE_OFC_CD 		= @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
