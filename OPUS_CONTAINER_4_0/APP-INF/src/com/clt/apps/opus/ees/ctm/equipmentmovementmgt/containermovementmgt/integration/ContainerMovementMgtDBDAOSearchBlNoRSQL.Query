<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchBlNoRSQL">
			<desc><![CDATA[search BL_NO]]></desc>
			<sql><![CDATA[
SELECT '0' AS FLG,
       BKG_NO
  FROM BKG_BOOKING
 WHERE 1 = 1
   AND BL_NO = @[bl_no]
UNION ALL
SELECT '1' AS FLG,
       MIN(BKG_NO)
  FROM CTM_BKG_BL_CMB
 WHERE 1 = 1
   AND BL_NO = @[bl_no]
AND BKG_NO IN (
                SELECT BKG_NO FROM CTM_BKG_CNTR
                WHERE CNTR_NO = @[cntr_number])			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_number" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
