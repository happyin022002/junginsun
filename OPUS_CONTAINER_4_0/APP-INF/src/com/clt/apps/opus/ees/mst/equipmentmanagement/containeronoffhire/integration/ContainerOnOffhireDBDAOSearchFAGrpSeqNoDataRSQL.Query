<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOSearchFAGrpSeqNoDataRSQL">
			<desc><![CDATA[SearchFAGrpSeqNoData]]></desc>
			<sql><![CDATA[
SELECT
    SUBSTR(@[fa_if_grp_seq], 1, 2)||'-'||TO_CHAR(SYSDATE, 'YYYYMMDD')|| '-' ||
    DECODE((SELECT LPAD(MAX(TO_NUMBER(SUBSTR(FA_IF_GRP_SEQ_NO, 13, 4)+1)),4,'0')
            FROM MST_CONTAINER
            WHERE FA_IF_GRP_SEQ_NO LIKE 
            SUBSTR(@[fa_if_grp_seq], 1, 2)||'-'||TO_CHAR(SYSDATE, 'YYYYMMDD')||'%'
           ), NULL,'0001', 
           (SELECT LPAD(MAX(TO_NUMBER(SUBSTR(FA_IF_GRP_SEQ_NO, 13, 4)+1)),4,'0')
            FROM MST_CONTAINER
            WHERE FA_IF_GRP_SEQ_NO LIKE 
            SUBSTR(@[fa_if_grp_seq], 1, 2)||'-'||TO_CHAR(SYSDATE, 'YYYYMMDD')||'%'
           )) GRP_SEQ_NO
FROM DUAL			]]></sql>
			<params>
				<param name="fa_if_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
