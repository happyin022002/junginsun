AND   VSL_CD       = @[vsl_cd]
AND   SKD_VOY_NO   = @[skd_voy_no]
AND   SKD_DIR_CD   = @[skd_dir_cd]
AND   BKG_EXE_SEQ  = @[bkg_exe_seq]	
AND   CNTR_TPSZ_CD = @[cntr_tpsz_cd]			]]></sql>
