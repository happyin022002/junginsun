<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOAddManageOSCARCtmCycNoListCSQL">
			<desc><![CDATA[Insert]]></desc>
			<sql><![CDATA[
INSERT INTO CTM_BKG_CNTR
            (BKG_NO,
            CNTR_NO,
            CNTR_TPSZ_CD,
            CNMV_YR,
            CNMV_ID_NO,
            CNMV_CYC_NO,
            CNMV_STS_CD,
            RCV_TERM_CD,
            DE_TERM_CD,
            CNTR_VOL_QTY,
            DCGO_FLG,
            RC_FLG,
            BB_CGO_FLG,
            AWK_CGO_FLG,
            RD_CGO_FLG,
            HNGR_FLG,
            SOC_FLG,
            CNMV_EVNT_DT,
            CNTR_CFM_FLG,
            CRE_USR_ID,
            CRE_DT,
            UPD_USR_ID,
            UPD_DT,
            EDW_UPD_DT,
            OSCA_CRE_DT,
            OSCA_UPD_DT,
            CYC_UPD_USR_ID,
            CYC_UPD_DT,
            MNL_INP_TP_CD
            )
    VALUES (@[bkg_no],
            @[cntr_no],
            (SELECT CNTR_TPSZ_CD
            FROM MST_CONTAINER
            WHERE CNTR_NO=@[cntr_no]),
            NVL(@[cnmv_yr], ''),
            NVL(@[cnmv_id_no], ''),
            NVL(@[cnmv_cyc_no], ''),
            NVL(@[cnmv_sts_cd], ''),
            (SELECT RCV_TERM_CD
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            (SELECT DE_TERM_CD
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            NVL(@[cntr_vol_qty], 1),
            (SELECT DCGO_FLG
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            (SELECT RC_FLG
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            (SELECT BB_CGO_FLG
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            (SELECT AWK_CGO_FLG
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            (SELECT RD_CGO_FLG
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            (SELECT HNGR_FLG
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            (SELECT SOC_FLG
            FROM CTM_BOOKING
            WHERE BKG_NO=@[bkg_no]),
            NVL(@[cnmv_evnt_dt], ''),
            NVL(@[cntr_cfm_flg], 'Y'),
            @[delt_usr_id],
            SYSDATE,
            @[delt_usr_id],
            SYSDATE,
            NVL(@[edw_upd_dt], ''),
            NVL(@[osca_cre_dt], ''),
            NVL(@[osca_upd_dt], ''),
            '',
            '',
            'M'
            )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
				<param name="cnmv_cyc_no" type="12" value="" out="N"/>
				<param name="cnmv_sts_cd" type="12" value="" out="N"/>
				<param name="cntr_vol_qty" type="12" value="" out="N"/>
				<param name="cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="cntr_cfm_flg" type="12" value="" out="N"/>
				<param name="delt_usr_id" type="12" value="" out="N"/>
				<param name="edw_upd_dt" type="12" value="" out="N"/>
				<param name="osca_cre_dt" type="12" value="" out="N"/>
				<param name="osca_upd_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
