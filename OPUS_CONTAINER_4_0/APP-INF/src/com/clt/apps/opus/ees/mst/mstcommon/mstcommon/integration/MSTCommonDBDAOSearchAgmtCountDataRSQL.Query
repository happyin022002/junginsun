<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MSTCommonDBDAOSearchAgmtCountDataRSQL">
			<desc><![CDATA[SearchAgmtCountData]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM   LSE_AGREEMENT A
     , MDM_VENDOR C
WHERE  A.VNDR_SEQ = C.VNDR_SEQ
#if (${agmt_cty_cd} != "") 
   AND A.AGMT_CTY_CD = @[agmt_cty_cd]
#end
#if (${agmt_seq} != "") 
   AND A.AGMT_SEQ = @[agmt_seq]
#end
#if (${vndr_seq} != "") 
   AND A.VNDR_SEQ = @[vndr_seq]
#end
#if (${lstm_cd} != "") 
   AND A.LSTM_CD = @[lstm_cd]
#end
#if (${ofc_cd} != "") 
   AND A.OFC_CD = @[ofc_cd]
#end
#if (${ref_no} != "") 
   AND A.REF_NO LIKE '%'||@[ref_no]||'%' 
#end
#if (${vndr_lgl_eng_nm} != "") 
   AND C.VNDR_LGL_ENG_NM LIKE '%'||@[vndr_lgl_eng_nm]||'%'
#end			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="ref_no" type="12" value="" out="N"/>
				<param name="vndr_lgl_eng_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
