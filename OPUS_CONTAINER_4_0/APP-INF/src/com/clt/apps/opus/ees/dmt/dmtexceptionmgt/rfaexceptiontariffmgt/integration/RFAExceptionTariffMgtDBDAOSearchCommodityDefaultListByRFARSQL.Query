<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchCommodityDefaultListByRFARSQL">
			<desc><![CDATA[Default Commodity Search]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.PRC_CMDT_DEF_CD CMDT_CD
     , B.CMDT_NM
  FROM (SELECT PRC_CMDT_DEF_CD
          FROM PRI_RP_SCP_RT_CMDT
         WHERE PROP_NO = @[prop_no]
           AND PRC_CMDT_TP_CD = 'C'
         UNION ALL
        SELECT PRC_CMDT_DEF_CD
          FROM PRI_RP_SCP_GRP_CMDT_DTL
         WHERE PROP_NO = @[prop_no]
           AND PRC_CMDT_TP_CD = 'C' ) A
     , MDM_COMMODITY B
 WHERE A.PRC_CMDT_DEF_CD = B.CMDT_CD
 ORDER BY CMDT_CD			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
