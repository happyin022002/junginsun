<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOCheckVVDCNTRAllRSQL">
			<desc><![CDATA[CheckVVDCNTRAll]]></desc>
			<sql><![CDATA[
SELECT BKG_NO 
FROM CTM_BKG_VVD 
WHERE 1 = 1
AND VSL_CD = SUBSTR(@[p_vvdcd], 1 ,4)
AND SKD_VOY_NO = SUBSTR(@[p_vvdcd], 5 ,4)
AND SKD_DIR_CD = SUBSTR(@[p_vvdcd], 9 ,1)
AND ROWNUM = 1

union all

SELECT BKG_NO 
FROM BKG_VVD 
WHERE 1 = 1
AND VSL_CD = SUBSTR(@[p_vvdcd], 1 ,4)
AND SKD_VOY_NO = SUBSTR(@[p_vvdcd], 5 ,4)
AND SKD_DIR_CD = SUBSTR(@[p_vvdcd], 9 ,1)
AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="p_vvdcd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
