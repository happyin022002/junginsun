<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyVerifyEstimateFileListByRepairDataUSQL">
			<desc><![CDATA[modifyVerifyEstimateFileListByRepairData]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
       SET A.INP_MSG4 = 'RE'
 WHERE A.TMP_SEQ = @[tmp_seq]
   AND A.INP_MSG4 = 'SS'
   AND NVL(A.INP_MSG5,'') NOT IN (SELECT EQ_CEDEX_OTR_CD
 									FROM MNR_CEDEX_OTR_CD
      					   			WHERE EQ_CEDEX_OTR_TP_CD = 'RPR')			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
