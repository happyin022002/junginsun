<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyInvoiceDetailAmountByPartialPaymentUSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
UPDATE  DMT_INV_DTL
   SET  CNTR_INV_AMT	= @[cntr_inv_amt]
 WHERE  DMDT_INV_NO		= @[dmdt_inv_no]
 AND    INV_DTL_SEQ		= @[inv_dtl_seq]			]]></sql>
			<params>
				<param name="cntr_inv_amt" type="12" value="" out="N"/>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="inv_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
