<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOLessorInvoiceFileImportNewPayImpSeqRSQL">
			<desc><![CDATA[Seacrh max value of Pay Imp Seq.]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(PAY_IMP_SEQ), 0) + 1 AS MAX_PAY_IMP_SEQ
FROM   LSE_PAY_RNTL_CHG_CO
WHERE  CO_COST_YRMON = @[co_cost_yrmon]			]]></sql>
			<params>
				<param name="co_cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
