<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TimeClockStopMgtDBDAODmtTimeClockStopSeqCSQL">
			<desc><![CDATA[INSERT DmtTimeClockStopSeq]]></desc>
			<sql><![CDATA[
INSERT INTO dmt_tm_clk_stop
            (clk_stop_no,
             dmdt_trf_cd,
             clk_stop_ofc_cd,
             clk_stop_fm_dt,
             clk_stop_to_dt,
             clk_stop_rmk,
             cxl_flg,
             cre_usr_id,
             cre_dt,
             cre_ofc_cd,
             upd_usr_id,
             upd_dt,
             upd_ofc_cd,
             all_yd_flg
            )
     VALUES (@[clk_stop_ofc_cd]
             || TO_CHAR (SYSDATE, 'rrmm')
             || LTRIM (TO_CHAR (@[seq], '0000')),
             @[dmdt_trf_cd],
             @[clk_stop_ofc_cd],
             TO_DATE (@[clk_stop_fm_dt], 'rrrrmmdd')
             + .00001,
             TO_DATE (@[clk_stop_to_dt], 'rrrrmmdd')
             + .99999,
             @[clk_stop_rmk],
             'N',
             @[cre_usr_id],
             SYSDATE,
             @[cre_ofc_cd],
             @[upd_usr_id],
             SYSDATE,
             @[upd_ofc_cd],
             @[all_yd_flg]
            )			]]></sql>
			<params>
				<param name="clk_stop_ofc_cd" type="12" value="" out="N"/>
				<param name="seq" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="clk_stop_fm_dt" type="12" value="" out="N"/>
				<param name="clk_stop_to_dt" type="12" value="" out="N"/>
				<param name="clk_stop_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="all_yd_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
