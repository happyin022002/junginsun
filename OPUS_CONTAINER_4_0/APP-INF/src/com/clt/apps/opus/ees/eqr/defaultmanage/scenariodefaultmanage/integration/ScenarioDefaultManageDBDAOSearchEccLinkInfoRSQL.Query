<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOSearchEccLinkInfoRSQL">
			<desc><![CDATA[특정 ecc 의 EQR_ECC_LNK  테이블의 정보 조회
2010.12.23 이준범 [CHM-201007870-01]
1. Default >Inquiry>Link Info 화면
> 조회시 Detail data에 대한 생성일자를 화면에 표시]]></desc>
			<sql><![CDATA[
SELECT 
    FM_ECC_CD
    ,TO_ECC_CD
    ,TRSP_MOD_CD
    ,TZ_DYS
    ,TZ_20FT_COST_AMT
    ,TZ_40FT_COST_AMT
    ,TZ_45FT_COST_AMT
    ,CNTR_MAX_CAPA_QTY
    ,CNTR_VOL_UT_CD     -- BOX1
    ,TRSP_FREQ_KNT
    ,EXPT_FM_YRWK
    ,EXPT_TO_YRWK
    ,EXPT_MAX_CAPA_QTY
    ,EXPT_VOL_UT_CD     -- BOX2
	,CRE_DT
FROM 
    EQR_ECC_LNK
	## WHERE 조건 변경 : DELT_FLG = 'N' ,즉, DELETE 되지 않은 항목만 조회 가능.
WHERE 
    NVL(DELT_FLG,'N') = 'N'		        
	AND 1=1                                   
		
	## where condition 
	#if (${fromStatus} != '') 
		AND FM_ECC_CD IN (
		#foreach($key IN ${fromEccArr}) 
			#if($velocityCount < $fromEccArr.size()) 
			'$key', 
			#else 
			'$key' 
			#end 
		#end 
		)
	#end
	
	#if (${toStatus} != '') 
		AND TO_ECC_CD IN (
		#foreach( $key in ${toEccArr}) 
			#if($velocityCount < $toEccArr.size()) 
				'$key', 
			#else 
				'$key' 
			#end 
		#end
		)
	#end

	#if (${mode} != '') 
		AND TRSP_MOD_CD = @[mode]
	#end

	#if (${transitTime} != '') 
		AND TZ_DYS >= @[transitTime]
	#end
	
ORDER BY 
    1
    ,2
    ,3			]]></sql>
			<params>
				<param name="mode" type="12" value="" out="N"/>
				<param name="transitTime" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
