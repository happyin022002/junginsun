<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOAddTermChangeCntrStatusHistoryDiFlgNSBODataCSQL">
			<desc><![CDATA[AddTermChangeCntrStatusHistoryDiFlgNSBOData]]></desc>
			<sql><![CDATA[
INSERT INTO MST_CNTR_STS_HIS
(
     CNTR_NO
    ,CNTR_STS_SEQ
    ,CO_CD
    ,YD_CD
    ,LOC_CD
    ,SCC_CD
    ,LCC_CD
    ,ECC_CD
    ,RCC_CD
    ,AGMT_CTY_CD
    ,AGMT_SEQ
    ,CNTR_STS_CD
    ,OFC_CD
    ,CNTR_STS_EVNT_DT
    ,CNTR_PKUP_CHG_AMT
    ,CURR_CD
    ,RNTL_CHG_FREE_DYS
    ,CNTR_DIR_ITCHG_FEE_AMT
    ,DIR_ITCHG_VNDR_SEQ   
    ,CNTR_LSTM_CNG_FLG
    ,PRNR_YD_CD
    ,PRNR_STS_SEQ
    ,CNMV_STS_CD
    ,CNTR_FULL_FLG
    ,CNTR_MIN_ONH_DYS
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT    
)
SELECT 
    @[cntr_no] AS CNTR_NO,
    B.CNTR_STS_SEQ,       
    'O' AS CO_CD,
    @[yd_cd]  AS YD_CD,
    @[loc_cd] LOC_CD,
    @[scc_cd] SCC_CD,
    @[lcc_cd] LCC_CD,
    @[ecc_cd] ECC_CD,
    @[rcc_cd] RCC_CD,    
    DECODE(B.SEQ,1,A.AGMT_CTY_CD,2,@[cur_agmt_cty_cd],3,@[aft_agmt_cty_cd],4,A.AGMT_CTY_CD ) AS AGMT_CTY_CD,
    DECODE(B.SEQ,1,A.AGMT_SEQ,2,@[cur_agmt_seq],3,@[aft_agmt_seq],4,A.AGMT_SEQ ) AS AGMT_SEQ,        
    CASE 
        WHEN 'OW' = @[aft_lstm_cd]  THEN   DECODE(B.SEQ,1,SUBSTR(@[cntr_sts_cd], 1, 2)||'I',2,'LSO',3,'OWN',4,SUBSTR(@[cntr_sts_cd], 1, 2)||'O') --SBO/SBI, MUO/MUI
        WHEN 'OW' != @[aft_lstm_cd] THEN   DECODE(B.SEQ,1,SUBSTR(@[cntr_sts_cd], 1, 2)||'I',2,'LSO',3,'LSI',4,SUBSTR(@[cntr_sts_cd], 1, 2)||'O') --SBO/SBI, MUO/MUI
    END CNTR_STS_CD,    
    @[ofc_cd]     AS OFC_CD,
    DECODE( CASE 
            WHEN 'OW' = @[aft_lstm_cd]  THEN   DECODE(B.SEQ,1,SUBSTR(@[cntr_sts_cd], 1, 2)||'I',2,'LSO',3,'OWN',4,SUBSTR(@[cntr_sts_cd], 1, 2)||'O') --SBO/SBI, MUO/MUI
            WHEN 'OW' != @[aft_lstm_cd] THEN   DECODE(B.SEQ,1,SUBSTR(@[cntr_sts_cd], 1, 2)||'I',2,'LSO',3,'LSI',4,SUBSTR(@[cntr_sts_cd], 1, 2)||'O') --SBO/SBI, MUO/MUI
            END
            , 'LSO', TO_DATE(@[act_dt],'YYYYMMDD')-1
            , 'SBI', TO_DATE(@[act_dt],'YYYYMMDD')-1
            , 'MUI', TO_DATE(@[act_dt],'YYYYMMDD')-1 
            , TO_DATE(@[act_dt],'YYYYMMDD')
          ) AS CNTR_STS_EVNT_DT,        -- Activity Date-- 
    DECODE(B.SEQ,1,0,2,0,3,(CASE WHEN TO_NUMBER(@[cntr_pkup_psv_amt]) > 0 THEN
                                            @[cntr_pkup_psv_amt]
                                        WHEN TO_NUMBER(@[cntr_pkup_ngv_amt]) > 0 THEN
                                            TO_CHAR(TO_NUMBER(@[cntr_pkup_ngv_amt])*-1)
                                        END
                            ),4,0) CNTR_PKUP_CHG_AMT,
	DECODE(B.SEQ,1,''
                ,2,''
                ,3,''
                ,4,NVL((SELECT /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */ A.CURR_CD
                          FROM MST_CNTR_STS_HIS A
                         WHERE A.CNTR_NO     = @[cntr_no]
                           AND A.CNTR_STS_CD = SUBSTR(@[cntr_sts_cd], 1, 2)||'O'
                           AND ROWNUM        = 1), '')
          ) AS CURR_CD,
	DECODE(B.SEQ,1,0
                ,2,0
                ,3,@[rntl_chg_free_dys]
                ,4,NVL((SELECT /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */
                              DECODE(SIGN((TRUNC(A.CNTR_STS_EVNT_DT) + A.RNTL_CHG_FREE_DYS ) -  TO_DATE(@[act_dt],'YYYYMMDD'))
                                         , 1, ((TRUNC(A.CNTR_STS_EVNT_DT) + A.RNTL_CHG_FREE_DYS ) -  TO_DATE(@[act_dt],'YYYYMMDD'))
                                         , 0) 
                          FROM MST_CNTR_STS_HIS A
                         WHERE A.CNTR_NO     = @[cntr_no]
                           AND A.CNTR_STS_CD = SUBSTR(@[cntr_sts_cd], 1, 2)||'O'
                           AND ROWNUM        = 1), 0)
          ) RNTL_CHG_FREE_DYS,
    NULL CNTR_DIR_ITCHG_FEE_AMT,
    NULL DIR_ITCHG_VNDR_SEQ,
    'Y' AS CNTR_LSTM_CNG_FLG, 
    (SELECT /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */  A.YD_CD
       FROM MST_CNTR_STS_HIS A
      WHERE   A.CNTR_NO = @[cntr_no]
        AND     A.CNTR_STS_CD IN (  DECODE(B.SEQ, 1, SUBSTR(@[cntr_sts_cd], 1, 2)||'O', 'XXX') -- Search SBO/MUO Parent Sequence
                                  , DECODE(B.SEQ, 2, 'OWN', 'XXX') -- Search LSO Parent Sequence - OWN and LSI
                                  , DECODE(B.SEQ, 2, 'LSI', 'XXX') -- Search LSO Parent Sequence - OWN and LSI
                                 )
        AND     ROWNUM =1) AS PRNR_YD_CD,
    (SELECT /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */  CNTR_STS_SEQ
       FROM MST_CNTR_STS_HIS A
      WHERE   A.CNTR_NO = @[cntr_no]
        AND     A.CNTR_STS_CD IN (  DECODE(B.SEQ, 1, SUBSTR(@[cntr_sts_cd], 1, 2)||'O', 'XXX') -- Search SBO/MUO Parent Sequence
                                  , DECODE(B.SEQ, 2, 'OWN', 'XXX') -- Search LSO Parent Sequence - OWN and LSI
                                  , DECODE(B.SEQ, 2, 'LSI', 'XXX') -- Search LSO Parent Sequence - OWN and LSI
                                 )
        AND     ROWNUM =1) AS PRNR_STS_SEQ,
    @[cnmv_sts_cd] AS CNMV_STS_CD,
    @[cntr_full_flg] AS CNTR_FULL_FLG,
    DECODE(B.SEQ,1,0,2,0,3,@[cntr_min_onh_dys],4,0) CNTR_MIN_ONH_DYS,
    @[cre_usr_id]     AS CRE_USR_ID,
    SYSDATE AS CRE_DT,
    @[upd_usr_id]      AS UPD_USR_ID,
    SYSDATE AS UPD_DT
FROM 
    (SELECT /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */ 
            AGMT_CTY_CD,A.AGMT_SEQ
    FROM MST_CNTR_STS_HIS A
    WHERE   A.CNTR_NO = @[cntr_no]
    AND     A.CNTR_STS_CD = @[cntr_sts_cd]
    AND     ROWNUM =1) A ,
    (SELECT LEVEL SEQ,
            MST_CNTR_STS_HIS_SEQ_FNC('N') AS CNTR_STS_SEQ
     FROM DUAL 
     CONNECT BY LEVEL <=4) B			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="cur_agmt_cty_cd" type="12" value="" out="N"/>
				<param name="aft_agmt_cty_cd" type="12" value="" out="N"/>
				<param name="cur_agmt_seq" type="12" value="" out="N"/>
				<param name="aft_agmt_seq" type="12" value="" out="N"/>
				<param name="aft_lstm_cd" type="12" value="" out="N"/>
				<param name="cntr_sts_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="cntr_pkup_psv_amt" type="12" value="" out="N"/>
				<param name="cntr_pkup_ngv_amt" type="12" value="" out="N"/>
				<param name="rntl_chg_free_dys" type="12" value="" out="N"/>
				<param name="cnmv_sts_cd" type="12" value="" out="N"/>
				<param name="cntr_full_flg" type="12" value="" out="N"/>
				<param name="cntr_min_onh_dys" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
