<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOCheckPrevMvmtRSQL">
			<desc><![CDATA[CheckPrevMvmt]]></desc>
			<sql><![CDATA[
SELECT MVMT_STS_CD, FCNTR_FLG, MTY_PLN_NO, MTY_REPO_NO
FROM CTM_MOVEMENT
WHERE CNTR_NO = @[cntr_no]
AND ROWNUM=1
ORDER BY CNMV_YR DESC, CNMV_SEQ DESC, CNMV_SPLIT_NO DESC			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
