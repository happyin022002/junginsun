<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AvailableOffHireDBDAOAvailableOffHireContainerLocationRSQL">
			<desc><![CDATA[2016.05.15 / Jiyeon Jeon - Target Off-Hire Location 조회]]></desc>
			<sql><![CDATA[
SELECT SUB.CONTI_CD AS LOC_CD, SUB.CONTI_NM AS LOC_NM
  FROM MDM_CONTINENT SUB
 WHERE SUB.DELT_FLG  = 'N'
   AND 'CT' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.CONTI_CD LIKE '%'||@[loc_cd]||'%'
#end
 UNION ALL
SELECT SUB.SCONTI_CD AS LOC_CD, SUB.SCONTI_NM AS LOC_NM
  FROM MDM_SUBCONTINENT SUB
 WHERE SUB.DELT_FLG   = 'N'
   AND 'ST' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.SCONTI_CD LIKE '%'||@[loc_cd]||'%'
#end
 UNION ALL
SELECT SUB.CNT_CD AS LOC_CD, SUB.CNT_NM AS LOC_NM
  FROM MDM_COUNTRY SUB
 WHERE SUB.DELT_FLG   = 'N'
   AND 'CN' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.CNT_CD LIKE '%'||@[loc_cd]||'%'
#end
 UNION ALL
SELECT DISTINCT SUB.RCC_CD AS LOC_CD, SUB.RCC_CD AS LOC_NM
  FROM MDM_EQ_ORZ_CHT SUB
 WHERE SUB.DELT_FLG    = 'N' 
   AND 'RC' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.RCC_CD LIKE '%'||@[loc_cd]||'%'
#end
 UNION ALL
SELECT DISTINCT SUB.ECC_CD AS LOC_CD, SUB.ECC_CD AS LOC_NM
  FROM MDM_EQ_ORZ_CHT SUB
 WHERE SUB.DELT_FLG    = 'N'
   AND 'EC' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.ECC_CD LIKE '%'||@[loc_cd]||'%'
#end
 UNION ALL
SELECT DISTINCT SUB.SCC_CD AS LOC_CD, SUB.SCC_CD AS LOC_NM
  FROM MDM_EQ_ORZ_CHT SUB
 WHERE SUB.DELT_FLG    = 'N'
   AND 'SC' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.SCC_CD LIKE '%'||@[loc_cd]||'%'
#end
 UNION ALL
SELECT SUB.LOC_CD AS LOC_CD, SUB.LOC_CD AS LOC_NM
  FROM MDM_LOCATION SUB
 WHERE SUB.DELT_FLG   = 'N'
   AND 'LO' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.LOC_CD LIKE '%'||@[loc_cd]||'%'
#end
 UNION ALL
SELECT DISTINCT SUB.LCC_CD AS LOC_CD, SUB.LCC_CD AS LOC_NM
  FROM MDM_EQ_ORZ_CHT SUB
 WHERE SUB.DELT_FLG    = 'N'
   AND 'LC' = @[eq_loc_tp_cd]
#if (${loc_cd} != '' )
   AND SUB.LCC_CD LIKE '%'||@[loc_cd]||'%'
#end			]]></sql>
			<params>
				<param name="eq_loc_tp_cd" type="12" value="1" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
