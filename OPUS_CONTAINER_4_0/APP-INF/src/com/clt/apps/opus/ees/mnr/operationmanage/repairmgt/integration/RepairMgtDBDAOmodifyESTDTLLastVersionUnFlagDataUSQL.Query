<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOmodifyESTDTLLastVersionUnFlagDataUSQL">
			<desc><![CDATA[modifyESTDTLLastVersionUnFlagData]]></desc>
			<sql><![CDATA[
UPDATE MNR_RPR_RQST_DTL A
     SET 
         A.RPR_RQST_LST_VER_FLG = 'N'
        ,A.UPD_USR_ID = @[upd_usr_id]
        ,A.UPD_DT = sysdate
WHERE A.RQST_EQ_NO = @[rqst_eq_no]
AND   A.RPR_RQST_SEQ = @[rpr_rqst_seq]
			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
				<param name="rpr_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
