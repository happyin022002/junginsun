<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffMgtDBDAOsearchRepairTariffNoDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT P2.PRE_TRF_NO||
       DECODE(LENGTH(POST_TRF_NO),
              1,'00'||P2.POST_TRF_NO,
              2,'0'||P2.POST_TRF_NO,
              P2.POST_TRF_NO)  TRF_NO
  FROM (         
        SELECT P.PRE_TRF_NO
              ,TO_CHAR(NVL(MAX(TO_NUMBER(SUBSTR(A.TRF_NO,LENGTH(A.TRF_NO)-2))),0)+1) POST_TRF_NO
          FROM MNR_RPR_TRF_HDR A,
               ( SELECT @[pre_trf_no]||TO_CHAR(SYSDATE,'YYYYMM')||'-'  PRE_TRF_NO   
                   FROM DUAL
               ) P
         WHERE A.TRF_NO LIKE P.PRE_TRF_NO||'%'
        ) P2			]]></sql>
			<params>
				<param name="pre_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
