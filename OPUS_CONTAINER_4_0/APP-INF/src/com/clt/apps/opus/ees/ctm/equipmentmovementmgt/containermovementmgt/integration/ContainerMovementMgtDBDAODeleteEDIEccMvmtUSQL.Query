<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAODeleteEDIEccMvmtUSQL">
			<desc><![CDATA[EDI Batch 실행시 BKG_BOOKING/EPP 파일에서 'D'인경우 
BKG_STS_CD을 X로 업데이트 한다.]]></desc>
			<sql><![CDATA[
UPDATE CTM_BOOKING SET
	BKG_STS_CD		=	'X'						,
	UPD_USR_ID		=	@[upd_usr_id]			,
	UPD_DT			=	TO_DATE(SUBSTR(@[upd_dt],0,19),'YYYY-MM-DD HH24:MI:SS') 
WHERE 
	BKG_NO			=	@[bkg_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
