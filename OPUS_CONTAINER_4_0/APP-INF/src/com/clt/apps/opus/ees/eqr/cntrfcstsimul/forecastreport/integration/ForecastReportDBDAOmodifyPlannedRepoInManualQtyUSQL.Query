<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ForecastReportDBDAOmodifyPlannedRepoInManualQtyUSQL">
			<desc><![CDATA[INTO EQR_CTRL_PLN_REPO_IN_QTY 에 Planned Repo In Manual 데이터 수정/입력 (merge)]]></desc>
			<sql><![CDATA[
-- EQR_CTRL_PLN_REPO_IN_QTY 입력/수정   
   
MERGE INTO EQR_CTRL_PLN_REPO_IN_QTY
USING DUAL   
ON (     FCAST_YRWK      = @[fcast_yrwk]               
     AND VSL_CD          = @[vsl_cd]                   
     AND SKD_VOY_NO      = @[skd_voy_no]               
     AND SKD_DIR_CD      = @[skd_dir_cd]               
     AND PLN_REPO_STS_CD = @[pln_repo_sts_cd]          
     AND POD_YD_CD       = @[pod_yd_cd]                
     AND TO_CHAR(TO_ETB_DT,'YYYY-MM-DD') = @[to_etb_dt]
     AND CNTR_TPSZ_CD    = @[cntr_tpsz_cd] )
WHEN MATCHED THEN
     UPDATE SET
            CNTR_QTY = @[cntr_qty]
WHEN NOT MATCHED THEN     
     INSERT ( FCAST_YRWK      
            , VSL_CD          
            , SKD_VOY_NO      
            , SKD_DIR_CD      
            , PLN_REPO_STS_CD 
            , POD_YD_CD       
            , TO_ETB_DT
            , CNTR_TPSZ_CD 
            , CNTR_QTY 
            , CRE_USR_ID
            , CRE_DT
            , UPD_USR_ID
            , UPD_DT )
     VALUES ( @[fcast_yrwk]      
            , @[vsl_cd]          
            , @[skd_voy_no]      
            , @[skd_dir_cd]      
            , @[pln_repo_sts_cd] 
            , @[pod_yd_cd]       
            , TO_DATE(@[to_etb_dt],'YYYY-MM-DD')
            , @[cntr_tpsz_cd] 
            , @[cntr_qty] 
            , @[cre_usr_id]
            , SYSDATE
            , @[upd_usr_id]
            , SYSDATE )			]]></sql>
			<params>
				<param name="fcast_yrwk" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pln_repo_sts_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="to_etb_dt" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_qty" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
