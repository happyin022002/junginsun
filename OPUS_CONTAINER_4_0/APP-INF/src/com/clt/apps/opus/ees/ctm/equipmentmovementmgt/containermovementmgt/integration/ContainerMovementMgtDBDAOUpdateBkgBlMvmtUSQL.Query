<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOUpdateBkgBlMvmtUSQL">
			<desc><![CDATA[CTM_BKG_BL_CMB Update]]></desc>
			<sql><![CDATA[
UPDATE CTM_BKG_BL_CMB SET
	BL_NO_TP	= 	NVL(@[bl_no_tp], 'N')  	,
	BL_TP_CD	= 	NVL(@[bl_tp_cd], 'N')	,
	DAT_MNPL_CD = 	NVL(@[cud_flg] , 'N')	,
	--CRE_USR_ID	= 	'Bridge'				,
	--CRE_DT		=	SYSDATE					,	
	UPD_USR_ID	= 	'Bridge'				,
	UPD_DT		=	SYSDATE					
WHERE 1=1  
	AND BKG_NO		= @[bkg_no]
	AND BL_NO		= @[bl_no]			]]></sql>
			<params>
				<param name="bl_no_tp" type="12" value="FF" out="N"/>
				<param name="bl_tp_cd" type="12" value="SS" out="N"/>
				<param name="cud_flg" type="12" value="FE" out="N"/>
				<param name="bkg_no" type="12" value="RW" out="N"/>
				<param name="bl_no" type="12" value="WER" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
