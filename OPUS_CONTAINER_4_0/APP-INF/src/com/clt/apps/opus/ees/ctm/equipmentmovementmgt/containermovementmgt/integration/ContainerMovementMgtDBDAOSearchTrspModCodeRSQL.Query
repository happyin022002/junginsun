<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchTrspModCodeRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT TRSP_MOD_CD
  FROM PRD_PROD_CTL_ROUT_DTL
 WHERE PCTL_NO IN (SELECT PCTL_NO
                     FROM SCE_COP_HDR
                    WHERE BKG_NO = @[bkg_number]
                      AND CNTR_NO = @[cntr_no])
   AND ORG_NOD_CD = @[event_yard]
   AND NOD_LNK_DIV_CD = 'L'
   AND TRSP_MOD_CD = 'RD'			]]></sql>
			<params>
				<param name="bkg_number" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="event_yard" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
