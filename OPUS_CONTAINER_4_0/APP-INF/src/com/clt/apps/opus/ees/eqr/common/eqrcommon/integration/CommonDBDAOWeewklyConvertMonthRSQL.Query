<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOWeewklyConvertMonthRSQL">
			<desc><![CDATA[SCNR_ID 기준으로 해서 MonthGap 후의 년 월을 구한다.]]></desc>
			<sql><![CDATA[
SELECT	    														
	PLN_YR															
  	,PLN_WK															
  	,PLN_MON															
FROM  
  	(
    	SELECT 
      		(ROWNUM)SEQ, 
      		PLN_YR, 
      		PLN_WK, 
      		PLN_MON 
    	FROM 
			EQR_WK_PRD	
    	WHERE 
      		PLN_YR IN( @[yyyy], @[next_yyyy] )										
      		AND PLN_YR || PLN_WK >= @[yyyyww]										
  	)B																
WHERE 
	SEQ IN ('1',@[WeeklyGap])			]]></sql>
			<params>
				<param name="yyyy" type="12" value="" out="N"/>
				<param name="next_yyyy" type="12" value="" out="N"/>
				<param name="yyyyww" type="12" value="" out="N"/>
				<param name="WeeklyGap" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
