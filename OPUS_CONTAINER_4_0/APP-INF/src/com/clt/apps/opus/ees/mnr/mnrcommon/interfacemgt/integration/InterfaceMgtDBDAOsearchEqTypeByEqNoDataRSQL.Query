<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchEqTypeByEqNoDataRSQL">
			<desc><![CDATA[searchEqTypeByEqNoData]]></desc>
			<sql><![CDATA[
SELECT A.EQ_TYPE FROM MNR_EQ_STS_V A 
WHERE  A.EQ_NO = @[rqst_eq_no]
       AND ROWNUM = 1			]]></sql>
			<params>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
