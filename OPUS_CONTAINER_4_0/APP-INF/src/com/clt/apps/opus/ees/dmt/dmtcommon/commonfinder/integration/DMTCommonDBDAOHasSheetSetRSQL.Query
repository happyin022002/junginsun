<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOHasSheetSetRSQL">
			<desc><![CDATA[Sheet Set 이 존재하는지를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	COUNT(OFC_CD) AS CNT
FROM	DMT_SH_SET
WHERE	OFC_CD = @[ofc_cd] 
	AND DMDT_TRF_CD = @[dmdt_trf_cd]
	AND DMDT_SH_TP_CD = @[dmdt_sh_tp_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_sh_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
