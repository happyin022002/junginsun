<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetBkgNoByBlNoForGateNewRSQL">
			<desc><![CDATA[BL 번호만 넘어 왔을 때 BL번호로 Bkg 번호를 조회한다]]></desc>
			<sql><![CDATA[
SELECT BKG_NO,
       NVL (BKG_STS_CD, '') AS BKG_STS_CD
  FROM BKG_BOOKING
 WHERE BL_NO = @[bl_no]
UNION ALL
SELECT BKG_NO,
       NVL (BKG_STS_CD, '') AS BKG_STS_CD
  FROM CTM_BOOKING
 WHERE BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
