<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrPlanGuidelineManageDBDAOCheckGlineRSQL">
			<desc><![CDATA[Guideline 존재 유무 체크]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(*) > 0 THEN 'F'
            ELSE 'T'
        END AS CHK_GLINE
  FROM EQR_CTRL_GLINE_HDR
 WHERE TRD_CD = @[trade]
   AND SUB_TRD_CD = SUBSTR(@[subtrade],4,2)
   AND VSL_LANE_CD = SUBSTR(@[lane],6)			]]></sql>
			<params>
				<param name="trade" type="12" value="" out="N"/>
				<param name="subtrade" type="12" value="" out="N"/>
				<param name="lane" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
