<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExtraDisposalMgtDBDAOmodifyExtraDisposalByEQDataUSQL">
			<desc><![CDATA[Scrapping/Donation Creation 에서 수정저장]]></desc>
			<sql><![CDATA[
UPDATE MNR_XTRA_DISP A
   SET A.EQ_TPSZ_CD = @[eq_tpsz_cd]
      ,A.XTRA_DISP_STS_CD = @[xtra_disp_sts_cd]
      ,A.MNR_XTRA_DISP_TP_CD = @[mnr_xtra_disp_tp_cd]
      ,A.CURR_CD = @[curr_cd]
      ,A.XTRA_DISP_EXPN_AMT = @[xtra_disp_expn_amt]
      ,A.XTRA_DISP_INCM_AMT = @[xtra_disp_incm_amt]
      ,A.ISS_DT = TO_DATE(@[iss_dt], 'yyyy-mm-dd')
      ,A.ISS_OFC_CD = @[iss_ofc_cd]
      ,A.ISS_YD_CD = @[iss_yd_cd]
      ,A.XTRA_DISP_DESC = @[xtra_disp_desc]
      ,A.XTRA_DISP_RMK = @[xtra_disp_rmk]
      ,A.FILE_SEQ = @[file_seq]
      ,A.IF_TRC_SEQ = @[if_trc_seq]
      ,A.UPD_USR_ID = @[upd_usr_id]
      ,A.UPD_DT = SYSDATE
 WHERE A.XTRA_DISP_SEQ = @[xtra_disp_seq]
   AND A.EQ_KND_CD = @[eq_knd_cd]
   AND A.EQ_NO = @[eq_no]			]]></sql>
			<params>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="xtra_disp_sts_cd" type="12" value="" out="N"/>
				<param name="mnr_xtra_disp_tp_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="xtra_disp_expn_amt" type="12" value="" out="N"/>
				<param name="xtra_disp_incm_amt" type="12" value="" out="N"/>
				<param name="iss_dt" type="12" value="" out="N"/>
				<param name="iss_ofc_cd" type="12" value="" out="N"/>
				<param name="iss_yd_cd" type="12" value="" out="N"/>
				<param name="xtra_disp_desc" type="12" value="" out="N"/>
				<param name="xtra_disp_rmk" type="12" value="" out="N"/>
				<param name="file_seq" type="12" value="" out="N"/>
				<param name="if_trc_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="xtra_disp_seq" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
