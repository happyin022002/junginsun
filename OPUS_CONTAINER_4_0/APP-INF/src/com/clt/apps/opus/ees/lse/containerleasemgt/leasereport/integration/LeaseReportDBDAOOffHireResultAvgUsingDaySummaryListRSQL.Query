				MAX(CASE WHEN C.RPT_DP_SEQ =27 THEN DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.USE_DAY,0) END) TPSZ_DP27_DAY,
                MAX(CASE WHEN C.RPT_DP_SEQ =28 THEN DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.USE_DAY,0) END) TPSZ_DP28_DAY,
                MAX(CASE WHEN C.RPT_DP_SEQ =29 THEN DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.USE_DAY,0) END) TPSZ_DP29_DAY,
                MAX(CASE WHEN C.RPT_DP_SEQ =30 THEN DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.USE_DAY,0) END) TPSZ_DP30_DAY,
                SUM(DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.VOL_CNT,0)) AS TPSZ_TOTAL_CNT,
                SUM(DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.USE_DAY,0)) AS TPSZ_TOTAL_DAY,
                ROUND(SUM(DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.USE_DAY,0)) / 
                      SUM(DECODE(A.CNTR_TPSZ_CD, C.CNTR_TPSZ_CD, A.VOL_CNT,0))) AS TPSZ_TOTAL_AVG                                  
        FROM   (SELECT  D.AGMT_CTY_CD, D.AGMT_SEQ, D.REF_NO, D.VNDR_SEQ, A.CNTR_TPSZ_CD, 
                        COUNT(D.CNTR_NO) AS VOL_CNT,
						CASE WHEN D.LSTM_CD IN('OW','LP','OL')
                        	 THEN ROUND(SUM(D.OFF_HR_DT - A.MFT_DT)) 
							 ELSE ROUND(SUM(D.OFF_HR_DT - B.CNTR_STS_EVNT_DT)) END USE_DAY,
						CASE WHEN D.LSTM_CD IN('OW','LP','OL')
                        	 THEN ROUND(SUM(D.OFF_HR_DT - A.MFT_DT) / COUNT(D.CNTR_NO))
							 ELSE ROUND(SUM(D.OFF_HR_DT - B.CNTR_STS_EVNT_DT) / COUNT(D.CNTR_NO)) END USE_AVG
                FROM    MST_CONTAINER A,
                        MST_CNTR_STS_HIS B,            
                        TEMP_INLINE D
                WHERE   A.CNTR_NO = B.CNTR_NO
                AND     D.CNTR_NO = B.CNTR_NO
                AND     D.PRNR_STS_SEQ = B.CNTR_STS_SEQ
                AND     B.AGMT_SEQ <> 999990
				AND     A.CO_CRE_FLG = 'N'
#if (${cntr_use_co_cd} != '' )
		        AND 	A.CNTR_USE_CO_CD = @[cntr_use_co_cd]
#end  
#if (${vndr_seq} != '' ) 
          		AND 	A.VNDR_SEQ = @[vndr_seq]
#end
#if (${cntr_sts_cd} == '1' ) 
				AND     B.CNTR_STS_CD IN ('DII','LSI','OWN')
#elseif (${cntr_sts_cd} == '2')
				AND     B.CNTR_STS_CD IN ('LSI','OWN')
#elseif (${cntr_sts_cd} == '3')
				AND     B.CNTR_STS_CD = 'DII'
#end
                GROUP BY D.AGMT_CTY_CD, D.AGMT_SEQ, D.REF_NO, D.VNDR_SEQ, D.LSTM_CD, A.CNTR_TPSZ_CD
                UNION ALL
                SELECT  'G.TTL', TO_NUMBER(''), '', TO_NUMBER(''), A.CNTR_TPSZ_CD, 
                        COUNT(D.CNTR_NO) AS VOL_CNT,
						CASE WHEN D.LSTM_CD IN('OW','LP','OL')
                        	 THEN ROUND(SUM(D.OFF_HR_DT - A.MFT_DT))
							 ELSE ROUND(SUM(D.OFF_HR_DT - B.CNTR_STS_EVNT_DT)) END USE_DAY,
						CASE WHEN D.LSTM_CD IN('OW','LP','OL')
							 THEN ROUND(SUM(D.OFF_HR_DT - A.MFT_DT) / COUNT(D.CNTR_NO))
							 ELSE ROUND(SUM(D.OFF_HR_DT - B.CNTR_STS_EVNT_DT) / COUNT(D.CNTR_NO)) END USE_AVG
                FROM    MST_CONTAINER A,
                        MST_CNTR_STS_HIS B,            
                        TEMP_INLINE D
                WHERE   A.CNTR_NO = B.CNTR_NO
                AND     D.CNTR_NO = B.CNTR_NO
                AND     D.PRNR_STS_SEQ = B.CNTR_STS_SEQ
                AND     B.AGMT_SEQ <> 999990
				AND     A.CO_CRE_FLG = 'N'
#if (${cntr_use_co_cd} != '' )
		        AND 	A.CNTR_USE_CO_CD = @[cntr_use_co_cd]
#end
#if (${vndr_seq} != '' ) 
          		AND 	A.VNDR_SEQ = @[vndr_seq]
#end  
#if (${cntr_sts_cd} == '1' ) 
				AND     B.CNTR_STS_CD IN ('DII','LSI','OWN')
#elseif (${cntr_sts_cd} == '2')
				AND     B.CNTR_STS_CD IN ('LSI','OWN')
#elseif (${cntr_sts_cd} == '3')
				AND     B.CNTR_STS_CD = 'DII'
#end
                GROUP BY D.LSTM_CD, A.CNTR_TPSZ_CD) A,
               (SELECT LEVEL AS DEPTH FROM DUAL CONNECT BY LEVEL < 4) B,
               (SELECT  CNTR_TPSZ_CD, ROW_NUMBER() OVER(ORDER BY RPT_DP_SEQ) AS RPT_DP_SEQ  
                FROM    MDM_CNTR_TP_SZ) C       
        WHERE   1 = 1
#if (${cntr_tpsz_cd} != '')
    	AND    	A.CNTR_TPSZ_CD IN (
	#foreach($key IN ${cntr_tpsz_cd_seq})
		#if($velocityCount < $cntr_tpsz_cd_seq.size())
			'$key',
		#else
			'$key'
		#end
	#end
				)
#end
        GROUP BY A.AGMT_CTY_CD, A.AGMT_SEQ, A.REF_NO, A.VNDR_SEQ, B.DEPTH
        ) A
WHERE   A.VNDR_SEQ = B.VNDR_SEQ(+)
ORDER BY AGMT_SEQ, RST_DIV DESC			]]></sql>
			<params>
				<param name="str_evnt_dt" type="12" value="" out="N"/>
				<param name="end_evnt_dt" type="12" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="co_cre_flg" type="12" value="" out="N"/>
				<param name="cntr_use_co_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
