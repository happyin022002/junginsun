		<query name="LeaseReportDBDAOOffHireResultAvgUsingDaySummaryListRSQL">
			<desc><![CDATA[반납 임차장비에 대한 계약번호별 평균사용 실적목록을 조회합니다.
]]></desc>
			<sql><![CDATA[
WITH TEMP_INLINE AS (
    SELECT  B.AGMT_CTY_CD, B.AGMT_SEQ, B.REF_NO, B.VNDR_SEQ, 
			B.LSTM_CD, A.CNTR_NO, A.YD_CD, PRNR_STS_SEQ,
            A.CNTR_STS_EVNT_DT AS OFF_HR_DT                
    FROM   (SELECT  TO_DATE(@[str_evnt_dt],'YYYYMMDD') AS STR_EVNT_DT,
                    TO_DATE(@[end_evnt_dt],'YYYYMMDD') AS END_EVNT_DT,
                    @[lstm_cd]                         AS LSTM_CD,
					@[agmt_cty_cd]                     AS AGMT_CTY_CD,
					@[agmt_seq]                        AS AGMT_SEQ,
					@[co_cre_flg]                     AS CO_CRE_FLG
            FROM    DUAL) P,
			MST_CONTAINER C,
            MST_CNTR_STS_HIS A,
            LSE_AGREEMENT B			
    WHERE   A.CNTR_NO = C.CNTR_NO
	AND		A.AGMT_CTY_CD = B.AGMT_CTY_CD
    AND     A.AGMT_SEQ = B.AGMT_SEQ
	AND     A.AGMT_SEQ <> 999990
    AND     A.CNTR_STS_CD IN ('LSO', 'DIO')
    AND     A.CNTR_STS_EVNT_DT BETWEEN P.STR_EVNT_DT AND P.END_EVNT_DT
#if (${co_cre_flg} != '' )
	AND     A.CNTR_LSTM_CNG_FLG = P.CO_CRE_FLG
#end  
#if (${agmt_seq} != '' )
    AND 	A.AGMT_CTY_CD = P.AGMT_CTY_CD       
    AND 	A.AGMT_SEQ = P.AGMT_SEQ  
#end  
#if (${lstm_cd} != '')
    AND     B.LSTM_CD IN (
	#foreach($key IN ${lstm_cd_seq})
		#if($velocityCount < $lstm_cd_seq.size())
			'$key',
		#else
			'$key'
		#end
	#end
			)
#end
)
