<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOSearchCntrTpszCdByEqrRefRSQL">
			<desc><![CDATA[SearchCntrTpszCdByEqrRef]]></desc>
			<sql><![CDATA[
SELECT V.CNTR_TPSZ_CD,
    V.CNTR_QTY AS OP_CNTR_QTY
FROM (SELECT VP.REF_ID,
        VQ.CNTR_TPSZ_CD,
        VQ.CNTR_QTY
    FROM EQR_VSL_LODG_DCHG_EXE_PLN VP, EQR_VSL_EXE_PLN_QTY VQ
    WHERE VP.REF_ID = VQ.REF_ID(+)
    AND VQ.CNTR_QTY > 0
    UNION ALL
    SELECT IP.REF_ID,
        IQ.CNTR_TPSZ_CD,
        IQ.CNTR_QTY
    FROM EQR_INLND_TRSP_EXE_PLN IP, EQR_INLND_TRSP_EXE_PLN_QTY IQ
    WHERE IP.REF_ID = IQ.REF_ID(+)
    AND IQ.CNTR_QTY > 0
    UNION ALL
    SELECT EP.REF_ID,
        EQ.CNTR_TPSZ_CD,
        EQ.CNTR_QTY
    FROM EQR_ECC_INTER_EXE_PLN EP, EQR_ECC_INTER_EXE_PLN_QTY EQ
    WHERE EP.REF_ID = EQ.REF_ID(+)
    AND EQ.CNTR_QTY > 0
    UNION ALL
    SELECT OP.REF_ID,
        OQ.CNTR_TPSZ_CD,
        OQ.CNTR_QTY
    FROM EQR_ONF_HIR_EXE_PLN OP, EQR_ONF_HIR_EXE_PLN_QTY OQ
    WHERE OP.REF_ID = OQ.REF_ID(+)
    AND OQ.CNTR_QTY > 0) V
WHERE V.REF_ID = @[mty_pln_no]			]]></sql>
			<params>
				<param name="mty_pln_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
