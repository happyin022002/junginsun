<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchTieredFreeTimeByRFARSQL">
			<desc><![CDATA[Tiered Free Time Search]]></desc>
			<sql><![CDATA[
SELECT RFA_EXPT_DAR_NO
     , RFA_EXPT_MAPG_SEQ
     , RFA_EXPT_VER_SEQ
     , RFA_RQST_DTL_SEQ
     , CVRG_CMB_SEQ
     , FT_SEQ
     , CNTR_FM_QTY
     , CNTR_TO_QTY
     , FT_DYS
  FROM DMT_RFA_EXPT_FREE_TM
 WHERE 1=1
   AND RFA_EXPT_DAR_NO   = @[rfa_expt_dar_no] 
   AND RFA_EXPT_MAPG_SEQ = @[rfa_expt_mapg_seq]
   AND RFA_EXPT_VER_SEQ  = @[rfa_expt_ver_seq]
   AND RFA_RQST_DTL_SEQ  = @[rfa_rqst_dtl_seq]
   AND CVRG_CMB_SEQ      = @[cvrg_cmb_seq]
 ORDER BY RFA_EXPT_DAR_NO
        , RFA_EXPT_MAPG_SEQ
        , RFA_EXPT_VER_SEQ
        , RFA_RQST_DTL_SEQ
        , CVRG_CMB_SEQ
        , FT_SEQ            			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
				<param name="rfa_rqst_dtl_seq" type="12" value="" out="N"/>
				<param name="cvrg_cmb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
