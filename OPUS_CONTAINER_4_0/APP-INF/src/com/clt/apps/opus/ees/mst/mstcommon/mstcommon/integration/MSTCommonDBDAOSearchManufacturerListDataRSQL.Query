<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MSTCommonDBDAOSearchManufacturerListDataRSQL">
			<desc><![CDATA[MST Common Manufacturer Select]]></desc>
			<sql><![CDATA[
SELECT MST_COMMON_PKG.MST_VNDR_SEQ_CONV_FNC(B.VNDR_SEQ) CODE ,NVL(B.VNDR_ABBR_NM,VNDR_LGL_ENG_NM) CODE_NM
FROM MDM_CNTR_VNDR_CLSS A, 
     MDM_VENDOR B
WHERE A.CNTR_VNDR_SVC_CD ='MFR'
AND A.VNDR_SEQ = B.VNDR_SEQ
AND NVL(A.DELT_FLG, 'N') = 'N'
AND NVL(B.DELT_FLG, 'N') = 'N'
#if(${vndr_seq} != '')
AND B.VNDR_SEQ = @[vndr_seq]
#end
ORDER BY CODE_NM			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
