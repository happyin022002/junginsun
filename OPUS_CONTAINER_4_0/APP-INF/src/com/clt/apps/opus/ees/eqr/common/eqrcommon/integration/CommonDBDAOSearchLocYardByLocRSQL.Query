<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLocYardByLocRSQL">
			<desc><![CDATA[Truck & Rail & Water Row Add 시 Execution 내 From Loc 및 To Loc 가져 오는 로직]]></desc>
			<sql><![CDATA[
SELECT 
	YD_CD
	,REPLACE(YD_NM, '''', ' ') YD_NM 
FROM 
	MDM_YARD
WHERE 
	DELT_FLG <> 'Y'
	AND LOC_CD = @[loc_cd]
ORDER BY YD_CD			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
