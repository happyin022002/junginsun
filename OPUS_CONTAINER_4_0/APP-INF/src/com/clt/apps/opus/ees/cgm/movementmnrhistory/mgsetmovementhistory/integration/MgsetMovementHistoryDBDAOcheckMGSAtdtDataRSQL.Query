<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MgsetMovementHistoryDBDAOcheckMGSAtdtDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  to_char( AA.ATCH_DT ,'yyyymmddHH24MIss') AS atch_dt
      , AA.DTCH_YD_CD AS dtch_yd_cd
      , AA.EQ_NO      AS eq_no
 FROM
	(
	SELECT  
		  A.ATCH_DT 
		, A.DTCH_YD_CD 
        , A.EQ_NO
	FROM CGM_EQ_ATCH_DTCH_HIS A
	WHERE 1=1 
#if(${eq_tpsz_cd} == 'UMG')
     AND  A.CHSS_NO = @[chss_no]
#else
     AND  A.CNTR_NO = @[cntr_no]
#end
     AND  A.EQ_KND_CD = 'G'
	ORDER BY A.ATCH_DT DESC
	) AA
WHERE ROWNUM =1			]]></sql>
			<params>
				<param name="chss_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
