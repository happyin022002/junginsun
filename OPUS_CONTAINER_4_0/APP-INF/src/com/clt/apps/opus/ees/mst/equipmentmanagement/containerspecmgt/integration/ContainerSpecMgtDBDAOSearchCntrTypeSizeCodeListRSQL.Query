<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOSearchCntrTypeSizeCodeListRSQL">
			<desc><![CDATA[ContainerSpecMgtDBDAOSearchCntrTypeSizeCodeListRSQL]]></desc>
			<sql><![CDATA[
SELECT 
a.CNTR_TPSZ_CD
,a.CNTR_TPSZ_DESC
,a.CNTR_TPSZ_ISO_CD
,b.ISO_CNTR_TPSZ_NM
,c.CNTR_SZ_TEU_CAPA
,decode(a.CNTR_TPSZ_GRP_CD,'D','DRY','S','SPE','R','RFR') AS CNTR_TPSZ_GRP_CD
,a.RPT_DP_SEQ
,a.ACIAC_DIV_CD
FROM MDM_CNTR_TP_SZ a, MST_ISO_CNTR_TP_SZ b, MDM_CNTR_SZ c
WHERE a.CNTR_TPSZ_ISO_CD = b.ISO_CNTR_TPSZ_CD
AND a.CNTR_SZ_CD = c.CNTR_SZ_CD
ORDER BY RPT_DP_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
