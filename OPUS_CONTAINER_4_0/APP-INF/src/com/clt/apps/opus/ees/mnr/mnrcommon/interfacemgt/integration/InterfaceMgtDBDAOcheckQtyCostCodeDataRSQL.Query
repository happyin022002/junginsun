<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOcheckQtyCostCodeDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT MNR_ORD_TP_CD || SUBSTR(PRNT_CD_ID, 1, 1) AS MNR_ORD_TP_CD
  FROM MNR_GEN_CD
 WHERE PRNT_CD_ID LIKE '_G'
   AND MNR_CD_ID = SUBSTR(@[cost_cd], 1, 6)			]]></sql>
			<params>
				<param name="cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
