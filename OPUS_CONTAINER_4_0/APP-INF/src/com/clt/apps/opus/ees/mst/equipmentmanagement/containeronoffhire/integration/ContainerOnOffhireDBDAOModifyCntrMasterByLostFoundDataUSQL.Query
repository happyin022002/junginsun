<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOModifyCntrMasterByLostFoundDataUSQL">
			<desc><![CDATA[ModifyCntrMasterByLostFoundData]]></desc>
			<sql><![CDATA[
UPDATE MST_CONTAINER A
SET  A.CNMV_DT       = TO_DATE(@[h_cnmv_evnt_dt],'YYYYMMDD')
	,(A.LST_STS_SEQ, A.CNTR_STS_CD,A.LST_STS_YD_CD) = (
	    SELECT /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */ A.CNTR_STS_SEQ, A.CNTR_STS_CD, A.YD_CD
	    FROM   MST_CNTR_STS_HIS A
	    WHERE A.CNTR_NO = @[cntr_no]
	    AND ROWNUM =1
		)	
    ,A.CRNT_YD_CD    = @[h_onh_yd_cd]
    ,A.LOC_CD = SUBSTR(@[h_onh_yd_cd],1,5)
	,(A.SCC_CD, A.LCC_CD, A.ECC_CD, A.RCC_CD) = (SELECT B.SCC_CD, B.LCC_CD, B.ECC_CD, B.RCC_CD FROM MDM_LOCATION A, MDM_EQ_ORZ_CHT B WHERE A.LOC_CD = SUBSTR(@[h_onh_yd_cd], 1, 5) AND A.SCC_CD = B.SCC_CD)
    ,A.CNMV_STS_CD   = DECODE(@[h_cntr_sts_cd],'LST','XX','MT')
    ,A.ACIAC_DIV_CD  = DECODE(@[h_cntr_sts_cd],'LST','I','A')
    ,A.CNTR_RMK     = @[cntr_rmk]
    ,A.UPD_USR_ID	= @[upd_usr_id]
    ,A.UPD_DT		= sysdate
WHERE A.CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="h_cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="h_onh_yd_cd" type="12" value="" out="N"/>
				<param name="h_cntr_sts_cd" type="12" value="" out="N"/>
				<param name="cntr_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
