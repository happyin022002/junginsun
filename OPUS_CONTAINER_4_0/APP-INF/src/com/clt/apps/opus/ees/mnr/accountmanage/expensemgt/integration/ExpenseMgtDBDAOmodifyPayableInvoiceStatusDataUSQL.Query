<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExpenseMgtDBDAOmodifyPayableInvoiceStatusDataUSQL">
			<desc><![CDATA[ExpenseMgtDBDAOmodifyPayableInvoiceStatusDataUSQL]]></desc>
			<sql><![CDATA[
UPDATE MNR_PAY_INV_WRK SET  
	MNR_INV_STS_CD = @[mnr_inv_sts_cd]
    ,INV_RGST_NO = ''
	,UPD_USR_ID = @[cre_usr_id]
    ,INV_CFM_DT = TO_DATE(@[inv_cfm_dt], 'yyyy-mm-dd')
    ,CFM_DT = TO_DATE(@[cfm_dt], 'yyyy-mm-dd')
	,UPD_DT = sysdate
WHERE PAY_INV_SEQ = @[pay_inv_seq]			]]></sql>
			<params>
				<param name="mnr_inv_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="inv_cfm_dt" type="12" value="" out="N"/>
				<param name="cfm_dt" type="12" value="" out="N"/>
				<param name="pay_inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
