<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LSECommonDBDAOSearchComIntgCdCodeListRSQL">
			<desc><![CDATA[COM_INTG_CD_DTL 코드 목록 조회]]></desc>
			<sql><![CDATA[
SELECT  INTG_CD_VAL_CTNT CODE, INTG_CD_VAL_DP_DESC AS CODE_NM --INTG_CD_VAL_DP_DESC
FROM    COM_INTG_CD_DTL
WHERE   INTG_CD_ID   = @[ingt_cd_id]
ORDER BY INTG_CD_VAL_DP_SEQ 			]]></sql>
			<params>
				<param name="ingt_cd_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
