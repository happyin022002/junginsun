<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAORemoveInitialOwnMovementDataDSQL">
			<desc><![CDATA[OWN으로 생성된 Initial Movement 정보를 삭제함.]]></desc>
			<sql><![CDATA[
DELETE
FROM CTM_MOVEMENT CM
WHERE CM.CNTR_NO = @[cntr_no]
AND NOT EXISTS ( 
	SELECT 'X'
    	FROM MST_CONTAINER MC
        WHERE CM.CNTR_NO = MC.CNTR_NO
)			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
