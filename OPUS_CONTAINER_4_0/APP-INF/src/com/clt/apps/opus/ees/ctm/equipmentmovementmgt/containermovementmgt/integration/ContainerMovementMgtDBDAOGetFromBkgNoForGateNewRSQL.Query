<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetFromBkgNoForGateNewRSQL">
			<desc><![CDATA[GetFromBkgNoForGateNew]]></desc>
			<sql><![CDATA[
SELECT NVL (BKG_NO, '') AS FROM_BKG
  FROM BKG_BOOKING
 WHERE TO_BKG_NO = @[bkg_number]
UNION ALL
SELECT NVL (BKG_NO, '') AS FROM_BKG
  FROM CTM_BOOKING
 WHERE TO_BKG_NO = @[bkg_number]			]]></sql>
			<params>
				<param name="bkg_number" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
