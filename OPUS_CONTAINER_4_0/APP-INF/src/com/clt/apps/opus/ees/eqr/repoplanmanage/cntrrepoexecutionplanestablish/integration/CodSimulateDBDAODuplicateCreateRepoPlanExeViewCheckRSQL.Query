<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodSimulateDBDAODuplicateCreateRepoPlanExeViewCheckRSQL">
			<desc><![CDATA[Repo Plan Id Check]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) PLANCNT FROM (
	SELECT 'Y' PLANCNT
		FROM EQR_REPO_EXE_PLN
		WHERE REPO_PLN_ID = @[repo_pln_id]
)			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
