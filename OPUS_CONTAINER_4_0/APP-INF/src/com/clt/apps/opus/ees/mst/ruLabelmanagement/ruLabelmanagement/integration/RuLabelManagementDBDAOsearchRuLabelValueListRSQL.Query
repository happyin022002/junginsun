<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RuLabelManagementDBDAOsearchRuLabelValueListRSQL">
			<desc><![CDATA[RU Label Value 데이타 조회]]></desc>
			<sql><![CDATA[
SELECT 
	RSTR_USG_LBL_NM 
FROM 
	MST_RSTR_USG_CD 
WHERE 1=1
AND DELT_FLG = 'N'
#if (${ru_label_type} != '') 
	AND RSTR_USG_TP_CD = @[ru_label_type]
#end
GROUP BY RSTR_USG_LBL_NM			]]></sql>
			<params>
				<param name="ru_label_type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
