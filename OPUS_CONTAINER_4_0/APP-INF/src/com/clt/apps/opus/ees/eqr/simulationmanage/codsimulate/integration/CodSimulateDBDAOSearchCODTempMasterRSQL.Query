<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodSimulateDBDAOSearchCODTempMasterRSQL">
			<desc><![CDATA[EQR_VSL_LDIS_PLN_COD_TMP 테이블의 특정 REPO PLAN ID 데이터의 repo plan id, create user 정보 검색]]></desc>
			<sql><![CDATA[
SELECT DISTINCT REPO_PLN_ID
               , CRE_USR_ID
               , to_char(CRE_DT ,'YYYY-MM-DD HH24:MI:SS') CRE_DT 
FROM EQR_VSL_LDIS_PLN_COD_TMP        
	WHERE REPO_PLN_ID = @[repo_pln_id]              
	AND CRE_USR_ID IS NOT NULL     
	AND CRE_DT IS NOT NULL          	
	ORDER BY CRE_DT DESC			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
