<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOConINTCommonVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO,
       CNMV_CYC_NO,
       VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD_CD,
       POR_CD,
       POD_CD,
       POL_CD,
       DEL_CD,
       BKG.BKG_NO,
       BL_NO BL_NO,
       MDT.CMDT_NM AS REP_CMDT_NM
FROM   BKG_BOOKING BKG, BKG_CONTAINER CON, MDM_COMMODITY MDT
WHERE  CNTR_NO = @[p_cntrno]||@[check_digit]
AND    CON.CNMV_EVNT_DT BETWEEN TO_DATE (@[p_date1], 'YYYY-MM-DD') AND TO_DATE (@[p_date2], 'YYYY-MM-DD') + 0.99999
AND    BKG.BKG_NO = CON.BKG_NO
AND    BKG.CMDT_CD = MDT.CMDT_CD(+)			]]></sql>
			<params>
				<param name="p_cntrno" type="12" value="" out="N"/>
				<param name="check_digit" type="12" value="" out="N"/>
				<param name="p_date1" type="12" value="" out="N"/>
				<param name="p_date2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
