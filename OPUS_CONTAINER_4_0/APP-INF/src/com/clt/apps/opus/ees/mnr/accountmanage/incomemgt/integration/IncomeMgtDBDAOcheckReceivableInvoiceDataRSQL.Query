<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncomeMgtDBDAOcheckReceivableInvoiceDataRSQL">
			<desc><![CDATA[IncomeMgtDBDAOcheckReceivableInvoiceDataRSQL]]></desc>
			<sql><![CDATA[
SELECT MNR_INV_STS_CD
FROM MNR_RCV_INV_WRK
WHERE INV_NO = @[input_inv_no]
AND MNR_PRNR_CNT_CD = SUBSTR(@[buyer_cd],1,2)
AND MNR_PRNR_SEQ = TO_NUMBER(SUBSTR(@[buyer_cd],3))
AND  MNR_INV_STS_CD IN ( 'HS', 'HC', 'HE')
#if (${rcv_inv_seq} != '')
AND  RCV_INV_SEQ <> @[rcv_inv_seq]
#end
AND ROWNUM  = 1			]]></sql>
			<params>
				<param name="input_inv_no" type="12" value="" out="N"/>
				<param name="buyer_cd" type="12" value="" out="N"/>
				<param name="rcv_inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
