<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodSimulateDBDAODeleteCodTempCleanDSQL">
			<desc><![CDATA[QTY가 없는 COD Temp를 삭제 하도록 기능 변경]]></desc>
			<sql><![CDATA[
DELETE FROM  EQR_VSL_LDIS_PLN_COD_TMP PLN
WHERE PLN.REPO_PLN_ID = @[repo_pln_id] 
AND NOT EXISTS ( SELECT 'X'
                   FROM EQR_VSL_PLN_COD_QTY QTY
                  WHERE PLN.REPO_PLN_ID = QTY.REPO_PLN_ID
                    AND PLN.REPO_PLN_ID = QTY.REPO_PLN_ID
                    AND PLN.PLN_YRWK    = QTY.PLN_YRWK
                    AND PLN.PLN_SEQ       = QTY.PLN_SEQ)			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
