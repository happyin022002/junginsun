<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOIsAcceptedVersionSTSRSQL">
			<desc><![CDATA[현재 버전의 상태가 Accepted 인지 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	COUNT(*)
FROM	DMT_SC_EXPT_VER
WHERE	PROP_NO 				= @[prop_no]
	AND	SC_EXPT_VER_SEQ 		= @[sc_expt_ver_seq]
	AND DMDT_EXPT_VER_STS_CD 	= 'A'			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
