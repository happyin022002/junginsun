<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchFinalFacilityRSQL">
			<desc><![CDATA[SearchFinalFacility]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(SCE_COP_DTL XPKSCE_COP_DTL) */
       NOD_CD
  FROM SCE_COP_DTL
 WHERE COP_NO IN (
                  SELECT COP_NO
                    FROM SCE_COP_HDR
                   WHERE BKG_NO = @[bkg_number]
                     AND CNTR_NO = @[cntr_no]
                  )
  AND ACT_CD LIKE 'FI%AD'
  AND SUBSTR(ACT_CD, 4, 1) <> 'Z'
  AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_number" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
