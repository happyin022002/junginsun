<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DVFactorMgtDBDAOsearchDVFactorListDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT NVL(A.EQ_DPC_YR, @[eq_dpc_yr]) EQ_DPC_YR,
       B.TBSZ_CD  CD_ID, 
       NVL(A.EQ_KND_CD, @[eq_knd_cd]) EQ_KND_CD,
       NVL(A.CURR_CD, 'USD') CURR_CD,
       NVL(A.EQ_INIT_PRC, 0) EQ_INIT_PRC,
       NVL(A.EQ_DPC_RT, DECODE(@[eq_knd_cd], 'G', 0.55, 0.45)) EQ_DPC_RT,
       A.EQ_MTRL_CD,
       A.CRE_USR_ID, 
       A.CRE_DT, 
       A.UPD_USR_ID, 
       A.UPD_DT,
       B.DESCRIPTION CD_DESC,
       NVL(A.MAX_DPC_RTO, 50) MAX_DPC_RTO
  FROM MNR_EQ_DPC A,
       (
        #if (${eq_knd_cd} == 'U') 
        SELECT CNTR_TPSZ_CD    TBSZ_CD,
               CNTR_TPSZ_DESC  DESCRIPTION
          FROM MDM_CNTR_TP_SZ
        #else 
        SELECT EQ_TPSZ_CD    TBSZ_CD,
               DIFF_DESC     DESCRIPTION
          FROM CGM_EQ_TP_SZ
         WHERE EQ_KND_CD = @[eq_knd_cd]
        #end
       ) B
 WHERE A.EQ_KND_CD(+) = @[eq_knd_cd]
   AND A.EQ_TPSZ_CD(+) = B.TBSZ_CD
   #if (${eq_dpc_yr} != '')
   AND A.EQ_DPC_YR(+) = @[eq_dpc_yr] 
   #end   
   #if (${eq_dpc_rt} != '') 
   AND A.EQ_DPC_RT = @[eq_dpc_rt]
   #end			]]></sql>
			<params>
				<param name="eq_dpc_yr" type="12" value="C" out="N"/>
				<param name="eq_knd_cd" type="12" value="C" out="N"/>
				<param name="eq_dpc_rt" type="12" value="C" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
