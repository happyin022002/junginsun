<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOGetYardCNTCodeRSQL">
			<desc><![CDATA[Yard Code의 Country code 를 검색해 옵니다.]]></desc>
			<sql><![CDATA[
SELECT 
    CNT_CD 				
FROM 
    EQR_ECC_MST A,				
    MDM_EQ_ORZ_CHT B		
WHERE 
    A.ECC_CD = B.ECC_CD	
    AND   B.SCC_CD = @[yard_code]
	AND A.DELT_FLG = 'N' -- 09.08.04 검색조건 추가 (삭제되지 않은 ECC만 조회) 기존버그 수정 By ChungEunHo			]]></sql>
			<params>
				<param name="yard_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
