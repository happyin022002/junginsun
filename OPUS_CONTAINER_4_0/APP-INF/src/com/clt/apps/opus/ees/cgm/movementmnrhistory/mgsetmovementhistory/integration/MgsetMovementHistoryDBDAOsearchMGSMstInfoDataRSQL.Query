<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MgsetMovementHistoryDBDAOsearchMGSMstInfoDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT /*+ index(b XPKCGM_EQ_STS_HIS )  */
       A.EQ_TPSZ_CD
      ,A.AGMT_LSTM_CD
      ,TO_CHAR(B.STS_EVNT_DT,'YYYY-MM-DD') ONH_DT
      ,A.EQ_NO
      ,A.EQ_STS_SEQ
      ,DECODE(A.ACIAC_DIV_CD,'A','Active','In Active') ACIAC_DIV_CD
 FROM CGM_EQUIPMENT  A
   ,  CGM_EQ_STS_HIS B
 WHERE A.EQ_KND_CD='G'
   AND A.EQ_NO = B.EQ_NO
   AND A.EQ_NO = @[eq_no]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
