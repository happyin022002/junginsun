<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOaddCHSOwnMasterLotDataCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO CGM_EQ_LOT (EQ_LOT_NO, EQ_KND_CD, EQ_LOT_ISS_DT, EQ_PFX_CD, FM_SER_NO,
                        TO_SER_NO, FINC_VNDR_SEQ, EQ_SPEC_NO, DE_YRMON,
                        AGMT_OFC_CTY_CD, AGMT_SEQ, AGMT_VER_NO, CRE_USR_ID, CRE_DT,
                        UPD_USR_ID, UPD_DT)
                VALUES (@[eq_lot_no], @[eq_knd_cd], TO_DATE(REPLACE(@[eq_lot_iss_dt], '-',''),'YYYYMMDD'), @[eq_pfx_cd], LPAD(@[fm_ser_no], 6, '0'),
                        LPAD(@[to_ser_no], 6, 0), @[finc_vndr_seq], @[eq_spec_no], @[de_yrmon],
                        @[agmt_ofc_cty_cd], @[agmt_seq], @[agmt_ver_no], @[cre_usr_id], SYSDATE,
                        @[upd_usr_id], SYSDATE)			]]></sql>
			<params>
				<param name="eq_lot_no" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_lot_iss_dt" type="12" value="" out="N"/>
				<param name="eq_pfx_cd" type="12" value="" out="N"/>
				<param name="fm_ser_no" type="12" value="" out="N"/>
				<param name="to_ser_no" type="12" value="" out="N"/>
				<param name="finc_vndr_seq" type="12" value="" out="N"/>
				<param name="eq_spec_no" type="12" value="" out="N"/>
				<param name="de_yrmon" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
