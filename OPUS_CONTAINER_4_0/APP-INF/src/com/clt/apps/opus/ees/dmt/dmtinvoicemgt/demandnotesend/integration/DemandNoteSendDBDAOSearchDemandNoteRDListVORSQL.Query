<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemandNoteSendDBDAOSearchDemandNoteRDListVORSQL">
			<desc><![CDATA[SearchDemandNoteRDListVO]]></desc>
			<sql><![CDATA[
SELECT B.CNTR_NO AS CNTR_NO
      ,B.CNTR_TPSZ_CD AS CNTR_TPSZ_CD
      ,TO_CHAR (C.FM_MVMT_DT
               ,'DDMonYY', 'NLS_DATE_LANGUAGE = American'
               ) AS FM_MVMT_DT
      ,TO_CHAR (C.TO_MVMT_DT
               ,'DDMonYY', 'NLS_DATE_LANGUAGE = American'
               ) AS TO_MVMT_DT
      ,CASE WHEN C.CXL_BKG_CHG_FLG = 'Y' AND C.SYS_AREA_GRP_ID != 'KOR' AND C.FT_DYS = 0
            THEN 
                 ''
            ELSE 
                 TO_CHAR (C.FT_CMNC_DT
                           ,'DDMonYY', 'NLS_DATE_LANGUAGE = American'
                          ) 
            END AS FT_CMNC_DT            
      ,CASE WHEN C.CXL_BKG_CHG_FLG = 'Y' AND C.SYS_AREA_GRP_ID != 'KOR' AND C.FT_DYS = 0
            THEN 
                 ''
            ELSE 
                 TO_CHAR (C.FT_END_DT
                           ,'DDMonYY', 'NLS_DATE_LANGUAGE = American'
                          ) 
            END AS FT_END_DT
      ,C.FT_DYS AS FT_DYS
      ,C.FX_FT_OVR_DYS AS FX_FT_OVR_DYS
      ,C.BZC_TRF_CURR_CD AS BZC_TRF_CURR_CD
      ,C.SYS_AREA_GRP_ID AS SVR_ID
      ,C.CNTR_CYC_NO AS CNTR_CYC_NO
      ,C.DMDT_TRF_CD AS DMDT_TRF_CD
      ,C.DMDT_CHG_LOC_DIV_CD AS DMDT_CHG_LOC_DIV_CD
      ,C.CHG_SEQ AS CHG_SEQ
      ,C.BZC_TRF_SEQ AS BZC_TRF_SEQ
      ,C.BZC_TRF_GRP_SEQ AS BZC_TRF_GRP_SEQ
      ,C.DMDT_CHG_STS_CD AS DMDT_CHG_STS_CD
      ,C.RFA_EXPT_DAR_NO AS RFA_EXPT_DAR_NO
      ,C.RFA_EXPT_MAPG_SEQ AS RFA_EXPT_MAPG_SEQ
      ,C.RFA_EXPT_VER_SEQ AS RFA_EXPT_VER_SEQ
      ,C.RFA_RQST_DTL_SEQ AS RFA_RQST_DTL_SEQ
      ,C.SC_NO AS SC_NO
      ,C.SC_EXPT_VER_SEQ AS SC_EXPT_VER_SEQ 
      ,C.SC_EXPT_GRP_SEQ AS SC_EXPT_GRP_SEQ
      ,C.DMDT_TRF_APLY_TP_CD AS DMDT_TRF_APLY_TP_CD
  FROM DMT_CHG_BKG_CNTR B
      ,DMT_CHG_CALC C
 WHERE B.SYS_AREA_GRP_ID = C.SYS_AREA_GRP_ID
   AND B.CNTR_NO = C.CNTR_NO
   AND B.CNTR_CYC_NO = C.CNTR_CYC_NO   
   AND C.SYS_AREA_GRP_ID = @[svr_id]
   AND C.CNTR_NO = @[cntr_no]
   AND C.CNTR_CYC_NO = @[cntr_cyc_no]
   AND C.DMDT_TRF_CD = @[dmdt_trf_cd]
   AND C.DMDT_CHG_LOC_DIV_CD = @[dmdt_chg_loc_div_cd]
   AND C.CHG_SEQ = @[chg_seq] 			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
