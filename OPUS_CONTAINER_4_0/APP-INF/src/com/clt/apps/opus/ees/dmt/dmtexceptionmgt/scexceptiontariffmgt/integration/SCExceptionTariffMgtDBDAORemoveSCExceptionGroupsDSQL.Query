<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAORemoveSCExceptionGroupDSQL">
			<desc><![CDATA[S/C Exception Terms Entry 에 선택된 Group 정보를 삭제하는 쿼리]]></desc>
			<sql><![CDATA[
DELETE FROM DMT_SC_EXPT_GRP
WHERE	PROP_NO = @[prop_no]
AND	SC_EXPT_VER_SEQ = @[sc_expt_ver_seq]
AND	SC_EXPT_GRP_SEQ = @[sc_expt_grp_seq]
			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="2" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
