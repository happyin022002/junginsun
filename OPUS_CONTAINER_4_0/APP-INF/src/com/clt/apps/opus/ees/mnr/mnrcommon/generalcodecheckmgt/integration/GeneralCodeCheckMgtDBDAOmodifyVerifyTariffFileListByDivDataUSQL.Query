<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyVerifyTariffFileListByDivDataUSQL">
			<desc><![CDATA[수정 - Tariff File Verify Div]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
   SET A.INP_MSG4 = 'DV'
 WHERE A.TMP_SEQ = @[tmp_seq]
   AND A.INP_MSG4 = 'SS'
   AND A.INP_MSG6 NOT IN (SELECT TO_RLT_CD
                            FROM MNR_CD_RLT
                           WHERE EQ_CEDEX_RLT_TP_CD = 'CTV'
                             AND FM_RLT_CD = A.INP_MSG1||A.INP_MSG2)
   AND A.INP_MSG6 IS NOT NULL 			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
