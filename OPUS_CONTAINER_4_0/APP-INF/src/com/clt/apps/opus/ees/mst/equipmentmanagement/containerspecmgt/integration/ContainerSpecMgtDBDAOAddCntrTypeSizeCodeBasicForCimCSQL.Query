<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOAddCntrTypeSizeCodeBasicForCimCSQL">
			<desc><![CDATA[MST에서 MDM의 CNTR TP/SZ 데이터 변경 시에 동일한 데이터를 CIM에 형성하기위해 MDM_CNTR_TP_SZ테이블의 데이터를 Insert한다.]]></desc>
			<sql><![CDATA[
INSERT INTO CIM_TP_SZ_DP_SEQ
SELECT LTRIM(TO_CHAR(ROW_NUMBER() OVER (ORDER BY RPT_DP_SEQ), '00')) AS DP_SEQ,
       CNTR_TPSZ_CD,
       CNTR_TP_CD,
       @[ofc_cd],
       @[ofc_cd],
       CRE_USR_ID,
       CRE_DT,
       UPD_USR_ID,
       UPD_DT
  FROM MDM_CNTR_TP_SZ
 WHERE 1=1
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
