<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchAproNoByPropApprovalNoRSQL">
			<desc><![CDATA[Approved 상태일때 Apro_no를 조회]]></desc>
			<sql><![CDATA[
SELECT 	RFA_EXPT_APRO_NO
FROM 	DMT_RFA_EXPT_TRF
WHERE PROP_NO 			= @[prop_no]
AND RFA_EXPT_DAR_NO 	= @[rfa_expt_dar_no]
AND RFA_EXPT_VER_SEQ 	= @[rfa_expt_ver_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
