<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOCheckBookingRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT BKG_NO,
       '' BKG_NO_SPLIT,
       RCV_TERM_CD,
       POR_CD,
       POL_CD,
       BL_NO,
       '' BL_NO_TP,
       '' BL_NO_CHK,
       BL_TP_CD,
       VSL_CD,
       SKD_DIR_CD,
       SKD_VOY_NO,
       BKG_CGO_TP_CD
FROM   BKG_BOOKING
WHERE  1 = 1
AND    BKG_NO = @[p_bkg_no]
AND		NVL(BKG_STS_CD,' ') <> 'S'
AND		NVL(BKG_STS_CD,' ') <> 'X'
UNION ALL
SELECT BKG_NO,
       '' BKG_NO_SPLIT,
       RCV_TERM_CD,
       POR_CD,
       POL_CD,
       BL_NO,
       '' BL_NO_TP,
       '' BL_NO_CHK,
       '' BL_TP_CD,
       VSL_CD,
       SKD_DIR_CD,
       SKD_VOY_NO,
       BKG_CGO_TP_CD
FROM   CTM_BOOKING
WHERE  1 = 1
AND    BKG_NO = @[p_bkg_no]
AND		NVL(BKG_STS_CD,' ') <> 'S'
AND		NVL(BKG_STS_CD,' ') <> 'X'			]]></sql>
			<params>
				<param name="p_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
