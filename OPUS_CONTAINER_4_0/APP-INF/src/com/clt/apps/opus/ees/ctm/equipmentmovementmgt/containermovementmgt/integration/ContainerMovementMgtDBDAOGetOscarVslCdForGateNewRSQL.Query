<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetOscarVslCdForGateNewRSQL">
			<desc><![CDATA[GetOscarVslCdForGateNew]]></desc>
			<sql><![CDATA[
SELECT MAP.VSL_CD 
  FROM  MDM_VSL_CNTR MAP 
 WHERE 1=1
   AND ((MAP.VSL_CD LIKE SUBSTR(TRIM(@[vsl_cd]),1,3)||'%'
   AND SUBSTR(MAP.VSL_CD,1,3) = SUBSTR(TRIM(@[vsl_cd]),1,3))
   OR MAP.MODI_VSL_CD LIKE SUBSTR(TRIM(@[vsl_cd]),1,3)||'%')
   AND DELT_FLG != 'Y'
   AND ROWNUM <= 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
