<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisHistoryDBDAOsearchPoolChsUseddaysFileStatusDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	  A.FILE_SEQ
	, A.ORG_FILE_NM
	, A.SAV_FILE_NM
	, A.CHSS_POOL_CD
	, A.COST_YRMON
	, A.FILE_IMP_PROC_STS_CD
    , B.INTG_CD_VAL_DP_DESC FILE_IMP_PROC_STS_NM
	, A.IMP_RSLT_DESC
	, to_char(A.FILE_IMP_DT,'yyyy-mm-dd hh24:mi:ss') FILE_IMP_DT
	, A.CRE_USR_ID
	, A.CRE_DT
	, A.UPD_USR_ID
	, A.UPD_DT
 FROM CGM_POOL_CHSS_IMP_FILE A
     , COM_INTG_CD_DTL B
 WHERE B.INTG_CD_ID ='CD02552'
   AND A.FILE_IMP_PROC_STS_CD = B.INTG_CD_VAL_CTNT
   AND A.CHSS_POOL_CD =  @[chss_pool_cd]
   AND A.COST_YRMON = @[cost_yrmon]			]]></sql>
			<params>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
