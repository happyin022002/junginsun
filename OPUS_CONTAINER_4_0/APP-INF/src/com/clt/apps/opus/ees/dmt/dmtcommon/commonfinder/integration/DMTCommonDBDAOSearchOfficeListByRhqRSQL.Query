<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchOfficeListByRhqRSQL">
			<desc><![CDATA[searchOfficeListByRhq]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
      ,OFC_ENG_NM
  FROM MDM_ORGANIZATION O
      , (SELECT DISTINCT DMDT_OFC_CD
                    FROM MDM_YARD Y
                        , (	SELECT DISTINCT OFC_CD
							  FROM MDM_ORGANIZATION
							 WHERE NVL (DELT_FLG, ' ') <> 'Y'
						#if (${rhq_ofc_cd} != ${head_office}) 
							CONNECT BY PRIOR OFC_CD = PRNT_OFC_CD
							START WITH OFC_CD = @[rhq_ofc_cd]
						#end
						  ) O
                   WHERE Y.DMDT_OFC_CD = O.OFC_CD
                     AND DELT_FLG = 'N') Y
 WHERE O.OFC_CD = Y.DMDT_OFC_CD
   --AND NVL (O.OFC_SLS_DELT_FLG, 'N') <> 'Y'
ORDER BY ofc_cd			]]></sql>
			<params>
				<param name="rhq_ofc_cd" type="12" value="SHAAS" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
