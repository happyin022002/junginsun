<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQAvailabilityFinderDBDAOsearchAvailONListRSQL">
			<desc><![CDATA[On-hire 계획 및 실적(조회당일)의 Detail 정보를 확인]]></desc>
			<sql><![CDATA[
SELECT
	 A.FCAST_DT
	,A.YD_CD
	,A.EVNT_DT
	,A.EVNT_YD_CD
	,A.CNTR_TPSZ_CD
	,A.CNTR_VOL_QTY
	,A.LSTM_CD 
	,A.LESSOR
	,A.AGMT_NO
	,A.CNTR_AUTH_NO
FROM
(
	SELECT   
		 TO_CHAR(A.FCAST_DT,'YYYY-MM-DD') FCAST_DT
		,A.YD_CD
		,NULL EVNT_DT
		,NULL EVNT_YD_CD
		,A.CNTR_TPSZ_CD
		,SUM(A.CNTR_QTY) CNTR_VOL_QTY
		,'OW' LSTM_CD 
		, NULL LESSOR
		,NULL AGMT_NO 
		, NULL CNTR_AUTH_NO
	FROM  CIM_AVAL_DTL A, MDM_LOCATION L, MDM_EQ_ORZ_CHT D
	WHERE A.CNTR_AVAL_FCAST_TP_CD ='N1'
    AND   L.SCC_CD = D.SCC_CD
#if (${loc_type_code} == 'S')
	AND D.SCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'E')
	AND D.ECC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'L')
	AND D.LCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'R')
	AND D.RCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'Y')
    AND L.LOC_CD = SUBSTR(@[loc_cd],1,5)
#else
	AND D.SCC_CD = @[loc_cd]
    AND NVL(@[loc_type_code], 'X') = NVL(@[loc_type_code], 'X')
#end
	AND   A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
	AND   L.LOC_CD = SUBSTR(A.YD_CD,1,5)
	#if (${fcast_dt} !='' )
		AND A.FCAST_DT BETWEEN TO_DATE(@[fcast_dt],'YYYYMMDD') AND TO_DATE(@[fcast_dt],'YYYYMMDD')+0.99999
	#end
	GROUP BY TO_CHAR(A.FCAST_DT,'YYYY-MM-DD'), A.YD_CD, A.CNTR_TPSZ_CD
	UNION ALL
	SELECT
         TO_CHAR(A.EVNT_DT,'YYYY-MM-DD') FCAST_DT
		,A.EVNT_YD_CD YD_CD
		,TO_CHAR(A.FCAST_DT,'YYYY-MM-DD')  EVNT_DT
		,A.YD_CD EVNT_YD_CD
		,A.CNTR_TPSZ_CD
		,SUM(A.CNTR_QTY) CNTR_VOL_QTY
		,MAX(B.LSTM_CD) LSTM_CD 
		,MAX(NVL(C.VNDR_ABBR_NM,C.VNDR_LGL_ENG_NM)) LESSOR
		,A.AGMT_CTY_CD||A.AGMT_SEQ AGMT_NO
		,A.CNTR_AUTH_NO
	FROM  CIM_AVAL_DTL A, MDM_LOCATION L, LSE_AGREEMENT B, MDM_VENDOR C, MDM_EQ_ORZ_CHT D
	WHERE A.CNTR_AVAL_FCAST_TP_CD = 'N2'
    AND   L.SCC_CD = D.SCC_CD
#if (${loc_type_code} == 'S')
	AND D.SCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'E')
	AND D.ECC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'L')
	AND D.LCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'R')
	AND D.RCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'Y')
    AND L.LOC_CD = SUBSTR(@[loc_cd],1,5)
#else
	AND D.SCC_CD = @[loc_cd]
    AND NVL(@[loc_type_code], 'X') = NVL(@[loc_type_code], 'X')
#end
	AND   A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
	AND   L.LOC_CD = SUBSTR(A.YD_CD,1,5)
	#if (${fcast_dt} !='' )
		AND A.FCAST_DT BETWEEN TO_DATE(@[fcast_dt],'YYYYMMDD') AND TO_DATE(@[fcast_dt],'YYYYMMDD')+0.99999
	#end
	AND   A.AGMT_CTY_CD = B.AGMT_CTY_CD(+)
	AND   A.AGMT_SEQ    = B.AGMT_SEQ(+)
	AND   B.VNDR_SEQ = C.VNDR_SEQ(+)
	GROUP BY TO_CHAR(A.FCAST_DT,'YYYY-MM-DD'), A.YD_CD, TO_CHAR(A.EVNT_DT,'YYYY-MM-DD'),A.EVNT_YD_CD,A.CNTR_TPSZ_CD, A.AGMT_CTY_CD, A.AGMT_SEQ , A.CNTR_AUTH_NO
	UNION ALL
	SELECT
		 NULL FCAST_DT
		,NULL YD_CD
		,TO_CHAR(A.FCAST_DT,'YYYY-MM-DD') EVNT_DT
		,A.YD_CD EVNT_YD_CD
		,A.CNTR_TPSZ_CD
		,SUM(A.CNTR_QTY) CNTR_VOL_QTY
		,MAX(B.LSTM_CD) LSTM_CD 
		,MAX(NVL(C.VNDR_ABBR_NM,C.VNDR_LGL_ENG_NM)) LESSOR
		,A.AGMT_CTY_CD||A.AGMT_SEQ AGMT_NO
		,NULL CNTR_AUTH_NO
	FROM  CIM_AVAL_DTL A, MDM_LOCATION L ,LSE_AGREEMENT B, MDM_VENDOR C, MDM_EQ_ORZ_CHT D
	WHERE A.CNTR_AVAL_FCAST_TP_CD ='N3'
    AND   L.SCC_CD = D.SCC_CD
#if (${loc_type_code} == 'S')
	AND D.SCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'E')
	AND D.ECC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'L')
	AND D.LCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'R')
	AND D.RCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'Y')
    AND L.LOC_CD = SUBSTR(@[loc_cd],1,5)
#else
	AND D.SCC_CD = @[loc_cd]
    AND NVL(@[loc_type_code], 'X') = NVL(@[loc_type_code], 'X')
#end
	AND   A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
	AND   L.LOC_CD = SUBSTR(A.YD_CD,1,5)
	#if (${fcast_dt} !='' )
		AND A.FCAST_DT BETWEEN TO_DATE(@[fcast_dt],'YYYYMMDD') AND TO_DATE(@[fcast_dt],'YYYYMMDD')+0.99999
	#end
	AND   A.AGMT_CTY_CD = B.AGMT_CTY_CD(+)
	AND   A.AGMT_SEQ    = B.AGMT_SEQ(+)
	AND   B.VNDR_SEQ = C.VNDR_SEQ(+)
	GROUP BY TO_CHAR(A.FCAST_DT,'YYYY-MM-DD'), A.YD_CD, A.CNTR_TPSZ_CD, A.AGMT_CTY_CD, A.AGMT_SEQ
	UNION ALL
	SELECT   
		 TO_CHAR(A.FCAST_DT,'YYYY-MM-DD') FCAST_DT
		,A.YD_CD
		,NULL EVNT_DT
		,NULL EVNT_YD_CD
		,A.CNTR_TPSZ_CD
		,SUM(A.CNTR_QTY) CNTR_VOL_QTY
		,MAX(B.LSTM_CD) LSTM_CD 
		,MAX(NVL(C.VNDR_ABBR_NM,C.VNDR_LGL_ENG_NM)) LESSOR
		,A.AGMT_CTY_CD||A.AGMT_SEQ AGMT_NO
		,A.CNTR_AUTH_NO CNTR_AUTH_NO
	FROM  CIM_AVAL_DTL A, MDM_LOCATION L ,LSE_AGREEMENT B, MDM_VENDOR C, MDM_EQ_ORZ_CHT D
	WHERE A.CNTR_AVAL_FCAST_TP_CD IN('N4')
    AND   L.SCC_CD = D.SCC_CD
#if (${loc_type_code} == 'S')
	AND D.SCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'E')
	AND D.ECC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'L')
	AND D.LCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'R')
	AND D.RCC_CD = @[loc_cd]
#elseif (${loc_type_code} == 'Y')
    AND L.LOC_CD = SUBSTR(@[loc_cd],1,5)
#else
	AND D.SCC_CD = @[loc_cd]
    AND NVL(@[loc_type_code], 'X') = NVL(@[loc_type_code], 'X')
#end
	AND   A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
	AND   L.LOC_CD = SUBSTR(A.YD_CD,1,5)
	#if (${fcast_dt} !='' )
		AND A.FCAST_DT BETWEEN TO_DATE(@[fcast_dt],'YYYYMMDD') AND TO_DATE(@[fcast_dt],'YYYYMMDD')+0.99999
	#end
	AND   A.AGMT_CTY_CD = B.AGMT_CTY_CD(+)
	AND   A.AGMT_SEQ    = B.AGMT_SEQ(+)
	AND   B.VNDR_SEQ = C.VNDR_SEQ(+)
	GROUP BY TO_CHAR(A.FCAST_DT,'YYYY-MM-DD'), A.YD_CD, A.CNTR_TPSZ_CD, A.AGMT_CTY_CD, A.AGMT_SEQ, A.CNTR_AUTH_NO
) A
ORDER BY A.FCAST_DT, A.YD_CD, NVL(A.EVNT_DT,'1111111111'),NVL(A.EVNT_YD_CD,'111111')			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="loc_type_code" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="fcast_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
