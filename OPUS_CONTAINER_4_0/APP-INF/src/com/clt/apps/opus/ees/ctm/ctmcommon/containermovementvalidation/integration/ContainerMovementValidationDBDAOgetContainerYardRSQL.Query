<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOgetContainerYardRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT A.CNTR_NO,
       A.CNMV_STS_CD,
       A.CNTR_TPSZ_CD,
       A.CRNT_YD_CD,
       A.ACIAC_DIV_CD,
       A.CO_CRE_FLG,
       A.IMDT_EXT_FLG,
       A.NEW_FLG,
       DECODE (B.CNTR_NO, NULL, 0, 1) CNTR_EX,
	   A.FCNTR_FLG,
       A.LSTM_CD,
       A.DMG_FLG
  FROM (SELECT MM.CNTR_NO,
               MM.CNMV_STS_CD,
               MM.CNTR_TPSZ_CD,
               MM.CRNT_YD_CD,
               MM.ACIAC_DIV_CD,
               MM.CO_CRE_FLG,
               MM.IMDT_EXT_FLG,
               MM.NEW_FLG,
               MM.FCNTR_FLG,
               MM.LSTM_CD,
               MM.DMG_FLG
          FROM (SELECT M.CNTR_NO,
                       M.CNMV_STS_CD,
                       M.CNTR_TPSZ_CD,
                       M.CRNT_YD_CD,
                       M.ACIAC_DIV_CD,
                       CASE WHEN SYS_AREA_GRP_ID = RGN_CD THEN 'Y' ELSE 'N' END AS CO_CRE_FLG,
                       M.IMDT_EXT_FLG,
                       M.CO_CRE_FLG AS NEW_FLG,
                       M.FULL_FLG AS FCNTR_FLG,
                       M.LSTM_CD,
                       M.DMG_FLG
                  FROM MST_CONTAINER M,
                       (SELECT SYS_AREA_GRP_ID AS RGN_CD
                          FROM COM_SYS_AREA_GRP_ID
                         WHERE CNT_CD = SUBSTR (@[yard_cd], 1, 2)
                           AND CO_IND_CD = 'H')
                 WHERE M.CNTR_NO LIKE @[cntr_no]||'%'
              ORDER BY CNMV_DT DESC) MM
         WHERE ROWNUM = 1) A,
       (SELECT CNTR_NO
          FROM (SELECT CNTR_NO
                  FROM CTM_MOVEMENT
                 WHERE CNTR_NO LIKE @[cntr_no]||'%'
              ORDER BY CNMV_EVNT_DT DESC)
         WHERE ROWNUM = 1) B
 WHERE A.CNTR_NO = B.CNTR_NO(+)
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="yard_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
