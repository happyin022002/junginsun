<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOSearchMGSLessorAgmtMatchingDataRSQL">
			<desc><![CDATA[ChassisMgsetInvoiceDB.SearchMGSLessorAgmtMatchingData]]></desc>
			<sql><![CDATA[
SELECT A.INV_REF_NO, 
	   A.AGMT_OFC_CTY_CD || LPAD(A.AGMT_SEQ, 6, '0') AS AGMT_NO,
       A.AGMT_OFC_CTY_CD,
       A.AGMT_SEQ,
       B.VNDR_SEQ,
       C.VNDR_LGL_ENG_NM
FROM CGM_INV_REF_NO_RGST A, CGM_AGREEMENT B, MDM_VENDOR C 
WHERE A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD
      AND A.AGMT_SEQ = B.AGMT_SEQ
	  AND B.VNDR_SEQ = C.VNDR_SEQ
	  AND B.LST_VER_FLG = 'Y'
	  AND A.EQ_KND_CD = @[eq_knd_cd]
	  AND B.EQ_KND_CD = @[eq_knd_cd]
      AND B.DELT_FLG = 'N'
ORDER BY A.INV_REF_NO			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
