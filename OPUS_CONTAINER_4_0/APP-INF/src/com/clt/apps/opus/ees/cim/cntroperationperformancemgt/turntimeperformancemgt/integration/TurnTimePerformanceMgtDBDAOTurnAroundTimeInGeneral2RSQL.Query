<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TurnTimePerformanceMgtDBDAOTurnAroundTimeInGeneral2RSQL">
			<desc><![CDATA[TurnAroundTimeInGeneral2VO 조회]]></desc>
			<sql><![CDATA[
SELECT	
        /*+ ORDERED USE_NL(D S) INDEX (D XAK1CIM_SHP_TURN_TM_SMRY) */
		D.CNTR_TPSZ_CD	loccode1,								
		D.TRD_CD||'-'||D.TRNK_DIR_CD loccode2,										
		--M.TRNK_DIR_CD boundCode,									
		ROUND(SUM( 
			OB_FULL_LAND_TT_DYS	+ 
			FULL_SEA_TT_DYS	+ 
			TS_LAND_TT_DYS	+ 
			TS_SEA_TT_DYS	+ 
			IB_FULL_LAND_TT_DYS	+ 
			MTY_LAND_TT_DYS	+ 
			MTY_SEA_TT_DYS 
		)/SUM(CNTR_KNT),1)  total,											
		ROUND(SUM(OB_FULL_LAND_TT_DYS)/SUM(CNTR_KNT),1) obFullLandTime,		
		ROUND(SUM(FULL_SEA_TT_DYS)/SUM(CNTR_KNT),1)  fullSeaTime,		
		ROUND(SUM(TS_LAND_TT_DYS)/SUM(CNTR_KNT),1)  tsLandTime,		
		ROUND(SUM(TS_SEA_TT_DYS)/SUM(CNTR_KNT),1)  tsSeaTime,		
		ROUND(SUM(IB_FULL_LAND_TT_DYS)/SUM(CNTR_KNT),1)  ibFullLandTime,		
		ROUND(SUM(MTY_LAND_TT_DYS)/SUM(CNTR_KNT),1)  mtyLandTime,		
		ROUND(SUM(MTY_SEA_TT_DYS)/SUM(CNTR_KNT),1)  mtySeaTime,	
		ROUND(365/(SUM( 
			OB_FULL_LAND_TT_DYS	+ 
			FULL_SEA_TT_DYS	+ 
			TS_LAND_TT_DYS	+ 
			TS_SEA_TT_DYS	+ 
			IB_FULL_LAND_TT_DYS	+ 
			MTY_LAND_TT_DYS	+ 
			MTY_SEA_TT_DYS 
		    )/	SUM(CNTR_KNT)),1) eq,
		SUM(CNTR_KNT) cntrVolume	
FROM	CIM_SHP_TURN_TM_SMRY	D,
		CIM_TP_SZ_DP_SEQ_V		S 
#if (${period} == 'M') 
WHERE	TGT_MVMT_DT	BETWEEN    @[from]	AND @[to]
#elseif (${period} == 'W') 
WHERE	TGT_YRWK	BETWEEN    @[from]	AND @[to]	
#end

AND		D.CNTR_TPSZ_CD		=	S.CNTR_TPSZ_CD
AND		D.CO_CD				= 	'O'

#if (${trade} != '') 
AND		D.TRD_CD			=	@[trade]
#end
#if (${bound} != '') 
AND		D.TRNK_DIR_CD		=	@[bound]
#end
#if (${lane} != '')  
AND		D.SLAN_CD			=	@[lane]
#end

#if (${tpsz} != '') 
AND     D.CNTR_TPSZ_CD		IN	(
#foreach($cntrtpszcd in ${vel_tpsz_cd})  
	'$cntrtpszcd',  
	#end  
	'') 
#end 



#if (${tscntr} == 'E') 
AND		D.TS_FLG		=	'N' 
#elseif (${tscntr} == 'O') 
AND		D.TS_FLG		=	'Y' 
#end 
#if (${soc} == 'E') 
AND		D.SOC_FLG		=	'N' 
#elseif (${soc} == 'O') 
AND		D.SOC_FLG		=	'Y' 
#end 

GROUP BY
		RollUp(S.DP_SEQ,
		D.CNTR_TPSZ_CD,									
		D.TRD_CD||'-'||D.TRNK_DIR_CD)
ORDER BY
		S.DP_SEQ,
		D.TRD_CD||'-'||D.TRNK_DIR_CD			]]></sql>
			<params>
				<param name="from" type="12" value="" out="N"/>
				<param name="to" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="lane" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
