<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOSearchCntrStdInfoDataRSQL">
			<desc><![CDATA[EDM에 등록된 Container Creation 기준정보 조회]]></desc>
			<sql><![CDATA[
SELECT  
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_OFC' THEN INTG_CD_VAL_DP_DESC END) AS OFC_CD,
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_LOC' THEN INTG_CD_VAL_DP_DESC END) AS LOC_CD,
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_RCC' THEN INTG_CD_VAL_DP_DESC END) AS RCC_CD,
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_LCC' THEN INTG_CD_VAL_DP_DESC END) AS LCC_CD,
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_ECC' THEN INTG_CD_VAL_DP_DESC END) AS ECC_CD,
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_SCC' THEN INTG_CD_VAL_DP_DESC END) AS SCC_CD,
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_YD' THEN INTG_CD_VAL_DP_DESC END)  AS YD_CD,
    MAX(CASE INTG_CD_VAL_CTNT WHEN 'ST_AREA' THEN INTG_CD_VAL_DP_DESC END)  AS AREA_CD
FROM COM_INTG_CD_DTL
WHERE INTG_CD_ID ='CD20014'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
