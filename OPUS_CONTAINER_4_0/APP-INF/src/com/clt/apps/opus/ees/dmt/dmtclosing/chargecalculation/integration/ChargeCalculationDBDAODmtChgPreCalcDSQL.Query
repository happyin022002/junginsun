<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAODmtChgPreCalcDSQL">
			<desc><![CDATA[ChargeCalculationDBDAODmtChgPreCalcCSQL]]></desc>
			<sql><![CDATA[
DELETE FROM DMT_CHG_PRE_CALC
WHERE	
	SYS_AREA_GRP_ID	= @[svr_id]
AND	CNTR_NO			= @[cntr_no]
AND	CNTR_CYC_NO 	= @[cntr_cyc_no]

#if (${dul_tp_expt_flg} == 'Y')
	#if (${dmdt_trf_cd} == 'CTIC')
		AND	DMDT_TRF_CD	IN ('CTIC', 'DMIF', 'DTIC')
	#elseif (${dmdt_trf_cd} == 'CTOC') 
		AND	DMDT_TRF_CD	IN ('CTOC', 'DTOC', 'DMOF')
	#else
		AND	DMDT_TRF_CD			= @[dmdt_trf_cd]
		AND	DMDT_CHG_LOC_DIV_CD = @[dmdt_chg_loc_div_cd]
	#end
#else
	AND	DMDT_TRF_CD			= @[dmdt_trf_cd]
	AND	DMDT_CHG_LOC_DIV_CD = @[dmdt_chg_loc_div_cd]
#end

AND	CHG_SEQ	= @[chg_seq]			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="2" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
