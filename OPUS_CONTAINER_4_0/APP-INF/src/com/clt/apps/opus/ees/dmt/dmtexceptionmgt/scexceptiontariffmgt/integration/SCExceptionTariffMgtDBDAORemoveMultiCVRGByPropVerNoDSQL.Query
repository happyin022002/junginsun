<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAORemoveMultiCVRGByPropVerNoDSQL">
			<desc><![CDATA[Prop No. 와 Ver No. 에 해당되는 Multi Coverage 정보를 삭제하는 쿼리]]></desc>
			<sql><![CDATA[
DELETE
FROM	DMT_SC_EXPT_CVRG
WHERE	PROP_NO = @[del_prop_no]
	AND	SC_EXPT_VER_SEQ = @[del_sc_expt_ver_seq]			]]></sql>
			<params>
				<param name="del_prop_no" type="12" value="" out="N"/>
				<param name="del_sc_expt_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
