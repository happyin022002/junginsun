<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOGetMTDateRSQL">
			<desc><![CDATA[getMTDate]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR (WEB_MTY_DT, 'YYYYMMDD') PRM_MT_DATE
  FROM DMT_CHG_CALC
 WHERE SYS_AREA_GRP_ID  = @[svr_id]
   AND CNTR_NO 			= @[cntr_no]
   AND CNTR_CYC_NO 		= @[cnmv_cyc_no]
   AND DMDT_TRF_CD 		= @[dtt_code]
   AND FM_MVMT_STS_CD 	= @[fm_cnms]
   AND WEB_IND_FLG 		= 'Y'			]]></sql>
			<params>
				<param name="svr_id" type="12" value="USA" out="N"/>
				<param name="cntr_no" type="12" value="EURU0670363" out="N"/>
				<param name="cnmv_cyc_no" type="12" value="6" out="N"/>
				<param name="dtt_code" type="12" value="DMIF" out="N"/>
				<param name="fm_cnms" type="12" value="VD" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
