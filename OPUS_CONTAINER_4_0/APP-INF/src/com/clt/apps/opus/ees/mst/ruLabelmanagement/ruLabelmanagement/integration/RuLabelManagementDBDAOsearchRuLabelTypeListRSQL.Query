AND RSTR_USG_TP_LBL_NM1 = @[ru_label_value]
#end

#if (${ru_label_type} == 'OWFU') 
AND RSTR_USG_TP_LBL_NM2 = @[ru_label_value]
#end

#if (${ru_label_type} == 'OFHR') 
AND RSTR_USG_TP_LBL_NM3 = @[ru_label_value]
#end

#if (${ru_label_type} == 'DOME') 
