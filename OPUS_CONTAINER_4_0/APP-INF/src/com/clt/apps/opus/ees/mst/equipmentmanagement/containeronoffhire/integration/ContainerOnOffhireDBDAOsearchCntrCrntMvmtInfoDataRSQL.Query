<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOsearchCntrCrntMvmtInfoDataRSQL">
			<desc><![CDATA[Movement Information 조회]]></desc>
			<sql><![CDATA[
SELECT MC.CNTR_NO AS EQ_NO
     , MC.CNMV_YR
     , MC.CNMV_ID_NO
     , MC.CNMV_STS_CD
FROM MST_CONTAINER MC
WHERE MC.CNTR_NO     = @[eq_no]
AND   MC.CNMV_STS_CD = 'XX'
AND   ROWNUM = 1			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
