<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DeliveryLocationDBDAOSearchCountryRSQL">
			<desc><![CDATA[Country 코드조회]]></desc>
			<sql><![CDATA[
SELECT 
   A.CNT_CD
FROM  MDM_COUNTRY A
WHERE A.CNT_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
