<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtForGateNewDBDAOSearchBkgStatusRSQL">
			<desc><![CDATA[BKG_BOOKING의 BKG_STS_CD를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_STS_CD FROM BKG_BOOKING
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
