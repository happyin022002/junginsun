<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchMGSOwnMasterLotDataRSQL">
			<desc><![CDATA[aa]]></desc>
			<sql><![CDATA[



SELECT A.EQ_SPEC_NO,
	   A.EQ_TPSZ_CD,
       A.MGST_VLTG_CAPA,
       A.MGST_FUEL_CAPA,
       (SELECT D.VNDR_LGL_ENG_NM
          FROM MDM_VENDOR D
         WHERE A.VNDR_SEQ = D.VNDR_SEQ) AS VNDR_LGL_ENG_NM_EQSPEC,
       A.VNDR_SEQ
  FROM CGM_EQ_SPEC A
 WHERE A.EQ_KND_CD = @[eq_knd_cd]
   AND A.EQ_SPEC_NO = @[eq_spec_no_tmp]			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_spec_no_tmp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
