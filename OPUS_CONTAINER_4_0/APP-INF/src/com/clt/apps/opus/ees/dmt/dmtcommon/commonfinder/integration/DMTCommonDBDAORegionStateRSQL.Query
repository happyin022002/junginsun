<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAORegionStateRSQL">
			<desc><![CDATA[Region and State ( 'US' & 'CA')]]></desc>
			<sql><![CDATA[
SELECT	CNT_CD, RGN_CD, RGN_NM
FROM	MDM_REGION
WHERE	CNT_CD NOT IN ('CA','US')
AND DELT_FLG = 'N'
UNION ALL
SELECT	CNT_CD, STE_CD, STE_NM
FROM	MDM_STATE
WHERE	CNT_CD IN ('CA','US')
AND DELT_FLG = 'N'
ORDER BY CNT_CD, RGN_CD ASC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
