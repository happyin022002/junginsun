			 @[wo_no],
			 @[edi_vvd_cd],
			 @[tir_no],
			 @[mty_pln_no],
			 @[mty_repo_no],
			 @[edi_crr_no],
			 @[trsp_doc_no],
			 @[osca_bkg_flg],
			 MST_COMMON_PKG.MST_RU_TP_GET_FNC(@[cntr_no]),
			 MST_COMMON_PKG.MST_RU_VAL_GET_FNC(@[cntr_no]),
			 @[pkup_no],
			 @[wbl_no],
			 TO_DATE (@[dmg_flg_dt], 'YYYY-MM-DD HH24:MI'),
			 TO_DATE (@[dmg_unflg_dt], 'YYYY-MM-DD HH24:MI'),
             GLOBALDATE_PKG.TIME_CONV_FNC (SUBSTR (@[org_yd_cd], 0, 5 ), TO_DATE (@[cnmv_evnt_dt], 'YYYY-MM-DD HH24:MI'), 'GMT' )
			 , @[vgm_doc_id_no]
			 , @[vgm_wgt]
			 , @[vgm_wgt_ut_cd]
			 , @[vgm_doc_tp_cd]
			 , @[vgm_dt_tp_cd]
			 , TO_DATE (@[vgm_hndl_dt], 'YYYYMMDDHH24MISS')
			 , @[vgm_cust_cntc_tp_cd]
			 , @[vgm_cust_cntc_nm]
			 , @[vgm_cust_fax_no]
			 , @[vgm_cust_eml]
			 , @[vgm_cust_phn_no]
			 , @[vgm_cust_addr]
             , @[usa_edi_cd]
            )			]]></sql>
