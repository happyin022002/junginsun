             DECODE (@[ob_cntr_flg], 'O', 'Y', 'I', 'N', @[ob_cntr_flg] ),
             DECODE (@[vndr_seq], 'null', '', @[vndr_seq] ),
             NVL (@[spcl_cgo_flg], 'N'),
             @[bkg_no],
             @[bkg_knt],
             @[bl_no],
             NVL (@[cntr_disp_flg], 'N'),
             NVL (@[imdt_ext_flg], 'N'),
             @[cntr_xch_cd],
             COM_CONSTANTMGR_PKG.COM_GETCOMPANYCODE_FNC(),
             GLOBALDATE_PKG.TIME_CONV_FNC (SUBSTR (@[org_yd_cd], 0, 5 ), SYSDATE, 'GMT' ),
             @[cre_usr_id],
             @[cre_usr_id],
             @[usr_nm],
             SYSDATE,
             GLOBALDATE_PKG.TIME_LOCAL_FNC(SUBSTR (@[org_yd_cd], 0, 5 )),
             SYSDATE,
             GLOBALDATE_PKG.TIME_LOCAL_FNC(SUBSTR (@[org_yd_cd], 0, 5 )),
