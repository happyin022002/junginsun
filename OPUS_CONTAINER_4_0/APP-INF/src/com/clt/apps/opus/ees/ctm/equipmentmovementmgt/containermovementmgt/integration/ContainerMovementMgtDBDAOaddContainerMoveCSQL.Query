             USR_NM,
             CRE_DT,
             CRE_LOCL_DT,
             UPD_DT,
             UPD_LOCL_DT,
             CNTR_NO,
             CNMV_YR,
             CNMV_ID_NO,
             CNMV_SEQ,
             CNMV_SPLIT_NO,
             MVMT_STS_CD,
             BKG_CGO_TP_CD,
             CNMV_CYC_NO,
             CNMV_EVNT_DT,
