			 TIR_NO,
			 MTY_PLN_NO,
			 MTY_REPO_NO,
			 EDI_CRR_NO,
			 TRSP_DOC_NO,
             OSCA_BKG_FLG,
			 RSTR_USG_LBL_NM_DESC,
