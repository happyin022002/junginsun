<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonFaxEmailDBDAOSearchPayerNameRSQL">
			<desc><![CDATA[FAX 발송할 수신자(Payer)의  이름을 조회]]></desc>
			<sql><![CDATA[
SELECT CUST_LGL_ENG_NM AS PAYER_NM, OFC_CD FROM MDM_CUSTOMER 
WHERE CUST_CNT_CD||CUST_SEQ = @[cust_cd]			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
