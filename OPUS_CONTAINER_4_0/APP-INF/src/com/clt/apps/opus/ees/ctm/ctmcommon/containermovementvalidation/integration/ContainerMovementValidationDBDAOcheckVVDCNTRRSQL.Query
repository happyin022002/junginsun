<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOcheckVVDCNTRRSQL">
			<desc><![CDATA[ContainerMovementValidationDB
checkVVDCNTR]]></desc>
			<sql><![CDATA[
SELECT BKG_NO 
#if (${osca_bkg_flg} == 'Y')
 FROM CTM_BKG_VVD
#else
 FROM BKG_VVD
#end
  
 WHERE 1 = 1
#if (${p_vvd_type} != '')
   #if (${p_vvd_type} == 'VL')
      AND POL_CD = SUBSTR(@[p_yardcd],0,5)
   #else
      AND POD_CD = SUBSTR(@[p_yardcd],0,5)
   #end
#end
#if (${p_vvdcd} != '')
   AND VSL_CD = SUBSTR(@[p_vvdcd], 1 ,4)
   AND SKD_VOY_NO = SUBSTR(@[p_vvdcd], 5 ,4)
   AND SKD_DIR_CD = SUBSTR(@[p_vvdcd], 9 ,1)
#end
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="p_yardcd" type="12" value="" out="N"/>
				<param name="p_vvdcd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
