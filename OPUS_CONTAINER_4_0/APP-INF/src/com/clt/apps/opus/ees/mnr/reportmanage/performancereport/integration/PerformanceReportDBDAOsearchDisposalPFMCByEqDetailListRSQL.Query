<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOsearchDisposalPFMCByEqDetailListRSQL">
			<desc><![CDATA[장비별 매각실적 상세목록을 조회합니다.
2011.11.30 남궁진호 [CHM-201007327-01]   Disposal Performance를 장비별 Detail 내역 조회]]></desc>
			<sql><![CDATA[
SELECT  A.RHQ_CD, A.RQST_OFC_CD, A.APRO_OFC_CD,
        A.EQ_KND_CD, A.EQ_NO, A.EQ_TPSZ_CD, 
        A.LESSOR_NM,A.LSTM_CD,
        A.RSTR_USG_LBL_TP,
        A.RSTR_USG_LBL_VAL, 
	    A.ASET_KND,
	    A.DISP_STS_CD, A.INV_NO, A.UPD_USR_ID, TO_CHAR(A.UPD_DT,'YYYY-MM-DD') AS UPD_DT,
	    A.MANU_DT,
        A.DISP_NO,
        A.DISP_SOLD_DT, A.CUST_CD, A.CUST_NM,
        B.RCC_CD, B.LCC_CD, B.SCC_CD, B.LOC_CD, A.DISP_YD_CD,
       (SELECT  MNR_CD_DP_DESC   FROM  MNR_GEN_CD   WHERE  PRNT_CD_ID = 'CD00038'
        AND     MNR_CD_ID = A.DISP_RSN_CD) AS DISP_RSN_CD, 
		A.CURR_CD, A.PART_AMT, A.CAL_PART_AMT,
        A.DISP_UT_PRC, A.DISP_TRF_UT_PRC, A.DISP_VRFY_TP_CD,
       (SELECT  MNR_CD_DP_DESC   FROM  MNR_GEN_CD   WHERE  PRNT_CD_ID = 'CD00080'
        AND     MNR_CD_ID = A.DISP_VRFY_TP_CD) AS DISP_VRFY_TP_NM,
        CASE WHEN A.DISP_RSN_CD != 'C' THEN A.RPR_COST_AMT END RPR_COST_AMT
FROM   (SELECT  A.DISP_NO, B.DISP_SOLD_DT, A.EQ_KND_CD, A.RQST_OFC_CD, A.CURR_CD,
                MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(A.RQST_OFC_CD) AS RHQ_CD,
                A.APRO_OFC_CD, B.DISP_DTL_SEQ, B.EQ_TPSZ_CD, D.ASET_KND,B.DISP_QTY, B.PART_AMT, B.DISP_YD_CD,
                MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(B.DISP_SOLD_DT, 'YYYYMM'),
                    A.CURR_CD, 'USD', B.PART_AMT) AS CAL_PART_AMT, D.MANU_DT,
                D.LESSOR_NM,D.LSTM_CD,
				#if(${p_eq_knd_cd} == 'U')
                MST_COMMON_PKG.MST_RU_TP_GET_FNC(E.CNTR_NO) AS RSTR_USG_LBL_TP,
                MST_COMMON_PKG.MST_RU_VAL_GET_FNC(E.CNTR_NO) AS RSTR_USG_LBL_VAL,
				#else
				'' AS RSTR_USG_LBL_TP,
                '' AS RSTR_USG_LBL_VAL,
				#end
	            A.DISP_STS_CD, B.INV_NO, B.UPD_USR_ID, B.UPD_DT,
                CASE WHEN B.MNR_PRNR_SEQ IS NOT NULL
                     THEN MNR_COMMON_PKG.MNR_CONV_PARTNER_CD_FNC(B.MNR_PRNR_SEQ, B.MNR_PRNR_CNT_CD)
                     ELSE 'ZZ999999' END CUST_CD,
                CASE WHEN B.MNR_PRNR_SEQ IS NOT NULL THEN C.MNR_PRNR_LGL_ENG_NM
                     ELSE 'Anonymous Buyer' END CUST_NM,
                B.EQ_NO, B.DISP_RSN_CD, B.DISP_UT_PRC, B.DISP_TRF_UT_PRC, D.RPR_COST_AMT,
                CASE WHEN B.DISP_VRFY_TP_CD IS NOT NULL THEN B.DISP_VRFY_TP_CD
                     WHEN B.DISP_RSN_CD != 'C' THEN 'DA'
                     WHEN B.DISP_TRF_UT_PRC IS NULL THEN 'NT'
                     WHEN B.DISP_UT_PRC >= B.DISP_TRF_UT_PRC THEN 'SS'
                     WHEN B.DISP_UT_PRC < B.DISP_TRF_UT_PRC  THEN 'UP'
                END DISP_VRFY_TP_CD
        FROM    MNR_DISP_HDR A,
                MNR_DISP_DTL B,
                MNR_PARTNER C,
                MNR_EQ_STS_V D
				#if(${p_eq_knd_cd} == 'U')
				,MST_CONTAINER E
				#end
        WHERE   A.DISP_NO = B.DISP_NO
        AND     A.EQ_KND_CD = D.EQ_TYPE
        AND     B.EQ_NO = D.EQ_NO
		#if(${p_eq_knd_cd} == 'U')
		AND     B.EQ_NO = E.CNTR_NO
		#end
        AND     B.EQ_TPSZ_CD = D.EQ_TPSZ_CD
        AND     B.MNR_PRNR_CNT_CD = C.MNR_PRNR_CNT_CD(+)
        AND     B.MNR_PRNR_SEQ    = C.MNR_PRNR_SEQ(+)
        AND     B.DISP_SOLD_DT IS NOT NULL
		#if(${p_eq_knd_cd} == 'U')
			#if (${rstr_usg_lbl} != '')
				AND	
				(
				E.RSTR_USG_TP_LBL_NM1 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM2 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM3 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM4 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM5 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM6 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM7 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM8 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM9 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM10 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				OR
				E.RSTR_USG_TP_LBL_NM11 IN (
						#foreach ($key IN ${labelvalue_list})
		                	#if($velocityCount < $labelvalue_list.size())
		                    	'$key',
		                    #else
		                        '$key'
		                    #end
		                #end			  
		           )
				)
			#end
		#end
        ) A,
       (SELECT  A.LOC_CD, A.RGN_CD, A.SCC_CD, A.EQ_CTRL_OFC_CD,
                C.LCC_CD, C.ECC_CD, C.RCC_CD
        FROM    MDM_LOCATION A,
                MDM_EQ_ORZ_CHT C
        WHERE   A.SCC_CD = C.SCC_CD
        ) B
WHERE   SUBSTR(A.DISP_YD_CD,1,5) = B.LOC_CD
AND     A.DISP_SOLD_DT BETWEEN TO_DATE(@[p_str_evnt_dt],'YYYY-MM-DD') AND TO_DATE(@[p_end_evnt_dt],'YYYY-MM-DD')
#if (${p_eq_knd_cd} != '' )
AND     A.EQ_KND_CD = @[p_eq_knd_cd]
#end
#if (${p_disp_rsn_cd} != '' )
AND     A.DISP_RSN_CD = @[p_disp_rsn_cd]
#end
#if (${aset_knd} != '' )
AND     A.ASET_KND = @[aset_knd]
#end
#if (${p_cust_cd} != '')
AND     A.CUST_CD = @[p_cust_cd]
#end
#if (${p_loc_cd} != '')
	#if (${p_loc_tp} == 'RCC')
AND    	B.RCC_CD = @[p_loc_cd]
	#elseif (${p_loc_tp} == 'LCC')
AND    	B.LCC_CD = @[p_loc_cd]
	#else
AND    	B.SCC_CD = @[p_loc_cd]
	#end
#end


ORDER BY A.DISP_NO, A.DISP_DTL_SEQ, A.DISP_SOLD_DT, A.EQ_TPSZ_CD, A.EQ_NO			]]></sql>
			<params>
				<param name="p_str_evnt_dt" type="12" value="" out="N"/>
				<param name="p_end_evnt_dt" type="12" value="" out="N"/>
				<param name="p_eq_knd_cd" type="12" value="" out="N"/>
				<param name="p_disp_rsn_cd" type="12" value="" out="N"/>
				<param name="aset_knd" type="12" value="" out="N"/>
				<param name="p_cust_cd" type="12" value="" out="N"/>
				<param name="p_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
