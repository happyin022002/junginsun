<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchTrspOrderExistForGateNewRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 1
  FROM TRS_TRSP_SVC_ORD
 WHERE LOCL_CRE_DT BETWEEN TO_DATE (@[event_date], 'YYYY-MM-DD HH24:MI') - 20 AND TO_DATE (@[event_date], 'YYYY-MM-DD HH24:MI') + 7
   AND TRSP_SO_TP_CD = 'Y'
   AND TRSP_COST_DTL_MOD_CD = 'DR'
   AND FM_NOD_CD = @[event_yard]
   AND TRSP_BND_CD = 'I'
   AND BKG_NO = @[bkg_number]
   AND EQ_NO = @[cntr_no]
   AND DELT_FLG = 'N'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="event_date" type="12" value="" out="N"/>
				<param name="event_yard" type="12" value="" out="N"/>
				<param name="bkg_number" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
