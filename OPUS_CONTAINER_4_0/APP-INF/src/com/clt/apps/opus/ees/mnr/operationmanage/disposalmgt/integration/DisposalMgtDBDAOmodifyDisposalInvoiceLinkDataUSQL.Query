<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DisposalMgtDBDAOmodifyDisposalInvoiceLinkDataUSQL">
			<desc><![CDATA[DisposalMgtDBDAOmodifyDisposalInvoiceLinkDataUSQL]]></desc>
			<sql><![CDATA[
UPDATE MNR_DISP_DTL SET  
	INV_NO = @[inv_no]
	,INV_AMT = REPLACE (@[inv_amt], ',', '')
	,RCV_INV_SEQ = @[rcv_inv_seq]
	,UPD_USR_ID = @[upd_usr_id]
	,UPD_DT = sysdate
WHERE DISP_NO = @[disp_no]
AND DISP_DTL_SEQ = @[disp_dtl_seq]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="rcv_inv_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="disp_no" type="12" value="" out="N"/>
				<param name="disp_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
