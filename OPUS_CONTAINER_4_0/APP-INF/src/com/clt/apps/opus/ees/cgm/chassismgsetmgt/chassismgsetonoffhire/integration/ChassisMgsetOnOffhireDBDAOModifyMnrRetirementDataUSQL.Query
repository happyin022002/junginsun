<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOModifyMnrRetirementDataUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE CGM_EQUIPMENT
 SET ACIAC_DIV_CD     = @[aciac_div_cd]
    ,CRNT_YD_CD       = @[crnt_yd_cd]
    ,CRNT_LOC_CD      = SUBSTR(@[crnt_yd_cd],0,5)
    ,CHSS_MVMT_STS_CD = @[chss_mvmt_sts_cd]
    ,EQ_STS_SEQ       =  NVL(CGM_GET_EQ_STS_HIS_MAXSEQ_FNC(@[eq_no]) ,0)
    ,CHSS_MVMT_DT     = TO_DATE(@[onh_dt],'YYYYMMDD')
    ,UPD_USR_ID       = @[upd_usr_id]
    ,UPD_DT           = sysdate 
 WHERE EQ_NO = @[eq_no]			]]></sql>
			<params>
				<param name="aciac_div_cd" type="12" value="" out="N"/>
				<param name="crnt_yd_cd" type="12" value="" out="N"/>
				<param name="chss_mvmt_sts_cd" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="onh_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
