<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOSearchCntrSpecRSQL">
			<desc><![CDATA[SearchCntrSpec]]></desc>
			<sql><![CDATA[
SELECT
    A.CNTR_SPEC_NO
,	A.CNTR_SPEC_TP_CD  AS OWN_CNTR_FLG
,	A.SPEC_YR
,	A.CNTR_TPSZ_CD
,	A.CNTR_MTRL_CD
,	A.LOD_CAPA
,	RTRIM(TO_CHAR(ROUND(TO_NUMBER(A.TARE_WGT), 3), 'FM9999999999999990D000'), '.') TARE_WGT
,	RTRIM(TO_CHAR(ROUND(TO_NUMBER(A.CNTR_GRS_WGT), 3), 'FM9999999999999990D000'), '.') CNTR_GRS_WGT
,	RTRIM(TO_CHAR(ROUND(TO_NUMBER(A.TARE_WGT) - TO_NUMBER(A.CNTR_GRS_WGT), 3), 'FM9999999999999990D000'), '.') PAY_LOAD
,	A.INTER_LEN
,	A.INTER_WDT
,	A.INTER_HGT
,	A.XTER_LEN
,	A.XTER_WDT
,	A.XTER_HGT
,	A.OPN_DOR_WDT
,	A.OPN_DOR_HGT
,	A.RC_LDB_CAPA
,	A.RC_LDB_HGT
,	A.TNK_CAPA
,	A.DIFF_RMK
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,	A.UPD_DT
,   LTRIM(TO_CHAR(A.VNDR_SEQ,'000000')) AS VNDR_SEQ2
,   MDM.VNDR_ABBR_NM
,	A.FRACK_CLPS_CTNT
,	A.FRACK_BED_TIK_CTNT
,	A.OPNTP_ROOF_OPN_CTNT
,	A.OPNTP_INTR_HGT_CTNT 
,	A.OPNTP_REAR_HDR_OPN_CTNT 
FROM MST_CNTR_SPEC A, MDM_VENDOR MDM 
WHERE	A.CNTR_SPEC_NO = @[cntr_spec_no]
AND A.VNDR_SEQ = MDM.VNDR_SEQ(+)			]]></sql>
			<params>
				<param name="cntr_spec_no" type="12" value="1986-D2-025" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
