<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchARIFCountRSQL">
			<desc><![CDATA[A/R I/F 발생건수 조회 와 INV_CUR VALIDATION CHECK]]></desc>
			<sql><![CDATA[
SELECT 
#if (${check_flg} == '1') 
	DECODE(DMDT_AR_IF_CD, 'Y', DMDT_INV_NO, '') AS DMDT_INV_NO
#elseif(${check_flg} == '2') 
	DECODE(INV_CURR_CD, NULL,'N','Y') AS INV_CHK
#end
FROM DMT_INV_MN
WHERE DMDT_INV_NO IN (
    #foreach( $dmdt_inv_no in ${dmdt_inv_no_list}) 
        #if($velocityCount < $dmdt_inv_no_list.size()) 
           '$dmdt_inv_no', 
        #else 
           '$dmdt_inv_no' 
        #end 
    #end
    )
AND CRE_OFC_CD = @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
