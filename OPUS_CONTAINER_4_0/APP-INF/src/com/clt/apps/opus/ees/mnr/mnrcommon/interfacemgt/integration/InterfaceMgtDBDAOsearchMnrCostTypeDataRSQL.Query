<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchMnrCostTypeDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT DECODE(MNR_ORD_TP_CD, 'LB', 'EST', 'TS', 'SPL', 'QT', DECODE(MNR_CD_ID, 'MRRUSP', 'RFS', 'EXT'), 'EST') MNR_ORD_TP_CD
  FROM MNR_GEN_CD
 WHERE PRNT_CD_ID = @[eq_knd_cd]||'G'
   AND MNR_CD_ID = @[cost_cd]			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
