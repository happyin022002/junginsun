<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CIMCommonDBDAOModifyBkgContainerEtcDataUSQL">
			<desc><![CDATA[Container 추가 수정]]></desc>
			<sql><![CDATA[
UPDATE BKG_CONTAINER BC
SET    BC.CNMV_YR          = NULL
      ,  BC.CNMV_ID_NO     = NULL
      ,  BC.CNMV_CYC_NO    = 9999
      ,  BC.CNMV_STS_CD    = NULL
      ,  BC.CNMV_EVNT_DT   = NULL
      ,  BC.ORG_YD_CD	   = NULL
      ,  BC.DIFF_RMK       = NULL
      ,  BC.CGO_RCV_DT     = NULL
      ,  BC.CGO_RCV_YD_CD  = NULL
WHERE BC.CRE_DT > TO_DATE('2015-08-01', 'YYYY-MM-DD')
AND   NVL(BC.CNMV_CYC_NO, 0) != 9999
AND   BC.CNTR_NO = @[cntr_no]                      
AND  (BC.CNTR_NO, BC.BKG_NO)  IN  ( SELECT SBC.CNTR_NO
                                         , SUBSTR (MAX(TO_CHAR (NVL (VPS.VPS_ETD_DT, SBB.CRE_DT), 'YYYYMMDDHH24MISS') || SBC.BKG_NO),15)
                                      FROM BKG_CONTAINER SBC, BKG_BOOKING SBB, BKG_VVD BV, VSK_VSL_PORT_SKD VPS
                                      WHERE SBB.BKG_NO                 = SBC.BKG_NO
                                      AND   SBC.CNTR_NO                = @[cntr_no]
                                      AND   NVL(SBB.BKG_STS_CD, 'X')  != 'X'
                                      AND   SBB.BKG_NO                 = BV.BKG_NO(+)
                                      AND   BV.VSL_PRE_PST_CD(+)       = 'T'
                                      AND   BV.VSL_CD                  = VPS.VSL_CD(+)
                                      AND   BV.SKD_VOY_NO              = VPS.SKD_VOY_NO(+)
                                      AND   BV.SKD_DIR_CD              = VPS.SKD_DIR_CD(+)
                                      AND   BV.POL_CD                  = SUBSTR (VPS.YD_CD(+), 1, 5)                                   
                                      GROUP BY SBC.CNTR_NO)
AND   NOT EXISTS (SELECT 'X' 
                    FROM  CTM_MOVEMENT  CM
                   WHERE  BC.BKG_NO  = CM.BKG_NO
                     AND  BC.CNTR_NO = CM.CNTR_NO)			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
