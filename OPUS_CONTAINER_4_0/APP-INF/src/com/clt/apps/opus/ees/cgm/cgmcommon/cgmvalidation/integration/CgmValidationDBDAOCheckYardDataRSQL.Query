<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmValidationDBDAOCheckYardDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT  A.YD_CD
       ,A.YD_NM
       ,A.OFC_CD
FROM    MDM_YARD A 
WHERE 	 A.YD_CD = @[yd_cd]
		AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
