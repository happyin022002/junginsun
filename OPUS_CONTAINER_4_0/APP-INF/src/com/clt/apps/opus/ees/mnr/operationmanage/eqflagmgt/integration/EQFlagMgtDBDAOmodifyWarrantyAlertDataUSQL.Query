<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQFlagMgtDBDAOmodifyWarrantyAlertDataUSQL">
			<desc><![CDATA[modifyWarrantyAlertData]]></desc>
			<sql><![CDATA[
UPDATE  MNR_EQ_RNG_STS A 
SET 
   A.EQ_TPSZ_CD = @[eq_tpsz_cd]
,  A.EQ_MKR_NM = @[eq_mkr_nm]
,  A.EQ_MDL_NM = @[eq_mdl_nm]
,  A.FM_WARR_DT = TO_DATE(@[fm_warr_dt],'YYYY-MM-DD')
,  A.TO_WARR_DT = TO_DATE(@[to_warr_dt],'YYYY-MM-DD')
,  A.WARR_RMK = @[warr_rmk]
,  A.MFT_YR = @[mft_yr]
,  A.EQ_QTY = SUBSTRB(MST_COMMON_PKG.MST_CNTR_CHK_DGT_FNC(@[lot_eq_pfx_cd] || @[to_ser_no]),5,11) - SUBSTRB(MST_COMMON_PKG.MST_CNTR_CHK_DGT_FNC(@[lot_eq_pfx_cd] || @[fm_ser_no]),5,11) + 1  
,  A.UPD_USR_ID = @[upd_usr_id]
,  A.UPD_DT = SYSDATE
WHERE 
   	A.LOT_EQ_PFX_CD = @[lot_eq_pfx_cd] 
	AND SUBSTRB(A.FM_SER_NO,1,LENGTH(A.FM_SER_NO) -1) = @[fm_ser_no] 
	AND SUBSTRB(A.TO_SER_NO,1,LENGTH(A.TO_SER_NO) -1) = @[to_ser_no] 
	AND A.MNR_GRP_TP_CD = @[mnr_grp_tp_cd]			]]></sql>
			<params>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="eq_mkr_nm" type="12" value="" out="N"/>
				<param name="eq_mdl_nm" type="12" value="" out="N"/>
				<param name="fm_warr_dt" type="12" value="" out="N"/>
				<param name="to_warr_dt" type="12" value="" out="N"/>
				<param name="warr_rmk" type="12" value="" out="N"/>
				<param name="mft_yr" type="12" value="" out="N"/>
				<param name="lot_eq_pfx_cd" type="12" value="" out="N"/>
				<param name="to_ser_no" type="12" value="" out="N"/>
				<param name="fm_ser_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mnr_grp_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
