<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanManageDBDAOModifyForecastLandInvCSQL">
			<desc><![CDATA[<EES_EQR_0051 컨테이너 이송계획 관리>
EQR_ADD_PLN 테이블의 qty수정 혹은 신규입력

<Change History>
1	2009.08.17	Lee ByoungHun	최초작성]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_ADD_PLN I
USING
(
    SELECT 
    @[repo_pln_id] AS REPO_PLN_ID
    ,@[pln_yrwk] AS PLN_YRWK
    ,@[ecc_cd] AS ECC_CD
    ,@[cntr_tpsz_cd] AS CNTR_TPSZ_CD
    FROM DUAL
) M
ON
(
    I.REPO_PLN_ID     = M.REPO_PLN_ID
    AND I.PLN_YRWK = M.PLN_YRWK
    AND I.ECC_CD = M.ECC_CD
    AND I.CNTR_TPSZ_CD  = M.CNTR_TPSZ_CD
)
WHEN MATCHED THEN
    UPDATE
    SET I.CNTR_QTY    	= @[cntr_qty],
    I.UPD_USR_ID      	= @[upd_usr_id],
    I.UPD_DT    	  	= SYSDATE
WHEN NOT MATCHED THEN
    INSERT
    (
        I.REPO_PLN_ID
        ,I.PLN_YRWK
        ,I.ECC_CD
        ,I.CNTR_TPSZ_CD
        ,I.CNTR_QTY
        ,I.CRE_USR_ID
        ,I.CRE_DT
        ,I.UPD_USR_ID
        ,I.UPD_DT
    )
    VALUES 
    (
        @[repo_pln_id]
        , @[pln_yrwk]
        , @[ecc_cd]
        , @[cntr_tpsz_cd]
        , @[cntr_qty]
        , @[upd_usr_id]
        , SYSDATE
        , @[upd_usr_id]
        , SYSDATE
    )			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_qty" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
