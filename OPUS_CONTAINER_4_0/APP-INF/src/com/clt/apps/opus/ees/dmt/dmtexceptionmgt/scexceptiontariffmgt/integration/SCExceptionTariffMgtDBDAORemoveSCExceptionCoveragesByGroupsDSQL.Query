<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAORemoveSCExceptionCoveragesByGroupsDSQL">
			<desc><![CDATA[SC Group 정보가 삭제될 때, 그에 딸린 모든 Coverage 정보들을 함께 삭제하는 쿼리]]></desc>
			<sql><![CDATA[
DELETE FROM DMT_SC_EXPT_CVRG
WHERE	PROP_NO = @[prop_no]
	AND	SC_EXPT_VER_SEQ = @[sc_expt_ver_seq]
	AND	SC_EXPT_GRP_SEQ = @[sc_expt_grp_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
