<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmValidationDBDAOCheckOfficeDataRSQL">
			<desc><![CDATA[CgmValidationDB.CheckOfficeData]]></desc>
			<sql><![CDATA[
SELECT  A.OFC_CD,
		A.OFC_ENG_NM,
		A.OFC_LOCL_NM AS OFC_KRN_NM
FROM	MDM_ORGANIZATION A 
WHERE   A.OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
