<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CIMCommonDBDAOAddCntrTypeSizeDivCSQL">
			<desc><![CDATA[CIM Container Type Size Division Creation]]></desc>
			<sql><![CDATA[
INSERT INTO CIM_TP_SZ_DP_SEQ (
	DP_SEQ,
    CNTR_TPSZ_CD,
    CNTR_TPSZ_DIV_CD,
    CRE_OFC_CD,
    UPD_OFC_CD,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
) VALUES( 
    @[dp_seq],
    @[cntr_tpsz_cd],
	@[cntr_tpsz_div_cd],
    @[cre_ofc_cd],
	@[upd_ofc_cd],
	@[cre_usr_id],
	SYSDATE,
	@[upd_usr_id],
	SYSDATE
)			]]></sql>
			<params>
				<param name="dp_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_div_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
