<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationTypeMgtDBDAOAddCombinationSetCSQL">
			<desc><![CDATA[Combination Set 정보를 생성 한다.]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_CMB_SET (
    DMDT_CNTR_TP_CD, 
    DMDT_CGO_TP_CD, 
    CRE_USR_ID, 
    CRE_DT, 
    UPD_USR_ID, 
    UPD_DT
) VALUES (
    @[dmdt_cntr_tp_cd],
    @[dmdt_cgo_tp_cd],
    @[cre_usr_id],
    SYSDATE, 
    @[upd_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="dmdt_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="dmdt_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
