<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAOCheckTariffGroupRSQL">
			<desc><![CDATA[Basic Tariff Creation - Group]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM DMT_TRF_GRP A, DMT_TRF_CMB B
WHERE A.SYS_AREA_GRP_ID = B.SYS_AREA_GRP_ID
AND A.DMDT_TRF_CD = B.DMDT_TRF_CD
AND A.TRF_SEQ = B.TRF_SEQ
AND A.TRF_GRP_SEQ = B.TRF_GRP_SEQ
AND A.SYS_AREA_GRP_ID = @[svr_id]
AND A.DMDT_TRF_CD = @[dmdt_trf_cd]
AND A.TRF_SEQ = @[trf_seq]
AND A.TRF_GRP_SEQ = @[trf_grp_seq]
AND B.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd]
AND B.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd]			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="trf_seq" type="12" value="" out="N"/>
				<param name="trf_grp_seq" type="12" value="" out="N"/>
				<param name="dmdt_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="dmdt_cgo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
