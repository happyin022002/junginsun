<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MisUseApprovalDBDAOMisUseApprovalInfoCSQL">
			<desc><![CDATA[Miss Use Approval 승인내역을 등록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_MSS_USD_APRO (
    APRO_NO, APRO_OFC_CD, 
	APRO_DT, APRO_USR_ID, 
    APRO_RMK, CRE_USR_ID, 
	CRE_DT, UPD_USR_ID, UPD_DT
) VALUES (
    @[apro_no], @[apro_ofc_cd], 
	TO_DATE(@[apro_dt],'YYYYMMDD'), @[apro_usr_id], 
    @[apro_rmk], @[cre_usr_id], 
	SYSDATE, @[cre_usr_id], SYSDATE
)			]]></sql>
			<params>
				<param name="apro_no" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="apro_dt" type="12" value="" out="N"/>
				<param name="apro_usr_id" type="12" value="" out="N"/>
				<param name="apro_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
