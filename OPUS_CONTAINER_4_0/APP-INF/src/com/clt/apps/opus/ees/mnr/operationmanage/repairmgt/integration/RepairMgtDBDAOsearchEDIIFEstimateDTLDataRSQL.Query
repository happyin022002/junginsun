<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOsearchEDIIFEstimateDTLDataRSQL">
			<desc><![CDATA[searchEDIIFEstimateDTLData]]></desc>
			<sql><![CDATA[
SELECT
         A.RQST_EQ_NO
		,A.RPR_RQST_TMP_SEQ AS RPR_RQST_SEQ 
		,A.RPR_RQST_TMP_VER_NO AS RPR_RQST_VER_NO 
        ,A.RPR_RQST_LST_VER_FLG
        ,A.EQ_LOC_CD
        ,A.EQ_LOC_CD_CHK_FLG
        ,A.EQ_CMPO_CD
        ,A.EQ_CMPO_CD_CHK_FLG
        ,A.EQ_DMG_CD
        ,A.EQ_DMG_CD_CHK_FLG
        ,A.EQ_RPR_CD
        ,A.EQ_RPR_CD_CHK_FLG
        ,A.TRF_DIV_CD
        ,A.TRF_OPT_CD
        ,A.VOL_TP_CD
        ,A.RPR_QTY
        ,A.RPR_SZ_NO
        ,A.RPR_LEN_NO
        ,A.RPR_WDT_NO
        ,A.RPR_LBR_HRS
        ,A.RPR_LBR_RT
        ,A.RPR_LBR_BZC_HRS
        ,A.RPR_LBR_BZC_RT
        ,A.MNR_LBR_BZC_AMT
        ,A.LBR_MTRL_BZC_AMT
        ,A.LBR_COST_AMT
        ,A.MTRL_COST_AMT
        ,A.XCH_MTRL_COST_AMT
        ,A.MTRL_RECO_AMT
        ,A.MNR_LR_ACCT_FLG
        ,A.N3PTY_FLG
        ,A.N3PTY_BIL_LBR_HRS
        ,A.N3PTY_BIL_LBR_RT
        ,A.N3PTY_BIL_LBR_COST_AMT
        ,A.N3PTY_BIL_MTRL_COST_AMT
        ,A.MNR_AGMT_AMT
        ,A.MNR_WRK_AMT
        ,A.N3PTY_BIL_AMT
        ,A.RPR_DTL_RMK
        ,A.MNR_VRFY_TP_CD
        ,A.CRE_USR_ID
        ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
        ,A.UPD_USR_ID
        ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
		,A.COST_CD
        ,'NR' AS COST_DTL_CD
FROM MNR_RPR_RQST_TMP_DTL A
WHERE A.RQST_EQ_NO = @[rqst_eq_no]
AND   A.RPR_RQST_TMP_SEQ = @[rpr_rqst_seq]
AND   A.RPR_RQST_TMP_VER_NO = @[rpr_rqst_ver_no]			]]></sql>
			<params>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
				<param name="rpr_rqst_seq" type="12" value="" out="N"/>
				<param name="rpr_rqst_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
