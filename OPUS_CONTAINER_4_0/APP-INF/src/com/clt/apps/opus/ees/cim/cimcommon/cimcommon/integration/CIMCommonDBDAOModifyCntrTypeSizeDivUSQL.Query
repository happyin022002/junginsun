<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CIMCommonDBDAOModifyCntrTypeSizeDivUSQL">
			<desc><![CDATA[CIM Container Type Size Division Modify]]></desc>
			<sql><![CDATA[
UPDATE CIM_TP_SZ_DP_SEQ SET 
	CNTR_TPSZ_CD = @[cntr_tpsz_cd],
    CNTR_TPSZ_DIV_CD = @[cntr_tpsz_div_cd],
    UPD_OFC_CD = @[upd_ofc_cd],
	UPD_USR_ID = @[upd_usr_id],
	UPD_DT = SYSDATE
WHERE	DP_SEQ = @[dp_seq]
			]]></sql>
			<params>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_div_cd" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="dp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
