<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetLccSccForGateNewRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT G1.SCC_CD AS DEL_SCC,
       G1.LCC_CD AS DEL_LCC,
       G2.SCC_CD AS EVENT_SCC,
       G2.LCC_CD AS EVENT_LCC
  FROM DOM_BOOKING DB, MDM_LOCATION L1, MDM_EQ_ORZ_CHT G1,
       MDM_LOCATION L2, MDM_EQ_ORZ_CHT G2
 WHERE DB.DMST_BKG_NO = @[dmst_bkg_no]
   AND L1.LOC_CD = DB.DEST_RAIL_LOC_CD
   AND L1.SCC_CD = G1.SCC_CD
   AND L2.LOC_CD = @[loc_cd]
   AND L2.SCC_CD = G2.SCC_CD			]]></sql>
			<params>
				<param name="dmst_bkg_no" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
