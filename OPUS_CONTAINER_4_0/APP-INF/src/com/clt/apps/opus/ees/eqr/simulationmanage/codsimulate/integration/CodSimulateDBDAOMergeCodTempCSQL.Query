<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodSimulateDBDAOMergeCodTempCSQL">
			<desc><![CDATA[EQR_VSL_LDIS_PLN_COD_TMP 테이블의 특정 REPO PLAN ID 데이터가 존재하면  update, 존재하지 않으면 insert]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_VSL_LDIS_PLN_COD_TMP A                       
	USING 
	(SELECT                                            
	       @[repo_pln_id]  AS REPO_PLN_ID,                                
	       @[pln_yrwk]  AS PLN_YRWK,                                   
	       @[pln_seq]  AS PLN_SEQ                                
	       FROM DUAL 
     ) B                                     
	ON  (
         A.REPO_PLN_ID  = B.REPO_PLN_ID                 
	     AND  A.PLN_YRWK     = B.PLN_YRWK                    
	     AND  A.PLN_SEQ      = B.PLN_SEQ                     
	 )            
	WHEN MATCHED THEN                                        
	     UPDATE                                              
	     SET  A.UPD_USR_ID  = @[upd_usr_id] ,                             
	          A.UPD_DT      = sysdate
	--CSRNO : R200806027055의거변경 
	WHEN NOT MATCHED THEN                                      
	     INSERT                                                
	     (REPO_PLN_ID            
	      ,PLN_YRWK            
	      ,PLN_SEQ             
	      ,TRSP_MOD_CD         
	      ,VSL_LANE_CD         
	      ,VSL_CD              
	      ,SKD_VOY_NO          
	      ,SKD_DIR_CD          
	      ,FM_ECC_CD           
	      ,FM_ETD_DT 
          ,FM_YD_CD          
	      ,TO_ECC_CD           
	      ,TO_ETB_DT
          ,TO_YD_CD           
	      ,PAST_REPO_PLN_FLG   
	      ,PRE_PLN_TS_FLG     
	      ,CRE_USR_ID         
	      ,CRE_DT             
	      ,UPD_USR_ID         
	      ,UPD_DT             
	     )
	      VALUES 
	     (@[repo_pln_id]
	      ,@[pln_yrwk]
	      ,@[pln_seq1]
	      ,'V'
	      ,@[vsl_lane_cd]
	      ,@[vsl_cd]
	      ,@[skd_voy_no]
	      ,@[skd_dir_cd]
	      ,@[fm_ecc_cd]
	      ,TO_DATE(@[fm_etd_dt],'YYYY-MM-DD HH24:MI:SS')
          ,@[fm_yard]
	      ,@[to_ecc_cd]
	      ,TO_DATE(@[to_eta_dt],'YYYY-MM-DD HH24:MI:SS')
          ,@[to_yard]
	      ,@[past_repo_pln_flg]
	      ,@[pre_pln_ts_flg]
	      ,@[cre_usr_id]
	      ,sysdate
	      ,@[upd_usr_id]
	      ,sysdate
	      )			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="pln_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pln_seq1" type="12" value="" out="N"/>
				<param name="vsl_lane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="fm_etd_dt" type="12" value="" out="N"/>
				<param name="fm_yard" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="to_eta_dt" type="12" value="" out="N"/>
				<param name="to_yard" type="12" value="" out="N"/>
				<param name="past_repo_pln_flg" type="12" value="" out="N"/>
				<param name="pre_pln_ts_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
