<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchCostYardCodeDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT DISTINCT YD_CD
  FROM MNR_AGMT_RT
 WHERE AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
   AND AGMT_SEQ = @[agmt_seq]
   AND AGMT_VER_NO = @[agmt_ver_no]
   AND COST_CD = @[cost_cd]
   AND COST_DTL_CD = @[cost_dtl_cd]
   AND MNR_RT_TP_CD = @[mnr_rt_tp_cd]			]]></sql>
			<params>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
				<param name="cost_dtl_cd" type="12" value="" out="N"/>
				<param name="mnr_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
