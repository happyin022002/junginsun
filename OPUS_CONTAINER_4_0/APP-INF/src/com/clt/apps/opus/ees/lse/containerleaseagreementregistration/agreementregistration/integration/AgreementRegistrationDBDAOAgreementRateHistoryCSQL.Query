<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRegistrationDBDAOAgreementRateHistoryCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_AGMT_RT_HIS (
       AGMT_CTY_CD
     , AGMT_SEQ
     , AGMT_VER_SEQ
     , LOC_CD
     , CNTR_TPSZ_CD
     , CNTR_RNTL_CHG_TP_CD
     , AGMT_CHG_VAL
     , N1ST_CHG_AMT
     , N2ND_CHG_AMT
     , AGMT_CHG_DYS
     , CNTR_SPEC_NO
     , GEN_RMK
     , EQ_LOC_TP_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
)
SELECT @[agmt_cty_cd]  AS AGMT_CTY_CD
     , @[agmt_seq]     AS AGMT_SEQ
     , @[agmt_ver_seq] AS AGMT_VER_SEQ
     , LOC_CD
     , CNTR_TPSZ_CD
     , CNTR_RNTL_CHG_TP_CD
     , AGMT_CHG_VAL
     , N1ST_CHG_AMT
     , N2ND_CHG_AMT
     , AGMT_CHG_DYS
     , CNTR_SPEC_NO
     , GEN_RMK
     , EQ_LOC_TP_CD
     , @[usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM   LSE_AGMT_RT
WHERE  AGMT_CTY_CD = @[agmt_cty_cd]
AND    AGMT_SEQ = @[agmt_seq]			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="agmt_ver_seq" type="2" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
