<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOModifyCntrMasterByMNRFlaggingDataUSQL">
			<desc><![CDATA[modifyCntrMasterByMNRFlaggingData]]></desc>
			<sql><![CDATA[
UPDATE MST_CONTAINER A
SET   A.UPD_USR_ID 				= @[flag_user_id]
    , A.UPD_DT					= SYSDATE
    , A.CNTR_HNGR_BAR_ATCH_KNT	= DECODE(@[flag_type],'HGR',@[hb_count],A.CNTR_HNGR_BAR_ATCH_KNT)
    , A.CNTR_HNGR_RCK_CD	    = DECODE(@[flag_type],'HGR',@[hr_type],A.CNTR_HNGR_RCK_CD)
    , A.DMG_FLG					= DECODE(@[flag_type],'DMG',@[sts_flag],A.DMG_FLG)
    , A.DISP_FLG				= DECODE(@[flag_type],'DSP',@[sts_flag],A.DISP_FLG)
    , A.MNR_HNGR_BAR_TP_CD      = DECODE(@[flag_type],'HGR',@[hb_type],A.MNR_HNGR_BAR_TP_CD)
WHERE A.CNTR_NO = @[eq_no]			]]></sql>
			<params>
				<param name="flag_user_id" type="12" value="" out="N"/>
				<param name="flag_type" type="12" value="" out="N"/>
				<param name="hb_count" type="12" value="" out="N"/>
				<param name="hr_type" type="12" value="" out="N"/>
				<param name="sts_flag" type="12" value="" out="N"/>
				<param name="hb_type" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
