<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOAddEqrWkPrdCSQL">
			<desc><![CDATA[EQR의 주차 정보를 등록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO EQR_WK_PRD
(
    PLN_YR
   ,PLN_WK
   ,PLN_MON
   ,WK_ST_DT
   ,WK_END_DT
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
)
VALUES
(
    @[pln_yr]
   ,@[pln_wk]
   ,@[pln_mon]
   ,@[wk_st_dt]
   ,@[wk_end_dt]
   ,@[cre_usr_id] -- 로그인한 유저 ID
   ,SYSDATE
   ,@[upd_usr_id] -- 로그인한 유저 ID
   ,SYSDATE
)
			]]></sql>
			<params>
				<param name="pln_yr" type="12" value="" out="N"/>
				<param name="pln_wk" type="12" value="" out="N"/>
				<param name="pln_mon" type="12" value="" out="N"/>
				<param name="wk_st_dt" type="12" value="" out="N"/>
				<param name="wk_end_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
