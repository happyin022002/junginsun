<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOModifymanageVVDListUSQL">
			<desc><![CDATA[Update]]></desc>
			<sql><![CDATA[
UPDATE CTM_BKG_VVD
SET VSL_PRE_PST_CD = DECODE(@[vsl_pre_pst_cd], 'PRE', 'S', 'TRUNK', 'T', 'POST', 'U', 'T'),
    VSL_SEQ = @[vsl_seq],
    SLAN_CD = @[slan_cd],
    VSL_CD = SUBSTR(@[vvd], 1,4),
    SKD_VOY_NO = SUBSTR(@[vvd], 5,4),
    SKD_DIR_CD = SUBSTR(@[vvd], 9,1),
    POL_CD = SUBSTR(@[pol_cd], 1,5),
    POL_YD_CD = @[pol_cd],
    POD_CD = SUBSTR(@[pod_cd], 1,5),
    POD_YD_CD = @[pod_cd],
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT = SYSDATE
WHERE BKG_NO = @[bkg_no]
AND VSL_PRE_PST_CD = DECODE(@[vsl_pre_pst_cd], 'PRE', 'S', 'TRUNK', 'T', 'POST', 'U', 'T')
AND VSL_SEQ = @[vsl_seq]			]]></sql>
			<params>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
