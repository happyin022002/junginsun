<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOsearchEmptyCostShipmentDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT A.COST_CD, 
       A.COST_DTL_CD,
       A.EQ_NO, 
       TO_CHAR(A.RPR_RSLT_DT, 'YYYYMMDD') AS RPR_RSLT_DT, 
       B.EQ_KND_CD,
       A.MNR_ORD_OFC_CTY_CD,
       A.MNR_ORD_SEQ,
       A.ORD_DTL_SEQ
  FROM MNR_ORD_DTL A, MNR_ORD_HDR B
 WHERE 1=1
   AND A.MNR_ORD_OFC_CTY_CD = B.MNR_ORD_OFC_CTY_CD
   AND A.MNR_ORD_SEQ = B.MNR_ORD_SEQ
   AND A.COST_CD||A.COST_DTL_CD IN ( SELECT PRNT_CD_ID || MNR_CD_ID 
                                       FROM MNR_GEN_CD
                                      WHERE COST_SHP_SRCH_PATT_NM IS NOT NULL)
--   AND A.CRE_DT BETWEEN TO_DATE(TO_CHAR(TO_DATE(TO_CHAR(SYSDATE, 'YYYYMMDD'), 'YYYYMMDD') - 60, 'YYYYMMDD'), 'YYYYMMDD') AND TO_DATE(TO_CHAR(SYSDATE, 'YYYYMMDD'), 'YYYYMMDD') + 0.99999
   AND (A.BKG_NO IS NULL OR A.TRD_CD IS NULL)
   AND A.PAY_INV_SEQ IS NULL			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
