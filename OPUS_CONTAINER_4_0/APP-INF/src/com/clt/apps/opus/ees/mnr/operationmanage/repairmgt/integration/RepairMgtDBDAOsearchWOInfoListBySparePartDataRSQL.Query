<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOsearchWOInfoListBySparePartDataRSQL">
			<desc><![CDATA[work order history 조회]]></desc>
			<sql><![CDATA[
SELECT  A.COST_OFC_CD AS ORD_ISS_OFC_CD
	   ,A.SPR_PRT_SPL_TP_CD
	   ,A.VSL_CD || A.SKD_VOY_NO || A.SKD_DIR_CD AS VSL_VVD
	   ,A.SPR_PRT_SPL_YD_CD AS YD_CD
#if (${cost_ofc_cd} != '')
       ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(),A.CRE_DT, @[cost_ofc_cd]),'yyyy-mm-dd') AS CRE_DT
#else
	   ,TO_CHAR(A.CRE_DT,'YYYY-MM-DD') AS CRE_DT
#end
       ,(SELECT MNR_CD_DESC FROM MNR_GEN_CD WHERE PRNT_CD_ID= 'CD00009' AND MNR_CD_ID=B.SPR_PRT_UT_TP_NM AND ROWNUM=1) SPR_UT_TP_NM
	   ,B.SPR_PRT_NO
	   ,C.SPR_PRT_NM
	   ,B.RPR_QTY
	   ,A.CURR_CD
	   ,(A.MNR_ORD_OFC_CTY_CD || A.MNR_ORD_SEQ) AS MNR_ORD_SEQ
	   ,B.SPR_PRT_UC_AMT
	   ,B.SPR_PRT_UC_AMT * B.RPR_QTY AS TOTAL_AMT
FROM MNR_ORD_HDR A,MNR_ORD_DTL B, MNR_RF_SPR_PRT_CD C
WHERE A.MNR_ORD_OFC_CTY_CD= B.MNR_ORD_OFC_CTY_CD
  AND A.MNR_ORD_SEQ=B.MNR_ORD_SEQ
  AND B.SPR_PRT_NO=C.SPR_PRT_NO(+)
  AND A.MNR_GRP_TP_CD='RPR'
  AND A.MNR_WO_TP_CD='RFS'
#if (${cost_ofc_cd} != '')
AND A.COST_OFC_CD =@[cost_ofc_cd]
#end
#if (${fromcal} != '' && ${tocal} != '') 
#if (${cost_ofc_cd} != '')
AND B.CRE_DT BETWEEN GLOBALDATE_PKG.TIME_CONV_OFC_FNC(@[cost_ofc_cd],TO_DATE(@[fromcal], 'YYYY-MM-DD'),MNR_COMMON_PKG.MNR_GET_HOOFC_FNC()) AND GLOBALDATE_PKG.TIME_CONV_OFC_FNC(@[cost_ofc_cd],TO_DATE(@[tocal], 'YYYY-MM-DD'),MNR_COMMON_PKG.MNR_GET_HOOFC_FNC())+0.99999
#else
AND B.CRE_DT BETWEEN TO_DATE(@[fromcal], 'YYYY-MM-DD') AND TO_DATE(@[tocal], 'YYYY-MM-DD')+0.99999
#end
#end
#if (${vsl_cd} != '')
AND A.VSL_CD =@[vsl_cd]
#end
#if (${yd_cd} != '')
AND A.SPR_PRT_SPL_YD_CD=@[yd_cd]
#end
AND A.SPR_PRT_SPL_TP_CD = C.SPR_PRT_SPL_TP_CD			]]></sql>
			<params>
				<param name="cost_ofc_cd" type="12" value="" out="N"/>
				<param name="fromcal" type="12" value="" out="N"/>
				<param name="tocal" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
