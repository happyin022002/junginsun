<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOSearchYardOfcRSQL">
			<desc><![CDATA[SearchYardOfc]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
  FROM MDM_YARD
 WHERE 1=1
   AND SUBSTR(YD_CD, 0, 5) = SUBSTR(@[yd_cd], 0, 5)
   AND DELT_FLG != 'Y'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
