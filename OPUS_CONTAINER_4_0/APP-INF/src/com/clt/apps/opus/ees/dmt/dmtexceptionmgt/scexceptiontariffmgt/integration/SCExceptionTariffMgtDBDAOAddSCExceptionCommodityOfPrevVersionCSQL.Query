<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOAddSCExceptionCommodityOfPrevVersionCSQL">
			<desc><![CDATA[이전 버전의 모든 S/C Exception Tariff Commodity 정보를 신규 버전으로 생성하는 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_SC_EXPT_CMDT(
		PROP_NO
	,	SC_EXPT_VER_SEQ
	,	SC_EXPT_GRP_SEQ
	,	CMDT_CD
	,	CRE_USR_ID
	,	CRE_DT
	,	UPD_USR_ID
	,	UPD_DT
	)
SELECT	PROP_NO
	,	@[sc_expt_ver_seq]
	,	SC_EXPT_GRP_SEQ
	,	CMDT_CD
	,	@[cre_usr_id]
	,	SYSDATE
	,	@[cre_usr_id]
	,	SYSDATE
FROM	DMT_SC_EXPT_CMDT
WHERE	PROP_NO 		= @[prop_no]
	AND	SC_EXPT_VER_SEQ = @[sc_expt_prev_ver_seq]			]]></sql>
			<params>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_prev_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
