<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOPayableChargeAuditRejectInvoiceUSQL">
			<desc><![CDATA[Charge Creation Reject 시 Invoice File Data 를 원상태로 Update.]]></desc>
			<sql><![CDATA[
UPDATE LSE_PAY_RNTL_CHG_CO
SET    LSE_PAY_CHG_TP_NM = LSE_PAY_CHG_TP_CD
     , UPD_USR_ID = @[usr_id]
     , UPD_DT     = SYSDATE
WHERE  CO_COST_YRMON = @[chg_cost_yrmon]
AND    AGMT_CTY_CD   = @[agmt_cty_cd]
AND    AGMT_SEQ      = @[agmt_seq]
AND    LSE_PAY_CHG_TP_CD <> LSE_PAY_CHG_TP_NM			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="chg_cost_yrmon" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
