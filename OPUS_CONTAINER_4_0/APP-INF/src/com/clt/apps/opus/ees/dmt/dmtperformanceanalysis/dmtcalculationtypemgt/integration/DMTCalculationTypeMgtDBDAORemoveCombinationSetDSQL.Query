<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationTypeMgtDBDAORemoveCombinationSetDSQL">
			<desc><![CDATA[Combination Set 정보를 생성 한다.]]></desc>
			<sql><![CDATA[
DELETE
  FROM DMT_CMB_SET
 WHERE DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd]
   AND DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd]			]]></sql>
			<params>
				<param name="dmdt_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="dmdt_cgo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
