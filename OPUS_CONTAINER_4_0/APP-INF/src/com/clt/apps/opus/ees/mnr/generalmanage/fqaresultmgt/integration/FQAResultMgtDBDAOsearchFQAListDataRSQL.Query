<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FQAResultMgtDBDAOsearchFQAListDataRSQL">
			<desc><![CDATA[FQAResultMgtDBDAOsearchFQAListDataRSQL]]></desc>
			<sql><![CDATA[
SELECT
	MNR_COMMON_PKG.MNR_CONV_PARTNER_CD_FNC(A.VNDR_SEQ) VNDR_SEQ,
	MAX(A.OFC_CD) OFC_CD,
	TO_CHAR(A.FLD_AUD_DT,'YYYY-MM-DD') FLD_AUD_DT,
	TRIM(MAX(A.FLD_AUD_DTL_SEQ)) FLD_AUD_DTL_SEQ,
	A.YD_CD,
	TRIM(MAX(A.MAX_PNT_NO)) MAX_PNT_NO,
	TRIM(SUM(A.PNT_NO)) PNT_NO,
	DECODE(MAX(A.PNT_CALC_FLG),NULL,'N',MAX(A.PNT_CALC_FLG))  PNT_CALC_FLG,
	MAX(A.FLD_AUD_RMK) FLD_AUD_RMK,
	MAX(A.FILE_SEQ) FILE_SEQ,
	MAX(A.CRE_USR_ID) CRE_USR_ID,
	TRIM(MAX(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.CRE_DT, @[ofc_cd]))) CRE_DT,
	MAX(A.UPD_USR_ID) UPD_USR_ID,
	TRIM(MAX(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.UPD_DT, @[ofc_cd]))) UPD_DT
FROM MNR_FLD_QLTY_AUD_RSLT A
WHERE  1=1
#if (${ofc_cd} != '')
	AND A.OFC_CD = @[ofc_cd]
#end
#if (${vndr_seq} != '')
	AND A.VNDR_SEQ = TO_NUMBER(@[vndr_seq])
#end
#if(${ar_hd_qtr_cd} != '')
   AND @[ar_hd_qtr_cd] = (SELECT AR_HD_QTR_OFC_CD
                            FROM MDM_ORGANIZATION 
                           WHERE OFC_CD = A.OFC_CD)
#end
GROUP BY VNDR_SEQ, YD_CD, FLD_AUD_DT			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="ar_hd_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
