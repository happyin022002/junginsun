<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchRFAByPropRSQL">
			<desc><![CDATA[Proposal No. 에 해당되는 RFA No. 를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	RFA_NO
FROM	PRI_RP_HDR
WHERE	PROP_NO = @[prop_no]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
