<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyVerifyLocalTariffFileListByCountDataUSQL">
			<desc><![CDATA[Verify - [EES_MNR_0190] 수정된 Local Tariff Data 의 Count 와 해당 Standard Tariff Data 의 Count 를 Verify 한다.]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
   SET A.INP_MSG4 = 'CU'
 WHERE A.TMP_SEQ =@[tmp_seq]
   AND (SELECT COUNT(*) FROM MNR_DAT_VRFY WHERE TMP_SEQ = @[tmp_seq])
        <>
       (SELECT COUNT(*) FROM MNR_RPR_TRF_DTL WHERE TRF_NO = @[std_trf_no])			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
				<param name="std_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
