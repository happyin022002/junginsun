<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RateMgtDBDAOaddAGMTRateDataCSQL">
			<desc><![CDATA[add AGMT Rate]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_AGMT_RT (
	AGMT_SEQ
,	AGMT_VER_NO
,	COST_CD
,	COST_DTL_CD
,	MNR_RT_TP_CD
,	RPR_QTY
,	AGMT_RT_AMT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	AGMT_OFC_CTY_CD
,   USR_DEF_RMK
,   YD_CD
,   AGMT_RT_SEQ
) VALUES( 
	@[agmt_seq]
,	@[agmt_ver_no]
,	@[cost_cd]
,	@[cost_dtl_cd]
,	@[mnr_rt_tp_cd]
,	NVL(@[rpr_qty],0)  
,	NVL(@[agmt_rt_amt],0)  
,	@[cre_usr_id]
,	sysdate
,	@[upd_usr_id]
,	sysdate
,	@[agmt_ofc_cty_cd]
,   @[usr_def_rmk]
,   @[yd_cd]
,  (SELECT NVL(MAX(AGMT_RT_SEQ), 0)+1
      FROM MNR_AGMT_RT
     WHERE AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
       AND AGMT_SEQ = @[agmt_seq]
       AND AGMT_VER_NO = @[agmt_ver_no])
)			]]></sql>
			<params>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="agmt_ver_no" type="2" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
				<param name="cost_dtl_cd" type="12" value="" out="N"/>
				<param name="mnr_rt_tp_cd" type="12" value="" out="N"/>
				<param name="rpr_qty" type="2" value="" out="N"/>
				<param name="agmt_rt_amt" type="6" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="usr_def_rmk" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
