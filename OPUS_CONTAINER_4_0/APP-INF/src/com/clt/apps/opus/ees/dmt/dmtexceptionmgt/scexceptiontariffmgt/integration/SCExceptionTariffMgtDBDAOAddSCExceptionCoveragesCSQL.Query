<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOAddSCExceptionCoveragesCSQL">
			<desc><![CDATA[S/C Exception Terms Entry 에 대한  Multi Coverage 입력용 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_SC_EXPT_CVRG (
	PROP_NO
,	SC_EXPT_VER_SEQ
,	SC_EXPT_GRP_SEQ
,	CVRG_SEQ
,	CNT_CD
,	RGN_CD
,	STE_CD
,	LOC_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[prop_no]
,	@[sc_expt_ver_seq]
,	@[sc_expt_grp_seq]
,	@[cvrg_seq]
,	NVL(@[cnt_cd], ' ')
,	NVL(@[rgn_cd], ' ')
,	NVL(@[ste_cd], ' ')
,	NVL(@[loc_cd], ' ')
,	@[cre_usr_id]
,	SYSDATE
,	@[cre_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="12" value="" out="N"/>
				<param name="cvrg_seq" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
