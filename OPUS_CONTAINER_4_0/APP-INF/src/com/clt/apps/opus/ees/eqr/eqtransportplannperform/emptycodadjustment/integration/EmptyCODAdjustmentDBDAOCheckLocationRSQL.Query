<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyCODAdjustmentDBDAOCheckLocationRSQL">
			<desc><![CDATA[checkLocation]]></desc>
			<sql><![CDATA[
#if (${locLevel} == 'Y')
SELECT *
FROM  MDM_YARD
WHERE YD_CD = @[locCD]

#elseif (${locLevel} == 'O' || ${locLevel} == 'C' || ${locLevel} == 'P')

SELECT * 
FROM    MDM_LOCATION
WHERE   LOC_CD          LIKE  @[locCD]||'%'
AND     PORT_INLND_FLG   =   DECODE( @[locLevel], 'P', 'Y', PORT_INLND_FLG )  
AND    ROWNUM =1

#else

SELECT *
FROM    MDM_EQ_ORZ_CHT
WHERE  	DECODE ( @[locLevel] ,  'S',  SCC_CD	,  /* SCC */ 
	                          'E',  ECC_CD	,  /* ECC */
	                          'L',  LCC_CD	,  /* LCC */
 		                            RCC_CD   ) /* RCC */
	       =	@[locCD]
	AND    ROWNUM =1

#end			]]></sql>
			<params>
				<param name="locCD" type="12" value="" out="N"/>
				<param name="locLevel" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
