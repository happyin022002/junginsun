<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOSearchCHSAgreementAgmtSeqDataRSQL">
			<desc><![CDATA[ChassisMgsetAgreementDB.SearchCHSAgreementAgmtSeqData]]></desc>
			<sql><![CDATA[
SELECT CASE
          WHEN NVL(MAX(AGMT_SEQ), 0) + 1 = 999998
          THEN 1000000
          ELSE NVL(MAX(AGMT_SEQ), 0) + 1
       END AS AGMT_SEQ
  FROM CGM_AGREEMENT
WHERE AGMT_SEQ NOT IN (999999, 999998)			]]></sql>
