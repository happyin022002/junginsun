<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOIsStateCodeRSQL">
			<desc><![CDATA[주어진 코드가 State 코드인지를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT  COUNT(*)
FROM    MDM_STATE
WHERE   CNT_CD IN ('CA', 'US') AND STE_CD = @[ste_cd]			]]></sql>
			<params>
				<param name="ste_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
