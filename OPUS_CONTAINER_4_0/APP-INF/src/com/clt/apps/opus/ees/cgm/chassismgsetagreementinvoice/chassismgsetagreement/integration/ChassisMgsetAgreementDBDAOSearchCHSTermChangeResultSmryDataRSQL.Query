<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOSearchCHSTermChangeResultSmryDataRSQL">
			<desc><![CDATA[ChassisMgsetAgreementDB.SearchCHSTermChangeResultSmryData]]></desc>
			<sql><![CDATA[
SELECT 
       T1.STS_EVNT_OFC_CD, 
       MIN(T2.VNDR_SEQ) AS OLD_VNDR_SEQ, 
       T2.AGMT_OFC_CTY_CD || LPAD(T2.AGMT_SEQ,6,'0') AS OLD_AGMT_NO,
       MIN(T2.AGMT_LSTM_CD) AS OLD_AGMT_LSTM_CD,
       MIN(T3.VNDR_SEQ) AS NEW_VNDR_SEQ, 
       T3.AGMT_OFC_CTY_CD || LPAD(T3.AGMT_SEQ,6,'0') AS NEW_AGMT_NO,
       MIN(T3.AGMT_LSTM_CD) AS NEW_AGMT_LSTM_CD,
		NVL(SUM(EQ_TPSZ_CD1),0) AS EQ_TPSZ_CD1,
		NVL(SUM(EQ_TPSZ_CD2),0) AS EQ_TPSZ_CD2,
		NVL(SUM(EQ_TPSZ_CD3),0) AS EQ_TPSZ_CD3,
		NVL(SUM(EQ_TPSZ_CD4),0) AS EQ_TPSZ_CD4,
		NVL(SUM(EQ_TPSZ_CD5),0) AS EQ_TPSZ_CD5,
		NVL(SUM(EQ_TPSZ_CD6),0) AS EQ_TPSZ_CD6,
		NVL(SUM(EQ_TPSZ_CD7),0) AS EQ_TPSZ_CD7,
		NVL(SUM(EQ_TPSZ_CD8),0) AS EQ_TPSZ_CD8,
		NVL(SUM(EQ_TPSZ_CD9),0) AS EQ_TPSZ_CD9,
		NVL(SUM(EQ_TPSZ_CD10),0) AS EQ_TPSZ_CD10,
		NVL(SUM(EQ_TPSZ_CD11),0) AS EQ_TPSZ_CD11,
		NVL(SUM(EQ_TPSZ_CD12),0) AS EQ_TPSZ_CD12,
		NVL(SUM(EQ_TPSZ_CD13),0) AS EQ_TPSZ_CD13,
		NVL(SUM(EQ_TPSZ_CD14),0) AS EQ_TPSZ_CD14,
		NVL(SUM(EQ_TPSZ_CD15),0) AS EQ_TPSZ_CD15,
		NVL(SUM(EQ_TPSZ_CD16),0) AS EQ_TPSZ_CD16,
		NVL(SUM(EQ_TPSZ_CD17),0) AS EQ_TPSZ_CD17,
		NVL(SUM(EQ_TPSZ_CD18),0) AS EQ_TPSZ_CD18,
		NVL(SUM(EQ_TPSZ_CD19),0) AS EQ_TPSZ_CD19,
		NVL(SUM(EQ_TPSZ_CD20),0) AS EQ_TPSZ_CD20
FROM
    ( 
    SELECT A.TERM_CNG_SEQ,
           MIN(A.EQ_NO) AS EQ_NO,
           MIN(DECODE(A.EQ_ASET_STS_CD, 'LSO', A.AGMT_OFC_CTY_CD)) AS OLD_AGMT_OFC_CTY_CD,
           MIN(DECODE(A.EQ_ASET_STS_CD, 'LSO', A.AGMT_SEQ))        AS OLD_AGMT_SEQ,
           MIN(DECODE(A.EQ_ASET_STS_CD, 'LSI', A.AGMT_OFC_CTY_CD)) AS NEW_AGMT_OFC_CTY_CD,
           MIN(DECODE(A.EQ_ASET_STS_CD, 'LSI', A.AGMT_SEQ))        AS NEW_AGMT_SEQ,
           MIN(A.STS_EVNT_OFC_CD) AS STS_EVNT_OFC_CD,
	 	   MIN(B.EQ_TPSZ_CD)      AS EQ_TPSZ_CD,
			MIN(CASE WHEN G.DP_SEQ = '1' THEN 1 END) AS EQ_TPSZ_CD1,
			MIN(CASE WHEN G.DP_SEQ = '2' THEN 1 END) AS EQ_TPSZ_CD2,
			MIN(CASE WHEN G.DP_SEQ = '3' THEN 1 END) AS EQ_TPSZ_CD3,
			MIN(CASE WHEN G.DP_SEQ = '4' THEN 1 END) AS EQ_TPSZ_CD4,
			MIN(CASE WHEN G.DP_SEQ = '5' THEN 1 END) AS EQ_TPSZ_CD5,
			MIN(CASE WHEN G.DP_SEQ = '6' THEN 1 END) AS EQ_TPSZ_CD6,
			MIN(CASE WHEN G.DP_SEQ = '7' THEN 1 END) AS EQ_TPSZ_CD7,
			MIN(CASE WHEN G.DP_SEQ = '8' THEN 1 END) AS EQ_TPSZ_CD8,
			MIN(CASE WHEN G.DP_SEQ = '9' THEN 1 END) AS EQ_TPSZ_CD9,
			MIN(CASE WHEN G.DP_SEQ = '10' THEN 1 END) AS EQ_TPSZ_CD10,
			MIN(CASE WHEN G.DP_SEQ = '11' THEN 1 END) AS EQ_TPSZ_CD11,
			MIN(CASE WHEN G.DP_SEQ = '12' THEN 1 END) AS EQ_TPSZ_CD12,
			MIN(CASE WHEN G.DP_SEQ = '13' THEN 1 END) AS EQ_TPSZ_CD13,
			MIN(CASE WHEN G.DP_SEQ = '14' THEN 1 END) AS EQ_TPSZ_CD14,
			MIN(CASE WHEN G.DP_SEQ = '15' THEN 1 END) AS EQ_TPSZ_CD15,
			MIN(CASE WHEN G.DP_SEQ = '16' THEN 1 END) AS EQ_TPSZ_CD16,
			MIN(CASE WHEN G.DP_SEQ = '17' THEN 1 END) AS EQ_TPSZ_CD17,
			MIN(CASE WHEN G.DP_SEQ = '18' THEN 1 END) AS EQ_TPSZ_CD18,
			MIN(CASE WHEN G.DP_SEQ = '19' THEN 1 END) AS EQ_TPSZ_CD19,
			MIN(CASE WHEN G.DP_SEQ = '20' THEN 1 END) AS EQ_TPSZ_CD20
    FROM CGM_EQ_STS_HIS A, CGM_EQUIPMENT B,
		(SELECT EQ_TPSZ_CD,ROW_NUMBER() OVER (ORDER BY EQ_TPSZ_REP_CD,EQ_TPSZ_CD) DP_SEQ 
  		   FROM CGM_EQ_TP_SZ WHERE EQ_KND_CD = 'Z' GROUP BY EQ_TPSZ_REP_CD,EQ_TPSZ_CD) G
    WHERE A.EQ_NO = B.EQ_NO
		  AND B.EQ_TPSZ_CD = G.EQ_TPSZ_CD
          AND A.EQ_KND_CD = @[eq_knd_cd]
          AND A.TERM_CNG_SEQ > 0
		  AND A.STS_EVNT_DT >= TO_DATE(@[sts_evnt_dt_fr],'YYYYMMDD')
          AND A.STS_EVNT_DT < TO_DATE(@[sts_evnt_dt_to],'YYYYMMDD') + 1
#if (${sts_evnt_ofc_cd} != '') 
		  AND A.STS_EVNT_OFC_CD IN ($sts_evnt_ofc_cd)
#end
    GROUP BY A.TERM_CNG_SEQ
    ) T1,
    CGM_AGREEMENT T2,
    CGM_AGREEMENT T3
WHERE
    T1.OLD_AGMT_OFC_CTY_CD = T2.AGMT_OFC_CTY_CD(+)
    AND T1.OLD_AGMT_SEQ = T2.AGMT_SEQ(+)
    AND T1.NEW_AGMT_OFC_CTY_CD = T3.AGMT_OFC_CTY_CD(+)
    AND T1.NEW_AGMT_SEQ = T3.AGMT_SEQ(+)
    AND T2.LST_VER_FLG(+) = 'Y'
    AND T3.LST_VER_FLG(+) = 'Y'
#if (${vndr_seq} != '') 
	AND (T2.VNDR_SEQ IN ($vndr_seq) OR T3.VNDR_SEQ IN ($vndr_seq))
#end
#if (${old_agmt_lstm_cd} != '') 
    AND T2.AGMT_LSTM_CD IN ($old_agmt_lstm_cd)
#end
#if (${new_agmt_lstm_cd} != '') 
    AND T3.AGMT_LSTM_CD IN ($new_agmt_lstm_cd)
#end
GROUP BY T1.STS_EVNT_OFC_CD, 
         T2.AGMT_OFC_CTY_CD, 
         T2.AGMT_SEQ,
         T3.AGMT_OFC_CTY_CD, 
         T3.AGMT_SEQ
ORDER BY T1.STS_EVNT_OFC_CD, 
         T2.AGMT_OFC_CTY_CD, 
         T2.AGMT_SEQ,
         T3.AGMT_OFC_CTY_CD, 
         T3.AGMT_SEQ			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="sts_evnt_dt_fr" type="12" value="" out="N"/>
				<param name="sts_evnt_dt_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
