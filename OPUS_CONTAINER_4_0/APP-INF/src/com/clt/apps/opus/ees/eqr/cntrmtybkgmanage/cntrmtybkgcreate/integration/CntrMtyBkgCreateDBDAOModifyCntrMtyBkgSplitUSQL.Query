<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrMtyBkgCreateDBDAOModifyCntrMtyBkgSplitUSQL">
			<desc><![CDATA[Multi Cod Update]]></desc>
			<sql><![CDATA[
UPDATE EQR_VSL_EXE_PLN_QTY EPQ
SET CNTR_QTY = ( SELECT COUNT(BC.CNTR_NO)
                         FROM BKG_CONTAINER BC
                          ,   EQR_VSL_LODG_DCHG_EXE_PLN EP
                        WHERE  EP.MTY_BKG_NO    = @[vl_bkg_no]
                           AND EP.REPO_PLN_ID   = EPQ.REPO_PLN_ID
                           AND EP.PLN_YRWK      = EPQ.PLN_YRWK
                           AND EP.PLN_SEQ       = EPQ.PLN_SEQ
                           AND EP.REF_ID        = EPQ.REF_ID
                           AND EP.MTY_BKG_NO    = BC.BKG_NO
                           AND EPQ.CNTR_TPSZ_CD = BC.CNTR_TPSZ_CD
                        GROUP BY EPQ.REPO_PLN_ID, EPQ.PLN_YRWK, EPQ.PLN_SEQ, EPQ.REF_ID, EPQ.CNTR_TPSZ_CD)
WHERE EXISTS  ( SELECT 'X'
                         FROM BKG_CONTAINER BC
                          ,   EQR_VSL_LODG_DCHG_EXE_PLN EP
                        WHERE  EP.MTY_BKG_NO    = @[vl_bkg_no]
                           AND EP.REPO_PLN_ID   = EPQ.REPO_PLN_ID
                           AND EP.PLN_YRWK      = EPQ.PLN_YRWK
                           AND EP.PLN_SEQ       = EPQ.PLN_SEQ
                           AND EP.REF_ID        = EPQ.REF_ID
                           AND EP.MTY_BKG_NO    = BC.BKG_NO
                           AND EPQ.CNTR_TPSZ_CD = BC.CNTR_TPSZ_CD
                        GROUP BY EPQ.REPO_PLN_ID, EPQ.PLN_YRWK, EPQ.PLN_SEQ, EPQ.REF_ID, EPQ.CNTR_TPSZ_CD)			]]></sql>
			<params>
				<param name="vl_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
