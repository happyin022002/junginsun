<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchInvArIfCntrDataRSQL">
			<desc><![CDATA[InterfaceMgtDBDAOsearchInvArIfCntrDataRSQL]]></desc>
			<sql><![CDATA[
SELECT 
		TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), SYSDATE, @[user_ofc_cd]), 'yyyymmdd') AS UPD_DT,
		'' AS SRC_IF_SEQ,
		MRW.CRE_USR_ID,
		MDD.EQ_NO AS CNTR_NO,
		MDD.EQ_TPSZ_CD AS CNTR_TPSZ_CD,
		'' AS CNTR_SEQ,
		TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), SYSDATE, @[user_ofc_cd]), 'yyyymmdd') AS CRE_DT,
		'' AS SRC_IF_DT,
		MRW.UPD_USR_ID
FROM MNR_RCV_INV_WRK MRW, MNR_DISP_DTL MDD
WHERE   MRW.RCV_INV_SEQ = MDD.RCV_INV_SEQ
 AND   MRW.RCV_INV_SEQ = @[rcv_inv_seq]			]]></sql>
			<params>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="rcv_inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
