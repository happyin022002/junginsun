<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireBalanceDBDAOmodifyOnhireStatusUSQL">
			<desc><![CDATA[UPDATE EQR_CTRL_ONH_ORD]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_CTRL_ONH_ORD
USING DUAL   
ON (     ONH_ORD_YR  = @[onh_ord_yr]               
     AND LCC_CD      = @[lcc_cd]                   
     AND EQ_LSTM_CD  = @[eq_lstm_cd]               
     AND LSE_PRD_SEQ = @[lse_prd_seq] )
WHEN MATCHED THEN
     UPDATE SET ONH_ORD_RMK = @[onh_ord_rmk]
              , UPD_USR_ID  = @[upd_usr_id] 
              , UPD_DT      = SYSDATE  
WHEN NOT MATCHED THEN     
     INSERT ( ONH_ORD_YR
            , LCC_CD
            , EQ_LSTM_CD
            , LSE_PRD_SEQ
            , RCC_CD
            , ONH_ORD_RMK
            , CRE_USR_ID
            , CRE_DT
            , UPD_USR_ID
            , UPD_DT )
     VALUES ( @[onh_ord_yr]
            , @[lcc_cd]
            , @[eq_lstm_cd]
            , @[lse_prd_seq]
            , @[rcc_cd]
            , @[onh_ord_rmk]
            , @[cre_usr_id]
            , SYSDATE
            , @[upd_usr_id]
            , SYSDATE )			]]></sql>
			<params>
				<param name="onh_ord_yr" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="eq_lstm_cd" type="12" value="" out="N"/>
				<param name="lse_prd_seq" type="12" value="" out="N"/>
				<param name="onh_ord_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
