<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanManageDBDAOSearchCntrOnHireRepoPlanDtRSQL">
			<desc><![CDATA[<EES_EQR_0053 컨테이너 수급 예측실적 및 정확도 조회>
EQR_ONF_HIR_PLN 테이블에서 데이터 조회

<Change History>
1	2009.09.03	Lee ByoungHun	최초작성
2	2010.01.11	Lee ByoungHun	TypeBy가 ECC 이외의 경우에는 PLN_SEQ를 Group By에서 제외한다.]]></desc>
			<sql><![CDATA[
SELECT
    PLN_YRWK
    , ECC_CD
    , CNTR_LSTM_CD
    #foreach ($key in ${arrCntrTpszCd})
        , SUM(DECODE ( CNTR_TPSZ_CD , '$key' , CNTR_QTY ))    ${key}CNTR_QTY
        , SUM(DECODE ( CNTR_TPSZ_CD , '$key' , ONF_HIR_COST_AMT))  ${key}ONF_HIR_COST_AMT
    #end
    , REPO_PLN_ID
    , ONF_HIR_DIV_CD
    , MAX(DECODE(CRE_DT,UPD_DT,'N','Y')) AS TIMEGAP
	-- ECC이외의 경우에는 PLN_SEQ를 Group By 에서 제외한다.
	#if (${typeby_3} == 'E')
		, PLN_SEQ
	#else
		, 0 PLN_SEQ
	#end
FROM (
    SELECT
        REPO_PLN_ID
        , PLN_YRWK
        , ONF_HIR_DIV_CD
        , (
            SELECT
                #if (${typeby_3} == 'R')
                    RCC_CD
                #elseif (${typeby_3} == 'C')
                    CNT_CD
                #elseif (${typeby_3} == 'L')
                    LCC_CD
                #else
                    ECC_CD
                #end
            FROM
                EQR_ECC_MST
            WHERE
                E.ECC_CD = ECC_CD
        ) ECC_CD
        , CNTR_LSTM_CD
        , CNTR_TPSZ_CD
        , CNTR_QTY
        , ONF_HIR_COST_AMT
        , UPD_USR_ID
        , UPD_DT
        , CRE_DT
		, PLN_SEQ
        , ROW_NUMBER() OVER (PARTITION BY PLN_YRWK,ECC_CD, CNTR_LSTM_CD, CNTR_TPSZ_CD ORDER BY ROWNUM) RUMM
    FROM (
        SELECT
            PLN.REPO_PLN_ID
            , PLN.PLN_YRWK
            , ONF_HIR_DIV_CD
            , PLN.ECC_CD ECC_CD
            , CNTR_LSTM_CD
            , CNTR_TPSZ_CD
            , CNTR_QTY
            , ONF_HIR_COST_AMT
            , QTY.UPD_USR_ID
            , QTY.UPD_DT
            , QTY.CRE_DT
			, QTY.PLN_SEQ
        FROM
            EQR_ONF_HIR_PLN PLN
			, EQR_ONF_HIR_PLN_QTY QTY
            #if (${type_3} != '' && $arrAtEccCd.size() > 0)
                , (SELECT ECC_CD FROM EQR_ECC_MST WHERE 1 = 1
                #if (${type_3} == 'R')
                    AND RCC_CD
                #elseif (${type_3} == 'L')
                    AND LCC_CD
                #else
                    AND ECC_CD
                #end
                IN(
                #foreach ($key in ${arrAtEccCd})
                    #if($velocityCount < $arrAtEccCd.size())
                    '$key',
                    #else
                    '$key'
                    #end
                #end
                ))C
            #end
        WHERE
			PLN.REPO_PLN_ID = QTY.REPO_PLN_ID
			AND PLN.PLN_YRWK = QTY.PLN_YRWK
			AND PLN.PLN_SEQ = QTY.PLN_SEQ
            AND PLN.REPO_PLN_ID = @[repo_pln_Id]
            AND ONF_HIR_DIV_CD = 'O'
            #if (${term_3} != '')
                AND CNTR_LSTM_CD = @[term_3]
            #end
            #if (${type_3} != '')
                AND (PLN.ECC_CD = C.ECC_CD)
            #end
            AND PLN.PLN_YRWK BETWEEN '${fmplnyr_3}${fmplnwk_3}' AND '${toplnyr_3}${toplnwk_3}'
            #if ($arrCntrTpszCd.size() > 0)
                AND CNTR_TPSZ_CD IN(
                #foreach ($key in ${arrCntrTpszCd})
                    #if($velocityCount < $arrCntrTpszCd.size())
                    '$key',
                    #else
                    '$key'
                    #end
                #end
                )
            #end
        ORDER BY
            PLN_YRWK DESC
    ) E
)
GROUP BY
    RUMM 
	, PLN_YRWK
	, ECC_CD
	, CNTR_LSTM_CD 
	, REPO_PLN_ID
	, ONF_HIR_DIV_CD
	-- ECC이외의 경우에는 PLN_SEQ를 Group By 에서 제외한다.
	#if (${typeby_3} == 'E')
		, PLN_SEQ
	#end
ORDER BY
    PLN_YRWK,  ECC_CD			]]></sql>
			<params>
				<param name="repo_pln_Id" type="12" value="" out="N"/>
				<param name="term_3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
