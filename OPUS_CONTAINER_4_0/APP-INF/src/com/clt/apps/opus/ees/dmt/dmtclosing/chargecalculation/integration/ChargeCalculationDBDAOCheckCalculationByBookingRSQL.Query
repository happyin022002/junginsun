<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOCheckCalculationByBookingRSQL">
			<desc><![CDATA[ChargeCalculationDBDAOCheckCalculationByBookingRSQL]]></desc>
			<sql><![CDATA[
SELECT ( SELECT DECODE(COUNT(*),0,'Y','N')
           FROM DMT_MNL_BAT_HIS
            WHERE 1=1
            AND BKG_NO = @[bkg_no]
            AND DMDT_TRF_CD = @[dmdt_trf_cd]
            AND BAT_RUN_TM_ID = @[bat_run_tm_id]
			AND BAT_RSLT_FLG = 'N' )
       ||'|'||
	   NVL(
	   BKG_JOIN_FULL_CLOB_FNC(
            CURSOR(
            SELECT DECODE(BAT_RSLT_FLG,'Y',BAT_RSLT_RMK,'E','Manual Batch System Error','')||' - COUNT : '||COUNT(*)
             FROM DMT_MNL_BAT_HIS
            WHERE 1=1
            AND BKG_NO = @[bkg_no]
            AND DMDT_TRF_CD = @[dmdt_trf_cd]
            AND BAT_RUN_TM_ID = @[bat_run_tm_id]
            AND BAT_RSLT_FLG IN ( 'Y', 'E' )
            AND BAT_RSLT_RMK IS NOT NULL
            GROUP BY DECODE(BAT_RSLT_FLG,'Y',BAT_RSLT_RMK,'E','Manual Batch System Error','')
            ), CHR(10)
        ),' ')||'|'
FROM DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="bat_run_tm_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
