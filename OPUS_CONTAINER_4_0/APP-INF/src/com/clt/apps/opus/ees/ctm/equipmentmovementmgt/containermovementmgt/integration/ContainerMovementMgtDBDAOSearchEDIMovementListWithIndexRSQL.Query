<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchEDIMovementListWithIndexRSQL">
			<desc><![CDATA[SearchEDIMovementListWithIndex]]></desc>
			<sql><![CDATA[
SELECT BKG_NO,
       BL_NO,
       CALL_SGN_LLOYD,
       CALL_SGN_NO,
       LLOYD_NO,
       CHSS_NO,
       CNTR_FULL_STS_CD,
       CNTR_NO,
       CNTR_SEAL_NO,
       CNTR_TPSZ_CD,
       CRE_LOCL_DT,
       VVD_CD,
       CRNT_VSL_CD, 
       CRNT_SKD_VOY_NO,
       CRNT_SKD_DIR_CD,
       DEST_YD_CD,
       POL_CD,
       POD_CD,
       EDI_BKG_NO,
       EDI_GATE_IO_CD,
       EDI_MVMT_STS_CD,
       EVNT_DT,
       EVNT_YD_CD,
       MVMT_EDI_MSG_AREA_CD,
       MVMT_EDI_MSG_SEQ,
       MVMT_EDI_MSG_TP_ID,
       MVMT_EDI_MSG_YRMONDY,
       MVMT_EDI_TP_CD,
       MVMT_EDI_RMK,
       CNMV_RMK,
       MVMT_EDI_RSLT_CD,
       MVMT_EDI_SGHT_CD,
       MVMT_TRSP_MOD_CD,
       RTY_KNT,
       VNDR_SEQ,
       MGST_NO,
       WBL_NO,
       PKUP_NO,
       LCC_CD,
       WO_NO,
       EDI_VVD_CD,
       TIR_NO,
       MTY_PLN_NO,
       EDI_MTY_EQ_REPO_REF_NO,
       EDI_CRR_NO,
       TRSP_DOC_NO,
       FLT_FILE_REF_NO,
       CNTR_DMG_FLG,
       DMG_FLG_DT,
       DMG_UNFLG_DT,
       UPD_USR_ID
       , DEST_LOC_NM
       , DEST_STE_NM
       , VGM_DOC_ID_NO
	   , VGM_WGT
	   , VGM_EDI_WGT_UT_CD
	   , VGM_DOC_TP_CD
	   , VGM_DT_TP_CD
	   , VGM_HNDL_DT
	   , VGM_CUST_CNTC_TP_CD
	   , VGM_CUST_CNTC_NM
	   , VGM_CUST_FAX_NO
	   , VGM_CUST_EML
	   , VGM_CUST_PHN_NO
	   , VGM_CUST_ADDR
	   , VSL_ENG_NM
       , USA_EDI_CD
       , CNTR_STWG_PSN_CTNT
  FROM (
       SELECT ROW_NUMBER() OVER (ORDER BY EVNT_DT_ORG, R_ID) AS ROWNO,
              BKG_NO,
              BL_NO,
              CALL_SGN_LLOYD,
              CALL_SGN_NO,
              LLOYD_NO,
              CHSS_NO,
              CNTR_FULL_STS_CD,
              CNTR_NO,
              CNTR_SEAL_NO,
              CNTR_TPSZ_CD,
              CRE_LOCL_DT,
              VVD_CD,
              CRNT_VSL_CD,
              CRNT_SKD_VOY_NO,
              CRNT_SKD_DIR_CD,
              DEST_YD_CD,
              POL_CD,
              POD_CD,
              EDI_BKG_NO,
              EDI_GATE_IO_CD,
              EDI_MVMT_STS_CD,
              EVNT_DT,
              EVNT_YD_CD,
              MVMT_EDI_MSG_AREA_CD,
              MVMT_EDI_MSG_SEQ,
              MVMT_EDI_MSG_TP_ID,
              MVMT_EDI_MSG_YRMONDY,
              MVMT_EDI_TP_CD,
              MVMT_EDI_RMK,
              CNMV_RMK,
              MVMT_EDI_SGHT_CD,
              MVMT_TRSP_MOD_CD,
              MVMT_EDI_RSLT_CD,
              RTY_KNT,
              VNDR_SEQ,
              MGST_NO,
              WBL_NO,
              PKUP_NO,
              LCC_CD,
              WO_NO,
              EDI_VVD_CD,
              TIR_NO,
              MTY_PLN_NO,
              EDI_MTY_EQ_REPO_REF_NO,
              EDI_CRR_NO,
              TRSP_DOC_NO,
	          FLT_FILE_REF_NO,
	          CNTR_DMG_FLG,
	          DMG_FLG_DT,
	          DMG_UNFLG_DT,
	          UPD_USR_ID
              , DEST_LOC_NM
              , DEST_STE_NM
              , VGM_DOC_ID_NO
	          , VGM_WGT
	          , VGM_EDI_WGT_UT_CD
	          , VGM_DOC_TP_CD
	          , VGM_DT_TP_CD
	          , VGM_HNDL_DT
	          , VGM_CUST_CNTC_TP_CD
	          , VGM_CUST_CNTC_NM
	          , VGM_CUST_FAX_NO
	          , VGM_CUST_EML
	          , VGM_CUST_PHN_NO
	          , VGM_CUST_ADDR
	          , VSL_ENG_NM
              , USA_EDI_CD
              , CNTR_STWG_PSN_CTNT
         FROM (
                 SELECT /*+ USE_NL(MSG D B A G) */
                        D.BKG_NO,
                        D.EDI_BL_NO AS BL_NO,
                        NVL(D.CALL_SGN_NO, D.LLOYD_NO) AS CALL_SGN_LLOYD,
                        D.CALL_SGN_NO AS CALL_SGN_NO,
                        D.LLOYD_NO AS LLOYD_NO,
                        D.CHSS_NO,
                        D.CNTR_FULL_STS_CD,
                        D.CNTR_NO,
                        D.CNTR_SEAL_NO,
                        D.CNTR_TPSZ_CD,
                        TO_CHAR(D.CRE_LOCL_DT, 'YYYYMMDDHH24MI') AS CRE_LOCL_DT,
                        D.CRNT_VSL_CD||D.CRNT_SKD_VOY_NO||D.CRNT_SKD_DIR_CD AS VVD_CD,
                        D.CRNT_VSL_CD,
                        D.CRNT_SKD_VOY_NO,
                        D.CRNT_SKD_DIR_CD,
                        D.DEST_YD_CD,
                        D.BKG_POL_CD AS POL_CD,
                        D.BKG_POD_CD AS POD_CD,
                        D.EDI_BKG_NO,
                        D.EDI_GATE_IO_CD,
                        D.EDI_MVMT_STS_CD,
                        TO_CHAR(D.EVNT_DT, 'YYYYMMDDHH24MI') AS EVNT_DT,
                        D.EVNT_YD_CD,
                        D.MVMT_EDI_MSG_AREA_CD,
                        D.MVMT_EDI_MSG_SEQ,
                        D.MVMT_EDI_MSG_TP_ID,
                        D.MVMT_EDI_MSG_YRMONDY,
                        D.MVMT_EDI_TP_CD,
                        D.MVMT_EDI_RMK,
                        D.CNMV_RMK,
                        D.MVMT_EDI_SGHT_CD,
                        D.MVMT_TRSP_MOD_CD,
                        D.MVMT_EDI_RSLT_CD,
                        D.RTY_KNT,
                        D.VNDR_SEQ,
                        D.MGST_NO,
                        D.WBL_NO,
                        D.PKUP_NO,
                        A.LCC_CD, --
                        D.WO_NO,
                        D.EDI_VVD_CD,
                        D.TIR_NO,
                        NVL(D.MTY_PLN_NO, D.MTY_REPO_NO) AS MTY_PLN_NO,
                        D.EDI_MTY_EQ_REPO_REF_NO,
                        D.EDI_CRR_NO,
                        D.TRSP_DOC_NO,
		                D.FLT_FILE_REF_NO,
		                D.CNTR_DMG_FLG,
		                D.DMG_FLG_DT,
		                D.DMG_UNFLG_DT,
		                D.UPD_USR_ID
                        , D.DEST_LOC_NM
                        , D.DEST_STE_NM
                        , D.VGM_DOC_ID_NO
	                    , D.VGM_WGT
	                    , D.VGM_EDI_WGT_UT_CD
	                    , D.VGM_DOC_TP_CD
	                    , D.VGM_DT_TP_CD
	                    , D.VGM_HNDL_DT
	                    , D.VGM_CUST_CNTC_TP_CD
	                    , D.VGM_CUST_CNTC_NM
	                    , D.VGM_CUST_FAX_NO
	                    , D.VGM_CUST_EML
	                    , D.VGM_CUST_PHN_NO
	                    , D.VGM_CUST_ADDR
	                    , D.VSL_ENG_NM
                        , D.USA_EDI_CD
                        , D.CNTR_STWG_PSN_CTNT
                        ,MSG.IDX_CRE_LOCL_DT
                        ,MSG.EVNT_DT AS EVNT_DT_ORG
                        ,MSG.R_ID
                   FROM 
                       (SELECT MSG.ROWID AS R_ID, IDX_CRE_LOCL_DT, EVNT_DT
--                              ,ROW_NUMBER() OVER (ORDER BY EVNT_DT) AS ROWNO
                        FROM   CTM_MVMT_EDI_MSG MSG
                        WHERE  1=1
                     #if (${event_receive} == 'RECEIVE')
                        #if (${p_date3} != '')
                           AND (MSG.IDX_CRE_LOCL_DT IN (${p_date3})
                        #else
                           AND (MSG.IDX_CRE_LOCL_DT BETWEEN REPLACE (@[p_date1], '-', '') AND REPLACE (@[p_date2], '-', '')
                        #end
                        #if (${null_flg} != '')
                           OR MSG.IDX_CRE_LOCL_DT = '00000000'
                        #end
                              )
                     #else
                        #if (${p_date3} != '')
                           AND (MSG.IDX_EVNT_DT IN (${p_date3})
                        #else
                           AND (MSG.IDX_EVNT_DT BETWEEN REPLACE (@[p_date1], '-', '') AND REPLACE (@[p_date2], '-', '')
                        #end
                        #if (${null_flg} != '')
                           OR MSG.IDX_EVNT_DT = '00000000'
                        #end
                              )
                     #end
                          AND MSG.MVMT_EDI_MSG_AREA_CD IN ('KOR', 'CHN', 'SWA', 'EUR', 'USA')
                     #if (${p_yard2} != '')
                        AND MSG.EVNT_YD_CD LIKE @[p_yard1]||@[p_yard2]
                     #elseif (${p_yard1} != '')
                        AND MSG.EVNT_YD_CD LIKE @[p_yard1]||'%'
                     #end
                     #if (${mvmt_edi_rslt_cd} != '')
                        #if (${mvmt_edi_rslt_cd} == 'ALL')
                           AND MSG.MVMT_EDI_RSLT_CD NOT IN ('D')
                        #elseif (${mvmt_edi_rslt_cd} == 'X')
                           AND MSG.MVMT_EDI_RSLT_CD IN ('X', 'I')
                        #else
                           AND MSG.MVMT_EDI_RSLT_CD = @[mvmt_edi_rslt_cd]
                        #end
                     #end
                     #if (${edi_mvmt_sts_cd} != '')
                        AND MSG.EDI_MVMT_STS_CD IN (${edi_mvmt_sts_cd})
                     #end
                     #if (${rty_knt} != '')
                        #if (${rty_knt} == '0')
                           AND MSG.RTY_KNT = 0
                        #else
                           AND MSG.RTY_KNT > 0
                        #end
                     #end
                     #if (${edi_gate_io_cd} != '')
                        AND TRIM (MSG.EDI_GATE_IO_CD) IN (${edi_gate_io_cd})
                     #end
                        ORDER BY MSG.EVNT_DT, MSG.ROWID) MSG,
                        CTM_MVMT_EDI_MSG D, MDM_YARD M, MDM_LOCATION B, MDM_EQ_ORZ_CHT A, COM_SYS_AREA_GRP_ID G
                  WHERE 1 = 1
                  AND   D.ROWID = MSG.R_ID
                  AND   D.EVNT_YD_CD          = M.YD_CD(+)
                  AND   SUBSTR(M.YD_CD, 1, 2) = G.CNT_CD(+)
             #if (${event_receive} == 'RECEIVE')
                #if (${p_date3} != '')
                   AND (D.IDX_CRE_LOCL_DT IN (${p_date3})
                #else
                   AND (D.IDX_CRE_LOCL_DT BETWEEN REPLACE (@[p_date1], '-', '') AND REPLACE (@[p_date2], '-', '')
                #end
                #if (${null_flg} != '')
                   OR D.IDX_CRE_LOCL_DT = '00000000'
                #end
                      )
             #else
                #if (${p_date3} != '')
                   AND (D.IDX_EVNT_DT IN (${p_date3})
                #else
                   AND (D.IDX_EVNT_DT BETWEEN REPLACE (@[p_date1], '-', '') AND REPLACE (@[p_date2], '-', '')
                #end
                #if (${null_flg} != '')
                   OR D.IDX_EVNT_DT = '00000000'
                #end
                      )
             #end
                  AND D.MVMT_EDI_MSG_AREA_CD IN ('KOR', 'CHN', 'SWA', 'EUR', 'USA')
                 #if (${p_yard2} != '')
                    AND D.EVNT_YD_CD LIKE @[p_yard1]||@[p_yard2]
                 #elseif (${p_yard1} != '')
                    AND D.EVNT_YD_CD LIKE @[p_yard1]||'%'
                 #end
                 #if (${mvmt_edi_rslt_cd} != '')
                    #if (${mvmt_edi_rslt_cd} == 'ALL')
                       AND D.MVMT_EDI_RSLT_CD NOT IN ('D')
                    #elseif (${mvmt_edi_rslt_cd} == 'X')
                       AND D.MVMT_EDI_RSLT_CD IN ('X', 'I')
                    #else
                       AND D.MVMT_EDI_RSLT_CD = @[mvmt_edi_rslt_cd]
                    #end
                 #end
                 #if (${edi_mvmt_sts_cd} != '')
                    AND D.EDI_MVMT_STS_CD IN (${edi_mvmt_sts_cd})
                 #end
                 #if (${rty_knt} != '')
                    #if (${rty_knt} == '0')
                       AND D.RTY_KNT = 0
                    #else
                       AND D.RTY_KNT > 0
                    #end
                 #end
                 #if (${edi_gate_io_cd} != '')
                    AND TRIM (D.EDI_GATE_IO_CD) IN (${edi_gate_io_cd})
                 #end
                 AND A.SCC_CD(+) = B.SCC_CD
                 AND B.LOC_CD(+) = SUBSTR(D.EVNT_YD_CD, 0, 5)
              )
       )
#if (${start_row_no} != '' && ${start_row_no} != '')
WHERE ROWNO BETWEEN @[start_row_no] AND @[end_row_no]
#end			]]></sql>
			<params>
				<param name="p_date1" type="12" value="" out="N"/>
				<param name="p_date2" type="12" value="" out="N"/>
				<param name="p_yard1" type="12" value="" out="N"/>
				<param name="p_yard2" type="12" value="" out="N"/>
				<param name="mvmt_edi_rslt_cd" type="12" value="" out="N"/>
				<param name="start_row_no" type="12" value="" out="N"/>
				<param name="end_row_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
