<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOCheckInvoiceCancelRSQL">
			<desc><![CDATA[Invoice Cancel 여부 체크]]></desc>
			<sql><![CDATA[
SELECT DECODE(CXL_BKG_CHG_FLG, 'Y', 'Y', 'N') AS CANCEL_YN
FROM DMT_INV_DTL A, DMT_CHG_CALC B
WHERE A.DMDT_INV_NO = @[s_invoice_no]
AND A.CRE_OFC_CD = @[cre_ofc_cd]
AND A.SYS_AREA_GRP_ID = B.SYS_AREA_GRP_ID
AND A.CNTR_NO = B.CNTR_NO
AND A.CNTR_CYC_NO = B.CNTR_CYC_NO
AND A.DMDT_TRF_CD = B.DMDT_TRF_CD
AND A.DMDT_CHG_LOC_DIV_CD = B.DMDT_CHG_LOC_DIV_CD
AND A.CHG_SEQ = B.CHG_SEQ			]]></sql>
			<params>
				<param name="s_invoice_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
