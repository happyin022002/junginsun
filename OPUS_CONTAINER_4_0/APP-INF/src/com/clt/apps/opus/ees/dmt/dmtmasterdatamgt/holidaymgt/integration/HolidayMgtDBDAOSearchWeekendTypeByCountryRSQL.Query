<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HolidayMgtDBDAOSearchWeekendTypeByCountryRSQL">
			<desc><![CDATA[Country 에 해당되는 주말 타입정보를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT 	WKND_TP_CD
FROM	DMT_WEEKEND
WHERE	CNT_CD = @[cnt_cd]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
