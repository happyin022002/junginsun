<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MgsetMovementHistoryDBDAOsearchMGSMovementHistoryDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT AAA.*
  FROM (SELECT ROWNUM AS NO, AA.*
          FROM (SELECT B.MVMT_STS_CD,
                       TO_CHAR(B.MVMT_DT, 'YYYY-MM-DD HH24:MI') MVMT_DT,
                       B.YD_CD,
                       B.DEST_YD_CD,
                       B.CNTR_NO,
                       DECODE(I.CNTR_NO, NULL, DECODE(B.MGST_OWNR_CO_CD, 'H', 'OWN', 'OTHER'), COM_CONSTANTMGR_PKG.COM_GETCOMPANYCODE_FNC()) CNTR_OWNR_CO_CD,
                       B.DIFF_RMK,
                       TO_CHAR(B.CRE_DT, 'YYYY-MM-DD HH24:MI') CRE_DT,
                       B.CRE_USR_ID,
                       C.OFC_CD CRE_OFC_CD,
                       C.USR_NM CRE_USR_NM,
                       TO_CHAR(B.UPD_DT, 'YYYY-MM-DD HH24:MI') UPD_DT,
                       B.UPD_USR_ID,
                       D.OFC_CD UPD_OFC_CD,
                       D.USR_NM UPD_USR_NM,
                       B.VNDR_SEQ,
                       E.VNDR_ABBR_NM,
                       B.CHSS_NO CHSS_NO,
                       B.CNMV_YR,
                       B.CNMV_ID_NO
                  FROM CGM_EQUIPMENT A,
                       CGM_MGST_MVMT_HIS B,
                       COM_USER C,
                       COM_USER D,
                       MDM_VENDOR E,
                       MST_CONTAINER I
                 WHERE A.EQ_KND_CD='G'
                   AND A.EQ_NO = @[eq_no]
                   AND A.EQ_NO = B.MGST_NO
                   AND B.CNTR_NO = I.CNTR_NO(+)
                   AND B.CRE_USR_ID = C.USR_ID(+)
                   AND B.UPD_USR_ID = D.USR_ID(+)
                   AND B.VNDR_SEQ = E.VNDR_SEQ (+) 
#if ( ${str_mvmt_dt} != '' )
                   AND B.MVMT_DT BETWEEN TO_DATE(@[str_mvmt_dt], 'YYYY-MM-DD') AND TO_DATE(@[end_mvmt_dt], 'YYYY-MM-DD') + 1
#end
                 ORDER BY MVMT_DT, CNMV_YR, CNMV_ID_NO) AA
                 ) AAA 
#if ( ${str_mvmt_dt} == '' )
  WHERE AAA.NO < 301 
#end			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="str_mvmt_dt" type="12" value="" out="N"/>
				<param name="end_mvmt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
