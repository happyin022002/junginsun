<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOmodifyCHSFoundDataUSQL">
			<desc><![CDATA[CGM_EQUIPMENT 업데이트]]></desc>
			<sql><![CDATA[
UPDATE CGM_EQUIPMENT
 SET ACIAC_DIV_CD = 'A'
    ,CRNT_YD_CD   = @[f_evnt_yd_cd]
    ,CRNT_LOC_CD  = SUBSTR(@[f_evnt_yd_cd],0,5) 
    ,CHSS_MVMT_DT = TO_DATE(@[f_evnt_dt] ,'YYYY-MM-DD HH24:MI:SS')	
    ,CHSS_MVMT_STS_CD = 'BI'
    ,GATE_IO_CD = 'I' 
    ,EQ_STS_SEQ       =  NVL(CGM_GET_EQ_STS_HIS_MAXSEQ_FNC(@[eq_no]) ,0)
    ,UPD_USR_ID       = @[user_id]    
    ,UPD_DT           = sysdate
 WHERE EQ_NO = @[eq_no]			]]></sql>
			<params>
				<param name="f_evnt_yd_cd" type="12" value="" out="N"/>
				<param name="f_evnt_dt" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
