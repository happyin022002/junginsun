<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceivableRentalCostDBDAOsearchReceivableRentalChargeSeqbyManualDataRSQL">
			<desc><![CDATA[searchReceivableRentalChargeSeqbyManualData]]></desc>
			<sql><![CDATA[
SELECT  CHG.RCV_RNTL_SEQ
FROM    LSE_RCV_RNTL_CHG CHG
    , (SELECT  @[cost_yrmon]     AS COST_YRMON, 
	            @[agmt_cty_cd]    AS AGMT_CTY_CD,    
    	        @[agmt_seq]       AS AGMT_SEQ,
        	    @[qty_yrmon]      AS QTY_YRMON,
	            @[vndr_seq]       AS VNDR_SEQ,
    	        @[vndr_abbr_nm]   AS VNDR_ABBR_NM,
        	    @[lstm_cd]        AS LSTM_CD,
	            @[eff_dt]         AS EFF_DT,
    	        @[exp_dt]         AS EXP_DT,
        	    @[ref_no]         AS REF_NO,  
	            @[ofc_cd]         AS OFC_CD, 
    	        @[cre_usr_id]     AS CRE_USR_ID    
	    FROM    DUAL) P
WHERE CHG.COST_YRMON  = P.COST_YRMON 
AND   CHG.AGMT_CTY_CD = P.AGMT_CTY_CD
AND   CHG.AGMT_SEQ    = P.AGMT_SEQ
AND   ROWNUM          = 1			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="qty_yrmon" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vndr_abbr_nm" type="12" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="ref_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
