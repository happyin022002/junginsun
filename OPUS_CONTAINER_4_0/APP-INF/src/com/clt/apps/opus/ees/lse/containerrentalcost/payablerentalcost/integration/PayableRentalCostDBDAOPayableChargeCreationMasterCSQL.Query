<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOPayableChargeCreationMasterCSQL">
			<desc><![CDATA[Payable Charge Creation Data Insert]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_PAY_RNTL_CHG (
       AGMT_CTY_CD
     , AGMT_SEQ
     , CHG_SEQ
     , CHG_COST_YRMON
     , VNDR_SEQ
     , PAY_LR_CD
     , LSTM_CD
     , AGMT_REF_NO
     , CURR_CD
     , TTL_COST_AMT
     , CR_TTL_AMT
     , PAY_RNTL_COST_AMT
     , SO_ISS_COST_AMT
     , CRE_OFC_CD
     , LSE_PAY_RNTL_STS_CD
     , EFF_DT
     , EXP_DT
     , CONV_COST_AMT
     , CONV_RT_AMT
     , CONV_CR_COST_AMT
     , CONV_TAX_AMT
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
)
SELECT BB.AGMT_CTY_CD
     , BB.AGMT_SEQ
     , @[chg_seq]
     , @[chg_cost_yrmon]
     , BB.VNDR_SEQ
     , SUBSTR(CC.VNDR_ABBR_NM, 0, 3) AS PAY_LR_CD
     , BB.LSTM_CD
     , BB.REF_NO AS AGMT_REF_NO
     , BB.CURR_CD
     , 0 AS TTL_COST_AMT
     , 0 AS CR_TTL_AMT
     , 0 AS PAY_RNTL_COST_AMT
     , 0 AS SO_ISS_COST_AMT
     , @[cre_ofc_cd]
     , 'H'
     , DD.EFF_DT
     , BB.LST_EXP_DT
     , 0 AS CONV_COST_AMT
     , 0 AS CONV_RT_AMT
     , 0 AS CONV_CR_COST_AMT
     , 0 AS CONV_TAX_AMT
     , @[usr_id]
     , SYSDATE
     , @[usr_id]
     , SYSDATE
FROM   LSE_AGREEMENT  BB
     , MDM_VENDOR     CC
     , LSE_AGMT_VER   DD
WHERE  DD.AGMT_VER_SEQ = 1
AND    DD.AGMT_CTY_CD  = BB.AGMT_CTY_CD
AND    DD.AGMT_SEQ     = BB.AGMT_SEQ
AND    CC.VNDR_SEQ     = BB.VNDR_SEQ
AND    BB.AGMT_CTY_CD  = @[agmt_cty_cd]
AND    BB.AGMT_SEQ     = @[agmt_seq]			]]></sql>
			<params>
				<param name="chg_seq" type="2" value="" out="N"/>
				<param name="chg_cost_yrmon" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
