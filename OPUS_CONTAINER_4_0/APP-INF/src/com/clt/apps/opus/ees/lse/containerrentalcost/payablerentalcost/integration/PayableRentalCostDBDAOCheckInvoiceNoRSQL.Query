<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOCheckInvoiceNoRSQL">
			<desc><![CDATA[File Import시에 Invoice No.를 읽어 Invoice No에 대한 Validation을 수행한다.]]></desc>
			<sql><![CDATA[
SELECT 'The Invoice No. already exists(Cost Month:'||SUBSTR(REF_PK, 1, 6)||', AGMT No:'||SUBSTR(REF_PK, 7, 20)||', Lessor:'||VNDR_SEQ||').  Please change to new Invoice No.' AS CHECK_WARNING
  FROM AP_INV_NO_CHK_V
 WHERE 1=1
   AND MDL_CD              = @[mdl_cd]
   AND INV_NO              = @[inv_no]
   AND VNDR_SEQ            = @[vndr_seq]
   AND NVL(REF_PK, 'X')   != @[cost_yrmon]||@[agmt_cty_cd]||@[agmt_seq]
   AND ROWNUM    = 1 			]]></sql>
			<params>
				<param name="mdl_cd" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
