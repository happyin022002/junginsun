<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodSimulateDBDAOMergeCodTempQtyCleanDSQL">
			<desc><![CDATA[COD Temp Qty를 Clean 시켜주는 SQL]]></desc>
			<sql><![CDATA[
DELETE FROM EQR_VSL_PLN_COD_QTY QTY 
WHERE QTY.REPO_PLN_ID = @[repo_pln_id] 
AND NOT EXISTS ( SELECT 'X'
                   FROM EQR_VSL_LDIS_PLN_COD_TMP PLN
                  WHERE PLN.REPO_PLN_ID = QTY.REPO_PLN_ID
                    AND PLN.PLN_YRWK    = QTY.PLN_YRWK
                    AND PLN.PLN_SEQ       = QTY.PLN_SEQ)			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
