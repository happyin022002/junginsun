	,	LODG_DCHG_COST_AMT	= @[lodg_dchg_cost_amt]
	,	PLN_UC_AMT			= @[pln_uc_amt]
	,	LODG_PORT_UC_AMT	= @[lodg_port_uc_amt]
	,	DCHG_PORT_UC_AMT	= @[dchg_port_uc_amt]
	,	UPD_USR_ID			= @[upd_usr_id]
	,	UPD_DT				= SYSDATE
WHERE	REPO_PLN_ID		= @[repo_pln_id]
	AND	PLN_YRWK		= @[pln_yrwk]
	AND	PLN_SEQ			= @[pln_seq]
	AND	REF_ID			= @[ref_id]
	AND	CNTR_TPSZ_CD	= @[cntr_tpsz_cd]
			]]></sql>
			<params>
				<param name="cntr_qty" type="2" value="" out="N"/>
				<param name="lodg_dchg_cost_amt" type="6" value="" out="N"/>
				<param name="pln_uc_amt" type="6" value="" out="N"/>
				<param name="lodg_port_uc_amt" type="6" value="" out="N"/>
				<param name="dchg_port_uc_amt" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
