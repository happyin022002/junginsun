<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOmodifyESTTempFlagDataUSQL">
			<desc><![CDATA[modifyESTTempFlagData]]></desc>
			<sql><![CDATA[
UPDATE MNR_RPR_RQST_TMP_HDR A
     SET A.EDI_ERR_CD = @[edi_err_cd]
WHERE A.RQST_EQ_NO = @[rqst_eq_no]
AND   A.RPR_RQST_TMP_SEQ = @[rpr_rqst_tmp_seq]
AND   A.RPR_RQST_TMP_VER_NO = @[rpr_rqst_tmp_ver_no]			]]></sql>
			<params>
				<param name="edi_err_cd" type="12" value="" out="N"/>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
				<param name="rpr_rqst_tmp_seq" type="12" value="" out="N"/>
				<param name="rpr_rqst_tmp_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
