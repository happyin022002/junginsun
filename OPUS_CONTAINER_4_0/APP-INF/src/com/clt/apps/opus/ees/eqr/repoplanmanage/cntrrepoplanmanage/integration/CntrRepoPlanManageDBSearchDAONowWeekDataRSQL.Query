<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanManageDBSearchDAONowWeekDataRSQL">
			<desc><![CDATA[DB에서 현재 주차 계산]]></desc>
			<sql><![CDATA[
SELECT (PLN_YR||PLN_WK) AS NOWWEEK 
FROM EQR_WK_PRD 
WHERE 1=1
AND @[nowdate] BETWEEN WK_ST_DT AND WK_END_DT			]]></sql>
			<params>
				<param name="nowdate" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
