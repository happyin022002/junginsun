                    FROM MST_LSE_TERM
                   ORDER BY DP_SEQ
                ) BBB
          WHERE AAA.LSTM_CD =  BBB.LSTM_CD
          ORDER BY BBB.DP_SEQ , AAA.CURR_CD , ABBR_NM1
       )
