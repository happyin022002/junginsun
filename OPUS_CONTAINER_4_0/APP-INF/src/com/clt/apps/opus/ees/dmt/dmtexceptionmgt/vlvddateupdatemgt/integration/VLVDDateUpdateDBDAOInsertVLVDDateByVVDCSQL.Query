<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VLVDDateUpdateDBDAOInsertVLVDDateByVVDCSQL">
			<desc><![CDATA[VL/VD DATE UPDATE 등록	
UI_DMT_2012	
VL/VD Date Update by VVD]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_VSL_DT_UPD
(
     VSL_CD
    ,SKD_VOY_NO
    ,SKD_DIR_CD
    ,LOC_CD
    ,VL_VD_DIV_CD
    ,PRV_DT
    ,FX_DT
    ,VSL_UPD_RMK
    ,CRE_USR_ID
    ,CRE_DT
    ,CRE_OFC_CD
    ,UPD_USR_ID
    ,UPD_DT
    ,UPD_OFC_CD
) VALUES (
     SUBSTR(@[vvd],1,4)
    ,SUBSTR(@[vvd],5,4)
    ,SUBSTR(@[vvd],9,1)
    ,@[xPort]
    ,@[xMvmt]
    ,TO_DATE(@[vldaten] , 'YYYY-MM-DD')
    ,TO_DATE(@[vldateb] , 'YYYYMMDD')
    ,''
    ,@[userid]
    ,SYSDATE
    ,@[offccd]
    ,@[userid]
    ,SYSDATE
    ,@[offccd]
)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="xPort" type="12" value="" out="N"/>
				<param name="xMvmt" type="12" value="" out="N"/>
				<param name="vldaten" type="12" value="" out="N"/>
				<param name="vldateb" type="12" value="" out="N"/>
				<param name="userid" type="12" value="" out="N"/>
				<param name="offccd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
