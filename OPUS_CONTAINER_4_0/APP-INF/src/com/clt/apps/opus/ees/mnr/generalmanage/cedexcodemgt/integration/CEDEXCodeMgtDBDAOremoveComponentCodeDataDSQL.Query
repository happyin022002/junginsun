<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CEDEXCodeMgtDBDAOremoveComponentCodeDataDSQL">
			<desc><![CDATA[삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MNR_EQ_CMPO_CD A
WHERE   1=1
   AND  (
        A.EQ_CMPO_NUM_ISO_CD = @[eq_cmpo_num_iso_cd]
        AND   A.EQ_CMPO_CD = @[eq_cmpo_cd]
        AND   A.EQ_CMPO_GRP_TP_CD = @[eq_cmpo_grp_tp_cd]
        )

#if (${eq_cmpo_grp_tp_cd} == '1') 
   OR   (
        A.EQ_CMPO_GRP_TP_CD = '2'
        AND @[eq_cmpo_cd] = A.EQ_PRNT_CMPO_CD
        )
   OR   (
        A.EQ_CMPO_GRP_TP_CD = '3'
        AND A.EQ_PRNT_CMPO_CD IN ( SELECT EQ_CMPO_CD
                                     FROM MNR_EQ_CMPO_CD
                                    WHERE EQ_CMPO_GRP_TP_CD = '2'
                                      AND EQ_PRNT_CMPO_CD = @[eq_cmpo_cd]
                                 )
        )
#elseif (${eq_cmpo_grp_tp_cd} == '2')
   OR   (
        A.EQ_CMPO_GRP_TP_CD = '3'
        AND @[eq_cmpo_cd] = A.EQ_PRNT_CMPO_CD
        )
#end			]]></sql>
			<params>
				<param name="eq_cmpo_num_iso_cd" type="12" value="" out="n"/>
				<param name="eq_cmpo_cd" type="12" value="" out="n"/>
				<param name="eq_cmpo_grp_tp_cd" type="12" value="" out="n"/>
			</params>
		</query>
	</querys>
</sqls>
