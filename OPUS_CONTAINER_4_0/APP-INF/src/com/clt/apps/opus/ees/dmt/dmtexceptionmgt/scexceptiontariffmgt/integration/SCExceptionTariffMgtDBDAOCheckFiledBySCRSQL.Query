<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOCheckFiledBySCRSQL">
			<desc><![CDATA[S/C 가 Filed STS 인지를 체크하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT 
	COUNT(PROP_NO) CNT
FROM	PRI_SP_MN
WHERE	PROP_NO = @[prop_no]
	AND	PROP_STS_CD = 'F'
			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
