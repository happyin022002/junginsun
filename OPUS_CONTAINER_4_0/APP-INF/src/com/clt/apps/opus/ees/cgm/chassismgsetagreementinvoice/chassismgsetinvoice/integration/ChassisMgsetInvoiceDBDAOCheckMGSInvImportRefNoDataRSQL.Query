<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOCheckMGSInvImportRefNoDataRSQL">
			<desc><![CDATA[ChassisMgsetInvoiceDB.CheckMGSInvImportRefNoData]]></desc>
			<sql><![CDATA[
SELECT
	A.AGMT_OFC_CTY_CD,
	A.AGMT_SEQ,
	B.AGMT_VER_NO,
	B.AGMT_LSTM_CD
FROM 
	CGM_INV_REF_NO_RGST A, 
	CGM_AGREEMENT B,
	CGM_LSE_CHG_HDR C
WHERE 
	A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD
    AND A.AGMT_SEQ = B.AGMT_SEQ
	AND A.AGMT_OFC_CTY_CD = C.AGMT_OFC_CTY_CD
	AND A.AGMT_SEQ = C.AGMT_SEQ
    AND B.LST_VER_FLG = 'Y' 
	AND A.INV_REF_NO = @[inv_ref_no]
	AND C.CHG_CRE_SEQ = @[chg_cre_seq]
	AND C.COST_YRMON = @[cost_yrmon]
	AND A.EQ_KND_CD = @[eq_knd_cd]
	AND B.EQ_KND_CD = @[eq_knd_cd]
	AND C.EQ_KND_CD = @[eq_knd_cd]
	AND B.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="inv_ref_no" type="12" value="" out="N"/>
				<param name="chg_cre_seq" type="2" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
