<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MTYEquipmentForecastDBDAOsearchYardListRSQL">
			<desc><![CDATA[ECC내 소속 야드 를 조회한다]]></desc>
			<sql><![CDATA[
SELECT 
    C.YD_CD,C.YD_NM 
FROM MDM_EQ_ORZ_CHT A,
     MDM_LOCATION B,
     MDM_YARD C
WHERE A.SCC_CD=B.SCC_CD
AND B.LOC_CD = SUBSTR(C.YD_CD,1,5)
AND A.ECC_CD = @[ecc_cd]
ORDER BY C.YD_CD			]]></sql>
			<params>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
