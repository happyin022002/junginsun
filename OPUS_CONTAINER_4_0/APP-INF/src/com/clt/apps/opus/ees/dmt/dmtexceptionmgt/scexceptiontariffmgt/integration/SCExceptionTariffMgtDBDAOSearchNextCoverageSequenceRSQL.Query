<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOSearchNextCoverageSequenceRSQL">
			<desc><![CDATA[Coverage 입력시 사용될 Sequence 를 구한다.]]></desc>
			<sql><![CDATA[
SELECT	NVL(MAX(CVRG_SEQ), 0) + 1
FROM	DMT_SC_EXPT_CVRG
WHERE	PROP_NO = @[prop_no]
	AND SC_EXPT_VER_SEQ = @[sc_expt_ver_seq]
	AND SC_EXPT_GRP_SEQ = @[sc_expt_grp_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
