<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOTitleMaxWeekRSQL">
			<desc><![CDATA[Grid title 의 기간을 구한다.]]></desc>
			<sql><![CDATA[
SELECT 
	DISTINCT (PLN_YR||'-'||PLN_WK)  YEAR_WEEK ,
  	(PLN_YR || PLN_MON) YEAR_MON  
FROM 
  	EQR_WK_PRD 
WHERE 
  	PLN_YR|| PLN_WK	  BETWEEN @[st_year_weekly] AND @[end_year_weekly]  
ORDER BY 
	YEAR_WEEK ASC			]]></sql>
			<params>
				<param name="st_year_weekly" type="12" value="" out="N"/>
				<param name="end_year_weekly" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
