<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOSearchMovementListByVvdForMultiComboVORSQL">
			<desc><![CDATA[EES_CTM_0409
--VVD_CD MultiCombo용 데이터 불러오기]]></desc>
			<sql><![CDATA[
SELECT VSL_CD || SKD_VOY_NO || SKD_DIR_CD || '|' || POL_CD AS VVD_CD,
       POL_CD
FROM BKG_VVD
WHERE VSL_PRE_PST_CD = @[vsl_pre_pst_cd]
AND BKG_NO = @[bkg_no]
ORDER BY VSL_SEQ ASC			]]></sql>
			<params>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
