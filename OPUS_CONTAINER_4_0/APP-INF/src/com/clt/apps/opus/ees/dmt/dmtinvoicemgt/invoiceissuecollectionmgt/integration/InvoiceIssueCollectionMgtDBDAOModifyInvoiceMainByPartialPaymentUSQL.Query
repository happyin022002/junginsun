<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyInvoiceMainByPartialPaymentUSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
UPDATE	DMT_INV_MN
SET
(
	 ORG_CHG_AMT
	,DMDT_EXPT_AMT
	,DC_AMT
	,BIL_AMT
	,INV_CHG_AMT
	,INV_AMT
	,TAX_AMT
	,INV_PRT_FLG
)
=
(
	SELECT
	 @[org_chg_amt]
	,@[dmdt_expt_amt]
	,@[dc_amt]
	,@[bil_amt]
	,@[inv_chg_amt]
	,@[inv_amt]
	,@[tax_amt]
	,'Y'
	FROM	DUAL
)
WHERE	DMDT_INV_NO	= @[dmdt_inv_no]
			]]></sql>
			<params>
				<param name="org_chg_amt" type="12" value="" out="N"/>
				<param name="dmdt_expt_amt" type="12" value="" out="N"/>
				<param name="dc_amt" type="12" value="" out="N"/>
				<param name="bil_amt" type="12" value="" out="N"/>
				<param name="inv_chg_amt" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="tax_amt" type="12" value="" out="N"/>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
