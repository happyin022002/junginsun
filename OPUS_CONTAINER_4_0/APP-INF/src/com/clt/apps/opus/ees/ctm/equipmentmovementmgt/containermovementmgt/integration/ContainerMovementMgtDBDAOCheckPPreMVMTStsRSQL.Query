<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOCheckPPreMVMTStsRSQL">
			<desc><![CDATA[CheckPPreMVMTSts]]></desc>
			<sql><![CDATA[
SELECT MVMT_STS_CD
FROM (
    SELECT ROW_NUMBER() OVER(ORDER BY CNMV_YR DESC, CNMV_SEQ DESC, CNMV_SPLIT_NO DESC) AS SEQ, CNMV_ID_NO, MVMT_STS_CD
    FROM CTM_MOVEMENT
    WHERE CNTR_NO = @[cntr_no]
    ORDER BY CNMV_YR DESC, CNMV_SEQ DESC, CNMV_SPLIT_NO DESC)
WHERE SEQ = 2			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
