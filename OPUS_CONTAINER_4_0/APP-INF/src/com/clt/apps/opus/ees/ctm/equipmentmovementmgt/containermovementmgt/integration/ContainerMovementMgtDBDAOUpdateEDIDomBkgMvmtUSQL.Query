<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOUpdateEDIDomBkgMvmtUSQL">
			<desc><![CDATA[배치 실행시 업데이트]]></desc>
			<sql><![CDATA[
UPDATE DOM_BOOKING SET
	DEST_RAIL_LOC_CD	= NVL((SELECT LOC_CD FROM MDM_LOCATION WHERE UN_LOC_CD = @[dest_loc_cd] AND ROWNUM=1), ''),
	ST_TURN_FLG			= NVL(@[st_turn_flg],'N')	,
	CNTR_NO				= NVL(@[cntr_no],'')		,
	DAT_MNPL_CD			= NVL(@[cud_flg],'')		
WHERE
	DMST_BKG_NO		= @[dmst_bkg_no]			]]></sql>
			<params>
				<param name="dest_loc_cd" type="12" value="" out="N"/>
				<param name="st_turn_flg" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cud_flg" type="12" value="" out="N"/>
				<param name="dmst_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
