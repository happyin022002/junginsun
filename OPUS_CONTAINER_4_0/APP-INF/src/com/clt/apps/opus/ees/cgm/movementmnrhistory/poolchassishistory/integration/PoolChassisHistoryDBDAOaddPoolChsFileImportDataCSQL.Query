<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisHistoryDBDAOaddPoolChsFileImportDataCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO 
CGM_POOL_CHSS_IMP_FILE(
     FILE_SEQ               
    ,ORG_FILE_NM            
    ,SAV_FILE_NM            
    ,CHSS_POOL_CD           
    ,COST_YRMON             
    ,FILE_IMP_PROC_STS_CD   
     
    ,FILE_IMP_DT            
    ,CRE_USR_ID             
    ,CRE_DT                  
    ,UPD_USR_ID             
    ,UPD_DT                 

) VALUES  (
    (select nvl(max(FILE_SEQ)+1,1) from CGM_POOL_CHSS_IMP_FILE)
   ,@[org_file_nm]
   ,@[sav_file_nm]
   ,@[chss_pool_cd]
   ,@[cost_yrmon]
   ,'P'
   , (select  GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC( @[save_chk]) from dual)
   , @[cre_usr_id]
   , SYSDATE
   , @[upd_usr_id]
   , SYSDATE
  
)			]]></sql>
			<params>
				<param name="org_file_nm" type="12" value="" out="N"/>
				<param name="sav_file_nm" type="12" value="" out="N"/>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="save_chk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
