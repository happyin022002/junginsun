<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchRHQHierarchyByCountryRSQL">
			<desc><![CDATA[Country 코드로 해당 상위 RHQ 코드를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	A.CNT_CD
	,	A.CNT_NM
	,	B.SYS_AREA_GRP_ID SVR_ID
FROM	MDM_COUNTRY A
	,	COM_SYS_AREA_GRP_ID B

WHERE	A.CNT_CD 	= @[cnt_cd]
	AND A.CNT_CD 	= B.CNT_CD
	AND B.CO_IND_CD = 'H'			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
