<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DualTypeExceptionMgtDBDAOCheckDualCoverageRSQL">
			<desc><![CDATA[Coverage 가 Dual Coverage 인지를 체크하기 위한 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT	COUNT(*)
FROM	DMT_CALC_TP
WHERE	DMDT_CALC_TP_CD = 'D'
	AND	CNT_CD = @[cnt_cd]
	AND DECODE(RGN_CD, ' ', NVL(@[rgn_cd], ' '), RGN_CD) = NVL(@[rgn_cd], ' ')
	AND DECODE(STE_CD, ' ', NVL(@[ste_cd], ' '), STE_CD) = NVL(@[ste_cd], ' ')
	AND DECODE(LOC_CD, ' ', NVL(@[loc_cd], ' '), LOC_CD) = NVL(@[loc_cd], ' ')
    AND IO_BND_CD = @[io_bnd_cd]
    AND EFF_DT <= TO_DATE(@[dul_expt_eff_dt], 'YYYYMMDD')
#if (${dul_expt_exp_dt} != '') 
    AND NVL(EXP_DT, TO_DATE(@[dul_expt_exp_dt], 'YYYYMMDD')) >= TO_DATE(@[dul_expt_exp_dt], 'YYYYMMDD')
#else 
	AND EXP_DT IS NULL
#end			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dul_expt_eff_dt" type="12" value="" out="N"/>
				<param name="dul_expt_exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
