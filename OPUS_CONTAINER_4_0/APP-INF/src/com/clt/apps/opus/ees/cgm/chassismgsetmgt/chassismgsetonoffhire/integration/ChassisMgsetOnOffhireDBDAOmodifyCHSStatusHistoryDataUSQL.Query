<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOmodifyCHSStatusHistoryDataUSQL">
			<desc><![CDATA[CGM_EQUIPMENT 업데이트]]></desc>
			<sql><![CDATA[
UPDATE CGM_EQ_STS_HIS
 SET STS_EVNT_YD_CD     = @[sts_evnt_yd_cd]
    ,STS_EVNT_LOC_CD    = SUBSTR(@[sts_evnt_yd_cd],0,5)
    ,STS_EVNT_DT        = TO_DATE(@[sts_evnt_dt],'YYYY-MM-DD HH24:MI')
    ,AGMT_OFC_CTY_CD    = @[agmt_ofc_cty_cd]	  
    ,AGMT_SEQ	        = @[agmt_seq]	
    ,UPD_USR_ID         = @[cre_usr_id]	
    ,UPD_DT             = sysdate
 WHERE EQ_NO            = @[eq_no]
   AND EQ_STS_SEQ       = @[eq_sts_seq]			]]></sql>
			<params>
				<param name="sts_evnt_yd_cd" type="12" value="" out="N"/>
				<param name="sts_evnt_dt" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="eq_sts_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
