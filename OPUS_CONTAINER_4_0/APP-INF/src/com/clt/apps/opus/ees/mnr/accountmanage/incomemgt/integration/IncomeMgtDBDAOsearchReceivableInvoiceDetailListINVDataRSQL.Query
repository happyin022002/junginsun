<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncomeMgtDBDAOsearchReceivableInvoiceDetailListINVDataRSQL">
			<desc><![CDATA[IncomeMgtDBDAOsearchReceivableInvoiceDetailListINVDataRSQL]]></desc>
			<sql><![CDATA[
SELECT
(
SELECT C.MNR_PRNR_LGL_ENG_NM
FROM MNR_PARTNER C
WHERE B.MNR_PRNR_CNT_CD = C.MNR_PRNR_CNT_CD
AND B.MNR_PRNR_SEQ = C.MNR_PRNR_SEQ
AND C.MNR_PRNR_STS_CD = 'C'
) MNR_PRNR_LGL_ENG_NM
,A.DISP_NO
,B.DISP_DTL_SEQ
,B.DISP_RLSE_NO
,B.EQ_NO
,B.DISP_QTY
,B.EQ_TPSZ_CD
,A.CURR_CD
,B.INV_AMT
,B.DISP_YD_CD
,TO_CHAR(B.DISP_SOLD_DT , 'yyyy-mm-dd') DISP_SOLD_DT
,B.MNR_DISP_DTL_RMK
,B.RCV_INV_SEQ
,'SS' MNR_VRFY_TP_CD
FROM MNR_DISP_HDR A, MNR_DISP_DTL B, MNR_RCV_INV_WRK D
WHERE A.DISP_NO= B.DISP_NO
#if (${invNos} != '')
	AND	D.INV_NO IN (
		#foreach ($user_invNos IN ${invNos})
			#if($velocityCount < $invNos.size())
				'$user_invNos',
			#else
				'$user_invNos'
			#end
		#end			  
	)
#end
	AND D.RCV_INV_SEQ = B.RCV_INV_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
