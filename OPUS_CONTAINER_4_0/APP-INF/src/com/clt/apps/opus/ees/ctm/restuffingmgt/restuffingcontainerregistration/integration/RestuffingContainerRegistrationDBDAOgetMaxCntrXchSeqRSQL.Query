<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RestuffingContainerRegistrationDBDAOgetMaxCntrXchSeqRSQL">
			<desc><![CDATA[컨테이너의 CNTR_XCH_SEQ 최대값을 구해온다.]]></desc>
			<sql><![CDATA[
SELECT NVL (MAX (CNTR_XCH_SEQ), 0) + 1 AS MX
  FROM CTM_MVMT_XCH_DTL
 WHERE CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
