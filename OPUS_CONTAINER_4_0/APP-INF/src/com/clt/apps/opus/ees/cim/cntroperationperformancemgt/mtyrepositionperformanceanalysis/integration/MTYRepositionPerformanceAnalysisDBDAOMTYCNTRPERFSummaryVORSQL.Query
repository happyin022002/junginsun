			SELECT	'MR'				MOV_STS,
					S.DP_SEQ			DSP_SEQ,
					S.CNTR_TPSZ_CD      CNTR_TPSZ_CD,	
					NVL(CNT,0)			CNT
			FROM	(
						SELECT 
