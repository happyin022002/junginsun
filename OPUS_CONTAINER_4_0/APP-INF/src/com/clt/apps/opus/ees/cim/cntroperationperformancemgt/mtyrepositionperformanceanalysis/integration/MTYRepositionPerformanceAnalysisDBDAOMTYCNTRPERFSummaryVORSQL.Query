	  #if (${soc} == 'E')
      AND 	NVL(C.LSTM_CD, '  ') <> 'SH' /* soc	*/ 
	  #elseif(${soc} == 'O')
	  AND 	NVL(C.LSTM_CD, '  ') = 'SH' /* soc	*/
	  #end	
	#if (${bound} == 'N')
		AND		M.OB_CNTR_FLG	=	'N'		/* 'N':Inbound, 'Y':Outbound */
	#elseif(${bound} == 'Y')
		AND		M.OB_CNTR_FLG	=	'Y'		/* 'N':Inbound, 'Y':Outbound */
	#end

    GROUP BY 'TS-VL',M.CNTR_TPSZ_CD) A,CIM_TP_SZ_DP_SEQ_V S    
     WHERE 	A.CNTR_TPSZ_CD(+) 	= S.CNTR_TPSZ_CD
