    GROUP BY 'VD-IC',M.CNTR_TPSZ_CD) A,CIM_TP_SZ_DP_SEQ_V S    
     WHERE 	A.CNTR_TPSZ_CD(+) 	= S.CNTR_TPSZ_CD
