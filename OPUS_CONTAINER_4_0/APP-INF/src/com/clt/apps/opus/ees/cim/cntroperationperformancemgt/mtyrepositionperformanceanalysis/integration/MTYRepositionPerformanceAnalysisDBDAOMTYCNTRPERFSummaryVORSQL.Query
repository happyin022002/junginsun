	SUM(DECODE(INSTR(@[tpsz],DECODE( L.DSP_SEQ, 1, CNTR_TPSZ_CD , '0'  )),0,NULL,DECODE( L.DSP_SEQ, 1, CNT , 0  ) )) AS count01,
	SUM(DECODE(INSTR(@[tpsz],DECODE( L.DSP_SEQ, 2, CNTR_TPSZ_CD , '0'  )),0,NULL, DECODE( L.DSP_SEQ, 2, CNT , 0) )) AS count02,
