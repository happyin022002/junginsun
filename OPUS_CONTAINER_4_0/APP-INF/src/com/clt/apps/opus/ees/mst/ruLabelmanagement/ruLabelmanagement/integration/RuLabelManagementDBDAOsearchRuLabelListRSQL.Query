<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RuLabelManagementDBDAOsearchRuLabelListRSQL">
			<desc><![CDATA[RU Label Maintenace 조회]]></desc>
			<sql><![CDATA[
SELECT
	RSTR_USG_CD_SEQ		rstr_usg_cd_seq
,	RSTR_USG_TP_CD		rstr_usg_tp_cd
,	RSTR_USG_LBL_NM		rstr_usg_lbl_nm
,	RSTR_USG_LBL_DESC	rstr_usg_lbl_desc
,	RSTR_USG_DP_SEQ		rstr_usg_dp_seq
,	USR_DEF_RMK			usr_def_rmk
,	DELT_FLG			delt_flg
,	CRE_USR_ID			cre_usr_id
,	CRE_DT				cre_dt
,	UPD_USR_ID			upd_usr_id
,	TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI:SS') upd_dt
FROM 
	MST_RSTR_USG_CD 
WHERE 1=1
AND DELT_FLG = 'N'
#if (${ru_label_type} != '') 
	AND RSTR_USG_TP_CD = @[ru_label_type]
#end

#if (${ru_label_value} != '')
	AND UPPER(RSTR_USG_LBL_NM) Like '%'||UPPER(@[ru_label_value])||'%'
#end

ORDER BY RSTR_USG_TP_CD,RSTR_USG_DP_SEQ			]]></sql>
			<params>
				<param name="ru_label_type" type="12" value="" out="N"/>
				<param name="ru_label_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
