<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOGetCustInfoRSQL">
			<desc><![CDATA[getCustInfo]]></desc>
			<sql><![CDATA[
SELECT AGMT_ACT_CNT_CD	act_cust_cnt_cd
	   ,TRIM (TO_CHAR (AGMT_ACT_CUST_SEQ, '000000')) act_cust_seq
 FROM BKG_BOOKING
WHERE BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
