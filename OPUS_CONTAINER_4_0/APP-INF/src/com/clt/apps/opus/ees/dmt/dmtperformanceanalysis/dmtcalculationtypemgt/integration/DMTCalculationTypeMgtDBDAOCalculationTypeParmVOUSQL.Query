<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationTypeMgtDBDAOCalculationTypeParmVOUSQL">
			<desc><![CDATA[CalculationTypeParmVO update]]></desc>
			<sql><![CDATA[
UPDATE DMT_CALC_TP
SET CNT_CD	    =	NVL(@[cnt_cd],' ')
    , RGN_CD	=	NVL(@[rgn_cd],' ')
    , STE_CD	=	NVL(@[ste_cd],' ')
    , LOC_CD	=	NVL(@[loc_cd],' ')
    , IO_BND_CD	=	NVL(@[io_bnd_cd],' ')
    , DMDT_CALC_TP_CD	=	NVL(@[dmdt_calc_tp_cd],' ')
    , EFF_DT	=	TO_DATE(@[eff_dt], 'YYYYMMDD')
    , EXP_DT	=	TO_DATE(@[exp_dt], 'YYYYMMDD')
    , UPD_USR_ID	=	@[upd_usr_id]
    , UPD_DT	=	SYSDATE
    , UPD_OFC_CD	=	@[upd_ofc_cd]
WHERE CNT_CD	=	NVL(@[cnt_cd],' ')
  AND RGN_CD	=	NVL(@[rgn_cd],' ')
  AND STE_CD	=	NVL(@[ste_cd],' ')
  AND LOC_CD	=	NVL(@[loc_cd],' ')
  AND IO_BND_CD	=	NVL(@[io_bnd_cd],' ')
  AND DMDT_CALC_TP_CD	=	NVL(@[dmdt_calc_tp_cd],' ')			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dmdt_calc_tp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
