<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyVerifyEstimateFileListByLocationDataUSQL">
			<desc><![CDATA[modifyVerifyEstimateFileListByLocationData]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
       SET A.INP_MSG4 = 'LE'
WHERE A.TMP_SEQ = @[tmp_seq]
AND A.INP_MSG4 = 'SS'
AND NVL(A.INP_MSG1,'') NOT IN 
(
	SELECT EQ_LOC_CD
		FROM MNR_EQ_LOC_CD
		WHERE EQ_LOC_CD_LVL = '4'
)			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
