<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchTradeRSQL">
			<desc><![CDATA[상단 콤보에 나오는 검색 쿼리]]></desc>
			<sql><![CDATA[
SELECT  TRD_CD trd_cd
         , TRD_NM trd_nm
         , FM_CONTI_CD fm_conti_cd
         , TO_CONTI_CD to_conti_cd
FROM (
        SELECT 'A' PRI
                , 'ALL' TRD_CD
                , '' TRD_NM
                , '' FM_CONTI_CD
                , '' TO_CONTI_CD
        FROM DUAL
        UNION ALL   
        SELECT 'B' PRI
                , TRD_CD
                , TRD_NM
                , FM_CONTI_CD
                , TO_CONTI_CD
        FROM MDM_TRADE        
        WHERE 1=1
        AND DELT_FLG = 'N'
   )
ORDER BY PRI, TRD_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
