<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOValidationAgmtNoTpszRSQL">
			<desc><![CDATA[AGMT 관련 tpsz 비교]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(MAX(SYS_CONNECT_BY_PATH (CNTR_TPSZ_CD,'|')), 2) AS CNTR_TPSZ_CD
  FROM (SELECT  LAR.CNTR_TPSZ_CD
               ,ROW_NUMBER() OVER(PARTITION BY LAR.AGMT_SEQ ORDER BY LAR.CNTR_TPSZ_CD) AS RN
          FROM LSE_AGMT_RT LAR
         WHERE 1=1
           AND LAR.AGMT_CTY_CD = @[agmt_cty_cd]
           AND LAR.AGMT_SEQ    = @[agmt_seq]
           AND LAR.CNTR_RNTL_CHG_TP_CD = 'GENV'    
         ORDER BY LAR.CNTR_TPSZ_CD) SUB
 START WITH RN = 1
CONNECT BY PRIOR RN = RN - 1			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
