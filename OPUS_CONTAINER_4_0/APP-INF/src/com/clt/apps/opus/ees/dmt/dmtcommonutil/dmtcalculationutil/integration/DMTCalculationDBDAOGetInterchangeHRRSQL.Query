<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOGetInterchangeHRRSQL">
			<desc><![CDATA[GetInterchangeHR]]></desc>
			<sql><![CDATA[
SELECT	DECODE	(
				SIGN( TO_NUMBER(SUBSTR(@[from_dt],9,4)) - TO_NUMBER(@[cmnc_hr]||'00') ),
				1,
				TO_CHAR( TO_DATE(@[from_dt],'YYYYMMDDHH24MI') + 1,  'YYYYMMDDHH24MI'),
				@[from_dt]
				)	from_dt
FROM	DUAL			]]></sql>
			<params>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="cmnc_hr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
