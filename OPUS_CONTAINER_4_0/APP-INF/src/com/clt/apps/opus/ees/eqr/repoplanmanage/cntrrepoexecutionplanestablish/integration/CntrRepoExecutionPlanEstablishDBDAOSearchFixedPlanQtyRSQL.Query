<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOSearchFixedPlanQtyRSQL">
			<desc><![CDATA[SearchFixedPlanQty]]></desc>
			<sql><![CDATA[
SELECT	CNTR_QTY
FROM	EQR_VSL_LODG_DCHG_PLN_QTY
WHERE	REPO_PLN_ID		= @[repo_pln_id]
	AND	PLN_YRWK		= @[pln_yrwk]
	AND	PLN_SEQ			= @[pln_seq]
	AND	CNTR_TPSZ_CD	= @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="pln_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
