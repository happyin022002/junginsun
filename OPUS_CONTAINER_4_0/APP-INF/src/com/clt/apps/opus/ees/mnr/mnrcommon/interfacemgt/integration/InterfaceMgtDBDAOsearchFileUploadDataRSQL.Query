<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchFileUploadDataRSQL">
			<desc><![CDATA[InterfaceMgtDBDAOsearchFileUploadDataRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
A.ORG_FILE_NM,		
A.FILE_PATH_NM,
A.CRE_USR_ID,
TO_CHAR(A.CRE_DT,'YYYY-MM-DD') CRE_DT,
A.UPD_USR_ID,
A.UPD_DT,
DECODE(A.FILE_SEQ, 0, '1','0') FILE_DW,
A.FILE_SEQ,
A.FILE_DTL_SEQ,
A.MNR_GRP_TP_CD
FROM MNR_FILE_ATCH A
WHERE A.FILE_SEQ = @[file_seq]			]]></sql>
			<params>
				<param name="file_seq" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
