<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOSearchInOutBoundByMovementRSQL">
			<desc><![CDATA[CNTR Movement 정보에 대해서 해당 CNTR의 In/Out Bound 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT	OB_CNTR_FLG
FROM	CTM_MOVEMENT
WHERE	CNTR_NO		= @[cntr_no]
AND		CNMV_CYC_NO	= @[cntr_cyc_no]
AND		MVMT_STS_CD	= 'VD'
AND		ORG_YD_CD	= @[fm_mvmt_yd_cd]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="fm_mvmt_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
