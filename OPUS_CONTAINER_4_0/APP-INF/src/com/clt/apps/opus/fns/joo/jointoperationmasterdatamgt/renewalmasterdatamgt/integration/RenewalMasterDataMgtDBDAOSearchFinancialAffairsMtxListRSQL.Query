<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOSearchFinancialAffairsMtxListRSQL">
			<desc><![CDATA[Financial Affairs Matrix Search]]></desc>
			<sql><![CDATA[
SELECT A.JO_CRR_CD
     , A.RLANE_CD
     , A.RE_DIVR_CD
     , A.JO_STL_ITM_CD
     , A.JO_STL_ITM_NM
     , A.CUST_CNT_CD
     , A.CUST_SEQ
     , A.CUST_CD
     , (SELECT MC.CUST_LGL_ENG_NM 
          FROM MDM_CUSTOMER MC 
         WHERE MC.DELT_FLG      = 'N'
           AND MC.CUST_CNT_CD   = A.CUST_CNT_CD 
           AND MC.CUST_SEQ      = A.CUST_SEQ) AS CUST_LGL_ENG_NM
     , A.VNDR_SEQ AS VNDR_SEQ
     , (SELECT MV.VNDR_LGL_ENG_NM
          FROM MDM_VENDOR MV
         WHERE MV.DELT_FLG      = 'N'
           AND MV.VNDR_SEQ      = A.VNDR_SEQ) AS VNDR_LGL_ENG_NM
     , A.DR_ACCT_CD
     , (SELECT MA.ACCT_ENG_NM
          FROM MDM_ACCOUNT MA
         WHERE MA.DELT_FLG = 'N'
           AND MA.ACCT_CD  = A.DR_ACCT_CD) AS DR_ACCT_NM
     , A.DR_CTR_CD
     , A.DR_LOC_CD
     , A.CR_ACCT_CD
     , (SELECT MA.ACCT_ENG_NM
          FROM MDM_ACCOUNT MA
         WHERE MA.DELT_FLG = 'N'
           AND MA.ACCT_CD  = A.CR_ACCT_CD) AS CR_ACCT_NM
     , A.CR_CTR_CD
     , A.CR_LOC_CD
     , A.LOCL_CURR_CD
     , A.CRE_DT
     , A.CRE_USR_ID
     , A.UPD_DT
     , A.UPD_USR_ID
  FROM (
        SELECT B.JO_CRR_CD
             , B.RLANE_CD
             , B.RE_DIVR_CD
             , B.JO_STL_ITM_CD
             , D.JO_STL_ITM_NM
             , A.CUST_CNT_CD
             , A.CUST_SEQ
             , A.CUST_CNT_CD||LPAD(A.CUST_SEQ,6,'0') AS CUST_CD
             , A.VNDR_SEQ AS VNDR_SEQ
             , C.DR_ACCT_CD
             , B.DR_CTR_CD
             , B.DR_LOC_CD
             , C.CR_ACCT_CD
             , B.CR_CTR_CD
             , B.CR_LOC_CD
             , B.LOCL_CURR_CD
             , TO_CHAR(A.CRE_DT,'YYYY-MM-DD HH24:MI:SS') AS CRE_DT
             , A.CRE_USR_ID
             , TO_CHAR(A.UPD_DT,'YYYY-MM-DD HH24:MI:SS') AS UPD_DT
             , A.UPD_USR_ID
             , D.ORD_SEQ
          FROM JOO_CARRIER A
             , JOO_FINC_MTX B
             , JOO_STL_ITM_ACCT C
             , JOO_STL_ITM D
         WHERE 1=1
           AND A.DELT_FLG       = 'N'
           AND A.JO_CRR_CD      = B.JO_CRR_CD
           AND A.RLANE_CD       = B.RLANE_CD   
           AND B.RE_DIVR_CD     = C.RE_DIVR_CD
           AND B.JO_STL_ITM_CD  = C.JO_STL_ITM_CD
           AND C.JO_STL_ITM_CD  = D.JO_STL_ITM_CD
           AND B.JO_CRR_CD      = @[jo_crr_cd]
           AND B.RLANE_CD       = @[rlane_cd]
        #if (${re_divr_cd} != '')    
           AND B.RE_DIVR_CD     = @[re_divr_cd]
        #end  
        #if (${jo_stl_itm_cd} != '')    
           AND B.JO_STL_ITM_CD     = @[jo_stl_itm_cd]
        #end   
        ) A
 ORDER BY A.ORD_SEQ			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="jo_stl_itm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
