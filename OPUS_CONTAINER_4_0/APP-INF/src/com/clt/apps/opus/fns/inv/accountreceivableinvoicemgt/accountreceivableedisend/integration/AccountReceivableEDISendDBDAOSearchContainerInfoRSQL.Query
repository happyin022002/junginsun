<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchContainerInfoRSQL">
			<desc><![CDATA[Search Container Info]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD
     , MEAS_QTY
     , MEAS_UT_CD
     , CNTR_WGT
     , WGT_UT_CD
FROM BKG_CONTAINER
WHERE BKG_NO = @[bkg_no]
AND CNTR_NO = @[cntr_no] 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
