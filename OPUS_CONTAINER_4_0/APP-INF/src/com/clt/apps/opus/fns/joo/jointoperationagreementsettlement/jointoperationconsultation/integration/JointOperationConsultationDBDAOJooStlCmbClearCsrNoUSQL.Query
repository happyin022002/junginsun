<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOJooStlCmbClearCsrNoUSQL">
			<desc><![CDATA[JOO_STL_CMB에 CSR #를 clear시킨다.]]></desc>
			<sql><![CDATA[
UPDATE JOO_INVOICE
SET    SLP_TP_CD   = NULL,
       SLP_FUNC_CD = NULL,
       SLP_OFC_CD  = NULL,
       SLP_ISS_DT  = NULL,
       SLP_SER_NO  = NULL,
       UPD_DT      = SYSDATE,
       UPD_USR_ID  = @[upd_usr_id]
WHERE  SLP_TP_CD || SLP_FUNC_CD || SLP_OFC_CD  || SLP_ISS_DT  || SLP_SER_NO  = @[csr_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
