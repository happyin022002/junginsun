<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOSearchIvaRateMstIFNoRSQL">
			<desc><![CDATA[ManualARCreationDBDAOSearchIvaRateMstIFNoRSQL]]></desc>
			<sql><![CDATA[
SELECT ROUND(IVA_AMT / CHG_AMT * 100,2) IVA_RATE 
  FROM (SELECT SUM(CHG_AMT) CHG_AMT 
          FROM INV_AR_CHG A,
               INV_AR_MN  B
         WHERE A.AR_IF_NO = B.AR_IF_NO
           AND B.AR_IF_NO = @[mst_if_no]
           AND B.AR_OFC_CD = @[ofc_cd]
           AND B.REV_TP_CD = 'M'
           AND B.REV_SRC_CD = 'IV'
           AND NVL(B.INV_DELT_DIV_CD ,'N') <>'Y'
           AND B.BL_INV_CFM_DT IS NOT NULL
           AND A.TVA_FLG = 'Y') A,
       (SELECT SUM(CHG_AMT) IVA_AMT 
          FROM INV_AR_CHG A,
               INV_AR_MN  B
         WHERE A.AR_IF_NO = B.AR_IF_NO
           AND B.AR_IF_NO = @[mst_if_no]
           AND B.AR_OFC_CD = @[ofc_cd]
           AND B.REV_TP_CD = 'M'
           AND B.REV_SRC_CD = 'IV'
           AND NVL(B.INV_DELT_DIV_CD ,'N') <>'Y'
           AND B.BL_INV_CFM_DT IS NOT NULL
           AND A.CHG_CD = 'IVA') B           			]]></sql>
			<params>
				<param name="mst_if_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
