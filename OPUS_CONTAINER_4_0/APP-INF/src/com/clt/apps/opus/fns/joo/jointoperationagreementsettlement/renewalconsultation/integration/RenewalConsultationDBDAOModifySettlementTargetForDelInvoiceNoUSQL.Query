<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalConsultationDBDAOModifySettlementTargetForDelInvoiceNoUSQL">
			<desc><![CDATA[Invoice 삭제시 Settlement Target에 ACT_AMT, Remark에 삭제 데이타의 INV_NO, 금액을 Update 함.]]></desc>
			<sql><![CDATA[
UPDATE JOO_STL_TGT
   SET ACT_AMT          = ROUND(@[act_amt], NVL((SELECT DP_PRCS_KNT FROM MDM_CURRENCY WHERE CURR_CD = @[locl_curr_cd]) , 2))
     , STL_RMK          = @[stl_rmk]
     , UPD_DT           = SYSDATE
     , UPD_USR_ID       = @[cre_usr_id]
 WHERE VSL_CD           = @[vsl_cd]
   AND SKD_VOY_NO       = @[skd_voy_no]
   AND SKD_DIR_CD       = @[skd_dir_cd]
   AND REV_DIR_CD       = @[rev_dir_cd]
   AND REV_YRMON        = @[rev_yrmon]
   AND STL_VVD_SEQ      = TO_NUMBER(@[stl_vvd_seq])			]]></sql>
			<params>
				<param name="act_amt" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="stl_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
