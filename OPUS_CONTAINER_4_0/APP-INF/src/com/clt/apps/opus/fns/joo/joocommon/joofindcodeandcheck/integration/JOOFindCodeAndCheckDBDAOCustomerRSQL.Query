<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOCustomerRSQL">
			<desc><![CDATA[Customer inquiry]]></desc>
			<sql><![CDATA[
select 
	cust_cnt_cd||lpad(cust_seq,6,'0') as code,
	cust_lgl_eng_nm as name
from mdm_customer
where delt_flg = 'N'
and	cust_cnt_cd = substr(@[code],1,2)
and	cust_seq = to_number(substr(@[code],3))
order by 1			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
