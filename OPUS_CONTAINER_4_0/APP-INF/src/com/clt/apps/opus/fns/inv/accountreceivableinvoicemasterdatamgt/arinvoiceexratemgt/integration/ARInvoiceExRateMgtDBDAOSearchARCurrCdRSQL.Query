<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceExRateMgtDBDAOSearchARCurrCdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM MDM_ORGANIZATION
 WHERE AR_HD_QTR_OFC_CD = (SELECT  AR_HD_QTR_OFC_CD 
                             FROM  MDM_ORGANIZATION
                            WHERE  OFC_CD = @[ofc] ) 
   AND NVL(DELT_FLG, 'N') <> 'Y'
   AND OFC_CD = AR_OFC_CD
   AND AR_CURR_CD = @[curr_cd]			]]></sql>
			<params>
				<param name="ofc" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
