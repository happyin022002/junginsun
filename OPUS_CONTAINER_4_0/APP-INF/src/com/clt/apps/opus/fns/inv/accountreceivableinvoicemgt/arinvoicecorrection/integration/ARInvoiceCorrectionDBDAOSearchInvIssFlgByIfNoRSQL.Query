<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchInvIssFlgByIfNoRSQL">
			<desc><![CDATA[Search Inv Iss Flg By If No]]></desc>
			<sql><![CDATA[
SELECT INV_ISS_FLG
FROM INV_AR_MN
WHERE AR_IF_NO = @[ar_if_no]			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
