<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOCheckCarrierUsedRSQL">
			<desc><![CDATA[하위 테이블 Carrier 체크 여부]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN EXIST_CNT > 0 THEN 'Y' ELSE 'N' END AS EXIST_FLG
  FROM (
        SELECT COUNT(*) AS EXIST_CNT
          FROM JOO_STL_VVD SV
         WHERE 1=1
           AND SV.JO_CRR_CD = @[jo_crr_cd]
           AND SV.RLANE_CD  = @[rlane_cd]
       )			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
