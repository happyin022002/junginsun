<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOSearchMasterInvNoRSQL">
			<desc><![CDATA[Miscellaneous Revenue Invoice에서 Rev.Type 을 MOS 로 선택시 Mster Invoice No.를 입력하여야 하는데 이 값은 Invoice Main 의 orginal Invoice No 에 존재하여야 한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*), 0, 'N', 'Y') AS CHK_ORG_INV_NO
FROM INV_AR_MN
WHERE AR_IF_NO LIKE '___Z%'
AND BL_SRC_NO LIKE '___ZZ%'
AND AR_OFC_CD = @[ar_ofc_cd]
AND ORG_INV_NO = @[mst_inv_no]			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="mst_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
