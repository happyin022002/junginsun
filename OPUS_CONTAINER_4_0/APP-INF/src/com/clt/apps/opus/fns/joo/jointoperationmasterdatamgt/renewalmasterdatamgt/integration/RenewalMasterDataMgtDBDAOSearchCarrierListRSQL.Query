<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOSearchCarrierListRSQL">
			<desc><![CDATA[Renewal Carrier 조회.]]></desc>
			<sql><![CDATA[
SELECT A.JO_CRR_CD
     , A.RLANE_CD
     , A.VNDR_SEQ
     , (SELECT MV.VNDR_LGL_ENG_NM
          FROM MDM_VENDOR MV
         WHERE MV.DELT_FLG  = 'N'
           AND MV.VNDR_SEQ = A.VNDR_SEQ) AS VNDR_LGL_ENG_NM
     , A.CUST_CNT_CD
     , A.CUST_SEQ
     , A.CUST_CNT_CD||A.CUST_SEQ AS CUST_CD
     , (SELECT MC.CUST_LGL_ENG_NM
          FROM MDM_CUSTOMER MC
         WHERE MC.CUST_CNT_CD = A.CUST_CNT_CD
           AND MC.CUST_SEQ = A.CUST_SEQ) AS CUST_LGL_ENG_NM
     , A.TRD_CD
     , A.JO_STL_OPT_CD
     , A.DELT_FLG
     , TO_CHAR(A.CRE_DT,'YYYY-MM-DD HH24:MI:SS') AS CRE_DT
     , A.CRE_USR_ID
     , TO_CHAR(A.UPD_DT,'YYYY-MM-DD HH24:MI:SS') AS UPD_DT
     , A.UPD_USR_ID
     , (SELECT CASE WHEN COUNT(*) > 0 THEN 'O'
                    ELSE 'X'
               END
          FROM JOO_CRR_AUTH CA
         WHERE CA.JO_CRR_CD = A.JO_CRR_CD
           AND CA.RLANE_CD = A.RLANE_CD ) AS EXIST_AUTH_FLG
     , (SELECT CASE WHEN COUNT(*) > 0 THEN 'O'
                    ELSE 'X'
               END
          FROM JOO_FINC_MTX CA
         WHERE CA.JO_CRR_CD = A.JO_CRR_CD
           AND CA.RLANE_CD = A.RLANE_CD ) AS EXIST_FINC_MTX_FLG
     , (SELECT CASE WHEN COUNT(*) > 0 THEN 'O'
                    ELSE 'X'
               END
          FROM JOO_FINC_MTX CA
             , JOO_STL_VVD SV
         WHERE CA.JO_CRR_CD = A.JO_CRR_CD
           AND CA.RLANE_CD = A.RLANE_CD
           AND CA.JO_CRR_CD = SV.JO_CRR_CD
           AND CA.RLANE_CD = SV.RLANE_CD
           AND CA.RE_DIVR_CD = SV.RE_DIVR_CD ) AS EXIST_CHILD_FLG
     , (SELECT CASE WHEN COUNT(*) > 0 THEN 'O'
                    ELSE 'X'
               END
          FROM JOO_STL_BSS_PORT BP
         WHERE BP.JO_CRR_CD = A.JO_CRR_CD
           AND BP.RLANE_CD = A.RLANE_CD ) AS EXIST_STL_BSS_PORT_FLG
     , NVL((SELECT LOCL_CURR_CD
              FROM JOO_FINC_MTX CA
             WHERE CA.JO_CRR_CD = A.JO_CRR_CD
               AND CA.RLANE_CD = A.RLANE_CD
               AND ROWNUM = 1
          ),'USD') AS LOCL_CURR_CD
  FROM JOO_CARRIER A
 WHERE 1=1
#if (${jo_crr_cd} != '')
   AND A.JO_CRR_CD  = @[jo_crr_cd]
#end
#if (${rlane_cd} != '')
   AND A.RLANE_CD   = @[rlane_cd]
#end
#if (${trd_cd} != '')
   AND A.TRD_CD     = @[trd_cd]
#end
#if (${auth_ofc_cd}!='')
   /* Condition Auth Office Cd */
   AND EXISTS   (   SELECT 'X'
                      FROM JOO_CRR_AUTH CA
                     WHERE 1=1
                       AND CA.DELT_FLG         = 'N'
                       AND CA.JO_CRR_CD        = A.JO_CRR_CD
                       AND CA.RLANE_CD         = A.RLANE_CD
                       AND CA.AUTH_OFC_CD      = @[auth_ofc_cd]
                     )
#end
#if (${delt_flg} != '')
   AND A.DELT_FLG   = @[delt_flg]
#end
 ORDER BY A.JO_CRR_CD, A.TRD_CD, A.RLANE_CD, A.VNDR_SEQ			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="auth_ofc_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
