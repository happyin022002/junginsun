<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOSearchFinancialAffairsMtxOfficeRSQL">
			<desc><![CDATA[Financial Affairs Matrix Office & AP Control Code Search]]></desc>
			<sql><![CDATA[
SELECT D.OFC_CD||'|'||D.LOC_CD||'|'||D.AP_CTR_CD AS OFF_CDS
  FROM MDM_ORGANIZATION D
     , MDM_ORGANIZATION E
 WHERE D.OFC_CD = E.AP_OFC_CD
   AND E.OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
