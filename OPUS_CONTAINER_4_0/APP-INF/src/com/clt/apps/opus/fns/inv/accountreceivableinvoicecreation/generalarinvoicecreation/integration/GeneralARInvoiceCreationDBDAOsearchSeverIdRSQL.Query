<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOsearchSeverIdRSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOsearchSeverIdRSQL]]></desc>
			<sql><![CDATA[
SELECT DECODE(C.SYS_AREA_GRP_ID, 'KOR', DECODE(SUBSTR(A.LOC_CD, 1, 2), 'JP', 'JPN'
                                                                           , 'KOR')
                                      , C.SYS_AREA_GRP_ID) SVR_ID
  FROM MDM_ORGANIZATION A
     , COM_SYS_AREA_GRP_ID C
 WHERE SUBSTR(A.LOC_CD, 1, 2) = C.CNT_CD
   AND C.CO_IND_CD = 'H'
   AND A.OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
