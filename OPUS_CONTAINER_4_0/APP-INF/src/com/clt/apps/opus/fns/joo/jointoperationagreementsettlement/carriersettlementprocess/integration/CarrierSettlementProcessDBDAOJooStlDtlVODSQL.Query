<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOJooStlDtlVODSQL">
			<desc><![CDATA[JOO_STL_DTL삭제]]></desc>
			<sql><![CDATA[
DELETE FROM JOO_STL_DTL
WHERE	ACCT_YRMON = REPLACE(@[acct_yrmon],'-','')
AND	STL_VVD_SEQ = @[stl_vvd_seq]
AND	STL_SEQ = @[stl_seq]			]]></sql>
