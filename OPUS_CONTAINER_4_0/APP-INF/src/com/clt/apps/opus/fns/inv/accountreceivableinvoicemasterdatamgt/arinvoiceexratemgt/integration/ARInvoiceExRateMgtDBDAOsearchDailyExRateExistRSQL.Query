<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceExRateMgtDBDAOsearchDailyExRateExistRSQL">
			<desc><![CDATA[ARInvoiceExRateMgtDBDAO::searchDailyExRateExist ( exRateVo )
Daily VVD 환율이 존재하는지 CHECK.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM INV_CUST_AND_DLY_XCH_RT
 WHERE XCH_RT_TP_CD = 'V'
   AND AR_OFC_CD = @[ar_ofc_cd]
   AND IO_BND_CD = @[io_bnd_cd]
   AND FM_DT IN (
#foreach(${key} in ${xch_rt_dt})  
	#if($velocityCount == 1)
        '${key}'
  	#else 
       ,'${key}'
  	#end
#end
  )			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
