<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOSearchDefaultDRRevAcctRSQL">
			<desc><![CDATA[searchDefaultDRRevAcct]]></desc>
			<sql><![CDATA[
SELECT AR_ACCT_CD ACCT_CD
     , ACCT_TP_NM ACCT_ENG_NM
FROM SAR_ACCT_MTX
WHERE ACCT_CTNT1 = 'REC'
AND ACCT_CTNT2 = 'INVAR'
AND ACCT_TP_CD = 'MTH'
#if (${gl_eff_dt} != '')
AND NVL(ACCT_END_DT, REPLACE(@[gl_eff_dt],'-','')) >= REPLACE(@[gl_eff_dt],'-','')
#end
AND ROWNUM = 1 			]]></sql>
			<params>
				<param name="gl_eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
