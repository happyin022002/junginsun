<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchMaxIFNoBKGNoByINVNoRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchMaxIFNoBKGNoByINVNoRSQL]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(MAX(DECODE(REV_TP_CD, 'M', 'A', 'B')||AR_IF_NO), 2, 11) AR_IF_NO
     , BKG_NO
     , IO_BND_CD
     , INV_REF_NO
     , BL_SRC_NO
  FROM INV_AR_MN
 WHERE AR_IF_NO IN (SELECT AR_IF_NO
                      FROM INV_AR_ISS_DTL
                     WHERE INV_NO = @[inv_no])
 GROUP BY BKG_NO
     , IO_BND_CD
     , INV_REF_NO
     , BL_SRC_NO			]]></sql>
			<params>
				<param name="inv_no" type="12" value="DBF000015" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
