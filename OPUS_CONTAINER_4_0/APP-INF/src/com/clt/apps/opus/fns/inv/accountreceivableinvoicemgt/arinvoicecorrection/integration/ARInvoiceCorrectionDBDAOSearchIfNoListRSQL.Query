<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchIfNoListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT DISTINCT MN.AR_IF_NO,
	   MN.IO_BND_CD BND,
	   MN.SAIL_ARR_DT SA_DT,
	   MN.AR_OFC_CD OFC_CD,
	   MN.GL_EFF_DT,
       MN.BKG_NO,
       MN.SAIL_DT,
       DTL.INV_NO
  FROM INV_AR_ISS_DTL DTL,
       INV_AR_CHG     CHG,
       INV_AR_MN      MN
 WHERE DTL.AR_IF_NO = CHG.AR_IF_NO
   AND DTL.CHG_SEQ      = CHG.CHG_SEQ
   AND CHG.AR_IF_NO     = MN.AR_IF_NO
   AND MN.AR_OFC_CD       = @[ar_ofc_cd]
   AND DTL.INV_NO         = @[inv_no]
   AND MN.REV_TP_CD <> 'M'
   AND NVL(MN.INV_DELT_DIV_CD,'N') <> 'Y'
   -- AND NVL(MN.INV_SPLIT_CD,'N') NOT IN ('M','X') 2010-01-18 이상희 과장
 ORDER BY MN.AR_IF_NO DESC			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
