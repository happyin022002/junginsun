<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOCheckSlipByStlCmbSeqRSQL">
			<desc><![CDATA[전표 체크.]]></desc>
			<sql><![CDATA[
SELECT MAX(A.SLP_TP_CD||A.SLP_FUNC_CD||A.SLP_OFC_CD||A.SLP_ISS_DT||A.SLP_SER_NO) AS CSR_NO
  FROM JOO_STL_CMB A
 WHERE 1=1
   AND A.RVS_CMB_FLG = 'N'
   AND A.RJCT_CMB_FLG = 'N'
   AND A.ACCT_YRMON = REPLACE(@[acct_yrmon],'-','')
   AND A.JO_CRR_CD = @[jo_crr_cd]
   AND A.STL_CMB_SEQ = @[stl_cmb_seq]			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="2015-07" out="N"/>
				<param name="jo_crr_cd" type="12" value="APL" out="N"/>
				<param name="stl_cmb_seq" type="12" value="2" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
