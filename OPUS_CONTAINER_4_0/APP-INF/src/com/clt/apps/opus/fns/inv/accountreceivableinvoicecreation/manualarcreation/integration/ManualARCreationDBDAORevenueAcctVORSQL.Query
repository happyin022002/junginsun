<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAORevenueAcctVORSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT AR_ACCT_CD ACCT_CD
		, ACCT_TP_NM ACCT_ENG_NM
		, 'MTH' REV_CD
		, ACCT_TP_CD CHG_CD
		, ACCT_TP_CD REP_CHG_CD
FROM SAR_ACCT_MTX
WHERE ACCT_CTNT1 = 'REV'
AND ACCT_CTNT4 = 'MTH'
AND REV_ACCT_DIV_CD = 'N'
#if (${gl_eff_dt} != '')
AND NVL(ACCT_END_DT, REPLACE(@[gl_eff_dt],'-','')) >= REPLACE(@[gl_eff_dt],'-','')
#end
UNION ALL
SELECT B.ACCT_CD
		, B.ACCT_ENG_NM
		, 'MTH' REV_CD
		, A.CHG_CD
		, A.REP_CHG_CD
FROM MDM_CHARGE A,
	 MDM_ACCOUNT B
WHERE A.CO_CHG_ACCT_CD = B.ACCT_CD
AND A.CHG_CD = 'TVA'
ORDER BY ACCT_CD			]]></sql>
			<params>
				<param name="gl_eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
