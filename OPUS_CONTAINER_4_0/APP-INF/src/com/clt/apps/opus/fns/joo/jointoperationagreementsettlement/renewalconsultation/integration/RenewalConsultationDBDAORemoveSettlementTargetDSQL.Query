<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalConsultationDBDAORemoveSettlementTargetDSQL">
			<desc><![CDATA[Delete Settlement Target]]></desc>
			<sql><![CDATA[
DELETE
  FROM JOO_STL_TGT ST
 WHERE 1=1
   AND ST.VSL_CD        = @[vsl_cd]
   AND ST.SKD_VOY_NO    = @[skd_voy_no]
   AND ST.SKD_DIR_CD    = @[skd_dir_cd]
   AND ST.REV_DIR_CD    = @[rev_dir_cd]
   AND ST.REV_YRMON     = @[rev_yrmon]
   AND ST.STL_VVD_SEQ   = TO_NUMBER(@[stl_vvd_seq])			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
