<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchCntrTpSzForERPRSQL">
			<desc><![CDATA[BookingARCreationDBDAOSearchCntrTpSzForERPRSQL]]></desc>
			<sql><![CDATA[
SELECT A.CNTR_TPSZ_CD 
     , NVL(COUNT(1),0) CNTR_TPSZ_CNT   
  FROM INV_AR_CNTR A
 WHERE A.AR_IF_NO = @[if_no]     
 GROUP BY A.CNTR_TPSZ_CD    			]]></sql>
			<params>
				<param name="if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
