<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOInvIssDtlECSQL">
			<desc><![CDATA[InvIssDtlE]]></desc>
			<sql><![CDATA[
INSERT INTO INV_AR_ISS_DTL
(INV_NO, AR_IF_NO, CHG_SEQ, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT) 
SELECT A.INV_NO
       ,A.AR_IF_NO
       ,B.CHG_SEQ
       , @[user_id]                     CRE_USR_ID     
       , SYSDATE                      CRE_DT         
       , @[user_id]                     UPD_USR_ID     
       , SYSDATE                      UPD_DT
  FROM ( 
        SELECT 
				#if (${ofc_cd} == 'DXBBB')
				INV_PFX_CD|| @[issue_type]||LPAD(NVL(INV_MAX_SEQ, 0) + ROW_NUMBER() OVER (PARTITION BY AR_OFC_CD ORDER BY  ACT_CUST_CNT_CD,ACT_CUST_SEQ,VSL_CD,SKD_VOY_NO,SKD_DIR_CD,IO_BND_CD,PORT_CD,SVC_SCP_CD,BL_SRC_NO,INV_ISS_TP_CD,USD_XCH_RT,AR_IF_NO), 6, '0') INV_NO
				#else
				INV_PFX_CD||LPAD(NVL(INV_MAX_SEQ, 0) + ROW_NUMBER() OVER (PARTITION BY AR_OFC_CD ORDER BY  ACT_CUST_CNT_CD,ACT_CUST_SEQ,VSL_CD,SKD_VOY_NO,SKD_DIR_CD,IO_BND_CD,PORT_CD,SVC_SCP_CD,BL_SRC_NO,INV_ISS_TP_CD,USD_XCH_RT,AR_IF_NO), 7, '0') INV_NO
				#end
             , AR_IF_NO
          FROM (
                SELECT V1.ACT_CUST_CNT_CD
                     , V1.ACT_CUST_SEQ
                     , V1.VSL_CD
                     , V1.SKD_VOY_NO
                     , V1.SKD_DIR_CD
                     , V1.IO_BND_CD
                     , V1.PORT_CD
                     , V1.SVC_SCP_CD
  					 #if (${inv_mlt_bl_iss_flg} != 'Y')
                     , V1.BL_SRC_NO
					 #else
					 ,'' BL_SRC_NO
  					 #end
                     , V1.INV_ISS_TP_CD
                     , V1.INV_SPLIT_CD
                     , V1.USD_XCH_RT
                     , @[inv_pfx_cd]  INV_PFX_CD --V2.INV_PFX_CD
                     , @[inv_max_seq] INV_MAX_SEQ --V2.INV_MAX_SEQ
                     , V1.AR_OFC_CD
                     , V1.AR_IF_NO
                  FROM INV_AR_ISS_FTR V1
                 WHERE INV_ISS_WRK_NO = @[wrk_no]
                 GROUP BY V1.ACT_CUST_CNT_CD
                     , V1.ACT_CUST_SEQ
                     , V1.VSL_CD
                     , V1.SKD_VOY_NO
                     , V1.SKD_DIR_CD
                     , V1.IO_BND_CD
                     , V1.PORT_CD
                     , V1.SVC_SCP_CD
					 #if (${inv_mlt_bl_iss_flg} != 'Y')
                     , V1.BL_SRC_NO
					 #else
					 ,''
					 #end
                     , V1.INV_ISS_TP_CD
                     , V1.INV_SPLIT_CD
                     , V1.USD_XCH_RT
                     , @[inv_pfx_cd] -- V2.INV_PFX_CD
                     , @[inv_max_seq] --V2.INV_MAX_SEQ
                     , V1.AR_OFC_CD
                     , V1.AR_IF_NO
               )
       ) A,
       INV_AR_CHG B
 WHERE A.AR_IF_NO = B.AR_IF_NO			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="issue_type" type="12" value="" out="N"/>
				<param name="inv_pfx_cd" type="12" value="" out="N"/>
				<param name="inv_max_seq" type="12" value="" out="N"/>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
