<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOTemplateVOUSQL">
			<desc><![CDATA[TemplateVO]]></desc>
			<sql><![CDATA[
UPDATE	INV_CPRT_TMPLT SET
		USR_DEF_NM = @[usr_def_nm]
		,UPD_USR_ID = @[upd_usr_id]
		,ITM_SEQ = @[itm_seq]
		,RPT_AUTH_ID = @[rpt_auth_id]
		,UPD_DT = SYSDATE
WHERE	RPT_TMPLT_NM = @[rpt_tmplt_nm]
AND		RPT_ITM_ID = @[rpt_itm_id]
AND     AR_OFC_CD = @[ar_ofc_cd]			]]></sql>
			<params>
				<param name="usr_def_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="itm_seq" type="12" value="" out="N"/>
				<param name="rpt_auth_id" type="12" value="" out="N"/>
				<param name="rpt_tmplt_nm" type="12" value="" out="N"/>
				<param name="rpt_itm_id" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
