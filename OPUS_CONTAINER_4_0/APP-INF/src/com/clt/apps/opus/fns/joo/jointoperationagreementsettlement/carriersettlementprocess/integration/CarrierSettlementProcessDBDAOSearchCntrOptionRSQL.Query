<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOSearchCntrOptionRSQL">
			<desc><![CDATA[Option Search]]></desc>
			<sql><![CDATA[
SELECT PPT_CD
     , LINE_SEQ
     , ATTR_CTNT1 AS SLAN_CD
     , ATTR_CTNT2 AS OPTION_A
     , ATTR_CTNT3 AS OPTION_B
  FROM JOO_COM_PPT
 WHERE PPT_CD = @[ppt_cd]			]]></sql>
			<params>
				<param name="ppt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
