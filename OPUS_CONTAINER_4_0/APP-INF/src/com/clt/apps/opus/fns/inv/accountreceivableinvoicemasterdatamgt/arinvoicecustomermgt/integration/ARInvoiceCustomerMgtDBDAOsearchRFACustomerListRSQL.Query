<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOsearchRFACustomerListRSQL">
			<desc><![CDATA[inv_0090]]></desc>
			<sql><![CDATA[
SELECT 'Customer' CUSTOMER_TYPE,A.CTRT_CUST_CNT_CD CUST_CD,LPAD(A.CTRT_CUST_SEQ,6,0) CUST_SEQ,C.CUST_LGL_ENG_NM CUST_NM 
FROM PRI_RP_MN A,PRI_RP_HDR B,MDM_CUSTOMER C
WHERE A.PROP_NO = B.PROP_NO
AND A.CTRT_CUST_CNT_CD = C.CUST_CNT_CD
AND A.CTRT_CUST_SEQ = C.CUST_SEQ
AND B.RFA_NO = @[rfa_no]
UNION
SELECT 'Affiliate' CUSTOMER_TYPE,A.CUST_CNT_CD CUST_CD,LPAD(A.CUST_SEQ,6,0) CUST_SEQ,C.CUST_LGL_ENG_NM CUST_NM 
FROM PRI_RP_AFIL A,PRI_RP_HDR B,MDM_CUSTOMER C
WHERE A.PROP_NO = B.PROP_NO
AND A.CUST_CNT_CD = C.CUST_CNT_CD
AND A.CUST_SEQ = C.CUST_SEQ
AND B.RFA_NO = @[rfa_no]
			]]></sql>
			<params>
				<param name="rfa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
