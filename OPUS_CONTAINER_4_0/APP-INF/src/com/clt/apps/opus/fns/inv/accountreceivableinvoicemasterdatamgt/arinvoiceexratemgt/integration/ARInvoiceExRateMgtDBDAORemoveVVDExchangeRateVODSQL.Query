<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceExRateMgtDBDAORemoveVVDExchangeRateVODSQL">
			<desc><![CDATA[ARInvoiceExRateMgtDBDAO::removeVVDExchangeRate ( vvdExrateVos )
INV_VVD_XCH_RT  테이블에 delete]]></desc>
			<sql><![CDATA[
DELETE FROM INV_VVD_XCH_RT
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	PORT_CD = @[port_cd]
AND	SVC_SCP_CD = @[svc_scp_cd]
AND	IO_BND_CD = @[io_bnd_cd]
AND	LOCL_CURR_CD = @[locl_curr_cd] 
AND	CHG_CURR_CD = @[chg_curr_cd]
AND AR_OFC_CD = @[ar_ofc_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="chg_curr_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
