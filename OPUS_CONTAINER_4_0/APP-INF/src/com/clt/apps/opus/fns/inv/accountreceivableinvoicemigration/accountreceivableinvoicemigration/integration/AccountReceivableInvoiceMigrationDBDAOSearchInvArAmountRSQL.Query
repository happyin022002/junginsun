<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableInvoiceMigrationDBDAOSearchInvArAmountRSQL">
			<desc><![CDATA[Search Inv Ar Amount]]></desc>
			<sql><![CDATA[
select AA.ar_if_no ar_if_no,
        rownum ar_if_ser_no, 
        tj_src_nm, 
        curr_cd, 
        inv_amt
   from 
		(select ar_if_no , tj_src_nm , curr_cd, sum(chg_amt) inv_amt  
 		   from OPUSADM_TMP.inv_ar_chg
 		  where ar_if_no = @[ar_if_no]
 		  group by ar_if_no , tj_src_nm , curr_cd ) AA			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
