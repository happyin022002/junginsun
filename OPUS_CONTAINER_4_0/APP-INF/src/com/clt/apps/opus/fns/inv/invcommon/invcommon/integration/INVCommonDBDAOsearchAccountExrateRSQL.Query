<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOsearchAccountExrateRSQL">
			<desc><![CDATA[searchAccountExrate]]></desc>
			<sql><![CDATA[
SELECT NVL(ROUND(A.USD_LOCL_XCH_RT/B.USD_LOCL_XCH_RT, 6), '0') EX_RATE
FROM GL_MON_XCH_RT A, GL_MON_XCH_RT B
#if (${bkg_no} != '') 
	WHERE A.ACCT_XCH_RT_YRMON = (SELECT  /*+index( B XPKBKG_VVD) */ MIN(SUBSTR(TO_CHAR(A.VPS_ETD_DT, 'yyyymmdd'), 1, 6))
                            	 FROM VSK_VSL_PORT_SKD A, BKG_VVD B
                           		 WHERE 1 = 1
                             	 AND B.BKG_NO           = @[bkg_no]                      
                             	 AND A.VSL_CD(+)       = B.VSL_CD
                             	 AND A.SKD_VOY_NO(+) = B.SKD_VOY_NO
                            	 AND A.SKD_DIR_CD(+)    = B.SKD_DIR_CD
                             	 AND A.VPS_PORT_CD(+)    = B.POL_CD
                             	 AND A.CLPT_IND_SEQ(+)  = '1'
                             	 AND A.VPS_ETD_DT IS NOT NULL)
#else
    WHERE A.ACCT_XCH_RT_YRMON = (SELECT SUBSTR(TO_CHAR(VPS_ETD_DT, 'yyyymmdd'), 1, 6)
  								 FROM VSK_VSL_PORT_SKD
 								 WHERE VSL_CD = @[vsl]
   								 AND SKD_VOY_NO = @[voy]
  								 AND SKD_DIR_CD = @[dep]
  								 AND VPS_PORT_CD = @[pol]
  								 AND CLPT_IND_SEQ ='1'
  								 AND VPS_ETD_DT IS NOT NULL)
#end 
AND A.ACCT_XCH_RT_LVL = '1'
AND A.CURR_CD = @[lcl_curr]
AND A.ACCT_XCH_RT_YRMON = B.ACCT_XCH_RT_YRMON
AND A.ACCT_XCH_RT_LVL = B.ACCT_XCH_RT_LVL
AND B.CURR_CD = @[curr]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vsl" type="12" value="" out="N"/>
				<param name="voy" type="12" value="" out="N"/>
				<param name="dep" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
				<param name="lcl_curr" type="12" value="" out="N"/>
				<param name="curr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
