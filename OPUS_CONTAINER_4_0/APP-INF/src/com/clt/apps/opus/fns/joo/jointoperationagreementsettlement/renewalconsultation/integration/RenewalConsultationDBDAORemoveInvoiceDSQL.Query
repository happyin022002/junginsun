<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalConsultationDBDAORemoveInvoiceDSQL">
			<desc><![CDATA[Invoice Delete]]></desc>
			<sql><![CDATA[
DELETE
  FROM JOO_INVOICE INV
 WHERE 1=1
   AND INV.ACCT_YRMON   = @[acct_yrmon]
   AND INV.JO_CRR_CD    = @[jo_crr_cd]
   AND INV.INV_NO       = @[inv_no]
   AND INV.RE_DIVR_CD   = @[re_divr_cd]			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
