<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOTradeRSQL">
			<desc><![CDATA[Trade 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.TRD_CD AS CODE
     , A.TRD_CD AS NAME
  FROM JOO_CARRIER A
     , JOO_CRR_AUTH AUTH
 WHERE A.DELT_FLG = 'N'
   AND A.JO_CRR_CD = AUTH.JO_CRR_CD
   AND A.RLANE_CD  = AUTH.RLANE_CD
#if (${auth_delcheck_yn} == 'N') -- N : WHERE 절에 걸지 않음.(AUTH.DELT_FLG)
   --AND NVL(AUTH.DELT_FLG,'N') = 'N'
#else
   AND NVL(AUTH.DELT_FLG,'N') = 'N'
#end
#if (${ofc_cd} != '')   
   AND AUTH.AUTH_OFC_CD = @[ofc_cd]
#end
#if (${auth_ofc_cd} != '')
   /*2016.08.01 추가 개발된 코드.*/
   AND AUTH.AUTH_OFC_CD = @[auth_ofc_cd]
#end
#if (${super_cd1} != '')
   AND A.JO_CRR_CD = @[super_cd1]
#end
#if (${super_cds1}!=''  && ${super_cds1} != 'ALL')
   /* Condition super_cds */
   AND A.JO_CRR_CD    IN ( #foreach($key IN ${super_cds1})#if($velocityCount < $super_cds1.size()) '$key', #else '$key' #end #end)
#end
#if (${code} != '')
   AND A.TRD_CD = @[code]
#end
ORDER BY 1			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="A" out="N"/>
				<param name="auth_ofc_cd" type="12" value="" out="N"/>
				<param name="super_cd1" type="12" value="A" out="N"/>
				<param name="code" type="12" value="A" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
