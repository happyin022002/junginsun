<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyNewInterfaceNoVOUSQL">
			<desc><![CDATA[[Max I/F Seq 를 구했을 경우] BookingARCreationDBDAO::modifyNewInterfaceNo ( ofcCd , maxSeq )]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_BKG_IF_NO SET 
	BKG_MAX_SEQ = @[max_seq]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	IF_PFX_CD = DECODE(SUBSTR(@[ofc_cd], 1,3),'FXT','LON',SUBSTR(@[ofc_cd], 1,3))			]]></sql>
			<params>
				<param name="max_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
