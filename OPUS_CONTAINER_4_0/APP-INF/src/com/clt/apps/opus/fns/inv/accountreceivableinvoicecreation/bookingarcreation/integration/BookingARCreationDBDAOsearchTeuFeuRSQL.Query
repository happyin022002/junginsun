<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchTeuFeuRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchTeuFeuRSQL]]></desc>
			<sql><![CDATA[
SELECT SUM(DECODE(SUBSTR(CNTR_TPSZ_CD, 2, 1), '2', OP_CNTR_QTY, 0)) AS TEU
     , SUM(DECODE(SUBSTR(CNTR_TPSZ_CD, 2, 1), '2', 0, OP_CNTR_QTY)) AS FEU
  FROM BKG_QUANTITY
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
