<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOmodifySendNoUSQL">
			<desc><![CDATA[InvoiceIssueDBDAOmodifySendNoUSQL]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_ISS SET 
 #if (${send_flag} == 'E') 
       EML_SND_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[iss_ofc_cd])
     , EML_SND_NO = NVL(@[snd_no], '')
 #else
       FAX_SND_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[iss_ofc_cd])
     , FAX_SND_NO = NVL(@[snd_no], '')
 #end
     , UPD_USR_ID = @[user_id]
     , UPD_DT = SYSDATE
 WHERE INV_NO = @[inv_no]
   AND INV_SEQ = (SELECT MAX(INV_SEQ) FROM INV_AR_ISS WHERE INV_NO = @[inv_no])			]]></sql>
			<params>
				<param name="iss_ofc_cd" type="12" value="" out="N"/>
				<param name="snd_no" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
