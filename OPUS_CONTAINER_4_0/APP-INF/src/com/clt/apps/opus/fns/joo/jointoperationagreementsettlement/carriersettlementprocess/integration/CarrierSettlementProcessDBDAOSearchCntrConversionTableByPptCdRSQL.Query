<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOSearchCntrConversionTableByPptCdRSQL">
			<desc><![CDATA[Search joo_com_ppt]]></desc>
			<sql><![CDATA[
SELECT DISTINCT B.ATTR_CTNT1 AS SLAN_CD
  FROM JOO_COM_PPT B
 WHERE B.PPT_CD = @[ppt_cd]			]]></sql>
			<params>
				<param name="ppt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
