<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOCheckAccountRSQL">
			<desc><![CDATA[Account Number Validation Check]]></desc>
			<sql><![CDATA[
select 
       acct_cd     as code,
       acct_eng_nm as name
from   mdm_account
where  delt_flg = 'N'
#if (${code} != '')
and    acct_cd = @[code]
#end
order  by 1			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
