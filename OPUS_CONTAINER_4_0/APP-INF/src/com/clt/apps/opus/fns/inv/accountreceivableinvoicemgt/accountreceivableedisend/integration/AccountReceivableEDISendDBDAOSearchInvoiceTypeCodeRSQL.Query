<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchInvoiceTypeCodeRSQL">
			<desc><![CDATA[SearchInvoiceTypeCode]]></desc>
			<sql><![CDATA[
SELECT DECODE(REV_TP_CD, 'M', '02', DECODE(INV_DELT_DIV_CD, 'N', '01', '03')) INV_TP_CD
FROM INV_AR_MN
WHERE AR_IF_NO = @[ar_if_no]
			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
