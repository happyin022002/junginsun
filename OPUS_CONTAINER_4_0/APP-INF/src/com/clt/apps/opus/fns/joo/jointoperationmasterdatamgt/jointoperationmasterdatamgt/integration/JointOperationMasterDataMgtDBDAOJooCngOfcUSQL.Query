<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOJooCngOfcUSQL">
			<desc><![CDATA[Office Mapping 정보 Update]]></desc>
			<sql><![CDATA[
UPDATE joo_cng_ofc SET
OFC_CD = @[ofc_cd],
CNG_OFC_CD = @[cng_ofc_cd],
EFF_DT = @[eff_dt],

#if(${delt_flg} == 'Y')
EXP_DT = to_char(sysdate,'yyyymmdd'),
#else
EXP_DT = @[exp_dt],
#end

OFC_CNG_RMK = @[ofc_cng_rmk],
DELT_FLG = nvl(@[delt_flg],'N'),
CRE_DT = sysdate,
CRE_USR_ID = @[cre_usr_id],
UPD_DT = sysdate,
UPD_USR_ID = @[upd_usr_id]
WHERE OFC_CD = @[ofc_cd]
AND CNG_OFC_CD = @[cng_ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cng_ofc_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="ofc_cng_rmk" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
