<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOCheckFinancialAffairsMtxCurrencyRSQL">
			<desc><![CDATA[Financial Affairs Matrix Local Currency Change Check.]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN NVL(@[locl_curr_cd],'USD') = A.LOCL_CURR_CD THEN 'N' ELSE 'Y' END AS CHK_FLG
  FROM (
        SELECT MAX(A.LOCL_CURR_CD) AS LOCL_CURR_CD
          FROM JOO_FINC_MTX A
         WHERE 1=1
           AND A.JO_CRR_CD      = @[jo_crr_cd]
           AND A.RLANE_CD       = @[rlane_cd] 
        ) A			]]></sql>
			<params>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
