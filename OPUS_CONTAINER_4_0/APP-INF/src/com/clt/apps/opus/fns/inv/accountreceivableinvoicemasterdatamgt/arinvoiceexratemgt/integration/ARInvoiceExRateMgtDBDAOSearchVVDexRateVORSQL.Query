<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceExRateMgtDBDAOSearchVVDexRateVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
select
vsl_cd||skd_voy_no||skd_dir_cd vsl_cd,
skd_voy_no,
skd_dir_cd,
port_cd,
svc_scp_cd,
decode(io_bnd_cd,'I','I/B','O/B') io_bnd_cd,
locl_curr_cd,
chg_curr_cd,
a.ar_ofc_cd ar_ofc_cd,
decode(XCH_RT_RVS_FLG,'Y',ivs_xch_rt,inv_xch_rt) inv_xch_rt,
decode(XCH_RT_RVS_FLG,'Y',inv_xch_rt,ivs_xch_rt) ivs_xch_rt
from inv_vvd_xch_rt a, inv_ar_stup_ofc b
where a.AR_OFC_CD = b.AR_OFC_CD
and	vsl_cd = @[vsl_cd]
and skd_voy_no=@[skd_voy_no] 
and skd_dir_cd=@[skd_dir_cd]
#if (${port_cd} != '') 
and	port_cd = @[port_cd]
#end
#if (${locl_curr_cd} != '') 
and	locl_curr_cd = @[locl_curr_cd]
#end
#if (${svc_scp_cd} != '') 
and	svc_scp_cd = upper(@[svc_scp_cd])
#end
#if (${io_bnd_cd} != '') 
and	io_bnd_cd = @[io_bnd_cd]
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="YUTT0016W" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
