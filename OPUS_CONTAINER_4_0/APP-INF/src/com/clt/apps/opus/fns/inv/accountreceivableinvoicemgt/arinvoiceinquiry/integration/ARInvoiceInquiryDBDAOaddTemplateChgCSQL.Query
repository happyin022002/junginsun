<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOaddTemplateChgCSQL">
			<desc><![CDATA[addTemplateChg]]></desc>
			<sql><![CDATA[
INSERT INTO INV_CPRT_TMPLT_CHG	(
           AR_OFC_CD         
          ,RPT_TMPLT_NM     
          ,RPT_ITM_ID       
          ,CPRT_TP_CTNT     
          ,CPRT_VAL_CTNT    
          ,DP_SEQ           
          ,CHG_CD 
          ,CPRT_CHG_GRP_FLG           
          ,CRE_USR_ID       
          ,CRE_DT           
          ,UPD_USR_ID       
          ,UPD_DT           
)VALUES( @[ar_ofc_cd]
		,@[rpt_tmplt_nm]
		,@[rpt_itm_id]
		,@[cprt_tp_ctnt]     
        ,@[cprt_val_ctnt]
		,@[dp_seq]
		,@[chg_cd]
		,@[cprt_chg_grp_flg]
		,NVL(@[cre_usr_id], @[upd_usr_id])
		,NVL(TO_DATE(@[cre_dt],'YYYYMMDDHH24MISS'),SYSDATE)
		,@[upd_usr_id]
		,SYSDATE
) 			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="rpt_tmplt_nm" type="12" value="" out="N"/>
				<param name="rpt_itm_id" type="12" value="" out="N"/>
				<param name="cprt_tp_ctnt" type="12" value="" out="N"/>
				<param name="cprt_val_ctnt" type="12" value="" out="N"/>
				<param name="dp_seq" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="cprt_chg_grp_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
