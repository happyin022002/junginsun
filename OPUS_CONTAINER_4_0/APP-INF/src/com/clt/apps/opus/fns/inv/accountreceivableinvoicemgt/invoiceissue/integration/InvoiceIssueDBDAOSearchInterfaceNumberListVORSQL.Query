<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOSearchInterfaceNumberListVORSQL">
			<desc><![CDATA[InvoiceIssueDBDAOSearchInterfaceNumberListVORSQL]]></desc>
			<sql><![CDATA[
SELECT V1.ACT_CUST_CNT_CD
     , V1.ACT_CUST_SEQ
     , V1.VSL_CD
     , V1.SKD_VOY_NO
     , V1.SKD_DIR_CD
     , V1.IO_BND_CD
     , V1.PORT_CD
     , V1.SVC_SCP_CD
     , V1.BL_SRC_NO
     , V1.INV_ISS_TP_CD
     , V1.SAIL_ARR_DT 
     --, V1.BKG_NO
     , V1.REV_TP_CD
     , V1.TRSP_RQST_ORD_FLG
     , V1.INV_SPLIT_CD
     --, MAX(V1.AR_IF_NO) AR_IF_NO    
     , V1.AR_IF_NO AR_IF_NO      
  FROM
(SELECT A.ACT_CUST_CNT_CD
     , A.ACT_CUST_SEQ
     , A.VSL_CD
     , A.SKD_VOY_NO
     , A.SKD_DIR_CD
     , A.IO_BND_CD
     , DECODE(A.IO_BND_CD, 'I', A.POD_CD, A.POL_CD) PORT_CD
     , A.SVC_SCP_CD
     , A.BL_SRC_NO
     , NVL(DECODE(A.INV_SPLIT_CD, 'C','*', A.INV_SPLIT_CD),'*') INV_SPLIT_CD
     , I.INV_ISS_TP_CD 
     , A.SAIL_ARR_DT 
     --, A.BKG_NO
     , A.REV_TP_CD
     , A.TRSP_RQST_ORD_FLG
     , A.AR_IF_NO 
     , G.CHG_AMT
  FROM INV_AR_MN A
     , MDM_CUSTOMER F  
     , INV_AR_CHG G    
     , INV_AR_STUP_OFC I
 WHERE A.AR_OFC_CD = @[ar_ofc_cd2] 
   AND NVL(A.INV_DELT_DIV_CD, 'N') <> 'Y'
   AND A.BL_INV_CFM_DT IS NOT NULL
   AND A.AR_IF_NO = G.AR_IF_NO
#if (${inv_dup_flg} != 'Y') 
   AND G.INV_ISS_FLG = 'N' 
#end
   AND I.AR_OFC_CD = @[ar_ofc_cd2]
   --AND A.AR_IF_NO = 'VLCM944126'
   AND A.AR_IF_NO NOT IN ( SELECT AR_IF_NO
                             FROM INV_AR_MN 
                            WHERE AR_IF_NO = A.AR_IF_NO
                              AND USD_XCH_RT = 0 )   
   AND A.REV_TP_CD||A.REV_SRC_CD||A.ACT_CUST_CNT_CD||A.ACT_CUST_SEQ NOT IN (SELECT 'MTH@'
                                                                              FROM DUAL
                                                                             UNION ALL
                                                                            SELECT 'MDJ@'
                                                                              FROM DUAL
                                                                             UNION ALL
                                                                            SELECT 'MSO@'
                                                                              FROM DUAL
                                                                             UNION ALL
                                                                            SELECT A.REV_TP_CD||A.REV_SRC_CD||S2.REP_CUST_CNT_CD||S2.REP_CUST_SEQ
                                                                              FROM INV_AR_STUP_OFC S1
                                                                                 , MDM_ORGANIZATION S2
                                                                             WHERE S1.AR_OFC_CD = @[ar_ofc_cd2] 
                                                                               AND S1.OTS_SMRY_CD = 'INV'
                                                                               AND S1.AR_OFC_CD = S2.AR_OFC_CD
                                                                               AND S2.DELT_FLG = 'N')
   AND A.ACT_CUST_CNT_CD = F.CUST_CNT_CD   
   AND A.ACT_CUST_SEQ = F.CUST_SEQ   
   AND F.DELT_FLG <> 'Y'  
#if (${bnd} != 'A' && ${bnd} != '')     
   AND A.IO_BND_CD = @[bnd]
#end
#if (${from_dt} != '' && ${to_dt} != '') 
	#if (${dt_option} == 'G') 
   AND A.BL_INV_CFM_DT BETWEEN REPLACE(@[from_dt],'-','') AND REPLACE(@[to_dt],'-','')
    #else 
   AND TO_CHAR(A.UPD_DT, 'YYYYMMDD') BETWEEN REPLACE(@[from_dt],'-','') AND REPLACE(@[to_dt],'-','')
    #end
#end
#if (${vvd} != '') 
   AND A.VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND A.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4) 
   AND A.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
#end
#if (${bnd} == 'I' && ${port} != '') 
   AND A.POD_CD = @[port]
#elseif (${bnd} == 'O' && ${port} != '') 
   AND A.POL_CD = @[port]
#end
#if (${scp} != '') 
   AND A.SVC_SCP_CD = @[scp]
#end
#if (${cust_cnt_cd} != '') 
   AND A.ACT_CUST_CNT_CD = @[cust_cnt_cd]
#end
#if (${cust_seq} != '') 
   AND A.ACT_CUST_SEQ = @[cust_seq]
#end
#if (${bl_nos} != '') 
   AND A.BL_SRC_NO IN (${bl_nos})
#end
#if (${if_user_id} != '') 
   AND A.UPD_USR_ID = @[if_user_id]
#end
) V1   
 WHERE V1.ACT_CUST_CNT_CD   = @[max_act_cust_cnt_cd]    
   AND V1.ACT_CUST_SEQ      = TO_NUMBER(@[max_act_cust_seq])     
   AND V1.VSL_CD            = @[max_vsl_cd]             
   AND V1.SKD_VOY_NO        = @[max_skd_voy_no]         
   AND V1.SKD_DIR_CD        = @[max_skd_dir_cd]         
   AND V1.IO_BND_CD         = @[max_io_bnd_cd]          
   AND V1.PORT_CD           = @[max_port_cd]            
   AND V1.SVC_SCP_CD        = @[max_svc_scp_cd]   
#if (${inv_mlt_bl_iss_flg} != 'Y')    
   AND NVL(V1.BL_SRC_NO, ' ')  = NVL(@[max_bl_src_no], ' ')     
#end  
   AND V1.INV_ISS_TP_CD     = @[max_inv_iss_tp_cd]   
   AND V1.SAIL_ARR_DT       = @[max_sail_arr_dt]    
   AND V1.TRSP_RQST_ORD_FLG = @[max_trsp_rqst_ord_flg]  
   AND V1.INV_SPLIT_CD = @[max_inv_split_cd]  
 GROUP BY V1.ACT_CUST_CNT_CD
     , V1.ACT_CUST_SEQ
     , V1.VSL_CD
     , V1.SKD_VOY_NO
     , V1.SKD_DIR_CD
     , V1.IO_BND_CD
     , V1.PORT_CD
     , V1.SVC_SCP_CD
     , V1.BL_SRC_NO
     , V1.INV_ISS_TP_CD
     , V1.SAIL_ARR_DT 
     --, V1.BKG_NO
     , V1.REV_TP_CD
     , V1.TRSP_RQST_ORD_FLG
     , V1.INV_SPLIT_CD
     --, DECODE(V1.INV_ISS_TP_CD, 'S', '', 'E', V1.AR_IF_NO)
     , V1.AR_IF_NO
#if (${ots_smry_cd} == 'BL') 
	   HAVING SUM(V1.CHG_AMT) <> 0 
#end
 ORDER BY V1.ACT_CUST_CNT_CD
     , V1.ACT_CUST_SEQ
     , V1.VSL_CD
     , V1.SKD_VOY_NO
     , V1.SKD_DIR_CD
     , V1.IO_BND_CD
     , V1.PORT_CD
     , V1.SVC_SCP_CD
     , V1.BL_SRC_NO
     , V1.INV_ISS_TP_CD
     , V1.SAIL_ARR_DT 
     --, V1.BKG_NO
     , V1.REV_TP_CD
     , V1.TRSP_RQST_ORD_FLG
     --, DECODE(V1.INV_ISS_TP_CD, 'S', '', 'E', V1.AR_IF_NO)
     , V1.AR_IF_NO			]]></sql>
			<params>
				<param name="ar_ofc_cd2" type="12" value="" out="N"/>
				<param name="bnd" type="12" value="" out="N"/>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="port" type="12" value="" out="N"/>
				<param name="scp" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="if_user_id" type="12" value="" out="N"/>
				<param name="max_act_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="max_act_cust_seq" type="12" value="" out="N"/>
				<param name="max_vsl_cd" type="12" value="" out="N"/>
				<param name="max_skd_voy_no" type="12" value="" out="N"/>
				<param name="max_skd_dir_cd" type="12" value="" out="N"/>
				<param name="max_io_bnd_cd" type="12" value="" out="N"/>
				<param name="max_port_cd" type="12" value="" out="N"/>
				<param name="max_svc_scp_cd" type="12" value="" out="N"/>
				<param name="max_bl_src_no" type="12" value="" out="N"/>
				<param name="max_inv_iss_tp_cd" type="12" value="" out="N"/>
				<param name="max_sail_arr_dt" type="12" value="" out="N"/>
				<param name="max_trsp_rqst_ord_flg" type="12" value="" out="N"/>
				<param name="max_inv_split_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
