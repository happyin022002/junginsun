<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchMaxIfNoRSQL">
			<desc><![CDATA[Search Max If No]]></desc>
			<sql><![CDATA[
SELECT MAX(AR_IF_NO) MAX_AR_IF_NO
FROM INV_AR_MN
WHERE (AR_OFC_CD, BL_SRC_NO) IN (SELECT AR_OFC_CD, BL_SRC_NO
                                 FROM INV_AR_MN
                                 WHERE AR_IF_NO = @[ar_if_no])
AND REV_TP_CD <> 'M'
AND INV_DELT_DIV_CD <> 'Y'			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
