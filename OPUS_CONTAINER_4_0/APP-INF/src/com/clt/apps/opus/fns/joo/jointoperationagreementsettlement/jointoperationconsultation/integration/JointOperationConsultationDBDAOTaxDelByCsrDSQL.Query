<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOTaxDelByCsrDSQL">
			<desc><![CDATA[CSR번호로 JOO_TAX를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE JOO_TAX
WHERE 1= 1
AND   SLP_TP_CD || SLP_FUNC_CD || SLP_OFC_CD || SLP_ISS_DT || SLP_SER_NO  = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
