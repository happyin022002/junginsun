<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOsearchTemplateNameRSQL">
			<desc><![CDATA[searchTemplateName]]></desc>
			<sql><![CDATA[
SELECT	RPT_TMPLT_NM
FROM	INV_CPRT_TMPLT
WHERE	RPT_TMPLT_NM = @[rpt_tmplt_nm]			]]></sql>
			<params>
				<param name="rpt_tmplt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
