<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOModifyAuthorityCarrierUSQL">
			<desc><![CDATA[Authority Carrier Update]]></desc>
			<sql><![CDATA[
UPDATE JOO_CRR_AUTH
   SET JO_CRR_AUTH_CD   = @[jo_crr_auth_cd]
     , DELT_FLG         = @[delt_flg]
     , UPD_DT           = SYSDATE
     , UPD_USR_ID       = @[cre_usr_id]
 WHERE JO_CRR_CD        = @[jo_crr_cd]
   AND RLANE_CD         = @[rlane_cd]
   AND AUTH_OFC_CD      = @[auth_ofc_cd]			]]></sql>
			<params>
				<param name="jo_crr_auth_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="auth_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
