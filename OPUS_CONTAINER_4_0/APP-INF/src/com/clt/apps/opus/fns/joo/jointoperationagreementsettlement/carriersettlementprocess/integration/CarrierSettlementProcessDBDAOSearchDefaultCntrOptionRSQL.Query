<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOSearchDefaultCntrOptionRSQL">
			<desc><![CDATA[Default Option Search]]></desc>
			<sql><![CDATA[
SELECT NVL(B.PPT_CD     , A.PPT_CD)     AS PPT_CD
     , NVL(B.LINE_SEQ   , A.LINE_SEQ)   AS LINE_SEQ
     , NVL(B.SLAN_CD    , A.SLAN_CD)    AS SLAN_CD
     , NVL(B.OPTION_A   , A.OPTION_A)   AS OPTION_A
     , NVL(B.OPTION_B   , A.OPTION_B)   AS OPTION_B
  FROM (SELECT 'OPTION'     AS PPT_CD
             , 1            AS LINE_SEQ
             , NULL         AS SLAN_CD
             , 'OPTION1'    AS OPTION_A
             , 'OPTION1'    AS OPTION_B
          FROM DUAL ) A
     , (SELECT PPT_CD
             , LINE_SEQ
             , ATTR_CTNT1   AS SLAN_CD
             , ATTR_CTNT2   AS OPTION_A
             , ATTR_CTNT3   AS OPTION_B
          FROM JOO_COM_PPT
         WHERE PPT_CD       = 'OPTION'
           AND ATTR_CTNT1   = @[slan_cd]
           AND ROWNUM       = 1) B
 WHERE A.PPT_CD = B.PPT_CD(+)			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
