<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOsearchInvoiceRemarkRSQL">
			<desc><![CDATA[InvoiceIssueDBDAOsearchInvoiceRemarkRSQL]]></desc>
			<sql><![CDATA[
SELECT INV_NO, INV_ISS_RMK
  FROM INV_AR_ISS
 WHERE INV_NO = @[inv_no]
   AND INV_SEQ = (SELECT MAX(INV_SEQ)
					FROM INV_AR_ISS
				   WHERE INV_NO = @[inv_no])			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
