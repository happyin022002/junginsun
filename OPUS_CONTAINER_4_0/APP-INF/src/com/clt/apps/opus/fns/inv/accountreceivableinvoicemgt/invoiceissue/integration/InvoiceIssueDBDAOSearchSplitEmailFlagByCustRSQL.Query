<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOSearchSplitEmailFlagByCustRSQL">
			<desc><![CDATA[Email Split 여부]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(SPRT_EML_INV_FLG),'N') AS  SPRT_EML_INV_FLG
FROM MDM_CUSTOMER
WHERE CUST_CNT_CD = @[cust_cnt_cd] 
AND CUST_SEQ = TO_NUMBER(@[cust_seq])			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
