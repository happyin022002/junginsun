<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOCheckVVDRSQL">
			<desc><![CDATA[CheckVVD]]></desc>
			<sql><![CDATA[
SELECT 
	vsl_cd
,	skd_voy_no
,	skd_dir_cd
FROM vsk_vsl_skd
WHERE	vsl_cd = substr(@[code],1,4)
AND	skd_voy_no = substr(@[code],5,4)
AND	skd_dir_cd = substr(@[code],9,1)
			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
