<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOSearchRevenueAcctMaxEndDateRSQL">
			<desc><![CDATA[SearchRevenueAcctMaxEndDate]]></desc>
			<sql><![CDATA[
SELECT MAX(FM_ACCT_END_DT) AS FM_ACCT_END_DT, MAX(TO_ACCT_END_DT) AS TO_ACCT_END_DT
FROM
( 
SELECT MAX(ACCT_END_DT) AS FM_ACCT_END_DT, '' AS TO_ACCT_END_DT
FROM SAR_ACCT_MTX
WHERE ACCT_CTNT1 = 'REV'
AND ACCT_CTNT4 = 'MTH'
AND REV_ACCT_DIV_CD = 'N'
AND NVL(ACCT_END_DT, REPLACE(@[gl_eff_dt],'-','')) < REPLACE(@[gl_eff_dt],'-','')
UNION ALL
SELECT '', MIN(ACCT_END_DT) AS TO_ACCT_END_DT
FROM SAR_ACCT_MTX
WHERE ACCT_CTNT1 = 'REV'
AND ACCT_CTNT4 = 'MTH'
AND REV_ACCT_DIV_CD = 'N'
AND NVL(ACCT_END_DT, REPLACE(@[gl_eff_dt],'-','')) >= REPLACE(@[gl_eff_dt],'-','')
)			]]></sql>
			<params>
				<param name="gl_eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
