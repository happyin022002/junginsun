<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOsearchARCreditRSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOsearchARCreditRSQL]]></desc>
			<sql><![CDATA[
SELECT CR_FLG 
     , NVL(IB_CR_TERM_DYS, 0) IB_CR_TERM_DYS
     , NVL(OB_CR_TERM_DYS, 0) OB_CR_TERM_DYS
  FROM MDM_CR_CUST
 WHERE ACT_CUST_CNT_CD = @[cust_cnt_cd]
   AND ACT_CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
