<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOGetRfTdrRgnChangeRSQL">
			<desc><![CDATA[R/F TDR의 RGN변경시 단가를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT H.PORT_CD AS FM_PORT_CD, D.POD AS TO_PORT_CD,
       SUM(DECODE(D.CNTR_SIZE,'2',1,'3',1,0)) USD_SLT_BSA_QTY_20,
       SUM(DECODE(D.CNTR_SIZE,'4',1,'H',1,'L',1,0)) USD_SLT_BSA_QTY_40
FROM   TDR_HEADER H, TDR_CNTR_DETAIL D
WHERE  1=1
AND    H.VSL_CD   = D.VSL_CD
AND    H.VOY_NO   = D.VOY_NO
AND    H.DIR_CD   = D.DIR_CD
AND    H.PORT_CD  = D.PORT_CD
AND    H.CALL_IND = D.CALL_IND
AND    D.STATUS   = 'LS'
AND    H.VSL_CD   = @[vsl_cd]
AND    H.VOY_NO   = @[skd_voy_no]
AND    H.DIR_CD   = @[skd_dir_cd]
AND    D.OPR_CD IN 
           (SELECT B.JO_CRR_CD
            FROM   JOO_STL_VVD A,
                   (
            		SELECT DECODE(@[re_divr_cd], 'E', COM_CONSTANTMGR_PKG.COM_getCompanyCode_FNC, @[jo_crr_cd]) AS JO_CRR_CD, TO_DATE('99991231','YYYYMMDD') AS EFF_ETA_DT
            		FROM   DUAL
            		UNION  ALL
            		SELECT JO_N2ND_CRR_CD AS JO_CRR_CD, EFF_ETA_DT
            		FROM   JOO_CRR_MRG A
            		WHERE  A.DELT_FLG       = 'N'
            		AND    A.ACCTG_CRR_CD   = @[jo_crr_cd]
            		AND    A.JO_N1ST_CRR_CD = DECODE(@[re_divr_cd], 'E', COM_CONSTANTMGR_PKG.COM_getCompanyCode_FNC, @[jo_crr_cd])
            		AND    A.TRD_CD         = @[trd_cd]
            		AND    A.RLANE_CD       = @[rlane_cd]
                   ) B
            WHERE  A.BZC_PORT_ETA_DT <= B.EFF_ETA_DT
            AND    A.ACCT_YRMON    = REPLACE(@[acct_yrmon],'-','')
            AND    A.JO_CRR_CD     = @[jo_crr_cd]
            AND    A.TRD_CD        = @[trd_cd]
            AND    A.RLANE_CD      = @[rlane_cd]
            AND    A.RE_DIVR_CD    = @[re_divr_cd]
            AND    A.JO_STL_ITM_CD = @[jo_stl_itm_cd]
            AND    A.JO_MNU_NM     = @[jo_mnu_nm]
            AND    A.JO_STL_CFM_CD = 'Y'
            AND    A.VSL_CD        = @[vsl_cd]
            AND    A.SKD_VOY_NO    = @[skd_voy_no]
            AND    A.SKD_DIR_CD    = @[skd_dir_cd]
            AND    A.REV_DIR_CD    = @[rev_dir_cd]
            )
AND    D.TEMP IS NOT NULL
GROUP BY H.PORT_CD, D.POD			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="jo_stl_itm_cd" type="12" value="" out="N"/>
				<param name="jo_mnu_nm" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
