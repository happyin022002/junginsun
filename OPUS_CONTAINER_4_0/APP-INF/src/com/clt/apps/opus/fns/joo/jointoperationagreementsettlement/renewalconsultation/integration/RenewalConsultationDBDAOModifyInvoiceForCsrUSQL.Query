<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalConsultationDBDAOModifyInvoiceForCsrUSQL">
			<desc><![CDATA[JOO_INVOCE의 전표번호 Update.]]></desc>
			<sql><![CDATA[
UPDATE JOO_INVOICE
   SET SLP_TP_CD    = @[slp_tp_cd]
     , SLP_FUNC_CD  = @[slp_func_cd]
     , SLP_OFC_CD   = @[slp_ofc_cd]
     , SLP_ISS_DT   = @[slp_iss_dt]
     , SLP_SER_NO   = @[slp_ser_no]
     , UPD_DT       = SYSDATE
     , UPD_USR_ID   = @[issuer_id]
 WHERE ACCT_YRMON   = REPLACE(@[acct_yrmon],'-','')
   AND INV_NO       = @[inv_no]
   AND RE_DIVR_CD   = @[re_divr_cd]
   AND ACCTG_CRR_CD = @[acctg_crr_cd]
   AND LOCL_CURR_CD = @[locl_curr_cd]
   AND PRNR_REF_NO  = @[prnr_ref_no]			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="issuer_id" type="12" value="" out="N"/>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="acctg_crr_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="prnr_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
