<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalConsultationDBDAORemoveInvoiceDetailDSQL">
			<desc><![CDATA[Invoice Detail Delete]]></desc>
			<sql><![CDATA[
DELETE
  FROM JOO_INV_DTL DTL
 WHERE 1=1
   AND DTL.ACCT_YRMON   = @[acct_yrmon]
   AND DTL.JO_CRR_CD    = @[jo_crr_cd]
   AND DTL.INV_NO       = @[inv_no]
   AND DTL.RE_DIVR_CD   = @[re_divr_cd]
#if (${vsl_cd}!='')
   AND DTL.VSL_CD       = @[vsl_cd]
#end
#if (${skd_voy_no}!='')
   AND DTL.SKD_VOY_NO   = @[skd_voy_no]
#end
#if (${skd_dir_cd}!='')
   AND DTL.SKD_DIR_CD   = @[skd_dir_cd]
#end
#if (${rev_dir_cd}!='')
   AND DTL.REV_DIR_CD   = @[rev_dir_cd]
#end
#if (${rev_yrmon}!='')
   AND DTL.REV_YRMON    = @[rev_yrmon]
#end
#if (${stl_vvd_seq}!='')
   AND DTL.STL_VVD_SEQ  = TO_NUMBER(@[stl_vvd_seq])
#end			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
