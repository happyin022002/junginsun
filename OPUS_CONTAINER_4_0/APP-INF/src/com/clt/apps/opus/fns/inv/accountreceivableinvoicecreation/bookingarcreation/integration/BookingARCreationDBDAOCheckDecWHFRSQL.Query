<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOCheckDecWHFRSQL">
			<desc><![CDATA[BookingARCreationDBDAOCheckDecWHFRSQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM INV_AR_MN MN,
       INV_AR_CHG CHG
 WHERE MN.AR_IF_NO = CHG.AR_IF_NO
   AND MN.AR_IF_NO = @[ar_if_no]
   AND CHG.CHG_CD = 'WHF' 
   AND (MN.WHF_DECL_NO IS NOT NULL OR MN.WHF_FLG = 'C')			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
