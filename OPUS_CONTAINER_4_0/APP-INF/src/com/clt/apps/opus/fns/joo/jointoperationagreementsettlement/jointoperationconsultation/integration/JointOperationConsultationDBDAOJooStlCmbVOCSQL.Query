<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOJooStlCmbVOCSQL">
			<desc><![CDATA[JOO_STL_CMB 에 INSERT한다.]]></desc>
			<sql><![CDATA[
INSERT INTO JOO_STL_CMB (
	ACCT_YRMON
,	JO_CRR_CD
,	STL_CMB_SEQ
,	RE_DIVR_CD
,	SLP_TP_CD
,	SLP_FUNC_CD
,	SLP_OFC_CD
,	SLP_ISS_DT
,	SLP_SER_NO
,   RVS_CMB_FLG
,   RJCT_CMB_FLG
,	CRE_DT
,	CRE_USR_ID
,	UPD_DT
,	UPD_USR_ID
) VALUES( 
	REPLACE(@[acct_yrmon],'-','')
,	@[jo_crr_cd]
,	@[stl_cmb_seq]
,	@[re_divr_cd]
,	@[slp_tp_cd]
,	@[slp_func_cd]
,	@[slp_ofc_cd]
,	@[slp_iss_dt]
,	@[slp_ser_no]
,   NVL(@[rvs_cmb_flg],'N')
,   NVL(@[rjct_cmb_flg],'N')
,	SYSDATE
,	@[cre_usr_id]
,	SYSDATE
,	@[cre_usr_id]
)			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="stl_cmb_seq" type="2" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="rvs_cmb_flg" type="12" value="" out="N"/>
				<param name="rjct_cmb_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
