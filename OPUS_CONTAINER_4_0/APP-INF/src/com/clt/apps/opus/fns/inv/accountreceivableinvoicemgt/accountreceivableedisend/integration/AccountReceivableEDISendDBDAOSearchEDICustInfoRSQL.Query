<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchEDICustInfoRSQL">
			<desc><![CDATA[SearchEDICustInfo]]></desc>
			<sql><![CDATA[
SELECT  EDI.EDI_GRP_CD
      , EDI.CUST_TRD_PRNR_ID AS RECEIVER_ID
      , EDI.PROV_TRD_PRNR_ID AS SENDER_ID
      , NVL(MC.INV_EDI_LVL_CD, 'B') AS EDI_LVL
FROM EDI_GROUP EDI, EDI_GRP_CUST ECUST, MDM_CUSTOMER MC
WHERE 1=1
AND   EDI. EDI_GRP_CD = ECUST.EDI_GRP_CD
AND   ECUST.CUST_CNT_CD = MC.CUST_CNT_CD
AND   ECUST.CUST_SEQ = MC.CUST_SEQ
AND   ECUST.CUST_CNT_CD = @[cust_cnt_cd]
AND   ECUST.CUST_SEQ = @[cust_seq]
AND  (  ( NVL(MC.INV_EDI_LVL_CD, 'B') = 'B' AND  EDI. EDI_GRP_CD IN (  'INVBL001' , 'INVBL002' , 'INVBL003' , 'INVBL004', 'INVBL005', 'INVBL006' , 'INVBL007', 'INVBL008', 'INVBL009'  ) ) 
       OR ( NVL(MC.INV_EDI_LVL_CD, 'B') <> 'B' AND  EDI. EDI_GRP_CD IN ( 'INVOIC001', 'INVOIC002' , 'INVOIC003'  )   ) )
GROUP BY EDI.EDI_GRP_CD, EDI.CUST_TRD_PRNR_ID, EDI.PROV_TRD_PRNR_ID, MC.INV_EDI_LVL_CD			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
