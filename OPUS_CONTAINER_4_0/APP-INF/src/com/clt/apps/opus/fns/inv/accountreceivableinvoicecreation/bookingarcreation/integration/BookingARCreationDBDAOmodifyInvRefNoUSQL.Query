<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOmodifyInvRefNoUSQL">
			<desc><![CDATA[[] BookingARCreationBackEndDBDAO::modifyInvRefNo ( invoiceVo) return result]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
   SET INV_REF_NO = @[inv_ref_no] 
 WHERE BL_SRC_NO = @[bl_src_no]
   AND AR_OFC_CD = @[ofc_cd]
   AND IO_BND_CD = 'I'			]]></sql>
			<params>
				<param name="inv_ref_no" type="12" value="" out="N"/>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
