<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableInvoiceMigrationDBDAOsearchArMstRevVvdRSQL">
			<desc><![CDATA[AccountReceivableInvoiceMigrationDBDAOsearchArMstRevVvdRSQL]]></desc>
			<sql><![CDATA[
SELECT 'X'
  FROM AR_MST_REV_VVD 
 WHERE VSL_CD = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SUBSTR(SKD_DIR_CD, 1, 1) = SUBSTR(@[skd_dir_cd], 1, 1)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
