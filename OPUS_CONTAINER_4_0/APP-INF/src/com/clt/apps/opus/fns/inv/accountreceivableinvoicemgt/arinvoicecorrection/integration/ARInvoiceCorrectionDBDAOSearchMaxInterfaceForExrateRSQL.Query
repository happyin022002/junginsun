<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchMaxInterfaceForExrateRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT AR_IF_NO
  FROM INV_AR_MN
 WHERE AR_IF_NO IN (SELECT MAX(AR_IF_NO) 
                      FROM INV_AR_MN
                     WHERE AR_OFC_CD = @[ar_ofc_cd]
                       AND BL_SRC_NO = @[bl_no]
                       AND NVL(INV_DELT_DIV_CD, 'N') <> 'Y'
                       AND REV_TP_CD <> 'M'
                       AND NVL(INV_SPLIT_CD, 'N') NOT IN ('S','X'))
   AND INV_NO = @[inv_no]
   AND NVL(INV_DELT_DIV_CD, 'N') = 'N'
   			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
