<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceExRateMgtDBDAOSearchXchRtTpCdRSQL">
			<desc><![CDATA[searchXchRtTpCd]]></desc>
			<sql><![CDATA[
SELECT NVL(IAS.XCH_RT_USD_TP_CD,'V') XCH_RT_TP_CD 
 FROM MDM_ORGANIZATION MO , INV_AR_STUP_OFC IAS
 WHERE MO.AR_OFC_CD = IAS.AR_OFC_CD(+)
 AND   MO.AR_OFC_CD = @[ar_ofc_cd] 			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
