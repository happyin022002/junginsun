<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchCmdtInfoRSQL">
			<desc><![CDATA[SearchCmdtInfo]]></desc>
			<sql><![CDATA[
SELECT CNTR_MF_SEQ
       , REPLACE(REPLACE(CNTR_MF_GDS_DESC,CHR(13)||CHR(10),CHR(10)),CHR(10), ' ') CNTR_MF_GDS_DESC
       , PCK_QTY
       , PCK_TP_CD
       , CNTR_MF_WGT
       , WGT_UT_CD
       , MEAS_QTY
       , MEAS_UT_CD
FROM BKG_CNTR_MF_DESC
WHERE BKG_NO = @[bkg_no]
AND CNTR_NO = @[cntr_no]
ORDER BY CNTR_MF_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
