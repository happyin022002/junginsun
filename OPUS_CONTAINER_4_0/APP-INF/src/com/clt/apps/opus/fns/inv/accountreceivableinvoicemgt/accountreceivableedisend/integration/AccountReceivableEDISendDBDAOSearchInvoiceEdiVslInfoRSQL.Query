<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchInvoiceEdiVslInfoRSQL">
			<desc><![CDATA[SearchInvoiceEdiVslInfo]]></desc>
			<sql><![CDATA[
SELECT '{VSL_INFO' || CHR(10) 
       || 'VSL_NM:' ||MDM.VSL_ENG_NM|| CHR(10) 
       || 'VSL_CD:' || IAM.VSL_CD||CHR(10) 
       || 'VSL_VOY_NO:' ||IAM.SKD_VOY_NO|| CHR(10) 
       || 'EX_VOY_REF:' ||DECODE(IAM.IO_BND_CD, 'I', VPS.IB_CSSM_VOY_NO, 'O', VPS.OB_CSSM_VOY_NO)|| CHR(10) 
       || 'VSL_LOYD_CD:' ||MDM.LLOYD_NO|| CHR(10) 
       || '}VSL_INFO' || CHR(10)
  FROM INV_AR_MN IAM, MDM_VSL_CNTR MDM, VSK_VSL_PORT_SKD VPS 
 WHERE IAM.AR_IF_NO = @[ar_if_no]
   AND IAM.VSL_CD = MDM.VSL_CD(+)
   AND VPS.VSL_CD = IAM.VSL_CD
   AND VPS.SKD_VOY_NO = IAM.SKD_VOY_NO
   AND VPS.SKD_DIR_CD = IAM.SKD_DIR_CD
   AND VPS.VPS_PORT_CD = DECODE(IAM.IO_BND_CD, 'I', IAM.POD_CD, 'O', IAM.POL_CD)
   AND VPS.CLPT_IND_SEQ = 1
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
