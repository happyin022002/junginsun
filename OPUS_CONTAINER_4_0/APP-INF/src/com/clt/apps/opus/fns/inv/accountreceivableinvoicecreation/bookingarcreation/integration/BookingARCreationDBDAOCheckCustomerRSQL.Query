<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOCheckCustomerRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM MDM_CUSTOMER
 WHERE CUST_CNT_CD = @[cust_cnt_cd]
   AND CUST_SEQ = @[cust_seq]
   AND NVL(DELT_FLG,'N') = 'N'
   --AND NVL(BLK_DIV_FLG,'N') <> 'Y' --2010-04-27
   AND NVL(NMD_CUST_FLG,'N') ='N' --2010-04-27
   AND NVL(CNTR_DIV_FLG,'N') ='Y' --2010-06-04 김현화수석			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
