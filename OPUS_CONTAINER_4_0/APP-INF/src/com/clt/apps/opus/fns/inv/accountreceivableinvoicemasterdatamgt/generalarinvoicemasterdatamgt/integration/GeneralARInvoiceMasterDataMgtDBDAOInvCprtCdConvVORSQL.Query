<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceMasterDataMgtDBDAOInvCprtCdConvVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 	A.SC_NO
	,	A.RFA_NO
	,	A.CPRT_CONV_TP_CD
	,	A.CO_CD_CTNT
	,	A.CUST_CONV_CD_CTNT
	,	A.CD_RMK
	,	A.CRE_USR_ID
	,	A.CRE_DT
	,	A.UPD_USR_ID
	,	A.UPD_DT
FROM	INV_CPRT_CD_CONV A
WHERE	1=1
#if (${sc_no} != '' || ${sc_no} != 'X')
AND		A.SC_NO = @[sc_no]
#end
#if (${rfa_no} != '' || ${rfa_no} != 'X')
AND		A.RFA_NO = @[rfa_no]
#end
#if (${cprt_conv_tp_cd} != '')
AND		A.CPRT_CONV_TP_CD = @[cprt_conv_tp_cd]
#end			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="cprt_conv_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
