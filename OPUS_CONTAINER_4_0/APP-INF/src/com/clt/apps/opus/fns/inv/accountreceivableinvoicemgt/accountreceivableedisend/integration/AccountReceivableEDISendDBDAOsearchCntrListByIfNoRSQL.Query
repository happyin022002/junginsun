<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOsearchCntrListByIfNoRSQL">
			<desc><![CDATA[searchCntrListByIfNo]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO
     , CNTR_TPSZ_CD
     , CNTR_SEQ
  FROM INV_AR_CNTR
 WHERE AR_IF_NO = @[ar_if_no]			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
