<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchTroCustomerCodeRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DISTINCT ACT_CNT_CD ACT_CUST_CNT_CD,
       ACT_CUST_SEQ ACT_CUST_SEQ
  FROM INV_BKG_IF_CHG
 WHERE BKG_NO = @[bkg_no]
   AND BKG_SEQ = @[bkg_seq]
   AND OFC_CD = @[ofc_cd]
   AND IO_BND_CD = @[io_bnd_cd]
   AND ACT_CNT_CD IS NOT NULL
   AND ACT_CUST_SEQ IS NOT NULL
#if (${io_bnd_cd} == 'O')
   AND CHG_CD = 'SRC'
#else 
   AND CHG_CD = 'DIH' AND DE_TERM_CD = 'H' AND AUTO_RAT_CD = 'A'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
