<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOchkSummaryCrrRmkRSQL">
			<desc><![CDATA[0036 조건에 해당하는 remark가 테이블에 이미 존재하는지 아닌지를 조회한다. ]]></desc>
			<sql><![CDATA[
SELECT ACCT_YRMON, TRD_CD, RLANE_CD, JO_CRR_CD
FROM JOO_SMRY_CRR_RMK
WHERE ACCT_YRMON = @[acct_yrmon]
AND TRD_CD = @[trd_cd]
AND RLANE_CD = @[rlane_cd]
AND JO_CRR_CD = @[jo_crr_cd]			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
