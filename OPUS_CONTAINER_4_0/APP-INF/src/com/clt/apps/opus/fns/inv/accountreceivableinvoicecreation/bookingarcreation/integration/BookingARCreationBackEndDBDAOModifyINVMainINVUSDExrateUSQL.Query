<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationBackEndDBDAOModifyINVMainINVUSDExrateUSQL">
			<desc><![CDATA[Modify INV Main INV USD Exrate]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
   SET INV_USD_XCH_RT = NVL(@[inv_xch_rt], 0),
       UPD_USR_ID = @[upd_usr_id],
	   UPD_DT = sysdate
 WHERE AR_IF_NO = @[ar_if_no]
   --AND INV_CURR_CD IS NOT NULL
   AND NVL(@[inv_xch_rt],0) <> 0			]]></sql>
			<params>
				<param name="inv_xch_rt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
