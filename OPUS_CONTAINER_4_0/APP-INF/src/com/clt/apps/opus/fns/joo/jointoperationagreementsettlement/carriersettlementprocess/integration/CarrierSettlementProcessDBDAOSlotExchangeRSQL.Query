<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOSlotExchangeRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  A.ACCT_YRMON                      ,
        A.STL_VVD_SEQ                     ,
        A.STL_SEQ                         ,
        A.TRD_CD                          ,
        A.JO_CRR_CD                       ,
        A.RLANE_CD                        ,
        A.RE_DIVR_CD                      ,
        A.JO_STL_ITM_CD                   ,
        A.JO_MNU_NM                       ,
        A.VSL_CD                          ,
        A.SKD_VOY_NO                      ,
        A.SKD_DIR_CD                      ,
        A.REV_DIR_CD                      ,
        A.STL_BZC_PORT_CD                 ,
        A.ETA_DT                          ,
        A.JO_STL_JB_CD                    ,
        A.BSA_QTY                         ,
        A.BSA_SLT_PRC                     ,
        A.LOCL_CURR_CD                    ,
        A.ADJ_BSA_QTY_LOCL_AMT            ,
        A.ADJ_BSA_SLT_PRC_LOCL_AMT        ,
        A.STL_LOCL_AMT                    ,
        A.STL_USD_AMT                     ,
        A.IOC_CD                          ,
        A.SCONTI_CD                       ,
        A.FNL_OWN_BSA_QTY                 ,
        A.FNL_BSA_WGT                     ,
        A.USD_SLT_BSA_QTY                 ,
        A.USD_SLT_WGT                     ,
        A.BSA_PER_WGT                     ,
        A.FM_PORT_CD                      ,
        A.TO_PORT_CD                      ,
        A.RF_SCG_STL_TP_CD                ,
        A.RF_SCG_PRC                      ,
        A.STL_RMK                         ,
        A.CMB_CFM_FLG                     ,
        A.STL_CMB_SEQ                     ,
        A.STL_TJ_NO                       ,
        A.STL_ADJ_FLG                     ,
        A.PRE_ACCT_YRMON                  ,
        A.PRE_STL_VVD_SEQ                 ,
        A.PRE_STL_SEQ                     ,
        A.STL_LST_FLG                     ,
        A.CRE_DT                          ,
        A.CRE_USR_ID                      ,
        A.UPD_DT                          ,
        A.UPD_USR_ID                      ,
        A.UC_BSS_PORT_CD                  ,
        A.UC_BSS_PORT_ETD_DT
FROM    JOO_SETTLEMENT A 
WHERE   A.ACCT_YRMON  =  @[acct_yrmon]
AND     A.STL_VVD_SEQ =  @[stl_vvd_seq]
AND     A.STL_SEQ     =  @[stl_seq]
AND     A.STL_LOCL_AMT<>0			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="12" value="" out="N"/>
				<param name="stl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
