<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchMRIRevenueVVDLaneRdRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchMRIRevenueVVDLaneRdRSQL]]></desc>
			<sql><![CDATA[
SELECT MAX(RLANE_CD) REV_LANE
     , MAX(VSL_CD||SKD_VOY_NO||SKD_DIR_CD) REV_VVD
  FROM AR_MST_REV_VVD
 WHERE VSL_CD = DECODE(@[vsl], 'COMC', @[vsl], 'USAC', @[vsl],  DECODE(@[vsl], 'CNTC', 'CNTC', 'CFDR'))
   AND REV_YRMON = (SELECT MAX(REV_YRMON)
                      FROM AR_MST_REV_VVD
                     WHERE VSL_CD = DECODE(@[vsl], 'COMC', @[vsl], 'USAC', @[vsl], DECODE(@[vsl], 'CNTC', 'CNTC', 'CFDR')))			]]></sql>
			<params>
				<param name="vsl" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
