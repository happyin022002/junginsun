<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOModifyCarrierUSQL">
			<desc><![CDATA[Modify Carrier]]></desc>
			<sql><![CDATA[
UPDATE JOO_CARRIER
   SET VNDR_SEQ         = @[vndr_seq]
     , CUST_CNT_CD      = @[cust_cnt_cd]
     , CUST_SEQ         = @[cust_seq]
     , TRD_CD           = @[trd_cd]
     , JO_STL_OPT_CD    = NVL(@[jo_stl_opt_cd],'Z')
     , DELT_FLG         = @[delt_flg]
     , UPD_DT           = SYSDATE
     , UPD_USR_ID       = @[cre_usr_id]
 WHERE JO_CRR_CD        = @[jo_crr_cd]
   AND RLANE_CD         = @[rlane_cd]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="jo_stl_opt_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
