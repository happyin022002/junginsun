<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOCheckExistFinancialAffairsMtxRSQL">
			<desc><![CDATA[Financial Affairs Matrix 존재 여부 체크]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN EXIST_CNT > 0 THEN 'Y' ELSE 'N' END AS EXIST_FLG
  FROM (
        SELECT COUNT(*) AS EXIST_CNT
          FROM JOO_FINC_MTX A
         WHERE 1=1
           AND A.JO_CRR_CD = @[jo_crr_cd]
           AND A.RLANE_CD  = @[rlane_cd]
#if (${re_divr_cd} != '') 
           AND A.RE_DIVR_CD= @[re_divr_cd]
#end
#if (${jo_stl_itm_cd} != '') 
           AND A.JO_STL_ITM_CD= @[jo_stl_itm_cd]
#end
       )			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="jo_stl_itm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
