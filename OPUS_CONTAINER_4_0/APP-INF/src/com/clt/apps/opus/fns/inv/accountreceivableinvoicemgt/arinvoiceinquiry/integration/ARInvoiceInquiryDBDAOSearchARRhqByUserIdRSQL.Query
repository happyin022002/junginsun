<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOSearchARRhqByUserIdRSQL">
			<desc><![CDATA[USER 소속  RHQ OFFICE]]></desc>
			<sql><![CDATA[
SELECT B.AR_HD_QTR_OFC_CD
FROM   MDM_ORGANIZATION B
WHERE  B.OFC_CD = @[useOfc]
