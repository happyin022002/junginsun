<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchEDI310InvoiceRSQL">
			<desc><![CDATA[Search EDI 310 Invoice]]></desc>
			<sql><![CDATA[
SELECT 
MERGE_CHK,EDI_HDR_SEQ,AR_IF_NO,BL_NO,BL_SEQ,BKG_NO,INV_NO,CNTR_NO,AR_OFC_CD,SOURCE_CD,REV_TP_SRC_CD,ACT_CUST_CNT_CD,ACT_CUST_SEQ,CUST_CD,CUST_NM,VSL_CD,SKD_VOY_NO,SKD_DIR_CD,TRNK_VVD_CD,SVC_SCP_CD,SLAN_CD,SAIL_ARR_DT,IO_BND_CD,POR_CD,POL_CD,POD_CD,DEL_CD,SC_NO,RFA_NO,BDR_IND_FLG,INV_DT,LOCL_CURR_CD,INV_TTL_LOCL_AMT,EDI_SND_FLG,EDI_SND_DT,EDI_TP_CD,CHG_SEQ,CHG_CD,CURR_CD,PER_TP_CD,TRF_RT_AMT,RAT_AS_CNTR_QTY,CHG_AMT,INV_DELT_DIV_CD 
FROM (
SELECT 
      DECODE(INV_NO, '', DECODE(EDI_TP_CD, 'INV_BL', BL_NO||'-'||LPAD(BL_SEQ, 3, '0'), CNTR_NO||'-'||LPAD((BL_SEQ + DENSE_RANK() OVER (PARTITION BY DECODE(INV_NO, '', CNTR_NO, '') ORDER BY DECODE(SUBSTR(REV_TP_SRC_CD, 1, 1), 'M', 'B','A'))), 3, '0')), INV_NO) MERGE_CHK
    , EDI_HDR_SEQ
    , AR_IF_NO
    , BL_NO
    , DECODE(INV_NO, '', (BL_SEQ + DECODE(EDI_TP_CD, 'INV_BL', 0, DENSE_RANK() OVER (PARTITION BY DECODE(INV_NO, '', CNTR_NO, '') ORDER BY DECODE(SUBSTR(REV_TP_SRC_CD, 1, 1), 'M', 'B','A')))), BL_SEQ) BL_SEQ
    , BKG_NO  
    , DECODE(INV_NO, '', DECODE(EDI_TP_CD, 'INV_BL', BL_NO||'-'||LPAD(BL_SEQ, 3, '0'), CNTR_NO||'-'||LPAD((BL_SEQ + DENSE_RANK() OVER (PARTITION BY DECODE(INV_NO, '', CNTR_NO, '') ORDER BY DECODE(SUBSTR(REV_TP_SRC_CD, 1, 1), 'M', 'B','A'))), 3, '0')), INV_NO) INV_NO
    , CNTR_NO
    , AR_OFC_CD
    , DECODE(SUBSTR(REV_TP_SRC_CD, 1, 1), 'M', 'DMT', 'BKG') SOURCE_CD
    , REV_TP_SRC_CD
    , ACT_CUST_CNT_CD
    , ACT_CUST_SEQ
	, ACT_CUST_CNT_CD||'-'||LPAD(ACT_CUST_SEQ, 6, '0') CUST_CD
    , (SELECT CUST_LGL_ENG_NM
       FROM MDM_CUSTOMER
       WHERE CUST_CNT_CD = ACT_CUST_CNT_CD
       AND CUST_SEQ = ACT_CUST_SEQ) CUST_NM
    , VSL_CD
    , SKD_VOY_NO
    , SKD_DIR_CD
    , TRNK_VVD_CD
    , SVC_SCP_CD
    , SLAN_CD
    , SAIL_ARR_DT
    , IO_BND_CD
    , POR_CD
    , POL_CD
    , POD_CD
    , DEL_CD
    , SC_NO
    , RFA_NO
    , BDR_IND_FLG
    , DECODE(INV_DT, '', TO_CHAR(SYSDATE, 'YYYYMMDD'), INV_DT) INV_DT
    , LOCL_CURR_CD
    , DECODE(INV_TTL_LOCL_AMT, '', (SUM(CHG_AMT * INV_XCH_RT) OVER (PARTITION BY AR_IF_NO, DECODE(EDI_TP_CD, 'INV_BL', AR_IF_NO, CNTR_NO))), INV_TTL_LOCL_AMT) INV_TTL_LOCL_AMT
    , EDI_SND_FLG
    , TO_CHAR(EDI_SND_DT, 'YYYY-MM-DD HH24:MI:SS') EDI_SND_DT
    , EDI_TP_CD
    , CHG_SEQ
    , CHG_CD
    , CURR_CD
    , PER_TP_CD
    , TRF_RT_AMT
    , RAT_AS_CNTR_QTY
    , CHG_AMT
    , INV_DELT_DIV_CD 
FROM (
        -- START INV_AR_MN MAX ----------------------------        
  		SELECT NULL EDI_HDR_SEQ 
			, A.AR_IF_NO 
	        , A.BL_SRC_NO BL_NO
            , DECODE(C.INV_EDI_LVL_CD, 'B', NVL((SELECT LENGTH(SUBSTR(XMLAGG (XMLELEMENT (X , ',', AR_IF_NO) ORDER BY AR_IF_NO).EXTRACT ( '//text()' ), 1))
                                                        - LENGTH(REPLACE(SUBSTR(XMLAGG (XMLELEMENT (X , ',', AR_IF_NO) ORDER BY AR_IF_NO).EXTRACT ( '//text()' ), 1), ',', ''))
                                                 FROM INV_AR_MN
                                                 WHERE BL_SRC_NO = A.BL_SRC_NO
                                                 AND AR_OFC_CD = A.AR_OFC_CD
                                                 AND REV_TP_CD <> 'M'
                                                 AND NVL(INV_SPLIT_CD, 'M') IN ('M','C','E','X')
                                                 AND BL_INV_CFM_DT IS NOT NULL
                                                 AND INV_DELT_DIV_CD <> 'Y'
                                                 AND AR_IF_NO <= A.AR_IF_NO), 1)
                                           , NVL((SELECT MAX(BL_SEQ)
                                                 FROM INV_EDI_HDR
                                                 WHERE CNTR_NO = B.CNTR_NO
                                                 AND AR_OFC_CD = A.AR_OFC_CD
												 AND EDI_TP_CD = 'INV_CNTR'), 0))    AS BL_SEQ     
            , A.BKG_NO
            , '' INV_NO
            , B.CNTR_NO
            , A.AR_OFC_CD
            , A.REV_TP_CD||A.REV_SRC_CD REV_TP_SRC_CD
            , A.ACT_CUST_CNT_CD
            , A.ACT_CUST_SEQ
            , A.VSL_CD
            , A.SKD_VOY_NO
            , A.SKD_DIR_CD
            , A.TRNK_VSL_CD||A.TRNK_SKD_VOY_NO||A.TRNK_SKD_DIR_CD TRNK_VVD_CD
            , A.SVC_SCP_CD
            , A.SLAN_CD
            , A.SAIL_ARR_DT
            , A.IO_BND_CD
            , A.POR_CD
            , A.POL_CD
            , A.POD_CD
            , A.DEL_CD
            , A.SC_NO
            , A.RFA_NO
            , B.BDR_IND_FLG
            , '' INV_DT
            , A.LOCL_CURR_CD
            , NULL INV_TTL_LOCL_AMT
            , 'N' EDI_SND_FLG
            , NULL EDI_SND_DT
            , DECODE(C.INV_EDI_LVL_CD, 'B', 'INV_BL', 'INV_CNTR') EDI_TP_CD
            , ROW_NUMBER() OVER (PARTITION BY A.BL_SRC_NO, B.CNTR_NO ORDER BY B.CHG_CD, B.CURR_CD, B.PER_TP_CD) CHG_SEQ
            , B.CHG_CD
            , B.CURR_CD
            , B.PER_TP_CD
            , B.TRF_RT_AMT
            , B.RAT_AS_CNTR_QTY
            , B.CHG_AMT
            , (SELECT INV_XCH_RT
               FROM INV_AR_CHG
               WHERE AR_IF_NO = A.AR_IF_NO
               AND CURR_CD = B.CURR_CD
               AND ROWNUM = 1) INV_XCH_RT
            , A.INV_DELT_DIV_CD 
        FROM INV_AR_MN A,      
            (SELECT X.AR_IF_NO
                      , X.BL_SRC_NO BL_NO
                      , X.ACT_CUST_CNT_CD
                      , X.ACT_CUST_SEQ
                      , X.BDR_IND_FLG
                      , Y.CNTR_NO
                      , Y.CHG_CD
                      , Y.CURR_CD
                      , Y.RAT_UT_CD PER_TP_CD
                      , Y.RAT_AS_QTY  RAT_AS_CNTR_QTY
                      , Y.CHG_UT_AMT  TRF_RT_AMT
                      , SUM(DECODE(X.INV_DELT_DIV_CD, 'X', 0, Y.CHG_AMT)) CHG_AMT
                 FROM 
                     (SELECT P.AR_IF_NO
                            , P.BL_SRC_NO
                            , P.BKG_NO
                            , P.ACT_CUST_CNT_CD
                            , P.ACT_CUST_SEQ
                            , P.INV_DELT_DIV_CD
                            , Q.BDR_IND_FLG
                            , R.OFC_CD
                            , R.IO_BND_CD
                            , R.N3RD_FLG
                            , T.INV_EDI_LVL_CD
                     FROM INV_AR_MN P,
                          INV_BKG_IF_MN Q,
                          INV_BKG_IF_CHG R,
                          MDM_ORGANIZATION S,
                          MDM_CUSTOMER T
                     WHERE P.BKG_NO = Q.BKG_NO
                     AND Q.BKG_SEQ = (SELECT MAX(BKG_SEQ) FROM INV_BKG_IF_MN WHERE BKG_NO = P.BKG_NO)
                     AND P.BKG_NO = R.BKG_NO
                     AND R.BKG_SEQ = (SELECT MAX(BKG_SEQ) FROM INV_BKG_IF_MN WHERE BKG_NO = P.BKG_NO)
                     AND R.OFC_CD = S.OFC_CD
                     AND P.AR_OFC_CD = S.AR_OFC_CD
                     AND P.IO_BND_CD = R.IO_BND_CD
                     AND P.ACT_CUST_CNT_CD = T.CUST_CNT_CD
                     AND P.ACT_CUST_SEQ = T.CUST_SEQ
                     AND P.OTS_PAY_CD IS NULL AND P.ORG_INV_NO IS NULL --Except migration data
                     AND P.AR_IF_NO IN (SELECT MAX(AR_IF_NO)
                                        FROM INV_AR_MN
                                        WHERE AR_OFC_CD = @[ar_ofc_cd]
										#if (${sc_rfa_no} != '')
                                        	#if (${cntc_tp_cd} == 'S')
                                            	AND SC_NO IN (@[sc_rfa_no])
                                        	#elseif (${cntc_tp_cd} == 'R')
                                            	AND RFA_NO IN (@[sc_rfa_no])
                                        	#end
										#end
                                        #if (${cust_cd} != '')
                                            AND ACT_CUST_CNT_CD||ACT_CUST_SEQ IN (@[cust_cd])
                                        #end
                                        AND REV_TP_CD <> 'M'
                                        AND NVL(INV_SPLIT_CD, 'M') IN ('M','C','E','X')
                                        AND BL_INV_CFM_DT IS NOT NULL
                                        AND INV_DELT_DIV_CD <> 'Y'
                                        GROUP BY BL_SRC_NO)  
                     AND P.INV_DELT_DIV_CD <> 'X' 
                     GROUP BY P.AR_IF_NO
                            , P.BL_SRC_NO
                            , P.BKG_NO
                            , P.ACT_CUST_CNT_CD
                            , P.ACT_CUST_SEQ
                            , P.INV_DELT_DIV_CD
                            , Q.BDR_IND_FLG
                            , R.OFC_CD
                            , R.IO_BND_CD
                            , R.N3RD_FLG
                            , T.INV_EDI_LVL_CD
                     ) X,
                     BKG_CNTR_RT Y,
					 BKG_RATE Z
                WHERE X.BKG_NO = Y.BKG_NO
				AND X.BKG_NO = Z.BKG_NO
                -- AND DECODE(X.IO_BND_CD,'O','P','I','C') = Y.FRT_TERM_CD
                AND X.OFC_CD = NVL(Y.N3PTY_RCV_OFC_CD, DECODE(Y.FRT_TERM_CD, 'P', Z.PPD_RCV_OFC_CD, Z.CLT_OFC_CD))
                AND X.AR_IF_NO || Y.CNTR_NO  NOT IN (SELECT AR_IF_NO || CNTR_NO  
                                                     FROM INV_EDI_HDR AA
                                                     WHERE AA.EDI_TP_CD IN ('INV_BL','INV_CNTR') ) 
                GROUP BY X.AR_IF_NO
                      , X.BL_SRC_NO
                      , X.ACT_CUST_CNT_CD
                      , X.ACT_CUST_SEQ
                      , X.BDR_IND_FLG
                      , X.INV_EDI_LVL_CD
                      , Y.CNTR_NO
                      , Y.CHG_CD
                      , Y.CURR_CD
                      , Y.RAT_UT_CD    
                      , Y.RAT_AS_QTY
                      , Y.CHG_UT_AMT                
            ) B,
            MDM_CUSTOMER C
        WHERE A.AR_IF_NO = B.AR_IF_NO    
        AND A.ACT_CUST_CNT_CD = C.CUST_CNT_CD
        AND A.ACT_CUST_SEQ = C.CUST_SEQ
        AND B.CHG_AMT <> 0  
        AND A.OTS_PAY_CD IS NULL AND A.ORG_INV_NO IS NULL --Except migration data
        -- END INV_AR_MN MAX ----------------------------
        UNION ALL
        -- START DM,DT MAX ----------------------------
        SELECT NULL EDI_HDR_SEQ 
			, A.AR_IF_NO 
	        , A.BL_SRC_NO BL_NO         
            , DECODE(D.INV_EDI_LVL_CD, 'B', NVL((SELECT LENGTH(SUBSTR(XMLAGG (XMLELEMENT (X , ',', AR_IF_NO) ORDER BY AR_IF_NO).EXTRACT ( '//text()' ), 1))
                                                        - LENGTH(REPLACE(SUBSTR(XMLAGG (XMLELEMENT (X , ',', AR_IF_NO) ORDER BY AR_IF_NO).EXTRACT ( '//text()' ), 1), ',', ''))
                                                 FROM INV_AR_MN
                                                 WHERE BL_SRC_NO = A.BL_SRC_NO
                                                 AND AR_OFC_CD = A.AR_OFC_CD
                                                 AND REV_TP_CD||REV_SRC_CD IN ('MDM', 'MDT')
                                                 AND BL_INV_CFM_DT IS NOT NULL
                                                 AND INV_DELT_DIV_CD <> 'Y'
                                                 AND AR_IF_NO <= A.AR_IF_NO), 1) + 900
                                           , NVL((SELECT MAX(BL_SEQ)
                                                 FROM INV_EDI_HDR
                                                 WHERE CNTR_NO = C.TRF_NO
                                                 AND AR_OFC_CD = A.AR_OFC_CD
												 AND EDI_TP_CD = 'INV_CNTR'), 0))    AS BL_SEQ     
            , A.BKG_NO
            , '' INV_NO
            , C.TRF_NO AS CNTR_NO
            , A.AR_OFC_CD
            , A.REV_TP_CD||A.REV_SRC_CD REV_TP_SRC_CD
            , A.ACT_CUST_CNT_CD
            , A.ACT_CUST_SEQ
            , A.VSL_CD
            , A.SKD_VOY_NO
            , A.SKD_DIR_CD
            , A.TRNK_VSL_CD||A.TRNK_SKD_VOY_NO||A.TRNK_SKD_DIR_CD TRNK_VVD_CD
            , A.SVC_SCP_CD
            , A.SLAN_CD
            , A.SAIL_ARR_DT
            , A.IO_BND_CD
            , A.POR_CD
            , A.POL_CD
            , A.POD_CD
            , A.DEL_CD
            , A.SC_NO
            , A.RFA_NO
            , '' BDR_IND_FLG
            , '' INV_DT
            , A.LOCL_CURR_CD
            , NULL INV_TTL_LOCL_AMT
            , 'N' EDI_SND_FLG
            , NULL EDI_SND_DT
            , DECODE(D.INV_EDI_LVL_CD, 'B', 'INV_BL', 'INV_CNTR') EDI_TP_CD
            , C.CHG_SEQ
            , C.CHG_CD
            , C.CURR_CD
            , C.PER_TP_CD
            , NULL TRF_RT_AMT
            , NULL RAT_AS_CNTR_QTY
            , C.CHG_AMT
            , (SELECT INV_XCH_RT
               FROM INV_AR_CHG
               WHERE AR_IF_NO = A.AR_IF_NO
               AND CURR_CD = C.CURR_CD
               AND ROWNUM = 1) INV_XCH_RT
             , A.INV_DELT_DIV_CD 
        FROM INV_AR_MN A,
             INV_AR_IF_MN B,
             INV_AR_IF_CHG C,
             MDM_CUSTOMER D
        WHERE A.AR_IF_NO = B.AR_IF_NO
        AND B.SRC_IF_DT = C.SRC_IF_DT
        AND B.SRC_IF_SEQ = C.SRC_IF_SEQ
        AND A.ACT_CUST_CNT_CD = D.CUST_CNT_CD
        AND A.ACT_CUST_SEQ = D.CUST_SEQ
        AND B.BL_INV_IF_FLG = 'Y'
        AND B.BL_INV_IF_DT IS NOT NULL
        AND A.REV_TP_CD = 'M'
        AND A.REV_SRC_CD IN ('DM', 'DT')
        AND A.OTS_PAY_CD IS NULL AND A.ORG_INV_NO IS NULL --Except migration data
        AND A.AR_OFC_CD = @[ar_ofc_cd]
		#if (${sc_rfa_no} != '')
        	#if (${cntc_tp_cd} == 'S')
            	AND A.SC_NO IN (@[sc_rfa_no])
        	#elseif (${cntc_tp_cd} == 'R')
            	AND A.RFA_NO IN (@[sc_rfa_no])
        	#end
		#end
        #if (${cust_cd} != '')
            AND A.ACT_CUST_CNT_CD||A.ACT_CUST_SEQ IN (@[cust_cd])
        #end
        AND A.AR_IF_NO NOT IN (SELECT AR_IF_NO   --** ADDED
                               FROM INV_EDI_HDR AA
                               WHERE AA.EDI_TP_CD IN ('INV_BL','INV_CNTR') ) 
        -- END DM,DT MAX ----------------------------                
        UNION ALL
        -- START EDI TRANSFERED DATA
        SELECT A.EDI_HDR_SEQ
			, A.AR_IF_NO 
            , A.BL_NO
            , A.BL_SEQ
            , A.BKG_NO
            , A.INV_NO
           -- , DECODE(A.EDI_TP_CD, 'INV_BL', (SELECT SUBSTR(XMLAGG(XMLELEMENT(X, ',', CNTR_NO) ORDER BY CNTR_NO).EXTRACT('//text()'), 2)
           --                                  FROM INV_EDI_CNTR
           --                                  WHERE EDI_HDR_SEQ = A.EDI_HDR_SEQ), A.CNTR_NO) CNTR_NO
            , A.CNTR_NO AS CNTR_NO                     
            , A.AR_OFC_CD
            , A.REV_TP_SRC_CD
            , A.ACT_CUST_CNT_CD
            , A.ACT_CUST_SEQ
            , A.VSL_CD
            , A.SKD_VOY_NO
            , A.SKD_DIR_CD
            , A.TRNK_VVD_CD
            , A.SVC_SCP_CD
            , A.SLAN_CD
            , A.SAIL_ARR_DT
            , A.IO_BND_CD
            , A.POR_CD
            , A.POL_CD
            , A.POD_CD
            , A.DEL_CD
            , A.SC_NO
            , A.RFA_NO
            , A.BDR_IND_FLG
            , A.INV_DT
            , A.LOCL_CURR_CD
            , A.INV_TTL_LOCL_AMT
            , A.EDI_SND_FLG
            , A.EDI_SND_DT
            , A.EDI_TP_CD
            , B.CHG_SEQ
            , B.CHG_CD
            , B.CURR_CD
            , B.PER_TP_CD
            , B.TRF_RT_AMT
            , B.RAT_AS_CNTR_QTY
            , B.CHG_AMT
            , NULL INV_XCH_RT
            , 'N' INV_DELT_DIV_CD 
        FROM INV_EDI_HDR A,
             INV_EDI_CHG B
        WHERE A.EDI_HDR_SEQ = B.EDI_HDR_SEQ
        AND A.AR_OFC_CD = @[ar_ofc_cd]
		#if (${sc_rfa_no} != '')
        	#if (${cntc_tp_cd} == 'S')
            	AND A.SC_NO IN (@[sc_rfa_no])
        	#elseif (${cntc_tp_cd} == 'R')
            	AND A.RFA_NO IN (@[sc_rfa_no])
        	#end
		#end
        #if (${cust_cd} != '')
            AND A.ACT_CUST_CNT_CD||A.ACT_CUST_SEQ IN (@[cust_cd])
        #end
        AND A.EDI_TP_CD IN ('INV_BL','INV_CNTR')
        -- END EDI TRANSFERED DATA
        UNION ALL
        -- START EDI CANCEL DATA  **** 
        SELECT NULL EDI_HDR_SEQ
			, C.NEXT_AR_IF_NO AS AR_IF_NO 
            , A.BL_NO
            , DECODE(A.EDI_TP_CD, 'INV_BL', NVL((SELECT LENGTH(SUBSTR(XMLAGG (XMLELEMENT (X , ',', AR_IF_NO) ORDER BY AR_IF_NO).EXTRACT ( '//text()' ), 1))
                                                        - LENGTH(REPLACE(SUBSTR(XMLAGG (XMLELEMENT (X , ',', AR_IF_NO) ORDER BY AR_IF_NO).EXTRACT ( '//text()' ), 1), ',', ''))
                                                 FROM INV_AR_MN
                                                 WHERE BL_SRC_NO = A.BL_NO
                                                 AND AR_OFC_CD = A.AR_OFC_CD
                                                 AND REV_TP_CD <> 'M'
                                                 AND NVL(INV_SPLIT_CD, 'M') IN ('M','C','E','X')
                                                 AND BL_INV_CFM_DT IS NOT NULL
                                                 AND INV_DELT_DIV_CD <> 'Y'
                                                 AND AR_IF_NO <= C.NEXT_AR_IF_NO), 1)
                                           , NVL((SELECT MAX(BL_SEQ)
                                                 FROM INV_EDI_HDR
                                                 WHERE CNTR_NO = A.CNTR_NO
                                                 AND AR_OFC_CD = A.AR_OFC_CD
												 AND EDI_TP_CD = 'INV_CNTR'), 0))    AS BL_SEQ     
            , A.BKG_NO
            , '' INV_NO
           -- , DECODE(A.EDI_TP_CD, 'INV_BL', (SELECT SUBSTR(XMLAGG(XMLELEMENT(X, ',', CNTR_NO) ORDER BY CNTR_NO).EXTRACT('//text()'), 2)
           --                                  FROM INV_EDI_CNTR
           --                                  WHERE EDI_HDR_SEQ = A.EDI_HDR_SEQ), A.CNTR_NO) CNTR_NO
            , A.CNTR_NO AS CNTR_NO
            , A.AR_OFC_CD
            , A.REV_TP_SRC_CD
            , A.ACT_CUST_CNT_CD
            , A.ACT_CUST_SEQ
            , A.VSL_CD
            , A.SKD_VOY_NO
            , A.SKD_DIR_CD
            , A.TRNK_VVD_CD
            , A.SVC_SCP_CD
            , A.SLAN_CD
            , A.SAIL_ARR_DT
            , A.IO_BND_CD
            , A.POR_CD
            , A.POL_CD
            , A.POD_CD
            , A.DEL_CD
            , A.SC_NO
            , A.RFA_NO
            , A.BDR_IND_FLG
            , A.INV_DT
            , A.LOCL_CURR_CD
            , A.INV_TTL_LOCL_AMT
            , 'N' EDI_SND_FLG
            , NULL EDI_SND_DT
            , A.EDI_TP_CD
            , B.CHG_SEQ
            , B.CHG_CD
            , B.CURR_CD
            , B.PER_TP_CD
            , B.TRF_RT_AMT
            , B.RAT_AS_CNTR_QTY
            , B.CHG_AMT * (-1) AS CHG_AMT
            , NULL INV_XCH_RT
            , 'X' AS INV_DELT_DIV_CD 
        FROM INV_EDI_HDR A,
             INV_EDI_CHG B,
             (
                    SELECT (SELECT MAX(AR_IF_NO)
                            FROM INV_AR_MN
                            WHERE BL_SRC_NO = IAM.BL_SRC_NO
                            AND AR_OFC_CD = IAM.AR_OFC_CD
                            AND REV_TP_CD <> 'M'
                            AND NVL(INV_SPLIT_CD, 'M') IN ('M','C','E','X')  
                            AND BL_INV_CFM_DT IS NOT NULL
                            AND INV_DELT_DIV_CD = 'N' 
                            AND AR_IF_NO < IAM.AR_IF_NO) AS MAX_AR_IF_NO, IAM.AR_IF_NO AS NEXT_AR_IF_NO
                    FROM INV_AR_MN IAM
                    WHERE AR_OFC_CD = @[ar_ofc_cd]
                    #if (${sc_rfa_no} != '')
                        #if (${cntc_tp_cd} == 'S')
                            AND SC_NO IN (@[sc_rfa_no])
                        #elseif (${cntc_tp_cd} == 'R')
                            AND RFA_NO IN (@[sc_rfa_no])
                        #end
                    #end
                    #if (${cust_cd} != '')
                        AND ACT_CUST_CNT_CD||ACT_CUST_SEQ IN (@[cust_cd])
                    #end
                    AND REV_TP_CD <> 'M'
                    AND NVL(INV_SPLIT_CD, 'M') IN ('M','C','E','X')
                    AND BL_INV_CFM_DT IS NOT NULL
                    AND INV_DELT_DIV_CD = 'X' 
                    AND IAM.OTS_PAY_CD IS NULL AND IAM.ORG_INV_NO IS NULL --Except migration data
                    AND AR_IF_NO NOT IN (SELECT AR_IF_NO   
                                         FROM INV_EDI_HDR
                                         WHERE EDI_TP_CD IN ('INV_BL','INV_CNTR')
                                        )                               
             ) C
        WHERE A.EDI_HDR_SEQ = B.EDI_HDR_SEQ
        AND   A.AR_IF_NO = C.MAX_AR_IF_NO
        AND A.EDI_TP_CD IN ('INV_BL','INV_CNTR')
        -- END -- START EDI CANCEL DATA
    )
WHERE 1 = 1
#if (${source_cd} == 'BKG')
    AND SUBSTR(REV_TP_SRC_CD, 1, 1) <> 'M'
#elseif (${source_cd} == 'DMT')
    AND SUBSTR(REV_TP_SRC_CD, 1, 1) = 'M'
#end
#if (${io_bnd_cd} != 'A')
    AND IO_BND_CD = @[io_bnd_cd]
#end
#if (${tvvd_cd} != '')
    AND TRNK_VVD_CD = @[tvvd_cd]
#end
#if (${bdr_ind_flg} != 'A')
    AND BDR_IND_FLG = @[bdr_ind_flg]     
#end
#if (${bl_no} != '')
    AND BL_NO = @[bl_no]
#end
#if (${sa_from_dt} != '' && ${sa_to_dt} != '')
    AND SAIL_ARR_DT BETWEEN REPLACE(@[sa_from_dt], '-', '') AND REPLACE(@[sa_to_dt], '-', '')
#end
#if (${svc_scp_cd} != '')
    AND SVC_SCP_CD = @[svc_scp_cd]
#end
#if (${chg_cd} != '')
    AND CHG_CD IN (@[chg_cd])
#end
#if (${cntr_no} != '')
    AND CNTR_NO LIKE '%'||@[cntr_no]||'%'
#end
)
ORDER BY  
          MERGE_CHK
        , DECODE(EDI_TP_CD, 'INV_BL', CNTR_NO, BL_NO)
        , AR_IF_NO
        , CHG_SEQ			]]></sql>
