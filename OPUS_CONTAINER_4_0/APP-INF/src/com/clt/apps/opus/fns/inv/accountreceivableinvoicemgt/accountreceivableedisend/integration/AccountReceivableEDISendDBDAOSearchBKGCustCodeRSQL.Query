<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchBKGCustCodeRSQL">
			<desc><![CDATA[SearchBKGCustCode]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD||CUST_SEQ AS BKG_CUST
FROM   BKG_CUSTOMER
WHERE  BKG_NO = @[bkg_no]
AND    CUST_CNT_CD IS NOT NULL
AND    CUST_SEQ IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
