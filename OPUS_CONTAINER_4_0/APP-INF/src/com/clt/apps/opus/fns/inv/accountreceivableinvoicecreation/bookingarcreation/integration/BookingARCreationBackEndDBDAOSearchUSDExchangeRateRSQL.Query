<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationBackEndDBDAOSearchUSDExchangeRateRSQL">
			<desc><![CDATA[Search USD Exchange Rate]]></desc>
			<sql><![CDATA[
#if (${chg_curr_cd} == 'USD')
    SELECT NVL(ROUND(DECODE(@[inv_xch_rt], 0, 0, 1/@[inv_xch_rt]), 6), 0) USD_XCH_RT
    FROM DUAL
#else
    #if (${xch_rt_tp_cd} == 'V')
        SELECT NVL(ROUND(DECODE(INV_XCH_RT, 0, 0, @[inv_xch_rt]/INV_XCH_RT), 6), 0) USD_XCH_RT
        FROM INV_VVD_XCH_RT
        WHERE VSL_CD = @[vsl_cd]
        AND SKD_VOY_NO = @[skd_voy_no]
        AND SKD_DIR_CD = @[skd_dir_cd]
        AND PORT_CD = @[port_cd]
        AND SVC_SCP_CD = @[svc_scp_cd]
        AND IO_BND_CD = @[io_bnd_cd]
        AND AR_OFC_CD = @[ar_ofc_cd]
        AND LOCL_CURR_CD = @[locl_curr_cd]
        AND CHG_CURR_CD = 'USD'
    #elseif (${xch_rt_tp_cd} == 'I')
        SELECT NVL(ROUND(DECODE(INV_XCH_RT, 0, 0, @[inv_xch_rt]/INV_XCH_RT), 6), 0) USD_XCH_RT
        FROM INV_CUST_AND_DLY_XCH_RT
        WHERE CUST_CNT_CD = @[cust_cnt_cd]
        AND CUST_SEQ = @[cust_seq]
        AND IO_BND_CD = @[io_bnd_cd]
        AND FM_DT = REPLACE(@[fm_dt],'-','')
        AND TO_DT = REPLACE(@[to_dt],'-','')
        AND LOCL_CURR_CD = @[locl_curr_cd]
        AND CHG_CURR_CD = 'USD'
    #elseif (${xch_rt_tp_cd} == 'D')
        SELECT NVL(ROUND(DECODE(INV_XCH_RT, 0, 0, @[inv_xch_rt]/INV_XCH_RT), 6), 0) USD_XCH_RT
        FROM INV_CUST_AND_DLY_XCH_RT
        WHERE CUST_CNT_CD = 'XX'
        AND CUST_SEQ = 0
        AND IO_BND_CD = @[io_bnd_cd]
        AND FM_DT = REPLACE(@[fm_dt],'-','')
        AND TO_DT = REPLACE(@[to_dt],'-','')
        AND LOCL_CURR_CD = @[locl_curr_cd]
        AND CHG_CURR_CD = 'USD'
		AND AR_OFC_CD = @[ar_ofc_cd]
    #end
#end			]]></sql>
			<params>
				<param name="inv_xch_rt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
