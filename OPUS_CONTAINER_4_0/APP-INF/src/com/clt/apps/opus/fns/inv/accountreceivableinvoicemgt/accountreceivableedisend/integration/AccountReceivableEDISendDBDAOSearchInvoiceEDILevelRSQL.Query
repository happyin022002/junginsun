<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchInvoiceEDILevelRSQL">
			<desc><![CDATA[Search Invoice EDI Level]]></desc>
			<sql><![CDATA[
SELECT NVL(INV_EDI_LVL_CD, 'B') INV_EDI_LVL_CD
FROM MDM_CUSTOMER
WHERE (CUST_CNT_CD, CUST_SEQ) IN 
#if (${cntc_tp_cd} == 'S')
    (SELECT DISTINCT PTY.CUST_CNT_CD
         , PTY.CUST_SEQ
         --, PTY.CTRT_PTY_NM
      FROM PRI_SP_CTRT_PTY PTY
         , PRI_SP_HDR HDR
     WHERE HDR.PROP_NO = PTY.PROP_NO
       AND PTY.PRC_CTRT_PTY_TP_CD = 'C'
       AND HDR.SC_NO = @[sc_rfa_no])
#else
    (SELECT DISTINCT MN.CTRT_CUST_CNT_CD
         , MN.CTRT_CUST_SEQ
      FROM PRI_RP_MN MN
         , PRI_RP_HDR HDR
     WHERE HDR.PROP_NO = MN.PROP_NO
       AND HDR.RFA_NO = @[sc_rfa_no])
#end			]]></sql>
			<params>
				<param name="sc_rfa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
