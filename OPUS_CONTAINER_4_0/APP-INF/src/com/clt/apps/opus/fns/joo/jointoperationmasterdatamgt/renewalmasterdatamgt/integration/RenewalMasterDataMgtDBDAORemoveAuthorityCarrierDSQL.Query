<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAORemoveAuthorityCarrierDSQL">
			<desc><![CDATA[Authority Carrier Delete]]></desc>
			<sql><![CDATA[
DELETE
  FROM JOO_CRR_AUTH
 WHERE 1=1
   AND JO_CRR_CD    = @[jo_crr_cd]
   AND RLANE_CD     = @[rlane_cd]
#if (${auth_ofc_cd} != '') 
   AND AUTH_OFC_CD  = @[auth_ofc_cd]
#end			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="auth_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
