<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOJooLaneRSQL">
			<desc><![CDATA[JOO_Carrier에서 LANE코드 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.RLANE_CD AS CODE
     , A.RLANE_CD AS NAME
  FROM JOO_CARRIER A
     , JOO_CRR_AUTH B
 WHERE A.DELT_FLG = 'N'
   AND A.JO_CRR_CD  = B.JO_CRR_CD
   AND A.RLANE_CD   = B.RLANE_CD
#if (${ofc_cd} != '')   
   AND B.AUTH_OFC_CD = @[ofc_cd]
#end
#if (${auth_ofc_cd} != '')
   /*2016.08.01 추가 개발된 코드.*/
   AND B.AUTH_OFC_CD = @[auth_ofc_cd]
#end
#if (${auth_delcheck_yn} == 'N') -- N : WHERE 절에 걸지 않음.(AUTH.DELT_FLG)
   --AND NVL(B.DELT_FLG,'N') = 'N'
#else
   AND NVL(B.DELT_FLG,'N') = 'N'
#end
#if (${super_cd1} != '')
   AND A.JO_CRR_CD  = @[super_cd1]
#end
#if (${super_cd2} != '')
   AND A.TRD_CD     = @[super_cd2]
#end
#if (${code} != '')
   AND A.RLANE_CD   = @[code]
#end
 ORDER BY 1			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="auth_ofc_cd" type="12" value="" out="N"/>
				<param name="super_cd1" type="12" value="COS" out="N"/>
				<param name="super_cd2" type="12" value="AES" out="N"/>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
