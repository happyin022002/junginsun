<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchAcctRateRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchAcctRateRSQL]]></desc>
			<sql><![CDATA[
SELECT USD_LOCL_XCH_RT 
  FROM GL_MON_XCH_RT
 WHERE ACCT_XCH_RT_LVL = '1'
   AND CURR_CD = SUBSTR(@[curr_cd], 1, 3)
   AND ACCT_XCH_RT_YRMON = TO_CHAR(SYSDATE, 'YYYYMM')

			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="EUR" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
