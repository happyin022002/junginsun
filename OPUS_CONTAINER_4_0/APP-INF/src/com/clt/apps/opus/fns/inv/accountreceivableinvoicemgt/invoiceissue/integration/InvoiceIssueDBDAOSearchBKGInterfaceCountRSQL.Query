<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOSearchBKGInterfaceCountRSQL">
			<desc><![CDATA[Search BKG Interface Count]]></desc>
			<sql><![CDATA[
SELECT COUNT(BKG_NO) CNT
FROM INV_BKG_IF_MN
WHERE BKG_NO IN ( SELECT BKG_NO FROM INV_AR_MN WHERE BL_SRC_NO IN (${bl_nos}) )
AND INV_BAT_CD <> 'Y'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
