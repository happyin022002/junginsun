<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalConsultationDBDAOCheckExistInvoiceDetailRSQL">
			<desc><![CDATA[JOO_INV_DTL 존재 여부]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(D.ACCT_YRMON) > 0 THEN 'Y' ELSE 'N' END AS EXIST_FLG
  FROM JOO_INV_DTL D
 WHERE 1=1
   AND D.VSL_CD         = @[vsl_cd]
   AND D.SKD_VOY_NO     = @[skd_voy_no]
   AND D.SKD_DIR_CD     = @[skd_dir_cd]
   AND D.REV_DIR_CD     = @[rev_dir_cd]
   AND D.REV_YRMON      = @[rev_yrmon]
   AND D.STL_VVD_SEQ    = TO_NUMBER(@[stl_vvd_seq])			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
