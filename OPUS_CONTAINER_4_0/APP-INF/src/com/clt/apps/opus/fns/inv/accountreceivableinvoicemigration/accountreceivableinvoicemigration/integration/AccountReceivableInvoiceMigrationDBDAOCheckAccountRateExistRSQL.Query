<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableInvoiceMigrationDBDAOCheckAccountRateExistRSQL">
			<desc><![CDATA[Check Account Rate Exist]]></desc>
			<sql><![CDATA[
SELECT 
	count(*) CNT
FROM GL_MON_XCH_RT
WHERE	ACCT_XCH_RT_YRMON = substr(REPLACE(@[eff_dt],'-',''),0,6)			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
