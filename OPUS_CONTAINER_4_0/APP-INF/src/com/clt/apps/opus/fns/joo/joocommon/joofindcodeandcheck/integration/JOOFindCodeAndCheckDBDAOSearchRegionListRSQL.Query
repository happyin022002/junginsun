<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchRegionListRSQL">
			<desc><![CDATA[Region Search]]></desc>
			<sql><![CDATA[
SELECT DISTINCT RDR_RGN_CD AS CODE
     , RDR_RGN_NM AS NAME
  FROM OPF_RDR_RGN_CD
 WHERE USE_FLG = 'Y'
 ORDER BY RDR_RGN_CD 			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
