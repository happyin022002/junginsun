			<sql><![CDATA[
UPDATE INV_REV_ACCT_CD 
 SET ACCT_CD = @[acct_cd]
 ,ACCT_LOCL_NM = ''
 ,ACCT_ENG_NM = @[acct_eng_nm]
 ,PRC_DIV_CD = @[prc_div_cd]
 ,DELT_FLG = @[delt_flg]
 ,REP_CHG_CD = @[rep_chg_cd]
 ,UPD_USR_ID = @[upd_usr_id]
 ,UPD_DT = SYSDATE
WHERE INV_SRC_CD = @[inv_src_cd]
AND REV_TP_GRP_CD = @[rev_tp_grp_cd]
AND REV_TP_SRC_CD = @[rev_tp_src_cd]
AND INV_ACCT_DIV_CD = @[inv_acct_div_cd]
AND CHG_CD = @[chg_cd]			]]></sql>
