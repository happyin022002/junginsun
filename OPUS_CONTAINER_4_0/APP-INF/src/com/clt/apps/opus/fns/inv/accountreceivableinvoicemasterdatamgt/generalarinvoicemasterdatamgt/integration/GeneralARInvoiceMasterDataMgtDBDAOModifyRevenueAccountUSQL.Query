				<param name="acct_krn_nm" type="12" value="" out="N"/>
				<param name="acct_eng_nm" type="12" value="" out="N"/>
				<param name="prc_div_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="rep_chg_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="inv_src_cd" type="12" value="" out="N"/>
				<param name="rev_tp_grp_cd" type="12" value="" out="N"/>
				<param name="rev_tp_src_cd" type="12" value="" out="N"/>
				<param name="inv_acct_div_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
