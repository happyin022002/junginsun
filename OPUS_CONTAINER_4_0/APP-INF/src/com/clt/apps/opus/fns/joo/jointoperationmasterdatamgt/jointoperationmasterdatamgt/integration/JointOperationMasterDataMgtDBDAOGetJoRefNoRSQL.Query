<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOGetJoRefNoRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT /*+INDEX_DESC(A XPKJOO_BZC_AGMT)*/
       A.JO_REF_NO,
       A.JO_REF_SEQ + 1 AS JO_REF_SEQ
  FROM JOO_BZC_AGMT A
 WHERE A.JO_REF_NO LIKE @[ofc_cd]||@[re_divr_cd]||@[trd_cd]||@[rlane_cd]||TO_CHAR(SYSDATE,'RRMM')||'%'
   AND A.AGMT_EFF_DT = REPLACE(@[agmt_eff_dt],'-','')
   AND A.AGMT_EXP_DT = REPLACE(@[agmt_exp_dt],'-','')
   AND A.DELT_FLG  = 'N'
   AND ROWNUM      = 1			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="agmt_eff_dt" type="12" value="" out="N"/>
				<param name="agmt_exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
