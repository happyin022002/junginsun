<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOsearchARSetupOfficeForIssueReIssueRSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOsearchARSetupOfficeForIssueReIssueRSQL]]></desc>
			<sql><![CDATA[
SELECT DECODE(@[inv_src_no], 'TPB', NVL(N3PTY_BIL_AR_INV_FLG ,'N')
                           , 'DEM', NVL(DMDT_AR_INV_ISS_FLG,'N')
                           , 'DET', NVL(DMDT_AR_INV_ISS_FLG,'N')
                                  , 'N') ISSUE_FLAG
     , DECODE(DECODE(@[inv_src_no], 'TPB', NVL(N3PTY_BIL_AR_INV_FLG,'N')
                                  , 'DEM', NVL(DMDT_AR_INV_ISS_FLG,'N')
                                  , 'DET', NVL(DMDT_AR_INV_ISS_FLG,'N')
                                         , 'N'), 'N'
                                               , 'Y') REISSUE_FLAG
  FROM INV_AR_STUP_OFC
 WHERE AR_OFC_CD = @[ofc_cd]
   AND OTS_SMRY_CD IN ('CLR','INV')			]]></sql>
			<params>
				<param name="inv_src_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
