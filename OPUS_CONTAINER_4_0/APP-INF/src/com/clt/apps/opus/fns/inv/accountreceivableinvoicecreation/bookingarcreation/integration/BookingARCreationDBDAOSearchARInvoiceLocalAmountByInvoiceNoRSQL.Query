<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchARInvoiceLocalAmountByInvoiceNoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT SUM(INV_TTL_LOCL_AMT) INV_TTL_LOCL_AMT
  FROM INV_AR_MN 
 WHERE AR_IF_NO IN (SELECT DISTINCT MN.AR_IF_NO
                      FROM INV_AR_MN MN,
                           INV_AR_CHG CHG,
                           INV_AR_ISS_DTL DTL
                     WHERE MN.AR_IF_NO = CHG.AR_IF_NO
                       AND CHG.AR_IF_NO = DTL.AR_IF_NO
                       AND CHG.CHG_SEQ = DTL.CHG_SEQ
                       --AND MN.REV_TP_CD <> 'M' 2009-12-02 김현화 수석
                       AND NVL(MN.INV_DELT_DIV_CD,'N') <> 'Y'
                       --AND NVL(MN.INV_SPLIT_CD,'N') NOT IN ('M','X') 2010-01-18 이상희 과장
                       AND DTL.INV_NO = @[inv_no]
                       AND MN.AR_OFC_CD = @[ofc_cd]
                       )			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
