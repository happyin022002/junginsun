<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyIssueXchRtUSQL">
			<desc><![CDATA[ModifyIssueXchRt]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_CHG A
    SET ISS_XCH_RT = INV_XCH_RT
      , UPD_USR_ID = @[user_id]
      , UPD_DT = SYSDATE
  WHERE EXISTS (SELECT AR_IF_NO, AR_IF_SER_NO, CHG_SEQ
                  FROM INV_AR_ISS_DTL
                 WHERE INV_NO = @[inv_no]
                   AND AR_IF_NO = A.AR_IF_NO
                   AND CHG_SEQ = A.CHG_SEQ)			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
