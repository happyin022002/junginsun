<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationBackEndDBDAOModifyOTSHisINVUSDExrateUSQL">
			<desc><![CDATA[Modify OTS His INV USD Exrate]]></desc>
			<sql><![CDATA[
UPDATE SAR_OTS_HIS
   SET INV_USD_XCH_RT = DECODE(INV_CURR_CD, 'USD', 1, NVL(@[usd_xch_rt], 0)),
	   UPD_USR_ID = @[upd_usr_id],
	   UPD_DT = SYSDATE 
WHERE REF_NO = @[ar_if_no]
AND OTS_HIS_TP_CD = 'OTS'
AND INV_CURR_CD IS NOT NULL
-- AND (NVL(INV_LOCL_XCH_RT, 0) = 0 OR NVL(INV_USD_XCH_RT, 0) = 0)
AND (INV_NO = '**********' OR (INV_NO <> '**********' AND NVL(INV_USD_XCH_RT, 0) = 0))			]]></sql>
			<params>
				<param name="usd_xch_rt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
