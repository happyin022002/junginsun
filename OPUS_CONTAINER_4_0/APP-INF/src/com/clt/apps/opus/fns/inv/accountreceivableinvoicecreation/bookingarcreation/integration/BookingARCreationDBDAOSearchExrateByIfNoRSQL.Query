<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchExrateByIfNoRSQL">
			<desc><![CDATA[Search Exrate By If No]]></desc>
			<sql><![CDATA[
SELECT DISTINCT CURR_CD, INV_XCH_RT, USD_XCH_RT, INV_XCH_RT_DT
FROM INV_AR_CHG
WHERE AR_IF_NO = @[ar_if_no]			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
