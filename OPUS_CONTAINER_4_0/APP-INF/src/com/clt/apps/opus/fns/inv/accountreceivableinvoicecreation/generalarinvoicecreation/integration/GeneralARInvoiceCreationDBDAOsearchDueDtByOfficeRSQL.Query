<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOsearchDueDtByOfficeRSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOsearchDueDtByOfficeRSQL]]></desc>
			<sql><![CDATA[
SELECT 'N' CUST_CR_FLG 
     , DECODE(@[io_bnd_cd], 'I', NVL(IB_CR_TERM_DYS, 0), NVL(OB_CR_TERM_DYS, 0)) CR_TERM_DYS
  FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[ofc_cd]   			]]></sql>
			<params>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
