<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOInvIssChgUSQL">
			<desc><![CDATA[InvIssChg]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_CHG
   SET INV_ISS_FLG = 'Y'
	  ,INV_CLR_FLG = 'N'
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
 WHERE AR_IF_NO IN (SELECT DISTINCT AR_IF_NO 
                      FROM INV_AR_ISS_FTR
                     WHERE INV_ISS_WRK_NO = @[wrk_no])			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
