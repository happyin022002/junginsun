<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOSearchCntrNextMaxLineSeqRSQL">
			<desc><![CDATA[Max Line Seq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(LINE_SEQ) + 1, 1) AS LINE_SEQ
  FROM JOO_COM_PPT
 WHERE PPT_CD = @[ppt_cd]			]]></sql>
			<params>
				<param name="ppt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
