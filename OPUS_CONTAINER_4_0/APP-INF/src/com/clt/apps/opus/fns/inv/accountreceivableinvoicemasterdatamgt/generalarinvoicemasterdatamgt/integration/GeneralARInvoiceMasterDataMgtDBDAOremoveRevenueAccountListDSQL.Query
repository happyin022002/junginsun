<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceMasterDataMgtDBDAOremoveRevenueAccountListDSQL">
			<desc><![CDATA[GeneralARInvoiceMasterDataMgtDBDAOremoveRevenueAccountListDSQL]]></desc>
			<sql><![CDATA[
DELETE FROM INV_REV_ACCT_CD
WHERE INV_SRC_CD = @[inv_src_cd]
AND REV_TP_GRP_CD = @[rev_tp_grp_cd]
AND REV_TP_SRC_CD = @[rev_tp_src_cd]
AND INV_ACCT_DIV_CD = @[inv_acct_div_cd]
AND CHG_CD = @[chg_cd]			]]></sql>
			<params>
				<param name="inv_src_cd" type="12" value="" out="N"/>
				<param name="rev_tp_grp_cd" type="12" value="" out="N"/>
				<param name="rev_tp_src_cd" type="12" value="" out="N"/>
				<param name="inv_acct_div_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
