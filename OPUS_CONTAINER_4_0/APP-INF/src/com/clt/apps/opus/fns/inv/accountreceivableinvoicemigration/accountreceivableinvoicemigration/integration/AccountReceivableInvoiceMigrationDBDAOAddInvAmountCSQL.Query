<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableInvoiceMigrationDBDAOAddInvAmountCSQL">
			<desc><![CDATA[Add Inv Amount]]></desc>
			<sql><![CDATA[
INSERT INTO OPUSADM_TMP.INV_AR_AMT(
		AR_IF_NO
	   ,AR_IF_SER_NO
	   ,TJ_SRC_NM
	   ,CURR_CD
	   ,INV_AMT
	   ,INV_ERP_IF_STS_CD
	   ,ERP_IF_DT
	   ,CRE_USR_ID
	   ,CRE_DT
	   ,UPD_USR_ID
	   ,UPD_DT
	   ,ERR_DESC
	   ,AR_INV_SRC_CD
	   ,INV_COA_CO_CD
	   ,INV_COA_RGN_CD
	   ,INV_COA_CTR_CD
	   ,INV_COA_ACCT_CD
	   ,INV_COA_INTER_CO_CD
	   ,INV_COA_VSL_CD
	   ,INV_COA_VOY_NO
	   ,INV_COA_SKD_DIR_CD
	   ,INV_COA_REV_DIR_CD
	   ,ERP_IF_GL_DT
	   ,ERP_IF_OFC_CD
)VALUES(
@[ar_if_no]
,@[ar_if_ser_no]
,@[tj_src_nm]
,SUBSTR(@[curr_cd], 1, 3)
,NVL(@[inv_amt], 0)
,'Y'
,TO_CHAR(SYSDATE, 'YYYYMMDD')
,@[cre_usr_id]
,SYSDATE
,@[upd_usr_id]
,SYSDATE
,@[err_desc]
,@[ar_inv_src_cd]
,@[inv_coa_co_cd]
,@[inv_coa_rgn_cd]
,@[inv_coa_ctr_cd]
,@[inv_coa_acct_cd]
,@[inv_coa_inter_co_cd]
,@[inv_coa_vsl_cd]
,@[inv_coa_voy_no]
,@[inv_coa_skd_dir_cd]
,@[inv_coa_rev_dir_cd]
,@[erp_if_gl_dt]
,@[erp_if_ofc_cd]
)			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="ar_if_ser_no" type="12" value="" out="N"/>
				<param name="tj_src_nm" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="err_desc" type="12" value="" out="N"/>
				<param name="ar_inv_src_cd" type="12" value="" out="N"/>
				<param name="inv_coa_co_cd" type="12" value="" out="N"/>
				<param name="inv_coa_rgn_cd" type="12" value="" out="N"/>
				<param name="inv_coa_ctr_cd" type="12" value="" out="N"/>
				<param name="inv_coa_acct_cd" type="12" value="" out="N"/>
				<param name="inv_coa_inter_co_cd" type="12" value="" out="N"/>
				<param name="inv_coa_vsl_cd" type="12" value="" out="N"/>
				<param name="inv_coa_voy_no" type="12" value="" out="N"/>
				<param name="inv_coa_skd_dir_cd" type="12" value="" out="N"/>
				<param name="inv_coa_rev_dir_cd" type="12" value="" out="N"/>
				<param name="erp_if_gl_dt" type="12" value="" out="N"/>
				<param name="erp_if_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
