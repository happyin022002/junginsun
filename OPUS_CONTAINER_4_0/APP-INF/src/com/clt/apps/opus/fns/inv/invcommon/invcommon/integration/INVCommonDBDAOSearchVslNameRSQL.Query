<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOSearchVslNameRSQL">
			<desc><![CDATA[SearchVslName]]></desc>
			<sql><![CDATA[
SELECT A.VSL_ENG_NM
FROM   MDM_VSL_CNTR A, VSK_VSL_SKD B
WHERE  A.VSL_CD = B.VSL_CD 
AND    B.VSL_CD || B.SKD_VOY_NO || B.SKD_DIR_CD = @[vvd]
AND    A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
