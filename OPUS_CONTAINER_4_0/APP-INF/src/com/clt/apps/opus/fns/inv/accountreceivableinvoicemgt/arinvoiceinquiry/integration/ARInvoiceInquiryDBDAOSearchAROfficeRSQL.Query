<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOSearchAROfficeRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.AR_OFC_CD,
  NVL(B.INV_DUP_FLG, 'N') INV_DUP_FLG
FROM MDM_ORGANIZATION A,
  INV_AR_STUP_OFC B
WHERE A.AR_OFC_CD = B.AR_OFC_CD(+)
  AND A.OFC_CD = @[ofc_cd]
  AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
