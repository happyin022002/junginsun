     , ATTR_CTNT9 
     , ATTR_CTNT10 
     , CRE_USR_ID 
     , CRE_DT 
     , UPD_USR_ID 
     , UPD_DT )
VALUES ( 
       @[ppt_cd]
     , @[line_seq]
     , @[src_tpsz] --ATTR_CTNT1
     , @[tgt_normal_tpsz] --ATTR_CTNT2
     , @[tgt_rad_tpsz] --ATTR_CTNT3
     , @[tgt_empty_tpsz] --ATTR_CTNT4
     , NULL --ATTR_CTNT5
     , NULL --ATTR_CTNT6
     , NULL --ATTR_CTNT7
     , NULL --ATTR_CTNT8
     , NULL --ATTR_CTNT9
     , NULL --ATTR_CTNT10
     , @[cre_usr_id]
     , SYSDATE
     , @[cre_usr_id]
     , SYSDATE )			]]></sql>
