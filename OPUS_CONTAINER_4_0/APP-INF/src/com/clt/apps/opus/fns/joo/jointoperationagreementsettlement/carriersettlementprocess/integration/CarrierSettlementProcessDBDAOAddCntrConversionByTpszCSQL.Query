				<param name="ppt_cd" type="12" value="" out="N"/>
				<param name="line_seq" type="12" value="" out="N"/>
				<param name="src_tpsz" type="12" value="" out="N"/>
				<param name="tgt_normal_tpsz" type="12" value="" out="N"/>
				<param name="tgt_rad_tpsz" type="12" value="" out="N"/>
