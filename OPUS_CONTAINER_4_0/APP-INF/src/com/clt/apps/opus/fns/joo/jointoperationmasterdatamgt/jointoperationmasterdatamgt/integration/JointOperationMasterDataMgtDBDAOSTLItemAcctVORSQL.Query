<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOSTLItemAcctVORSQL">
			<desc><![CDATA[정산ITEM과 Acount code list 조회]]></desc>
			<sql><![CDATA[
SELECT
       A.JO_STL_ITM_CD
      ,A.JO_STL_ITM_NM
      ,DECODE(A.JO_AUTO_CRE_FLG,'Y','1','0') AS JO_AUTO_CRE_FLG
      ,DECODE(A.JO_MNL_CRE_FLG,'Y','1','0') AS JO_MNL_CRE_FLG
      ,MAX(DECODE(B.RE_DIVR_CD,'E',B.DR_ACCT_CD,'')) E_DR_ACCT_CD
      ,MAX(DECODE(B.RE_DIVR_CD,'E',B.CR_ACCT_CD,'')) E_CR_ACCT_CD
      ,MAX(DECODE(B.RE_DIVR_CD,'E',B.JO_ESTM_ACCT_CD,'')) E_ES_ACCT_CD
      ,MAX(DECODE(B.RE_DIVR_CD,'R',B.DR_ACCT_CD,'')) R_DR_ACCT_CD
      ,MAX(DECODE(B.RE_DIVR_CD,'R',B.CR_ACCT_CD,'')) R_CR_ACCT_CD
      ,MAX(DECODE(B.RE_DIVR_CD,'R',B.JO_ESTM_ACCT_CD,'')) R_ES_ACCT_CD
      ,'' AS USR_ID
      ,'' AS RE_DIVR_CD
      ,A.ORD_SEQ 
FROM  JOO_STL_ITM      A,
      JOO_STL_ITM_ACCT B
WHERE A.JO_STL_ITM_CD = B.JO_STL_ITM_CD
GROUP BY
       A.ORD_SEQ
      ,A.JO_STL_ITM_CD
      ,A.JO_STL_ITM_NM
      ,A.JO_AUTO_CRE_FLG
      ,A.JO_MNL_CRE_FLG
ORDER BY
      A.ORD_SEQ, A.JO_STL_ITM_CD ASC, 5, 3 DESC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
