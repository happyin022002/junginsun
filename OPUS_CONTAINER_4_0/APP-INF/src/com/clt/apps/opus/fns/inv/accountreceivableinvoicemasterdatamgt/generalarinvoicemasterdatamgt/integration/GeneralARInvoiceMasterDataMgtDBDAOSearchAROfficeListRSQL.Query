<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceMasterDataMgtDBDAOSearchAROfficeListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT A.OFC_CD AR_OFC_CD
	 , @[ofc_cd] AR_OFC_CD_LOGIN
	 , DECODE(A.DELT_FLG,'Y','X','') DELT_FLG
FROM MDM_ORGANIZATION A
   , COM_SYS_AREA_GRP_ID B
   , (
       SELECT C.SYS_AREA_GRP_ID 
         FROM COM_SYS_AREA_GRP_ID C, MDM_LOCATION D, MDM_ORGANIZATION E
        WHERE E.OFC_CD = @[ofc_cd]
          AND E.LOC_CD = D.LOC_CD
          AND C.CNT_CD = D.CNT_CD
          AND C.CO_IND_CD = 'H'
     ) C
WHERE SUBSTR(A.LOC_CD,1,2) = B.CNT_CD
AND B.SYS_AREA_GRP_ID = C.SYS_AREA_GRP_ID
AND A.AR_OFC_CD IS NOT NULL
AND A.AR_HD_QTR_OFC_CD IS NOT NULL
ORDER BY A.OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
