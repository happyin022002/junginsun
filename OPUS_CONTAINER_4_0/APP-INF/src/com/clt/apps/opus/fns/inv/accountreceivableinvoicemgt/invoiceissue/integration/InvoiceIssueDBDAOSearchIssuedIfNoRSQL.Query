<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOSearchIssuedIfNoRSQL">
			<desc><![CDATA[Search Issued If No]]></desc>
			<sql><![CDATA[
SELECT AR_IF_NO
FROM INV_AR_MN
WHERE  AR_IF_NO IN (SELECT DISTINCT AR_IF_NO 
                    FROM INV_AR_ISS_FTR
                    WHERE INV_ISS_WRK_NO = @[wrk_no])
AND INV_CLR_FLG = 'Y'
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
