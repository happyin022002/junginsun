<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchFincCtrlOfficeRSQL">
			<desc><![CDATA[Search Finc Ctrl Office]]></desc>
			<sql><![CDATA[
SELECT B.AR_OFC_CD FINC_CTRL_OFC_CD
FROM MDM_LOCATION A,
     MDM_ORGANIZATION B
WHERE A.FINC_CTRL_OFC_CD = B.OFC_CD
AND A.LOC_CD = DECODE(SUBSTR(@[bnd],1,1), 'O', @[pol], @[pod])			]]></sql>
			<params>
				<param name="bnd" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
