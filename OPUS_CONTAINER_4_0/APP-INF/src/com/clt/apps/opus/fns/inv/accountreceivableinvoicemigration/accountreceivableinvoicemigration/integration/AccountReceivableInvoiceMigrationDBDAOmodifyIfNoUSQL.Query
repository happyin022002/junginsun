<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableInvoiceMigrationDBDAOmodifyIfNoUSQL">
			<desc><![CDATA[AccountReceivableInvoiceMigrationDBDAOmodifyIfNoUSQL]]></desc>
			<sql><![CDATA[
UPDATE MIGADM.MIG_INV_AR_IF_MN 
   SET AR_IF_NO = @[ar_if_no]
     , BL_INV_IF_FLG = 'Y'
     --, BL_INV_IF_DT = TO_CHAR(SYSDATE, 'YYYYMMDD')
     , BL_INV_IF_DT = TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]),'YYYYMMDD')
     , UPD_USR_ID = @[user_id]
	 , UPD_DT = SYSDATE
 WHERE SRC_IF_DT = @[src_if_dt]
   AND SRC_IF_SEQ = @[src_if_seq]			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="src_if_dt" type="12" value="" out="N"/>
				<param name="src_if_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
