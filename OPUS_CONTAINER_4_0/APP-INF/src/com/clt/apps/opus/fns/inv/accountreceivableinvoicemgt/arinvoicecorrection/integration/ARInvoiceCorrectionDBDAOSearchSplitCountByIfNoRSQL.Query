<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchSplitCountByIfNoRSQL">
			<desc><![CDATA[Search Split Count By If No]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM INV_AR_MN
WHERE (AR_OFC_CD, BL_SRC_NO) IN (SELECT AR_OFC_CD, BL_SRC_NO
                                 FROM INV_AR_MN
                                 WHERE AR_IF_NO = @[ar_if_no])
AND REV_TP_CD <> 'M'
AND INV_DELT_DIV_CD <> 'Y'
AND BL_INV_CFM_DT IS NOT NULL
AND NVL(INV_SPLIT_CD, 'N') = 'S'			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
