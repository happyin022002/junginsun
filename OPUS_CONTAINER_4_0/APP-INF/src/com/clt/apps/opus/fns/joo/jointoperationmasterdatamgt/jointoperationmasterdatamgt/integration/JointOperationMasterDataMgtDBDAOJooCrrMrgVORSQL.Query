<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOJooCrrMrgVORSQL">
			<desc><![CDATA[선사Merge 조회]]></desc>
			<sql><![CDATA[
SELECT 
	rlane_cd
,   acctg_crr_cd
,	jo_n1st_crr_cd
,	jo_n2nd_crr_cd
,	trd_cd
,	vsl_cd
,	skd_voy_no
,	skd_dir_cd
,	rev_dir_cd
,   to_char(eff_eta_dt,'yyyymmdd') as eff_eta_dt
,	delt_flg
,	mrg_rmk
,	cre_dt
,	cre_usr_id
,	upd_dt
,	upd_usr_id
FROM joo_crr_mrg
WHERE	1 = 1
#if (${trd_cd} != '') 
AND	trd_cd = @[trd_cd]
#end
#if (${rlane_cd} != '') 
AND rlane_cd = @[rlane_cd]
#end
#if (${jo_crr_cd} != '') 
AND acctg_crr_cd = @[jo_crr_cd]
#end
#if (${delt_flg} != 'A')
AND DELT_FLG = @[delt_flg]
#end
ORDER BY rlane_cd, acctg_crr_cd, jo_n1st_crr_cd, jo_n2nd_crr_cd			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
