<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOsearchTDRCreateListByLaneRSQL">
			<desc><![CDATA[TDR Creation Inquiry 조회
[2015.07.21]Virtual Add Calling 처리. VSK_VSL_PORT_SKD.NVL(VT_ADD_CALL_FLG, 'N') = 'N']]></desc>
			<sql><![CDATA[
SELECT V.SLAN_CD
     , V.VSL_CD
     , V.SKD_VOY_NO
     , V.SKD_DIR_CD
     , V.CLPT_SEQ
     , V.VPS_PORT_CD
     , TO_CHAR(V.VPS_ETD_DT,'YYYYMMDDHH24MI') AS VPS_ETD_DT
     , (SELECT CO.INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL CO WHERE CO.INTG_CD_ID = 'CD01825' AND CO.INTG_CD_VAL_CTNT = V.SKD_CNG_STS_CD) AS SKD_CNG_STS_CD
     , TO_CHAR(T.UPDATE_TIME,'YYYYMMDDHH24MI') AS UPDATE_TIME
     , DECODE(U.VSL_CD, NULL, 'No', 'Yes') SLOT_USED
     , '' FROM_DT
     , '' TO_DT

     ,NVL2( T.VSL_CD, 'Y','N')pagerows

  FROM VSK_VSL_PORT_SKD V, TDR_HEADER T , TDR_UTILIZE U
 WHERE V.VSL_CD       = T.VSL_CD(+)
   AND V.SKD_VOY_NO   = T.VOY_NO(+) 
   AND V.SKD_DIR_CD   = T.DIR_CD(+)
   AND V.VPS_PORT_CD  = T.PORT_CD(+)
   AND V.CLPT_IND_SEQ = T.CALL_IND(+)
   AND V.VSL_CD       = U.VSL_CD(+)
   AND V.SKD_VOY_NO   = U.VOY_NO(+)
   AND V.SKD_DIR_CD   = U.DIR_CD(+)
   AND V.VPS_PORT_CD  = U.PORT_CD(+)
   AND V.CLPT_IND_SEQ = U.CALL_IND(+)
   AND V.SLAN_CD LIKE @[lane]||'%'
   AND V.VPS_ETA_DT BETWEEN TO_DATE(@[fromDt], 'YYYYMMDD') AND TO_DATE(@[toDt], 'YYYYMMDD') + 0.99999
   AND V.TURN_PORT_IND_CD NOT IN ('D', 'V', 'F')
   AND NVL(V.VT_ADD_CALL_FLG, 'N') = 'N' /*2015.07.21 Add*/
GROUP BY V.SLAN_CD, V.VSL_CD, V.SKD_VOY_NO, V.SKD_DIR_CD, V.CLPT_SEQ, V.VPS_PORT_CD, V.VPS_ETD_DT, V.SKD_CNG_STS_CD, 
         DECODE(U.VSL_CD(+), NULL, 'No', 'Yes'), V.CLPT_IND_SEQ,  T.VSL_CD, T.UPDATE_TIME
ORDER BY V.SLAN_CD
     , V.VSL_CD
     , V.SKD_VOY_NO
     , V.SKD_DIR_CD			]]></sql>
			<params>
				<param name="lane" type="12" value="" out="N"/>
				<param name="fromDt" type="12" value="" out="N"/>
				<param name="toDt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
