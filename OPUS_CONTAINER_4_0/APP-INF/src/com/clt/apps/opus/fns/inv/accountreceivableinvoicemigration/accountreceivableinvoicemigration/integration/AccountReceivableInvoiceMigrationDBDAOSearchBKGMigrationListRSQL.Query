<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableInvoiceMigrationDBDAOSearchBKGMigrationListRSQL">
			<desc><![CDATA[Search BKG Migration List]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
       , BKG_SEQ
	   , CTRT_OFC_CD BKG_OFC_CD
       , CRE_USR_ID USER_ID
FROM MIGADM.MIG_INV_BKG_IF_MN
WHERE INV_AR_IF_CD = 'N'
#if (${fm_bkg_ofc_cd} != '')
    AND CTRT_OFC_CD >= @[fm_bkg_ofc_cd]
#end
#if (${to_bkg_ofc_cd} != '')
    AND CTRT_OFC_CD <= @[to_bkg_ofc_cd]
#end
#if (${bkg_no} != '')
    AND BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="fm_bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="to_bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
