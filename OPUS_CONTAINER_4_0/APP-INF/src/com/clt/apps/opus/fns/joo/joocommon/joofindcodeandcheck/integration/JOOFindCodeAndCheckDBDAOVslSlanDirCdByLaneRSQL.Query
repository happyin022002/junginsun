<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOVslSlanDirCdByLaneRSQL">
			<desc><![CDATA[MDM_VSL_SVC_LANE_DIR Search]]></desc>
			<sql><![CDATA[
SELECT VSL_SLAN_DIR_CD AS CODE
  FROM MDM_VSL_SVC_LANE_DIR M
 WHERE M.VSL_SLAN_CD = SUBSTR(@[super_cd1],1,3)
 ORDER BY M.VSL_SLAN_DIR_SEQ DESC			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="LP7AE" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
