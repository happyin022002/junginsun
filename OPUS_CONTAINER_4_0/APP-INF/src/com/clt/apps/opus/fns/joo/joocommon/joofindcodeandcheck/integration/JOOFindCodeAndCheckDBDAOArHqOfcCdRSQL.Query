<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOArHqOfcCdRSQL">
			<desc><![CDATA[Login한 사용자의 HQ 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
       OFC_CD AS NAME,
       AR_HD_QTR_OFC_CD AS CODE
FROM   MDM_ORGANIZATION
WHERE  OFC_CD = @[code]			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
