<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchInvoiceChargeListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT CHG_CD,
       CURR_CD,
       PER_TP_CD,
       TRF_RT_AMT,
       RAT_AS_CNTR_QTY,
       CHG_AMT,
       INV_XCH_RT,
       TRF_NO,
	   DE_TERM_CD,
	   AUTO_RAT_CD,
	   WHF_FLG
  FROM INV_BKG_IF_CHG
 WHERE BKG_NO = @[bkg_no]
   AND BKG_SEQ = @[bkg_seq]
   AND OFC_CD IN ( SELECT OFC_CD FROM MDM_ORGANIZATION
                       WHERE AR_OFC_CD = ( SELECT AR_OFC_CD 
                         FROM MDM_ORGANIZATION
                        WHERE OFC_CD = @[ofc_cd] ))
#if (${svr_id} != 'USA') 
   AND NVL(CHG_AMT,0) <> 0 
#end
 ORDER BY CURR_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
