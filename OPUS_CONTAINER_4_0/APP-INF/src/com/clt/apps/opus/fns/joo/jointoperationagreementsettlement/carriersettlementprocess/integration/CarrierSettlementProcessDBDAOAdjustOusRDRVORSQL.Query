<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOAdjustOusRDRVORSQL">
			<desc><![CDATA[RDR Settlement Adjust]]></desc>
			<sql><![CDATA[
WITH JO_CRR AS (
  SELECT B.JO_CRR_CD, A.VSL_CD, A.SKD_VOY_NO, A.SKD_DIR_CD
    FROM JOO_STL_VVD A,
         (
    		 SELECT DECODE(@[re_divr_cd], 'E', COM_CONSTANTMGR_PKG.COM_getCompanyCode_FNC, @[jo_crr_cd]) AS JO_CRR_CD, TO_DATE('99991231','YYYYMMDD') AS EFF_ETA_DT
    		 FROM   DUAL
    		 UNION  ALL
    		 SELECT JO_N2ND_CRR_CD AS JO_CRR_CD, EFF_ETA_DT
    		 FROM   JOO_CRR_MRG A
    		 WHERE  A.DELT_FLG       = 'N'
    		 AND    A.ACCTG_CRR_CD   = @[jo_crr_cd]
    		 AND    A.JO_N1ST_CRR_CD = DECODE(@[re_divr_cd], 'E', COM_CONSTANTMGR_PKG.COM_getCompanyCode_FNC, @[jo_crr_cd])
    		 AND    A.TRD_CD         = @[trd_cd]
    		 AND    A.RLANE_CD       = @[rlane_cd]
         ) B
   WHERE A.BZC_PORT_ETA_DT <= B.EFF_ETA_DT
     AND A.ACCT_YRMON   >= REPLACE(@[fm_acct_yrmon],'-','')
     AND A.ACCT_YRMON   <= REPLACE(@[to_acct_yrmon],'-','')
     AND A.JO_CRR_CD     = @[jo_crr_cd]
     AND A.TRD_CD        = @[trd_cd]
     AND A.RLANE_CD      = @[rlane_cd]
     AND A.RE_DIVR_CD    = @[re_divr_cd]
     AND A.JO_STL_ITM_CD = 'OUS'
     AND A.JO_MNU_NM    IN ('RDR','TDR')
     AND A.JO_STL_CFM_CD = 'Y'
)
SELECT 
       VSL_CD, SKD_VOY_NO, SKD_DIR_CD, REV_DIR_CD, UC_BSS_PORT_CD, IOC_CD, SCONTI_CD, FM_PORT_CD, FM_PORT_ETD_DT, JO_STL_ITM_CD, LOCL_CURR_CD, STL_CMB_SEQ,
       COA_TEU, COA_WGT, FNL_OWN_BSA_QTY, FNL_BSA_WGT, DIFF_TEU, DIFF_WGT, USD_SLT_BSA_QTY, USD_SLT_WGT, RDR_TEU, RDR_WGT, BSA_QTY, UC_AMT, BSA_SLT_PRC
FROM   (
SELECT
       J.VSL_CD, J.SKD_VOY_NO, J.SKD_DIR_CD, J.REV_DIR_CD, J.UC_BSS_PORT_CD, J.IOC_CD, J.SCONTI_CD, J.FM_PORT_CD, J.FM_PORT_ETD_DT, J.JO_STL_ITM_CD, J.LOCL_CURR_CD, J.STL_CMB_SEQ,
       NVL(A.TEU,0) AS COA_TEU, NVL(A.WGT,0) AS COA_WGT, J.FNL_OWN_BSA_QTY, J.FNL_BSA_WGT, NVL(A.TEU,0) - J.FNL_OWN_BSA_QTY AS DIFF_TEU, NVL(A.WGT,0) - J.FNL_BSA_WGT AS DIFF_WGT,
       J.USD_SLT_BSA_QTY, J.USD_SLT_WGT, NVL(R.RDR_TEU,0) AS RDR_TEU, NVL(R.RDR_WGT,0) AS RDR_WGT, J.BSA_QTY, NVL(C.UC_AMT,0) AS UC_AMT, J.BSA_SLT_PRC
FROM   (
       SELECT 
              J.ACCT_YRMON, J.STL_VVD_SEQ, J.STL_SEQ, L.STL_CMB_SEQ,
              J.VSL_CD, J.SKD_VOY_NO, J.SKD_DIR_CD, J.REV_DIR_CD, J.UC_BSS_PORT_CD, J.IOC_CD, J.SCONTI_CD, J.FM_PORT_CD, V.FM_PORT_ETD_DT,
              J.JO_STL_ITM_CD, J.FNL_OWN_BSA_QTY, J.FNL_BSA_WGT, J.USD_SLT_BSA_QTY, J.USD_SLT_WGT, J.BSA_QTY, J.BSA_SLT_PRC, J.LOCL_CURR_CD
       FROM   JOO_SETTLEMENT  J,
              JOO_STL_CMB_DTL K,
              JOO_STL_CMB     L,
              (
              SELECT X.VSL_CD, X.SKD_VOY_NO, X.SKD_DIR_CD, X.VPS_PORT_CD, X.SLAN_CD, 
                     MIN(TO_CHAR(X.VPS_ETD_DT, 'YYYYMMDDHH24MISS')) AS FM_PORT_ETD_DT
              FROM   VSK_VSL_PORT_SKD X
              GROUP  BY X.VSL_CD, X.SKD_VOY_NO, X.SKD_DIR_CD, X.VPS_PORT_CD, X.SLAN_CD
              ) V
       WHERE  J.ACCT_YRMON   >= REPLACE(@[fm_acct_yrmon],'-','')
       AND    J.ACCT_YRMON   <= REPLACE(@[to_acct_yrmon],'-','')
       AND    J.RE_DIVR_CD    = @[re_divr_cd]
       AND    J.JO_CRR_CD     = @[jo_crr_cd]
       AND    J.TRD_CD        = @[trd_cd]
       AND    J.RLANE_CD      = @[rlane_cd]
       AND    J.JO_STL_ITM_CD = 'OUS'
       AND    J.JO_MNU_NM     IN ('RDR', 'M/S')
       AND   (CASE WHEN J.JO_MNU_NM = 'M/S' 
                   THEN ( SELECT JO_STL_TGT_TP_CD 
                            FROM JOO_STL_BSS_PORT P 
                           WHERE P.JO_CRR_CD     = J.JO_CRR_CD 
                             AND P.RLANE_CD      = J.RLANE_CD
                             AND P.SKD_DIR_CD    = J.SKD_DIR_CD 
                             AND P.JO_STL_ITM_CD = J.JO_STL_ITM_CD)
                   ELSE 'R' END) = 'R'
       AND    J.IOC_CD        = 'O'
       AND    J.ACCT_YRMON    = K.ACCT_YRMON 
       AND    J.STL_VVD_SEQ   = K.STL_VVD_SEQ
       AND    J.STL_SEQ       = K.STL_SEQ    
       AND    K.ACCT_YRMON    = L.ACCT_YRMON
       AND    K.JO_CRR_CD     = L.JO_CRR_CD
       AND    K.STL_CMB_SEQ   = L.STL_CMB_SEQ
       AND    K.RE_DIVR_CD    = L.RE_DIVR_CD
       AND    J.STL_LST_FLG   = 'Y'
       AND    J.CMB_CFM_FLG   = 'Y'
       AND    L.SLP_SER_NO    IS NOT NULL
       AND    J.VSL_CD        = V.VSL_CD     (+)
       AND    J.SKD_VOY_NO    = V.SKD_VOY_NO (+)
       AND    J.SKD_DIR_CD    = V.SKD_DIR_CD (+)
       AND    J.FM_PORT_CD    = V.VPS_PORT_CD(+)
       AND    SUBSTR(J.RLANE_CD,1,3) = V.SLAN_CD(+)       
       ) J, 
       (
       SELECT  H.VSL_CD, H.VOY_NO, H.DIR_CD, H.REGION, H.PORT_CD, 
               SUM(A.BSA_SLOT) AS TEU, 
               SUM(A.BSA_WGT ) AS WGT
         FROM  RDR_HEADER H, 
               (
               SELECT VSL_CD, VOY_NO, DIR_CD, REGION, BSA_SLOT, BSA_WGT
                 FROM RDR_ALLOCATION
                WHERE (OPR_CD, VSL_CD, VOY_NO, DIR_CD) IN (SELECT JO_CRR_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD FROM JO_CRR)
               ) A
        WHERE  H.VSL_CD  = A.VSL_CD(+)
          AND  H.VOY_NO  = A.VOY_NO(+)
          AND  H.DIR_CD  = A.DIR_CD(+)
          AND  H.REGION  = A.REGION(+)
        GROUP  BY H.VSL_CD, H.VOY_NO, H.DIR_CD, H.REGION, H.PORT_CD
       ) A,
       (
       SELECT
              CP.DIR_CD,
              CP.UC_AMT
       FROM   BSA_OCN_OVR_SLT_PRC CP,
              BSA_OCN_OVR_MST     CM
       WHERE CM.BSA_SLT_COST_TP_CD  = CP.BSA_SLT_COST_TP_CD
       AND   CM.OVR_USD_SLT_PRC_SEQ = CP.OVR_USD_SLT_PRC_SEQ
       AND   CM.TRD_CD              = CP.TRD_CD
       AND   CM.RLANE_CD            = CP.RLANE_CD
       AND   CM.DIR_CD              = CP.DIR_CD
       AND   CP.BSA_SLT_COST_TP_CD  = '020'
       AND   CP.CRR_CD              = @[jo_crr_cd]
       AND   CP.TRD_CD              = @[trd_cd]
       AND   CP.RLANE_CD            = @[rlane_cd]
       AND   CM.DELT_FLG            = 'N'
       AND   CP.OVR_USD_SLT_PRC_SEQ = (
                SELECT MAX(Y.OVR_USD_SLT_PRC_SEQ)
                FROM   BSA_OCN_OVR_SLT_PRC Y,
                       BSA_OCN_OVR_MST     X
                WHERE  X.BSA_SLT_COST_TP_CD  = Y.BSA_SLT_COST_TP_CD
                AND    X.OVR_USD_SLT_PRC_SEQ = Y.OVR_USD_SLT_PRC_SEQ
                AND    X.TRD_CD              = Y.TRD_CD
                AND    X.RLANE_CD            = Y.RLANE_CD
                AND    X.DIR_CD              = Y.DIR_CD
                AND    X.BSA_SLT_COST_TP_CD  = CP.BSA_SLT_COST_TP_CD
                AND    Y.CRR_CD              = CP.CRR_CD
                AND    X.TRD_CD              = CP.TRD_CD
                AND    X.RLANE_CD            = CP.RLANE_CD
                AND    X.DIR_CD              = CP.DIR_CD
                AND    X.DELT_FLG            ='N'
             )
       ) C,              
       (
       SELECT
              U.VSL_CD, U.VOY_NO AS SKD_VOY_NO, H.PORT_CD, U.DIR_CD AS SKD_DIR_CD, H.REGION,
              SUM(U.SLOT_QTY) AS RDR_TEU, SUM(U.WEIGHT) AS RDR_WGT
       FROM   RDR_HEADER H, 
               (
               SELECT VSL_CD, VOY_NO, DIR_CD, REGION, SLOT_QTY, WEIGHT
                 FROM RDR_UTILIZE U
                WHERE (U.VSL_CD, U.VOY_NO, U.DIR_CD, U.OPR_CD) IN (SELECT VSL_CD, SKD_VOY_NO, SKD_DIR_CD, JO_CRR_CD FROM JO_CRR)
                  AND TYPE <> 'R' /*Reefer 제외.*/
               ) U
       WHERE  H.VSL_CD  = U.VSL_CD(+)
       AND    H.VOY_NO  = U.VOY_NO(+)
       AND    H.DIR_CD  = U.DIR_CD(+)
       AND    H.REGION  = U.REGION(+)
       GROUP  BY U.VSL_CD, U.VOY_NO, H.PORT_CD, U.DIR_CD, H.REGION
       ) R
WHERE  1 = 1   
AND    J.VSL_CD        = A.VSL_CD     (+)
AND    J.SKD_VOY_NO    = A.VOY_NO     (+)
AND    J.SKD_DIR_CD    = A.DIR_CD     (+)
AND    J.SCONTI_CD     = A.REGION     (+)
AND    J.FM_PORT_CD    = A.PORT_CD    (+)
AND    J.SKD_DIR_CD    = C.DIR_CD     (+)    
AND    J.VSL_CD        = R.VSL_CD     (+)
AND    J.SKD_VOY_NO    = R.SKD_VOY_NO (+)
AND    J.SKD_DIR_CD    = R.SKD_DIR_CD (+)
AND    J.FM_PORT_CD    = R.PORT_CD    (+)
AND    J.SCONTI_CD     = R.REGION     (+)
)
WHERE  DIFF_TEU <> 0
OR     DIFF_WGT <> 0
OR     USD_SLT_BSA_QTY <> RDR_TEU
OR     USD_SLT_WGT     <> RDR_WGT
OR     UC_AMT          <> BSA_SLT_PRC
ORDER  BY VSL_CD, SKD_VOY_NO, SKD_DIR_CD, REV_DIR_CD			]]></sql>
			<params>
				<param name="re_divr_cd" type="12" value="E" out="N"/>
				<param name="jo_crr_cd" type="12" value="EMC" out="N"/>
				<param name="trd_cd" type="12" value="TAS" out="N"/>
				<param name="rlane_cd" type="12" value="NTATA" out="N"/>
				<param name="fm_acct_yrmon" type="12" value="200901" out="N"/>
				<param name="to_acct_yrmon" type="12" value="200910" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
