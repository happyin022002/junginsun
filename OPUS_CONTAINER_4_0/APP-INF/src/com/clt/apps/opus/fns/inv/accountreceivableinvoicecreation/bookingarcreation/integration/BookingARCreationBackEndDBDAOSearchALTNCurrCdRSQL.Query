<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationBackEndDBDAOSearchALTNCurrCdRSQL">
			<desc><![CDATA[Search ALTN Curr Cd]]></desc>
			<sql><![CDATA[
SELECT NVL(ALTN_CURR_DIV_CD, 'LCL') ALTN_CD
FROM MDM_ORGANIZATION
WHERE OFC_CD = @[ofc_cd]			]]></sql>
