<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationAccrualCreationDBDAOJooEstmClzVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE JOO_ESTM_CLZ SET 
	ESTM_CLZ_YR = @[estm_clz_yr]
,	ESTM_CLZ_MON = @[estm_clz_mon]
,	ESTM_CLZ_FLG = DECODE(@[estm_clz_flg], '0', 'N', '1', 'Y')
,	ESTM_CLZ_DT = TO_DATE(to_char(sysdate, 'YYYY-MM-DD HH24:MI:SS'),  'YYYY-MM-DD HH24:MI:SS')
,	ESTM_RMK = @[estm_rmk]
,	UPD_DT = TO_DATE(sysdate,'YYYY-MM-DD')
,	UPD_USR_ID = @[upd_usr_id]
WHERE	ESTM_CLZ_YR = @[estm_clz_yr]
AND	ESTM_CLZ_MON = @[estm_clz_mon]			]]></sql>
			<params>
				<param name="estm_clz_yr" type="12" value="" out="N"/>
				<param name="estm_clz_mon" type="12" value="" out="N"/>
				<param name="estm_clz_flg" type="12" value="" out="N"/>
				<param name="estm_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
