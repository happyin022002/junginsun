<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchLocationInfoRSQL">
			<desc><![CDATA[Search Location Info]]></desc>
			<sql><![CDATA[
SELECT LOC_NM
       , DECODE(LENGTH(LOC_AMS_PORT_CD), 4, 'D', 5, 'K') QUALIFIER
       , LOC_AMS_PORT_CD
       , STE_CD
FROM MDM_LOCATION
WHERE LOC_CD = @[loc_cd] 			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
