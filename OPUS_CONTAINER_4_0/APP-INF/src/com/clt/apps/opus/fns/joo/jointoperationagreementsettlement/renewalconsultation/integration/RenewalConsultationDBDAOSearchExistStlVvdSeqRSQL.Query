<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalConsultationDBDAOSearchExistStlVvdSeqRSQL">
			<desc><![CDATA[기등록된 STL_VVD_SEQ를 조회.]]></desc>
			<sql><![CDATA[
SELECT STL_VVD_SEQ
  FROM JOO_STL_TGT ST
 WHERE 1=1
   AND ST.VSL_CD        = @[vsl_cd]
   AND ST.SKD_DIR_CD    = @[skd_dir_cd]
   AND ST.REV_DIR_CD    = @[rev_dir_cd]
   AND ST.REV_YRMON     = @[rev_yrmon]
   AND ST.JO_CRR_CD     = @[jo_crr_cd]
   AND ST.RLANE_CD      = @[rlane_cd]
   AND ST.RE_DIVR_CD    = @[re_divr_cd]
   AND ST.JO_STL_ITM_CD = @[jo_stl_itm_cd]
   AND ST.JO_STL_JB_CD  = @[jo_stl_jb_cd]
   AND ST.ESTM_YRMON    = @[estm_yrmon]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="jo_stl_itm_cd" type="12" value="" out="N"/>
				<param name="jo_stl_jb_cd" type="12" value="" out="N"/>
				<param name="estm_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
