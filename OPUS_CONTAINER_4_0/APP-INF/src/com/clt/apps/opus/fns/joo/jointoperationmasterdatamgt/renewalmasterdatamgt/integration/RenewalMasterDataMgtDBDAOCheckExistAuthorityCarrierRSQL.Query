<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RenewalMasterDataMgtDBDAOCheckExistAuthorityCarrierRSQL">
			<desc><![CDATA[Authority Carrier 존재 체크]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN EXIST_CNT > 0 THEN 'Y' ELSE 'N' END AS EXIST_FLG
  FROM (
        SELECT COUNT(*) AS EXIST_CNT
          FROM JOO_CRR_AUTH A
         WHERE 1=1
           AND A.JO_CRR_CD = @[jo_crr_cd]
           AND A.RLANE_CD  = @[rlane_cd]
#if (${auth_ofc_cd} != '') 
           AND A.AUTH_OFC_CD= @[auth_ofc_cd]
#end
       )			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="auth_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
