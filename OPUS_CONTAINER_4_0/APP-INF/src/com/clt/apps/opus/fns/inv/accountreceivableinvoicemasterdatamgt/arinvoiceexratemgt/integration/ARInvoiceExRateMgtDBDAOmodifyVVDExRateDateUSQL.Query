<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceExRateMgtDBDAOmodifyVVDExRateDateUSQL">
			<desc><![CDATA[INV_VVD_XCH_RT_DT
[] ARInvoiceExRateMgtDBDAO::modifyVVDExRateDate ( vvdExrateVos )]]></desc>
			<sql><![CDATA[
UPDATE INV_VVD_XCH_RT_DT SET 
	XCH_RT_DT = @[xch_rt_dt]
,   CNG_RMK = @[cng_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	VSL_CD = @[vsl_cd] 
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	PORT_CD = @[port_cd]
AND	SVC_SCP_CD = @[svc_scp_cd]
AND	IO_BND_CD = @[io_bnd_cd]
AND AR_OFC_CD = @[ar_ofc_cd]			]]></sql>
			<params>
				<param name="xch_rt_dt" type="12" value="" out="N"/>
				<param name="cng_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
