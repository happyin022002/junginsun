<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchArHqOfcAllListRSQL">
			<desc><![CDATA[RHQ 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT(A.AR_OFC_CD) AS CODE
FROM MDM_ORGANIZATION A
WHERE A.AR_OFC_CD IN (
            SELECT AR_HD_QTR_OFC_CD
            FROM MDM_ORGANIZATION
            WHERE DELT_FLG = 'N'             
            )
AND A.DELT_FLG = 'N' 
ORDER BY AR_OFC_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
