<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOJooStlVvdVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE JOO_STL_VVD SET 
	ACCT_YRMON = REPLACE(@[acct_yrmon],'-')
,	STL_VVD_SEQ = @[stl_vvd_seq]
,	TRD_CD = @[trd_cd]
,	JO_CRR_CD = @[jo_crr_cd]
,	RLANE_CD = @[rlane_cd]
,	RE_DIVR_CD = @[re_divr_cd]
,	JO_STL_ITM_CD = @[jo_stl_itm_cd]
,	JO_MNU_NM = @[jo_mnu_nm]
,	VSL_CD = @[vsl_cd]
,	SKD_VOY_NO = @[skd_voy_no]
,	SKD_DIR_CD = @[skd_dir_cd]
,	REV_DIR_CD = @[rev_dir_cd]
,	STL_BZC_PORT_CD = @[stl_bzc_port_cd]
,	BZC_PORT_ETA_DT = TO_DATE(@[bzc_port_eta_dt],'YYYYMMDDHH24MISS')
,	BZC_PORT_ETD_DT = TO_DATE(@[bzc_port_etd_dt],'YYYYMMDDHH24MISS')
,	STL_PAIR_PORT_CD = @[stl_pair_port_cd]
,	PAIR_PORT_ETA_DT = TO_DATE(@[pair_port_eta_dt],'YYYYMMDDHH24MISS')
,	PAIR_PORT_ETD_DT = TO_DATE(@[pair_port_etd_dt],'YYYYMMDDHH24MISS')
,	STL_TGT_VVD_BSS_CD = @[stl_tgt_vvd_bss_cd]
,	JO_STL_CFM_CD = @[jo_stl_cfm_cd]
,	PROC_JB_FLG = @[proc_jb_flg]
,	STL_RMK = @[stl_rmk]
,   UC_BSS_PORT_CD = @[uc_bss_port_cd]
,   UC_BSS_PORT_ETD_DT = TO_DATE(@[uc_bss_port_etd_dt],'YYYYMMDDHH24MISS')
,   AGMT_MON_COND_CD      = @[agmt_mon_cond_cd]    
,   AGMT_PORT_COND_CD     = @[agmt_port_cond_cd]   
,   AGMT_PORT_TP_COND_CD  = @[agmt_port_tp_cond_cd]
,   AGMT_OP_TP_COND_CD    = @[agmt_op_tp_cond_cd]  
,	UPD_DT = SYSDATE
,	UPD_USR_ID = @[upd_usr_id]
WHERE ACCT_YRMON = REPLACE(@[acct_yrmon],'-')
AND	STL_VVD_SEQ = @[stl_vvd_seq]			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="2" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="jo_stl_itm_cd" type="12" value="" out="N"/>
				<param name="jo_mnu_nm" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="stl_bzc_port_cd" type="12" value="" out="N"/>
				<param name="bzc_port_eta_dt" type="12" value="" out="N"/>
				<param name="bzc_port_etd_dt" type="12" value="" out="N"/>
				<param name="stl_pair_port_cd" type="12" value="" out="N"/>
				<param name="pair_port_eta_dt" type="12" value="" out="N"/>
				<param name="pair_port_etd_dt" type="12" value="" out="N"/>
				<param name="stl_tgt_vvd_bss_cd" type="12" value="" out="N"/>
				<param name="jo_stl_cfm_cd" type="12" value="" out="N"/>
				<param name="proc_jb_flg" type="12" value="" out="N"/>
				<param name="stl_rmk" type="12" value="" out="N"/>
				<param name="uc_bss_port_cd" type="12" value="" out="N"/>
				<param name="uc_bss_port_etd_dt" type="12" value="" out="N"/>
				<param name="agmt_mon_cond_cd" type="12" value="" out="N"/>
				<param name="agmt_port_cond_cd" type="12" value="" out="N"/>
				<param name="agmt_port_tp_cond_cd" type="12" value="" out="N"/>
				<param name="agmt_op_tp_cond_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
