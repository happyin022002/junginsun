<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOMdmTradeRSQL">
			<desc><![CDATA[MDM TRADE조회]]></desc>
			<sql><![CDATA[
select 
	trd_cd as code,
	trd_nm as name
from mdm_trade
where 1=1 --vsl_tp_cd = 'C'
and	delt_flg = 'N'
and trd_cd <> 'COM'
#if (${code} != '')
and	trd_cd = @[code]
#end
order by trd_cd			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
