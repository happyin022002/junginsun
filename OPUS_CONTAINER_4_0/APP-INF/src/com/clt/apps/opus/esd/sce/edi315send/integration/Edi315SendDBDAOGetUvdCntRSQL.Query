<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetUvdCntRSQL">
			<desc><![CDATA[GetUvdCnt]]></desc>
			<sql><![CDATA[
select count(*)
from sce_cop_dtl
where cop_no = @[cop_no]
and   stnd_edi_sts_cd = 'UVD'			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
