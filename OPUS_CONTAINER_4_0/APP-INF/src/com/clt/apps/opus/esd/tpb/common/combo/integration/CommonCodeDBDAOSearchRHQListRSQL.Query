<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchRHQListRSQL">
			<desc><![CDATA[SearchRHQList]]></desc>
			<sql><![CDATA[
SELECT DISTINCT TPB_GET_HNDL_OFC_FNC('R',NVL(n3pty_ofc_cd,ofc_cd)) AS ofc_cd,
       TPB_GET_HNDL_OFC_FNC('R',NVL(n3pty_ofc_cd,ofc_cd)) AS ofc_cd2
  FROM TPB_HNDL_OFC
 WHERE delt_flg = 'N'
   AND TPB_GET_HNDL_OFC_FNC('R',NVL(n3pty_ofc_cd,ofc_cd)) IS NOT NULL
   AND n3pty_ofc_tp_cd = 'G'
#if (${s_office_level} == 'R')
   AND rhq_cd = @[s_ofc_cd_for_rhq]
#elseif (${s_office_level} == 'G' || ${s_office_level} == '')
   AND TPB_GET_HNDL_OFC_FNC('R',n3pty_ofc_cd) = @[s_ofc_cd_for_rhq]
#end
 ORDER BY ofc_cd			]]></sql>
			<params>
				<param name="s_ofc_cd_for_rhq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
