<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOSearchActualToBeUpdatedRSQL">
			<desc><![CDATA[Replan 시 actual 에 재 mapping 될 정보를 찾는다.]]></desc>
			<sql><![CDATA[
SELECT --DECODE(MIN(SRC),'0','VVD','1','ORG','2','SPL','3','CMB','4','LCL') SCR,                                      
   BKG_NO BKG_NO,
   ACT_RCV_DT AS ACT_RCV_DT, 
   ACT_RCV_NO AS ACT_RCV_NO,
   CNTR_NO CNTR_NO,
   COP_NO COP_NO,
   ACT_DT ACT_DT,
   ACT_STS_MAPG_CD ACT_STS_MAPG_CD ,
   COP_DTL_SEQ  COP_DTL_SEQ ,
   NOD_CD NOD_CD,
   ACT_RCV_TP_CD ACT_RCV_TP_CD ,
   VSL_CD VSL_CD, 
   SKD_VOY_NO SKD_VOY_NO,
   SKD_DIR_CD SKD_DIR_CD,
   VPS_PORT_CD VPS_PORT_CD ,
   EDI_SND_RSLT_FLG EDI_SND_RSLT_FLG,
   USR_ID USR_ID,
   ACT_UMCH_TP_CD ACT_UMCH_TP_CD ,
   CLPT_IND_SEQ CLPT_IND_SEQ,
   ACT_DAT_RCV_DT  ACT_DAT_RCV_DT,
   USR_ID  AS UPD_USR_ID,
   EDI_MSG_TP_CD 
   FROM (
SELECT   '0',
          BKG_NO,
          ACT_RCV_DT,
          ACT_RCV_NO,
          CNTR_NO,
          COP_NO,
          ACT_DT,
          ACT_STS_MAPG_CD,
          COP_DTL_SEQ,
          NOD_CD,
          ACT_RCV_TP_CD,
          VSL_CD,
          SKD_VOY_NO,
          SKD_DIR_CD,
          VPS_PORT_CD,
          EDI_SND_RSLT_FLG,
          USR_ID ,
          ACT_UMCH_TP_CD,
          CLPT_IND_SEQ,
          ACT_DAT_RCV_DT,
          EDI_MSG_TP_CD
        FROM (
      SELECT '1',
              RANK() OVER (PARTITION BY ACT_STS_MAPG_CD, NOD_CD
                ORDER BY ACT_RCV_NO DESC) AS RNK,
      V.BKG_NO,
      ACT_RCV_DT,
      ACT_RCV_NO,
      V.CNTR_NO,
      @[cop_no] AS COP_NO ,
      TO_CHAR(ACT_DT, 'YYYYMMDDHH24MISS')ACT_DT,
      ACT_STS_MAPG_CD,
      (
        SELECT  COP_DTL_SEQ
        FROM SCE_COP_DTL
        WHERE COP_NO=@[cop_no]
          AND ACT_CD='FITZAD') COP_DTL_SEQ,
      NOD_CD,
      ACT_RCV_TP_CD ,
      VSL_CD,
      SKD_VOY_NO,
      SKD_DIR_CD,
      VPS_PORT_CD,
      EDI_SND_RSLT_FLG,
      'ACT_MAPG_RPLN' USR_ID ,
      ACT_UMCH_TP_CD,
      CLPT_IND_SEQ ,
      TO_CHAR(ACT_DAT_RCV_DT, 'YYYYMMDDHH24MISS') ACT_DAT_RCV_DT,
      AD.EDI_MSG_TP_CD
    FROM SCE_ACT_RCV_IF AD,
      (
        SELECT '1' SRC,
          BKG_NO,
          CNTR_NO,
          TRNK_VSL_CD,
          TRNK_SKD_VOY_NO,
          TRNK_SKD_DIR_CD
        FROM SCE_COP_HDR
        WHERE COP_NO = @[cop_no]
          AND COP_STS_CD NOT IN ('X','O') ) V,
      SCE_COP_HDR SRC
    WHERE AD.BKG_NO = SRC.BKG_NO
      AND AD.CNTR_NO = SRC.CNTR_NO
      AND SRC.CNTR_NO = V.CNTR_NO
      AND SRC.TRNK_VSL_CD = V.TRNK_VSL_CD
      AND SRC.TRNK_SKD_VOY_NO = V.TRNK_SKD_VOY_NO
      AND SRC.TRNK_SKD_DIR_CD = V.TRNK_SKD_DIR_CD
      AND AD.CNTR_NO = V.CNTR_NO
      AND AD.ACT_UMCH_TP_CD NOT IN ('00','XX')
     AND AD.CRE_USR_ID NOT IN ('CrtBy1TimeSplt', 'CrtBy1TimeCmbn') 
     )
     WHERE RNK = 1
 UNION ALL
 SELECT '0',
          BKG_NO,
          ACT_RCV_DT,
          ACT_RCV_NO,
          CNTR_NO,
          COP_NO,
          ACT_DT,
          ACT_STS_MAPG_CD,
          COP_DTL_SEQ,
          NOD_CD,
          ACT_RCV_TP_CD,
          VSL_CD,
          SKD_VOY_NO,
          SKD_DIR_CD,
          VPS_PORT_CD,
          EDI_SND_RSLT_FLG,
          USR_ID ,
          ACT_UMCH_TP_CD,
          CLPT_IND_SEQ,
          ACT_DAT_RCV_DT,
          EDI_MSG_TP_CD
        FROM (
            SELECT '0',
              RANK() OVER (PARTITION BY AD.ACT_STS_MAPG_CD, AD.NOD_CD, AD.VSL_CD, AD.SKD_VOY_NO, AD.SKD_DIR_CD, AD.CLPT_IND_SEQ
                ORDER BY AD.CRE_DT DESC) AS RNK,
              AD.BKG_NO,
              ACT_RCV_DT,
              ACT_RCV_NO,
              AD.CNTR_NO,
              @[cop_no] AS COP_NO,
              TO_CHAR(ACT_DT, 'YYYYMMDDHH24MISS')ACT_DT,
              AD.ACT_STS_MAPG_CD,
              V.COP_DTL_SEQ,
              AD.NOD_CD,
              ACT_RCV_TP_CD ,
              AD.VSL_CD,
              AD.SKD_VOY_NO,
              AD.SKD_DIR_CD,
              AD.VPS_PORT_CD,
              EDI_SND_RSLT_FLG,
              'ACT_MAPG_RPLNV' USR_ID ,
              ACT_UMCH_TP_CD,
              AD.CLPT_IND_SEQ,
              TO_CHAR(NVL(V.ACT_DAT_RCV_DT, AD.ACT_DAT_RCV_DT), 'YYYYMMDDHH24MISS') ACT_DAT_RCV_DT,
              AD.EDI_MSG_TP_CD
            FROM SCE_ACT_RCV_IF AD,
              (
                SELECT SUBSTR(NOD_CD, 1, 5) NOD_CD,
                  VSL_CD,
                  SKD_VOY_NO,
                  SKD_DIR_CD,
                  'AT'||SUBSTR(ACT_CD, 5, 1) ACT_STS_MAPG_CD,
                  CLPT_IND_SEQ,
                  COP_DTL_SEQ,
                  ACT_DAT_RCV_DT
                FROM SCE_COP_DTL
        WHERE COP_NO = @[cop_no]
          AND VSL_CD IS NOT NULL
          AND SKD_VOY_NO IS NOT NULL
          AND SKD_DIR_CD IS NOT NULL
          AND SUBSTR(ACT_CD,5,1) IN ('A','B','D')
        GROUP BY NOD_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD,SUBSTR(ACT_CD,5,1),CLPT_IND_SEQ, COP_DTL_SEQ, ACT_DAT_RCV_DT) V
    WHERE AD.VSL_CD = V.VSL_CD
      AND AD.SKD_VOY_NO = V.SKD_VOY_NO
      AND AD.SKD_DIR_CD = V.SKD_DIR_CD
      AND AD.VPS_PORT_CD = V.NOD_CD
      AND AD.CLPT_IND_SEQ = V.CLPT_IND_SEQ
      AND AD.ACT_STS_MAPG_CD = V.ACT_STS_MAPG_CD
      AND AD.ACT_UMCH_TP_CD NOT IN ('00','XX')
      AND AD.CRE_USR_ID NOT IN ('CrtBy1TimeSplt', 'CrtBy1TimeCmbn')
      )
    WHERE RNK =  1
    )
    ORDER BY ACT_RCV_NO			]]></sql>
			<params>
				<param name="cop_no" type="12" value="CHAM0831647431" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
