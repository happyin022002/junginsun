<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VendorCmDBDAOModifyTrsTrspSvcOrdUSQL">
			<desc><![CDATA[VendorCmDBDAOModifyTrsTrspSvcOrd]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD T
   SET LOCL_UPD_DT   = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(T.CRE_OFC_CD)
      ,UPD_USR_ID     = @[upd_usr_id]
      ,UPD_DT         = SYSDATE
 WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND TRSP_SO_SEQ = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
