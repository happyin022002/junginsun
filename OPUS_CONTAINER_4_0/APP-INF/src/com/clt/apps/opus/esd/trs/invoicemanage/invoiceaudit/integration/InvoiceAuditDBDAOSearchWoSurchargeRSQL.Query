<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchWoSurchargeRSQL">
			<desc><![CDATA[해당 SO의 Suchargy를 가지고 온다.]]></desc>
			<sql><![CDATA[
SELECT	TRSP_SO_OFC_CTY_CD			,	
		TRSP_SO_SEQ				,	
		LGS_COST_CD				,	
	 	SCG_AMT				,	
	 	DRY_RUN_RLBL_PTY_TP_CD			,	
	 	FNE_CUZ_DESC				,	
	 	FUMG_COST_TP_CD			,	
	 	MGST_TPSZ_CD				,	
	 	INSP_RF_PTI_CSTMS_TP_CD		,	
	 	LFTG_KNT				,	
	 	LFTG_CUZ_DESC				,	
	 	STOP_LOC_NOD_CD			,	
	 	GRS_WGT				,	
	 	TO_CHAR(INCRT_DT,'YYYYMMDD') INCRT_DT				,	
	 	SCL_STOP_PLC_NOD_CD			,	
	 	STO_DYS				,	
	 	OB_BKG_NO				,	
	 	WT_HRS					,	
	 	OTR_RMK				,	
	 	N3PTY_BIL_FLG				,	
	 	CUST_CNT_CD				,	
	 	CUST_SEQ				,	
	 	N3PTY_VNDR_SEQ				,	
	 	N3PTY_OFC_CD				,	
	 	N3PTY_AMT				,	
	 	N3PTY_DESC				,	
	 	CRE_OFC_CD				,	
	 	CRE_USR_ID				,	
	 	TO_CHAR(CRE_DT,'YYYYMMDDHH24MISS') CRE_DT,
	 	CHSS_NO				,
	 	TO_CHAR(INCUR_DT,'YYYYMMDD') INCUR_DT,
		TRSP_AGMT_OFC_CTY_CD,
		TRSP_AGMT_SEQ,
		TRSP_AGMT_RT_TP_SER_NO,
		TRSP_AGMT_SCG_NOD_SEQ,
		TRSP_AGMT_SCG_RT_SEQ,
		COM_SCG_KND_CD,
		COM_SCG_SEQ,
		SCG_DTL_SEQ,
     	CURR_CD,
     	ORG_SCG_AMT
  FROM	TRS_TRSP_SCG_DTL			
 WHERE 	TRSP_SO_OFC_CTY_CD		= @[trsp_so_ofc_cty_cd]	
   AND	TRSP_SO_SEQ				= @[trsp_so_seq]		
   AND	NVL(SCG_AMT, 0)			<> 0
   AND	SUBSTR(LGS_COST_CD, 3, 2)<> 'FU'			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
