<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOMultiSingleTransportationEqrUSQL">
			<desc><![CDATA[EmptyRepo S/O의 발급상태정보를 EQR에 Update]]></desc>
			<sql><![CDATA[
UPDATE EQR_REPO_EXE_SO_IF SET
	TRSP_SO_STS_CD = 'C',
	UPD_USR_ID 	= @[upd_usr_id],
	UPD_DT		= SYSDATE 
	WHERE REPO_PLN_ID	= @[repo_pln_id] 
	AND PLN_YRWK		= @[pln_yrwk] 
	AND REF_ID		= @[ref_id] 
	AND REF_SEQ		= @[ref_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="ref_id" type="12" value="" out="N"/>
				<param name="ref_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
