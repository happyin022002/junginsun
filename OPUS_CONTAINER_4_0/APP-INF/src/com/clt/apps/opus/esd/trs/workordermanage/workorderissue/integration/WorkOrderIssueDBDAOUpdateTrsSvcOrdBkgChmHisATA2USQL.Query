			<desc><![CDATA[UpdateTrsSvcOrdBkgChmHisATA2]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD_CNG 
SET LOD_REF_NO = @[lod_ref_no] 
	,DOR_ZIP_ID = @[dor_zip_id]
	,DOR_ADDR = @[dor_addr]   
	,ARR_DT = @[arr_dt] 
	,CNTC_PSON_NM = @[cntc_pson_nm]   
	,CNTC_PHN_NO = @[cntc_phn_no]     
	,CNTC_EML = @[cntc_eml]
    ,LOCL_UPD_DT=GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])
    ,UPD_USR_ID=@[upd_usr_id]
    ,upd_dt=SYSDATE   
WHERE 1=1
AND TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
AND TRSP_SO_SEQ = @[trsp_so_seq]
AND TRSP_SO_SUB_SEQ = @[trsp_so_sub_seq] 			]]></sql>
			<params>
				<param name="lod_ref_no" type="12" value="" out="N"/>
				<param name="dor_zip_id" type="12" value="" out="N"/>
				<param name="dor_addr" type="12" value="" out="N"/>
				<param name="arr_dt" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_phn_no" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="trsp_so_sub_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
