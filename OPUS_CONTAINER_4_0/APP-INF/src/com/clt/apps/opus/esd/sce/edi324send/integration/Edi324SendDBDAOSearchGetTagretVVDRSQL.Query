<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi324SendDBDAOSearchGetTagretVVDRSQL">
			<desc><![CDATA[EDI 324 발송 대상이 되는 VVD별로 조회를 해온다.]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ,
       BKG_NO,
       CNTR_NO,
       VSL_CD,
       SKD_VOY_NO,
       SKD_DIR_CD,
       EDI_SND_SEQ,
       MNL_FLG,
       EDI_SND_TP_CD,
       COP_NO,
       LLOYD_VSL_NO,
       VSL_NM,
       POL_YD_CD,
       TO_CHAR(POL_DEP_ACT_DT,'YYYYMMDDHH24MISS') POL_DEP_ACT_DT,
       POD_YD_CD,
       POD_NM,
       ACT_CD,
       TO_CHAR(POD_ESTM_ARR_DT,'YYYYMMDDHH24MISS') POD_ESTM_ARR_DT,
       TO_CHAR(POD_ESTM_ARR_GDT,'YYYYMMDDHH24MISS') POD_ESTM_ARR_GDT,
       BL_NO,
       CNTR_WGT,
       CNTR_WGT_UT_CD,
       CNTR_LBS_WGT,
       CNTR_TPSZ_CD,
       CNTR_LEN,
       CNTR_HGT,
       CNTR_SEAL_NO,
       ORG_YD_LOC_CTY_NM,
       ORG_YD_LOC_STE_CD,
       ORG_LOC_NM,
       ORG_YD_CD,
       DEST_YD_LOC_CTY_NM,
       DEST_YD_LOC_STE_CD,
       DEST_LOC_NM,
       DEST_YD_CD,
       DEST_YD_NM,
       DG_FLG,
       CRE_USR_ID,
       UPD_USR_ID
FROM SCE_EDI_324_SND_RSLT SLT
WHERE EDI_SND_TP_CD ='S'
AND   EDI_SND_RMK ='Saved'
ORDER BY VSL_CD ,SKD_VOY_NO, SKD_DIR_CD ASC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
