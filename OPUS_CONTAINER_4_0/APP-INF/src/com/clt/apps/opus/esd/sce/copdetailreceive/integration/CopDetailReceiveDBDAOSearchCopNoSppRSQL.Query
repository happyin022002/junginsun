<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchCopNoSppRSQL">
			<desc><![CDATA[SearchCopNoSpp]]></desc>
			<sql><![CDATA[
SELECT cop_no
FROM   sce_cop_hdr
WHERE  cntr_no = @[cntr_no] 
and    bkg_no = @[bkg_no] 
AND    cop_sts_cd IN ('C', 'T', 'F')
AND    ROWNUM = 1
			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
