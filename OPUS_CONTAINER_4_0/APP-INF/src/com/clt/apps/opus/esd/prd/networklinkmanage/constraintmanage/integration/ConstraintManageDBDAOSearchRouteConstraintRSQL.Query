<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOSearchRouteConstraintRSQL">
			<desc><![CDATA[SearchRouteConstraint]]></desc>
			<sql><![CDATA[
SELECT DELT_FLG
      ,TRNK_LANE_CD
      ,DIR_CD
      ,SUBSTR(POR_NOD_CD, 1, 5) POR_CD
      ,SUBSTR(POR_NOD_CD, 6) POR_NODE
      ,SUBSTR(POL_NOD_CD, 1, 5) POL_CD
      ,SUBSTR(POL_NOD_CD, 6) POL_NOD
      ,ROUT_CNST_SEQ
      ,N1ST_LANE_CD
      ,N1ST_TS_PORT_CD
      ,N2ND_LANE_CD
      ,N2ND_TS_PORT_CD
      ,N3RD_LANE_CD
      ,SUBSTR(POD_NOD_CD, 1, 5) POD_CD
      ,SUBSTR(POD_NOD_CD, 6) POD_NOD
      ,SUBSTR(DEL_NOD_CD, 1, 5) DEL_CD
      ,SUBSTR(DEL_NOD_CD, 6) DEL_NODE
      ,SVC_USE_FLG
      ,ROUT_CNST_RMK
      ,TO_CHAR(CRE_DT, 'YYYY-MM-DD') CRE_DT
      ,CRE_OFC_CD
      ,CRE_USR_ID
      ,TO_CHAR(UPD_DT, 'YYYY-MM-DD') UPD_DT
      ,UPD_OFC_CD
      ,UPD_USR_ID
      ,SPCL_CGO_CNTR_TP_CD
      ,CNTR_TP_CD
      ,CNTR_SZ_CD
  FROM PRD_ROUT_CNST
 WHERE DECODE(TRNK_LANE_CD, 'ALL', @[tlane] || '%', TRNK_LANE_CD) LIKE @[tlane] || '%'
   AND NVL(POR_NOD_CD, ' ') LIKE @[por] || '%'
   AND POL_NOD_CD LIKE @[pol] || '%'
   AND (NVL(N1ST_TS_PORT_CD, ' ') LIKE @[tsport] || '%' OR NVL(N2ND_TS_PORT_CD, ' ') LIKE @[tsport] || '%')
   AND POD_NOD_CD LIKE @[pod] || '%'
   AND NVL(DEL_NOD_CD, ' ') LIKE @[del] || '%'
   AND NVL(DELT_FLG, 'N') = 'N'
   AND NVL(dir_cd, 'X') like @[dir_cd] || '%'
   AND SVC_USE_FLG = DECODE(@[svc], 'A', SVC_USE_FLG, @[svc])			]]></sql>
			<params>
				<param name="tlane" type="12" value="" out="N"/>
				<param name="por" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
				<param name="tsport" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
				<param name="del" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="svc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
