<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAORemoveTrsInterRmkUSQL">
			<desc><![CDATA[SingleTransportationSOManageDBDAORemoveTrsInterRmk]]></desc>
			<sql><![CDATA[
UPDATE TRS_INTER_RMK
   SET DELT_FLG      = 'Y'
      ,UPD_USR_ID    = @[upd_usr_id]
      ,UPD_DT        = SYSDATE
      ,LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(CRE_OFC_CD)
 WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND TRSP_SO_SEQ = @[trsp_so_seq]
   AND NVL(DELT_FLG, 'N') = 'N' 			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
