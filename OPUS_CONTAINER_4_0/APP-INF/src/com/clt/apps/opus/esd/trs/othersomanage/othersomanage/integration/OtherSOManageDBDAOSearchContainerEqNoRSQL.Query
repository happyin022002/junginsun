<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OtherSOManageDAOSearchContainerEqNoRSQL">
			<desc><![CDATA[Container의 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT                                     
		CNTR_NO AS EQ_NO
		,CNTR_TPSZ_CD AS EQ_TPSZ_CD
		,@[row] AS S_ROW
FROM 	MST_CONTAINER
WHERE
		CNTR_NO in (

#foreach( ${key} in ${eqNo}) 

	#if($velocityCount < $eqNo.size()) 
		'$key', 
	#else 
		'$key' 
	#end 

#end
		)			]]></sql>
			<params>
				<param name="row" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
