<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchCheckEqNoRSQL">
			<desc><![CDATA[SearchCheckEqNo]]></desc>
			<sql><![CDATA[
#if (${s_eq_knd_cd} == 'U')
    -- Container
    SELECT CNTR_TPSZ_CD AS TPSZ_CD, NULL
      FROM MST_CONTAINER
     WHERE CNTR_NO = @[s_eq_no]
       AND ROWNUM = 1
#else
    -- Chassis & Genset
    SELECT EQ_TPSZ_CD AS TPSZ_CD, NULL
      FROM CGM_EQUIPMENT
     WHERE EQ_NO = @[s_eq_no]
       AND EQ_KND_CD = @[s_eq_knd_cd]
       AND ROWNUM = 1
#end			]]></sql>
			<params>
				<param name="s_eq_no" type="12" value="" out="N"/>
				<param name="s_eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
