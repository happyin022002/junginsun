<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTermainalInvoiceManageDBDAOCheckTerminalInvoiceStatusRSQL">
			<desc><![CDATA[CheckTerminalInvoiceStatus]]></desc>
			<sql><![CDATA[
SELECT tml_inv_sts_cd,
       tml_inv_rjct_sts_cd
FROM  tes_tml_so_hdr
WHERE vndr_seq = @[vndr_seq]
AND   inv_no   = @[inv_no]
AND   NVL(delt_flg,'N')   <> 'Y'			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
