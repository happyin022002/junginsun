<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifySceVpsIfByResultUSQL">
			<desc><![CDATA[ModifySceVpsIfByResult]]></desc>
			<sql><![CDATA[
update sce_vps_if
set    vps_if_sts_cd = @[vps_if_sts_cd]
      ,upd_usr_id    = 'VPS_EST'
      ,upd_dt        = sysdate
where  vps_rcv_dt    = @[vps_rcv_dt]
and    vps_rcv_no    = @[vps_rcv_no]			]]></sql>
			<params>
				<param name="vps_if_sts_cd" type="12" value="" out="N"/>
				<param name="vps_rcv_dt" type="12" value="" out="N"/>
				<param name="vps_rcv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
