<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsInterfaceDBDAOModifyClmIfUSQL">
			<desc><![CDATA[WAS restart 시 SCE_CLM_IF 의 SO_MAPG_STS_CD 를 원복한다. (XX ==> 00)]]></desc>
			<sql><![CDATA[
update sce_clm_if
set so_mapg_sts_cd = '00'
where (CNTR_NO,
      CNMV_YR,
      CNMV_ID_NO,
      CLM_SEQ) in (
    select CNTR_NO,
      CNMV_YR,
      CNMV_ID_NO,
      CLM_SEQ
    from SCE_CLM_IF
    where SO_MAPG_STS_CD = 'XX'
      and upd_dt > sysdate - 3/24 )			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
