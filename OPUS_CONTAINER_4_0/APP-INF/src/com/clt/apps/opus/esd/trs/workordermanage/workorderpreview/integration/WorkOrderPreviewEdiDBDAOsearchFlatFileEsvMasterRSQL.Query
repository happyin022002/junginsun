<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewEdiDBDAOsearchFlatFileEsvMasterRSQL">
			<desc><![CDATA[NYK-Hask_FFile(ESV_N) - JOEDI
{EU]]></desc>
			<sql><![CDATA[
SELECT @[wo_iss_sts_cd] WO_ISS_STS_CD
      ,DECODE(@[wo_iss_sts_cd], 'I', 'Create', 'N', 'Cancel', 'C', 'Update', 'R', 'Update', 'X', 'Draft') AS JO_ITEM_MSG_TYPE
      ,'NYKS-' || SUBSTR(M.OFC_CD, 1, 3) AS SND_CD
      ,V.USA_EDI_CD || '-' || SUBSTR(L.SCC_CD, 3, 3) AS RCV_CD
      ,SO.TRSP_WO_OFC_CTY_CD || SO.TRSP_WO_SEQ || '-' || SO.TRSP_SO_OFC_CTY_CD || SO.TRSP_SO_SEQ AS JO_ITEM_REF
      ,'NYKS' CARRIER_CD
      ,CASE WHEN SO.TRSP_SO_TP_CD = 'M' THEN 'Empty'
            WHEN SO.TRSP_BND_CD = 'I' THEN 'Import'
            WHEN SO.TRSP_BND_CD = 'O' THEN 'Export'
            ELSE ''
       END AS MOVE_TYPE
      ,DECODE(SO.TRSP_COST_DTL_MOD_CD, 'DR', 'Round-Trip', 'One-Way') AS TRIP_TYPE
      ,CASE WHEN SO.EQ_KND_CD = 'Z' THEN  'Chassis Reposition'
            WHEN SO.TRSP_SO_TP_CD = 'M' THEN 'Container Reposition'
            WHEN SO.TRSP_BND_CD = 'I' THEN 'Import/Inbound Dray'
            WHEN SO.TRSP_BND_CD = 'O' THEN 'Export/Outbound Dray'
            ELSE ''
       END AS CATEGORY
      ,U.USR_NM AS CREATEBY
      ,NVL((SELECT MAX(WO_ISS_KNT) - 1 FROM TRS_TRSP_WRK_ORD_HIS S WHERE S.TRSP_WO_OFC_CTY_CD = WO.TRSP_WO_OFC_CTY_CD AND S.TRSP_WO_SEQ = WO.TRSP_WO_SEQ), 0) AS AMEND_NO
      ,NVL((SELECT MAX(WO_ISS_KNT) - 1 FROM TRS_TRSP_WRK_ORD_HIS S WHERE S.TRSP_WO_OFC_CTY_CD = WO.TRSP_WO_OFC_CTY_CD AND S.TRSP_WO_SEQ = WO.TRSP_WO_SEQ), 0) AS VERSION_NO
      ,TO_CHAR(WO.LOCL_CRE_DT, 'YYYYMMDDHH24MISS') AS JO_ISSUE_DT
      ,TO_CHAR(WO.LOCL_CRE_DT + 2 / 24, 'YYYYMMDDHH24MISS') AS RESPOND_DT
      ,DECODE(SO.TRSP_CRR_MOD_CD, 'TD', 'Truck', 'TR', 'Truck Rail', 'TW', 'Truck Feeder', 'RD', 'Rail', 'RT', 'Rail Truck', 'RW', 'Rail Feeder', 'WD', 'Feeder', 'WR', 'Feeder Rail', 'WT', 'Feeder Truck') AS TRANS_MD
      ,DECODE(SO.TRSP_BND_CD, 'O', REPLACE(REPLACE(WO.WO_RMK, CHR(10), '!@#'), CHR(13), ''), REPLACE(REPLACE(WO.WO_RMK, CHR(10), ''), CHR(13), '')) AS M_COMMENT
      ,'' AS REASON_CD
      ,'' AS CNTR_SEQ
      ,'' AS TOTAL_CNTR
      ,'' AS WAYBILL_NO
      ,SO.TRSP_SO_OFC_CTY_CD
      ,SO.TRSP_SO_SEQ
      ,SO.EQ_NO
      ,SO.EQ_TPSZ_CD
      ,SO.BKG_NO
      ,SO.VNDR_SEQ
	  ,SO.TRSP_SO_TP_CD
      ,SO.CGO_TP_CD
      ,@[wo_prv_grp_seq] WO_PRV_GRP_SEQ
      ,@[wo_iss_no] WO_ISS_NO
  FROM TRS_TRSP_SVC_ORD SO
      ,TRS_TRSP_WRK_ORD WO
      ,MDM_YARD         M
      ,MDM_VENDOR       V
      ,COM_USER         U
      ,MDM_LOCATION     L
 WHERE SO.TRSP_WO_OFC_CTY_CD = WO.TRSP_WO_OFC_CTY_CD
   AND SO.TRSP_WO_SEQ = WO.TRSP_WO_SEQ
   AND WO.WO_ISS_STS_CD <> 'X'
   AND SO.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND SO.TRSP_SO_SEQ = @[trsp_so_seq]
   AND SO.FM_NOD_CD = M.YD_CD(+)
   AND SO.VNDR_SEQ = V.VNDR_SEQ(+)
   AND SO.UPD_USR_ID = U.USR_ID(+)
   AND U.USE_FLG(+) = 'Y'
   AND SUBSTR(SO.FM_NOD_CD, 1, 5) = L.LOC_CD(+)			]]></sql>
			<params>
				<param name="wo_iss_sts_cd" type="12" value="" out="N"/>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
