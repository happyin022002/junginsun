<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DOFChgTrfmanageDBDAOsearchDofChgDupCntRSQL">
			<desc><![CDATA[Drop Off Charge Tariff 중복 갯수 조회]]></desc>
			<sql><![CDATA[
select count(*) dup_cnt									
 from trs_drff_chg_trf  
 where to_char(eff_dt,'YYYYMMDD') = @[streffdt]                      

#if( ${strcnt_cd} != '')
	   and  fm_loc_cd = @[strcnt_cd]    								
#end

#if( ${cust_info} != '')
	   and cnt_cd = substr(@[cust_info],0,2)                                      
	   and cust_seq = substr(@[cust_info],3)                                    			
#end
   and conti_cd = @[conti_cd]                                        
   and cntr_tp_cd = @[s_cntr_tp_cd]			]]></sql>
			<params>
				<param name="streffdt" type="12" value="" out="N"/>
				<param name="strcnt_cd" type="12" value="20090909" out="N"/>
				<param name="cust_info" type="12" value="2" out="N"/>
				<param name="conti_cd" type="12" value="3" out="N"/>
				<param name="s_cntr_tp_cd" type="12" value="4" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
