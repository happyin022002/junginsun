<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOSearchInvoiceSheetSetRSQL">
			<desc><![CDATA[searchInvoiceSheetSet]]></desc>
			<sql><![CDATA[
SELECT inv_iss_ofc_cd
      ,co_nm
      ,ofc_addr
      ,ofc_phn_no
      ,ofc_fax_no
      ,bil_to_loc_div_cd
      ,inv_rmk1
      ,inv_rmk2
      ,vat_xch_rt
      ,vat_xch_rt / 100 vat_xch_rt_div
  FROM TPB_INV_SH_SET
 WHERE inv_iss_ofc_cd = @[s_inv_iss_ofc_cd]  			]]></sql>
			<params>
				<param name="s_inv_iss_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
