<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsCommonDBDAOTrsComboCustCodeRSQL">
			<desc><![CDATA[Select]]></desc>
			<sql><![CDATA[
SELECT   'CD99999' AS INTG_CD_ID
,        CUST_CNT_CD AS INTG_CD_VAL_CTNT
,        CUST_CNT_CD AS INTG_CD_VAL_DESC
,        CUST_CNT_CD AS INTG_CD_VAL_DP_DESC
,        CUST_SEQ AS INTG_CD_VAL_DP_SEQ
FROM     MDM_CUSTOMER
WHERE    DELT_FLG = 'N'
AND      CUST_CNT_CD = SUBSTR(@[cust_code], 1, 2)
AND      CUST_SEQ = SUBSTR(@[cust_code], 3)			]]></sql>
			<params>
				<param name="cust_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
