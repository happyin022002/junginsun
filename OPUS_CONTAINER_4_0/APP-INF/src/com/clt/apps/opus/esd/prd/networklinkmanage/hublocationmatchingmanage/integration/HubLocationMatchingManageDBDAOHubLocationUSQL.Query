<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HubLocationMatchingManageDBDAOHubLocationUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE prd_hub_loc_mtch
 SET    hub_loc_cd       = @[hub_loc_cd] ,     
        upd_usr_id       = @[upd_usr_id] ,     
        upd_dt           = sysdate ,
        TRSP_MOD_CD      = @[trsp_mod_cd]       ,
        HUB_LOC_MTCH_RMK = @[hub_loc_mtch_rmk]        
 WHERE  port_cd          = @[port_cd]        
 AND    loc_cd           = @[loc_cd]       			]]></sql>
			<params>
				<param name="hub_loc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="hub_loc_mtch_rmk" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
