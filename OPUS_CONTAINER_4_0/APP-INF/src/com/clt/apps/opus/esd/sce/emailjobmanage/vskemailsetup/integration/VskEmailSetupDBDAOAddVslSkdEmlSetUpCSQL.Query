<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VskEmailSetupDBDAOAddVslSkdEmlSetUpCSQL">
			<desc><![CDATA[VSL E-MAIL 대상 추가 ( SCE_VSL_SKD_EML )]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_VSL_SKD_EML(
    EML_JB_ID,
    SVC_ST_DT,
    SVC_END_DT,
    EML_GRP_ID,
    VSL_SLAN_CD,
    FM_PORT_CD,
    TO_PORT_CD,
    VSKD_DUR_ID,
    DELT_FLG,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
) VALUES (
    @[emlJbId],
    NVL(@[svcStDt], TO_CHAR(SYSDATE, 'YYYYMMDD')),
    NVL(@[svcEndDt], '99991231'),
    @[emlGrpId],
    @[vslSlanCd],
    @[fmPortCdHis],
    @[toPortCd],
    @[vskdDurId]||'D',
    'N',
    'SYSTEM',
    SYSDATE,
    'SYSTEM',
    SYSDATE
)			]]></sql>
			<params>
				<param name="emlJbId" type="12" value="" out="N"/>
				<param name="svcStDt" type="12" value="" out="N"/>
				<param name="svcEndDt" type="12" value="" out="N"/>
				<param name="emlGrpId" type="12" value="" out="N"/>
				<param name="vslSlanCd" type="12" value="" out="N"/>
				<param name="fmPortCdHis" type="12" value="" out="N"/>
				<param name="toPortCd" type="12" value="" out="N"/>
				<param name="vskdDurId" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
