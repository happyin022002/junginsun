<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementImportDBDAOMultiCorrScgAgmtScgRateDSQL">
			<desc><![CDATA[Correction시 체크박스 해제시 Rate를 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM TRS_AGMT_SCG_RT A
WHERE (A.TRSP_AGMT_OFC_CTY_CD, A.TRSP_AGMT_SEQ, A.TRSP_AGMT_RT_TP_SER_NO, A.TRSP_AGMT_SCG_NOD_SEQ, A.TRSP_AGMT_SCG_RT_SEQ)
     IN (SELECT TRSP_AGMT_OFC_CTY_CD, TRSP_AGMT_SEQ, TRSP_AGMT_RT_TP_SER_NO, TRSP_AGMT_NOD_SEQ, TRSP_AGMT_RT_SEQ
         FROM TRS_AGMT_TMP
        WHERE TRSP_TMP_SEQ = @[trsp_tmp_seq]
          AND DELT_FLG = 'Y'
      )			]]></sql>
			<params>
				<param name="trsp_tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
