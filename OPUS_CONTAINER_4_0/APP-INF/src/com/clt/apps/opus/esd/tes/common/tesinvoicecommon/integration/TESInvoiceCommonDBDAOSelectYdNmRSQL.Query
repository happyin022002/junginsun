<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESInvoiceCommonDBDAOSelectYdNmRSQL">
			<desc><![CDATA[SelectYdNm]]></desc>
			<sql><![CDATA[
SELECT  YD_NM
FROM	MDM_YARD
WHERE	YD_CD = @[yd_cd]
AND		DELT_FLG = 'N'
			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
