<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOAgreementHistoryTSCSQL">
			<desc><![CDATA[Agreement Type Size Insert]]></desc>
			<sql><![CDATA[
INSERT INTO TES_TML_AGMT_TP_SZ (
			  TML_AGMT_OFC_CTY_CD
			, TML_AGMT_SEQ
			, TML_AGMT_VER_NO
			, TML_AGMT_DTL_SEQ
			, CNTR_TPSZ_CD
			, CNTR_APLY_TP_CD
			, AGMT_RT
			, AGMT_DYS
			, CRE_USR_ID
			, CRE_DT
			, UPD_USR_ID
			, UPD_DT
			, LOCL_CRE_DT
			, EQ_TPSZ_CD
			, EQ_KND_CD
	)
	(
    SELECT  @[tml_agmt_ofc_cty_cd]                                                                            
    		, @[tml_agmt_seq]                                                                   
    		, @[tml_agmt_ver_no_new]                                                                      
    		, tml_agmt_dtl_seq                                                               
    		, cntr_tpsz_cd                                                                   
    		, cntr_aply_tp_cd                                                                
    		, agmt_rt                                                                        
    		, agmt_dys                                                                       
    		, @[cre_usr_id]                                                                     
    		, SYSDATE
    		, @[cre_usr_id]                                                                     
    		, SYSDATE
    		, GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])   
			, EQ_TPSZ_CD
			, EQ_KND_CD                                         
	FROM	TES_TML_AGMT_TP_SZ                                         
	WHERE	tml_agmt_ofc_cty_cd	= @[tml_agmt_ofc_cty_cd]                                   
	AND		tml_agmt_seq		= @[tml_agmt_seq]
	AND		tml_agmt_ver_no		= @[tml_agmt_ver_no]
	)			]]></sql>
			<params>
				<param name="tml_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_agmt_seq" type="2" value="" out="N"/>
				<param name="tml_agmt_ver_no_new" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="tml_agmt_ver_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
