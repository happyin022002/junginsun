<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchCostCodeListRSQL">
			<desc><![CDATA[Cost Code를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD,'|')),'|') AS COST_CODE 
  FROM 
       (SELECT ROWNUM ROW_ID 
            , LGS_COST_CD 
         FROM TES_TML_SO_COST 
         ORDER BY LGS_COST_CD
       ) 
CONNECT BY PRIOR ROW_ID = ROW_ID - 1 START 
WITH ROW_ID = 1			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
