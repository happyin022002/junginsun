<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOCheckActualCodeRSQL">
			<desc><![CDATA[CheckActualCode]]></desc>
			<sql><![CDATA[
SELECT COUNT(0) AS KNT 
FROM MDM_ACTIVITY
WHERE 1=1
AND ACT_CD = @[act_cd]			]]></sql>
			<params>
				<param name="act_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
