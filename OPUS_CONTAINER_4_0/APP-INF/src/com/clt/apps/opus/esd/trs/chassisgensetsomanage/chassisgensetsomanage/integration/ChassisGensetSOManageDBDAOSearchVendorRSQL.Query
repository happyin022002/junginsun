<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisGensetSOManageDBDAOSearchVendorRSQL">
			<desc><![CDATA[Vendor List Retrieve]]></desc>
			<sql><![CDATA[
SELECT vndr_seq AS vndr_no
      ,vndr_lgl_eng_nm AS vndr_nm_eng
	  ,NVL(b.curr_cd,a.PAY_CURR_CD) AS vndr_cnt_curr_cd
      ,NVL(A.WO_EDI_USE_FLG, 'N') AS wo_edi_use_flg
  FROM mdm_vendor a
      ,mdm_currency b
 WHERE a.vndr_seq = @[VNDR_SEQ]
   AND a.vndr_cnt_cd = b.cnt_cd(+)
   AND b.delt_flg(+) = 'N'
   AND a.delt_flg = 'N'			]]></sql>
			<params>
				<param name="VNDR_SEQ" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
