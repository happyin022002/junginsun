<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WebGateDBDAOCheckDateRSQL">
			<desc><![CDATA[Date를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN SYSDATE+0.125 < TO_DATE(@[act_dt], 'YYYYMMDDHH24MI') THEN 1   
		    WHEN SYSDATE-7 > TO_DATE(@[act_dt], 'YYYYMMDDHH24MI') THEN -1   
		    ELSE 0  
       END STS_CD  
FROM DUAL			]]></sql>
			<params>
				<param name="act_dt" type="12" value="200909090909" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
