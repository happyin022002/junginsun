<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOSearchVerifyInvoiceFileImportEqNoRSQL">
			<desc><![CDATA[Container No Verify 조회]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO
      ,CNTR_TPSZ_CD
  FROM MST_CONTAINER
 WHERE CNTR_NO LIKE  CASE WHEN LENGTH(@[cntr_no]) >= 10 THEN SUBSTR(@[cntr_no], 0, 10) || '%' END
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
