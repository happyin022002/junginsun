<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAORemoveAgreementTHRPCOSTDSQL">
			<desc><![CDATA[Agmt Thrp Cost정보 삭제]]></desc>
			<sql><![CDATA[
-- tes_tml_agmt_thrp_cost 일거 같은데 DG_CGO_CLSS 만 두번 삭제..??
DELETE FROM  TES_TML_AGMT_THRP_COST 
WHERE	tml_agmt_ofc_cty_cd = @[tml_agmt_ofc_cty_cd]
AND		tml_agmt_seq		= @[tml_agmt_seq] 
AND		tml_agmt_ver_no		= @[tml_agmt_ver_no]
AND 	lgs_cost_cd			= @[lgs_cost_cd]			]]></sql>
			<params>
				<param name="tml_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_agmt_seq" type="2" value="" out="N"/>
				<param name="tml_agmt_ver_no" type="2" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
