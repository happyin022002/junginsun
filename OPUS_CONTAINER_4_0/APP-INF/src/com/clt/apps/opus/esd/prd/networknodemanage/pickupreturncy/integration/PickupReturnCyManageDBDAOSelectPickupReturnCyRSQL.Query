<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickupReturnCyManageDBDAOSelectPickupReturnCyRSQL">
			<desc><![CDATA[SelectPickupReturnCy
2010.10.1 채창호 CHM-201006135-01 Pick Up & Return CY Export 화면 수정 요청
내용: PRD_PKUP_RTN_YD 테이블의 FULL_PKUP_RTN_YD_CD 컬럼 삭제]]></desc>
			<sql><![CDATA[
SELECT POR_DEL_CD, POL_POD_CD, VSL_SLAN_CD, IO_BND_CD, SPCL_CGO_CD, MTY_PKUP_RTN_YD_CD, CNTR_TP_CD, CNTR_SZ_CD, DECODE(DELT_FLG, 'Y', 1, 0) DELT_FLG, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT
  FROM PRD_PKUP_RTN_YD
 WHERE POR_DEL_CD LIKE @[por_del_cd] || '%'
   AND POL_POD_CD LIKE @[pol_pod_cd] || '%'
   AND VSL_SLAN_CD LIKE @[vsl_slan_cd] || '%'
   AND IO_BND_CD = DECODE(@[io_bnd_cd], 'AL', IO_BND_CD, @[io_bnd_cd])
   AND SPCL_CGO_CD =  DECODE(@[spcl_cgo_cd], 'AL', SPCL_CGO_CD, @[spcl_cgo_cd])
   AND DELT_FLG =  DECODE(@[delt_flg], 'A', DELT_FLG, @[delt_flg])			]]></sql>
			<params>
				<param name="por_del_cd" type="12" value="" out="N"/>
				<param name="pol_pod_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
