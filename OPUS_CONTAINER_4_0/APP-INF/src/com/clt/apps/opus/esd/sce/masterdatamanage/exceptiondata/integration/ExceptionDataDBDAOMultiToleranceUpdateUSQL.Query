<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExceptionDataDBDAOMultiToleranceUpdateUSQL">
			<desc><![CDATA[update data]]></desc>
			<sql><![CDATA[
update SCE_EXPT_TOL
	set FOML_TM_DY = @[r_foml_tm_dys]
	,FOML_TM_HR = @[r_foml_tm_hrs]
	,FOML_TM_MNT = @[r_foml_tm_min]
	,UPD_USR_ID = @[r_usr_id]
	,UPD_DT = sysdate
	,ACT_FLG = @[r_act_flg]
where 1=1

#if(${r_expt_tp} == '10000000' || ${r_expt_tp} == '40000000')
and EXPT_CD = @[r_to_act]
#else
and EXPT_CD = @[r_fm_act]
#end

#if(${r_loc_cd} == '')
and YD_CD = @[r_nod_cd]
#else
and YD_CD = @[r_loc_cd]
#end			]]></sql>
			<params>
				<param name="r_foml_tm_dys" type="12" value="" out="N"/>
				<param name="r_foml_tm_hrs" type="12" value="" out="N"/>
				<param name="r_foml_tm_min" type="12" value="" out="N"/>
				<param name="r_usr_id" type="12" value="" out="N"/>
				<param name="r_act_flg" type="12" value="" out="N"/>
				<param name="r_to_act" type="12" value="" out="N"/>
				<param name="r_fm_act" type="12" value="" out="N"/>
				<param name="r_nod_cd" type="12" value="" out="N"/>
				<param name="r_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
