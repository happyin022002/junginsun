<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOSelectMdmYardRSQL">
			<desc><![CDATA[ManualInputDBDAOSelectMdmYard]]></desc>
			<sql><![CDATA[
SELECT YD_CD, YD_NM, LOC_CD 
  FROM MDM_YARD 
WHERE 1 = 1
#if (${frm_yd_cd} != '') 
  AND YD_CD = @[frm_yd_cd]
#end			]]></sql>
			<params>
				<param name="frm_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
