<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCNRailInbondAndBillNbrRSQL">
			<desc><![CDATA[CANADIAN NATIONAL RAILROAD INBOND NBR and Rail Bill Nbr
2015.04.16 Modify WBL_NO > RAIL_CRR_REF_NO , TRS_TRSP_EDI_RAIL_ORD > TRS_TRSP_RAIL_BIL_ORD]]></desc>
			<sql><![CDATA[
select IBD_NO, RAIL_CRR_REF_NO 
from
	TRS_TRSP_RAIL_BIL_ORD
where bkg_no =@[bkg_no] and eq_no = @[cntr_no] and delt_flg = 'N'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
