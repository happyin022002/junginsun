<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDAOsearchMdmCommodityRSQL">
			<desc><![CDATA[searchMdmCommodity]]></desc>
			<sql><![CDATA[
SELECT												
	B.rep_cmdt_cd, 												
	B.rep_cmdt_nm, 												
	A.cmdt_cd,													
	A.cmdt_nm													
FROM mdm_commodity A, mdm_rep_cmdt B
WHERE 1=1

#if (${rep_cmdt_cd} != '') 
	AND B.rep_cmdt_cd = @[rep_cmdt_cd]
#end

#if (${cmdt_cd} != '') 
	AND A.cmdt_cd = @[cmdt_cd]
#end

	AND A.rep_cmdt_cd=B.rep_cmdt_cd			]]></sql>
			<params>
				<param name="rep_cmdt_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
