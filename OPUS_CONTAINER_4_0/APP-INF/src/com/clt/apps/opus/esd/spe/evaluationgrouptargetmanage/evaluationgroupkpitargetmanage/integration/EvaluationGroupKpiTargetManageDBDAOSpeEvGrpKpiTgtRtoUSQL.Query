<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupKpiTargetManageDBDAOSpeEvGrpKpiTgtRtoUSQL">
			<desc><![CDATA[SpeEvGrpKpiTgtRtoVOUSQL.Query]]></desc>
			<sql><![CDATA[
UPDATE SPE_EV_GRP_KPI_TGT_RTO SET 
	KPI_UT_CD = @[kpi_ut_cd]
,	KPI_TGT_RTO = @[kpi_tgt_rto]
,	KPI_WGT_RTO = @[kpi_wgt_rto]
,	KPI_PERF_RTO = @[kpi_perf_rto]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT =SYSDATE
WHERE	EG_ID = substr(@[eg_id],1,5)
AND	EG_ID_SEQ = TO_NUMBER(substr(@[eg_id],7,3))
AND	SP_KPI_CD = @[sp_kpi_cd]
AND	EV_YR = @[ev_yr]			]]></sql>
			<params>
				<param name="kpi_ut_cd" type="12" value="" out="N"/>
				<param name="kpi_tgt_rto" type="6" value="" out="N"/>
				<param name="kpi_wgt_rto" type="6" value="" out="N"/>
				<param name="kpi_perf_rto" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="sp_kpi_cd" type="12" value="" out="N"/>
				<param name="ev_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
