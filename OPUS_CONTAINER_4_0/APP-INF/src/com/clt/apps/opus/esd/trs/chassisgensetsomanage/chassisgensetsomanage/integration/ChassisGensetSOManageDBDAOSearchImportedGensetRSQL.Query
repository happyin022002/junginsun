<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisGensetSOManageDBDAOSearchImportedGensetRSQL">
			<desc><![CDATA[Imported Genset List Inquiry
f_cmd ; SEARCH07]]></desc>
			<sql><![CDATA[
SELECT   a.eq_no 
        ,a.eq_tpsz_cd
        ,a.eq_tpsz_cd chss_tpsz_desc
        ,SUBSTR (a.onh_yd_cd, 1, 5) fm_loc_value
        ,SUBSTR (a.onh_yd_cd, 6, 2) fm_yard_value
        ,a.vndr_seq
        ,c.vndr_abbr_nm
        ,a.agmt_lstm_cd
    FROM cgm_equipment a
        ,mdm_vendor c
   WHERE a.vndr_seq = c.vndr_seq(+)
     AND a.eq_knd_cd = 'G'
     AND a.eq_no IN (
#foreach(${key} in ${eq_no})  
	#if($velocityCount == 1)
        'key'
  	#else 
       ,'key'
  	#end
#end
  )
ORDER BY a.eq_no			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
