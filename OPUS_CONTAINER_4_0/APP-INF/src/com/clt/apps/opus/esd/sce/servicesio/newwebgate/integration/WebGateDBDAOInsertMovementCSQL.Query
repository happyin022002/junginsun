<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WebGateDBDAOInsertMovementCSQL">
			<desc><![CDATA[Container Movement 상태를 업데이트 한다.]]></desc>
			<sql><![CDATA[
INSERT ALL
INTO SCE_ACT_RCV_IF (
	    ACT_RCV_DT
       ,ACT_RCV_NO
       ,BKG_NO
       ,CNTR_NO
       ,ACT_DT
       ,ACT_STS_MAPG_CD
       ,NOD_CD
       ,ACT_RCV_TP_CD
       ,ACT_UMCH_TP_CD
	   ,VNDR_SEQ
       ,ACT_GDT
       ,ACT_DAT_RCV_DT
       ,VNDR_NM
	   ,CRE_USR_ID
       ,CRE_DT
	   ,UPD_USR_ID
	   ,UPD_DT
) VALUES( 
	    TO_CHAR(SYSDATE, 'YYYYMMDD')
       ,SCE_ACT_RCV_IF_SEQ1.NEXTVAL
       ,@[bkg_no]
       ,@[cntr_no]
       ,TO_DATE(@[act_dt],'YYYYMMDDHH24MI')
       ,@[act_cd]
       ,@[nod_cd]
       ,'9'
       ,'00'
	   ,@[vndr_seq]
       ,NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[office]),SYSDATE)
       ,NVL((SELECT GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,LOC_CD)FROM MDM_VENDOR WHERE VNDR_SEQ = @[vndr_seq]), SYSDATE)
       ,@[user_id]||'(e-Tracker)'
	   ,@[user_id]
       ,SYSDATE
	   ,@[user_id]
       ,SYSDATE
)
INTO SCE_SVC_PTAL_ACT_IF (
	    ACT_RCV_DT
       ,ACT_RCV_NO
       ,BKG_NO
       ,CNTR_NO
       ,ACT_DT
       ,ACT_STS_MAPG_CD
       ,NOD_CD
       ,ACT_RCV_TP_CD
       ,ACT_UMCH_TP_CD
	   ,VNDR_SEQ
       ,ACT_GDT
       ,ACT_DAT_RCV_DT
       ,VNDR_NM
       ,CRE_USR_ID
       ,CRE_DT
	   ,UPD_USR_ID
	   ,UPD_DT
) VALUES( 
	    TO_CHAR(SYSDATE, 'YYYYMMDD')
       ,SCE_SVC_PTAL_ACT_IF_SEQ1.NEXTVAL
       ,@[bkg_no]
       ,@[cntr_no]
       ,TO_DATE(@[act_dt],'YYYYMMDDHH24MI')
       ,@[act_cd]
       ,@[nod_cd]
       ,'9'
       ,'00'
	   ,@[vndr_seq]
       ,NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[office]),SYSDATE)
       ,NVL((SELECT GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,LOC_CD)FROM MDM_VENDOR WHERE VNDR_SEQ = @[vndr_seq]), SYSDATE)
       ,@[user_id]||'(e-Tracker)'
	   ,@[user_id]
       ,SYSDATE
	   ,@[user_id]
       ,SYSDATE
)
SELECT * FROM DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="act_cd" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="office" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
