<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOVerifyRefInvNoRSQL">
			<desc><![CDATA[VerifyRefInvNo]]></desc>
			<sql><![CDATA[
SELECT INV_NO
      ,INV_VNDR_SEQ
      ,REF_INV_NO
  FROM TRS_TRSP_SVC_ORD
 WHERE INV_VNDR_SEQ = @[inv_vndr_seq]
   AND REF_INV_NO = @[ref_inv_no]			]]></sql>
			<params>
				<param name="inv_vndr_seq" type="12" value="" out="N"/>
				<param name="ref_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
