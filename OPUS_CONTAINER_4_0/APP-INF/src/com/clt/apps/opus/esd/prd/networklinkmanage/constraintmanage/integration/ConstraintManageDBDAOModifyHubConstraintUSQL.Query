<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOModifyHubConstraintUSQL">
			<desc><![CDATA[ConstraintManageDBDAOModifyHubConstraint]]></desc>
			<sql><![CDATA[
UPDATE PRD_HUB_LOC_CNST_MGMT
   SET SVC_USE_FLG      = @[svc_use_flg]
      ,HUB_LOC_CNST_RMK = @[hub_loc_cnst_rmk]
      ,CNTR_TP_CD       = @[cntr_tp_cd]
      ,CNTR_SZ_CD       = @[cntr_sz_cd]
      ,UPD_USR_ID       = @[upd_usr_id]
      ,UPD_DT           = SYSDATE
 WHERE PORT_CD          = @[port_cd]
   AND HUB_LOC_CD       = @[hub_loc_cd]
   AND NOD_CD           = @[nod_cd]
   AND IO_BND_CD        = @[io_bnd_cd]
   AND SPCL_CGO_CNTR_TP_CD = @[spcl_cgo_cntr_tp_cd]
   AND CNST_LANE_CD        = @[cnst_lane_cd]
   AND DIR_CD              = @[dir_cd]			]]></sql>
			<params>
				<param name="svc_use_flg" type="12" value="" out="N"/>
				<param name="hub_loc_cnst_rmk" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="hub_loc_cd" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cnst_lane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
