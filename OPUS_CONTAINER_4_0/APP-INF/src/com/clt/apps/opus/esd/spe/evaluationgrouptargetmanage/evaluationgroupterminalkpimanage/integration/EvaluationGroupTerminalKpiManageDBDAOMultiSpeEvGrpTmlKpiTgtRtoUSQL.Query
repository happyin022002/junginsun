<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupTerminalKpiManageDBDAOMultiSPE_EV_GRP_TML_KPI_TGT_RTOUSQL">
			<desc><![CDATA[spe_ev_grp_tml_kpi_tgt_rto Update]]></desc>
			<sql><![CDATA[
UPDATE SPE_EV_GRP_TML_KPI_TGT_RTO SET 
	KPI_UT_CD = DECODE(@[kpi_ut_cd],'%','PC','PT','PO','MM','MM')
,	KPI_TGT_RTO = @[kpi_tgt_rto]
,	KPI_WGT_RTO = @[kpi_wgt_rto]
,	KPI_PERF_RTO = @[kpi_perf_rto]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate 
WHERE	EG_ID = @[eg_id]
AND	EG_ID_SEQ = TO_NUMBER(@[eg_id_seq])
AND	EV_YR = @[ev_yr]
AND	YD_CD = UPPER(@[yd_cd])
AND VNDR_SEQ = TO_NUMBER(@[vndr_seq])			]]></sql>
			<params>
				<param name="kpi_ut_cd" type="12" value="" out="N"/>
				<param name="kpi_tgt_rto" type="6" value="" out="N"/>
				<param name="kpi_wgt_rto" type="6" value="" out="N"/>
				<param name="kpi_perf_rto" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="eg_id_seq" type="2" value="" out="N"/>
				<param name="ev_yr" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
