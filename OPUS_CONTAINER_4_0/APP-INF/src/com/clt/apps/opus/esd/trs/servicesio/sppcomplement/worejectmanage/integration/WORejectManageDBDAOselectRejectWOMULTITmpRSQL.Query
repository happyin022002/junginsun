<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WORejectManageDBDAOselectRejectWOMULTITmpRSQL">
			<desc><![CDATA[selectRejectWOMULTITmp]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 'MULTI01' as f_cmd,                                                                                                                                                   
	  WO_PRV_GRP_SEQ,                                                                                                                                                   	
	  WO_ISS_NO,                                                                                                                                                   			
	  DECODE(WO_FMT_TP_CD,'NC','WO_NORMAL','CC','WO_COMBINED1','IB','WO_COMBINED2_1','CY','WO_COMBINED2_2','CM','WO_COMBINED2_3','MM','WO_EMPTY')||'.mrd'  AS FAX_APP_CD,   
	  '[]['||WO_PRV_GRP_SEQ||']['||WO_ISS_NO||'][SYSTEM][SPP_IF]' AS FAX_PARAM,                                                                                             
	  VNDR_LGL_ENG_NM AS FAX_RCV_INFO                                                                                                                                       
	FROM                                                                                                                                                                    
	  TRS_TRSP_WRK_ORD_PRV_TMP WO,                                                                                                                                          
	  MDM_VENDOR VD                                                                                                                                                         
	WHERE                                                                                                                                                                   
	  WO.VNDR_SEQ = VD.VNDR_SEQ                                                                                                                                             
	  AND WO_PRV_GRP_SEQ = @[WO_PRV_GRP_SEQ]
	  AND WO_ISS_NO = @[WO_ISS_NO]			]]></sql>
			<params>
				<param name="SELECT DISTINCT 'MULTI01' as f_cmd,                                                                                                                                                   
	  WO_PRV_GRP_SEQ,                                                                                                                                                   	
	  WO_ISS_NO,                                                                                                                                                   			
	  DECODE(WO_FMT_TP_CD,'NC','WO_NORMAL','CC','WO_COMBINED1','IB','WO_COMBINED2_1','CY','WO_COMBINED2_2','CM','WO_COMBINED2_3','MM','WO_EMPTY')||'.mrd'  AS FAX_APP_CD,   
	  '[" type="12" value="" out="N"/>
				<param name="WO_PRV_GRP_SEQ" type="12" value="" out="N"/>
				<param name="WO_ISS_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
