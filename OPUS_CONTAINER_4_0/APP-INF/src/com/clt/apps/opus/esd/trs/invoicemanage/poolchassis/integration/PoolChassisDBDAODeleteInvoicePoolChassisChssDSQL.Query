<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisDBDAODeleteInvoicePoolChassisChssDSQL">
			<desc><![CDATA[Pool Chss Invoice 테이블의 자료를 삭제한다.]]></desc>
			<sql><![CDATA[
 DELETE FROM TRS_TRSP_POOL_CHSS_INV 
  WHERE INV_NO = @[inv_no]			      
    AND INV_VNDR_SEQ = @[paymt_sp_cd]				]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="paymt_sp_cd" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
