<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOModifySoStsPlnByUmchSoUSQL">
			<desc><![CDATA[SCE_PLN_SO_LIST 의 SO Status 가 Planned 가 아닐 경우 해당 route 에 unmatched S/O 가 생성되었을 때 이를 다시 Planned 로 복구]]></desc>
			<sql><![CDATA[
update sce_pln_so_list
set
trsp_so_sts_cd = 'P'
where
cop_no = @[cop_no]
and cost_act_grp_seq = @[cost_act_grp_seq]
and trsp_so_sts_cd != 'P'
and (select nvl(rpln_umch_flg, 'N') 
		from trs_trsp_svc_ord where cop_no = @[cop_no] and cost_act_grp_seq = @[cost_act_grp_seq]
		and NVL(trsp_frst_flg, 'N') = 'N' and nvl(delt_flg, 'N') = 'N' and rownum = 1
	) = 'Y'				]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
