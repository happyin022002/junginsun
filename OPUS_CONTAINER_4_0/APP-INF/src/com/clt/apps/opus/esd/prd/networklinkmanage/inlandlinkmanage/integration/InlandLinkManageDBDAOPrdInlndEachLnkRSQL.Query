<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandLinkManageDBDAOPrdInlndEachLnkRSQL">
			<desc><![CDATA[PrdInlndEachLnk]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(l.lnk_org_nod_cd, 1, 5) org_loc
      ,SUBSTR(l.lnk_org_nod_cd, 6) org_type
      ,SUBSTR(l.lnk_dest_nod_cd, 1, 5) dest_loc
      ,SUBSTR(l.lnk_dest_nod_cd, 6) dest_type
      ,l.trsp_mod_cd
      ,l.vndr_seq
      ,v.vndr_cnt_cd
      ,(v.vndr_cnt_cd || l.vndr_seq) sp_cd
      ,v.VNDR_LGL_ENG_NM vndr_name
      ,trim(to_char(trunc(l.tztm_hrs / 24, 0), '00')) || trim(to_char(mod(l.tztm_hrs, 24), '00')) fmt_tztm_hrs
      ,l.lnk_dist
      ,l.dist_ut_cd
      ,l.rail_crr_tp_cd
      ,l.lnk_org_nod_cd
      ,l.lnk_dest_nod_cd
      ,l.tztm_hrs
      ,CASE
         WHEN SUBSTR(l.lnk_org_nod_cd, 1, 2) IN ('US', 'CA') AND SUBSTR(l.lnk_dest_nod_cd, 1, 2) IN ('US', 'CA') AND l.trsp_mod_cd = 'RD' THEN 'T'
         ELSE 'F'
       END fc
      ,a.TRSP_AGMT_OFC_CTY_CD
      ,a.TRSP_AGMT_SEQ
      ,decode(nvl(l.TRSP_AGMT_OFC_CTY_CD, '000'), '000', '', l.TRSP_AGMT_OFC_CTY_CD || lpad(l.TRSP_AGMT_SEQ, 6, '0')) agmt_no
      ,a.AGMT_REF_NO
      ,CASE
         WHEN l.TRSP_AGMT_OFC_CTY_CD IS NULL AND l.TRSP_AGMT_SEQ IS NULL THEN 'NOT'
         WHEN l.TRSP_AGMT_OFC_CTY_CD IS NOT NULL AND l.TRSP_AGMT_SEQ IS NOT NULL THEN
          CASE
            WHEN NVL(l.TRSP_AGMT_OFC_CTY_CD, 'X') = NVL(A.TRSP_AGMT_OFC_CTY_CD, 'X') AND NVL(to_char(l.TRSP_AGMT_SEQ), 'X') = NVL(to_char(A.TRSP_AGMT_SEQ), 'X') AND l.vndr_seq <> AV.VNDR_SEQ THEN 'OK'
            ELSE 'NOT'
          END
         ELSE 'NOT'
       END UNMATCH
  FROM prd_inlnd_each_lnk l
      ,mdm_vendor         v
      ,TRS_AGMT_HDR       a
      ,TRS_AGMT_APLY_VNDR av
 where l.vndr_seq = v.vndr_seq(+)
   and l.TRSP_AGMT_OFC_CTY_CD = a.TRSP_AGMT_OFC_CTY_CD(+)
   AND l.TRSP_AGMT_SEQ = a.TRSP_AGMT_SEQ(+)
   and l.TRSP_AGMT_OFC_CTY_CD = AV.TRSP_AGMT_OFC_CTY_CD(+)
   AND l.TRSP_AGMT_SEQ = AV.TRSP_AGMT_SEQ(+)
   AND l.VNDR_SEQ = AV.VNDR_SEQ(+)
   and l.lnk_org_nod_cd LIKE @[i_org_cd] || '%'
   AND l.lnk_dest_nod_cd LIKE @[i_dest_cd] || '%'
   AND nvl(l.TRSP_AGMT_OFC_CTY_CD, 'X') like @[cty_cd] || '%'
   AND nvl(TO_CHAR(l.TRSP_AGMT_SEQ), 'X') LIKE decode(@[agmt_seq], '0', '', @[agmt_seq]) || '%'
   AND nvl(l.delt_flg, 'N') <> 'Y'
   AND l.VNDR_SEQ <> '6257'			]]></sql>
			<params>
				<param name="i_org_cd" type="12" value="" out="N"/>
				<param name="i_dest_cd" type="12" value="" out="N"/>
				<param name="cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
