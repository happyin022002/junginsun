<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOCheckSumLineAmountTotalAmountRSQL">
			<desc><![CDATA[Total Amt와 Line Amt 비교한다]]></desc>
			<sql><![CDATA[
SELECT  																								
			DECODE(		NVL(	(SELECT (H.CSR_AMT)     FROM AP_INV_HDR  H WHERE H.CSR_NO = @[CSR_NO]), 0)		
				, 	NVL(	(SELECT  SUM(D.INV_AMT) FROM AP_INV_DTRB D WHERE D.CSR_NO = @[CSR_NO]), 1)		
				, 	'Y'																				
				, 	'N'																				
			  )																						
FROM    	DUAL																						
			]]></sql>
			<params>
				<param name="CSR_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
