<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeManageDBDAOMultiTPBOfficeUSQL">
			<desc><![CDATA[MultiTPBOffice]]></desc>
			<sql><![CDATA[
MERGE INTO tpb_hndl_ofc a 
    USING DUAL 
    ON ( a.n3pty_ofc_tp_cd = @[n3pty_ofc_tp_cd] 
        AND a.ofc_cd = @[ofc_cd] ) 
WHEN MATCHED THEN        
    UPDATE 
    SET rhq_cd = @[rhq_cd]
          ,n3pty_ctrl_ofc_cd = @[n3pty_ctrl_ofc_cd]
          ,n3pty_ofc_cd = @[n3pty_ofc_cd]
          ,n3pty_ar_ofc_cd = @[n3pty_ar_ofc_cd]
          ,delt_flg = @[delt_flg]
          ,upd_usr_id = @[upd_usr_id]
          ,upd_dt= SYSDATE
    WHERE n3pty_ofc_tp_cd = @[n3pty_ofc_tp_cd]
    AND ofc_cd = @[ofc_cd]
          
WHEN NOT MATCHED THEN 
    INSERT 
                (
                 n3pty_ofc_tp_cd 
                ,ofc_cd
                ,rhq_cd
                ,n3pty_ctrl_ofc_cd
                ,n3pty_ofc_cd
                ,n3pty_ar_ofc_cd
                ,delt_flg
                ,cre_usr_id
                ,cre_dt
                ,upd_usr_id
                ,upd_dt
                )
         VALUES (
                 @[n3pty_ofc_tp_cd]
                ,@[ofc_cd]
                ,@[rhq_cd]
                ,@[n3pty_ctrl_ofc_cd]
                ,@[n3pty_ofc_cd]
                ,@[n3pty_ar_ofc_cd]
                ,@[delt_flg]
                ,@[upd_usr_id]
                ,SYSDATE
                ,@[upd_usr_id]
                ,SYSDATE
                )			]]></sql>
			<params>
				<param name="n3pty_ofc_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="n3pty_ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="n3pty_ofc_cd" type="12" value="" out="N"/>
				<param name="n3pty_ar_ofc_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
