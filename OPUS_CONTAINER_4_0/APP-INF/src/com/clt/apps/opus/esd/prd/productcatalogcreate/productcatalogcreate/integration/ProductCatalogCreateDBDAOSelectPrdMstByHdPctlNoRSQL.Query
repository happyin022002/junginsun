<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProductCatalogCreateDBDAOSelectPrdMstByHdPctlNoRSQL">
			<desc><![CDATA[SelectPrdMstByHdPctlNo]]></desc>
			<sql><![CDATA[
SELECT * 
FROM PRD_PROD_CTL_MST D 
WHERE d.PCTL_NO like @[hd_pctl_no]||'%'			]]></sql>
			<params>
				<param name="hd_pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
