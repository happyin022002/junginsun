<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExceptionAckRailRoadDBDAOMultiExceptionAckRailRoadVendorCSQL">
			<desc><![CDATA[ExceptionAckRailRoadDBDAOMultiExceptionAckRailRoadVendor]]></desc>
			<sql><![CDATA[
MERGE INTO TRS_EXPT_ACK_RAIL_VNDR A
USING DUAL
ON (A.EXPT_ACK_RAIL_VNDR_SEQ = @[expt_ack_rail_vndr_seq])
WHEN MATCHED THEN
  UPDATE SET DELT_FLG   = DECODE(@[delt_flg], '1' , 'Y', 'N')
  DELETE WHERE  (A.EXPT_ACK_RAIL_VNDR_SEQ = @[expt_ack_rail_vndr_seq])
WHEN NOT MATCHED THEN
  INSERT (
	 EXPT_ACK_RAIL_VNDR_SEQ
    ,VNDR_SEQ
    ,DELT_FLG
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
  ) VALUES (
     @[expt_ack_rail_vndr_seq]
    ,@[vndr_seq]
    ,'N'
    ,@[cre_usr_id]
    ,SYSDATE
    ,@[upd_usr_id]
    ,SYSDATE
  )			]]></sql>
			<params>
				<param name="expt_ack_rail_vndr_seq" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
