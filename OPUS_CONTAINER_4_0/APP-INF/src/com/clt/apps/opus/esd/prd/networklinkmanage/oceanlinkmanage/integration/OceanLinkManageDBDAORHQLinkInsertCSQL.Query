<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanLinkManageDBDAORHQLinkInsertCSQL">
			<desc><![CDATA[RHQLinkInsert]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_FDR_LNK ( 
    VSL_SLAN_CD, LNK_ORG_LOC_CD, LNK_DEST_LOC_CD,   
    TZTM_HRS, FDR_FREQ_KNT, VNDR_SEQ,   
    PCTL_IO_BND_CD, CRE_OFC_CD, LNK_RMK,   
    CRE_USR_ID, CRE_DT, DELT_FLG,   
    UPD_USR_ID, UPD_DT, SKD_DIR_CD, 
    SUN_ST_FLG, MON_ST_FLG, TUE_ST_FLG, WED_ST_FLG, 
    THU_ST_FLG, FRI_ST_FLG, SAT_ST_FLG)  					
VALUES ( 
    @[s_lane], @[s_from], @[s_to], 
    TO_NUMBER(@[s_t_time]), TO_NUMBER(@[s_fqc]), TO_NUMBER(@[s_sp_bd]), 
    @[s_bd], @[cre_ofc_cd], '', 
    @[cre_usr_id], sysdate, 'N', 
    @[upd_usr_id], sysdate, @[s_dr], 
    DECODE(@[s_sn],'1','Y','N'), DECODE(@[s_mn],'1','Y','N'), DECODE(@[s_te],'1','Y','N'), DECODE(@[s_wb],'1','Y','N'), 
    DECODE(@[s_tu],'1','Y','N'), DECODE(@[s_fi],'1','Y','N'), DECODE(@[s_st],'1','Y','N')
)			]]></sql>
			<params>
				<param name="s_lane" type="12" value="" out="N"/>
				<param name="s_from" type="12" value="" out="N"/>
				<param name="s_to" type="12" value="" out="N"/>
				<param name="s_t_time" type="12" value="" out="N"/>
				<param name="s_fqc" type="12" value="" out="N"/>
				<param name="s_sp_bd" type="12" value="" out="N"/>
				<param name="s_bd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="s_dr" type="12" value="" out="N"/>
				<param name="s_sn" type="12" value="" out="N"/>
				<param name="s_mn" type="12" value="" out="N"/>
				<param name="s_te" type="12" value="" out="N"/>
				<param name="s_wb" type="12" value="" out="N"/>
				<param name="s_tu" type="12" value="" out="N"/>
				<param name="s_fi" type="12" value="" out="N"/>
				<param name="s_st" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
