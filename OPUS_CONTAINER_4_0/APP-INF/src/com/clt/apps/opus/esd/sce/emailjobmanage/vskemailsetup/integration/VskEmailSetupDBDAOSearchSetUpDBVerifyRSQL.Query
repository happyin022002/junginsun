<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VskEmailSetupDBDAOSearchSetUpDBVerifyRSQL">
			<desc><![CDATA[vsl_slan_cd, port_cd가 Pair로 유효한지 체크]]></desc>
			<sql><![CDATA[
SELECT
	B.VSL_SLAN_CD
FROM
	VSK_PF_SKD A, 
	VSK_PF_CALL_PORT B
WHERE A.VSL_SLAN_CD = B.VSL_SLAN_CD
AND A.PF_SVC_TP_CD = B.PF_SVC_TP_CD
AND A.SLAN_STND_FLG = 'Y' 
AND A.DELT_FLG = 'N'
AND B.VSL_SLAN_CD = @[vslSlanCd]
AND B.PORT_CD = @[toPortCd]			]]></sql>
			<params>
				<param name="vslSlanCd" type="12" value="" out="N"/>
				<param name="toPortCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
