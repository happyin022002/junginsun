<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchSubTrdCdListRSQL">
			<desc><![CDATA[Sub Trdae Code 조회]]></desc>
			<sql><![CDATA[
SELECT LTRIM(MAX(SYS_CONNECT_BY_PATH(SUB_TRD_CD,'|')),'|') AS SUB_TRD_CD 
  FROM 
       (SELECT ROWNUM ROW_ID 
            , Z.SUB_TRD_CD 
         FROM 
              (SELECT SUB_TRD_CD 
                FROM MDM_SUB_TRD 
               WHERE DELT_FLG='N' 
               ORDER BY SUB_TRD_CD 
              ) Z 
       ) 
CONNECT BY PRIOR ROW_ID = ROW_ID - 1 START WITH ROW_ID = 1			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
