<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAORegBeforeCheckAccmRSQL">
			<desc><![CDATA[Accm select]]></desc>
			<sql><![CDATA[
SELECT	a.vndr_seq
FROM	TES_TML_SO_ACCM_MZD a, TES_TML_SO_ACCM_YD b, TES_TML_SO_ACCM_COST c
WHERE	a.vndr_seq	= @[vndr_seq]
AND		b.yd_cd		= @[yd_cd]
AND		c.lgs_cost_cd = @[lgs_cost_cd]
AND		a.vndr_seq	= b.vndr_seq 
AND		a.vndr_seq	= c.vndr_seq 
AND		a.accm_seq	= b.accm_seq 
AND		a.accm_seq	= c.accm_seq 
			]]></sql>
			<params>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
