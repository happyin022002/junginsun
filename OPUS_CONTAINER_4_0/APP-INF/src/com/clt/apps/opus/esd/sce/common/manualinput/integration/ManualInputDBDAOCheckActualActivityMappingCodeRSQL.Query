<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOCheckActualActivityMappingCodeRSQL">
			<desc><![CDATA[CheckActualActivityMappingCode]]></desc>
			<sql><![CDATA[
SELECT COUNT(0) AS CHK
FROM SCE_ACT_ACT_MAPG
WHERE 1=1
AND ACT_CD = @[act_cd]
AND ACT_RCV_TP_CD = @[act_rcv_tp_cd]
AND ACT_STS_MAPG_CD = @[act_sts_mapg_cd]			]]></sql>
			<params>
				<param name="act_cd" type="12" value="" out="N"/>
				<param name="act_rcv_tp_cd" type="12" value="" out="N"/>
				<param name="act_sts_mapg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
