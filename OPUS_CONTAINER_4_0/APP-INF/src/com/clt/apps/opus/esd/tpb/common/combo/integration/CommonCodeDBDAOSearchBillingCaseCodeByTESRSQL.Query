		<query name="CommonCodeDBDAOSearchBillingCaseCodeByTESRSQL">
			<desc><![CDATA[SearchBillingCaseCodeByTES]]></desc>
			<sql><![CDATA[
SELECT n3pty_bil_tp_nm AS name,
       n3pty_bil_tp_cd AS cd
  FROM TPB_N3RD_PTY_BIL_TP
 WHERE 1=1 
   AND act_flg = 'Y' 
   AND n3pty_bil_tp_cd != 'JO'
 ORDER BY n3pty_bil_tp_cd			]]></sql>
