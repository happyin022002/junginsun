<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VskEmailSetupDBDAOSearchEmlSubSeqRSQL">
			<desc><![CDATA[Subscriber에 대한 Sequence 값을 조회]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(MAX(SUBSC_SEQ), 0) + 1 
FROM SCE_EML_JB_SUBSC
WHERE EML_JB_ID = @[emlJbId] 
AND SVC_ST_DT = @[svcStDt]
AND SVC_END_DT = '99991231' 
AND EML_GRP_ID = @[emlGrpId]
AND VSL_SLAN_CD = @[vslSlanCd]
AND TO_PORT_CD = @[toPortCd]			]]></sql>
			<params>
				<param name="emlJbId" type="12" value="" out="N"/>
				<param name="svcStDt" type="12" value="" out="N"/>
				<param name="emlGrpId" type="12" value="" out="N"/>
				<param name="vslSlanCd" type="12" value="" out="N"/>
				<param name="toPortCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
