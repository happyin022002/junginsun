<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderIssueDBDAOUpdateCYContainerNoUSQL">
			<desc><![CDATA[WorkOrderIssueDBDAOUpdateCYContainerNo]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD O
   SET O.EQ_NO       = (SELECT SCE.CNTR_NO FROM SCE_COP_HDR SCE WHERE SCE.COP_NO = O.COP_NO AND SCE.COP_STS_CD IN ('C', 'T', 'F'))
      ,O.EQ_ATCH_DT  = SYSDATE      
      ,O.LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(O.CRE_OFC_CD)
      ,O.UPD_USR_ID  = @[upd_usr_id]
      ,O.UPD_DT      = SYSDATE
 WHERE O.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND O.TRSP_SO_SEQ = @[trsp_so_seq]
   AND O.EQ_NO IS NULL
   AND O.COP_NO IS NOT NULL
   AND O.TRSP_SO_TP_CD = 'Y'
   AND O.TRSP_COST_DTL_MOD_CD IN ('CY', 'LS', 'TS')
   AND EXISTS (SELECT 1
          FROM SCE_COP_HDR SCE
         WHERE SCE.COP_NO = O.COP_NO
           AND SCE.COP_STS_CD IN ('C', 'T', 'F')
           AND SCE.CNTR_NO <> 'COMU0000000')			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
