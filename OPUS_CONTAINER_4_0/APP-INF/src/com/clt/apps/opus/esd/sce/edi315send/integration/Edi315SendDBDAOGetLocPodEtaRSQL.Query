<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetLocPodEtaRSQL">
			<desc><![CDATA[for getLocPodEta]]></desc>
			<sql><![CDATA[
select /*+ index_desc(d XPKSCE_COP_DTL) */ 
            to_char(estm_dt, 'YYYYMMDDHH24MI') 
from sce_cop_dtl 
 where 1=1 
      and cop_no = @[e_cop_no]
      and act_cd in ('FUWMAD','FUVMAD') 
      and rownum = 1			]]></sql>
			<params>
				<param name="e_cop_no" type="12" value="a" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
