<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOSearchActivityGroupRSQL">
			<desc><![CDATA[SearchActivityGroup]]></desc>
			<sql><![CDATA[
SELECT NOD_TP_CD
     , BKG_TERM_CD
     , BFR_TRSP_MOD_CD
     , AFT_TRSP_MOD_CD
     , TRSP_BND_CD
     , SPCL_NOD_TP_CD
     , COP_DTL_GRP_CD
     , (REPLACE(NOD_TP_CD,'*','A')||(REPLACE(BKG_TERM_CD,'*','A'))||(SUBSTR(BFR_TRSP_MOD_CD,1,1))||(SUBSTR(AFT_TRSP_MOD_CD,1,1))||TRSP_BND_CD||(REPLACE((SUBSTR(SPCL_NOD_TP_CD,1,1)),'P','D'))) AS COP_DTL_GRP_CD_COMBO
	 , '' AS COMBO_DUMMY_COL
FROM SCE_ACT_GRP
ORDER BY NOD_TP_CD, BKG_TERM_CD, BFR_TRSP_MOD_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
