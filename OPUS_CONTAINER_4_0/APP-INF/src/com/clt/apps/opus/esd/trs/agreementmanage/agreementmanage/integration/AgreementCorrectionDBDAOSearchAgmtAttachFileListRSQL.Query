<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementCorrectionDBDAOSearchAgmtAttachFileListRSQL">
			<desc><![CDATA[Agreement Attach File List를 가져오는 SQL]]></desc>
			<sql><![CDATA[
SELECT IMG.TRSP_AGMT_OFC_CTY_CD||IMG.TRSP_AGMT_SEQ AS AGMT_NO,
       IMG.IMG_SEQ AS IMG_SEQ,
       IMG.FILE_NM AS FILE_NM,
       UPLD.FILE_SZ_CAPA AS FILE_SIZE,
       IMG.FILE_PATH_RMK AS FILE_PATH_RMK,
       IMG.FILE_SAV_ID AS FILE_SAV_ID
  FROM TRS_AGMT_IMG_STO IMG,
       COM_UPLD_FILE UPLD
 WHERE IMG.TRSP_AGMT_OFC_CTY_CD = SUBSTR(@[agmt_no], 1, 3)
   AND IMG.TRSP_AGMT_SEQ = SUBSTR(@[agmt_no], 4)
   AND IMG.FILE_SAV_ID = UPLD.FILE_SAV_ID(+)			]]></sql>
			<params>
				<param name="agmt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
