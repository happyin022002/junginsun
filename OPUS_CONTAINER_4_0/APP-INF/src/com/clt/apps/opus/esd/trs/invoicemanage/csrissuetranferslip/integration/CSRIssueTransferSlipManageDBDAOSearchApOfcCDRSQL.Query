<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOSearchApOfcCDRSQL">
			<desc><![CDATA[CSR Ap Office Cd 조회]]></desc>
			<sql><![CDATA[
SELECT		NVL(AP_OFC_CD	, '') 	AP_OFC_CD                   
		,	NVL(FINC_RGN_CD	, '') 	FINC_RGN_CD                 
		,	NVL(AP_CTR_CD	, '') 	AP_CTR_CD					
FROM		MDM_ORGANIZATION               						
WHERE		OFC_CD	= @[OFC_CD]
			AND		NVL(DELT_FLG,'N') = 'N'				]]></sql>
			<params>
				<param name="OFC_CD" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
