<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerEdiDBDAOGetMyPerformanceSelectRSQL">
			<desc><![CDATA[GetMyPerformanceSelect]]></desc>
			<sql><![CDATA[
SELECT EDI_GRP_CD A, 
       CUST_TRD_PRNR_ID B,
       EDI_GRP_DESC C,EDI_CGO_RMK D 
       FROM( SELECT CUST.EDI_GRP_CD , 
                    CUST.CUST_TRD_PRNR_ID , 
                    CUST.EDI_GRP_DESC , 
                    CUST.EDI_CGO_RMK ,
                    STS.EDI_STND_STS_CD, 
                    STS.CUST_EDI_STS_CD ,
                    ROWNUM RNUM FROM EDI_USR_CUST CUST, EDI_USR_STS STS 
                    WHERE CUST.CRE_USR_ID = STS.CRE_USR_ID 
                    AND CUST.EDI_GRP_CD = STS.EDI_GRP_CD 
                    AND CUST.EDI_STS_SEQ = STS.EDI_STS_SEQ 
#if(${user_id} != '')
                    AND CUST.CRE_USR_ID = @[user_id]
#end 
                    AND CUST.EDI_STS_SEQ = 2 ) 
                    GROUP BY EDI_GRP_CD, CUST_TRD_PRNR_ID,EDI_GRP_DESC,EDI_CGO_RMK			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
