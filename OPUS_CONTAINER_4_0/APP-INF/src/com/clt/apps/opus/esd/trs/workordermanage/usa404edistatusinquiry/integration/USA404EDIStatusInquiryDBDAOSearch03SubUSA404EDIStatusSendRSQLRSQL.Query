<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USA404EDIStatusInquiryDBDAOSearch03SubUSA404EDIStatusSendRSQLRSQL">
			<desc><![CDATA[Danger Cargo정보, Reefer Cargo 조회]]></desc>
			<sql><![CDATA[
SELECT STCC.STCC_CD DG_STCC
      ,DG.IMDG_UN_NO DG_UN
      ,DECODE(IMDG.CFR_FLG, 'Y', 'NA' || IMDG.IMDG_UN_NO) CFR_NBR
      ,DG.PRP_SHP_NM DG_NAME
      ,DG.HZD_DESC DG_TECH
      ,DG.HZD_DESC DG_CONTENTS
      ,DG.IMDG_CLSS_CD DG_CLASS
      ,REGEXP_REPLACE((NVL2(DG.IMDG_SUBS_RSK_LBL_CD1, DG.IMDG_SUBS_RSK_LBL_CD1 || ',' , '' ) 
       || NVL2(DG.IMDG_SUBS_RSK_LBL_CD2, DG.IMDG_SUBS_RSK_LBL_CD2 || ',' , '' ) 
       || NVL2(DG.IMDG_SUBS_RSK_LBL_CD3, DG.IMDG_SUBS_RSK_LBL_CD3 || ',' , '' ) 
       || NVL2(DG.IMDG_SUBS_RSK_LBL_CD4, DG.IMDG_SUBS_RSK_LBL_CD4 || ',' , '' )), ',{1}$')  AS DG_CLASS2
--      ,(SELECT LISTAGG(IMDG_SUBS_RSK_LBL_CD, ',') WITHIN GROUP(ORDER BY IMDG_SUBS_RSK_LBL_CD) FROM SCG_IMDG_SUBS_RSK_LBL  WHERE IMDG_UN_NO = DG.IMDG_UN_NO AND IMDG_UN_NO_SEQ = DG.IMDG_UN_NO_SEQ) AS DG_CLASS2
      ,IMDG.CFR_PSN_INH_ZN_CD AS DG_ZONE
      ,DG.RSD_FLG DG_RESIDUE
      ,DG.ERAP_NO AS DG_ERAP_CD
      ,DG.ERAP_CNTC_NO AS DG_ERAP_TEL
      ,STCC.PCK_GRP_VAL_CD DG_PGRP
      ,DG.PSA_NO DG_PSAGRP
      ,DG.MRN_POLUT_FLG DG_MP
      ,DG.FLSH_PNT_CDO_TEMP DG_FLSHTEMP
      ,'C' DG_FLSHUNIT
      ,NVL(DG.OUT_IMDG_PCK_QTY1, 0) DG_QTY
      ,DG.OUT_IMDG_PCK_CD1 DG_QUNIT
      ,NVL(DG.GRS_WGT, 0) DG_WEIGHT
      ,NVL(DG.WGT_UT_CD, 'KGS') DG_WUNIT
      ,'' DG_CLASS1NEW
      ,'' DG_CLASS1NEWUNIT
      ,DG.IMDG_LMT_QTY_FLG DG_LIMIT
      ,IMDG.IMDG_LMT_QTY DG_LIMIT_QTY
      ,DG.DOT_AUTH_NO
      ,DG.DOT_SPCL_APRO_NO
      ,DG.DOT_EXP_NO
      ,DG.EMER_CNTC_PHN_NO_CTNT AS DG_TEL
      ,DG.EMER_CNTC_PSON_NM AS DG_CONTACT
   	  ,(SELECT DE.DECL_NM FROM BKG_DG_DECL DE WHERE DE.BKG_NO = DG.BKG_NO AND DE.CNTR_NO = DG.CNTR_NO AND DE.DECL_NM IS NOT NULL AND ROWNUM = 1) AS DG_DECLARANT
      ,DG.IMDG_UN_NO_SPCL_PROVI_CTNT AS DG_SPCL_PROVISION
  FROM BKG_DG_CGO DG
      ,SCG_IMDG_UN_NO IMDG
      ,(SELECT BKG_NO
              ,DCGO_SEQ
              ,STCC_CD
              ,STCC_SEQ
              ,PCK_GRP_VAL_CD
          FROM (SELECT D.BKG_NO
                      ,D.DCGO_SEQ
                      ,STCC.STCC_CD
                      ,STCC.STCC_SEQ
                      ,STCC.PCK_GRP_VAL_CD
                      ,ROW_NUMBER() OVER(PARTITION BY D.BKG_NO, D.DCGO_SEQ ORDER BY STCC.STCC_CD, STCC.STCC_SEQ) RN
                  FROM BKG_DG_CGO            D
                      ,TRS_STCC              STCC
                      ,TRS_TRSP_RAIL_BIL_ORD SO
                 WHERE SO.BKG_NO = D.BKG_NO
                   AND SO.EQ_NO = D.CNTR_NO
                   AND SO.EQ_TPSZ_CD = D.CNTR_TPSZ_CD
		           AND D.IMDG_UN_NO = NVL(STCC.UN_CMDT_CD, D.IMDG_UN_NO)
		           AND D.IMDG_UN_NO_SEQ = NVL(STCC.IMDG_UN_NO_SEQ, D.IMDG_UN_NO_SEQ)
		           AND D.IMDG_CLSS_CD = NVL(STCC.HZD_MTRL_CLSS_CD, D.IMDG_CLSS_CD)
		           AND NVL(D.IMDG_PCK_GRP_CD, 'XX') = NVL2(D.IMDG_PCK_GRP_CD, NVL(STCC.PCK_GRP_CD, D.IMDG_PCK_GRP_CD), 'XX')
		           AND UPPER(REPLACE(TRIM(D.PRP_SHP_NM), ' ', '')) = UPPER(REPLACE(TRIM(STCC.PRP_SHP_NM), ' ', ''))
                   AND STCC.DELT_FLG = 'N'
                   AND D.BKG_NO = @[dgBkgNo]
                   AND D.CNTR_NO = @[dgEqNo])
         WHERE RN = 1) STCC
 WHERE DG.IMDG_UN_NO = IMDG.IMDG_UN_NO(+)
   AND DG.IMDG_UN_NO_SEQ = IMDG.IMDG_UN_NO_SEQ(+)
   AND DG.BKG_NO = STCC.BKG_NO(+)
   AND DG.DCGO_SEQ = STCC.DCGO_SEQ(+)
   AND DG.BKG_NO = @[dgBkgNo]
   AND DG.CNTR_NO = @[dgEqNo]
 ORDER BY DG.CNTR_CGO_SEQ			]]></sql>
			<params>
				<param name="dgBkgNo" type="12" value="" out="N"/>
				<param name="dgEqNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
