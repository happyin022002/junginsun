<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchEQNoRSQL">
			<desc><![CDATA[SearchEQNo]]></desc>
			<sql><![CDATA[
select eq_tpsz_cd ||'\\'|| eq_knd_cd as eq_tpsz_cd
from CGM_EQUIPMENT
where 1=1
and eq_no=@[eq_no]			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
