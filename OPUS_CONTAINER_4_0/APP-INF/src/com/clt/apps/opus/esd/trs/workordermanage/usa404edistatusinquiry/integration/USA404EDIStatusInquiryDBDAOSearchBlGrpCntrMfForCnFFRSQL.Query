<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USA404EDIStatusInquiryDBDAOSearchBlGrpCntrMfForCnFFRSQL">
			<desc><![CDATA[USA404EDIStatusInquiryDBDAOSearchBlGrpCntrMfForCnFF]]></desc>
			<sql><![CDATA[
WITH V1 AS (
 SELECT ROW_NUMBER() OVER(ORDER BY BKG_NO, CNTR_MF_SEQ) RN
      ,BKG_NO
      ,CNTR_MF_SEQ
      ,PCK_QTY
      ,PCK_TP_CD
      ,REPLACE(REPLACE(CNTR_MF_GDS_DESC, CHR(10), ' '), CHR(13), ' ') CNTR_MF_GDS_DESC
      ,CNTR_MF_WGT
  FROM BKG_CNTR_MF_DESC D
 WHERE (BKG_NO, CNTR_NO) IN
       (SELECT /*+ LEADING(BC) USE_NL(B) USE_NL(B1)*/ 
			   BC.BKG_NO, BC.CNTR_NO
          FROM BKG_CONTAINER BC, BKG_BOOKING B, BKG_BOOKING B1
         WHERE 1 = 1
           AND EXISTS (SELECT 1
                  FROM SCE_COP_HDR M, SCE_COP_HDR S, TRS_TRSP_RAIL_BIL_ORD O
                 WHERE O.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
                   AND O.TRSP_SO_SEQ = @[trsp_so_seq]
                   AND O.TRSP_BND_CD = 'I'
                   AND NVL(O.DELT_FLG, 'N') <> 'Y'
                   AND S.COP_NO = O.COP_NO
                   AND S.MST_COP_NO = M.COP_NO
                   AND M.BKG_NO = B.BKG_NO)
           AND B.BKG_NO <> B1.BKG_NO
           AND B.VSL_CD = B1.VSL_CD
           AND B.SKD_VOY_NO = B1.SKD_VOY_NO
           AND B.SKD_DIR_CD = B1.SKD_DIR_CD
           AND B1.BKG_NO = BC.BKG_NO
           AND BC.CNTR_NO = @[eq_no]
           AND B1.BKG_CGO_TP_CD = 'F'
           AND (B1.BKG_STS_CD = 'W' OR B1.BKG_STS_CD = 'F')
        UNION ALL
        SELECT O.BKG_NO, O.EQ_NO AS CNTR_NO
          FROM TRS_TRSP_RAIL_BIL_ORD O
         WHERE O.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
           AND O.TRSP_SO_SEQ = @[trsp_so_seq]
           AND O.TRSP_BND_CD = 'I'
           AND NVL(O.DELT_FLG, 'N') <> 'Y')
   AND 'I' = @[bound_ind]
)
SELECT T.RN               CM_LINE_ITEM
      ,T.PCK_QTY          CM_PACKQTY
      ,T.CNTR_MF_GDS_DESC CM_PACKDESC
      ,T.PCK_TP_CD        CM_PACKFORM
      ,T.CNTR_MF_WGT      CM_WEIGHT
  FROM (SELECT RN, PCK_QTY, CNTR_MF_GDS_DESC, PCK_TP_CD, CNTR_MF_WGT
          FROM V1
         WHERE RN < 25
        UNION ALL
        SELECT RN, PCK_QTY, CNTR_MF_GDS_DESC, PCK_TP_CD, CNTR_MF_WGT
          FROM (SELECT MIN(RN) RN
                      ,SUM(PCK_QTY) PCK_QTY
                      ,MIN(CNTR_MF_GDS_DESC) CNTR_MF_GDS_DESC
                      ,CASE WHEN MAX(PTC) > 1 THEN 'PC'
                            ELSE MAX(PCK_TP_CD)
                       END PCK_TP_CD
                      ,SUM(CNTR_MF_WGT) CNTR_MF_WGT
                  FROM (SELECT RN
                              ,PCK_QTY
                              ,PCK_TP_CD
                              ,RANK() OVER(ORDER BY PCK_TP_CD) PTC
                              ,FIRST_VALUE(CNTR_MF_GDS_DESC) OVER(ORDER BY RN ASC) CNTR_MF_GDS_DESC
                              ,CNTR_MF_WGT
                          FROM V1
                         WHERE RN >= 25))
         WHERE RN IS NOT NULL) T
 RIGHT OUTER JOIN DUAL
    ON 1 = 1			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="bound_ind" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
