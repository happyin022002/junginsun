<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOsearchBkgCancelListRSQL">
			<desc><![CDATA[searchBkgCancelList]]></desc>
			<sql><![CDATA[
SELECT 
	B.BKG_NO
FROM TRS_TRSP_WRK_ORD_PRV_TMP A, TRS_TRSP_SVC_ORD B
WHERE A.WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
    AND A.WO_ISS_NO = @[wo_iss_no]
	AND A.WO_CXL_FLG = 'Y'
	AND A.TRSP_RJCT_RSN_CD = 'B'
	AND B.BKG_NO IS NOT NULL
    AND (B.TRSP_SO_OFC_CTY_CD, B.TRSP_SO_SEQ) IN ((A.TRSP_SO_OFC_CTY_CD, A.TRSP_SO_SEQ))
    -- /* 2008.04.29 ETS OPEN */
GROUP BY B.BKG_NO			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
