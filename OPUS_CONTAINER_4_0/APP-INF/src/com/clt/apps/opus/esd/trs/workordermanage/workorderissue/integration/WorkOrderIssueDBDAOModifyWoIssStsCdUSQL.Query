<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderIssueDBDAOModifyWoIssStsCdUSQL">
			<desc><![CDATA[Tranport Status Update 시 TRS_TRSP_WRK_ORD.WO_ISS_STS_CD = 'I' 로 변환하는 코드]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_WRK_ORD
	 SET UPD_USR_ID     = @[upd_usr_id]
		,UPD_DT         = sysdate
		,WO_ISS_STS_CD  = 'I'
 WHERE TRSP_WO_OFC_CTY_CD = @[trsp_wo_ofc_cty_cd]
   AND TRSP_WO_SEQ = @[trsp_wo_seq]
   AND WO_ISS_STS_CD = 'X'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
