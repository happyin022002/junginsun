<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceProviderCategoryManageDBDAOSearchOfficeCodeAllListRSQL">
			<desc><![CDATA[SearchOfficeCodeAllList]]></desc>
			<sql><![CDATA[
SELECT A.OFC_CD, 
	    DECODE(B.CONTI_CD, 'F', 'HAM','E','HAM','M','NYC','A',DECODE(SUBSTR(A.AR_HD_QTR_OFC_CD,1,3),'SIN','SIN','SHA')) REG_GROUP		
FROM  MDM_ORGANIZATION A, 
      MDM_LOCATION B                                               		
WHERE A.LOC_CD = B.LOC_CD                                                              		
 AND  A.DELT_FLG <> 'Y'                                                              		
ORDER BY REG_GROUP,OFC_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
