<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementCorrectionDBDAOManageAgmtAttachFileListUSQL">
			<desc><![CDATA[COM_UPLD_FILE의 데이터로 TRS_AGMT_IMG_STO를 UPDATE하는 SQL]]></desc>
			<sql><![CDATA[
UPDATE (SELECT IMG.FILE_NM AS IMG_FILE_NM,
               IMG.FILE_PATH_RMK AS IMG_FILE_PATH,
               UPLD.FILE_UPLD_NM AS UPLD_FILE_NM,
               UPLD.FILE_PATH_URL AS UPLD_FILE_PATH
          FROM TRS_AGMT_IMG_STO IMG,
               COM_UPLD_FILE UPLD
         WHERE IMG.FILE_SAV_ID = UPLD.FILE_SAV_ID
           AND IMG.TRSP_AGMT_OFC_CTY_CD = SUBSTR(@[agmt_no], 1, 3)
           AND IMG.TRSP_AGMT_SEQ = SUBSTR(@[agmt_no], 4))
   SET IMG_FILE_NM = UPLD_FILE_NM,
       IMG_FILE_PATH = UPLD_FILE_PATH			]]></sql>
			<params>
				<param name="agmt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
