<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSearchCostModeRSQL">
			<desc><![CDATA[CY&Door구분에 따른 CostMode 조회]]></desc>
			<sql><![CDATA[
SELECT B.INTG_CD_VAL_CTNT || B.INTG_CD_VAL_DP_DESC AS CODE
   FROM COM_INTG_CD_DTL B
  WHERE INTG_CD_ID = 'CD00594'
    AND INTG_CD_VAL_CTNT IN ('CY', 'DR', 'LS', 'TS')
  #if (${cydoordiv} == 'DR')
    AND INTG_CD_VAL_CTNT = 'DR'
  #else
    AND INTG_CD_VAL_CTNT <> 'DR'
  #end
  ORDER BY B.INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
