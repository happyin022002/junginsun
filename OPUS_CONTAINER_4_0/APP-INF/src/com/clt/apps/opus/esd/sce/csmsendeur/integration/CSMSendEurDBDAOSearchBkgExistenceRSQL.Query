<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSMSendEurDBDAOSearchBkgExistenceRSQL">
			<desc><![CDATA[SearchBkgExistence]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
  FROM ${table_name}
 WHERE 1=1
   AND (BKG_NO) IN (
                     SELECT BKG_NO 
                       FROM SCE_CSM_TGT_EUR 
                      WHERE 1=1
                        AND ACT_RCV_DT = @[act_rcv_dt] 
                        AND ACT_RCV_NO = @[act_rcv_no]
                   )			]]></sql>
			<params>
				<param name="act_rcv_dt" type="12" value="" out="N"/>
				<param name="act_rcv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
