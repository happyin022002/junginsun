<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOAddSceFltFileNoGenCSQL">
			<desc><![CDATA[AddSceFltFileNoGen]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_FLT_FILE_NO_GEN
       (
         EDI_SND_YRMONDY,
         EDI_SND_SEQ,
         EDI_SND_DESC,
         RSLT_FLG,
         EDI_STS_CD,
         BL_NO,
         BKG_NO,
         CRE_USR_ID,
         CRE_DT,
         UPD_USR_ID,
         UPD_DT
       )
       VALUES 
       (
            @[edi_snd_yrmondy],
            @[edi_snd_seq],
            @[edi_snd_desc],
            @[rslt_flg],
            @[edi_sts_cd],
            substr(@[bl_no],1,12),
            @[bkg_no],
            nvl(@[cre_usr_id],'-'),
            sysdate,
            nvl(@[upd_usr_id],'-'),
            sysdate
       )			]]></sql>
			<params>
				<param name="edi_snd_yrmondy" type="12" value="" out="N"/>
				<param name="edi_snd_seq" type="12" value="" out="N"/>
				<param name="edi_snd_desc" type="2005" value="" out="N"/>
				<param name="rslt_flg" type="12" value="" out="N"/>
				<param name="edi_sts_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
