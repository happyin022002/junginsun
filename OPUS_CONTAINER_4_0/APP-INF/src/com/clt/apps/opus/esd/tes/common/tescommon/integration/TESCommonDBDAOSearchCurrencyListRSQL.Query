<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchCurrencyListRSQL">
			<desc><![CDATA[Currency Inquiry]]></desc>
			<sql><![CDATA[
SELECT
	(SELECT	AR_CURR_CD
	FROM	MDM_ORGANIZATION
	WHERE	OFC_CD = @[ofc_cd] AND DELT_FLG = 'N') DEF
	,(SELECT LTRIM(MAX(SYS_CONNECT_BY_PATH(CURR_CD,'|')),'|')
	FROM	(SELECT	ROWNUM ROW_ID
			, Z.CURR_CD
		FROM	(SELECT CURR_CD
			FROM	MDM_CURRENCY
			WHERE	NVL(DELT_FLG,'N')<>'Y'  
			ORDER BY CURR_CD
			)  Z
		)
	CONNECT BY PRIOR ROW_ID = ROW_ID - 1
	START WITH ROW_ID = 1
	) CURR_LIST
FROM	DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
