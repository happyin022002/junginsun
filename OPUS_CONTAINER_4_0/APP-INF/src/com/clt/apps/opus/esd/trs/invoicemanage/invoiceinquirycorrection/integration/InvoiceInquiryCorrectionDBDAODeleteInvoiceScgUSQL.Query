<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceInquiryCorrectionDBDAODeleteInvoiceScgUSQL">
			<desc><![CDATA[Surcharge 정보를 수정]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SCG_DTL SET
 	INV_SCG_AMT			= ''
 ,	INV_DRY_RUN_RLBL_PTY_TP_CD	= ''
 ,	INV_FNE_CUZ_DESC		= ''
 ,	INV_FUMG_COST_TP_CD		= ''
 ,	INV_MGST_TPSZ_CD		= ''
 ,	INV_INSP_RF_PTI_CSTMS_TP_CD	= ''
 ,	INV_LFTG_KNT			= ''
 ,	INV_LFTG_CUZ_DESC		= ''
 ,	INV_STOP_LOC_NOD_CD		= ''
 ,	INV_GRS_WGT			= ''
 ,	INV_INCRT_DT			= ''
 ,	INV_SCL_STOP_PLC_NOD_CD		= ''
 ,	INV_STO_DYS			= ''
 ,	INV_OB_BKG_NO			= ''
 ,	INV_WT_HRS			= ''
 ,	INV_OTR_RMK			= ''
 ,	UPD_USR_ID			= @[FORM_CRE_USR_ID]	
 ,	UPD_DT				= SYSDATE		
 ,	LOCL_UPD_DT			= GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[FORM_USR_OFC_CD])
 ,  INV_CHSS_NO 			= ''	
 ,	INV_INCUR_DT		= ''
 WHERE	
 TRSP_SO_SEQ	IN 	
 (		
 	SELECT TRSP_SO_SEQ FROM TRS_TRSP_SVC_ORD 	
 	WHERE 
 	INV_NO			= @[inv_no]			
 	AND INV_VNDR_SEQ	= @[inv_vndr_seq]	
 )		
 AND NVL(SCG_AMT, 0)	<> 0
			]]></sql>
			<params>
				<param name="FORM_CRE_USR_ID" type="12" value="" out="N"/>
				<param name="FORM_USR_OFC_CD" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
