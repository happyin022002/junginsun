<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOModifyActivityGroupUSQL">
			<desc><![CDATA[ModifyActivityGroup]]></desc>
			<sql><![CDATA[
UPDATE SCE_ACT_GRP
SET COP_DTL_GRP_CD = @[cop_dtl_grp_cd]
WHERE NOD_TP_CD = @[nod_tp_cd]
AND BKG_TERM_CD = @[bkg_term_cd]
AND BFR_TRSP_MOD_CD = @[bfr_trsp_mod_cd]
AND AFT_TRSP_MOD_CD = @[aft_trsp_mod_cd]
AND TRSP_BND_CD = @[trsp_bnd_cd]
AND SPCL_NOD_TP_CD = @[spcl_nod_tp_cd]			]]></sql>
			<params>
				<param name="cop_dtl_grp_cd" type="12" value="" out="N"/>
				<param name="nod_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_term_cd" type="12" value="" out="N"/>
				<param name="bfr_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="aft_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="trsp_bnd_cd" type="12" value="" out="N"/>
				<param name="spcl_nod_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
