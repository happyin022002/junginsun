<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOCopyPlnSoListCSQL">
			<desc><![CDATA[org_cop_no 로 sce_pln_So_list 를 조회하여 신규 cop_no 로 생성한다.]]></desc>
			<sql><![CDATA[
insert into SCE_PLN_SO_LIST (
CTRL_OFC_CD                 ,
TRSP_SO_STS_CD              ,
PCTL_COST_MOD_CD            ,
N1ST_NOD_PLN_DT             ,
COP_NO                      ,
COST_ACT_GRP_SEQ            ,
COST_ACT_GRP_CD             ,
N1ST_NOD_CD                 ,
N2ND_NOD_CD                 ,
N3RD_NOD_CD                 ,
N4TH_NOD_CD                 ,
PCTL_IO_BND_CD              ,
N1ST_VNDR_SEQ               ,
TRNS_RQST_OFC_CD            ,
TRNS_RQST_USR_ID            ,
TRNS_RQST_RSN               ,
TRSP_MOD_CD                 ,
INLND_ROUT_INV_BIL_PATT_CD  ,
INV_BIL_PATT_DIV_FLG        ,
CRE_USR_ID                  ,
CRE_DT                      ,
UPD_USR_ID                  ,
UPD_DT                      ,
DELT_USR_ID                 ,
DELT_DT                     ,
DOR_ARR_DT                  ,
LST_NOD_PLN_DT							
)
(SELECT 
CTRL_OFC_CD                 ,
TRSP_SO_STS_CD              ,
PCTL_COST_MOD_CD            ,
N1ST_NOD_PLN_DT             ,
@[cop_no]                   ,
COST_ACT_GRP_SEQ            ,
COST_ACT_GRP_CD             ,
N1ST_NOD_CD                 ,
N2ND_NOD_CD                 ,
N3RD_NOD_CD                 ,
N4TH_NOD_CD                 ,
PCTL_IO_BND_CD              ,
N1ST_VNDR_SEQ               ,
TRNS_RQST_OFC_CD            ,
TRNS_RQST_USR_ID            ,
TRNS_RQST_RSN               ,
TRSP_MOD_CD                 ,
INLND_ROUT_INV_BIL_PATT_CD  ,
INV_BIL_PATT_DIV_FLG        ,
CRE_USR_ID                  ,
CRE_DT                      ,
UPD_USR_ID                  ,
UPD_DT                      ,
DELT_USR_ID                 ,
DELT_DT                     ,
DOR_ARR_DT                  ,
LST_NOD_PLN_DT							
FROM SCE_PLN_SO_LIST
WHERE COP_NO = @[org_cop_no])			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="org_cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
