<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewDraftChkDupleRSQLRSQL">
			<desc><![CDATA[AddWorkOrderPreView 
Draft 체크]]></desc>
			<sql><![CDATA[
SELECT COUNT(SO.TRSP_WO_OFC_CTY_CD) CNT
  FROM (SELECT TRSP_WO_OFC_CTY_CD, TRSP_WO_SEQ,WO_ISS_STS_CD
          FROM TRS_TRSP_WRK_ORD
         WHERE (TRSP_WO_OFC_CTY_CD, TRSP_WO_SEQ) IN (SELECT TRSP_WO_OFC_CTY_CD, TRSP_WO_SEQ
                                 FROM trs_trsp_wrk_ord_prv_tmp O
                                                      WHERE wo_prv_grp_seq = @[group_seq]
                                                        )) WRK
      ,TRS_TRSP_SVC_ORD SO
 WHERE WRK.TRSP_WO_OFC_CTY_CD = SO.TRSP_WO_OFC_CTY_CD
   AND WRK.TRSP_WO_SEQ = SO.TRSP_WO_SEQ
   AND WRK.WO_ISS_STS_CD = 'I'
   AND NVL(SO.TRS_SUB_STS_CD, 'DF') <> 'DF'			]]></sql>
			<params>
				<param name="group_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
