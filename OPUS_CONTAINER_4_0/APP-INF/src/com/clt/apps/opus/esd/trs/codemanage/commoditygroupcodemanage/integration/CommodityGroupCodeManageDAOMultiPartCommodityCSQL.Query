<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDAOMultiPartCommodityCSQL">
			<desc><![CDATA[Rail VENDOR 별 GROUP COMMODITY를 재 정의하여 입력]]></desc>
			<sql><![CDATA[
INSERT INTO trs_cmdt_grp_cz (                                                               
	trsp_grp_cmdt_cd,                                               
	cmdt_cd,                                               
	vndr_seq,                                               
	cre_usr_id,                                               
	cre_dt,                                                             
	delt_flg                                                            
) VALUES (
	@[trsp_grp_cmdt_cd],
	@[cmdt_cd],                                               
	@[vndr_seq],                                               
	@[cre_usr_id],                                               
	TO_DATE( @[cre_dt], 'YYYYMMDD HH24:MI:SS'),
	'N' 
)			]]></sql>
			<params>
				<param name="trsp_grp_cmdt_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
