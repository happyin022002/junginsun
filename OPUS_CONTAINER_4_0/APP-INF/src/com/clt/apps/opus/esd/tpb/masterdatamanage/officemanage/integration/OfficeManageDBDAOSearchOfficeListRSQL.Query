<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeManageDBDAOSearchOfficeListRSQL">
			<desc><![CDATA[SearchOfficeList]]></desc>
			<sql><![CDATA[
SELECT n3pty_ofc_tp_cd
      ,ofc_cd
      ,rhq_cd
      ,n3pty_ctrl_ofc_cd
      ,n3pty_ofc_cd
      ,n3pty_ar_ofc_cd
      ,delt_flg
      ,cre_usr_id
      ,to_char(cre_dt,'YYYY/MM/DD') AS cre_dt
      ,upd_usr_id
      ,to_char(upd_dt,'YYYY/MM/DD') AS upd_dt
  FROM tpb_hndl_ofc
 WHERE 1 = 1
 
#if (${s_n3pty_ofc_tp_cd} != '') 
   AND n3pty_ofc_tp_cd = @[s_n3pty_ofc_tp_cd]
#end
#if (${s_if_rhq_cd} != '')
   AND rhq_cd = @[s_if_rhq_cd]
#end
#if (${s_if_ofc_cd} != '') 
   AND ofc_cd = @[s_if_ofc_cd]
#end
#if (${s_ofc_cd} != '') 
   AND ofc_cd LIKE @[s_ofc_cd]||'%'
#end
 ORDER BY rhq_cd, n3pty_ofc_cd, ofc_cd			]]></sql>
			<params>
				<param name="s_n3pty_ofc_tp_cd" type="12" value="" out="N"/>
				<param name="s_if_rhq_cd" type="12" value="" out="N"/>
				<param name="s_if_ofc_cd" type="12" value="" out="N"/>
				<param name="s_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
