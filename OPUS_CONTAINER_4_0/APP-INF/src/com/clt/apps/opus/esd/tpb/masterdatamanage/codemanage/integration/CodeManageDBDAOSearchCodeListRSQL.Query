<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchCodeListRSQL">
			<desc><![CDATA[SearchCodeList]]></desc>
			<sql><![CDATA[
SELECT n3pty_if_tp_cd
      ,n3pty_expn_tp_cd
      ,n3pty_bil_tp_cd
      ,n3pty_bil_tp_nm
      ,n3pty_bil_tp_desc
      ,cop_expt_cs_cd
      ,cml_sys_if_cd
      ,act_flg
      ,cre_usr_id
	  ,'A' AS ibflag
	  ,ar_acct_cd
#if (${cre_dt} != '')
--       ,TO_CHAR(TPB_GET_LCL_DATE_FNC(cre_dt,@[cre_dt]),'YYYY-MM-DD') AS cre_dt
#end
      ,upd_usr_id
#if (${upd_dt} != '') 
--      ,TO_CHAR(TPB_GET_LCL_DATE_FNC(upd_dt,@[upd_dt]),'YYYY-MM-DD') AS upd_dt
#end
  FROM tpb_n3rd_pty_bil_tp
 WHERE act_flg = 'Y'
#if (${s_billing_case_cd} != '') 
   AND n3pty_bil_tp_cd LIKE @[s_billing_case_cd]||'%'
#end			]]></sql>
			<params>
				<param name="cre_dt" type="12" value="20090101" out="Y"/>
				<param name="upd_dt" type="12" value="20090101" out="Y"/>
				<param name="s_billing_case_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
