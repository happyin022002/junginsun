<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOModifySceEdiHisDtlEdiRmk1USQL">
			<desc><![CDATA[ModifySceEdiHisDtlEdiRmk1]]></desc>
			<sql><![CDATA[
UPDATE SCE_EDI_HIS_DTL
SET EDI_RMK1 = EDI_RMK1||'('||@[edi_rmk1]||'['||to_char(sysdate,'yyyymmddhh24miss')||'])'
WHERE EDI_RCV_DT     = @[rcv_dt]
AND EDI_RCV_SEQ      = to_number(@[rcv_seq])
AND EDI_RCV_DTL_SEQ  = to_number(@[rcv_dtl_seq])			]]></sql>
			<params>
				<param name="edi_rmk1" type="12" value="" out="N"/>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="rcv_seq" type="12" value="" out="N"/>
				<param name="rcv_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
