<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupStrategicImportanceManageDBDAOMultiSpeEvGrpStrgImptRsltCSQL">
			<desc><![CDATA[SI Analysis Result 테이블에 Insert]]></desc>
			<sql><![CDATA[
INSERT INTO SPE_EV_GRP_STRG_IMPT_RSLT 
        (EG_ID , 
         EG_ID_SEQ , 
         EV_YR , 
         BI_SCRE , 
         DS_SCRE , 
         SI_SCRE , 
         SI_GRP_CD , 
         SI_GRP_NM , 
         CRE_USR_ID , 
         CRE_DT , 
         UPD_USR_ID , 
         UPD_DT)
VALUES ( @[eg_id]  
        ,@[eg_id_seq]
        ,@[ev_yr]
        ,TO_NUMBER(@[bi_scre]) -- bi_scre
        ,TO_NUMBER(@[ds_scre]) -- ds_scre
        ,ROUND( CASE WHEN @[si_grp_cd] = 'CR' 
					      THEN SQRT(((TO_NUMBER(@[bi_scre]) - 2.5) * (TO_NUMBER(@[bi_scre]) - 2.5)) + ((TO_NUMBER(@[ds_scre]) - 2.5) * (TO_NUMBER(@[ds_scre]) - 2.5))) 
				     WHEN @[si_grp_cd] = 'LV' 
                	      THEN SQRT(((TO_NUMBER(@[bi_scre]) - 2.5) * (TO_NUMBER(@[bi_scre]) - 2.5)) + (TO_NUMBER(@[ds_scre]) * TO_NUMBER(@[ds_scre]))) 
				     WHEN @[si_grp_cd] = 'BN' 
					      THEN SQRT((TO_NUMBER(@[bi_scre]) * TO_NUMBER(@[bi_scre]) ) + ((TO_NUMBER(@[ds_scre]) - 2.5) * (TO_NUMBER(@[ds_scre]) - 2.5))) 
				     WHEN @[si_grp_cd] = 'RT' 
				          THEN SQRT((TO_NUMBER(@[bi_scre]) * TO_NUMBER(@[bi_scre])) + (TO_NUMBER(@[ds_scre]) * TO_NUMBER(@[ds_scre]))) END , 2) -- si_scre
       ,@[si_grp_cd] -- si_grp_cd
       ,@[si_grp_nm] -- si_grp_nm
       ,@[cre_usr_id]-- cre_usr_id
       ,SYSDATE -- cre_dt
       ,@[upd_usr_id] -- upd_usr_id
       ,SYSDATE -- upd_dt
      )			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="eg_id_seq" type="12" value="" out="N"/>
				<param name="ev_yr" type="12" value="" out="N"/>
				<param name="bi_scre" type="12" value="" out="N"/>
				<param name="ds_scre" type="12" value="" out="N"/>
				<param name="si_grp_cd" type="12" value="" out="N"/>
				<param name="si_grp_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
