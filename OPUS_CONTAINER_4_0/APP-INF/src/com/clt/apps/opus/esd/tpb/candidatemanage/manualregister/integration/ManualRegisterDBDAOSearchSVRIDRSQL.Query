<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualRegisterDBDAOSearchSVRIDRSQL">
			<desc><![CDATA[SearchSVRID]]></desc>
			<sql><![CDATA[
SELECT SVR.SYS_AREA_GRP_ID SVR_ID
  FROM MDM_ORGANIZATION ORG,
	   MDM_LOCATION LOC,
	   COM_SYS_AREA_GRP_ID SVR
 WHERE ORG.LOC_CD = LOC.LOC_CD
   AND LOC.CNT_CD = SVR.CNT_CD
   AND ORG.OFC_CD = @[s_if_ofc_cd]
   AND NVL(ORG.DELT_FLG,'N') = 'N'
   AND NVL(LOC.DELT_FLG,'N') = 'N'
   AND SVR.SVR_USD_FLG = 'Y'
   AND SVR.CO_IND_CD = 'H'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="s_if_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
