<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDetailNextStatusUSQL">
			<desc><![CDATA[ModifyCopDetailNextStatus]]></desc>
			<sql><![CDATA[
update  sce_cop_dtl
   set  act_sts_cd = 'C'
where  (cop_no, cop_dtl_seq) in 
   (select (case when lag(c.act_sts_cd, 1, c.act_sts_cd) 
                      over (partition by c.cop_no 
                      order by c.cop_no, c.cop_dtl_seq) = 'F'
                   and c.act_sts_cd = 'N' then c.cop_no end) aa ,
            (case when lag(c.act_sts_cd, 1, c.act_sts_cd) 
                       over (partition by c.cop_no 
                       order by c.cop_no, c.cop_dtl_seq) = 'F'
                   and c.act_sts_cd = 'N' then c.cop_dtl_seq end) bb
    from  sce_cop_dtl c
    where c.cop_no = @[cop_no] )
and     cop_no = @[cop_no]
			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
