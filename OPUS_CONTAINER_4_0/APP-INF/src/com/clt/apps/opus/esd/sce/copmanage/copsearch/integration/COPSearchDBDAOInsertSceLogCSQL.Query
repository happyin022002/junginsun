<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="COPSearchDBDAOInsertSceLogCSQL">
			<desc><![CDATA[cop change master]]></desc>
			<sql><![CDATA[
INSERT INTO sce_cop_his 
                       ( COP_NO
                       , ACCL_SEQ
                       , CNTR_NO
                       , BKG_NO
                       , CNTR_TPSZ_CD
                       , PCTL_NO
                       , BKG_STS_CD
                       , COP_STS_CD
                       , BKG_EVNT_TP_CD
                       , UMCH_STS_CD
                       , OB_TRO_SEQ
                       , OB_TRO_SUB_SEQ
                       , IB_TRO_SEQ
                       , IB_TRO_SUB_SEQ
                       , CRE_USR_ID
                       , CRE_DT
                       , UPD_USR_ID
                       , UPD_DT
				       , MST_COP_NO
                       )
SELECT hdr.cop_no
     , SCE_COP_HIS_SEQ.NEXTVAL
     , hdr.cntr_no
     , hdr.bkg_no
     , hdr.cntr_tpsz_cd
     , hdr.pctl_no
     , bk.bkg_sts_cd
     , hdr.cop_sts_cd
     , @[bkg_event_tp_cd] 
     , ''
     , ''
     , ''
     , ''
     , ''
     , @[cre_usr_id]  AS cre_usr_id
     , SYSDATE AS cre_dt
     , @[cre_usr_id]  AS upd_usr_id
     , SYSDATE AS upd_dt
	 , MST_COP_NO
  FROM sce_cop_hdr hdr
     , bkg_booking bk
 WHERE hdr.bkg_no = bk.bkg_no
   AND hdr.cop_no = @[cop_no]			]]></sql>
			<params>
				<param name="bkg_event_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
