<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchBkgHblNoRSQL">
			<desc><![CDATA[edi315 HBL NO new item]]></desc>
			<sql><![CDATA[
SELECT HBL_NO
FROM BKG_HBL
WHERE BKG_NO = @[bkg_no]
AND HBL_NO IS NOT NULL
ORDER BY HBL_SEQ 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
