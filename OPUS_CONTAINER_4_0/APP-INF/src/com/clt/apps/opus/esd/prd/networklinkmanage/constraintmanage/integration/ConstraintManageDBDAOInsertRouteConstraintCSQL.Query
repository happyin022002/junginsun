<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOInsertRouteConstraintCSQL">
			<desc><![CDATA[InsertRouteConstraint]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_ROUT_CNST (
     TRNK_LANE_CD
	,POL_NOD_CD
	,POD_NOD_CD
	,ROUT_CNST_SEQ
	,DEL_NOD_CD
	,POR_NOD_CD
	,N1ST_LANE_CD
	,N1ST_TS_PORT_CD
	,N2ND_LANE_CD
	,N2ND_TS_PORT_CD
	,N3RD_LANE_CD
	,SVC_USE_FLG
	,ROUT_CNST_RMK
	,CRE_USR_ID
	,CRE_DT
	,CRE_OFC_CD
	,UPD_USR_ID
	,DIR_CD
	,SPCL_CGO_CNTR_TP_CD
	,CNTR_TP_CD
	,CNTR_SZ_CD
) VALUES (
	 @[s_trunk_lane]
	,@[s_pol_node]
	,@[s_pod] || @[s_pod_node]
	,(SELECT NVL(MAX(ROUT_CNST_SEQ), 0) + 1 FROM PRD_ROUT_CNST	WHERE TRNK_LANE_CD = @[s_trunk_lane] AND POL_NOD_CD = @[s_pol_node]	AND POD_NOD_CD = @[s_pod] || @[s_pod_node])
	,@[s_del] || @[s_del_node]
    ,@[s_por] || @[s_por_node]
	,@[s_lane]
	,@[s_ts1_port]
	,@[s_ts1_lane]
	,@[s_ts2_port]
	,@[s_ts2_lane]
	,@[s_svc_flg]
	,@[s_remark]
	,@[cre_usr_id]
	,SYSDATE
	,@[cre_ofc_cd]
	,@[upd_usr_id]
	,@[s_dir_cd]
    ,@[spcl_cgo_cntr_tp_cd]
    ,@[cntr_tp_cd]
    ,@[cntr_sz_cd]
)			]]></sql>
			<params>
				<param name="s_trunk_lane" type="12" value="" out="N"/>
				<param name="s_pol_node" type="12" value="" out="N"/>
				<param name="s_pod" type="12" value="" out="N"/>
				<param name="s_pod_node" type="12" value="" out="N"/>
				<param name="s_del" type="12" value="" out="N"/>
				<param name="s_del_node" type="12" value="" out="N"/>
				<param name="s_por" type="12" value="" out="N"/>
				<param name="s_por_node" type="12" value="" out="N"/>
				<param name="s_lane" type="12" value="" out="N"/>
				<param name="s_ts1_port" type="12" value="" out="N"/>
				<param name="s_ts1_lane" type="12" value="" out="N"/>
				<param name="s_ts2_port" type="12" value="" out="N"/>
				<param name="s_ts2_lane" type="12" value="" out="N"/>
				<param name="s_svc_flg" type="12" value="" out="N"/>
				<param name="s_remark" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="s_dir_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
