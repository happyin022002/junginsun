<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchCodescListDtlRSQL">
			<desc><![CDATA[SearchCodescListDtl]]></desc>
			<sql><![CDATA[
SELECT
DISTINCT LGS_COST_DTL_CD
FROM TES_LGS_COST
WHERE LGS_COST_DTL_CD IS NOT NULL
AND SUBSTR(LGS_COST_CD,1,2) = @[lgs_cost_subj_cd]			]]></sql>
			<params>
				<param name="lgs_cost_subj_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
