<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOCalculateExchangeRateRSQL">
			<desc><![CDATA[Currency 환률을 계산한다.]]></desc>
			<sql><![CDATA[
SELECT A.TRSP_SO_OFC_CTY_CD || A.TRSP_SO_SEQ TRSP_SO_OFC_CTY_CD_SEQ
      ,ROUND((NVL(A.BZC_AMT, 0) + NVL(A.NEGO_AMT, 0) + NVL(A.FUEL_SCG_AMT, 0) + NVL(A.ETC_ADD_AMT, 0)) 
		#if( ${trsp_inv_calc_lgc_tp_cd} == 'DV' )
             /
 		#else
			 *
 		#end			  
			 TO_NUMBER(@[inv_xch_rt]),
			  NVL((SELECT DP_PRCS_KNT FROM MDM_CURRENCY WHERE CURR_CD = @[apply_currency]), 0)
			) AS INV_BZC_AMT
  FROM TRS_TRSP_SVC_ORD A
 WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND TRSP_SO_SEQ = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="inv_xch_rt" type="6" value="" out="N"/>
				<param name="apply_currency" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
