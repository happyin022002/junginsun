<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewEdiDBDAOsearchFlatFileEsvChargeRSQL">
			<desc><![CDATA[NYK-Hawk_FFile(ESV_N)-JOEDI_EU_OUTBOUND
{CHARGE]]></desc>
			<sql><![CDATA[
SELECT T1.*
  FROM (
		SELECT NVL(SO.BZC_AMT, 0) + NVL(SO.NEGO_AMT, 0) AS FREIGHT
			  ,SO.CURR_CD AS CURRENCY
			  ,TO_CHAR(D.FUEL_RTO) AS FSC_PERCENT
			  ,'true' AS BILLABLE
			  ,'' AS CHARGE_REF
			  ,'' AS CHARGE_COMMENT
			  ,NVL(SO.BZC_AMT, 0) + NVL(SO.FUEL_SCG_AMT, 0) + NVL(SO.NEGO_AMT, 0) AS TOTAL_CHARGE
		  FROM TRS_TRSP_SVC_ORD SO
			  ,TRS_TRSP_SCG_DTL D
		 WHERE SO.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD(+)
		   AND SO.TRSP_SO_SEQ = D.TRSP_SO_SEQ(+)
		   AND SUBSTR(D.LGS_COST_CD(+), 3, 2) = 'FU'
		   AND SO.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
		   AND SO.TRSP_SO_SEQ = @[trsp_so_seq]
 ) T1
 RIGHT OUTER JOIN DUAL
    ON 1 = 1			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
