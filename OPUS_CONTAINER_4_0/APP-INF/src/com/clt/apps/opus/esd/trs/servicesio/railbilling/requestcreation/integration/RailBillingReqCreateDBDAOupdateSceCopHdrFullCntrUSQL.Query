<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailBillingReqCreateDBDAOupdateSceCopHdrFullCntrUSQL">
			<desc><![CDATA[SCE_COP_HDR Update SQL (Full CNTR)]]></desc>
			<sql><![CDATA[
UPDATE SCE_COP_HDR SET	          
		 			PROV_CNTR_NO       = @[prov_cntr_no],   
		 			PROV_CNTR_TPSZ_CD  = @[prov_cntr_tpsz_cd]    
                    ,UPD_USR_ID        = @[upd_usr_id]                
                    ,UPD_DT            = globaldate_pkg.time_local_ofc_fnc(@[vndr_seq])                         
		 	  WHERE COP_NO             = @[cop_no]			]]></sql>
			<params>
				<param name="prov_cntr_no" type="12" value="" out="N"/>
				<param name="prov_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
