<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManagerDBDAOVerifyECCRSQL">
			<desc><![CDATA[From Nod와 To Nod가 동일 ECC_CD인지 확인]]></desc>
			<sql><![CDATA[
SELECT 'X' ECC_FLG
  FROM (SELECT ECC_CD
          FROM MDM_EQ_ORZ_CHT A
              ,MDM_LOCATION B
              ,(SELECT YD_CD
                      ,LOC_CD
                  FROM MDM_YARD Y
                 WHERE Y.YD_CD = @[fm_nod_cd]
                UNION ALL
                SELECT LSE_CO_YD_CD
                      ,SUBSTR(LSE_CO_YD_CD, 1, 5)
                  FROM MDM_LSE_CO_YD L
                 WHERE L.LSE_CO_YD_CD = @[fm_nod_cd]) C
         WHERE A.DELT_FLG = 'N'
           AND A.SCC_CD = B.SCC_CD
           AND B.LOC_CD = C.LOC_CD) AA
      ,(SELECT ECC_CD
          FROM MDM_EQ_ORZ_CHT A
              ,MDM_LOCATION B
              ,(SELECT YD_CD
                      ,LOC_CD
                  FROM MDM_YARD Y
                 WHERE Y.YD_CD = @[to_nod_cd]
                UNION ALL
                SELECT LSE_CO_YD_CD
                      ,SUBSTR(LSE_CO_YD_CD, 1, 5)
                  FROM MDM_LSE_CO_YD L
                 WHERE L.LSE_CO_YD_CD = @[to_nod_cd]) C
         WHERE A.DELT_FLG = 'N'
           AND A.SCC_CD = B.SCC_CD
           AND B.LOC_CD = C.LOC_CD) BB
 WHERE AA.ECC_CD = BB.ECC_CD			]]></sql>
			<params>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
