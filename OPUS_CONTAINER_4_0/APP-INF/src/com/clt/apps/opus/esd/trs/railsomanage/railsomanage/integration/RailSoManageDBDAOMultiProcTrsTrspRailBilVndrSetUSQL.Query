<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOMultiProcTrsTrspRailBilVndrSetUSQL">
			<desc><![CDATA[요율정보를 SO Vendor 테이블에 수정 SQL]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_RAIL_BIL_VNDR_SET 
SET 
#if(${po_local_curr_cd} == "")
	CURR_CD = "", 
#else
	CURR_CD = @[po_local_curr_cd], 
#end
	BZC_AMT = @[po_basic_rt], 
	FUEL_SCG_AMT = @[po_fuel_scg_rt], 
	NEGO_AMT = 0, 
	OVR_WGT_SCG_AMT = @[po_over_wgt_scg_rt], 
	TRSP_AGMT_WY_TP_CD = @[po_way_type], 
	TRSP_AGMT_RT_TP_CD = @[po_trsp_agmt_rt_tp_cd], 
    HZD_MTRL_SCG_AMT = @[po_hzs_scg_rt], -- add 20100803
    ETC_ADD_AMT = @[po_ttl_scg_rt]  -- add 20100803
WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd] 
AND   TRSP_SO_SEQ = @[trsp_so_seq] 
AND   SUB_RAIL_SEQ = @[sub_rail_seq]			]]></sql>
			<params>
				<param name="po_local_curr_cd" type="12" value="" out="N"/>
				<param name="po_basic_rt" type="12" value="" out="N"/>
				<param name="po_fuel_scg_rt" type="12" value="" out="N"/>
				<param name="po_over_wgt_scg_rt" type="12" value="" out="N"/>
				<param name="po_way_type" type="12" value="" out="N"/>
				<param name="po_trsp_agmt_rt_tp_cd" type="12" value="" out="N"/>
				<param name="po_hzs_scg_rt" type="12" value="" out="N"/>
				<param name="po_ttl_scg_rt" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="sub_rail_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
