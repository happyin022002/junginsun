<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalStorageInvoiceManageDBDAOSearchStorageInvoiceRejectInfoRSQL">
			<desc><![CDATA[SearchStorageInvoiceRejectInfo]]></desc>
			<sql><![CDATA[
SELECT
  A.TML_SO_OFC_CTY_CD   ,
  A.TML_SO_SEQ          ,
  A.INV_NO              ,
  A.VNDR_SEQ            ,
  A.TML_INV_STS_CD      ,
  A.TML_INV_RJCT_STS_CD ,
  A.INV_RJCT_RMK        ,
  A.UPD_USR_ID          ,
  TO_CHAR(A.UPD_DT,'YYYY-MM-DD') UPD_DT
FROM TES_TML_SO_HDR A
WHERE 1=1
AND A.VNDR_SEQ = @[vndr_seq]
AND A.INV_NO = @[inv_no]
AND A.TML_INV_TP_CD = 'ST'
AND NVL(A.DELT_FLG,'N') <> 'Y'
			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
