<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="COPSearchDBDAOSearchSCInfoRSQL">
			<desc><![CDATA[cop inquiry]]></desc>
			<sql><![CDATA[
SELECT grp.cop_no
     , trsp_so_ofc_cty_cd
     , trsp_so_seq
     , bkg_no				
  FROM trs_trsp_svc_ord grp
      ,sce_cop_hdr hdr										
 WHERE grp.cop_no = hdr.cop_no
  AND grp.cop_no = @[old_cop_no]											
  AND NVL(trsp_so_ofc_cty_cd,' ')  <> ' '					
  AND NVL(trsp_so_ofc_cty_cd,' ')  <> 'P'					
  AND NVL(TO_CHAR(trsp_so_seq),' ') <> ' '			]]></sql>
			<params>
				<param name="old_cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
