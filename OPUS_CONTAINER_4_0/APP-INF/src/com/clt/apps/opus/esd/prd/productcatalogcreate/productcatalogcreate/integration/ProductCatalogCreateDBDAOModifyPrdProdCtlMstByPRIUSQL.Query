<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProductCatalogCreateDBDAOModifyPrdProdCtlMstByPRIUSQL">
			<desc><![CDATA[ProductCatalogCreateDBDAOModifyPrdProdCtlMstByPRI]]></desc>
			<sql><![CDATA[
UPDATE PRD_PROD_CTL_MST SET
  BKG_CGO_TP_CD = 'F'
, PRC_CGO_TP_CD = @[cgo_tp_cd]
, EQ_TP_CD = @[eq_tp_cd]
, SVC_SCP_CD = @[svc_scp_cd]
, SOC_FLG = NVL(@[soc_flg],'N')
, DG_SPCL_FLG = DECODE(@[cgo_tp_cd], 'DG', 'Y', 'N')
, RF_SPCL_FLG = DECODE(@[cgo_tp_cd], 'RF', 'Y', 'N')
, SPCL_AWK_CGO_FLG = DECODE(@[cgo_tp_cd], 'AK', 'Y', 'N')
, BB_SPCL_FLG = DECODE(@[cgo_tp_cd], 'BB', 'Y', 'N')
#if (${goh_cd} != '')
, BKG_HNGR_BAR_TP_CD = @[goh_cd]
#end
, CMDT_CD = @[cmdt_cd]
#if (${cust_cnt_cd} != '')
, ACT_CUST_CNT_CD = @[cust_cnt_cd]
#end
#if (${cust_seq} != '')
, ACT_CUST_SEQ = @[cust_seq]
#end
#if (${ctrt_tp} == 'S') 
, SC_NO = @[ctrt_no]
#elseif (${ctrt_tp} == 'R') 
, RFA_NO = @[ctrt_no]
#elseif (${ctrt_tp} == 'T') 
, TAA_NO = @[ctrt_no]
#end
,UPD_USR_ID = @[upd_usr_id]
,UPD_DT = SYSDATE
WHERE PCTL_NO LIKE @[pctl_no]||'%'
			]]></sql>
			<params>
				<param name="cgo_tp_cd" type="12" value="" out="N"/>
				<param name="eq_tp_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="soc_flg" type="12" value="" out="N"/>
				<param name="goh_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
