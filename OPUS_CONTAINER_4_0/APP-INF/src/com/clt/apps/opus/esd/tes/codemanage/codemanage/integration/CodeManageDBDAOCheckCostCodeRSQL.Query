<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOCheckCostCodeRSQL">
			<desc><![CDATA[Cost Code Check]]></desc>
			<sql><![CDATA[
SELECT	lgs_cost_cd                 
FROM	TES_LGS_COST                  
WHERE	lgs_cost_cd = @[lgs_cost_cd]  			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
