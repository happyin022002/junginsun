<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdiUsaTroPicRSQL">
			<desc><![CDATA[searchEdi_USA_TRO_PIC]]></desc>
			<sql><![CDATA[
SELECT usr.usr_nm as tro_pic_name
	,REPLACE(ORG.OFC_ADDR, CHR(13)||CHR(10), ' ') as tro_pic_addr
	,'' as tro_pic_city
	,'' as tro_pic_state
	,org.ofc_zip_cd	as tro_pic_zip
	,usr.xtn_phn_no	as tro_pic_tel
  ,org.ofc_fax_no	as tro_pic_fax
	,usr.usr_eml as tro_pic_email	
FROM	com_user usr
	,mdm_organization org
	,trs_trsp_svc_ord so
WHERE so.delt_flg <> 'y'
AND so.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
AND so.trsp_so_seq = @[trsp_so_seq]
AND DECODE(LENGTH(TRIM(so.upd_usr_id)),0,so.cre_usr_id,so.upd_usr_id) = usr.usr_id
AND usr.ofc_cd = org.ofc_cd
			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
