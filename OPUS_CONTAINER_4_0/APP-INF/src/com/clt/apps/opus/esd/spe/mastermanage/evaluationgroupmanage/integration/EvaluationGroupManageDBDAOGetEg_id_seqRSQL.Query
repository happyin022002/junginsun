<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupManageDBDAOGetEg_id_seqRSQL">
			<desc><![CDATA[EG ID 채번]]></desc>
			<sql><![CDATA[
SELECT MAX(EG_ID_SEQ) + 1 AS  EG_ID_SEQ 
FROM SPE_EV_GRP	
WHERE 	EG_ID =SUBSTR(@[eg_id],1,5)			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
