<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisDBDAODupChkPoolChassisInvoiceNoRSQL">
			<desc><![CDATA[Invoice No에 대한 중복 체크]]></desc>
			<sql><![CDATA[
SELECT COUNT(A.INV_NO) INV_FLAG		      
		 FROM 										  
		 		(	SELECT INV_NO                     
		 		     	  ,INV_VNDR_SEQ               
		   	   	  FROM TRS_TRSP_INV_WRK 		  		  
		 	     UNION ALL							  
		 		 	SELECT INV_NO                     
		 			      ,INV_VNDR_SEQ               
		    		  FROM TRS_TRSP_RAIL_INV_WRK	  
		 		) A									  
		 WHERE INV_NO = @[inv_no]							  
		   AND INV_VNDR_SEQ = @[paymt_sp_cd]

			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="paymt_sp_cd" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
