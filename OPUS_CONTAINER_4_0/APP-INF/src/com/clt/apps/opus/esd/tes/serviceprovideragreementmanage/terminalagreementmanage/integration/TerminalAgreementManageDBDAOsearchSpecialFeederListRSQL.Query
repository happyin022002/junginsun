<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOsearchSpecialFeederListRSQL">
			<desc><![CDATA[China Special Feeder화면 조회]]></desc>
			<sql><![CDATA[
SELECT F.FDR_POL_CD, L.LOC_NM POL_LOC_NM, F.FDR_POD_CD, M.LOC_NM POD_LOC_NM
FROM TES_TML_SPCL_FDR F, MDM_LOCATION L, MDM_LOCATION M
WHERE F.FDR_POL_CD = L.LOC_CD
AND F.FDR_POD_CD = M.LOC_CD

#if (${pol_cd}!='')
    AND FDR_POL_CD LIKE '%'||@[pol_cd]||'%'
#end

#if (${pod_cd}!='')
    AND FDR_POD_CD LIKE '%'||@[pod_cd]||'%'
#end

ORDER BY F.FDR_POL_CD,F.FDR_POD_CD			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
