<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOCreateThoroughputCCCSQL">
			<desc><![CDATA[Throughput Cost Code Insert]]></desc>
			<sql><![CDATA[
INSERT INTO TES_TML_AGMT_THRP_COST (  
		  tml_agmt_ofc_cty_cd                   
		, tml_agmt_seq                          
		, tml_agmt_ver_no                       
		, thrp_seq                              
		, lgs_cost_cd                           
		, thrp_lgs_cost_cd                      
		, cre_usr_id                            
		, cre_dt                                
		, upd_usr_id                            
		, upd_dt                                
		, locl_cre_dt
		)
VALUES (
		  @[tml_agmt_ofc_cty_cd]
		, @[tml_agmt_seq]
		, @[tml_agmt_ver_no]
		, @[thrp_seq]
		, @[lgs_cost_cd]
		, @[thrp_lgs_cost_cd]
		, @[cre_usr_id]
		, SYSDATE
		, @[cre_usr_id]
		, SYSDATE
		, GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])  
		)			]]></sql>
			<params>
				<param name="tml_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_agmt_seq" type="2" value="" out="N"/>
				<param name="tml_agmt_ver_no" type="2" value="" out="N"/>
				<param name="thrp_seq" type="2" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="thrp_lgs_cost_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
