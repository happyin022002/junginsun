<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchCopHdrByBkgNosRSQL">
			<desc><![CDATA[복수개의 Booking no 로 cancel 되지 않은 COP 를 찾는다.]]></desc>
			<sql><![CDATA[
SELECT 	COP_UPD_RMK, 
	COP_NO, 
	BKG_NO, 
	CNTR_NO, 
	CNTR_TPSZ_CD, 
	CNMV_YR, 
	COP_STS_CD, 
	PCTL_NO, 
	MST_COP_NO, 
	TO_CHAR(COP_FSH_DT, 'YYYYMMDDHH24MISS') AS COP_FSH_DT, 
	TRNK_VSL_CD, 
	TRNK_SKD_VOY_NO, 
	TRNK_SKD_DIR_CD, 
	POR_NOD_CD, 
	POL_NOD_CD, 
	POD_NOD_CD, 
	DEL_NOD_CD, 
	COP_RAIL_CHK_CD, 
	IB_TRO_FLG, 
	OB_TRO_FLG, 
	RAIL_RCV_COFF_DT_SRC_TP_CD, 
	TO_CHAR(RAIL_RCV_COFF_FM_DT, 'YYYYMMDDHH24MISS') AS RAIL_RCV_COFF_FM_DT, 
	CRE_USR_ID, 
	TO_CHAR(CRE_DT, 'YYYYMMDDHH24MISS') AS CRE_DT, 
	UPD_USR_ID, 
	TO_CHAR(UPD_DT, 'YYYYMMDDHH24MISS') AS UPD_DT, 
	TO_CHAR(RAIL_RCV_COFF_TO_DT, 'YYYYMMDDHH24MISS') AS RAIL_RCV_COFF_TO_DT, 
	COP_SUB_STS_CD, 
	UMCH_STS_CD, 
	PROV_CNTR_NO, 
	PROV_CNTR_TPSZ_CD, 
	TO_CHAR(CFM_OB_DOR_ARR_DT, 'YYYYMMDDHH24MISS') AS CFM_OB_DOR_ARR_DT, 
	TO_CHAR(CFM_APNT_DT, 'YYYYMMDDHH24MISS') AS CFM_APNT_DT
FROM SCE_COP_HDR
WHERE
    BKG_NO IN (
	#foreach($ele IN ${bkg_no})
		#if($velocityCount == 1 ) 
			('$ele')
		#else 
			,('$ele') 
		#end 
	#end
	)
    AND COP_STS_CD != 'X'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
