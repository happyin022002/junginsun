<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchWorkOrderPreviewGroupWOListRSQL">
			<desc><![CDATA[SearchWorkOrderPreviewGroupWOList]]></desc>
			<sql><![CDATA[
SELECT D.TRSP_SO_OFC_CTY_CD,
       D.TRSP_SO_SEQ,
       D.TRSP_SO_STS_CD,
       DENSE_RANK() OVER (
         ORDER BY D.WO_ISS_NO
       ) WO_ISS_NO,
       D.TRSP_WO_OFC_CTY_CD,
       D.TRSP_WO_SEQ,
       D.WO_FMT_TP_CD,
       D.WO_FMT_TP_NM,
       D.TRSP_SO_CMB_TP_CD,
       D.TRSP_COST_DTL_MOD_CD,
       D.CGO_TP_CD,
       D.VNDR_SEQ,
       D.TRSP_CRR_MOD_CD,
       D.FM_NOD_CD,
       D.VIA_NOD_CD,
       D.DOR_NOD_CD,
       D.TO_NOD_CD,
       D.FDR_VSL_CD,
       D.FDR_SKD_VOY_NO,
       D.FDR_SKD_DIR_CD
  FROM (SELECT B.TRSP_SO_OFC_CTY_CD,
               B.TRSP_SO_SEQ,
               B.TRSP_SO_STS_CD,
               MIN(ROWNUM) over 
               (PARTITION BY B.TRSP_SO_TP_CD,
                             B.WO_FMT_TP_CD,
                             B.VNDR_SEQ, 
                             B.TRSP_CRR_MOD_CD, 
                             B.FM_NOD_CD, 
                             B.VIA_NOD_CD, 
                             B.DOR_NOD_CD, 
                             B.DOR_DE_ADDR, 
                             B.TO_NOD_CD, 
                             B.FDR_VSL_CD||B.FDR_SKD_VOY_NO||B.FDR_SKD_DIR_CD, 
                             B.IB_VVD_CD,
                             B.REF_ID
                    ORDER BY B.TRSP_SO_TP_CD, 
                             B.WO_FMT_TP_CD, 
                             B.VNDR_SEQ, 
                             B.TRSP_CRR_MOD_CD, 
                             B.FM_NOD_CD, 
                             B.VIA_NOD_CD, 
                             B.DOR_NOD_CD, 
                             B.DOR_DE_ADDR, 
                             B.TO_NOD_CD, 
                             B.FDR_VSL_CD||B.FDR_SKD_VOY_NO||B.FDR_SKD_DIR_CD, 
                             B.IB_VVD_CD,
                             B.REF_ID
               ) WO_ISS_NO,
               B.TRSP_WO_OFC_CTY_CD,
               B.TRSP_WO_SEQ,
               B.WO_FMT_TP_CD,
               COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD00879', B.WO_FMT_TP_CD) WO_FMT_TP_NM ,
               B.TRSP_SO_CMB_TP_CD,
               B.TRSP_COST_DTL_MOD_CD,
               B.CGO_TP_CD,
               B.VNDR_SEQ,
               B.TRSP_CRR_MOD_CD,
               B.FM_NOD_CD,
               B.VIA_NOD_CD,
               B.DOR_NOD_CD,
               B.TO_NOD_CD,
               B.FDR_VSL_CD,
               B.FDR_SKD_VOY_NO,
               B.FDR_SKD_DIR_CD
          FROM (SELECT
                CASE
                  WHEN A.TRSP_SO_CMB_TP_CD IS NULL
                       AND A.TRSP_COST_DTL_MOD_CD IN ('CY', 'DR')
                       AND A.CGO_TP_CD = 'F'
                       AND A.TRSP_SO_TP_CD IN ('Y', 'S')
                       THEN 'NC'
                  WHEN A.TRSP_SO_CMB_TP_CD = 'CF'
                       AND A.TRSP_COST_DTL_MOD_CD IN ('CY', 'DR')
                       AND A.CGO_TP_CD IN ('F', 'M')
                       AND A.TRSP_SO_TP_CD IN ('Y', 'S')
                       THEN 'CC'
                  WHEN A.TRSP_SO_CMB_TP_CD = 'FF'
                       AND A.TRSP_COST_DTL_MOD_CD = 'DR'
                       AND A.CGO_TP_CD = 'F'
                       AND A.TRSP_SO_TP_CD IN ('Y', 'S')
                       THEN 'IB'
                  WHEN A.TRSP_SO_CMB_TP_CD = 'FF'
                       AND A.TRSP_COST_DTL_MOD_CD = 'CY'
                       AND A.CGO_TP_CD = 'F'
                       AND A.TRSP_SO_TP_CD IN ('Y', 'S')
                       THEN 'CY'
                  WHEN A.TRSP_SO_CMB_TP_CD = 'FM'
                       AND A.TRSP_COST_DTL_MOD_CD = 'CY'
                       AND A.CGO_TP_CD IN ('F', 'M')
                       AND A.TRSP_SO_TP_CD IN ('Y', 'S')
                       THEN 'CM'
                  WHEN (A.TRSP_SO_CMB_TP_CD IS NULL OR A.TRSP_SO_CMB_TP_CD = 'CF')
                       AND A.TRSP_COST_DTL_MOD_CD IN ('ER', 'CN', 'CF')
                       AND A.CGO_TP_CD = 'M'
                       AND A.TRSP_SO_TP_CD IN ('M', 'S')
                       THEN 'MM'
                  WHEN A.TRSP_SO_TP_CD = 'H'
                       THEN 'MM'
                  WHEN A.TRSP_SO_TP_CD = 'O'
                       AND A.TRSP_COST_DTL_MOD_CD IN ('ER', 'CN', 'CF')
                       THEN 'MM'
                  WHEN A.TRSP_SO_TP_CD = 'O'
                       AND A.TRSP_COST_DTL_MOD_CD NOT IN ('ER', 'CN', 'CF') THEN 'NC'
                  ELSE 'NC'
                   END WO_FMT_TP_CD,
                       A.TRSP_SO_TP_CD,
                       A.TRSP_SO_OFC_CTY_CD,
                       A.TRSP_SO_SEQ,
                       A.TRSP_SO_CMB_TP_CD,
                       A.TRSP_SO_STS_CD,
                       A.TRSP_WO_OFC_CTY_CD,
                       A.TRSP_WO_SEQ,
                       A.TRSP_COST_DTL_MOD_CD,
                       A.CGO_TP_CD,
                       NVL(C.VNDR_SEQ, A.VNDR_SEQ) VNDR_SEQ,
                       A.TRSP_CRR_MOD_CD,
                       A.FM_NOD_CD,
                       A.VIA_NOD_CD,
                       A.DOR_NOD_CD,
                       A.TO_NOD_CD,
                       A.FDR_VSL_CD,
                       A.FDR_SKD_VOY_NO,
                       A.FDR_SKD_DIR_CD,
                       A.IB_VVD_CD,
                       A.DOR_DE_ADDR,
                       A.REF_ID
                  FROM TRS_TRSP_SVC_ORD A ,
                       TRS_TRSP_WRK_ORD_PRV_TMP C
                 WHERE C.WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
                   AND C.WO_ISS_NO = 1
                   AND A.TRSP_SO_OFC_CTY_CD = C.TRSP_SO_OFC_CTY_CD
                   AND A.TRSP_SO_SEQ = C.TRSP_SO_SEQ
                   #if($sonumberArr.size() > 0) 
                       AND ( (A.TRSP_SO_OFC_CTY_CD, A.TRSP_SO_SEQ) IN (
                               #foreach( ${key} in ${sonumberArr}) 			
                                   #if($velocityCount == 1)
                                       ('${key.trspSoOfcCtyCd}', ${key.trspSoSeq})
                                   #else 
                                       , ('${key.trspSoOfcCtyCd}', ${key.trspSoSeq})		      		
                                   #end 
                               #end 
                           )
                       )
                   #end 
               ) B
       ) D
ORDER BY TRSP_SO_OFC_CTY_CD, TRSP_SO_SEQ			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
