<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCommonManageDBDAOValidationFdrLaneRSQL">
			<desc><![CDATA[ValidationFdrLane]]></desc>
			<sql><![CDATA[
SELECT DECODE(vsl_svc_tp_cd,'O','O','X') check_data
FROM mdm_vsl_svc_lane
WHERE vsl_slan_cd = @[check_data] AND NVL( delt_flg,'N') <> 'Y' 
--AND vsl_svc_tp_cd = 'O'			]]></sql>
			<params>
				<param name="check_data" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
