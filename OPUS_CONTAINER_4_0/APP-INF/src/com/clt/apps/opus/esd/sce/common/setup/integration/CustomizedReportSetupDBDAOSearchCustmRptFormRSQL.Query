<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomizedReportSetupDBDAOSearchCustmRptFormRSQL">
			<desc><![CDATA[Customized Report Setup 정보 조회]]></desc>
			<sql><![CDATA[
SELECT RPT_INFO_CTNT ALL_COL   -- report setup info              
FROM  SCE_PG_STUP_MST
WHERE EXE_USR_ID = @[user_id]	-- user id
AND   MOFC_ID    = @[user_ofc_cd]	-- user office
AND   PGM_NO = @[pgm_no]	-- program no.
AND   RPT_INFO_CTNT IS NOT NULL			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="pgm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
