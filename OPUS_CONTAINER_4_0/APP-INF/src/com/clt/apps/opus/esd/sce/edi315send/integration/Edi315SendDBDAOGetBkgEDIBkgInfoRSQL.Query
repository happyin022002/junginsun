<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetBkgEDIBkgInfoRSQL">
			<desc><![CDATA[GetBkgEDIBkgInfo]]></desc>
			<sql><![CDATA[
SELECT '{I_BKG_INFO'           ||CHR(10)||
        'IBI_POR_CD : ' ||NVL(POR_CTNT, ' ')    ||CHR(10)||
        'IBI_POR_CD_TP_CD : ' ||NVL(XTER_POR_TP_CD, ' ')    ||CHR(10)||
        'IBI_POR_NM : ' ||NVL(POR_NM,   ' ')    ||CHR(10)||
        'IBI_POL_CD : '    ||NVL(POL_CD, ' ')         ||CHR(10)||
        'IBI_POL_CD_TP_CD : '    ||NVL(XTER_POL_TP_CD, ' ')         ||CHR(10)||
        'IBI_POL_NM : ' ||NVL(POL_NM,   ' ')    ||CHR(10)||
  'IBI_POD_CD :' ||NVL(POD_CTNT, ' ')    ||CHR(10)||
  'IBI_POD_CD_TP_CD :' ||NVL(XTER_POD_TP_CD, ' ')    ||CHR(10)||
  'IBI_POD_NM : ' ||NVL(POD_NM,   ' ')    ||CHR(10)||
  'IBI_DEL_CD : ' ||NVL(DEL_CTNT, ' ')    ||CHR(10)||
  'IBI_DEL_CD_TP_CD : ' ||NVL(XTER_DEL_TP_CD, ' ')    ||CHR(10)||
  'IBI_DEL_NM : ' ||NVL(DEL_NM,   ' ')    ||CHR(10)||
    '}I_BKG_INFO'||CHR(10) I_BKG_INFO, rqst_dt
  FROM(
  SELECT inst.POR_CTNT, inst.POR_NM, mst.POL_CD,inst.POL_NM, inst.POD_CTNT,
         inst.POD_NM, inst.DEL_CTNT, inst.DEL_NM, mst.rqst_dt,
         inst.xter_por_tp_cd, inst.xter_pol_tp_cd, inst.xter_pod_tp_cd, inst.xter_del_tp_cd
  FROM bkg_xter_rqst_mst mst
        , bkg_xter_QTY qty
        , bkg_xter_instr inst
 where mst.xter_sndr_id  = qty.xter_sndr_id  (+)
   and mst.xter_rqst_no  = qty.xter_rqst_no  (+)
   and mst.xter_rqst_seq = qty.xter_rqst_seq (+)
   and mst.xter_sndr_id  = inst.xter_sndr_id (+)
   and mst.xter_rqst_no  = inst.xter_rqst_no (+)
   and mst.xter_rqst_seq = inst.xter_rqst_seq(+)
   AND mst.bkg_no        = @[e_bkg_no]
   --and mst.DOC_TP_CD     = 'B'
  -- and mst.XTER_SNDR_ID  <> 'WEB'
  -- and BKG_UPLD_STS_CD   = 'F'
   ORDER BY mst.rqst_dt DESC
  )
  WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
