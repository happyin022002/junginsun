<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOModifyTrsRailInvDtlDSQL">
			<desc><![CDATA[Rail Invoice 내역을 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM TRS_TRSP_RAIL_INV_DTL
WHERE INV_NO 		= @[invNo]
AND   INV_VNDR_SEQ 	= @[invVndrSeq]			]]></sql>
			<params>
				<param name="invNo" type="12" value="" out="N"/>
				<param name="invVndrSeq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
