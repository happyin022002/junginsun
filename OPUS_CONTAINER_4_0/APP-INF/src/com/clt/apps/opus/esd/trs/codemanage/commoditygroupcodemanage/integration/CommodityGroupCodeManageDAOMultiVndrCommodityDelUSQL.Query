<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDAOMultiVndrCommodityDelUSQL">
			<desc><![CDATA[Rail Vendor별로 정의된 Commodity Group을 Delete 상태로 변경]]></desc>
			<sql><![CDATA[
UPDATE	trs_trsp_cmdt_grp				        
SET		delt_flg			= 'Y',			   				
		upd_usr_id			= @[upd_usr_id],			   					
		upd_dt			= TO_DATE(@[upd_dt], 'YYYYMMDD HH24:MI:SS')			   								
WHERE	trsp_grp_cmdt_cd	= @[trsp_grp_cmdt_cd]			   							
		AND vndr_seq		= @[vndr_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="trsp_grp_cmdt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
