<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetPreSendRsltCountByNodRSQL">
			<desc><![CDATA[GetPreSendRsltCountByNod]]></desc>
			<sql><![CDATA[
SELECT count(1) cnt
  FROM SCE_EDI_MNG_AMS_STS M
     , SCE_EDI_SND_RSLT R
     , SCE_COP_HDR H
 WHERE 1 = 1
   AND M.EDI_ORG_STS_CD = @[sts_cd]  
   AND R.EDI_STS_CD IN (M.EDI_PRE_SNT_STS_CD,@[sts_cd])
--   AND EDI_EVNT_STS_CD = 'AVN'
   AND R.NOD_CD LIKE SUBSTR(@[nod_cd],1,5)||'%'
   AND R.BKG_NO = @[bkg_no]
   AND R.CNTR_NO = @[cntr_no]
   AND R.BKG_NO = H.BKG_NO
   AND R.CNTR_NO = H.CNTR_NO
   AND M.EDI_ORG_STS_CD <> R.EDI_STS_CD			]]></sql>
			<params>
				<param name="sts_cd" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
