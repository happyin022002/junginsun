<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchVendorRSQL">
			<desc><![CDATA[Vendor 정보를 조회한다]]></desc>
			<sql><![CDATA[
SELECT						
	VNDR_SEQ 		VNDR_NO		
,	VNDR_LGL_ENG_NM VNDR_NM_ENG		
,	B.CURR_CD		VNDR_CNT_CURR_CD
FROM 						
	MDM_VENDOR		A		
,	MDM_CURRENCY 	B			
WHERE						
	A.VNDR_SEQ 		= @[combo_svc_provider]	
AND	A.VNDR_CNT_CD	= B.CNT_CD(+)		
AND	B.DELT_FLG(+)	= 'N'			
			]]></sql>
			<params>
				<param name="combo_svc_provider" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
