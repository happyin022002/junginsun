<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewIssuedEstmActIfUSQL">
			<desc><![CDATA[SAC_TRSP_ESTM_ACT_IF 테이블에 업데이트 처리]]></desc>
			<sql><![CDATA[
UPDATE SAC_TRSP_ESTM_ACT_IF
   SET CXL_FLG = 'Y'
     , CXL_DT = SYSDATE
     , UPD_USR_ID = 'TRS'
     , UPD_DT = SYSDATE
WHERE WO_NO IN (
        SELECT TRSP_WO_OFC_CTY_CD||TRSP_WO_SEQ
          FROM TRS_TRSP_WRK_ORD_PRV_TMP
         WHERE WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
           AND WO_ISS_NO = @[wo_iss_no]
		   AND (WO_CXL_FLG = 'Y' OR WO_ISS_STS_CD = 'R')
       )
--  AND ESTM_ACT_DIV_CD = 'E'			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
