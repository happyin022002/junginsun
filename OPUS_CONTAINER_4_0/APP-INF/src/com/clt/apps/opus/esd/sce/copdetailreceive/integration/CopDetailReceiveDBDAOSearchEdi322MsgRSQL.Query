<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchEdi322MsgRSQL">
			<desc><![CDATA[SearchEdi322Msg]]></desc>
			<sql><![CDATA[
select evnt_dt
,eq_no
,edi_322_sts_cd
,sndr_id
,rcvr_id
,evnt_yd_cd
,evnt_cty_nm
,evnt_ste_cd
,evnt_cnt_cd
,eq_desc_cd
,eq_sts_cd
,chss_edi_322_no
,vsl_cd
,lloyd_vsl_no
,vsl_nm
,vsl_voy_dir_no
,spcl_hndl_cd
,bl_edi_322_no
,bkg_edi_322_no
,cre_dt
,psn_cd
,pkup_edi_322_no
,err_msg
,rail_dest_n1st_eta_dt
from   edi_322_msg
where  evnt_dt = to_date(@[evnt_dt],'YYYYMMDDHH24MISS')
and    eq_no = @[eq_no]
and    edi_322_sts_cd = @[edi_322_sts_cd]			]]></sql>
			<params>
				<param name="evnt_dt" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="edi_322_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
