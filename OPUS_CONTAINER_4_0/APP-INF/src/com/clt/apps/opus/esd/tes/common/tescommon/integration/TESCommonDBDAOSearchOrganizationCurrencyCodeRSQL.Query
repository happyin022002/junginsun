<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchOrganizationCurrencyCodeRSQL">
			<desc><![CDATA[Currency Code Inquiry]]></desc>
			<sql><![CDATA[
SELECT	AR_CURR_CD 
FROM	MDM_ORGANIZATION 
WHERE	OFC_CD		= @[inv_ofc_cd]
AND	DELT_FLG	= 'N'			]]></sql>
			<params>
				<param name="inv_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
