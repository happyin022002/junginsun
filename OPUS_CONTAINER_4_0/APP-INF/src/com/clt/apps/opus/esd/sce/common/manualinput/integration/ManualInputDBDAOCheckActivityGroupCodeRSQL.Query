<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOCheckActivityGroupCodeRSQL">
			<desc><![CDATA[CheckActivityGroupCode]]></desc>
			<sql><![CDATA[
SELECT COUNT(0) AS CHK
FROM SCE_ACT_GRP
WHERE NOD_TP_CD = @[chk_nod_tp_cd]
AND BKG_TERM_CD = @[chk_bkg_term_cd]
AND BFR_TRSP_MOD_CD = @[chk_bfr_trsp_mod_cd]
AND AFT_TRSP_MOD_CD = @[chk_aft_trsp_mod_cd]
AND TRSP_BND_CD = @[chk_trsp_bnd_cd]
AND SPCL_NOD_TP_CD = @[chk_spcl_nod_tp_cd]			]]></sql>
			<params>
				<param name="chk_nod_tp_cd" type="12" value="" out="N"/>
				<param name="chk_bkg_term_cd" type="12" value="" out="N"/>
				<param name="chk_bfr_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="chk_aft_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="chk_trsp_bnd_cd" type="12" value="" out="N"/>
				<param name="chk_spcl_nod_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
