<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VendorCmDBDAOModifyRcvMsgStsCdUSQL">
			<desc><![CDATA[VendorCmDBDAOModifyRcvMsgStsCdUSQL]]></desc>
			<sql><![CDATA[
UPDATE TRS_EDI_USA_RCV_MSG M
   SET M.RCV_MSG_STS_CD = @[rcv_msg_sts_cd]
      ,M.UPD_DT         = SYSDATE
      ,M.UPD_USR_ID     = @[upd_usr_id]
 WHERE M.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND M.TRSP_SO_SEQ = @[trsp_so_seq]
   AND M.RCV_MSG_SEQ = @[rcv_msg_seq]			]]></sql>
			<params>
				<param name="rcv_msg_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="rcv_msg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
