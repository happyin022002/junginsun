<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewEdiDBDAOsearchFlatFileEsvMrnRSQL">
			<desc><![CDATA[NYK-Hawk_FFile(ESV_N)-JOEDI_EU_OUTBOUND
{MRN_NUMBER_LIST]]></desc>
			<sql><![CDATA[
SELECT T1.*
  FROM (
		SELECT A.MVMT_REF_NO AS MRN_NO
		  FROM TRS_TRSP_SVC_ORD SO
			  ,BKG_CSTMS_EUR_BL A
		 WHERE SO.VSL_CD = A.VSL_CD
		   AND SO.SKD_VOY_NO = A.SKD_VOY_NO
		   AND SO.SKD_DIR_CD = A.SKD_DIR_CD
		   AND SO.BL_NO = A.BL_NO
		   AND SO.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
		   AND SO.TRSP_SO_SEQ = @[trsp_so_seq]
 ) T1
 RIGHT OUTER JOIN DUAL
    ON 1 = 1			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
