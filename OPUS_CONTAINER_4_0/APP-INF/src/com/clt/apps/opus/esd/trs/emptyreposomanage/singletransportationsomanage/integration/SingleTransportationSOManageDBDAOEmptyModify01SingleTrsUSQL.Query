		<query name="SingleTransportationSOManageDBDAOEmptyModify01SingleTrsUSQL">
			<desc><![CDATA[f_cmd : MODIFY01]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD
   SET TRSP_SO_CMB_SEQ    = NULL
      ,CMB_SO_RLT_STS_FLG = NULL
      ,TRSP_SO_CMB_SRT_NO = NULL      
      ,LOCL_UPD_DT        = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(CRE_OFC_CD)
      ,UPD_USR_ID         = @[upd_usr_id]
      ,UPD_DT             = SYSDATE
 WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND TRSP_SO_SEQ 	= @[trsp_so_seq]
   AND TRSP_SO_CMB_SEQ = @[trsp_so_cmb_seq]
   AND NVL(DELT_FLG, 'N') = 'N'
			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
				<param name="trsp_so_cmb_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
