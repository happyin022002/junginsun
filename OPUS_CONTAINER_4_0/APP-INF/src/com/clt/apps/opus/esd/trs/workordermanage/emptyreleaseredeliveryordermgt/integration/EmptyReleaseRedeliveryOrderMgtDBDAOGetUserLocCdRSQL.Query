<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyReleaseRedeliveryOrderMgtDBDAOGetUserLocCdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT LOC_CD
  FROM MDM_ORGANIZATION O,
       COM_USER U
 WHERE U.USR_ID = @[user_id]
   AND O.OFC_CD = U.OFC_CD			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
