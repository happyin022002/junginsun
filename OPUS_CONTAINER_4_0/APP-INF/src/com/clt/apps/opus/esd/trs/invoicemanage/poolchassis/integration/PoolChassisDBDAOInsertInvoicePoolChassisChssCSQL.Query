<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisDBDAOInsertInvoicePoolChassisChssCSQL">
			<desc><![CDATA[Pool Chss Invoice 테이블에 자료를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_POOL_CHSS_INV                       
           ( INV_NO  						
            ,INV_VNDR_SEQ                                
            ,SUB_INV_SEQ                                 
            ,TRSP_POOL_CHSS_QTY                          
            ,TRSP_POOL_CHSS_INV_AMT                      
            ,TRSP_POOL_CHSS_TAX_AMT                      
            ,HNDL_PRD_FM_DT                              
            ,HNDL_PRD_TO_DT                              
            ,DELT_FLG 				          
            ,CRE_OFC_CD                                  
            ,CRE_USR_ID                                  
            ,CRE_DT 	
			,UPD_USR_ID                                  
            ,UPD_DT 
			,LOCL_CRE_DT
			,LOCL_UPD_DT				  
           )                                             
             SELECT  @[inv_no] 				  
                    ,@[paymt_sp_cd] 					
                    ,NVL(MAX(SUB_INV_SEQ) ,0 )+1         
                    ,@[trsp_pool_chss_qty] 					
                    ,@[trsp_pool_chss_inv_amt] 					
                    ,@[trsp_pool_chss_tax_amt]			          
                    ,TO_DATE( @[hndl_prd_fm_dt], 'YYYYMMDD')             
                    ,TO_DATE( @[hndl_prd_to_dt], 'YYYYMMDD')             
                    ,'N' 				  
                    ,@[ofc_cd]				  
                    ,@[usr_id] 					
                    ,SYSDATE
					,@[usr_id] 	
					,SYSDATE				
                    ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])
					,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])
               FROM TRS_TRSP_POOL_CHSS_INV               
              WHERE INV_NO = @[inv_no]                            
                AND INV_VNDR_SEQ = @[paymt_sp_cd]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="paymt_sp_cd" type="2" value="" out="N"/>
				<param name="trsp_pool_chss_qty" type="2" value="" out="N"/>
				<param name="trsp_pool_chss_inv_amt" type="6" value="" out="N"/>
				<param name="trsp_pool_chss_tax_amt" type="6" value="" out="N"/>
				<param name="hndl_prd_fm_dt" type="12" value="" out="N"/>
				<param name="hndl_prd_to_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
