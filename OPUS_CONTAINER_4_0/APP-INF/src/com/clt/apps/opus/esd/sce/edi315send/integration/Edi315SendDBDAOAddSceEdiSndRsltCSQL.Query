<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOAddSceEdiSndRsltCSQL">
			<desc><![CDATA[for AddSceEdiSndRslt
AddSceEdiSndRslt
Edi315SendDBDAOAddSceEdiSndRsltCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_EDI_SND_RSLT
(
   EDI_GRP_CD,
   EDI_STS_CD,
   EDI_SUB_STS_CD,
   BKG_NO,
   CNTR_NO,
   PO_NO,             
   EDI_SND_KNT,                                       
   BL_NO,
   NOD_CD,              
   ACT_DT,            
   MNL_FLG,             
   EDI_SND_RMK,
   EDI_SND_TP_CD,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   EDI_SND_YRMONDY,
   EDI_SND_SEQ,
   PKUP_EDI_322_NO,
   EDI_SND_RSV_DT,
   VSL_CD,
   SKD_VOY_NO,
   SKD_DIR_CD,
   GMT_DT,
   FLT_FILE_REF_NO,
   COP_NO,
   TRSP_LEG_NM
)
VALUES
(
			@[edi_grp_cd],--EDI_GRP_CD
			@[edi_sts_cd],--EDI_STS_CD
			@[edi_sub_sts_cd],--EDI_SUB_STS_CD
			@[bkg_no],--BKG_NO
			@[cntr_no],--CNTR_NO
			nvl(@[po_no],'  '),--PO_NO
			@[edi_snd_knt],--EDI_SND_KNT
			@[bl_no],--BL_NO
			@[nod_cd],--NOD_CD
			TO_DATE(substr(@[act_dt], 1, 14), 'YYYY/MM/DD HH24:MI:SS'),--ACT_DT
			nvl(@[man_flg],'N'),--MNL_FLG
			decode(@[rslt_flag],'Y','SUCCESS(SENT)','R','RESERVED','L','SAVED','F','FAIL','N','FAIL',''),--EDI_SND_RMK
			@[rslt_flag],--EDI_SND_TP_CD
			nvl(@[cre_usr_id],'-'),
			sysdate,
			nvl(@[upd_usr_id],'-'),
			sysdate,
			nvl(@[edi_snd_yrmondy],to_char(sysdate,'yymmdd')),--EDI_SND_YRMONDY
			nvl(@[edi_snd_seq],'0'),--EDI_SND_SEQ
			@[pkup_edi_322_no],--PKUP_EDI_322_NO
			decode(@[rslt_flag], 'R', sysdate + (TO_NUMBER(@[edi_snd_itval_hr])/24), ''),--EDI_SND_RSV_DT
			substr(@[trunk_vvd],1,4),--VSL_CD
			substr(@[trunk_vvd],5,4),--SKD_VOY_NO
			substr(@[trunk_vvd],9,1),--SKD_DIR_CD
			decode(@[rslt_flag],'L', GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),1,5), SYSDATE, SUBSTR(@[nod_cd], 1, 5))
                               ,'K', GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),1,5), SYSDATE, SUBSTR(@[nod_cd], 1, 5))
                               ,'Y', GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),1,5), SYSDATE, SUBSTR(@[nod_cd], 1, 5))
							   ,'R', GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),1,5), SYSDATE, SUBSTR(@[nod_cd], 1, 5))
								   , TO_DATE('20090101000000','yyyymmddhh24miss')),--GMT_DT
			nvl(@[flt_file_ref_no],''),--FLT_FILE_REF_NO
			nvl(@[cop_no],''),
            nvl(@[trsp_leg_nm],'')--2015.08.04 TRSP_LEG_NM
)			]]></sql>
			<params>
				<param name="edi_grp_cd" type="12" value="" out="N"/>
				<param name="edi_sts_cd" type="12" value="" out="N"/>
				<param name="edi_sub_sts_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="po_no" type="12" value="  " out="N"/>
				<param name="edi_snd_knt" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="man_flg" type="12" value="" out="N"/>
				<param name="rslt_flag" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="DD" out="N"/>
				<param name="upd_usr_id" type="12" value="DD" out="N"/>
				<param name="edi_snd_yrmondy" type="12" value="" out="N"/>
				<param name="edi_snd_seq" type="12" value="" out="N"/>
				<param name="pkup_edi_322_no" type="12" value="" out="N"/>
				<param name="edi_snd_itval_hr" type="12" value="" out="N"/>
				<param name="trunk_vvd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="trsp_leg_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
