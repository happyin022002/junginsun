<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRailScgDBDAOUpdateRailFixScgAgmtUSQL">
			<desc><![CDATA[US RAIL Surcharge 화면의 US RAIL Agreement Fixed Surcharge 저장( Update 시)]]></desc>
			<sql><![CDATA[
UPDATE TRS_AGMT_RAIL_SCG_RT
	SET LBS_OVR_WGT = @[lbs_ovr_wgt],
		CURR_CD = @[curr_cd],
		FX_SCG_ALL_RT = @[fx_scg_all_rt],
		FX_SCG_20FT_RT = @[fx_scg_20ft_rt],
		FX_SCG_40FT_RT = @[fx_scg_40ft_rt],
		FX_SCG_45FT_RT = @[fx_scg_45ft_rt],
		EFF_FM_DT = TO_DATE(@[eff_fm_dt], 'YYYYMMDD'),
		EFF_TO_DT = TO_DATE(@[eff_to_dt], 'YYYYMMDD'),
		FUEL_SCG_APLY_FLG = @[fuel_scg_aply_flg],
		DELT_FLG = @[delt_flg],
		UPD_USR_ID = '${sUsrId}',
		UPD_DT = SYSDATE,
		LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC('${sctrlOfcCd}'),
        COM_SCG_APLY_FLG = @[com_scg_aply_flg],
        WO_APLY_FLG = @[wo_aply_flg],
        USR_DEF_RMK = @[usr_def_rmk],
        SPCL_CGO_CNTR_TP_CD = @[spcl_cgo_cntr_tp_cd],
        AGMT_COST_FLG = @[agmt_cost_flg]
WHERE	TRSP_AGMT_SCG_SEQ = @[trsp_agmt_scg_seq]			]]></sql>
			<params>
				<param name="lbs_ovr_wgt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="fx_scg_all_rt" type="12" value="" out="N"/>
				<param name="fx_scg_20ft_rt" type="12" value="" out="N"/>
				<param name="fx_scg_40ft_rt" type="12" value="" out="N"/>
				<param name="fx_scg_45ft_rt" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="fuel_scg_aply_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="com_scg_aply_flg" type="12" value="" out="N"/>
				<param name="wo_aply_flg" type="12" value="" out="N"/>
				<param name="usr_def_rmk" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="agmt_cost_flg" type="12" value="" out="N"/>
				<param name="trsp_agmt_scg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
