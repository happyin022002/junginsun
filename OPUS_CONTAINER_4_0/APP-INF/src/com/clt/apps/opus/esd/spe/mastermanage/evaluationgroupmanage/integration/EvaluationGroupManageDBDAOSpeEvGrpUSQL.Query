<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupManageDBDAOSpeEvGrpVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE SPE_EV_GRP
SET EG_RHQ_CD     = @[eg_rhq_cd],
    EG_CTY_CD     = UPPER(@[eg_cty_cd]),
    SVC_CATE_CD   = DECODE(@[svc_cate_cd], 'Truck','TR',	 'Rail','RL',	  'ODCY','CY', 'Terminal','TM',	 'Water','WT', 'EQ M&R','EQ'),
    EG_PIC_USR_ID = @[eg_pic_usr_id],
    UPD_USR_ID    = @[upd_usr_id],
    UPD_DT        = SYSDATE
WHERE EG_ID       = SUBSTR(@[eg_id],1,5)
  AND EG_ID_SEQ   = @[eg_id_seq]				]]></sql>
			<params>
				<param name="eg_rhq_cd" type="12" value="" out="N"/>
				<param name="eg_cty_cd" type="12" value="" out="N"/>
				<param name="svc_cate_cd" type="12" value="" out="N"/>
				<param name="eg_pic_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="eg_id_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
