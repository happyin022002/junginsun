<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WebGateDBDAOGetFullBkgNoRSQL">
			<desc><![CDATA[Booking Number를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_NO AS FULL_BOOKING_NO 
  FROM BKG_BOOKING 
 WHERE BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="SHAE30660402" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
