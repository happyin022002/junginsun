		<query name="WorkOrderPreviewDBDAOUpdateWorkOrderPreviewDraftStatusUSQL">
			<desc><![CDATA[WorkOrderPreviewDBDAOUpdateWorkOrderPreviewDraftStatusUSQL]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_WRK_ORD_PRV_TMP T
	 SET T.WO_ISS_STS_CD = 'N'
 WHERE T.WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
	 AND T.WO_ISS_NO = @[wo_iss_no]
	 AND T.WO_ISS_STS_CD = 'X'
	 AND T.WO_CXL_FLG = 'Y'			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
