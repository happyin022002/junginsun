<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageDBDAOUpdateEmptyDetPsUSQL">
			<desc><![CDATA[UpdateEmptyDetPs]]></desc>
			<sql><![CDATA[
UPDATE prd_inlnd_rout_mst m
   SET inlnd_rout_inv_bil_patt_cd = @[i_inv]
      ,rout_pln_cd                = @[i_rout_pln_cd]
      ,mcntr_rout_flg             = ''
      ,inlnd_rout_rmk             = @[i_route_rmk]
      ,wrs_full_cmdt_cd           = ''
      ,wrs_mty_cmdt_cd            = @[wrs_chk]
      ,inlnd_rout_bkg_flg         = ''
      ,upd_usr_id                 = @[upd_usr_id]
      ,upd_dt                     = sysdate
      ,d2_capa_flg                = NVL2(@[wrs_chk], d2_capa_flg, 'N')
      ,d4_capa_flg                = NVL2(@[wrs_chk], d4_capa_flg, 'N')
      ,d5_capa_flg                = NVL2(@[wrs_chk], d5_capa_flg, 'N')
      ,d7_capa_flg                = NVL2(@[wrs_chk], d7_capa_flg, 'N')
      ,o2_capa_flg                = NVL2(@[wrs_chk], o2_capa_flg, 'N')
      ,o4_capa_flg                = NVL2(@[wrs_chk], o4_capa_flg, 'N')
      ,a2_capa_flg                = NVL2(@[wrs_chk], a2_capa_flg, 'N')
      ,a4_capa_flg                = NVL2(@[wrs_chk], a4_capa_flg, 'N')
      ,r5_capa_flg                = NVL2(@[wrs_chk], r5_capa_flg, 'N')
      ,r2_capa_flg                = NVL2(@[wrs_chk], r2_capa_flg, 'N')
      ,r4_capa_flg                = NVL2(@[wrs_chk], r4_capa_flg, 'N')
 WHERE m.rout_org_nod_cd = @[i_rout_org_nod_cd]
   AND m.rout_dest_nod_cd = @[i_rout_dest_nod_cd]
   AND m.rout_seq = @[i_rout_seq]
			]]></sql>
			<params>
				<param name="i_inv" type="12" value="" out="N"/>
				<param name="i_rout_pln_cd" type="12" value="" out="N"/>
				<param name="i_route_rmk" type="12" value="" out="N"/>
				<param name="wrs_chk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="i_rout_org_nod_cd" type="12" value="" out="N"/>
				<param name="i_rout_dest_nod_cd" type="12" value="" out="N"/>
				<param name="i_rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
