<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESInvoiceCommonDBDAOSearchOldInvExistChkRSQL">
			<desc><![CDATA[Invoice가 처음 만들어진 후 3개월이 경과한 건이 있는지 check하는 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT DECODE( SIGN(COUNT(0)), 1, 'Y', 'N') AS INV_EXIST_CHK
FROM TES_TML_SO_HDR H
WHERE (H.DELT_FLG = 'N' OR DELT_FLG IS NULL)
  AND H.CRE_DT < SYSDATE-90 
  AND H.INV_OFC_CD = @[ofc_cd]
  AND H.TML_INV_STS_CD IN ('R', 'C')			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
