<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCustRefNoRSQL">
			<desc><![CDATA[for getCustRefNo]]></desc>
			<sql><![CDATA[
SELECT CUST_REF_NO_CTNT
FROM BKG_REFERENCE
WHERE 1=1
AND BKG_NO = @[bkg_no]
AND BKG_REF_TP_CD = @[bkg_ref_tp_cd] --'EBRF'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_ref_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
