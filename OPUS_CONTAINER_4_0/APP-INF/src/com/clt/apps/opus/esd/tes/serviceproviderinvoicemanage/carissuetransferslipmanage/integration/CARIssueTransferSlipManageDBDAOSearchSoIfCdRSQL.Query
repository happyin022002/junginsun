<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CARIssueTransferSlipManageDBDAOSearchSoIfCdRSQL">
			<desc><![CDATA[SearchSoIfCd]]></desc>
			<sql><![CDATA[
SELECT so_if_cd
FROM mdm_organization
WHERE ofc_cd = (SELECT AR_OFC_CD FROM   MDM_ORGANIZATION WHERE  OFC_CD = @[ofc_cd])			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
