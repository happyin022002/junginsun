<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCreateManageDBDAOSelectPrdProdMstByCnstSvcRSQL">
			<desc><![CDATA[SelectPrdProdMstByCnstSvc]]></desc>
			<sql><![CDATA[
SELECT PCTL_NO
	FROM PRD_PROD_CTL_MST X
 WHERE X.PCTL_NO LIKE @[hd_pctl_no]||'%'
	 AND X.CNST_FLG = 'X'			]]></sql>
			<params>
				<param name="hd_pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
