<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailBillingReqCreateDBDAOsearchVndrSeqRSQL">
			<desc><![CDATA[searchVndrSeq]]></desc>
			<sql><![CDATA[
        	 SELECT   sub_rail_seq                 
        	         ,vndr_seq                     
        	     FROM trs_trsp_rail_bil_vndr_set   
        	    WHERE trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]       
        	      AND trsp_so_seq = @[trsp_so_seq]              
        	 ORDER BY sub_rail_seq                 
			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
