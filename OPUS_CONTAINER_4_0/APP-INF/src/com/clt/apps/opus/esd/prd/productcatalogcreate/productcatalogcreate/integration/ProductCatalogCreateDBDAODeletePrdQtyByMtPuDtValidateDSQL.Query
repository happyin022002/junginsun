<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProductCatalogCreateDBDAODeletePrdQtyByMtPuDtValidateDSQL">
			<desc><![CDATA[DeletePrdQtyByMtPuDtValidate]]></desc>
			<sql><![CDATA[

DELETE PRD_PROD_CTL_QTY Q
WHERE NOT EXISTS
(
    SELECT 'X' FROM PRD_PROD_CTL_ROUT_DTL D
    WHERE D.PCTL_NO = Q.PCTL_NO
)
AND PCTL_NO = @[pctl_no]			]]></sql>
			<params>
				<param name="pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
