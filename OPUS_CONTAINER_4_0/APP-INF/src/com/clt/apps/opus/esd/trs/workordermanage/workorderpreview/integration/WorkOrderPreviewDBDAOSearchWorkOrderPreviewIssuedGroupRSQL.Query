<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchWorkOrderPreviewIssuedGroupRSQL">
			<desc><![CDATA[searchWorkOrderPreviewIssuedGroup]]></desc>
			<sql><![CDATA[
SELECT trsp_so_ofc_cty_cd							
	  ,trsp_so_seq									
	  ,@[wo_prv_grp_seq] wo_prv_grp_seq
	  ,@[wo_iss_sts_cd] wo_iss_sts_cd
	  ,1 wo_iss_no
	  ,NVL(@[vndr_seq], vndr_seq) vndr_seq
	  ,NVL(@[wo_cxl_flg], 'N') wo_cxl_flg
	  ,NVL(@[dtn_use_flg],dtn_use_flg)	dtn_use_flg
	  ,NVL(@[wo_bl_no_iss_flg],wo_bl_no_iss_flg) wo_bl_no_iss_flg
	  ,NVL(@[curr_cd],curr_cd) curr_cd
	  ,NVL(@[bzc_amt],bzc_amt) bzc_amt
	  ,NVL(@[nego_amt],nego_amt) nego_amt
	  ,NVL(@[etc_add_amt],etc_add_amt) etc_add_amt
	  ,NVL(@[fuel_scg_amt],fuel_scg_amt) fuel_scg_amt
	  ,NVL(@[ovr_wgt_scg_amt],ovr_wgt_scg_amt) ovr_wgt_scg_amt		
	  ,NVL(@[n3pty_bil_flg],n3pty_bil_flg) n3pty_bil_flg			
	  ,@[usd_ttl_amt] usd_ttl_amt
	  ,NVL(@[cust_cnt_cd],cust_cnt_cd) cust_cnt_cd
	  ,NVL(@[cust_seq],cust_seq) cust_seq		
	  ,NVL(@[cust_nomi_trkr_flg],cust_nomi_trkr_flg) cust_nomi_trkr_flg
	  ,NVL(@[trsp_agmt_rt_tp_cd],trsp_agmt_rt_tp_cd) trsp_agmt_rt_tp_cd
	  ,NVL(@[trsp_agmt_wy_tp_cd],trsp_agmt_wy_tp_cd) trsp_agmt_wy_tp_cd
	  ,NVL(@[trsp_frst_flg],trsp_frst_flg) trsp_frst_flg			
	  ,NVL(@[trsp_rjct_rsn_cd],trsp_rjct_rsn_cd) trsp_rjct_rsn_cd	
	  ,NVL(@[trsp_dflt_vndr_flg],trsp_dflt_vndr_flg) trsp_dflt_vndr_flg
	  ,NVL(@[n1st_nod_pln_dt],TO_CHAR(n1st_nod_pln_dt, 'YYYYMMDDHH24MISS')) n1st_nod_pln_dt
	  ,NVL(@[lst_nod_pln_dt],TO_CHAR(lst_nod_pln_dt, 'YYYYMMDDHH24MISS')) lst_nod_pln_dt
	  ,NVL(@[dor_nod_pln_dt],TO_CHAR(dor_nod_pln_dt, 'YYYYMMDDHH24MISS')) dor_nod_pln_dt
	  ,NVL(@[inter_rmk],inter_rmk) inter_rmk					
	  ,NVL(@[spcl_instr_rmk],spcl_instr_rmk) spcl_instr_rmk
	  ,NVL(@[fctry_nm],fctry_nm) fctry_nm		
	  ,NVL(@[dor_pst_cd],dor_pst_cd) dor_pst_cd
	  ,NVL(@[cntc_pson_phn_no],cntc_pson_phn_no) cntc_pson_phn_no
	  ,NVL(@[cntc_pson_fax_no],cntc_pson_fax_no) cntc_pson_fax_no
	  ,NVL(@[cntc_pson_nm],cntc_pson_nm) cntc_pson_nm
	  ,@[n3pty_cust_cnt_cd] n3pty_cust_cnt_cd
	  ,@[n3pty_cust_seq] n3pty_cust_seq
	  ,@[n3pty_desc] n3pty_desc
	  ,@[n3pty_vndr_seq] n3pty_vndr_seq
	  ,@[n3pty_ofc_cd] n3pty_ofc_cd
	  ,@[n3pty_bil_bzc_amt] n3pty_bil_bzc_amt
	  ,@[n3pty_bil_tp_cd] n3pty_bil_tp_cd
	  ,@[n3pty_curr_cd] n3pty_curr_cd
	  ,@[trsp_agmt_ofc_cty_cd] trsp_agmt_ofc_cty_cd
	  ,@[trsp_agmt_seq] trsp_agmt_seq
	  ,NVL(@[wgt_meas_ut_cd], wgt_meas_ut_cd) wgt_meas_ut_cd
	  ,NVL(@[cntr_kgs_wgt], cntr_kgs_wgt) cntr_kgs_wgt
      ,NVL(@[cntr_lbs_wgt], cntr_lbs_wgt) cntr_lbs_wgt
	  ,CASE WHEN  TRSP_COST_DTL_MOD_CD = 'CY'  OR (TRSP_COST_DTL_MOD_CD = 'DR'  AND TRS_GET_FOC_INFO_FNC(BL_NO, TRSP_SO_OFC_CTY_CD, TRSP_SO_SEQ, NULL) = DECODE(SUBSTR(DOR_NOD_CD, 1, 2), 'US', 'YYY', 'YY') ) THEN @[cntr_pkup_no] END cntr_pkup_no
      ,@[scg_ind_cd] scg_ind_cd
  FROM trs_trsp_svc_ord
 WHERE trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
   AND trsp_so_seq = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_sts_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="wo_cxl_flg" type="12" value="" out="N"/>
				<param name="dtn_use_flg" type="12" value="" out="N"/>
				<param name="wo_bl_no_iss_flg" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="bzc_amt" type="12" value="" out="N"/>
				<param name="nego_amt" type="12" value="" out="N"/>
				<param name="etc_add_amt" type="12" value="" out="N"/>
				<param name="fuel_scg_amt" type="12" value="" out="N"/>
				<param name="ovr_wgt_scg_amt" type="12" value="" out="N"/>
				<param name="n3pty_bil_flg" type="12" value="" out="N"/>
				<param name="usd_ttl_amt" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nomi_trkr_flg" type="12" value="" out="N"/>
				<param name="trsp_agmt_rt_tp_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_wy_tp_cd" type="12" value="" out="N"/>
				<param name="trsp_frst_flg" type="12" value="" out="N"/>
				<param name="trsp_rjct_rsn_cd" type="12" value="" out="N"/>
				<param name="trsp_dflt_vndr_flg" type="12" value="" out="N"/>
				<param name="n1st_nod_pln_dt" type="12" value="" out="N"/>
				<param name="lst_nod_pln_dt" type="12" value="" out="N"/>
				<param name="dor_nod_pln_dt" type="12" value="" out="N"/>
				<param name="inter_rmk" type="12" value="" out="N"/>
				<param name="spcl_instr_rmk" type="12" value="" out="N"/>
				<param name="fctry_nm" type="12" value="" out="N"/>
				<param name="dor_pst_cd" type="12" value="" out="N"/>
				<param name="cntc_pson_phn_no" type="12" value="" out="N"/>
				<param name="cntc_pson_fax_no" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="n3pty_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="n3pty_cust_seq" type="12" value="" out="N"/>
				<param name="n3pty_desc" type="12" value="" out="N"/>
				<param name="n3pty_vndr_seq" type="12" value="" out="N"/>
				<param name="n3pty_ofc_cd" type="12" value="" out="N"/>
				<param name="n3pty_bil_bzc_amt" type="12" value="" out="N"/>
				<param name="n3pty_bil_tp_cd" type="12" value="" out="N"/>
				<param name="n3pty_curr_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="wgt_meas_ut_cd" type="12" value="" out="N"/>
				<param name="cntr_kgs_wgt" type="12" value="" out="N"/>
				<param name="cntr_lbs_wgt" type="12" value="" out="N"/>
				<param name="cntr_pkup_no" type="12" value="" out="N"/>
				<param name="scg_ind_cd" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
