<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailBillingCancelManageDBDAOMultiCancelRailBillingListSoSeqRSQL">
			<desc><![CDATA[Rail So Sequence, Spnd Flag 정보 조회]]></desc>
			<sql><![CDATA[
SELECT  
    TRSP_SO_SEQ,
    SPND_FLG 
FROM  
    TRS_TRSP_RAIL_BIL_ORD 
WHERE TRSP_SO_OFC_CTY_CD = @[trspSoOfcCtyCd]
AND   TRSP_SO_SEQ = @[trspSoSeq]			]]></sql>
			<params>
				<param name="trspSoOfcCtyCd" type="12" value="" out="N"/>
				<param name="trspSoSeq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
