<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OutstandinggroupManageDBDAOSearchTPBGroupRemakingRSQL">
			<desc><![CDATA[TPBGroupRemaking]]></desc>
			<sql><![CDATA[
SELECT '' AS GRP_SRT_NO,
	   A.OTS_DTL_SEQ,
	   A.N3PTY_NO AS N3PTY_NO_ORG,
	   A.N3PTY_NO_DP_SEQ AS N3PTY_NO_DP_SEQ_ORG,
       A.N3PTY_NO AS N3PTY_NO,
       A.N3PTY_NO_DP_SEQ,
       A.N3PTY_EXPN_TP_CD,
       A.N3PTY_BIL_TP_CD,
       TPB_GET_N3PTY_BIL_TP_NM_FNC(A.N3PTY_BIL_TP_CD) AS N3PTY_BIL_TP_NM,
       A.N3PTY_SRC_NO,
       A.BKG_NO AS BKG_NO_ALL,
       A.BL_NO AS BL_NO_ALL,
       A.VSL_CD || A.SKD_VOY_NO || SUBSTR(A.FINC_DIR_CD,1,1) AS REV_VVD,
       A.VVD_CD AS ACT_VVD, A.CSR_NO, SUBSTRB(A.GL_DT,1,6) AS GL_MONTH,
       A.EQ_NO,
       CASE A.VNDR_CUST_DIV_CD WHEN 'V' THEN LPAD(TO_CHAR(A.VNDR_SEQ),6,'0')
			WHEN 'C' THEN A.CUST_CNT_CD || LPAD(A.CUST_SEQ,6,'0')
			WHEN 'S' THEN A.N3PTY_OFC_CD
	   END AS TRD_PARTY_CODE,
	   CASE A.VNDR_CUST_DIV_CD WHEN 'V' THEN A.VNDR_LGL_ENG_NM
			WHEN 'C' THEN A.CUST_LGL_ENG_NM
			WHEN 'S' THEN A.N3PTY_OFC_CD
       END AS TRD_PARTY_NAME,
	   COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD00588','Z') AS OTS_STS_NM,
       DECODE( (SELECT COUNT(1) FROM TPB_OTS_GRP G, TPB_ADJ_STS D, TPB_OTS_GRP_STS S
    			 WHERE G.N3PTY_DELT_TP_CD = 'N'
				   --AND D.STL_STS_LST_FLG = 'Y'
				   AND S.OTS_STS_LST_FLG = 'Y'
				   AND D.STL_TO_CLT_CNG_OFC_CD IS NOT NULL
				   AND D.STL_APRO_OFC_CD IS NOT NULL
				   AND G.N3PTY_NO = D.N3PTY_NO
				   AND D.N3PTY_NO = S.N3PTY_NO
				   AND D.N3PTY_NO = A.N3PTY_NO),0,'Y','N') AS CANDIDATE_YN,
       DECODE(A.FM_CLT_CNG_OFC_N3PTY_NO,NULL,'Y','N') AS ROC_CANDIDATE_YN,
	   A.IF_CURR_CD,
	   A.IF_AMT,
	   A.CFM_CURR_CD,
	   A.CFM_AMT
  FROM TPB_OTS_DTL A
 WHERE 1=1
   AND A.N3PTY_NO IS NULL
   AND A.OFC_CD IN ( SELECT OFC_CD FROM TPB_HNDL_OFC WHERE N3PTY_OFC_TP_CD = 'T' AND DELT_FLG = 'N' AND RHQ_CD = @[s_if_rhq_cd] AND OFC_CD = @[s_if_ofc_cd] )
   AND A.N3PTY_DELT_TP_CD IN ('N','C','S')
   AND A.N3PTY_BIL_TP_CD IN ( SELECT N3PTY_BIL_TP_CD FROM (SELECT N3PTY_BIL_TP_CD FROM TPB_N3RD_PTY_BIL_TP WHERE ACT_FLG = 'Y' AND N3PTY_BIL_TP_CD != 'JO') )
#if (${s_n3pty_no_search} == '' && ${s_bkg_no_all} == '' && ${s_bl_no_all} == '' && ${s_sdate} != '' && ${s_edate} != '' && ${s_user_ofc_cd} != '')
   AND A.IF_DT BETWEEN TPB_GET_SRCH_DATE_FNC(@[s_sdate],'YYYY-MM-DD',@[s_user_ofc_cd]) AND TPB_GET_SRCH_DATE_FNC(@[s_edate],'YYYY-MM-DD',@[s_user_ofc_cd]) + 1
#end
#if (${s_n3pty_src_sub_sys_cd} != '')
   	AND A.N3PTY_EXPN_TP_CD = @[s_n3pty_src_sub_sys_cd]
    #if (${s_n3pty_bil_tp_cd} != '')
        AND A.N3PTY_BIL_TP_CD = @[s_n3pty_bil_tp_cd]
   	#end
#end
#if (${s_vndr_cust_div_cd} != '')
   	AND A.VNDR_CUST_DIV_CD = @[s_vndr_cust_div_cd]
	#if (${s_trd_party_val} != '')
   	    #if (${s_vndr_cust_div_cd} == 'V')
   		    AND A.VNDR_SEQ = TO_NUMBER(@[s_trd_party_val])
		#elseif (${s_vndr_cust_div_cd} == 'C')
			AND A.CUST_CNT_CD = SUBSTRB(TRIM(@[s_vndr_cust_div_cd]),1,2)
		#elseif (${s_vndr_cust_div_cd} == 'S')
			AND A.N3PTY_OFC_CD = @[s_trd_party_val]
   		#end
   	#end
#end
#if (${s_vvd} != '')
   AND A.VSL_CD = SUBSTRB(@[s_vvd],1,4)
   AND A.SKD_VOY_NO = SUBSTRB(@[s_vvd],5,4)
   AND A.FINC_DIR_CD LIKE SUBSTRB(@[s_vvd],9) || '%'
#end
#if (${s_bkg_no_all} != '')
   AND A.BKG_NO = @[s_bkg_no_all]
#end
#if (${s_bl_no_all} != '')
   AND A.BL_NO = @[s_bl_no_all]
#end
#if (${s_eq_no} != '')
   AND A.EQ_NO = @[s_eq_no]
#end
#if (${s_n3pty_no_search} != '')
   AND 0=1	-- n3pty_no
#end
#if (${s_candidate_include_flag} == 'E')
   AND 0=1	-- candidate exclude
#end
--//
UNION ALL
--//
SELECT '' AS GRP_SRT_NO,
	   A.OTS_DTL_SEQ,
	   A.N3PTY_NO AS N3PTY_NO_ORG,
	   A.N3PTY_NO_DP_SEQ AS N3PTY_NO_DP_SEQ_ORG,
       A.N3PTY_NO AS N3PTY_NO,
       A.N3PTY_NO_DP_SEQ,
       A.N3PTY_EXPN_TP_CD,
       A.N3PTY_BIL_TP_CD,
       TPB_GET_N3PTY_BIL_TP_NM_FNC(A.N3PTY_BIL_TP_CD) AS N3PTY_BIL_TP_NM,
       A.N3PTY_SRC_NO,
       A.BKG_NO AS BKG_NO_ALL,
       A.BL_NO AS BL_NO_ALL,
       A.VSL_CD || A.SKD_VOY_NO || SUBSTR(A.FINC_DIR_CD,1,1) AS REV_VVD,
       A.VVD_CD AS ACT_VVD, A.CSR_NO, SUBSTRB(A.GL_DT,1,6) AS GL_MONTH, A.EQ_NO,
       CASE A.VNDR_CUST_DIV_CD WHEN 'V' THEN LPAD(TO_CHAR(A.VNDR_SEQ),6,'0')
			WHEN 'C' THEN A.CUST_CNT_CD || LPAD(A.CUST_SEQ,6,'0')
			WHEN 'S' THEN A.N3PTY_OFC_CD
       END AS TRD_PARTY_CODE,
       CASE A.VNDR_CUST_DIV_CD WHEN 'V' THEN A.VNDR_LGL_ENG_NM
			WHEN 'C' THEN A.CUST_LGL_ENG_NM
			WHEN 'S' THEN A.N3PTY_OFC_CD
       END AS TRD_PARTY_NAME,
       COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD00588',C.OTS_STS_CD) AS OTS_STS_NM,
       DECODE( (SELECT COUNT(1) FROM TPB_OTS_GRP G, TPB_ADJ_STS D, TPB_OTS_GRP_STS S
    			 WHERE G.N3PTY_DELT_TP_CD = 'N'
				   --AND D.STL_STS_LST_FLG = 'Y'
				   AND S.OTS_STS_LST_FLG = 'Y'
				   AND D.STL_TO_CLT_CNG_OFC_CD IS NOT NULL
				   AND D.STL_APRO_OFC_CD IS NOT NULL
				   AND G.N3PTY_NO = D.N3PTY_NO
				   AND D.N3PTY_NO = S.N3PTY_NO
				   AND D.N3PTY_NO = B.N3PTY_NO),0,'Y','N') AS CANDIDATE_YN,
       DECODE(A.FM_CLT_CNG_OFC_N3PTY_NO,NULL,'Y','N') AS ROC_CANDIDATE_YN,
       A.IF_CURR_CD,
       A.IF_AMT,
       A.CFM_CURR_CD,
       A.CFM_AMT
  FROM TPB_OTS_GRP B,
       TPB_OTS_DTL A,
       TPB_OTS_GRP_STS C
 WHERE 1=1
   AND B.N3PTY_NO = C.N3PTY_NO
   AND B.N3PTY_NO = A.N3PTY_NO
   AND B.OFC_CD IN ( SELECT OFC_CD FROM TPB_HNDL_OFC WHERE N3PTY_OFC_TP_CD = 'T' AND DELT_FLG = 'N' AND RHQ_CD = @[s_if_rhq_cd] AND OFC_CD = @[s_if_ofc_cd] )
   AND A.N3PTY_DELT_TP_CD IN ('N','S')
   AND B.N3PTY_DELT_TP_CD IN ('N')
   AND C.OTS_STS_LST_FLG = 'Y'
   AND C.OTS_STS_CD IN ('O','M','J')
   AND A.N3PTY_BIL_TP_CD IN ( SELECT N3PTY_BIL_TP_CD FROM (SELECT N3PTY_BIL_TP_CD FROM TPB_N3RD_PTY_BIL_TP WHERE ACT_FLG = 'Y' AND N3PTY_BIL_TP_CD != 'JO') )
#if (${s_n3pty_no_search} == '' && ${s_bkg_no_all} == '' && ${s_bl_no_all} == '' && ${s_sdate} != '' && ${s_edate} != '' && ${s_user_ofc_cd} != '')
   AND A.CFM_DT BETWEEN TPB_GET_SRCH_DATE_FNC(@[s_sdate],'YYYY-MM-DD',@[s_user_ofc_cd]) AND TPB_GET_SRCH_DATE_FNC(@[s_edate],'YYYY-MM-DD',@[s_user_ofc_cd]) + 1
#end
#if (${s_n3pty_src_sub_sys_cd} != '')
   	AND A.N3PTY_EXPN_TP_CD = @[s_n3pty_src_sub_sys_cd]
	#if (${s_n3pty_bil_tp_cd} != '')
   		AND A.N3PTY_BIL_TP_CD = @[s_n3pty_bil_tp_cd]
   	#end
#end
#if (${s_vndr_cust_div_cd} != '')
   	AND A.VNDR_CUST_DIV_CD = @[s_vndr_cust_div_cd]
	#if (${s_trd_party_val} != '')
   	    #if (${s_vndr_cust_div_cd} == 'V')
   		    AND A.VNDR_SEQ = TO_NUMBER(@[s_trd_party_val])
		#elseif (${s_vndr_cust_div_cd} == 'C')
			AND A.CUST_CNT_CD = SUBSTRB(TRIM(@[s_vndr_cust_div_cd]),1,2)
		#elseif (${s_vndr_cust_div_cd} == 'S')
			AND A.N3PTY_OFC_CD = @[s_trd_party_val]
   		#end
   	#end
#end
#if (${s_vvd} != '')
   AND A.VSL_CD = SUBSTRB(@[s_vvd],1,4)
   AND A.SKD_VOY_NO = SUBSTRB(@[s_vvd],5,4)
   AND A.FINC_DIR_CD LIKE SUBSTRB(@[s_vvd],9) || '%'
#end
#if (${s_bkg_no_all} != '')
   AND A.BKG_NO = @[s_bkg_no_all]
#end
#if (${s_bl_no_all} != '')
   AND A.BL_NO = @[s_bl_no_all]
#end
#if (${s_eq_no} != '')
   AND A.EQ_NO = @[s_eq_no]
#end
#if (${s_n3pty_no_search} != '')
   AND A.N3PTY_NO = @[s_n3pty_no_search]
#end
 ORDER BY N3PTY_NO, N3PTY_NO_DP_SEQ			]]></sql>
			<params>
				<param name="s_if_rhq_cd" type="12" value="" out="N"/>
				<param name="s_if_ofc_cd" type="12" value="" out="N"/>
				<param name="s_sdate" type="12" value="" out="N"/>
				<param name="s_user_ofc_cd" type="12" value="" out="N"/>
				<param name="s_edate" type="12" value="" out="N"/>
				<param name="s_n3pty_src_sub_sys_cd" type="12" value="" out="N"/>
				<param name="s_n3pty_bil_tp_cd" type="12" value="" out="N"/>
				<param name="s_vndr_cust_div_cd" type="12" value="" out="N"/>
				<param name="s_trd_party_val" type="12" value="" out="N"/>
				<param name="s_vvd" type="12" value="" out="N"/>
				<param name="s_bkg_no_all" type="12" value="" out="N"/>
				<param name="s_bl_no_all" type="12" value="" out="N"/>
				<param name="s_eq_no" type="12" value="" out="N"/>
				<param name="s_n3pty_no_search" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
