<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOInsertLinkConstraintCSQL">
			<desc><![CDATA[InsertLinkConstraint]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_LNK_CNST_MGMT ( 
   LNK_ORG_NOD_CD
  ,LNK_DEST_NOD_CD
  ,TRSP_MOD_CD
  ,LNK_CNST_ITM_CD
  ,PCTL_CNST_ITM_NM
  ,LNK_CNST_RMK
  ,CNTR_TP_CD
  ,EFF_FM_DT
  ,EFF_TO_DT
  ,SVC_USE_FLG
  ,CRE_DT
  ,CRE_USR_ID
  ,CRE_OFC_CD
  ,UPD_USR_ID
  ,CMDT_CD
  ,LNK_CNST_SEQ
  ,SPCL_CGO_CNTR_TP_CD
  ,CNTR_SZ_CD
) VALUES (
   @[s_node_fm]
  ,@[s_node_to]
  ,@[s_ts_mode]
  ,@[s_item_code]
  ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01386' AND INTG_CD_VAL_CTNT = @[s_item_code])
  ,@[s_remark]
  ,@[cntr_tp_cd]
  ,@[eff_fm_date]
  ,@[eff_to_date]
  ,@[s_svc_flg]
  ,SYSDATE
  ,@[cre_usr_id]
  ,@[cre_ofc_cd]
  ,@[upd_usr_id]
  ,@[s_commodity_code]
  ,NVL((select max(lnk_cnst_seq) + 1 from PRD_LNK_CNST_MGMT), 1)
  ,@[spcl_cgo_cntr_tp_cd]
  ,@[cntr_sz_cd]   
)			]]></sql>
			<params>
				<param name="s_node_fm" type="12" value="" out="N"/>
				<param name="s_node_to" type="12" value="" out="N"/>
				<param name="s_ts_mode" type="12" value="" out="N"/>
				<param name="s_item_code" type="12" value="" out="N"/>
				<param name="s_remark" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="eff_fm_date" type="12" value="" out="N"/>
				<param name="eff_to_date" type="12" value="" out="N"/>
				<param name="s_svc_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="s_commodity_code" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
