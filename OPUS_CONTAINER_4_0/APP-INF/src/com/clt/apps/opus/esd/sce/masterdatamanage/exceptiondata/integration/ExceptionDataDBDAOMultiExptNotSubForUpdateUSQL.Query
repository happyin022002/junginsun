<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExceptionDataDBDAOMultiExptNotSubForUpdateUSQL">
			<desc><![CDATA[update exptnotsub]]></desc>
			<sql><![CDATA[
update sce_expt_subsc_mst
set NTFD_OFC_CD = @[r_ntfd_ofc_cd]
	,NTFD_SUBSC_ID = @[r_global_id]
	,NTFD_SUBSC_NM = @[r_name]
	,NTFD_SUBSC_USR_EML = @[r_email]
	,ACT_FLG = @[r_act]
	,UPD_USR_ID = @[r_usr_id]
	,UPD_DT = SYSDATE
	,COP_EXPT_SUBSC_GRP_CD = @[r_subsc_grp_cd]
	,CTRL_OFC_CD = @[r_cnt_ofc]
where COP_EXPT_SUBSC_CS_SEQ = @[cop_expt_subsc_cs_seq]			]]></sql>
			<params>
				<param name="r_ntfd_ofc_cd" type="12" value="" out="N"/>
				<param name="r_global_id" type="12" value="" out="N"/>
				<param name="r_name" type="12" value="" out="N"/>
				<param name="r_email" type="12" value="" out="N"/>
				<param name="r_act" type="12" value="" out="N"/>
				<param name="r_usr_id" type="12" value="" out="N"/>
				<param name="r_subsc_grp_cd" type="12" value="" out="N"/>
				<param name="r_cnt_ofc" type="12" value="" out="N"/>
				<param name="cop_expt_subsc_cs_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
