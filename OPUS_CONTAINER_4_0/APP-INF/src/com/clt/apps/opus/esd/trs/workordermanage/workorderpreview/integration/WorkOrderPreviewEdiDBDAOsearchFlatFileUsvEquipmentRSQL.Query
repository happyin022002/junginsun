<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewEdiDBDAOsearchFlatFileUsvEquipmentRSQL">
			<desc><![CDATA[NYK-Hawk_FFile(USV_N)-JOEDIUPDT_IAS_OUTBOUND
- EQUIPMENT]]></desc>
			<sql><![CDATA[
SELECT T1.* 
FROM 
(
SELECT CNTR_NO
      ,BKG_NO
      ,CNTR_TPSZ
      ,PKG_QTY
      ,CASE WHEN TRSP_SO_TP_CD = 'M' THEN '' 
            ELSE TRIM(REGEXP_SUBSTR(WGT_STR, '[^|]+', 1, 1)) 
       END N_EGT
      ,TRIM(REGEXP_SUBSTR(WGT_STR, '[^|]+', 1, 2)) AS T_WGT
      ,TRIM(REGEXP_SUBSTR(WGT_STR, '[^|]+', 1, 3)) AS G_WGT
      ,WGTUNIT
      ,VOL
      ,VOLUNIT
      ,FREIGHT_DESC
      ,MTREL_NO
      ,HAZMAT
      ,OVWGT
      ,SHIP_NO
      ,'' AS HEIGHT
      ,'' AS WIDTH
      ,'' AS DEPTH
      ,NVL2(EQ_COMM, 'REEFER DETAILS:'||CHR(9)||EQ_COMM, '')  AS EQ_COMMENT
  FROM (SELECT SO.EQ_NO AS CNTR_NO
              ,SO.BKG_NO AS BKG_NO
              ,SO.EQ_TPSZ_CD AS CNTR_TPSZ
              ,CNTR.PCK_QTY AS PKG_QTY
              ,TRS_GET_COM_SO_RAIL_WGT_FNC('S', SO.TRSP_SO_OFC_CTY_CD, SO.TRSP_SO_SEQ, NULL, SO.BKG_NO, SO.EQ_NO, SO.EQ_TPSZ_CD,NVL(NVL(CNTR.WGT_UT_CD, SO.WGT_MEAS_UT_CD), 'KGS'), SO.COP_NO, 'Y') WGT_STR
              ,DECODE(NVL(NVL(CNTR.WGT_UT_CD, SO.WGT_MEAS_UT_CD), 'KGS'), 'KGS', 'KG', 'LBS', 'LB', '') AS WGTUNIT
              ,'' AS VOL
              ,'' AS VOLUNIT
              ,SUBSTR(REPLACE(REPLACE(NVL2(SO.EQ_NO, NVL(CNTR.DIFF_RMK, MC.CMDT_NM), MC.CMDT_NM), CHR(10), ''), CHR(13), ''), 1, 50) AS FREIGHT_DESC
			  ,NVL((CASE WHEN SO.TRSP_COST_DTL_MOD_CD = 'CY'  OR (SO.TRSP_COST_DTL_MOD_CD = 'DR' AND TRS_GET_FOC_INFO_FNC(SO.BL_NO, SO.TRSP_SO_OFC_CTY_CD, SO.TRSP_SO_SEQ, NULL) = DECODE(SUBSTR(DOR_NOD_CD, 1, 2), 'US', 'YYY', 'YY')) THEN SO.CNTR_PKUP_NO END) , SO.BKG_NO) AS MTREL_NO
              ,DECODE(BKG.DCGO_FLG, 'Y', 'true', 'false') AS HAZMAT
              ,'false' AS OVWGT
              ,CASE WHEN SO.TRSP_SO_TP_CD = 'M'  THEN SO.REF_ID
                    WHEN SO.TRSP_SO_TP_CD = 'Y' THEN SO.COP_NO
               END SHIP_NO
              ,C.EQ_COMM
              ,SO.TRSP_SO_TP_CD
          FROM TRS_TRSP_SVC_ORD SO
              ,BKG_BOOKING BKG
              ,BKG_CONTAINER CNTR
              ,MST_CONTAINER M
              ,MDM_CNTR_TP_SZ A
              ,MST_CNTR_SPEC X
              ,MDM_COMMODITY MC
              ,(SELECT TRS_JOIN_FNC(CURSOR (SELECT 'TEMPERATURE: ' || TEMP || 'C' || ' VENT SETTING: ' || VENT || ' HUMIDITY: ' || HUMIDITY || ' GENSET: ' || GENSET || ' DRAIN: ' || DRAIN_HOLE
                                       FROM (SELECT B.CDO_TEMP AS TEMP
                                                   ,CASE WHEN (NVL(B.VENT_RTO, 0) > 0 OR NVL(B.CBM_PER_HR_QTY, 0) > 0) THEN 'Open'
                                                         ELSE'Closed'
                                                    END VENT
                                                   ,B.HUMID_NO AS HUMIDITY
                                                   ,B.PWR_SPL_CBL_FLG AS GENSET
                                                   ,DECODE(B.CNTR_DRN_CD, 'O', 'Open', 'Closed') AS DRAIN_HOLE
                                               FROM BKG_RF_CGO       B
                                                   ,TRS_TRSP_SVC_ORD S
                                              WHERE S.BKG_NO = B.BKG_NO
                                                AND S.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
                                                AND S.TRSP_SO_SEQ = @[trsp_so_seq]
                                                AND NVL(S.EQ_NO, 'XX') = NVL2(S.EQ_NO, NVL(B.CNTR_NO, NVL(S.EQ_NO, 'XX')), 'XX')
                                                AND NVL(S.EQ_TPSZ_CD, 'XX') = NVL2(S.EQ_TPSZ_CD, NVL(B.CNTR_TPSZ_CD, NVL(S.EQ_TPSZ_CD, 'XX')), 'XX'))),
                                    CHR(9)) EQ_COMM
                  FROM DUAL) C
         WHERE SO.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
           AND SO.TRSP_SO_SEQ = @[trsp_so_seq]
           AND SO.BKG_NO = BKG.BKG_NO(+)
           AND SO.BKG_NO = CNTR.BKG_NO(+)
           AND SO.EQ_NO = CNTR.CNTR_NO(+)
           AND SO.EQ_NO = M.CNTR_NO(+)
           AND M.CNTR_SPEC_NO = X.CNTR_SPEC_NO(+)
           AND SO.EQ_TPSZ_CD = A.CNTR_TPSZ_CD(+)
           AND NVL(A.DELT_FLG(+), 'N') = 'N'
           AND BKG.CMDT_CD = MC.CMDT_CD(+)
		)
) T1 RIGHT OUTER JOIN DUAL
ON 1= 1			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
