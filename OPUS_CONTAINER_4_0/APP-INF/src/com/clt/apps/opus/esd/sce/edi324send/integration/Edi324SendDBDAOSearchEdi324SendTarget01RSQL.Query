<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi324SendDBDAOSearchEdi324SendTarget01RSQL">
			<desc><![CDATA[대상을 가상으로 만들기 위한 데이터 설정 > 사용하지않음.]]></desc>
			<sql><![CDATA[
SELECT G.VNDR_SEQ
     , D.COP_NO
     , D.COP_DTL_SEQ
     , TO_CHAR(D.ACT_DT,'YYYY-MM-DD HH24:MI:SS') POL_DEP_ACT_DT
     , D.NOD_CD POL_YD_CD
     , D.VSL_CD
     , D.SKD_VOY_NO
     , D.SKD_DIR_CD
     , H.BKG_NO
     , H.CNTR_NO
     , P.N1ST_NOD_CD ORG_YD_CD
     , DECODE(P.N4TH_NOD_CD, NULL, DECODE(P.N3RD_NOD_CD, NULL, P.N2ND_NOD_CD, P.N3RD_NOD_CD), P.N4TH_NOD_CD ) DEST_YD_CD
     , @[cre_usr_id] CRE_USR_ID
     , @[upd_usr_id] UPD_USR_ID 
  FROM EDI_324_GRP G
     , SCE_COP_HDR H
     , SCE_PLN_SO_LIST P
     , SCE_COP_DTL D
 WHERE 1=1   
   AND H.COP_NO = H.MST_COP_NO
   AND H.COP_NO = P.COP_NO
   AND H.COP_STS_CD = 'T'
   --AND G.CTRL_OFC_CD = P.CTRL_OFC_CD            --'PHXSC'
   AND P.TRSP_SO_STS_CD = 'P'
   AND P.PCTL_COST_MOD_CD = 'C'
   AND G.COST_ACT_GRP_CD = P.COST_ACT_GRP_CD    --'IYRD'
   AND G.VNDR_SEQ = P.N1ST_VNDR_SEQ             --119993
   AND G.EDI_SND_FLG = 'Y'
   AND G.DELT_FLG = 'N'
   AND H.COP_NO = D.COP_NO
   AND D.ACT_CD LIKE 'FL__D_'
   AND D.ACT_STS_CD IN('F','C','N') 
   AND D.ACT_DT BETWEEN TO_DATE('20120102','YYYYMMDD') AND TO_DATE('20120103','YYYYMMDD')
   AND NOT EXISTS
       (SELECT 'X'
          FROM SCE_EDI_324_SND_RSLT 
         WHERE 1 = 1
           AND VNDR_SEQ = 119993
           AND BKG_NO = H.BKG_NO
           AND CNTR_NO = H.CNTR_NO
           AND VSL_CD = D.VSL_CD
           AND SKD_VOY_NO = D.SKD_VOY_NO
           AND SKD_DIR_CD = D.SKD_DIR_CD
           AND MNL_FLG = 'N'
           AND EDI_SND_TP_CD = 'Y'
       )
   AND ROWNUM < 10
   ORDER BY  D.VSL_CD || D.SKD_VOY_NO ||  D.SKD_DIR_CD			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
