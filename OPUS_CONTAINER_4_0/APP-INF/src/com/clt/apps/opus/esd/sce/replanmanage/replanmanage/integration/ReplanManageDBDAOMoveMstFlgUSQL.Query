<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOMoveMstFlgUSQL">
			<desc><![CDATA[master cop no 를 변경함으로서 slave 와 master 를 맞바꾼다.]]></desc>
			<sql><![CDATA[
update sce_cop_hdr
set
mst_cop_no = @[to_cop_no]
where 
mst_cop_no in (@[fm_mst_cop_no], @[to_mst_cop_no])			]]></sql>
			<params>
				<param name="to_cop_no" type="12" value="" out="N"/>
				<param name="fm_mst_cop_no" type="12" value="" out="N"/>
				<param name="to_mst_cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
