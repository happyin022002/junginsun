<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyReleaseRedeliveryOrderMgtDBDAOGetDgCfmFlgRSQL">
			<desc><![CDATA[DG booking의 Special Cargo Approval 여부를 조회하는 SQL]]></desc>
			<sql><![CDATA[
SELECT CASE
           WHEN DCGO_FLG = 'N'
           THEN 'Y'
           WHEN BKG_STS_CD <> 'F'
           THEN 'N'
           ELSE 'Y'
       END DG_CFM_FLG
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
