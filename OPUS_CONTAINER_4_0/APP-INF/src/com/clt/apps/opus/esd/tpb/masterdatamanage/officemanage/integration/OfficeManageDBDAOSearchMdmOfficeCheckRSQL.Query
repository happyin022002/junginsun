<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeManageDBDAOSearchMdmOfficeCheckRSQL">
			<desc><![CDATA[OfficeManageDBDAO SearchMdmOfficeCheck]]></desc>
			<sql><![CDATA[
SELECT count(*) cnt
  FROM MDM_ORGANIZATION M
 WHERE M.DELT_FLG = 'N'
   AND OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
