<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdiLayoutListRSQL">
			<desc><![CDATA[SearchEdiLayoutList]]></desc>
			<sql><![CDATA[
SELECT																 
  		A.EDI_VNDR_SEQ							 
  		, A.FLT_FILE_NO								 
  		, A.COL_NM									 
  		, A.COL_SEQ									 
  		, A.COL_LVL									 
  		, A.COL_KND_CD								 
  		, A.COL_LEN									 
  		, A.COL_DESC														 
  	FROM																 
  		TRS_TRSP_EDI_FLT_FILE_PRTP A									 
  	WHERE																 
  		A.EDI_VNDR_SEQ										= @[edi_vndr_seq]			 
  		AND A.FLT_FILE_NO									= @[flt_file_no]			 
	ORDER BY															 
  		A.COL_SEQ			]]></sql>
			<params>
				<param name="edi_vndr_seq" type="12" value="" out="N"/>
				<param name="flt_file_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
