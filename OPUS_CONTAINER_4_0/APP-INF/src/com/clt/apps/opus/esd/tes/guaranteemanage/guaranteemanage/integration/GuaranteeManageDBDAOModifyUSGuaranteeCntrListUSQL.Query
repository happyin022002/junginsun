<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeManageDBDAOModifyUSGuaranteeCntrListUSQL">
			<desc><![CDATA[Guarantee CNTR 목록 수정]]></desc>
			<sql><![CDATA[
UPDATE	TES_GNTE_CNTR_LIST SET
		  CNTR_NO			= @[cntr_no]
		, CNTR_TPSZ_CD		= @[cntr_tpsz_cd]
		, BKG_NO            = @[bkg_no]
		, BL_NO             = @[bl_no]
		, VVD_CD            = @[vvd_cd]
		, SC_NO             = @[sc_no]
		, FM_DT             = TO_DATE(@[fm_dt], 'YYYYMMDD')
		, TO_DT             = TO_DATE(@[to_dt], 'YYYYMMDD')
		, GNTE_AMT          = @[gnte_amt]
		, BKG_NO_LIST_CTNT	= SUBSTRB(@[bkg_no_list_ctnt], 0, 1000 )
		, GNTE_PROC_TP_CD   = @[gnte_proc_tp_cd]
		, IRR_NO            = @[irr_no]
		, TML_IF_SEQ        = @[tml_if_seq]
		, TML_IF_OFC_CD     = @[tml_if_ofc_cd]
		, UPD_USR_ID        = @[upd_usr_id]
		, UPD_DT            = SYSDATE
WHERE	1	= 1
#if (${gnte_no} != '') 
AND		GNTE_NO		= @[gnte_no]
#end
#if (${irr_no} != '') 
AND		IRR_NO		= @[irr_no]
#end
AND		TML_GNTE_CNTR_LIST_SEQ = @[tml_gnte_cntr_list_seq]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="gnte_amt" type="6" value="" out="N"/>
				<param name="bkg_no_list_ctnt" type="12" value="" out="N"/>
				<param name="gnte_proc_tp_cd" type="12" value="" out="N"/>
				<param name="irr_no" type="12" value="" out="N"/>
				<param name="tml_if_seq" type="2" value="" out="N"/>
				<param name="tml_if_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="gnte_no" type="12" value="" out="N"/>
				<param name="tml_gnte_cntr_list_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
