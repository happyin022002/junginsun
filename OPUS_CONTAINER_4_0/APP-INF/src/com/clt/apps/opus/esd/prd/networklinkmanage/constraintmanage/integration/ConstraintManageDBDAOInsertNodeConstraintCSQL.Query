<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOInsertNodeConstraintCSQL">
			<desc><![CDATA[InsertNodeConstraint]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_NOD_CNST_MGMT (
   NOD_CD
  ,NOD_CNST_ITM_CD
  ,PCTL_CNST_ITM_NM
  ,NOD_CNST_RMK
  ,CNTR_TP_CD
  ,SVC_USE_FLG
  ,EFF_FM_DT
  ,EFF_TO_DT
  ,CRE_DT
  ,CRE_USR_ID
  ,CRE_OFC_CD
  ,UPD_USR_ID
  ,CMDT_CD
  ,NOD_CNST_SEQ
  ,PORT_PNT_CD
  ,SPCL_CGO_CNTR_TP_CD
  ,VSL_SLAN_CD
  ,CNTR_SZ_CD  
) VALUES (
   @[s_node_code]
  ,@[s_item_code]
  ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01386'  AND INTG_CD_VAL_CTNT = @[s_item_code])
  ,@[s_remark]
  ,@[cntr_tp_cd]
  ,@[s_svc_flg]
  ,@[eff_fm_date]
  ,@[eff_to_date]
  ,SYSDATE
  ,@[cre_usr_id]
  ,@[cre_ofc_cd]
  ,@[upd_usr_id]
  ,@[s_commodity_code]
  ,NVL((select max(nod_cnst_seq) + 1 from prd_nod_cnst_mgmt), 1)
  ,@[s_point_code]
  ,@[spcl_cgo_cntr_tp_cd]
  ,@[vsl_slan_cd]
  ,@[cntr_sz_cd]
)			]]></sql>
			<params>
				<param name="s_node_code" type="12" value="" out="N"/>
				<param name="s_item_code" type="12" value="" out="N"/>
				<param name="s_remark" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="s_svc_flg" type="12" value="" out="N"/>
				<param name="eff_fm_date" type="12" value="" out="N"/>
				<param name="eff_to_date" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="s_commodity_code" type="12" value="" out="N"/>
				<param name="s_point_code" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
