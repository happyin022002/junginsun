<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USAActualCustomerCodeManageDBDAOSearchUSAActualCustomerCodeExcelRSQL">
			<desc><![CDATA[Actual Customer Excel Download 조회]]></desc>
			<sql><![CDATA[
 SELECT
     NVL(B.delt_flg, 'N')    delt_flg
    ,NVL(B.dflt_act_cust_flg, '0')  dflt_act_cust_flg
    ,A.act_cust_bnd_cd           act_cust_bnd_cd
    ,substr(A.dor_nod_cd,1,5)  dr_loc_value
    ,substr(A.dor_nod_cd,6,2)  dr_yard_value
    ,A.act_cust_cnt_cd||A.act_cust_seq  act_cust_cnt_cd
    ,A.act_cust_nm  act_cust_nm1
    ,B.act_cust_nm  act_cust_nm2
    ,B.act_cust_zip_cd
    ,B.act_cust_addr
    ,B.act_cust_phn_no
    ,B.act_cust_fax_no
    ,B.cntc_pson_nm
    ,B.act_cust_eml
    ,B.act_cust_rmk
    ,B.cre_usr_id
    ,to_char(B.cre_dt,'YYYYMMDD')    cre_dt
    ,B.cre_ofc_cd
    ,B.delt_usr_id
    ,to_char(B.delt_dt,'YYYYMMDD')   delt_dt
    ,B.delt_ofc_cd
 FROM   TRS_TRSP_USA_ACT_CUST A
     ,TRS_TRSP_USA_ACT_CUST_DTL B
 WHERE A.trsp_act_cust_no = B.trsp_act_cust_no(+)
#if (${status} != 'A')
   AND   A.delt_flg = @[status]
#end

#if (${bound} != 'A')
   AND   A.act_cust_bnd_cd = @[bound]
#end

##${dor_nod}
#if ($dor_nod.length() == 7) 
   AND   A.dor_nod_cd      = @[dor_nod]
#elseif ($dor_nod.length() == 5) 
   AND substr(A.dor_nod_cd,1,5)    = @[dor_nod]
#elseif ($dor_nod.length() > 0) 
   AND A.dor_nod_cd like '%' || @[dor_nod] ||'%'
#end 

#if (${input_cust_cd} != '')
   AND A.act_cust_cnt_cd = SUBSTR(@[input_cust_cd], 1, 2)
   AND A.act_cust_seq    = to_number(SUBSTR(@[input_cust_cd], 3, 6))
#end 

##${input_cust_nm}
#if ($input_cust_nm.length() > 0) 
   AND UPPER(B.act_cust_nm)     like '%' || @[input_cust_nm] ||'%'
#end

#if ($OfcCd.size() > 0) 
   AND B.cre_ofc_cd in (
#foreach( ${key} in ${OfcCd}) 
	#if($velocityCount < $OfcCd.size()) 
		'$key', 
	#else 
		'$key' 
	#end 
#end
		)
#end

ORDER BY A.act_cust_bnd_cd, A.dor_nod_cd, A.act_cust_cnt_cd
			]]></sql>
			<params>
				<param name="status" type="12" value="1" out="N"/>
				<param name="bound" type="12" value="1" out="N"/>
				<param name="dor_nod" type="12" value="1" out="N"/>
				<param name="input_cust_cd" type="12" value="1" out="N"/>
				<param name="input_cust_nm" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
