<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USARailWoAckManageDBDAOAddManageUSARail417WoAckUSQL">
			<desc><![CDATA[AddManageUSARail417WoAck]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_EDI_RAIL_ORD T
	 SET T.RAIL_CRR_REF_NO = @[cnru_no]
        ,T.UPD_DT = SYSDATE
 WHERE (T.TRSP_SO_OFC_CTY_CD, T.TRSP_SO_SEQ) = (SELECT TRSP_SO_OFC_CTY_CD ,TRSP_SO_SEQ
													FROM TRS_TRSP_RAIL_BIL_ORD O
												WHERE O.BL_NO = @[bl_no]
												  AND O.EQ_NO LIKE (CASE WHEN LENGTH(@[cntr_no]) >= 10 THEN SUBSTR(@[cntr_no], 1, 10) ELSE '-'  END) || '%')			]]></sql>
			<params>
				<param name="cnru_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
