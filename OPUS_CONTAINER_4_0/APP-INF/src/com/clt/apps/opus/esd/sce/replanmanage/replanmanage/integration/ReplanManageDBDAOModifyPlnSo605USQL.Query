<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOModifyPlnSo605USQL">
			<desc><![CDATA[SCE_PLN_SO_LIST 의 COST_ACT_GRP_SEQ 를 605 로 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_PLN_SO_LIST
SET COST_ACT_GRP_SEQ = 605
WHERE
COP_NO = @[cop_no] AND COST_ACT_GRP_SEQ = 600			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
