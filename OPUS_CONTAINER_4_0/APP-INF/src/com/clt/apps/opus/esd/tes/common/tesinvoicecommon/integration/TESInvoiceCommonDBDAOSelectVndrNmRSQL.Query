<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESInvoiceCommonDBDAOSelectVndrNmRSQL">
			<desc><![CDATA[SelectVndrNm]]></desc>
			<sql><![CDATA[
SELECT	VNDR_SEQ, VNDR_LGL_ENG_NM 
FROM	MDM_VENDOR 
WHERE	VNDR_SEQ = @[vndr_seq]  
AND		DELT_FLG = 'N' 			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
