<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchFindEdiSndRsltRSQL">
			<desc><![CDATA[for FIND_EDI_SND_RSLT]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM SCE_EDI_SND_RSLT  
 WHERE EDI_GRP_CD  = @[e_edi_grp_cd]  
   AND BKG_NO  = @[e_bkg_no]  
   AND CNTR_NO = @[e_cntr_no]  
   AND EDI_STS_CD = @[e_edi_sts_cd]  
   AND EDI_SUB_STS_CD = @[e_edi_sub_sts_cd]
   AND UPPER(EDI_SND_RMK) LIKE 'SUCCESS(SENT)%'  
   AND EDI_SND_TP_CD = 'Y'			]]></sql>
			<params>
				<param name="e_edi_grp_cd" type="12" value="" out="N"/>
				<param name="e_bkg_no" type="12" value="" out="N"/>
				<param name="e_cntr_no" type="12" value="" out="N"/>
				<param name="e_edi_sts_cd" type="12" value="" out="N"/>
				<param name="e_edi_sub_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
