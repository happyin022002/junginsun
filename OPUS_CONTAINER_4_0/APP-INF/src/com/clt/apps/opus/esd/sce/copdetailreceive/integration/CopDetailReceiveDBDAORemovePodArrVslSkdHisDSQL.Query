<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAORemovePodArrVslSkdHisDSQL">
			<desc><![CDATA[List 내 data 의 VVD 를 대상으로 SCE_POD_ARR_VSL_SKD_HIS table 에서 delete]]></desc>
			<sql><![CDATA[
DELETE
  FROM SCE_POD_ARR_VSL_SKD_HIS
 WHERE VSL_CD = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
