<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchExpenseTypeRSQL">
			<desc><![CDATA[SearchExpenseType]]></desc>
			<sql><![CDATA[
SELECT intg_cd_val_dp_desc AS name,
       intg_cd_val_ctnt AS code
  FROM COM_INTG_CD_DTL
 WHERE intg_cd_id = 'CD00904'
   AND intg_cd_val_ctnt = DECODE(@[s_n3pty_if_tp_cd],'X','MNR',intg_cd_val_ctnt)
   AND intg_cd_val_ctnt NOT IN ('EXT')
 ORDER BY intg_cd_val_dp_seq			]]></sql>
			<params>
				<param name="s_n3pty_if_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
