<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchEdiSendRsltVeStsRSQL">
			<desc><![CDATA[SearchEdiSendRslt VE Status]]></desc>
			<sql><![CDATA[
SELECT  COUNT(*) CNT
FROM   SCE_COP_DTL A          
      ,SCE_COP_HDR B
      ,SCE_EDI_SND_RSLT C
WHERE  A.VSL_CD       = @[vsl_cd]  
AND    A.SKD_VOY_NO   = @[skd_voy_no] 
AND    A.SKD_DIR_CD   = @[skd_dir_cd]  
AND    SUBSTR(A.NOD_CD,1,5)    = @[vps_port_cd] 
#if (${event_tp_cd} == 'ETA' )
AND    A.STND_EDI_STS_CD IN ('VAD', 'VAT')
#end
#if (${event_tp_cd} == 'ETD' )
AND    A.STND_EDI_STS_CD IN ('VDL', 'VDT')
#end
AND    NVL(A.CLPT_IND_SEQ,'1') = NVL(@[clpt_ind_seq],'1')
AND    B.COP_NO       = A.COP_NO
AND    B.COP_STS_CD   IN ('C','T')
AND    B.CNTR_NO      <> 'COMU0000000'
AND    B.BKG_NO = C.BKG_NO
AND    B.CNTR_NO = C.CNTR_NO
AND    C.EDI_STS_CD = 'VE'
AND    C.EDI_SND_TP_CD = 'Y'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
