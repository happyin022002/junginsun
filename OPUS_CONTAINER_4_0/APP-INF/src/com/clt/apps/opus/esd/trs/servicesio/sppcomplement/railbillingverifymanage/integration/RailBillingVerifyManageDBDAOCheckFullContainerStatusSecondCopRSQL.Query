<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailBillingVerifyManageDBDAOCheckFullContainerStatusSecondCopRSQL">
			<desc><![CDATA[COP에서 더미 CNTR 정보를 조회]]></desc>
			<sql><![CDATA[
SELECT 
    G.COP_NO||G.COST_ACT_GRP_SEQ CNTR_NO,
    H.CNTR_TPSZ_CD CNTR_TPSZ_CD,
    H.COP_NO COP_NO,
    G.COST_ACT_GRP_SEQ COST_ACT_GRP_SEQ,
    'IT''S NOGOOD' NOGOOD,
    H.BKG_NO BKG_NO,
    H.CNTR_NO CNTR_NO_ORIGIN
FROM 
    SCE_COP_HDR H,
    SCE_PLN_SO_LIST G
WHERE H.COP_NO = G.COP_NO
AND   H.COP_STS_CD IN ('C', 'T', 'F')
AND   DECODE (H.MST_COP_NO, H.COP_NO, 'P', 'X') = 'P'
AND   G.TRSP_SO_STS_CD = 'P'
AND   G.TRSP_MOD_CD = 'RD'
AND   SUBSTR(G.COST_ACT_GRP_CD , 1,1) = 'O'
AND   H.CNTR_NO = 'COMU0000000'
AND   H.BKG_NO = @[bkgNo]
AND   DECODE(H.CNTR_TPSZ_CD, 'D4', 'D45', 'D5', 'D45', H.CNTR_TPSZ_CD)= @[eqTpSzCd]
ORDER BY 
    H.COP_NO ASC			]]></sql>
			<params>
				<param name="bkgNo" type="12" value="" out="N"/>
				<param name="eqTpSzCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
