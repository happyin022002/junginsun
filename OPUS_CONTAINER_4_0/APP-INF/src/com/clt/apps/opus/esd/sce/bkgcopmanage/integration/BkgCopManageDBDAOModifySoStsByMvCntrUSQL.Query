<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifySoStsByMvCntrUSQL">
			<desc><![CDATA[SCE_PLN_SO_LIST 를 move 하기 이전 cop 가 가지고 있던 Status 로 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_PLN_SO_LIST A
SET
TRSP_SO_STS_CD              = NVL(@[trsp_so_sts_cd],             A.TRSP_SO_STS_CD            )
WHERE
    COP_NO = @[cop_no]
    AND COST_ACT_GRP_SEQ = @[cost_act_grp_seq]
	AND COST_ACT_GRP_CD NOT LIKE 'OD%'			]]></sql>
			<params>
				<param name="trsp_so_sts_cd" type="12" value="12" out="N"/>
				<param name="cop_no" type="12" value="2" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="3" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
