<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOCheckAPinvoiceRSQL">
			<desc><![CDATA[Invoice Office의 AP Office 존재유무를 조회하는 SQL]]></desc>
			<sql><![CDATA[
SELECT NVL(AP_OFC_CD, 'N') AS AP_OFC_CD
  FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[cost_ofc_cd]			]]></sql>
			<params>
				<param name="cost_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
