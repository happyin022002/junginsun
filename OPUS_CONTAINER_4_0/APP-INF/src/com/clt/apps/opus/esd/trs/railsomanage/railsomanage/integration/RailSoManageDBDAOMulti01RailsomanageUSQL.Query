<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOMulti01RailsomanageUSQL">
			<desc><![CDATA[multi01Railsomanage UPDATE]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_RAIL_BIL_ORD
SET    CXL_RQST_RJCT_RSN = @[cxl_rqst_rjct_rsn],
       CXL_RQST_RJCT_FLG = 'Y',
       CXL_RQST_FLG      = 'N',
       CXL_RQST_RJCT_DT  = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[soffice_cd]),
       UPD_DT            = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[soffice_cd]),
       UPD_USR_ID        = @[suer_ctl_id]
WHERE  TRSP_SO_OFC_CTY_CD= @[trsp_so_ofc_cty_cd]
AND    TRSP_SO_SEQ       = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="cxl_rqst_rjct_rsn" type="12" value="" out="N"/>
				<param name="soffice_cd" type="12" value="" out="N"/>
				<param name="suer_ctl_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
