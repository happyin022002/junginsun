<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewIssuedWrkOrdUSQL">
			<desc><![CDATA[AddWorkOrderPreviewIssuedWrkOrd]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_WRK_ORD			
SET							
	WO_ISS_STS_CD          	= @[wo_iss_sts_cd]					
	, WO_FMT_TP_CD		   	= @[wo_fmt_tp_cd]			
	, WO_VNDR_SEQ		   	= @[wo_vndr_seq]			
	, WO_PRN_USE_FLG	   	= @[wo_prn_use_flg]				
	, WO_FAX_USE_FLG	   	= @[wo_fax_use_flg]					
	, WO_EML_USE_FLG   	   	= @[wo_eml_use_flg]					
	, WO_EDI_USE_FLG		= @[wo_edi_use_flg]				
	, WO_N1ST_FAX_NO		= @[wo_n1st_fax_no]				
	, WO_N2ND_FAX_NO		= @[wo_n2nd_fax_no]				
	, WO_N3RD_FAX_NO   		= @[wo_n3rd_fax_no]					
	, WO_N1ST_FAX_RSLT_FLG	= 'N'		
	, WO_N2ND_FAX_RSLT_FLG	= 'N'			
	, WO_N3RD_FAX_RSLT_FLG	= 'N'			
	, WO_N1ST_EML	     	= @[wo_n1st_eml]			
	, WO_N2ND_EML	     	= @[wo_n2nd_eml]   				
	, WO_N3RD_EML			= @[wo_n3rd_eml]		
	, WO_N1ST_EML_RSLT_FLG	= 'N'			
	, WO_N2ND_EML_RSLT_FLG	= 'N'		
	, WO_N3RD_EML_RSLT_FLG	= 'N'			
	, WO_RMK			    = @[wo_rmk]			
	, RT_DP_USE_FLG		  	= @[rt_dp_use_flg]				
	, CMDT_DP_USE_FLG	 	= @[cmdt_dp_use_flg]			
	, PRE_DIS_USE_FLG		= @[pre_dis_use_flg]			
	, INTER_USE_FLG		  	= @[inter_use_flg]				
	, DELT_FLG		       	= 'N'			
	, UPD_DT           		= SYSDATE
	, UPD_USR_ID       		= @[upd_usr_id]
	, LOCL_UPD_DT           = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd])
WHERE TRSP_WO_OFC_CTY_CD = @[trsp_wo_ofc_cty_cd]
    AND TRSP_WO_SEQ = @[trsp_wo_seq]			]]></sql>
			<params>
				<param name="wo_iss_sts_cd" type="12" value="" out="N"/>
				<param name="wo_fmt_tp_cd" type="12" value="" out="N"/>
				<param name="wo_vndr_seq" type="12" value="" out="N"/>
				<param name="wo_prn_use_flg" type="12" value="" out="N"/>
				<param name="wo_fax_use_flg" type="12" value="" out="N"/>
				<param name="wo_eml_use_flg" type="12" value="" out="N"/>
				<param name="wo_edi_use_flg" type="12" value="" out="N"/>
				<param name="wo_n1st_fax_no" type="12" value="" out="N"/>
				<param name="wo_n2nd_fax_no" type="12" value="" out="N"/>
				<param name="wo_n3rd_fax_no" type="12" value="" out="N"/>
				<param name="wo_n1st_eml" type="12" value="" out="N"/>
				<param name="wo_n2nd_eml" type="12" value="" out="N"/>
				<param name="wo_n3rd_eml" type="12" value="" out="N"/>
				<param name="wo_rmk" type="12" value="" out="N"/>
				<param name="rt_dp_use_flg" type="12" value="" out="N"/>
				<param name="cmdt_dp_use_flg" type="12" value="" out="N"/>
				<param name="pre_dis_use_flg" type="12" value="" out="N"/>
				<param name="inter_use_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
