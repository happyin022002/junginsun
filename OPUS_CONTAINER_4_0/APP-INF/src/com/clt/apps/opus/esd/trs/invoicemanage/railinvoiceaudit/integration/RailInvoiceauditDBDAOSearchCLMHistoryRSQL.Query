<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOSearchCLMHistoryRSQL">
			<desc><![CDATA[searchCLMHistory SELECT]]></desc>
			<sql><![CDATA[
SELECT
      C.CLM_CRR_NM       ,
      S.CLM_SGHT_ABBR_NM ,
      TO_CHAR(C.ARR_DT   , 'YYYY-MM-DD HH24:MI:SS') ARR_DT ,
      C.ARR_LOC_NM       ,
      C.ARR_STE_CD       ,
      C.DEP_LOC_NM       ,
      C.DEP_STE_CD
FROM
      SCE_CLM_IF   C ,
      SCE_CLM_SGHT S ,


      trs_trsp_rail_bil_ord   str,
      sce_rail_splc rss

WHERE C.CNTR_NO     = @[eq_no]
AND   C.CLM_SGHT_CD = S.CLM_SGHT_CD
AND   C.ARR_DT BETWEEN TO_DATE( @[wbl_dt]||'000001', 'YYYYMMDDHH24MISS') AND TO_DATE( @[wbl_dt]||'235959', 'YYYYMMDDHH24MISS')+30
  AND    c.cntr_no                 = str.eq_no
  AND    SUBSTR(str.to_nod_cd, 1, 5) = rss.loc_cd
  AND    c.dep_ste_cd              = rss.ste_cd
  AND    c.arr_dt between str.cre_dt and ADD_MONTHS(str.cre_dt,1)
  AND    str.delt_flg  = 'N'
  AND    rss.splc_cd                 > 500
  AND    DEST_GATE_OUT_DT IS NULL
ORDER BY ARR_DT DESC			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="wbl_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
