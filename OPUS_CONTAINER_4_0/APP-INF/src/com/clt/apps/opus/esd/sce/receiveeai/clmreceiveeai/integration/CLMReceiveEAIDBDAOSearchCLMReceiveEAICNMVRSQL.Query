<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLMReceiveEAIDBDAOSearchCLMReceiveEAICNMVRSQL">
			<desc><![CDATA[SearchCLMReceiveEAICNMV]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(C XUK1CTM_MOVEMENT) */ 
      C.CNTR_NO,C.CNMV_YR,C.CNMV_ID_NO,C.CNMV_CYC_NO
      ,(SELECT NVL(MAX(I.CLM_SEQ),0)+1 FROM SCE_CLM_IF I 
                               WHERE I.CNTR_NO LIKE @[CNTR_NO] ||'%' 
                               AND   I.CNMV_YR = C.CNMV_YR
                               AND   I.CNMV_ID_NO = C.CNMV_ID_NO
                               AND   I.CLM_CYC_NO = C.CNMV_CYC_NO) CLM_SEQ
FROM  CTM_MOVEMENT C
WHERE C.CNTR_NO LIKE @[CNTR_NO] ||'%'
AND   ROWNUM = 1			]]></sql>
			<params>
				<param name="CNTR_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
