<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandLinkManageDBDAOInlandLinkUpdateUSQL">
			<desc><![CDATA[InlandLinkUpdate]]></desc>
			<sql><![CDATA[
UPDATE prd_inlnd_each_lnk l 
			    SET vndr_seq = @[vndr_seq],                
			        tztm_hrs = @[tztm_hrs],                
			        lnk_dist = @[lnk_dist],                
			        dist_ut_cd = @[dist_ut_cd],              
			       rail_crr_tp_cd = @[rail_crr_tp_cd],          
			        upd_usr_id = @[upd_usr_id],              
			        upd_dt = sysdate,            
			        trsp_agmt_ofc_cty_cd = @[cty_cd],    
			       trsp_agmt_seq = TO_NUMBER(@[i_agmt_seq])            
			  WHERE  l.lnk_org_nod_cd = @[lnk_org_nod_cd]        
			        AND l.lnk_dest_nod_cd = @[lnk_dest_nod_cd]   
			         AND l.trsp_mod_cd  = @[trsp_mod_cd]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="tztm_hrs" type="12" value="" out="N"/>
				<param name="lnk_dist" type="12" value="" out="N"/>
				<param name="dist_ut_cd" type="12" value="" out="N"/>
				<param name="rail_crr_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cty_cd" type="12" value="" out="N"/>
				<param name="i_agmt_seq" type="12" value="" out="N"/>
				<param name="lnk_org_nod_cd" type="12" value="" out="N"/>
				<param name="lnk_dest_nod_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
