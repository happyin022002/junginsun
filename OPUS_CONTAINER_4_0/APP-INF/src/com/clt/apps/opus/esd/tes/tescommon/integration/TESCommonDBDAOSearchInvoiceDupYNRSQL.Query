<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchInvoiceDupYNRSQL">
			<desc><![CDATA[Invoice Duplication YN]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(B.ATTR_CTNT1), 0, 'N', 'Y') AS INV_DUP_YN 
  FROM AP_INV_HDR A 
     , AP_INV_DTRB B 
 WHERE A.CSR_NO = B.CSR_NO 
       AND B.ATTR_CTNT1 = @[inv_no] 
       AND A.VNDR_NO = @[vndr_seq] 
       AND A.AFT_ACT_FLG NOT IN ('N', 'X')			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
