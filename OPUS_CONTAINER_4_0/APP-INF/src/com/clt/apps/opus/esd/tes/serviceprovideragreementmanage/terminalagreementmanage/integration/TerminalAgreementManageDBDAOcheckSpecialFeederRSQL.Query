<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOcheckSpecialFeederRSQL">
			<desc><![CDATA[POL과 POD가 중복되는지 check]]></desc>
			<sql><![CDATA[
SELECT COUNT(0)
FROM TES_TML_SPCL_FDR
WHERE FDR_POL_CD = @[chk_pol_cd] 
AND FDR_POD_CD = @[chk_pod_cd]			]]></sql>
			<params>
				<param name="chk_pol_cd" type="12" value="" out="N"/>
				<param name="chk_pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
