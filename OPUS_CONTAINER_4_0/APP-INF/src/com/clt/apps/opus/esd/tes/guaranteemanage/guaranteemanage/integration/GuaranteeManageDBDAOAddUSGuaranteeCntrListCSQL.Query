<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeManageDBDAOAddUSGuaranteeCntrListCSQL">
			<desc><![CDATA[Guarantee CNTR 목록 추가]]></desc>
			<sql><![CDATA[
INSERT INTO TES_GNTE_CNTR_LIST GL (
		  GNTE_NO
		, TML_GNTE_CNTR_LIST_SEQ 
		, GNTE_PROC_TP_CD
		, IRR_NO
		, TML_IF_OFC_CD
		, TML_IF_SEQ
		, CNTR_NO
		, CNTR_TPSZ_CD
		, BKG_NO
		, BL_NO
		, VVD_CD
		, SC_NO
		, FM_DT
		, TO_DT
		, GNTE_AMT
		, BKG_NO_LIST_CTNT
		, CRE_USR_ID
		, CRE_DT
		, UPD_USR_ID
		, UPD_DT
		) 
VALUES	(
		  @[gnte_no]
		, @[tml_gnte_cntr_list_seq]
		, @[gnte_proc_tp_cd]
		, @[irr_no]
		, @[tml_if_ofc_cd]
		, @[tml_if_seq]
		, @[cntr_no]
		, @[cntr_tpsz_cd]
		, @[bkg_no]
		, @[bl_no]
		, @[vvd_cd]
		, @[sc_no]
		, TO_DATE(@[fm_dt], 'YYYYMMDD')
		, TO_DATE(@[to_dt], 'YYYYMMDD')
		, @[gnte_amt]
		, SUBSTRB(@[bkg_no_list_ctnt], 0, 1000 )
		, @[cre_usr_id]
		, SYSDATE
		, @[cre_usr_id]
		, SYSDATE
		)			]]></sql>
			<params>
				<param name="gnte_no" type="12" value="" out="N"/>
				<param name="tml_gnte_cntr_list_seq" type="2" value="" out="N"/>
				<param name="gnte_proc_tp_cd" type="12" value="" out="N"/>
				<param name="irr_no" type="12" value="" out="N"/>
				<param name="tml_if_ofc_cd" type="12" value="" out="N"/>
				<param name="tml_if_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="gnte_amt" type="6" value="" out="N"/>
				<param name="bkg_no_list_ctnt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
