<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCustRefNoCtntRSQL">
			<desc><![CDATA[BKG_REFERENCE.CUST_REF_NO_CTNT info 조회]]></desc>
			<sql><![CDATA[
select CUST_REF_NO_CTNT
from BKG_REFERENCE
where BKG_NO = @[bkg_no]
and BKG_REF_TP_CD = @[bkg_ref_tp_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_ref_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
