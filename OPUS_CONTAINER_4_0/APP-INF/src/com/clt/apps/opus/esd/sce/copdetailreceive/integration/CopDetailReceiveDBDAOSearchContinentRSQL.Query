<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchContinentRSQL">
			<desc><![CDATA[SearchContinent]]></desc>
			<sql><![CDATA[
select conti_cd
from mdm_location 
where loc_cd = substr(@[nod_cd],1,5)
			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
