<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsCommonDBDAOTrsCommonDBDAOSearchCommodityRSQLRSQL">
			<desc><![CDATA[commodity code를 직접 입력하고 tab를 누르는 경우 자동으로 detail 내용이 표시되지 않음]]></desc>
			<sql><![CDATA[
SELECT CMDT_CD,
	CMDT_NM,
	REP_CMDT_CD,
	REP_IMDG_LVL_CD
FROM MDM_COMMODITY
WHERE CMDT_CD = @[cmdt_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="6" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
