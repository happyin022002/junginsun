<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDAOSearchSubCommodityRSQL">
			<desc><![CDATA[SearchSubCommodity]]></desc>
			<sql><![CDATA[
SELECT
	A.trsp_grp_cmdt_cd,
	A.cmdt_cd,
	A.vndr_seq,
	A.cre_usr_id,
	to_char(A.cre_dt,'YYYYMMDD') cre_dt,
	B.cmdt_nm,
	'N' deleteval
FROM trs_cmdt_grp_cz A, mdm_commodity B
WHERE A.delt_flg!='Y'
	AND A.cmdt_cd=B.cmdt_cd
	AND A.trsp_grp_cmdt_cd = @[trsp_grp_cmdt_cd]
	AND A.vndr_seq = @[vndr_seq]			]]></sql>
			<params>
				<param name="trsp_grp_cmdt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
