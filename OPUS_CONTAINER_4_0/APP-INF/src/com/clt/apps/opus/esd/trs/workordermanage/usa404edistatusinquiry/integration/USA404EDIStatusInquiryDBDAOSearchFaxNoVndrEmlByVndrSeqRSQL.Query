<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USA404EDIStatusInquiryDBDAOSearchFaxNoVndrEmlByVndrSeqRSQL">
			<desc><![CDATA[SearchFaxNoVndrEmlByVndrSeq]]></desc>
			<sql><![CDATA[
SELECT A.VNDR_SEQ
      ,A.VNDR_LGL_ENG_NM
      ,A.VNDR_LOCL_LANG_NM
      ,B.FAX_NO
      ,B.VNDR_EML
  FROM MDM_VENDOR        A
      ,MDM_VNDR_CNTC_PNT B
 WHERE A.VNDR_SEQ = B.VNDR_SEQ(+)
   AND B.PRMRY_CHK_FLG(+) = 'Y'
   AND B.DELT_FLG(+) <> 'Y'
   AND NVL(A.DELT_FLG, 'N') = 'N'
   AND A.VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
