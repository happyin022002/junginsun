<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CARIssueTransferSlipManageDBDAOSearchAP_INV_HDRRSQL">
			<desc><![CDATA[SearchAP_INV_HDR]]></desc>
			<sql><![CDATA[
SELECT CSR_NO, VNDR_NO, CSR_CURR_CD
FROM AP_INV_HDR
WHERE CSR_NO = @[csr_no]  AND SRC_CTNT = 'SO_TERMINAL'			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
