<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchCOPInfoDelRSQL">
			<desc><![CDATA[SearchCOPInfoDel]]></desc>
			<sql><![CDATA[

    SELECT 
      LOC_NM DEL_NAME,
      LOC_CD DEL_CODE,
      DECODE(CNT_CD, 'US', 'D', 'K') DEL_AMSQUAL,
      LOC_AMS_PORT_CD DEL_AMSPORT
    FROM MDM_LOCATION
    WHERE 1=1
    AND LOC_CD = @[e_del_loc]			]]></sql>
			<params>
				<param name="e_del_loc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
