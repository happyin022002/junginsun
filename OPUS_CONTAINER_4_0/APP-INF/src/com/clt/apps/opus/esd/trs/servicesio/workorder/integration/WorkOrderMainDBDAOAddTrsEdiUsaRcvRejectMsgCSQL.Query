<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderMainDBDAOAddTrsEdiUsaRcvRejectMsgCSQL">
			<desc><![CDATA[WorkOrderMainDBDAOAddTrsEdiUsaRcvRejectMsg]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_EDI_USA_RCV_MSG (
     TRSP_SO_OFC_CTY_CD
    ,TRSP_SO_SEQ
    ,RCV_MSG_SEQ
    ,RCV_MSG_SUB_SEQ
    ,TRSP_WO_OFC_CTY_CD
    ,TRSP_WO_SEQ
    ,RCV_MSG_STS_CD
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
    ,RCV_MSG_TP_CD
    ,EDI_RJCT_RSN_CD
) VALUES (
   @[trsp_so_ofc_cty_cd]
  ,@[trsp_so_seq]
  ,@[rcv_msg_seq]
  ,NVL((SELECT MAX(RCV_MSG_SUB_SEQ) + 1 FROM TRS_EDI_USA_RCV_MSG WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd] AND TRSP_SO_SEQ = @[trsp_so_seq] AND RCV_MSG_SEQ = @[rcv_msg_seq]), 1)
  ,@[trsp_wo_ofc_cty_cd]
  ,@[trsp_wo_seq]
  ,@[rcv_msg_sts_cd]
  ,'EDIUSER'
  ,SYSDATE
  ,'EDIUSER'
  ,SYSDATE
  ,@[rcv_msg_tp_cd]
  ,@[edi_rjct_rsn_cd]
)			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="rcv_msg_seq" type="12" value="" out="N"/>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
				<param name="rcv_msg_sts_cd" type="12" value="" out="N"/>
				<param name="rcv_msg_tp_cd" type="12" value="" out="N"/>
				<param name="edi_rjct_rsn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
