<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchTmnlSoCostCodeRSQL">
			<desc><![CDATA[Terninal SO Cost Code 조회]]></desc>
			<sql><![CDATA[
SELECT LGS_COST_CD,
  COST_CALC_MZD_CD,
  TML_AGMT_MGMT_CD,
  MRN_TML_FLG,
  ODCK_RAIL_CHG_FLG,
  FDCK_CY_TML_FLG,
  FDCK_CY_STO_FLG,
  STO_INV_FLG,
  CFS_FLG,
  RAIL_RMP_FLG,
  XCLD_TML_FLG,
  RVIS_CNTR_LIST_CD,
  CNTR_STY_CD,
  STO_EQ_INV_FLG,
  OFF_DCK_STO_EQ_FLG
FROM TES_TML_SO_COST

#if (${lgs_cost_cd} != '') 
WHERE LGS_COST_CD LIKE @[lgs_cost_cd]||'%'
#else
WHERE LGS_COST_CD LIKE ''||'%'
#end

ORDER BY LGS_COST_CD			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
