<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOModifyActTmlIfStsCdUSQL">
			<desc><![CDATA[SCE_ACT_TML_IF table 의 TML_IF_STS_CD 를 'XX' 로 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_ACT_TML_IF SET 
		TML_IF_STS_CD = 'XX',             
		COP_EVNT_SEQ  = COP_EVNT_SEQ + 1  
	WHERE ACT_RCV_DT =  @[act_rcv_dt]
	AND ACT_RCV_NO   =  @[act_rcv_no]			]]></sql>
			<params>
				<param name="act_rcv_dt" type="12" value="" out="N"/>
				<param name="act_rcv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
