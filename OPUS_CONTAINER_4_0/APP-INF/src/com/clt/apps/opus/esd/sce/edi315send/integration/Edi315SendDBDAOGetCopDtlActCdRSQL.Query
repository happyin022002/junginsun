<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCopDtlActCdRSQL">
			<desc><![CDATA[GetCopDtlSeq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(ACT_CD), 'F') AS ACT_CD
  FROM SCE_COP_DTL A
 WHERE COP_NO = @[cop_no]
   AND (STND_EDI_STS_CD = @[edi_sts_cd] OR ACT_CD = @[edi_sts_cd])
   AND SUBSTR(NOD_CD,1,5) =  SUBSTR(@[event_yd_cd],1,5)
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="edi_sts_cd" type="12" value="" out="N"/>
				<param name="event_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
