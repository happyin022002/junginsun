<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCopDtlSeqRSQL">
			<desc><![CDATA[GetCopDtlSeq]]></desc>
			<sql><![CDATA[
SELECT COP_DTL_SEQ
  FROM SCE_COP_DTL A
 WHERE COP_NO = @[cop_no]
   AND STND_EDI_STS_CD = @[edi_sts_cd]
   AND SUBSTR(NOD_CD,1,5) =  SUBSTR(@[event_yd_cd],1,5)			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="edi_sts_cd" type="12" value="" out="N"/>
				<param name="event_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
