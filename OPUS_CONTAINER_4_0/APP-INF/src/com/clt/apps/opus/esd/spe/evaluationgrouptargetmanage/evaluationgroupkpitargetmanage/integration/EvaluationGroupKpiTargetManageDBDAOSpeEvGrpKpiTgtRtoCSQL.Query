<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupKpiTargetManageDBDAOSpeEvGrpKpiTgtRtoCSQL">
			<desc><![CDATA[SpeEvGrpKpiTgtRtoCSQL.Query]]></desc>
			<sql><![CDATA[
INSERT INTO SPE_EV_GRP_KPI_TGT_RTO (
	EG_ID
,	EG_ID_SEQ
,	SP_KPI_CD
,	EV_YR
,	KPI_UT_CD
,	KPI_TGT_RTO
,	KPI_WGT_RTO
,	KPI_PERF_RTO
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	substr(@[eg_id],1,5)
,	TO_NUMBER(substr(@[eg_id],7,3))
,	@[sp_kpi_cd]
,	@[ev_yr]
,	@[kpi_ut_cd]
,	@[kpi_tgt_rto]
,	@[kpi_wgt_rto]
,	@[kpi_perf_rto]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="sp_kpi_cd" type="12" value="" out="N"/>
				<param name="ev_yr" type="12" value="" out="N"/>
				<param name="kpi_ut_cd" type="12" value="" out="N"/>
				<param name="kpi_tgt_rto" type="6" value="" out="N"/>
				<param name="kpi_wgt_rto" type="6" value="" out="N"/>
				<param name="kpi_perf_rto" type="6" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
