<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOCheckPsNodeRSQL">
			<desc><![CDATA[CheckPsNode]]></desc>
			<sql><![CDATA[
SELECT  'X' 
FROM PRD_NOD_CNST_MGMT 
WHERE NOD_CD = @[s_node_code] 
AND NOD_CNST_ITM_CD = @[s_item_code] 
AND NVL(CMDT_CD,'N') = NVL(@[s_commodity_code],'N')
AND PORT_PNT_CD = @[s_point_code]
AND NVL(DELT_FLG, 'N') ='N'
AND NVL(VSL_SLAN_CD, 'AL') = NVL(@[vsl_slan_cd], 'AL')
AND NOD_CNST_SEQ <> NVL(@[s_nod_cnst_seq], 0)			]]></sql>
			<params>
				<param name="s_node_code" type="12" value="" out="N"/>
				<param name="s_item_code" type="12" value="" out="N"/>
				<param name="s_commodity_code" type="12" value="" out="N"/>
				<param name="s_point_code" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="s_nod_cnst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
