<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VslConnBufferTimeDBDAOMultiVslConnBufferTimeCSQL">
			<desc><![CDATA[VslConnBufferTimeDBDAOMultiVslConnBufferTime]]></desc>
			<sql><![CDATA[
MERGE INTO prd_vsl_cnn_tm_mgmt
USING DUAL
ON (dchg_cnt_cd = @[dchg_cnt_cd] AND dchg_tml_cd = @[dchg_tml_cd] AND dchg_slan_cd = @[dchg_slan_cd] AND lod_cnt_cd = @[lod_cnt_cd] AND lod_tml_cd = @[lod_tml_cd] AND lod_slan_cd = @[lod_slan_cd])
WHEN MATCHED THEN
  update
     set io_bnd_cd    	= @[io_bnd_cd]
        ,cnn_buf_hrs  	= NVL(@[cnn_buf_hrs], 0)
		,vsl_cnn_tp_cd 	= @[vsl_cnn_tp_cd]
		,vsl_cnn_rmk  	= @[vsl_cnn_rmk]
        ,upd_usr_id   	= @[upd_usr_id]
        ,upd_dt       	= SYSDATE
WHEN NOT MATCHED THEN
  INSERT (
     dchg_cnt_cd
    ,dchg_tml_cd
    ,dchg_slan_cd
    ,io_bnd_cd
    ,lod_cnt_cd
    ,lod_tml_cd
    ,lod_slan_cd
    ,cnn_buf_hrs
	,vsl_cnn_tp_cd
	,vsl_cnn_rmk
    ,cre_usr_id
    ,cre_dt
    ,upd_usr_id
    ,upd_dt
  ) VALUES (
     @[dchg_cnt_cd]
    ,@[dchg_tml_cd]
    ,@[dchg_slan_cd]
    ,@[io_bnd_cd]
    ,@[lod_cnt_cd]
    ,@[lod_tml_cd]
    ,@[lod_slan_cd]
    ,NVL(@[cnn_buf_hrs], 0)
	,@[vsl_cnn_tp_cd]
	,@[vsl_cnn_rmk]
    ,@[cre_usr_id]
    ,SYSDATE
    ,@[upd_usr_id]
    ,SYSDATE
)			]]></sql>
			<params>
				<param name="dchg_cnt_cd" type="12" value="" out="N"/>
				<param name="dchg_tml_cd" type="12" value="" out="N"/>
				<param name="dchg_slan_cd" type="12" value="" out="N"/>
				<param name="lod_cnt_cd" type="12" value="" out="N"/>
				<param name="lod_tml_cd" type="12" value="" out="N"/>
				<param name="lod_slan_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="cnn_buf_hrs" type="12" value="" out="N"/>
				<param name="vsl_cnn_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_cnn_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
