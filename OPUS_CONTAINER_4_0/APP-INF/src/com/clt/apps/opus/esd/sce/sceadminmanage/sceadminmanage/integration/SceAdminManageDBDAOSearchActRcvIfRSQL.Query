<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SceAdminManageDBDAOSearchActRcvIfRSQL">
			<desc><![CDATA[actual mapping 내역을 조회한다.]]></desc>
			<sql><![CDATA[
select act_rcv_dt,
  act_rcv_no,
  bkg_no,
  cntr_no,
  act_dt,
  act_sts_mapg_cd,
  nod_cd,
  act_rcv_tp_cd,
  umch_chk_dt,
  vsl_cd,
  skd_voy_no,
  skd_dir_cd,
  err_msg,
  cop_evnt_seq
from sce_act_rcv_if
where
	act_rcv_dt between @[act_fm_dt] and @[act_to_dt]
#if (${act_bkg_no} != '') 
	and bkg_no = @[act_bkg_no]
#end

#if (${act_cntr_no} != '') 
	and cntr_no = @[act_cntr_no]
#end

#if (${act_bl_no} != '') 
	and bkg_no = (select bkg_no from bkg_booking where bl_no = @[act_bl_no])
#end

#if (${act_cop_no} != '') 
	and cop_no = @[act_cop_no]
#end

#if (${act_umch_tp_cd} != '') 
	and act_umch_tp_cd = @[act_umch_tp_cd]
#end

#if (${act_rcv_tp_cd} != '') 
	and act_rcv_tp_cd = @[act_rcv_tp_cd]
#end			]]></sql>
			<params>
				<param name="act_fm_dt" type="12" value="" out="N"/>
				<param name="act_to_dt" type="12" value="" out="N"/>
				<param name="act_bkg_no" type="12" value="" out="N"/>
				<param name="act_cntr_no" type="12" value="" out="N"/>
				<param name="act_bl_no" type="12" value="" out="N"/>
				<param name="act_cop_no" type="12" value="" out="N"/>
				<param name="act_umch_tp_cd" type="12" value="" out="N"/>
				<param name="act_rcv_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
