<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementCorrectionDBDAOManageAgmtAttachFileListDSQL">
			<desc><![CDATA[TRS_AGMT_IMG_STO에서 ROW를 삭제하는 SQL]]></desc>
			<sql><![CDATA[
DELETE FROM TRS_AGMT_IMG_STO
 WHERE TRSP_AGMT_OFC_CTY_CD = SUBSTR(@[agmt_no], 1, 3)
   AND TRSP_AGMT_SEQ = SUBSTR(@[agmt_no], 4)
   AND FILE_SAV_ID = @[file_sav_id]
   AND IMG_SEQ = @[img_seq]			]]></sql>
			<params>
				<param name="agmt_no" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="img_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
