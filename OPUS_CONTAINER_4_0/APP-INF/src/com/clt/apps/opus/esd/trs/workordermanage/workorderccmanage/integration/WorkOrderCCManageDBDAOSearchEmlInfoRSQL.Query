<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderCCManageDBDAOSearchEmlInfoRSQL">
			<desc><![CDATA[eml 정보 조회]]></desc>
			<sql><![CDATA[
SELECT WO_EML
FROM TRS_TRSP_WRK_ORD_CC_EML
WHERE VNDR_SEQ = @[vndr_seq]
	AND WO_CC_SEQ = @[wo_cc_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="wo_cc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
