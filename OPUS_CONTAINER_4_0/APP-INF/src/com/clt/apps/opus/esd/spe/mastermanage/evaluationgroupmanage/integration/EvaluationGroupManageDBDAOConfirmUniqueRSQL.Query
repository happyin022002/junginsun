<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupManageDBDAOConfirmUniqueRSQL">
			<desc><![CDATA[ConfirmUnique]]></desc>
			<sql><![CDATA[
SELECT *
FROM SPE_EV_GRP	
WHERE 	EG_ID = SUBSTR(@[eg_id]	,1,5)
AND EG_ID_SEQ   = @[eg_id_seq]			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="eg_id_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
