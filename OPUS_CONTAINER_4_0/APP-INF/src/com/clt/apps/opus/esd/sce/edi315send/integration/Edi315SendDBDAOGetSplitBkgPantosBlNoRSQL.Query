<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetSplitBkgPantosBlNoRSQL">
			<desc><![CDATA[GetSplitBkgPantosBlNo]]></desc>
			<sql><![CDATA[
SELECT BBK.bl_no||BL_NO_TP||BL_TP_CD PANTOS_BL_NO
FROM BKG_BOOKING BBK,
(
    SELECT FM_BKG_NO -- SPLIT 하기 전 원 BOOKING NO
    FROM BKG_BOOKING
    WHERE BKG_NO = @[bkg_no]
    AND BKG_CRE_TP_CD = 'S' -- SPLIT
) BK
WHERE BBK.BKG_NO = BK.FM_BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
