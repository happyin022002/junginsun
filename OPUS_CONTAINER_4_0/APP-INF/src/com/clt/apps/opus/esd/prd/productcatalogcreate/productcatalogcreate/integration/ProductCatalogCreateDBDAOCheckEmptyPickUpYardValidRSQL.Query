<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProductCatalogCreateDBDAOCheckEmptyPickUpYardValidRSQL">
			<desc><![CDATA[checkEmptyPickUpYardValid]]></desc>
			<sql><![CDATA[
SELECT COUNT(YD_CD) CNT
	FROM MDM_YARD X
 WHERE X.YD_CD = @[chk_yd]
	 AND (X.YD_FCTY_TP_MRN_TML_FLG = 'Y' OR X.YD_FCTY_TP_CY_FLG = 'Y' OR X.YD_FCTY_TP_RAIL_RMP_FLG = 'Y')
	 AND X.DELT_FLG = 'N'
			]]></sql>
			<params>
				<param name="chk_yd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
