<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanLinkManageDBDAORHQLinkDelete3USQL">
			<desc><![CDATA[RHQLinkDelete3]]></desc>
			<sql><![CDATA[
UPDATE prd_ocn_rout a 
    SET upd_ind_cd = 'D', 
        OCN_ROUT_UPD_DT = SYSDATE, 
        upd_ofc_cd = @[cre_ofc_cd] ,  
        upd_usr_id = @[upd_usr_id] , 
        ocn_rout_rmk = 'Deleted by CCA Status', 
 	OCN_ROUT_DELT_RMK = SYSDATE||' ID:'||@[upd_usr_id] ||'  ,Deleted by RHQ Ocean Link, N2ND lane'          			 			 
  WHERE ts_ind_cd = 'T' 
    AND upd_ind_cd IN ('C', 'U', 'S') 
    AND lnk_knt > 1 
    AND n2nd_pol_cd = RTRIM (@[s_from] )  
    AND n2nd_pod_cd = RTRIM (@[s_to] )  
    AND n2nd_lane_fdr_flg = 'Y'			]]></sql>
			<params>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="s_from" type="12" value="" out="N"/>
				<param name="s_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
