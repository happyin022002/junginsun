<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchMdmOrganizationRSQL">
			<desc><![CDATA[Office의 Currency Cd를 조회한다]]></desc>
			<sql><![CDATA[
SELECT													
 	BIL_CURR_CD												
 FROM										 				
 	MDM_ORGANIZATION										
 WHERE														
 	 OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
