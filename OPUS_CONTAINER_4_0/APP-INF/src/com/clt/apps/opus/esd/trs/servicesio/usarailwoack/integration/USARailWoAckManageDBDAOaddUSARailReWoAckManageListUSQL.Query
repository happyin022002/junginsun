<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USARailWoAckManageDBDAOaddUSARailReWoAckManageListUSQL">
			<desc><![CDATA[TRS_TRSP_EDI_RAIL_ORD_RSND 테이블 update]]></desc>
			<sql><![CDATA[
UPDATE    TRS_TRSP_EDI_RAIL_ORD_RSND      X		
SET       X.BIL_EDI_RSND_RCV_RSLT_CD = NVL2(X.BIL_EDI_RSND_DT, @[bil_edi_rcv_rslt_cd], NULL) 
      ,   X.BIL_EDI_RSND_RCV_RSLT_DT = NVL2(X.BIL_EDI_RSND_DT, GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])	, NULL)
      ,   X.UPD_USR_ID               = 'EDIUSER'
	  ,   X.UPD_DT 					 = SYSDATE
WHERE     X.BIL_EDI_CTRL_SEQ		 = TO_NUMBER(@[bil_edi_ctrl_seq])			]]></sql>
			<params>
				<param name="bil_edi_rcv_rslt_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="bil_edi_ctrl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
