<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderMainDBDAOAddTrsEdiWrkOrdHisCSQL">
			<desc><![CDATA[WorkOrderMainDBDAOAddTrsEdiWrkOrdHis]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_EDI_WRK_ORD_HIS (
   TRSP_SO_OFC_CTY_CD
  ,TRSP_SO_SEQ
  ,EDI_MSG_SEQ
  ,TRSP_WO_OFC_CTY_CD
  ,TRSP_WO_SEQ
  ,VNDR_SEQ
  ,MSG_BND_CD
  ,MSG_TP_CD
  ,CRE_OFC_CD
  ,LOCL_CRE_DT
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
)
  SELECT T.TRSP_SO_OFC_CTY_CD
        ,T.TRSP_SO_SEQ
        ,T.EDI_MSG_SEQ
        ,T.TRSP_WO_OFC_CTY_CD
        ,T.TRSP_WO_SEQ
        ,SO.VNDR_SEQ
        ,T.MSG_BND_CD
        ,T.MSG_TP_CD
        ,SO.CRE_OFC_CD
        ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(SO.CRE_OFC_CD) LOCL_CRE_DT
        ,T.CRE_USR_ID
        ,SYSDATE CRE_DT
		,T.UPD_USR_ID
        ,SYSDATE UPD_DT
    FROM (SELECT @[trsp_so_ofc_cty_cd] TRSP_SO_OFC_CTY_CD
                ,@[trsp_so_seq] TRSP_SO_SEQ
                ,@[rcv_msg_seq] AS EDI_MSG_SEQ
                ,@[trsp_wo_ofc_cty_cd] TRSP_WO_OFC_CTY_CD
                ,@[trsp_wo_seq] TRSP_WO_SEQ
                ,'I' MSG_BND_CD
                ,@[rcv_msg_tp_cd] MSG_TP_CD
                ,'EDIUSER' CRE_USR_ID
                ,'EDIUSER' UPD_USR_ID
            FROM DUAL) T
        ,TRS_TRSP_SVC_ORD SO
   WHERE T.TRSP_SO_OFC_CTY_CD = SO.TRSP_SO_OFC_CTY_CD(+)
     AND T.TRSP_SO_SEQ = SO.TRSP_SO_SEQ(+)			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="rcv_msg_seq" type="12" value="" out="N"/>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
				<param name="rcv_msg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
