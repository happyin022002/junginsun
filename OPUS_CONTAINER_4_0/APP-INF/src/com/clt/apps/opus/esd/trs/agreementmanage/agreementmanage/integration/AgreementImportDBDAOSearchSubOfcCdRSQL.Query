<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementImportDBDAOSearchSubOfcCdRSQL">
			<desc><![CDATA[Office의 Sub Office 조회]]></desc>
			<sql><![CDATA[
#foreach( ${key} in ${arr_office})
  #if($velocityCount != 1) 
    UNION
  #end
  SELECT OFC_CD FROM MDM_ORGANIZATION
  WHERE DELT_FLG = 'N'
  CONNECT BY NOCYCLE PRIOR OFC_CD = PRNT_OFC_CD
  START WITH OFC_CD = '$key'
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
