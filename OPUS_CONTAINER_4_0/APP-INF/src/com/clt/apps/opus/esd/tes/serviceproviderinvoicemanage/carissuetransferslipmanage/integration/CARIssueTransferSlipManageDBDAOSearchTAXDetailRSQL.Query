<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CARIssueTransferSlipManageDBDAOSearchTAXDetailRSQL">
			<desc><![CDATA[SearchTAXDetail]]></desc>
			<sql><![CDATA[
SELECT DECODE(vndr_cnt_cd,'KR',vndr_locl_lang_nm, vndr_lgl_eng_nm) vndr_nm,
                 bzct_nm,
                 bztp_nm,
                 DECODE(vndr_cnt_cd,'KR',locl_lang_addr, eng_addr) vndr_addr,
                 vndr_seq,
                 ceo_nm
FROM   mdm_vendor
WHERE  rgst_no = @[compNo]			]]></sql>
			<params>
				<param name="compNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
