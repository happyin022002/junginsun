<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchN3ptyCurrCdRSQL">
			<desc><![CDATA[기존 저장된 3rd party billing currency를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT											
	NVL(N3PTY_CURR_CD, 0) N3PTY_CURR_CD	
FROM										 		
	TRS_TRSP_SVC_ORD					
WHERE												
	  TRSP_SO_OFC_CTY_CD = @[TrsSoOfcCtyCd]
AND TRSP_SO_SEQ = @[TrspSoSeq]			]]></sql>
			<params>
				<param name="TrsSoOfcCtyCd" type="12" value="" out="N"/>
				<param name="TrspSoSeq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
