<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOSearch13RailSoManageRSQL">
			<desc><![CDATA[search13RailSoManage SELECT]]></desc>
			<sql><![CDATA[
SELECT
      B.VNDR_LGL_ENG_NM,
      A.PROV_VNDR_SEQ  ,
      A.PROV_USR_ID    ,
      A.PROV_PHN_NO
FROM  TRS_TRSP_RAIL_BIL_ORD A,
      MDM_VENDOR            B
WHERE A.TRSP_SO_OFC_CTY_CD = @[f_trsp_so_ofc_cty_cd]
AND   A.TRSP_SO_SEQ        = @[f_trsp_so_seq]
AND   A.PROV_VNDR_SEQ      = @[f_prov_vndr_seq]
AND   A.PROV_VNDR_SEQ      = B.VNDR_SEQ			]]></sql>
			<params>
				<param name="f_trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="f_trsp_so_seq" type="12" value="" out="N"/>
				<param name="f_prov_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
