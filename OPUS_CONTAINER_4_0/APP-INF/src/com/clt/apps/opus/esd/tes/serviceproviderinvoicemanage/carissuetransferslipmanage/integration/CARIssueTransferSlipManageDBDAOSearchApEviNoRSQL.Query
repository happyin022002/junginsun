<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CARIssueTransferSlipManageDBDAOSearchApEviNoRSQL">
			<desc><![CDATA[SearchApEviNo]]></desc>
			<sql><![CDATA[
SELECT LPAD(nvl(max(substr(evid_no,13,4)),0)+1, 4, '0') tax_no3 
FROM AP_EVID_NO 
WHERE substr(evid_no,1,length(@[evid_no])) = @[evid_no]			]]></sql>
			<params>
				<param name="evid_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
