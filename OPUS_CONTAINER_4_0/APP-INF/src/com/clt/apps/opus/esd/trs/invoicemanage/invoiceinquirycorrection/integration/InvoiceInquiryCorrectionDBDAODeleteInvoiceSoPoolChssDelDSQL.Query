<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceInquiryCorrectionDBDAODeleteInvoiceSoPoolChssDelDSQL">
			<desc><![CDATA[Invoice Pool Chassis Reposition 정보를 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM TRS_TRSP_POOL_CHSS_INV
WHERE
INV_NO			= @[inv_no]
AND INV_VNDR_SEQ	= @[inv_vndr_seq]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
