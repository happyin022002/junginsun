<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOSearchContainerTypeSizeRSQL">
			<desc><![CDATA[ConstraintManageDBDAOSearchContainerTypeSize]]></desc>
			<sql><![CDATA[
#if (${p_gubun} == 'TP')
SELECT CNTR_TP_CD
  FROM (SELECT O.CNTR_TP_CD
              ,MIN(O.RPT_DP_SEQ) SEQ
          FROM MDM_CNTR_TP_SZ O
         WHERE O.CNTR_SZ_CD = NVL(@[cntr_sz_cd], O.CNTR_SZ_CD)
           AND O.DELT_FLG = 'N'
         GROUP BY CNTR_TP_CD)
 ORDER BY SEQ
#elseif (${p_gubun} == 'SZ')
SELECT CNTR_SZ_CD
  FROM (SELECT CNTR_SZ_CD
              ,MIN(O.RPT_DP_SEQ) SEQ
          FROM MDM_CNTR_TP_SZ O
         WHERE O.CNTR_TP_CD = NVL(@[cntr_tp_cd], O.CNTR_TP_CD)
         GROUP BY CNTR_SZ_CD)
 ORDER BY SEQ
#else
select *
  from MDM_CNTR_TP_SZ
 where CNTR_TPSZ_CD = @[cntr_tp_cd] || @[cntr_sz_cd]
   AND DELT_FLG = 'N'
#end			]]></sql>
			<params>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
