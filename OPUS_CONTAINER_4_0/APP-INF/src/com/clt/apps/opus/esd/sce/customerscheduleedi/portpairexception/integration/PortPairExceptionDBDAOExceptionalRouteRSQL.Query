<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairExceptionDBDAOExceptionalRouteRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT rout_seq ,
  cust_trd_prnr_id ,
  lnk_knt ,
  por_cd ,
  org_loc_cd,
  n1st_pol_cd ,
  n1st_lane_cd,
  n1st_pod_cd ,
  n1st_skd_dir_cd ,
  n2nd_pol_cd ,
  n2nd_lane_cd,
  n3rd_lane_cd,
  n2nd_pod_cd,
  n3rd_pol_cd,
  n2nd_skd_dir_cd,
  n3rd_skd_dir_cd,
  dest_loc_cd,
  del_cd ,
  usr_rmk ,
  delt_flg ,
  cre_usr_id ,
  TO_CHAR(cre_dt, 'YYYYMMDDHH24MI') AS cre_dt,
  upd_usr_id AS upd_usr_id ,
  TO_CHAR(upd_dt, 'YYYYMMDDHH24MI') AS upd_dt,  
  /* Condition */
  '' partner_name,
  '' del_port_cd,
  '' pol_port_cd,
  '' partner_id,
  '' por_port_cd,
  '' pod_port_cd
FROM SCE_PORT_PAIR_EXPT
WHERE cust_trd_prnr_id = @[cust_trd_prnr_id] 
  AND DELT_FLG = 'N' 
/* condition - POR */
 #if (${por_port_cd} != '') 
	#foreach($key1 IN ${por_port_cd})
 	AND por_cd IN ('$key1')
	#end
 #end
 
  /* condition - POL */
  #if (${pol_port_cd} != '') 
	#foreach($key2 IN ${pol_port_cd})
 	AND n1st_pol_cd IN ('$key2')
	#end
 #end
 
  /* condition - POD */ 
  #if (${pod_port_cd} != '') 
	#foreach($key3 IN ${pod_port_cd})
 	AND dest_loc_cd IN ('$key3')
	#end
 #end
 
  /* condition - DEL */
  #if (${del_port_cd} != '')
	#foreach($key4 IN ${del_port_cd})
 	AND del_cd IN ('$key4')
	#end
 #end			]]></sql>
			<params>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
