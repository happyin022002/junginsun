<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOCreateErpDataToTPBSmryCSQL">
			<desc><![CDATA[CreateErpDataToTPBSmry]]></desc>
			<sql><![CDATA[
INSERT INTO tpb_inv_if_smry (
    ar_if_no, n3pty_inv_no, if_bl_no, /* changed to use if_bl_no instead of bl_no In 2007-04-26 by Kim Jin-seung */
    bkg_no, if_rhq_cd, if_ofc_cd, inv_cust_cnt_cd, inv_cust_seq,
    vsl_cd, skd_voy_no, finc_dir_cd, sail_arr_dt, por_cd,
    pol_cd, pod_cd, del_cd, lane_cd, rcv_due_dt,
    inv_if_usd_amt, inv_if_locl_amt, inv_if_desc, inv_if_cty_cd, curr_cd,
    inv_if_flg, inv_if_no, inv_if_dt, inv_if_ofc_cd, cre_usr_id,
    cre_dt, upd_usr_id, upd_dt, mon_xch_rt,  /* add mon_xch_rt */
    csr_no, gl_dt, vvd_cd  /* CSR No., GL Date, Actual VVD added By Kim Jin-seung In 2007-08-14 */
) VALUES (
@[ar_if_no]
,@[n3pty_inv_no]
,@[bl_no]
,@[bkg_no]
,@[if_rhq_cd]
,@[if_ofc_cd]
,@[inv_cust_cnt_cd]
,@[inv_cust_seq]
,@[vsl_cd]
,@[skd_voy_no]
,@[finc_dir_cd]
,@[sail_arr_dt]
,@[por_cd]
,@[pol_cd]
,@[pod_cd]
,@[del_cd]
,@[lane_cd]
, TO_DATE(@[rcv_due_dt], 'YYYYMMDD')
,@[inv_if_usd_amt]
,@[inv_if_locl_amt]
,@[inv_if_desc]
,@[inv_if_cty_cd]
,@[curr_cd]
, 'Y'
,@[inv_if_no]
, TO_DATE(@[inv_if_dt], 'YYYY-MM-DD HH24:MI:SS')
,@[user_ofc_cd] /* model1.getInv_if_ofc_cd,  user_ofc_cd */
,@[user_id] /* model1.getCre_usr_id() */
, sysdate /* inv_if_flg : 'Y' (in sysnc way ) */
,'TPB AR I/F' /* model1.getUpd_usr_id(), user_id */
, sysdate
,@[mon_xch_rt] /* add mon_xch_rt */
/* upd_usr_id : 'TPB AR I/F' (in sysnc way ) */
,@[csr_no] /* CSR No., GL Date, Actual VVD added By Kim Jin-seung In 2007-08-14 */
,@[gl_dt] /* CSR No., GL Date, Actual VVD added By Kim Jin-seung In 2007-08-14 */
/* CSR No., GL Date, Actual VVD added By Kim Jin-seung In 2007-08-14 */
,@[vvd_cd] /* CSR No., GL Date, Actual VVD added By Kim Jin-seung In 2007-08-14 */
)  			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="n3pty_inv_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="if_rhq_cd" type="12" value="" out="N"/>
				<param name="if_ofc_cd" type="12" value="" out="N"/>
				<param name="inv_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="inv_cust_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="finc_dir_cd" type="12" value="" out="N"/>
				<param name="sail_arr_dt" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="rcv_due_dt" type="12" value="" out="N"/>
				<param name="inv_if_usd_amt" type="12" value="" out="N"/>
				<param name="inv_if_locl_amt" type="12" value="" out="N"/>
				<param name="inv_if_desc" type="12" value="" out="N"/>
				<param name="inv_if_cty_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="inv_if_no" type="12" value="" out="N"/>
				<param name="inv_if_dt" type="12" value="" out="N"/>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="mon_xch_rt" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
				<param name="gl_dt" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
