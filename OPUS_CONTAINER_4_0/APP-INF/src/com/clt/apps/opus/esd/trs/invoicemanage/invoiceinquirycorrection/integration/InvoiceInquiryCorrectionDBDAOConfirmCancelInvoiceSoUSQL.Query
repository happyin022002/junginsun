<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceInquiryCorrectionDBDAOConfirmCancelInvoiceSoUSQL">
			<desc><![CDATA[SO 테이블의 Invoice 상태를 변경]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD SET				
 TRSP_INV_ACT_STS_CD	= 'C',
 UPD_USR_ID = @[FORM_CRE_USR_ID],
 UPD_DT	= sysdate		
 WHERE
 INV_NO			= @[inv_no]			
 AND INV_VNDR_SEQ	= @[inv_vndr_seq]			
			]]></sql>
			<params>
				<param name="FORM_CRE_USR_ID" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
