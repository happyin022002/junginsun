<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRailScgDBDAOUpdateRailFuelScgAgmtUSQL">
			<desc><![CDATA[US RAIL Surcharge 화면의 US RAIL Agreement Fuel Surcharge 저장( Update 시)]]></desc>
			<sql><![CDATA[
UPDATE TRS_AGMT_RAIL_SCG_RT
	SET AGMT_ROUT_ALL_FLG = @[agmt_rout_all_flg],
        FM_NOD_CD = @[fm_nod_cd]||@[fm_nod_yard],
        TO_NOD_CD = @[to_nod_cd]||@[to_nod_yard],
        CGO_TP_CD = @[cgo_tp_cd],
        TRSP_RAIL_RTO = NVL(@[trsp_rail_rto], 0),
		EFF_FM_DT = TO_DATE(@[eff_fm_dt], 'YYYYMMDD'),
		EFF_TO_DT = TO_DATE(@[eff_to_dt], 'YYYYMMDD'),
		RAIL_RTO_NO = @[rail_rto_no],
		DELT_FLG = @[delt_flg],
		UPD_USR_ID = '${sUsrId}',
		UPD_DT = SYSDATE,
		LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC('${sctrlOfcCd}'),
        COM_SCG_APLY_FLG = @[com_scg_aply_flg],
        WO_APLY_FLG = @[wo_aply_flg],
        USR_DEF_RMK = @[usr_def_rmk],
        SPCL_CGO_CNTR_TP_CD = @[spcl_cgo_cntr_tp_cd],
        AGMT_COST_FLG = @[agmt_cost_flg]
WHERE	TRSP_AGMT_SCG_SEQ = @[trsp_agmt_scg_seq]			]]></sql>
			<params>
				<param name="agmt_rout_all_flg" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yard" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_yard" type="12" value="" out="N"/>
				<param name="cgo_tp_cd" type="12" value="" out="N"/>
				<param name="trsp_rail_rto" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="rail_rto_no" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="com_scg_aply_flg" type="12" value="" out="N"/>
				<param name="wo_aply_flg" type="12" value="" out="N"/>
				<param name="usr_def_rmk" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="agmt_cost_flg" type="12" value="" out="N"/>
				<param name="trsp_agmt_scg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
