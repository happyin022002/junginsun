<sqls>
	<querys>
		<query name="TrsCommonDBDAOsearchMdmPckTpRSQL">
			<desc><![CDATA[Select Package Code from MDM_PCK_TP table]]></desc>
			<sql><![CDATA[
SELECT PCK_CD
  FROM MDM_PCK_TP
 WHERE 1=1
   AND DELT_FLG = 'N'
 ORDER BY PCK_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
