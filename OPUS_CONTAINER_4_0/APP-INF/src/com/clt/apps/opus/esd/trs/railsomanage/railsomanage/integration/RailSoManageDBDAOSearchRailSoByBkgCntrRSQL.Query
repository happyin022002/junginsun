<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOSearchRailSoByBkgCntrRSQL">
			<desc><![CDATA[US Rail O/B BKG#, CNTR#으로 SO 대상 조회 SQL]]></desc>
			<sql><![CDATA[
SELECT 																							
	BK.BKG_NO BKG_NO ,
	A.CNTR_NO BKG_CNTR_NO,   									
	B.CNTR_NO COP_CNTR_NO, 
	B.CNTR_TPSZ_CD EQ_TPSZ_CD,   											
	MAX( C.TRSP_SO_OFC_CTY_CD||C.TRSP_SO_SEQ) SO_NO, 
	MAX(C.FM_NOD_CD) RAIL_ORG,  					
	MAX(C.TO_NOD_CD) RAIL_DEST, 
	MAX( TO_CHAR(C.WO_ISS_DT,'RRRRMMDD HH24:MI:SS'))  EDI_SENT_TIME, 	
	MAX( DECODE(D.SUB_RAIL_SEQ, 1, V.USA_EDI_CD)||DECODE(D.SUB_RAIL_SEQ, 2, ' / ' 					
	||V.USA_EDI_CD)||DECODE(D.SUB_RAIL_SEQ, 3, ' / '||V.USA_EDI_CD)  ) RAIL_ROAD					
FROM 
    BKG_BOOKING BK, 
    BKG_CONTAINER A, 
    SCE_COP_HDR B, 
    TRS_TRSP_RAIL_BIL_ORD C,  				
    TRS_TRSP_RAIL_BIL_VNDR_SET D, 
    MDM_VENDOR V												
WHERE BK.BKG_NO = A.BKG_NO														
AND   A.BKG_NO = B.BKG_NO(+)
AND   A.CNTR_NO = B.CNTR_NO(+)																		
AND   B.COP_NO = C.COP_NO																		
AND   C.TRSP_SO_OFC_CTY_CD = D.TRSP_SO_OFC_CTY_CD 												
AND   C.TRSP_SO_SEQ = D.TRSP_SO_SEQ																
AND   D.VNDR_SEQ = V.VNDR_SEQ																		
AND   NVL(C.DELT_FLG, 'N') <>'Y'

#if($bkgNo.size() > 0)
	AND BK.BKG_NO IN (
	#foreach($key IN ${bkgNo})	
		#if($velocityCount == 1)		
			$key
		#else
			,$key
		#end	
	#end
	)
#end

#if($cntrNo.size() > 0)
	AND B.COP_STS_CD IN ('C','T')
	AND A.CNTR_NO IN (
	#foreach($key IN ${cntrNo})
		#if($velocityCount == 1)		
			$key
		#else
			,$key
		#end
	#end
	)
#end


		
GROUP BY
    BK.BKG_NO, A.CNTR_NO, B.COP_NO, B.CNTR_NO,	B.CNTR_TPSZ_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
