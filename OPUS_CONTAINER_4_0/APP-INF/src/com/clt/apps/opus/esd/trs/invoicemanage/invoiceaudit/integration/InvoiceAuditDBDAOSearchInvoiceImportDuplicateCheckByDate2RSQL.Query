<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchInvoiceImportDuplicateCheckByDate2RSQL">
			<desc><![CDATA[S/O Creation 날짜 기준으로 Invoice 중복여부를 조회]]></desc>
			<sql><![CDATA[
SELECT                                                       
      A.TRSP_SO_OFC_CTY_CD                                    
 ,    A.TRSP_SO_SEQ                                           
 ,    TO_CHAR(A.LOCL_CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT         
 ,    A.FM_NOD_CD                                             
 ,    A.TO_NOD_CD                                             
 ,    A.TRSP_COST_DTL_MOD_CD                                  
 ,    A.TRSP_CRR_MOD_CD                                       
 FROM TRS_TRSP_SVC_ORD     A                                  
 WHERE A.EQ_NO   = @[eq_no]                
 AND  A.FM_NOD_CD = @[fm_nod_cd]                              
 AND  A.TO_NOD_CD = @[to_nod_cd]
 AND  NVL(A.DELT_FLG, 'N') <> 'Y'
 AND  NVL(A.TRSP_FRST_FLG, 'N') <> 'Y'
 AND  A.REF_ID = @[ref_id]
 AND  A.TRSP_SO_TP_CD = 'M'			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="ref_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
