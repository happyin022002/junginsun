<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchEdiVEDRSQL">
			<desc><![CDATA[SearchEdiVED]]></desc>
			<sql><![CDATA[
SELECT  a.cop_no
        ,a.cop_dtl_seq 
        ,b.cntr_no 
        ,DECODE(a.STND_EDI_STS_CD, 'VAD', 'VE', 'VDL', 'VDE', 'VDT', 'VET' ,'VAT', 'VETS') edi_sts
        ,b.bkg_no 
        ,a.nod_cd 
FROM   sce_cop_dtl a          
      ,sce_cop_hdr b
WHERE  a.vsl_cd       = @[vsl_cd]  
AND    a.skd_voy_no   = @[skd_voy_no] 
AND    a.skd_dir_cd   = @[skd_dir_cd]  
AND    SUBSTR(a.nod_cd,1,5)    = @[vps_port_cd] 
#if (${event_tp_cd} == 'ETA' )
AND    a.STND_EDI_STS_CD IN ('VAD', 'VAT')
#end
#if (${event_tp_cd} == 'ETD' )
AND    a.STND_EDI_STS_CD IN ('VDL', 'VDT')
#end
AND    NVL(a.clpt_ind_seq,'1') = NVL(@[clpt_ind_seq],'1')
AND    b.cop_no       = a.cop_no
AND    b.cop_sts_cd   IN ('C','T')
AND    b.cntr_no      <> 'COMU0000000'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
