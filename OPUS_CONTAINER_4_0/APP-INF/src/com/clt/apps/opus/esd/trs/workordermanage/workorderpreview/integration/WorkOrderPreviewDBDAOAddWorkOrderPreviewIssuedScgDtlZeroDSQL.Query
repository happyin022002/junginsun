<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewIssuedScgDtlZeroDSQL">
			<desc><![CDATA[trs_trsp_scg_dtl 에서 해당 금액정보가 0인 데이터 삭제]]></desc>
			<sql><![CDATA[
DELETE
FROM TRS_TRSP_SCG_DTL
WHERE TRSP_SO_SEQ IN (
        SELECT TRSP_SO_SEQ
        FROM TRS_TRSP_WRK_ORD_PRV_TMP
        WHERE WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
            AND WO_ISS_NO = @[wo_iss_no]
            AND NVL(FUEL_SCG_AMT, 0) = 0
            AND NVL(ETC_ADD_AMT, 0) = 0 )
			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
