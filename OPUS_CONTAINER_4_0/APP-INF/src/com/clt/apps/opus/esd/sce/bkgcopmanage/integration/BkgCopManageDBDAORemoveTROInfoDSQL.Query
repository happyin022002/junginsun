<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAORemoveTROInfoDSQL">
			<desc><![CDATA[SCE_TRO_MAPG table 의 data 를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM SCE_TRO_MAPG
WHERE
BKG_NO = @[bkg_no]
AND COP_NO = @[cop_no]
AND IO_BND_CD = @[io_bnd_cd]
AND TRO_SEQ = @[tro_seq]
AND TRO_SUB_SEQ = @[tro_sub_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="T" out="N"/>
				<param name="cop_no" type="12" value="G" out="N"/>
				<param name="io_bnd_cd" type="12" value="S" out="N"/>
				<param name="tro_seq" type="12" value="GG" out="N"/>
				<param name="tro_sub_seq" type="12" value="GG" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
