<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OffdockCYInvoiceManageDBDAOCalOffdockCYInvoiceCostTMNLseparateRSQL">
			<desc><![CDATA[CalOffdockCYInvoiceCostTMNLseparate]]></desc>
			<sql><![CDATA[
#if (${agmt_cost_yn} == 'Y') 
WITH AGMTCOST AS
(
SELECT ltrim(regexp_substr((select COST_CD_FTR_RMK
                            from tes_tml_so_hdr
                            where TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd] AND TML_SO_SEQ = @[tml_so_seq]), '[^|]+', 1, level ) ,'|') as cost_cd
 FROM dual
 connect by level<= ( length((select COST_CD_FTR_RMK
                                from tes_tml_so_hdr
                               where TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd] AND TML_SO_SEQ = @[tml_so_seq]))+1 - length(replace((select COST_CD_FTR_RMK
                                                                                from tes_tml_so_hdr
                                                                               where TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd] AND TML_SO_SEQ = @[tml_so_seq]), '|')) ) / length('|')
)
SELECT A.*
FROM (
#end

SELECT 'I' IBFLAG, 'A' CALC_TP_CD, 'TM' CALC_COST_GRP_CD,
       L.LGS_COST_CD,
       CNTR_TPSZ_CD,
       DCGO_CLSS_CD  DCGO_IND_CD,
       SUM(CALC_VOL_QTY) CALC_VOL_QTY,
       SUM(RVIS_VOL_QTY) RVIS_VOL_QTY,
       VOL_TR_UT_CD,   
       MAX(CTRT_RT) CTRT_RT,
       ROUND((SUM(CALC_VOL_QTY) * CTRT_RT)*INV_XCH_RT,2) INV_AMT,     -- 수정(20070514)
       ROUND((SUM(CALC_VOL_QTY) * CTRT_RT)*INV_XCH_RT,2) CALC_AMT,    -- 수정(20070514)
       C.ACCT_CD,
       L.TML_AGMT_OFC_CTY_CD, L.TML_AGMT_SEQ, L.TML_AGMT_VER_NO, L.CURR_CHK, CURR_CD, INV_XCH_RT, DECODE(L.RC_FLG,'Y','Y','N') RC_FLG, L.TML_TRNS_MOD_CD
FROM (
SELECT DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') LGS_COST_CD,
       L.CNTR_TPSZ_CD, L.DCGO_CLSS_CD,     
       SUM(DECODE(L.INV_GATE_IN_DT,NULL,0,DECODE(SIGN(TO_CHAR(L.INV_GATE_IN_DT,'YYYYMMDD')-@[fm_prd_dt]),-1,0,1))) CALC_VOL_QTY,       
       SUM(DECODE(L.INV_GATE_IN_DT,NULL,0,DECODE(SIGN(TO_CHAR(L.INV_GATE_IN_DT,'YYYYMMDD')-@[fm_prd_dt]),-1,0,1))) RVIS_VOL_QTY,
       TD.TML_AGMT_VOL_UT_CD VOL_TR_UT_CD,
       DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.AGMT_RT,TD.AGMT_UT_RT) CTRT_RT,
       H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO, DECODE(@[curr_cd],TD.CURR_CD,'Y','N') CURR_CHK, TD.CURR_CD, DECODE(L.RC_FLG,'Y','Y','N') RC_FLG, L.TML_TRNS_MOD_CD,
       ROUND((SELECT USD_LOCL_XCH_RT FROM GL_MON_XCH_RT WHERE ACCT_XCH_RT_YRMON = SUBSTR(REPLACE(@[to_prd_dt],'-'),1,6) AND CURR_CD =@[curr_cd])/(select USD_LOCL_XCH_RT from GL_MON_XCH_RT where ACCT_XCH_RT_YRMON = SUBSTR(REPLACE(@[to_prd_dt],'-'),1,6) and CURR_CD = TD.CURR_CD),4) INV_XCH_RT
FROM   (SELECT VRFY_RSLT_IND_CD, TML_SO_OFC_CTY_CD, TML_SO_SEQ, CNTR_STY_CD, CNTR_TPSZ_CD, DCGO_CLSS_CD, 
                INV_GATE_IN_DT, DECODE(HO,0,WRK_DY,'HO') TML_WRK_DY_CD, IO_BND_CD, RC_FLG, AWK_CGO_FLG, TML_TRNS_MOD_CD
           FROM (SELECT VRFY_RSLT_IND_CD, TML_SO_OFC_CTY_CD, TML_SO_SEQ, TML_SO_CNTR_LIST_SEQ, CNTR_STY_CD, CNTR_TPSZ_CD, DCGO_CLSS_CD, 
                        INV_GATE_IN_DT, DECODE(TO_CHAR(INV_GATE_IN_DT,'D'),7,'SA',1,'SU','WD') WRK_DY,
                        MAX((SELECT COUNT(*)
        				       FROM DMT_HOLIDAY
        				      WHERE 1=1
        					    AND TO_CHAR(HOL_DT,'YYYYMMDD') = TO_CHAR(INV_GATE_OUT_DT,'YYYYMMDD')
        					    AND CNT_CD = SUBSTR(@[yd_cd],1,2)
        					)) HO,
                         IO_BND_CD, RC_FLG, AWK_CGO_FLG, TML_TRNS_MOD_CD
                   FROM TES_TML_SO_CNTR_LIST
                  WHERE VRFY_RSLT_IND_CD    = 'CO'
                    AND TML_SO_OFC_CTY_CD   = @[tml_so_ofc_cty_cd]
                    AND TML_SO_SEQ          = @[tml_so_seq]
                    GROUP BY VRFY_RSLT_IND_CD, TML_SO_OFC_CTY_CD, TML_SO_SEQ, TML_SO_CNTR_LIST_SEQ, CNTR_STY_CD, CNTR_TPSZ_CD, DCGO_CLSS_CD,
                             INV_GATE_IN_DT, DECODE(TO_CHAR(INV_GATE_IN_DT,'D'),7,'SA',1,'SU','WD'),
                             IO_BND_CD, RC_FLG, AWK_CGO_FLG, TML_TRNS_MOD_CD, DCGO_CLSS_CD
                             ) ) L, 
       TES_TML_AGMT_HDR H,
       TES_TML_AGMT_DTL TD, TES_TML_AGMT_TP_SZ TC, TES_TML_AGMT_DG_CGO_CLSS TG, TES_TML_AGMT_APLY_DY TA
WHERE  L.VRFY_RSLT_IND_CD    = 'CO'
AND    L.TML_SO_OFC_CTY_CD   = @[tml_so_ofc_cty_cd]
AND    L.TML_SO_SEQ          = @[tml_so_seq]
AND    H.YD_CD               = @[yd_cd]
AND    H.VNDR_SEQ            = @[vndr_seq]
AND    H.TML_AGMT_STS_CD     = 'C'
AND    H.DELT_FLG            = 'N'
AND    TO_CHAR(H.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
AND    TO_CHAR(H.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt]
AND    H.TML_AGMT_VER_NO = ( SELECT MAX(M.TML_AGMT_VER_NO)
                             FROM   TES_TML_AGMT_HDR M
                             WHERE  M.YD_CD               = @[yd_cd]
                             AND    M.VNDR_SEQ            = @[vndr_seq]
                             AND    M.TML_AGMT_STS_CD     = 'C'
                             AND    M.DELT_FLG            = 'N'
                             AND    TO_CHAR(M.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
                             AND    TO_CHAR(M.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt] )
AND    H.TML_AGMT_OFC_CTY_CD = TD.TML_AGMT_OFC_CTY_CD(+)
AND    H.TML_AGMT_SEQ        = TD.TML_AGMT_SEQ(+)
AND    H.TML_AGMT_VER_NO     = TD.TML_AGMT_VER_NO(+)
AND    DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') = TD.LGS_COST_CD
AND    TD.TML_AGMT_TP_CD     = 'T'
AND    TD.AUTO_CALC_FLG      = 'Y'
AND    NVL(TD.THC_TP_CD,'T') = 'T'
AND    NVL(TD.IO_BND_CD,'N') = DECODE(NVL(TD.IO_BND_CD,'N'),'N','N','S','S',L.IO_BND_CD)
AND    TD.TML_AGMT_OFC_CTY_CD = TC.TML_AGMT_OFC_CTY_CD(+)
AND    TD.TML_AGMT_SEQ        = TC.TML_AGMT_SEQ(+)
AND    TD.TML_AGMT_VER_NO     = TC.TML_AGMT_VER_NO(+)
AND    TD.TML_AGMT_DTL_SEQ    = TC.TML_AGMT_DTL_SEQ(+)
AND    NVL(TD.TML_TRNS_MOD_CD,'S') = DECODE(NVL(TD.TML_TRNS_MOD_CD,'S'),'S','S',
                                                                                (SELECT DECODE((SELECT NVL('X','N')
                    															 FROM   TES_TML_AGMT_DTL
                    															 WHERE  TML_AGMT_OFC_CTY_CD = TD.TML_AGMT_OFC_CTY_CD
                    															 AND    TML_AGMT_SEQ        = TD.TML_AGMT_SEQ
                    															 AND    TML_AGMT_VER_NO     = TD.TML_AGMT_VER_NO
                    															 AND    TML_TRNS_MOD_CD     = L.TML_TRNS_MOD_CD
                    															 AND    ROWNUM = 1),'X',L.TML_TRNS_MOD_CD,'O')
                    											                 FROM DUAL ))
AND    DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.CNTR_TPSZ_CD,'N') = DECODE(TD.TML_AGMT_VOL_UT_CD,'C',DECODE(SUBSTR(L.CNTR_TPSZ_CD,1,1),
                                                      'O',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,DECODE(L.CNTR_STY_CD,'M',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1))),
                                                      'F',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,DECODE(L.CNTR_STY_CD,'M',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1))),
                                                      'P',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,DECODE(L.CNTR_STY_CD,'M',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1))),
                                                      'A',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,DECODE(L.CNTR_STY_CD,'M',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1))),
                                                      'S',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,DECODE(L.CNTR_STY_CD,'M',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1))),
                                                       L.CNTR_TPSZ_CD),'N')
AND    TD.TML_AGMT_OFC_CTY_CD = TG.TML_AGMT_OFC_CTY_CD(+)
AND    TD.TML_AGMT_SEQ        = TG.TML_AGMT_SEQ(+)
AND    TD.TML_AGMT_VER_NO     = TG.TML_AGMT_VER_NO(+)
AND    TD.TML_AGMT_DTL_SEQ    = TG.TML_AGMT_DTL_SEQ(+)
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'N','N',TG.DCGO_APLY_TP_CD) = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'N','N','R')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','N',TG.DCGO_SAM_CLSS_FLG),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','N','Y'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N',TG.DCGO_NON_CLSS_FLG,'N'),'S',DECODE(L.DCGO_CLSS_CD,'N',TG.DCGO_NON_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','Y','N'),'S',DECODE(L.DCGO_CLSS_CD,'N','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'1',TG.DCGO_N1ST_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'1','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'2',TG.DCGO_N2ND_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'2','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'3',TG.DCGO_N3RD_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'3','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'4',TG.DCGO_N4TH_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'4','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'5',TG.DCGO_N5TH_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'5','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'6',TG.DCGO_N6TH_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'6','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'7',TG.DCGO_N7TH_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'7','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'8',TG.DCGO_N8TH_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'8','Y','N'),'N')
AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'9',TG.DCGO_N9TH_CLSS_FLG,'N'),'N')
       = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'9','Y','N'),'N')
AND    TD.TML_AGMT_OFC_CTY_CD  = TA.TML_AGMT_OFC_CTY_CD(+)
AND    TD.TML_AGMT_SEQ         = TA.TML_AGMT_SEQ(+)
AND    TD.TML_AGMT_VER_NO      = TA.TML_AGMT_VER_NO(+)
AND    TD.TML_AGMT_DTL_SEQ     = TA.TML_AGMT_DTL_SEQ(+)   
AND    ( DECODE(TD.TML_DY_APLY_TP_CD,'P',L.TML_WRK_DY_CD,'S') = DECODE(TD.TML_DY_APLY_TP_CD,'P',DECODE(TA.WKDY_FLG,'Y','WD'),'S')
OR       DECODE(TD.TML_DY_APLY_TP_CD,'P',L.TML_WRK_DY_CD,'S') = DECODE(TD.TML_DY_APLY_TP_CD,'P',DECODE(TA.SAT_FLG,'Y','SA'),'S')
OR       DECODE(TD.TML_DY_APLY_TP_CD,'P',L.TML_WRK_DY_CD,'S') = DECODE(TD.TML_DY_APLY_TP_CD,'P',DECODE(TA.SUN_FLG,'Y','SU'),'S')
OR       DECODE(TD.TML_DY_APLY_TP_CD,'P',L.TML_WRK_DY_CD,'S') = DECODE(TD.TML_DY_APLY_TP_CD,'P',DECODE(TA.HOL_FLG,'Y','HO'),'S') ) 
GROUP BY DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT'), L.TML_WRK_DY_CD,
        L.CNTR_TPSZ_CD, L.DCGO_CLSS_CD, TD.TML_AGMT_VOL_UT_CD,
        DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.AGMT_RT,TD.AGMT_UT_RT),
        H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO, TD.CURR_CD, DECODE(L.RC_FLG,'Y','Y','N'), L.TML_TRNS_MOD_CD 
UNION ALL
SELECT DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') LGS_COST_CD,
       L.CNTR_TPSZ_CD, L.DCGO_CLSS_CD,       
       SUM(DECODE(L.INV_GATE_OUT_DT,NULL,0,DECODE(SIGN(@[to_prd_dt]-TO_CHAR(L.INV_GATE_OUT_DT,'YYYYMMDD')),-1,0,1))) CALC_VOL_QTY,       
       SUM(DECODE(L.INV_GATE_OUT_DT,NULL,0,DECODE(SIGN(@[to_prd_dt]-TO_CHAR(L.INV_GATE_OUT_DT,'YYYYMMDD')),-1,0,1))) RVIS_VOL_QTY,
       TD.TML_AGMT_VOL_UT_CD VOL_TR_UT_CD,
       DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.AGMT_RT,TD.AGMT_UT_RT) CTRT_RT,
       H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO, DECODE(@[curr_cd],TD.CURR_CD,'Y','N') CURR_CHK, TD.CURR_CD, DECODE(L.RC_FLG,'Y','Y','N') RC_FLG, L.TML_TRNS_MOD_CD,
       ROUND((SELECT USD_LOCL_XCH_RT FROM GL_MON_XCH_RT WHERE ACCT_XCH_RT_YRMON = SUBSTR(REPLACE(@[to_prd_dt],'-'),1,6) AND CURR_CD =@[curr_cd])/(select USD_LOCL_XCH_RT from GL_MON_XCH_RT where ACCT_XCH_RT_YRMON = SUBSTR(REPLACE(@[to_prd_dt],'-'),1,6) and CURR_CD = TD.CURR_CD),4) INV_XCH_RT
FROM   (SELECT VRFY_RSLT_IND_CD, TML_SO_OFC_CTY_CD, TML_SO_SEQ, CNTR_STY_CD, CNTR_TPSZ_CD, DCGO_CLSS_CD, 
                INV_GATE_OUT_DT, DECODE(HO,0,WRK_DY,'HO') TML_WRK_DY_CD, IO_BND_CD, RC_FLG, AWK_CGO_FLG, TML_TRNS_MOD_CD
           FROM (SELECT VRFY_RSLT_IND_CD, TML_SO_OFC_CTY_CD, TML_SO_SEQ, TML_SO_CNTR_LIST_SEQ, CNTR_STY_CD, CNTR_TPSZ_CD, DCGO_CLSS_CD, 
                        INV_GATE_OUT_DT, DECODE(TO_CHAR(INV_GATE_OUT_DT,'D'),7,'SA',1,'SU','WD') WRK_DY,
                        MAX((SELECT COUNT(*)
        				       FROM DMT_HOLIDAY
        				      WHERE 1=1
        					    AND TO_CHAR(HOL_DT,'YYYYMMDD') = TO_CHAR(INV_GATE_OUT_DT,'YYYYMMDD')
        					    AND CNT_CD = SUBSTR(@[yd_cd],1,2)
        					)) HO,
                         IO_BND_CD, RC_FLG, AWK_CGO_FLG, TML_TRNS_MOD_CD
                   FROM TES_TML_SO_CNTR_LIST
                  WHERE VRFY_RSLT_IND_CD    = 'CO'
                    AND TML_SO_OFC_CTY_CD   = @[tml_so_ofc_cty_cd]
                    AND TML_SO_SEQ          = @[tml_so_seq]
                    GROUP BY VRFY_RSLT_IND_CD, TML_SO_OFC_CTY_CD, TML_SO_SEQ, TML_SO_CNTR_LIST_SEQ, CNTR_STY_CD, CNTR_TPSZ_CD, DCGO_CLSS_CD,
                             INV_GATE_OUT_DT, DECODE(TO_CHAR(INV_GATE_OUT_DT,'D'),7,'SA',1,'SU','WD'),
                             IO_BND_CD, RC_FLG, AWK_CGO_FLG, TML_TRNS_MOD_CD, DCGO_CLSS_CD
                             ) ) L, 
       TES_TML_AGMT_HDR H,
       TES_TML_AGMT_DTL TD, TES_TML_AGMT_TP_SZ TC, TES_TML_AGMT_DG_CGO_CLSS TG, TES_TML_AGMT_APLY_DY TA
WHERE  L.VRFY_RSLT_IND_CD    = 'CO'
AND    L.TML_SO_OFC_CTY_CD   = @[tml_so_ofc_cty_cd]
AND    L.TML_SO_SEQ          = @[tml_so_seq]
AND    H.YD_CD               = @[yd_cd]
AND    H.VNDR_SEQ            = @[vndr_seq]
AND    H.TML_AGMT_STS_CD     = 'C'
AND    H.DELT_FLG            = 'N'
AND    TO_CHAR(H.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
AND    TO_CHAR(H.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt]
AND    H.TML_AGMT_VER_NO = ( SELECT MAX(M.TML_AGMT_VER_NO)
                             FROM   TES_TML_AGMT_HDR M
                             WHERE  M.YD_CD               = @[yd_cd]
                             AND    M.VNDR_SEQ            = @[vndr_seq]
                             AND    M.TML_AGMT_STS_CD     = 'C'
                             AND    M.DELT_FLG            = 'N'
                             AND    TO_CHAR(M.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
                             AND    TO_CHAR(M.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt] )
AND    H.TML_AGMT_OFC_CTY_CD = TD.TML_AGMT_OFC_CTY_CD(+)
AND    H.TML_AGMT_SEQ        = TD.TML_AGMT_SEQ(+)
AND    H.TML_AGMT_VER_NO     = TD.TML_AGMT_VER_NO(+)
AND    DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') = TD.LGS_COST_CD
AND    TD.TML_AGMT_TP_CD     = 'T'
AND    TD.AUTO_CALC_FLG      = 'Y'
AND    NVL(TD.THC_TP_CD,'T') = 'T'
AND    NVL(TD.IO_BND_CD,'N') = DECODE(NVL(TD.IO_BND_CD,'N'),'N','N','S','S',L.IO_BND_CD)
AND    TD.TML_AGMT_OFC_CTY_CD = TC.TML_AGMT_OFC_CTY_CD(+)
AND    TD.TML_AGMT_SEQ        = TC.TML_AGMT_SEQ(+)
AND    TD.TML_AGMT_VER_NO     = TC.TML_AGMT_VER_NO(+)
AND    TD.TML_AGMT_DTL_SEQ    = TC.TML_AGMT_DTL_SEQ(+)
AND    NVL(TD.TML_TRNS_MOD_CD,'S') = DECODE(NVL(TD.TML_TRNS_MOD_CD,'S'),'S','S',
                                                                                (SELECT DECODE((SELECT NVL('X','N')
                    															 FROM   TES_TML_AGMT_DTL
                    															 WHERE  TML_AGMT_OFC_CTY_CD = TD.TML_AGMT_OFC_CTY_CD
                    															 AND    TML_AGMT_SEQ        = TD.TML_AGMT_SEQ
                    															 AND    TML_AGMT_VER_NO     = TD.TML_AGMT_VER_NO
                    															 AND    TML_TRNS_MOD_CD     = L.TML_TRNS_MOD_CD
                    															 AND    ROWNUM = 1),'X',L.TML_TRNS_MOD_CD,'O')
                    											                 FROM DUAL ))
AND    DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.CNTR_TPSZ_CD,'N') = DECODE(TD.TML_AGMT_VOL_UT_CD,'C',DECODE(SUBSTR(L.CNTR_TPSZ_CD,1,1),
                                                      'O',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,DECODE(L.CNTR_STY_CD,'M',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1))),
                                                      'F',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,DECODE(L.CNTR_STY_CD,'M',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1))),
