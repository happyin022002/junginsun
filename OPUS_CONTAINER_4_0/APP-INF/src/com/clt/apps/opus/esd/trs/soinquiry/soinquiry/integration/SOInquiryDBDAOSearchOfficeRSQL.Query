<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SOInquiryDBDAOSearchOfficeRSQL">
			<desc><![CDATA[Office Search]]></desc>
			<sql><![CDATA[
SELECT      
   mst_ofc_id,              
   ofc_cd		              
  FROM  MDM_ORGANIZATION    
  WHERE mst_ofc_id = @[office_cd] 	  			]]></sql>
			<params>
				<param name="office_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
