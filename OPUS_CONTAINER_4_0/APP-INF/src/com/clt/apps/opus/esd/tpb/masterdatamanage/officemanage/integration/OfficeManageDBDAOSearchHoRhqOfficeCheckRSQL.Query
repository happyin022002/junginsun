<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeManageDBDAOSearchHoRhqOfficeCheckRSQL">
			<desc><![CDATA[SearchHoRhqOfficeCheck]]></desc>
			<sql><![CDATA[
SELECT count(*) cnt
FROM TPB_HNDL_OFC
WHERE N3PTY_OFC_TP_CD in ('R', 'S', 'H')
AND OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
