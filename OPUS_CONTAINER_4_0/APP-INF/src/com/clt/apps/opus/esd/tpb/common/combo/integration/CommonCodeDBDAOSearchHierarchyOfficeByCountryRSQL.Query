<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchHierarchyOfficeByCountryRSQL">
			<desc><![CDATA[SearchHierarchyOfficeByCountry]]></desc>
			<sql><![CDATA[
SELECT H.ofc_cd, H.ofc_cd
  FROM TPB_HNDL_OFC H
 WHERE ofc_cd IN ( SELECT ofc_cd 
                    FROM MDM_CUSTOMER
                   WHERE cust_cnt_cd = @[cnt_cd]
                     AND (nmd_cust_flg IS NULL OR nmd_cust_flg != 'Y')
                     AND delt_flg = 'N'
                 )
   AND H.n3pty_ofc_tp_cd = @[ofc_grd]
   AND H.delt_flg = 'N'
 ORDER BY h.ofc_cd
			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="ofc_grd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
