<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetSonyInvNoRSQL">
			<desc><![CDATA[for getSonyInvNo]]></desc>
			<sql><![CDATA[
select CUST_REF_NO_CTNT
    from bkg_reference
where 1=1
   and bkg_no = @[e_bkg_no]
   and bkg_ref_tp_cd = 'FINV'			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
