<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCommonManageDBDAOCreatePrdNodeByZoneCSQL">
			<desc><![CDATA[CCD Zone Creation시 PRD Node에도 해당 정보를 Insert한다.]]></desc>
			<sql><![CDATA[
INSERT INTO prd_node (                                      					
			 	nod_cd 	       ,nod_nm 	       ,nod_tp_cd      ,loc_cd 	       ,			
			 	onf_hir_yd_flg ,delt_flg       ,cre_usr_id     ,cre_dt 	       ,			
			 	upd_usr_id     ,upd_dt
			  )                                                           					
VALUES  (													 					
			 	@[zn_cd] ,@[zn_nm] ,'Z' ,SUBSTR(@[zn_cd],1,5) ,
				'N' ,@[delt_flg] ,@[usr_id] ,sysdate,
				@[usr_id] ,sysdate
		 )			]]></sql>
			<params>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="zn_nm" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
