<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OtherSOManageDBDAOAddTrsSvcConvAmtEurCSQL">
			<desc><![CDATA[EUR 통화변환 금액을 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_EXPN_CONV_AMT											(	
 	TRSP_SO_OFC_CTY_CD								,								
 	TRSP_SO_SEQ										,								
 	CURR_CD											,								
 	BZC_AMT											,								
 	FUEL_SCG_AMT									,								
 	OVR_WGT_SCG_AMT									,								
 	ETC_ADD_AMT										,								
 	CRE_OFC_CD										,								
 	CRE_USR_ID										,								
 	CRE_DT											,								
 	UPD_USR_ID										,								
 	UPD_DT																			
 )																				
 VALUES																			
 (																				
 @[TRSP_SO_OFC_CTY_CD]								,								
 @[TRSP_SO_SEQ]										,								
 'USD'												,								
 (																				
 	SELECT																			
 	ROUND(( TO_NUMBER(@[BZC_AMT]) / RAT.USD_LOCL_XCH_RT),2) BZC_USD							
 	FROM   GL_MON_XCH_RT RAT														
 	WHERE  RAT.CURR_CD								= @[CURR_CD]								
 	AND    RAT.ACCT_XCH_RT_LVL						= '1'							
 	AND    RAT.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 )												,								
 (																				
 	SELECT																			
 	ROUND((TO_NUMBER(@[FUEL_SCG_AMT]) / RAT.USD_LOCL_XCH_RT),2) FUEL_UDS							
 	FROM   GL_MON_XCH_RT RAT														
 	WHERE  RAT.CURR_CD								= @[CURR_CD]								
 	AND    RAT.ACCT_XCH_RT_LVL						= '1'							
 	AND    RAT.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 )												,								
 (																				
 	SELECT 																			
 	ROUND((TO_NUMBER(@[OVR_WGT_SCG_AMT]) / RAT.USD_LOCL_XCH_RT),2) OVR_USD							
 	FROM   GL_MON_XCH_RT RAT														
 	WHERE  RAT.CURR_CD								= @[CURR_CD]								
 	AND    RAT.ACCT_XCH_RT_LVL						= '1'							
 	AND    RAT.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 )												,								
 (																				
 	SELECT 																			
 	ROUND((TO_NUMBER(@[ETC_ADD_AMT]) / RAT.USD_LOCL_XCH_RT),2) ETC_USD							
 	FROM   GL_MON_XCH_RT RAT														
 	WHERE  RAT.CURR_CD								= @[CURR_CD]								
 	AND    RAT.ACCT_XCH_RT_LVL						= '1'							
 	AND    RAT.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 )												,								
 @[FORM_USR_OFC_CD]								,								
 @[FORM_CRE_USR_ID]								,								
 GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC( @[FORM_USR_OFC_CD]),												
 @[FORM_CRE_USR_ID]								,								
 GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC( @[FORM_USR_OFC_CD])												
 )			]]></sql>
			<params>
				<param name="TRSP_SO_OFC_CTY_CD" type="12" value="" out="N"/>
				<param name="TRSP_SO_SEQ" type="12" value="" out="N"/>
				<param name="BZC_AMT" type="12" value="" out="N"/>
				<param name="CURR_CD" type="12" value="" out="N"/>
				<param name="FUEL_SCG_AMT" type="12" value="" out="N"/>
				<param name="OVR_WGT_SCG_AMT" type="12" value="" out="N"/>
				<param name="ETC_ADD_AMT" type="12" value="" out="N"/>
				<param name="FORM_USR_OFC_CD" type="12" value="" out="N"/>
				<param name="FORM_CRE_USR_ID" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
