<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSMSendDBDAOSearchBkgBoundRSQL">
			<desc><![CDATA[SearchBkgBound]]></desc>
			<sql><![CDATA[
select case when pod_cd = 'NOT_EXIST' then pod_cd  
            when pod_cd like 'US%' or del_cd like 'US%' then 'US_BOUND'  
            else 'NOT_US_BOUND' end as CK 
from (select nvl(max(pod_cd), 'NOT_EXIST') as pod_cd, 
             nvl(max(del_cd), 'NOT_EXIST') as del_cd 
      from bkg_booking 
      where bkg_No = @[bkg_no])			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
