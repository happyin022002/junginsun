<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchOBDoorDtlRSQL">
			<desc><![CDATA[OB TRO 에서 Door arrival date 가 적용될 cop detail 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  COP_NO, COP_DTL_SEQ
            ,TO_CHAR(ESTM_DT, 'YYYYMMDDHH24MISS') AS ESTM_DT
    FROM    SCE_COP_DTL  
    WHERE   COP_NO = @[cop_no]
    AND     ACT_CD = 'MOTZAD'
    AND     ROWNUM = 1			]]></sql>
			<params>
				<param name="cop_no" type="12" value="A" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
