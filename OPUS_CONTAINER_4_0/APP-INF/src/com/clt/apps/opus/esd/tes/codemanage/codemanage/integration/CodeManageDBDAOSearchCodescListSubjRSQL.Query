<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchCodescListSubjRSQL">
			<desc><![CDATA[SearchCodescListSubj]]></desc>
			<sql><![CDATA[
SELECT LGS_COST_SUBJ_CD 
FROM (
        SELECT  LGS_COST_SUBJ_CD
        , MIN(LGS_COST_OPT_NO)  
        FROM TES_LGS_COST 
        WHERE LGS_COST_SUBJ_CD IS NOT NULL 
        GROUP BY LGS_COST_SUBJ_CD 
        ORDER BY MIN(LGS_COST_OPT_NO)
       )			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
