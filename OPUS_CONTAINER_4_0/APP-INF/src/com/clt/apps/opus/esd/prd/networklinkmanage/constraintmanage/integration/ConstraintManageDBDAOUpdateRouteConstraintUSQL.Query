<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOUpdateRouteConstraintUSQL">
			<desc><![CDATA[UpdateRouteConstraint]]></desc>
			<sql><![CDATA[
UPDATE PRD_ROUT_CNST
   SET SVC_USE_FLG         = @[s_svc_flg]
      ,ROUT_CNST_RMK       = @[s_remark]
      ,UPD_USR_ID          = @[upd_usr_id]
      ,UPD_DT              = SYSDATE
      ,UPD_OFC_CD          = @[cre_ofc_id]
      ,SPCL_CGO_CNTR_TP_CD = @[spcl_cgo_cntr_tp_cd]
      ,CNTR_TP_CD          = @[cntr_tp_cd]
      ,CNTR_SZ_CD          = @[cntr_sz_cd]
 WHERE TRNK_LANE_CD = @[s_trunk_lane]
   AND POL_NOD_CD = @[s_pol_node]
   AND POD_NOD_CD = @[s_pod] || @[s_pod_node]
   AND ROUT_CNST_SEQ = @[s_rout_seq]			]]></sql>
			<params>
				<param name="s_svc_flg" type="12" value="" out="N"/>
				<param name="s_remark" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_id" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
				<param name="s_trunk_lane" type="12" value="" out="N"/>
				<param name="s_pol_node" type="12" value="" out="N"/>
				<param name="s_pod" type="12" value="" out="N"/>
				<param name="s_pod_node" type="12" value="" out="N"/>
				<param name="s_rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
