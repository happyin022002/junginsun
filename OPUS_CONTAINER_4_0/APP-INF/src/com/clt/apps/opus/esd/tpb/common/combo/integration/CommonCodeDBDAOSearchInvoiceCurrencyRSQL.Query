<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchInvoiceCurrencyRSQL">
			<desc><![CDATA[Invoice Creation Currency]]></desc>
			<sql><![CDATA[
SELECT CURR_CD AS BIL_CURR_CD, CURR_CD AS BIL_CURR_CD1 FROM MDM_CURRENCY WHERE CNT_CD = 'US'
UNION
SELECT AR_CURR_CD AS BIL_CURR_CD, AR_CURR_CD AS BIL_CURR_CD1 FROM MDM_ORGANIZATION WHERE OFC_CD = @[s_ofc_cd]			]]></sql>
			<params>
				<param name="s_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
