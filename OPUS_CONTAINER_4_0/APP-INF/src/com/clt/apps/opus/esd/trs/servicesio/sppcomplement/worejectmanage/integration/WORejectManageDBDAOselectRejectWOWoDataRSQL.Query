<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WORejectManageDBDAOselectRejectWOWoDataRSQL">
			<desc><![CDATA[selectRejectWOWoData]]></desc>
			<sql><![CDATA[
SELECT																																		
		 'SEARCH02' as f_cmd,																												
		 @[FORM_CRE_USR_ID]	as FORM_CRE_USR_ID,																										
		 @[FORM_USR_OFC_CD] as FORM_USR_OFC_CD,																										
		 'Y' 		as issued,																												
		 '' 		as SCG_GRP_SEQ,																										    
		 WO_VNDR_SEQ,																														
		 ( SELECT																															
		     DECODE(COUNT(*),0,'N','C')																										
		  FROM																																
		     TRS_TRSP_SVC_ORD																												
		  WHERE																																
		     TRSP_WO_OFC_CTY_CD= @[TRSP_WO_OFC_CTY_CD]																											
		     AND TRSP_WO_SEQ= @[TRSP_WO_SEQ]																												
		     AND DELT_FLG = 'N'																												
		     AND NVL(WO_RJCT_FLG,'N') <> 'Y')																								
		 AS WO_ISS_STS_CD,																													
		 TRSP_WO_OFC_CTY_CD,																												
		 TRSP_WO_SEQ,																														
		 'TRS'		as FAX_SYS_CD,																											
		 ''			as FAX_APP_CD,																											
		 'N'		as FAX_BATCH_IND,																									    
		 'Hanjin Shipping Work Order'	as FAX_TITLE,																						
		 ''			as FAX_PARAM,																											
		 ''			as FAX_RCV_INFO,																										
		 'Hanjin Shipping Work Order'	as EMAIL_TITLE,																						
		 'Dear  '||VNDR_LGL_ENG_NM ||																										
		  'Please refer to the attached file.'||			
		 'You can also retrieve detailed information on this work order in the following web site http://partner.hanjin.com/' ||			
		 'Thank you. \n Hanjin Shipping '	 as EMAIL_CONTENTS,																				
		 DECODE(WO.WO_PRN_USE_FLG,'Y','PRN')	WO_PRN_USE_FLG,																				
		 DECODE(WO.WO_EML_USE_FLG,'Y','EML')	WO_EML_USE_FLG,																				
		 DECODE(WO.WO_FAX_USE_FLG,'Y','FAX')    WO_FAX_USE_FLG,																				
		 DECODE(WO.WO_EDI_USE_FLG,'Y','EDI')    WO_EDI_USE_FLG,																				
		 WO.RT_DP_USE_FLG		RT_DP_USE_FLG,																								
		 WO.CMDT_DP_USE_FLG		CMDT_DP_USE_FLG,																							
		 WO.PRE_DIS_USE_FLG		PRE_DIS_USE_FLG,																							
		 WO.WO_N1ST_EML			WO_N1ST_EML,																								
		 WO.WO_N2ND_EML			WO_N2ND_EML,																								
		 WO.WO_N3RD_EML			WO_N3RD_EML,																								
		 WO.WO_N1ST_FAX_NO		WO_N1ST_FAX_NO,																								
		 WO.WO_N2ND_FAX_NO		WO_N2ND_FAX_NO,																								
		 WO.WO_N3RD_FAX_NO		WO_N3RD_FAX_NO																								
 FROM																																		
		TRS_TRSP_WRK_ORD WO, MDM_VENDOR VD																									
 WHERE																																		
		WO.WO_VNDR_SEQ=VD.VNDR_SEQ (+)																										
		AND TRSP_WO_OFC_CTY_CD= @[TRSP_WO_OFC_CTY_CD]
		AND TRSP_WO_SEQ= @[TRSP_WO_SEQ]																													
		AND WO.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="FORM_CRE_USR_ID" type="12" value="" out="N"/>
				<param name="FORM_USR_OFC_CD" type="12" value="" out="N"/>
				<param name="TRSP_WO_OFC_CTY_CD" type="12" value="" out="N"/>
				<param name="TRSP_WO_SEQ" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
