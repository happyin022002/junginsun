<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOAddCopHdrByValuesCSQL">
			<desc><![CDATA[Value 를 받아 SCE_COP_HDR 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_COP_HDR (
    COP_NO                      ,
    BKG_NO                      ,
    CNTR_NO                     ,
    CNTR_TPSZ_CD                ,
    CNMV_YR                     ,
    COP_STS_CD                  ,
    PCTL_NO                     ,
    MST_COP_NO                  ,
    COP_FSH_DT                  ,
    TRNK_VSL_CD                 ,
    TRNK_SKD_VOY_NO             ,
    TRNK_SKD_DIR_CD             ,
    POR_NOD_CD                  ,
    POL_NOD_CD                  ,
    POD_NOD_CD                  ,
    DEL_NOD_CD                  ,
    COP_RAIL_CHK_CD             ,
    IB_TRO_FLG                  ,
    OB_TRO_FLG                  ,
    RAIL_RCV_COFF_DT_SRC_TP_CD  ,
    RAIL_RCV_COFF_FM_DT         ,
    CRE_USR_ID                  ,
    CRE_DT                      ,
    UPD_USR_ID                  ,
    UPD_DT                      ,
    COP_SUB_STS_CD              ,
    RAIL_RCV_COFF_TO_DT         ,
    UMCH_STS_CD                 ,
    PROV_CNTR_NO                ,
    PROV_CNTR_TPSZ_CD           ,
    CFM_OB_DOR_ARR_DT           ,
    CFM_APNT_DT                 ,
    COP_UPD_RMK									            
)
VALUES
(
@[cop_no],
@[bkg_no],
@[cntr_no],
@[cntr_tpsz_cd],
@[cnmv_yr],
@[cop_sts_cd],
@[pctl_no],
@[mst_cop_no],
TO_DATE(@[cop_fsh_dt], 'YYYYMMDDHH24MISS'),
@[trnk_vsl_cd],
@[trnk_skd_voy_no],
@[trnk_skd_dir_cd],
@[por_nod_cd],
@[pol_nod_cd],
@[pod_nod_cd],
@[del_nod_cd],
@[cop_rail_chk_cd],
@[ib_tro_flg],
@[ob_tro_flg],
@[rail_rcv_coff_dt_src_tp_cd],
TO_DATE(@[rail_rcv_coff_fm_dt], 'YYYYMMDDHH24MISS'),
'SYSTEM',
SYSDATE,
'SYSTEM',
SYSDATE,
@[cop_sub_sts_cd],
TO_DATE(@[rail_rcv_coff_to_dt], 'YYYYMMDDHH24MISS'),
@[umch_sts_cd],
@[prov_cntr_no],
@[prov_cntr_tpsz_cd],
TO_DATE(@[cfm_ob_dor_arr_dt], 'YYYYMMDDHH24MISS'),
TO_DATE(@[cfm_apnt_dt], 'YYYYMMDDHH24MISS'),
TO_CHAR(SYSDATE, 'YYYYMMDDHH24MI') || '> ' || @[cop_upd_rmk]
)			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cop_sts_cd" type="12" value="" out="N"/>
				<param name="pctl_no" type="12" value="" out="N"/>
				<param name="mst_cop_no" type="12" value="" out="N"/>
				<param name="cop_fsh_dt" type="12" value="" out="N"/>
				<param name="trnk_vsl_cd" type="12" value="" out="N"/>
				<param name="trnk_skd_voy_no" type="12" value="" out="N"/>
				<param name="trnk_skd_dir_cd" type="12" value="" out="N"/>
				<param name="por_nod_cd" type="12" value="" out="N"/>
				<param name="pol_nod_cd" type="12" value="" out="N"/>
				<param name="pod_nod_cd" type="12" value="" out="N"/>
				<param name="del_nod_cd" type="12" value="" out="N"/>
				<param name="cop_rail_chk_cd" type="12" value="" out="N"/>
				<param name="ib_tro_flg" type="12" value="" out="N"/>
				<param name="ob_tro_flg" type="12" value="" out="N"/>
				<param name="rail_rcv_coff_dt_src_tp_cd" type="12" value="" out="N"/>
				<param name="rail_rcv_coff_fm_dt" type="12" value="" out="N"/>
				<param name="cop_sub_sts_cd" type="12" value="" out="N"/>
				<param name="rail_rcv_coff_to_dt" type="12" value="" out="N"/>
				<param name="umch_sts_cd" type="12" value="" out="N"/>
				<param name="prov_cntr_no" type="12" value="" out="N"/>
				<param name="prov_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cfm_ob_dor_arr_dt" type="12" value="" out="N"/>
				<param name="cfm_apnt_dt" type="12" value="" out="N"/>
				<param name="cop_upd_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
