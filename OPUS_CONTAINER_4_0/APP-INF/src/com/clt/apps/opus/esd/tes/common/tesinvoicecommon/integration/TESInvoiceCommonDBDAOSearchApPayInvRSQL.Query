<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESInvoiceCommonDBDAOSearchApPayInvRSQL">
			<desc><![CDATA[SearchApPayInv]]></desc>
			<sql><![CDATA[
select 
    'TES' INV_SUB_SYS_CD,
    COST_OFC_CD,
    INV_OFC_CD,
    VNDR_SEQ,
    YD_CD,
    INV_NO,
    to_char(ISS_DT, 'YYYYMMDD') INV_ISS_DT,
    to_char(RCV_DT, 'YYYYMMDD')  INV_RCV_DT,
    to_char(EFF_DT, 'YYYYMMDD')  INV_EFF_DT,
    to_char(INV_CFM_DT, 'YYYYMMDD')  INV_CFM_DT,
    '' VNDR_TERM_NM,
    to_char(PAY_DUE_DT, 'YYYYMMDD')  PAY_DUE_DT,       --iss + Term
    CURR_CD INV_CURR_CD,
    NVL(TTL_INV_AMT, 0) - NVL(VAT_AMT, 0)  INV_NET_AMT,	
    NVL(TTL_INV_AMT, 0)  INV_TTL_AMT,
    VAT_AMT INV_VAT_AMT,
    WHLD_TAX_AMT WHLD_TAX_AMT, 
    'N' INV_CXL_FLG,
    'N' DELT_FLG,
    @[cre_usr_id] CRE_USR_ID,
    SYSDATE CRE_DT,
    @[cre_usr_id] UPD_USR_ID,
    SYSDATE UPD_DT
from TES_TML_SO_HDR
where 
1=1
and TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
and TML_SO_SEQ = @[tml_so_seq]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
