<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderIssueDBDAOsetFrustrateSceUSQL">
			<desc><![CDATA[setFrustrate]]></desc>
			<sql><![CDATA[
UPDATE SCE_PLN_SO_LIST
   SET TRSP_SO_STS_CD = 'F'
 WHERE (COP_NO, COST_ACT_GRP_SEQ) IN
       (SELECT COP_NO, COST_ACT_GRP_SEQ
          FROM TRS_TRSP_SVC_ORD
         WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
           AND TRSP_SO_SEQ = @[trsp_so_seq]
           AND TRSP_SO_TP_CD = 'Y'
           AND NVL(RPLN_UMCH_FLG, 'N') = 'N'
           AND NVL(TRSP_FRST_FLG, 'N') = 'Y'
        )			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
