<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SceAdminManageDBDAOSearchMstCopNoDiffRSQL">
			<desc><![CDATA[master cop no 의 diff 내역을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_No,
  CNTR_NO,
  CNTR_TPSZ_CD,
  CNMV_YR,
  COP_NO,
  COP_STS_CD,
  PCTL_NO,
  MST_COP_NO,
  TRNK_VSL_CD,
  TRNK_SKD_VOY_NO,
  TRNK_SKD_DIR_CD,
  POR_NOD_CD,
  POL_NOD_CD,
  POD_NOD_CD,
  DEL_NOD_CD,
  IB_TRO_FLG,
  OB_TRO_FLG,
  COP_UPD_RMK
FROM SCE_COP_HDR
WHERE (CNTR_NO,
      TRNK_VSL_CD,
      TRNK_SKD_VOY_NO,
      TRNK_SKD_DIR_CD) IN (
    SELECT CNTR_NO,
      TRNK_VSL_CD,
      TRNK_SKD_VOY_NO,
      TRNK_SKD_DIR_CD
    FROM (
        SELECT DISTINCT MST_COP_NO,
          CNTR_NO,
          TRNK_VSL_CD,
          TRNK_SKD_VOY_NO,
          TRNK_SKD_DIR_CD
        FROM BKG_BOOKING A,
          SCE_COP_HDR B
        WHERE A.BKG_NO = B.BKG_NO
          AND B.COP_STS_CD IN ('C',
              'T',
              'F')
          AND A.BKG_CRE_DT BETWEEN TO_DATE(@[fm_dt], 'YYYYMMDD') AND TO_DATE(@[to_dt], 'YYYYMMDD')
          AND B.CNTR_NO != 'COMU0000000' )
    GROUP BY CNTR_NO, TRNK_VSL_CD, TRNK_SKD_VOY_NO, TRNK_SKD_DIR_CD
    HAVING COUNT(*) >= 2 )
  AND COP_STS_cD IN ('C',
      'T',
      'F')
  AND CNTR_NO != 'COMU0000000'
ORDER BY BKG_NO, CNTR_NO, TRNK_VSL_CD, TRNK_SKD_VOY_NO, TRNK_SKD_DIR_CD			]]></sql>
			<params>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
