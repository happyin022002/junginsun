<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeManageDBDAOSearchUSGuaranteeHdrSeqRSQL">
			<desc><![CDATA[Guarantee Header Max Seq 조회]]></desc>
			<sql><![CDATA[
SELECT
		(SUBSTR(@[ofc_cd], 1, 3) || TO_CHAR(SYSDATE, 'YYMM') ||
		(SELECT LPAD((NVL(MAX(CASE WHEN GNTE_NO IS NOT NULL AND LENGTH(GNTE_NO) > 7
									THEN SUBSTR(GNTE_NO, 8)
									ELSE NULL 
								END), 0) + 1), 5, 0)
		FROM	TES_GNTE_HDR GH2 
		WHERE	GH2.OFC_CD LIKE SUBSTR(@[ofc_cd], 1, 3) || '%' ) ) GRN_NO 
FROM DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
