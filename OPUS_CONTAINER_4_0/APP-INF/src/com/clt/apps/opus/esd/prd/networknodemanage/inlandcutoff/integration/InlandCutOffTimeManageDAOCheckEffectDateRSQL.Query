<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandCutOffTimeManageDAOCheckEffectDateRSQL">
			<desc><![CDATA[InlandCutOffTimeManageDAOCheckEffectDate]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM PRD_INLND_CUT_OFF_TM_MGMT X
 WHERE LANE_CD = @[lane_cd]
   AND ORG_YD_CD = @[org_yd_cd]
   AND DEST_YD_CD = @[dest_yd_cd]
   AND SPCL_CGO_CNTR_TP_CD = @[spcl_cgo_cntr_tp_cd]	
   AND (  TO_DATE(@[eff_fm_dt], 'YYYYMMDD') BETWEEN X.EFF_FM_DT AND X.EFF_TO_DT 
        OR TO_DATE(@[eff_to_dt], 'YYYYMMDD') BETWEEN X.EFF_FM_DT AND X.EFF_TO_DT      
        OR (TO_DATE(@[eff_fm_dt], 'YYYYMMDD') < X.EFF_FM_DT AND TO_DATE(@[eff_to_dt], 'YYYYMMDD') > X.EFF_TO_DT)
       )			]]></sql>
			<params>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="org_yd_cd" type="12" value="" out="N"/>
				<param name="dest_yd_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
