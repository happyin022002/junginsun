<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USATruckEdiWoAckManageDBDAOaddUSATruckEdi990WoAckManageListUSQL">
			<desc><![CDATA[TRSP_SO_OFC_CTY_CD와 TRSP_SO_SEQ에 따른 TRS_TRSP_SVC_ORD테이블 update]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD SET 
	      EDI_RCV_RSLT_CD				= NVL(@[edi_rcv_rslt_cd],'A')
	    , EDI_RCV_RSLT_DT				= GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
	    , EDI_RCV_MSG_TP_CD				= @[edi_rcv_msg_tp_cd]	
   WHERE	TRSP_SO_OFC_CTY_CD	 = SUBSTR(@[edi_so_no],1,3)
    AND    	TRSP_SO_SEQ	 = SUBSTR(@[edi_so_no],4,11) 			]]></sql>
			<params>
				<param name="edi_rcv_rslt_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="edi_rcv_msg_tp_cd" type="12" value="" out="N"/>
				<param name="edi_so_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
