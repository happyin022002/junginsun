      ,LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(CRE_OFC_CD)
      ,UPD_USR_ID  = @[upd_usr_id]
 WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND TRSP_SO_SEQ = @[trsp_so_seq]			]]></sql>
