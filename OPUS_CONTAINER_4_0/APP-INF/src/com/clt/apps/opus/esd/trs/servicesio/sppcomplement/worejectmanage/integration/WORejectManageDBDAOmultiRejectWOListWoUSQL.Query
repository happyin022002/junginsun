<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WORejectManageDBDAOmultiRejectWOListWoUSQL">
			<desc><![CDATA[multiRejectWOList]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_WRK_ORD SET																											
	    UPD_USR_ID  		= 'SPP_IF',																									
	 	UPD_DT     			=  GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[OFC_CD]),																									    
	    WO_ISS_STS_CD =																													
			CASE																														
		     WHEN  0 <  (																																					                      
			    SELECT COUNT(TRSP_SO_OFC_CTY_CD || TRSP_SO_SEQ)  FROM TRS_TRSP_SVC_ORD													
			    WHERE  TRSP_WO_OFC_CTY_CD = @[TRSP_WO_OFC_CTY_CD]																							
			    AND    TRSP_WO_SEQ = @[TRSP_WO_SEQ]
			    		)																												
		     THEN 'P'																													
		     ELSE 'J'																													
			 END																														
	WHERE  TRSP_WO_OFC_CTY_CD = @[TRSP_WO_OFC_CTY_CD]
	AND    TRSP_WO_SEQ = @[TRSP_WO_SEQ]			]]></sql>
			<params>
				<param name="OFC_CD" type="12" value="" out="N"/>
				<param name="TRSP_WO_OFC_CTY_CD" type="12" value="" out="N"/>
				<param name="TRSP_WO_SEQ" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
