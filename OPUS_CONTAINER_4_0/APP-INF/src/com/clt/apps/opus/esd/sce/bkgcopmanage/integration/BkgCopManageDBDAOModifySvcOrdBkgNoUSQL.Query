<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifySvcOrdBkgNoUSQL">
			<desc><![CDATA[TRS_TRSP_SVC_ORD table 의 booking no 를 cop 별로 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD A
SET BKG_NO = NVL(@[bkg_no], A.BKG_NO),
	BL_NO = NVL(@[bl_no], A.BL_NO)
WHERE
	COP_NO = @[cop_no]
	AND COST_ACT_GRP_CD NOT LIKE 'OD%'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="A" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="C" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
