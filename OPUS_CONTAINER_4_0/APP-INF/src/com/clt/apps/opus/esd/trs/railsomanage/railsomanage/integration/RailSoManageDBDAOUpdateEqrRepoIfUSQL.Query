<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOUpdateEqrRepoIfUSQL">
			<desc><![CDATA[Rail So의 EQR 상태코드 변경]]></desc>
			<sql><![CDATA[
UPDATE EQR_REPO_EXE_SO_IF 
SET TRSP_SO_STS_CD = 'C',
	UPD_USR_ID = @[cre_usr_id],
    UPD_DT = SYSDATE
WHERE REPO_PLN_ID = @[repo_pln_id]
AND   PLN_YRWK = @[pln_yrwk]
AND   REF_ID = @[ref_id]
AND   REF_SEQ = @[ref_seq]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="ref_id" type="12" value="" out="N"/>
				<param name="ref_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
