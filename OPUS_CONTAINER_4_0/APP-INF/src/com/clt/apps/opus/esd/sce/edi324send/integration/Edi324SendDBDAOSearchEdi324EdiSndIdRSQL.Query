<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi324SendDBDAOSearchEdi324EdiSndIdRSQL">
			<desc><![CDATA[edi_snd_id를 만들기 위한 값을 만들어 조회를 해온다.]]></desc>
			<sql><![CDATA[
SELECT  SUBSTR(NVL(MAX(EDI_SND_ID),0),10,12)  MAXCOUNT FROM SCE_EDI_324_SND_RSLT
WHERE EDI_SND_ID LIKE '%'||@[edi_current_day]||'%'			]]></sql>
			<params>
				<param name="edi_current_day" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
