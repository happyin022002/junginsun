<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairRouteDBDAOModifyPortPairMasterUSQL">
			<desc><![CDATA[PortPairMaster 정보 수정]]></desc>
			<sql><![CDATA[
UPDATE sce_port_pair_mst        
   SET use_flg = @[use_flg]              
#if (${usr_id} != '')         
		      , upd_usr_id = '$usr_id'
#end           
      ,upd_dt = SYSDATE         
 WHERE cust_trd_prnr_id = @[cust_trd_prnr_id]     
   AND por_cd = @[por_cd]               
   AND pol_cd = @[pol_cd]               
   AND pod_cd = @[pod_cd]               
   AND del_cd = @[del_cd]			]]></sql>
			<params>
				<param name="use_flg" type="12" value="" out="N"/>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
