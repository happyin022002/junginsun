<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearch322PMsgRSQL">
			<desc><![CDATA[Search322PMsg]]></desc>
			<sql><![CDATA[
SELECT evnt_cty_nm, evnt_ste_cd, sndr_id
    FROM   EDI_322_MSG
    WHERE  EQ_NO          = @[cntr_no] 
    AND    EDI_322_STS_CD = @[act_sts_mapg_cd]
    AND    EVNT_DT        = TO_DATE(@[act_dt],'YYYY/MM/DD HH24:MI:SS')			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="act_sts_mapg_cd" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
