<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOSearchLinkConstraintRSQL">
			<desc><![CDATA[SearchLinkConstraint]]></desc>
			<sql><![CDATA[
SELECT LNK_ORG_NOD_CD
      ,LNK_DEST_NOD_CD
      ,TRSP_MOD_CD
      ,LNK_CNST_ITM_CD
      ,PCTL_CNST_ITM_NM
      ,LNK_CNST_RMK
      ,SVC_USE_FLG
      ,CNTR_TP_CD
      ,EFF_FM_DT
      ,EFF_TO_DT
      ,TO_CHAR(CRE_DT, 'YYYY-MM-DD') CRE_DT
      ,CRE_OFC_CD
      ,CRE_USR_ID
      ,lnk_cnst_seq s_lnk_cnst_seq
      ,P.cmdt_cd
      ,(SELECT CMDT_NM FROM MDM_COMMODITY M WHERE M.CMDT_CD = P.CMDT_CD) CMDT_NM
      ,P.SPCL_CGO_CNTR_TP_CD
      ,P.CNTR_SZ_CD
  FROM PRD_LNK_CNST_MGMT P
 WHERE LNK_ORG_NOD_CD LIKE @[from_nd] || '%'
   AND LNK_DEST_NOD_CD LIKE @[to_nd] || '%'
   AND NVL(DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="from_nd" type="12" value="" out="N"/>
				<param name="to_nd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
