<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewCctVgmHisManageDBDAOSearchPrdTmlVgmCctHisRSQL">
			<desc><![CDATA[NewCctVgmHisManageDBDAOSearchPrdTmlVgmCctHis]]></desc>
			<sql><![CDATA[
SELECT HIS_SEQ
      ,PCTL_USE_FLG
      ,YD_CD
      ,VSL_SLAN_CD
      ,VSL_SLAN_DIR_CD
      ,CGO_TP_CD
      ,VVD_CD
      ,VGM_FLG
      ,DELT_FLG
      ,CCT_TP_CD
      ,CCT_HR
      ,CCT_DY_CD
      ,CCT_HRMNT
      ,VPS_PORT_CD
      ,CLPT_IND_SEQ
      ,CCT_FILE_FLG
      ,YD_BSE_CALC_FLG
      ,XCLD_HOL_FRI_FLG
      ,XCLD_HOL_SAT_FLG
      ,XCLD_HOL_SUN_FLG
      ,XCLD_HOL_HOL_FLG
      ,CGO_CLZ_DT
      ,DCGO_CLZ_DT
      ,RC_CLZ_DT
      ,AWK_CGO_CLZ_DT
      ,BB_CGO_CLZ_DT
      ,VGM_CLZ_DT
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
  FROM PRD_TML_VGM_CCT_HIS
 WHERE PCTL_USE_FLG = @[pctl_use_flg]
#if (${pctl_use_flg} == 'Y')
   AND YD_CD = @[yd_cd]
   AND VSL_SLAN_CD = @[vsl_slan_cd]
   AND VSL_SLAN_DIR_CD = @[vsl_slan_dir_cd]
   AND CGO_TP_CD = @[cgo_tp_cd]
   AND VGM_FLG = @[vgm_flg]
#else
   AND VVD_CD = @[vvd_cd]
   AND VSL_SLAN_CD = @[vsl_slan_cd]   
   AND VPS_PORT_CD = @[vps_port_cd]
   AND CLPT_IND_SEQ = @[clpt_ind_seq]
   AND VGM_FLG = @[vgm_flg]
#end
 ORDER BY UPD_DT DESC			]]></sql>
			<params>
				<param name="pctl_use_flg" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_cd" type="12" value="" out="N"/>
				<param name="cgo_tp_cd" type="12" value="" out="N"/>
				<param name="vgm_flg" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
