<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOCancelCSRAPifRailInvUSQL">
			<desc><![CDATA[CSR cancel시 TRS_TRSP_RAIL_INV_WRK update(Disapproved 건의 취소)]]></desc>
			<sql><![CDATA[
UPDATE		TRS_TRSP_RAIL_INV_WRK    												
SET			TRSP_INV_AUD_STS_CD 	= @[TRSP_INV_AUD_STS_CD], 
			UPD_USR_ID 				= @[USR_ID], 
			LOCL_UPD_DT             = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(CRE_OFC_CD),
			UPD_DT 					= GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(CRE_OFC_CD),
			CSR_NO 					= '',
			INV_RJCT_FLG			= '',
			INV_RJCT_DT				= '' 
WHERE 		INV_NO 					= @[INV_NO]
AND 		INV_VNDR_SEQ 			= @[INV_VNDR_SEQ]			]]></sql>
			<params>
				<param name="TRSP_INV_AUD_STS_CD" type="12" value="" out="N"/>
				<param name="USR_ID" type="12" value="" out="N"/>
				<param name="INV_NO" type="12" value="" out="N"/>
				<param name="INV_VNDR_SEQ" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
