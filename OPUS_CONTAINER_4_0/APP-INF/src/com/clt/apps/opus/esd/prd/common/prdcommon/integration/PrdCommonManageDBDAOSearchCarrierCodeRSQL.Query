<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCommonManageDBDAOSearchCarrierCodeRSQL">
			<desc><![CDATA[SearchCarrierCode]]></desc>
			<sql><![CDATA[
SELECT crr_cd carrier_code, crr_nm carrier_name
FROM mdm_carrier c
WHERE c.crr_cd like @[carrier_code]||'%'
AND c.crr_nm like @[carrier_name]||'%'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
