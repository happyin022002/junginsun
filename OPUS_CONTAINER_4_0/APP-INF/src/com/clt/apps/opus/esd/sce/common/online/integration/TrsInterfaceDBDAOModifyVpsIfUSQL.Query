<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsInterfaceDBDAOModifyVpsIfUSQL">
			<desc><![CDATA[WAS restart 시 SCE_VPS_IF 의 VPS_IF_STS_CD 를 원복시킨다. (XX ~> 00)]]></desc>
			<sql><![CDATA[
update SCE_VPS_IF
set VPS_IF_STS_CD = '00'
where ( VPS_RCV_DT,
      VPS_RCV_NO,
      VSL_CD,
      SKD_VOY_NO,
      SKD_DIR_CD,
      VPS_PORT_CD,
      CLPT_IND_SEQ) in (
    select VPS_RCV_DT,
      VPS_RCV_NO,
      VSL_CD,
      SKD_VOY_NO,
      SKD_DIR_CD,
      VPS_PORT_CD,
      CLPT_IND_SEQ
    from SCE_VPS_IF
    where VPS_IF_STS_CD = 'XX'
      and VPS_RCV_DT > to_char(sysdate- 1, 'yyyymmdd') ) 			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
