<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageDBDAOValidCheckPrdInlndEachLinkRSQL">
			<desc><![CDATA[InlandRouteManageDBDAOValidCheckPrdInlndEachLink]]></desc>
			<sql><![CDATA[
SELECT COUNT(1)
  FROM PRD_INLND_EACH_LNK
 WHERE LNK_ORG_NOD_CD = @[lnk_org_nod_cd]
   AND LNK_DEST_NOD_CD = @[lnk_dest_nod_cd]
   AND TRSP_MOD_CD = @[trsp_mod_cd]
   AND NVL(DELT_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="lnk_org_nod_cd" type="12" value="" out="N"/>
				<param name="lnk_dest_nod_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
