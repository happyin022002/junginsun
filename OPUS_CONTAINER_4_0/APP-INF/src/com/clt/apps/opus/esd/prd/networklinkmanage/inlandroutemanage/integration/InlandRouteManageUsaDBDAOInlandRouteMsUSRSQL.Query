<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageUsaDBDAOInlandRouteMsUSRSQL">
			<desc><![CDATA[InlandRouteMsUS]]></desc>
			<sql><![CDATA[
select m.ROUT_ORG_NOD_CD, m.ROUT_DEST_NOD_CD, m.ROUT_SEQ, nvl(m.DELT_FLG,'N') DELT_FLG
from  
( 
    select ROUT_ORG_NOD_CD, ROUT_DEST_NOD_CD, ROUT_SEQ
#if(${param_val} != '')	
#foreach(${itm} in ${param_val}) 
	           , MIN(DECODE(r, '${itm.a}',LNK_ORG_NOD_CD)) LNK_ORG_NOD_CD${itm.a} 
	           , MIN(DECODE(r,'${itm.a}',TRSP_MOD_CD)) TRSP_MOD_CD${itm.a}
	           , MIN(DECODE(r,'${itm.a}',sp)) sp${itm.a} 
	           , MIN(DECODE(r,'${itm.a}',cofc)) cofc${itm.a}  
	           , MIN(DECODE(r,'${itm.a}',AGMT)) AGMT${itm.a}  
	           , MIN(DECODE(r,'${itm.a}',CMB)) CMB${itm.a} 
	           , MIN(DECODE(r,'${itm.a}',trim(JUNK))) JUNK${itm.a}
#if(${itm.a} == ${param_size})
		       , MIN(DECODE(r,'${itm.a}' ,LNK_dest_NOD_CD)) LNK_dest_NOD_CD${itm.a} 
#end	
#end
#end
    from 
    ( 
        SELECT   
            b.ROUT_ORG_NOD_CD, b.ROUT_DEST_NOD_CD,b.ROUT_SEQ, b.LNK_ORG_NOD_CD,   
            b.LNK_DEST_NOD_CD, b.ROUT_DTL_SEQ, b.TRSP_MOD_CD, B.VNDR_SEQ SP,B.RAIL_CRR_TP_CD cofc,  
            DECODE(B.TRSP_AGMT_OFC_CTY_CD, NULL,'', B.TRSP_AGMT_OFC_CTY_CD||B.TRSP_AGMT_SEQ) AGMT, 
            B.INLND_ROUT_CMB_FLG CMB, B.INLND_ROUT_JUNC_NM JUNK, 
            ROW_NUMBER() OVER(PARTITION BY b.ROUT_ORG_NOD_CD, b.ROUT_DEST_NOD_CD,b.ROUT_SEQ  ORDER BY b.ROUT_ORG_NOD_CD, b.ROUT_DEST_NOD_CD, b.ROUT_SEQ,  b.ROUT_DTL_SEQ) r 
        FROM prd_inlnd_rout_dtl B, 
        ( 
        select COUNT(*),rout_org_nod_cd,rout_dest_nod_cd, ROUT_SEQ  
        from prd_inlnd_rout_dtl a 
        where rout_org_nod_cd = @[i_rout_org_nod_cd] --'USLGBPT' 
        and rout_dest_nod_cd = @[i_rout_dest_nod_cd] --'USCLE01' 
        HAVING COUNT(*)= ${param_size}
        GROUP BY rout_org_nod_cd,rout_dest_nod_cd,ROUT_SEQ  
        ) AA 
        WHERE B.ROUT_ORG_NOD_CD = AA.ROUT_ORG_NOD_CD  
        AND   B.rout_dest_nod_cd = AA.rout_dest_nod_cd 
        AND   B.ROUT_SEQ = AA.ROUT_SEQ 
    ) 
    group by ROUT_ORG_NOD_CD,ROUT_DEST_NOD_CD,ROUT_SEQ 
) dtl, 
prd_inlnd_rout_mst m 
where 1=1  
and m.ROUT_ORG_NOD_CD = dtl.ROUT_ORG_NOD_CD 
and m.ROUT_DEST_NOD_CD = dtl.ROUT_DEST_NOD_CD 
and m.ROUT_SEQ = dtl.ROUT_SEQ 
--and nvl(m.DELT_FLG,'N') <> 'Y'
and m.PCTL_IO_BND_CD = @[r_inbound] 
and nvl(m.INLND_ROUT_INV_BIL_PATT_CD,'X')= nvl(@[i_inv] ,'X')  
and nvl(m.ROUT_PLN_CD     ,'X')=nvl( @[i_rout_pln_cd]     ,'X') 
and nvl(m.WRS_FULL_CMDT_CD,'X')=nvl(@[i_wrs_fl_cd]       ,'X') 
and nvl(m.WRS_MTY_CMDT_CD ,'X')=nvl(@[i_wrs_mt_cd]       ,'X')
#if(${param_val} != '')	
#foreach(${itm} in ${param_val}) 
	AND LNK_ORG_NOD_CD${itm.a} = '${itm.lnk_org_loc}${itm.lnk_org_type}'  
	AND TRSP_MOD_CD${itm.a} = '${itm.trsp_mod_cd}' 

#if(${itm.a} == ${param_size})
		AND LNK_DEST_NOD_CD${itm.a} = @[i_rout_dest_nod_cd]
#end
	and nvl(dtl.SP${itm.a}           ,0)=nvl(${itm.vndr_seq}     ,0) 
	and nvl(dtl.AGMT${itm.a}         ,'X')=nvl('${itm.agmt_no}','X') 
	and nvl(dtl.CMB${itm.a}          ,'N')=nvl( '${itm.inlnd_rout_cmb_flg}'       ,'N') 
	and nvl(dtl.COFC${itm.a}         ,'X')=nvl( '${itm.rail_crr_tp_cd}'     ,'X') 
	and nvl(dtl.JUNK${itm.a}         ,'X')=nvl('${itm.inlnd_rout_junc_nm}'     ,'X') 

#end
#end			]]></sql>
			<params>
				<param name="i_rout_org_nod_cd" type="12" value="" out="N"/>
				<param name="i_rout_dest_nod_cd" type="12" value="" out="N"/>
				<param name="r_inbound" type="12" value="" out="N"/>
				<param name="i_inv" type="12" value="" out="N"/>
				<param name="i_rout_pln_cd" type="12" value="" out="N"/>
				<param name="i_wrs_fl_cd" type="12" value="" out="N"/>
				<param name="i_wrs_mt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
