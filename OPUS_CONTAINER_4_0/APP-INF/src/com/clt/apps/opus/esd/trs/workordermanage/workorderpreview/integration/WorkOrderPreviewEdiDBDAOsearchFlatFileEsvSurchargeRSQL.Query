<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewEdiDBDAOsearchFlatFileEsvSurchargeRSQL">
			<desc><![CDATA[NYK-Hawk_FFile(ESV_N)-JOEDI_EU_OUTBOUND
{SURCHARGE]]></desc>
			<sql><![CDATA[
SELECT T1.*
  FROM (
		SELECT D.LGS_COST_CD AS SURCHARGE_TYPE
			  ,D.SCG_AMT AS AMOUNT
			  ,SO.CURR_CD AS CURRENCY
			  ,'true' AS BILLABLE
			  ,'' AS SURCHARGE_REF
			  ,'' AS SURCHARGE_COMMENT
			  ,D.LGS_COST_CD || '-' || C.INTG_CD_VAL_DP_DESC AS DESCRIPTION
		  FROM TRS_TRSP_SCG_DTL D
			  ,TRS_TRSP_SVC_ORD SO
			  ,COM_INTG_CD_DTL  C
		 WHERE D.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
		   AND D.TRSP_SO_SEQ = @[trsp_so_seq]
		   AND D.TRSP_SO_OFC_CTY_CD = SO.TRSP_SO_OFC_CTY_CD
		   AND D.TRSP_SO_SEQ = SO.TRSP_SO_SEQ
		   AND SUBSTR(D.LGS_COST_CD, 3, 2) <> 'FU'
		   AND D.LGS_COST_CD = C.INTG_CD_VAL_CTNT(+)
		   AND C.INTG_CD_ID(+) = 'CD30002'
 ) T1
 RIGHT OUTER JOIN DUAL
    ON 1 = 1			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
