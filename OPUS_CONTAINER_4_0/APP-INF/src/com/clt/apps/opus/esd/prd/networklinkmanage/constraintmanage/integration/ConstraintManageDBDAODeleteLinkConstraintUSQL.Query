<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAODeleteLinkConstraintUSQL">
			<desc><![CDATA[DeleteLinkConstraint]]></desc>
			<sql><![CDATA[
UPDATE PRD_LNK_CNST_MGMT 
	SET DELT_FLG = 'Y', 
	UPD_USR_ID = @[upd_usr_id], 
	UPD_OFC_CD = @[cre_ofc_cd], 
	UPD_DT	= SYSDATE 
WHERE	LNK_ORG_NOD_CD = @[s_node_fm] 
	AND LNK_DEST_NOD_CD = @[s_node_to]
	AND TRSP_MOD_CD = @[s_ts_mode] 
	AND LNK_CNST_ITM_CD = @[s_org_item_code]
	AND lnk_cnst_seq = @[s_lnk_cnst_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="s_node_fm" type="12" value="" out="N"/>
				<param name="s_node_to" type="12" value="" out="N"/>
				<param name="s_ts_mode" type="12" value="" out="N"/>
				<param name="s_org_item_code" type="12" value="" out="N"/>
				<param name="s_lnk_cnst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
