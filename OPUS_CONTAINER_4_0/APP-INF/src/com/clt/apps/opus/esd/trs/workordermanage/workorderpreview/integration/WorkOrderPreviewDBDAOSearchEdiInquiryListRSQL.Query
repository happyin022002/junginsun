<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdiInquiryListRSQL">
			<desc><![CDATA[searchEdiInquiryList]]></desc>
			<sql><![CDATA[
SELECT b.eq_no
 	  , b.trsp_so_ofc_cty_cd
 	  , b.trsp_so_seq
 	  , b.trsp_so_ofc_cty_cd||b.trsp_so_seq trsp_so_ofc_cty_cd_seq	
	  , TO_CHAR(his.LOCL_CRE_DT, 'YYYYMMDDHH24MISS') edi_iss_dt
 	  , NVL(b.edi_rcv_rslt_msg, his.wo_edi_rcv_rslt_msg) edi_rcv_rslt_msg
 	  , NVL(b.edi_rcv_rslt_cd, his.wo_edi_rcv_rslt_cd) edi_rcv_rslt_cd
 	  , TO_CHAR(NVL(b.edi_rcv_rslt_dt, his.wo_edi_rcv_rslt_dt) , 'YYYYMMDDHH24MISS')  edi_rcv_rslt_dt
FROM trs_trsp_wrk_ord a
	, trs_trsp_svc_ord b
	, trs_trsp_wrk_ord_his his
WHERE a.trsp_wo_ofc_cty_cd = his.trsp_wo_ofc_cty_cd
	AND a.trsp_wo_seq = his.trsp_wo_seq
	AND a.trsp_wo_ofc_cty_cd	= @[trsp_wo_ofc_cty_cd]
   	AND a.trsp_wo_seq = @[trsp_wo_seq]
   	AND a.trsp_wo_ofc_cty_cd = b.trsp_wo_ofc_cty_cd(+)
   	AND a.trsp_wo_seq = b.trsp_wo_seq(+)			]]></sql>
			<params>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
