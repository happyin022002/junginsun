<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USALastCityManageDBDAOSearchUSALastCityComboListRSQL">
			<desc><![CDATA[USA/CA CY&Door S/O Creation에서 해당하는 node의 usa last city를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT LST_LOC_CD
  FROM TRS_DMST_LST_CTY
 WHERE DELT_FLG = 'N'
   AND ORG_LOC_CD = @[org_loc_cd]
   AND DEST_LOC_CD = @[dest_loc_cd]
 ORDER BY UPD_DT DESC			]]></sql>
			<params>
				<param name="org_loc_cd" type="12" value="" out="N"/>
				<param name="dest_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
