<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCopVadChkRSQL">
			<desc><![CDATA[GetCopVadChk]]></desc>
			<sql><![CDATA[
SELECT COUNT(H.BKG_NO) AS CHECK_CNT
FROM SCE_COP_HDR H, BKG_BOOKING B
WHERE H.BKG_NO    = @[bkg_no]
AND H.CNTR_NO     = @[cntr_no]
AND H.BKG_NO      = B.BKG_NO
AND B.DE_TERM_CD  = 'Y'
AND H.POD_NOD_CD  LIKE @[pod_cd] ||'%'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
