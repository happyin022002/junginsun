<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsCommonDBDAOsearchMdmCntrTpSzRSQL">
			<desc><![CDATA[searchMdmCntrTpSz]]></desc>
			<sql><![CDATA[

#if( ${eq_radio} == 'U')
SELECT CNTR_TPSZ_CD
  FROM MDM_CNTR_TP_SZ
 WHERE DELT_FLG = 'N'
 ORDER BY RPT_DP_SEQ
#end
#if( ${eq_radio} == 'Z')
SELECT EQ_TPSZ_CD AS CNTR_TPSZ_CD
  FROM CGM_EQ_TP_SZ
 WHERE EQ_KND_CD = 'Z'
 ORDER BY DP_SEQ
#end
#if( ${eq_radio} == 'G')
SELECT EQ_TPSZ_CD AS CNTR_TPSZ_CD
  FROM CGM_EQ_TP_SZ
 WHERE EQ_KND_CD = 'G'
 ORDER BY DP_SEQ
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
