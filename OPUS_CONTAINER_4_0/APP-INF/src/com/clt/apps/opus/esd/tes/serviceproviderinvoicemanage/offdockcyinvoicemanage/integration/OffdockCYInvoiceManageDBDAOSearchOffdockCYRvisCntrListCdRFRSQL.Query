<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OffdockCYInvoiceManageDBDAOSearchOffdockCYRvisCntrListCdRFRSQL">
			<desc><![CDATA[SearchOffdockCYRvisCntrListCdRF]]></desc>
			<sql><![CDATA[
SELECT
   L.TML_SO_OFC_CTY_CD,
   L.TML_SO_SEQ,
   L.TML_SO_CNTR_LIST_SEQ,
   L.CNTR_NO,
   L.CNTR_TPSZ_CD,
   L.CNTR_STY_CD,
   L.IO_BND_CD,
   @[param_lgs_cost_cd] AS LGS_COST_CD,
   L.BL_NO,
   TO_CHAR(L.INV_GATE_IN_DT,'YYYY-MM-DD HH24:MI') INV_GATE_IN_DT,
   TO_CHAR(L.INV_GATE_OUT_DT,'YYYY-MM-DD HH24:MI') INV_GATE_OUT_DT,
   1  RVIS_GATE_IN_FLG,
   1  RVIS_GATE_OUT_FLG,
   L.RVIS_GATE_IN_FLG,
   L.RVIS_GATE_OUT_FLG,
   L.AWK_CGO_FLG,
   L.RC_FLG,
   L.CNTR_RMK
FROM TES_TML_SO_HDR H, TES_TML_SO_CNTR_LIST L
WHERE 1=1
AND H.TML_SO_OFC_CTY_CD = L.TML_SO_OFC_CTY_CD
AND H.TML_SO_SEQ        = L.TML_SO_SEQ
AND H.TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
AND H.TML_SO_SEQ = @[tml_so_seq]
AND L.VRFY_RSLT_IND_CD = 'CO'
AND L.RC_FLG IS NOT NULL AND L.RC_FLG = 'Y'
ORDER BY LGS_COST_CD ASC, CNTR_NO ASC, CNTR_TPSZ_CD ASC, CNTR_STY_CD ASC, IO_BND_CD ASC
			]]></sql>
			<params>
				<param name="param_lgs_cost_cd" type="12" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
