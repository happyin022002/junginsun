<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USA404TrsStccDBDAOModifyTrsStccUSQL">
			<desc><![CDATA[ModifyTrsStcc]]></desc>
			<sql><![CDATA[
UPDATE TRS_STCC
   SET UN_CMDT_CD = @[un_cmdt_cd]
      ,IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
      ,HZD_MTRL_CLSS_CD = @[hzd_mtrl_clss_cd]
      ,PCK_GRP_CD = @[pck_grp_cd]
      ,PCK_GRP_VAL_CD = @[pck_grp_val_cd]
      ,PRP_SHP_NM = @[prp_shp_nm]
      ,DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = sysdate
 WHERE STCC_CD = @[stcc_cd]
   AND STCC_SEQ = @[stcc_seq]			]]></sql>
			<params>
				<param name="un_cmdt_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
				<param name="hzd_mtrl_clss_cd" type="12" value="" out="N"/>
				<param name="pck_grp_cd" type="12" value="" out="N"/>
				<param name="pck_grp_val_cd" type="12" value="" out="N"/>
				<param name="prp_shp_nm" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="stcc_cd" type="12" value="" out="N"/>
				<param name="stcc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
