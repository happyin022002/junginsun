<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRailScgDBDAOInsertRailFuelFixScgAgmtCSQL">
			<desc><![CDATA[US RAIL Surcharge 화면의 US RAIL Agreement Surcharge 저장( Insert 시)]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_AGMT_RAIL_SCG_RT (
	TRSP_AGMT_SCG_SEQ,
    VNDR_SEQ,
    TRSP_RAIL_SCG_CD,
	TRSP_AGMT_OFC_CTY_CD,
    TRSP_AGMT_SEQ,
    AGMT_ROUT_ALL_FLG,
    FM_NOD_CD,
    TO_NOD_CD,
    CGO_TP_CD,
	AGMT_EQ_SZ_NO,
    TRSP_RAIL_RTO,
    EFF_FM_DT,
    EFF_TO_DT,
    RAIL_RTO_NO,
    LBS_OVR_WGT,
    CURR_CD,
    FX_SCG_ALL_RT,
    FX_SCG_20FT_RT,
    FX_SCG_40FT_RT,
    FX_SCG_45FT_RT,
    FUEL_SCG_APLY_FLG,
    DELT_FLG,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
    LOCL_CRE_DT,
    LOCL_UPD_DT,
    COM_SCG_APLY_FLG,
    WO_APLY_FLG,
    USR_DEF_RMK,
    SPCL_CGO_CNTR_TP_CD,
    AGMT_COST_FLG 
) VALUES (
	@[trsp_agmt_scg_seq],
    @[vndr_seq],
    @[trsp_rail_scg_cd],
    @[trsp_agmt_ofc_cty_cd],
    @[trsp_agmt_seq],
    @[agmt_rout_all_flg],
    @[fm_nod_cd]||@[fm_nod_yard],
    @[to_nod_cd]||@[to_nod_yard],
    @[cgo_tp_cd],
	@[agmt_eq_sz_no],
    NVL(@[trsp_rail_rto], 0),
    TO_DATE(@[eff_fm_dt], 'YYYYMMDD'),
    TO_DATE(@[eff_to_dt], 'YYYYMMDD'),
    @[rail_rto_no],
    @[lbs_ovr_wgt],
    @[curr_cd],
    @[fx_scg_all_rt],
    @[fx_scg_20ft_rt],
    @[fx_scg_40ft_rt],
    @[fx_scg_45ft_rt],
    @[fuel_scg_aply_flg],
    @[delt_flg],
	'${sUsrId}',
    SYSDATE,
    '${sUsrId}',
    SYSDATE,
    GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC('${sctrlOfcCd}'),
    GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC('${sctrlOfcCd}'),
    @[com_scg_aply_flg],
    @[wo_aply_flg],
    @[usr_def_rmk],
    @[spcl_cgo_cntr_tp_cd],
    @[agmt_cost_flg]
)			]]></sql>
			<params>
				<param name="trsp_agmt_scg_seq" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="trsp_rail_scg_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_rout_all_flg" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yard" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_yard" type="12" value="" out="N"/>
				<param name="cgo_tp_cd" type="12" value="" out="N"/>
				<param name="agmt_eq_sz_no" type="12" value="" out="N"/>
				<param name="trsp_rail_rto" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="rail_rto_no" type="12" value="" out="N"/>
				<param name="lbs_ovr_wgt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="fx_scg_all_rt" type="12" value="" out="N"/>
				<param name="fx_scg_20ft_rt" type="12" value="" out="N"/>
				<param name="fx_scg_40ft_rt" type="12" value="" out="N"/>
				<param name="fx_scg_45ft_rt" type="12" value="" out="N"/>
				<param name="fuel_scg_aply_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="com_scg_aply_flg" type="12" value="" out="N"/>
				<param name="wo_aply_flg" type="12" value="" out="N"/>
				<param name="usr_def_rmk" type="12" value="" out="N"/>
				<param name="spcl_cgo_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="agmt_cost_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
