<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TPBFileUploadDBDAODeleteRemoveUploadFilesRSQL">
			<desc><![CDATA[DeleteRemoveUploadFiles]]></desc>
			<sql><![CDATA[
UPDATE TPB_TTL_FILE_MGMT
   SET delt_flg = 'Y',
       upd_usr_id = @[user_id],
	   upd_dt = SYSDATE
 WHERE file_no = @[file_no]
   AND file_no_seq = @[file_no_seq]
   AND cre_usr_id = @[user_id]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="file_no" type="12" value="" out="N"/>
				<param name="file_no_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
