<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchObRailEtaRSQL">
			<desc><![CDATA[SearchObRailEta]]></desc>
			<sql><![CDATA[
SELECT NVL(TO_NUMBER(SUBSTR(
(SELECT MIN(COP_NO||COP_DTL_SEQ) 
        FROM   SCE_COP_DTL
        WHERE  COP_NO      = @[cop_no]
        AND    COP_DTL_SEQ < 4000
        AND    ACT_CD      = 'FORRDO'
        AND    ACT_STS_MAPG_CD = 'RL'
        AND    ACT_DT  IS NOT NULL
        AND    (COP_NO||COP_DTL_SEQ) < (SELECT MIN(COP_NO||COP_DTL_SEQ)
                                                     FROM  SCE_COP_DTL
                                                     WHERE  COP_NO      = @[cop_no]
                                                     AND    COP_DTL_SEQ < 4000
                                                     AND    ACT_CD      = 'FORRAD'
                                                     AND    ACT_DT      IS NULL
                                                     AND    EXISTS (SELECT 'X'
                                                                    FROM   SCE_COP_DTL
                                                                    WHERE  COP_NO      = @[cop_no]
                                                                    AND    COP_DTL_SEQ < 4000
                                                                    AND    ACT_DT      IS NOT NULL))) ,15,4)),0) cop_evntp_chk
FROM DUAL
			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
