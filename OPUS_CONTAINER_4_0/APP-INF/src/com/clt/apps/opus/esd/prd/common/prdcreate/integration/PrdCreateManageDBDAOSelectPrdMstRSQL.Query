<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCreateManageDBDAOSelectPrdMstRSQL">
			<desc><![CDATA[SelectPrdMst]]></desc>
			<sql><![CDATA[
SELECT m.pctl_no 
FROM prd_prod_ctl_mst m 
WHERE m.pctl_no LIKE  @[hd_pctl_no]||'%'			]]></sql>
			<params>
				<param name="hd_pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
