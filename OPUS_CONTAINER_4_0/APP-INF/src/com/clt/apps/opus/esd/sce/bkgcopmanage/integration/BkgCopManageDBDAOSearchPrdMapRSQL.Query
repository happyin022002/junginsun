<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchPrdMapRSQL">
			<desc><![CDATA[PRD_BKG_COP_MAP 의 정보를 복수의 booking 에 대해 조회한다.]]></desc>
			<sql><![CDATA[
SELECT PCTL_NO           ,
       BKG_NO            ,
       COP_NO            ,
       COP_MAPG_SEQ      ,
       CRNT_FLG          ,
       CNTR_NO           ,
       CNTR_TPSZ_CD      ,
       COP_OP_TP_CD      ,
       OB_ITCHG_CTNT     ,
       MTY_PKUP_YD_CD    ,
       BKG_OP_RMK        ,
       MTY_RTN_YD_CD     ,
       IB_ITCHG_CTNT     ,
       POR_NOD_CD        ,
       OCN_ITCHG_CTNT    ,
       POL_NOD_CD        ,
       BKG_RCV_TERM_CD   ,
       BKG_DE_TERM_CD    ,
       OB_TRO_FLG        ,
       IB_TRO_FLG        ,
       COP_PATT_ORD_NO   ,
       COP_SO_KNT		 ,
       CRE_USR_ID		 ,
       UPD_USR_ID		  				
  FROM PRD_BKG_COP_MAP
 WHERE
	#foreach($bkg_no IN ${bkg_no})
		#set ($ndx = $velocityCount)
		#if($velocityCount == 1 )
       (    BKG_NO = '$bkg_no'
			#if (${cop_mapg_seq} != '')
				#foreach($cop_mapg_seq IN ${cop_mapg_seq})
					#if($velocityCount == $ndx )
        AND COP_MAPG_SEQ IN (
                   NVL('$cop_mapg_seq', (SELECT MAX(COP_MAPG_SEQ) FROM PRD_BKG_COP_MAP WHERE BKG_NO = '$bkg_no' AND NVL(CRNT_FLG, 'N') = 'Y'))
            )
					#end
				#end
			#else
        AND COP_MAPG_SEQ IN (
                   NVL(NULL, (SELECT MAX(COP_MAPG_SEQ) FROM PRD_BKG_COP_MAP WHERE BKG_NO = '$bkg_no' AND NVL(CRNT_FLG, 'N') = 'Y'))
            )
			#end
       )
		#else 
       OR
       (    BKG_NO = '$bkg_no'
			#if (${cop_mapg_seq} != '')
			#foreach($cop_mapg_seq IN ${cop_mapg_seq})
			#if($velocityCount == $ndx )
        AND COP_MAPG_SEQ IN (
                   NVL('$cop_mapg_seq', (SELECT MAX(COP_MAPG_SEQ) FROM PRD_BKG_COP_MAP WHERE BKG_NO = '$bkg_no' AND NVL(CRNT_FLG, 'N') = 'Y'))
            )
			#end
			#end
			#else
        AND COP_MAPG_SEQ IN (
                   NVL(NULL, (SELECT MAX(COP_MAPG_SEQ) FROM PRD_BKG_COP_MAP WHERE BKG_NO = '$bkg_no' AND NVL(CRNT_FLG, 'N') = 'Y'))
            )
			#end
       )
		#end 
	#end
 ORDER BY COP_MAPG_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
