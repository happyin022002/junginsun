<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExceptionDataDBDAOSearchExptDetailList3RSQL">
			<desc><![CDATA[select list 3]]></desc>
			<sql><![CDATA[
SELECT TOA.EXPT_TP_CD COP_EXPT_TP_CD, TP.EXPT_CD_NM, TOA.EXPT_TP_DTL_CD||'00000' COP_EXPT_TP_DTL_CD, DTP.EXPT_TP_DTL_NM COP_EXPT_TP_DTL_NM, TOA.EXPT_TP_DTL_DESC COP_EXPT_TP_DTL_DESC
	,FMA.EXPT_CD FM_EXPT_CD
	,FMA.FM_ACT_CD FM_ACT, FMA.FM_ACT_NM
	,TOA.EXPT_CD TO_EXPT_CD
  ,(CASE WHEN TOA.TO_ACT_EXPT_CD<>'00' THEN TOA.TO_ACT_CD END) TO_ACT
  ,(CASE WHEN TOA.TO_ACT_EXPT_CD<>'00' THEN TOA.TO_ACT_NM END) TO_ACT_NM  
  ,TOA.UPD_USR_ID USR_ID, TO_CHAR(TOA.UPD_DT,'YYYY/MM/DD HH24:MI:SS') UPD_DT, TOA.ACT_FLG  
FROM   
( SELECT SUBSTR(EXPT_CD,1,1) EXPT_TP_CD, EXPT_CD_NM, EXPT_CD_DESC FROM SCE_EXPT_CD WHERE SUBSTR(EXPT_CD, 2, LENGTH(EXPT_CD) ) = '0000000' AND ACT_FLG = 'Y' ) TP,  
( SELECT SUBSTR(EXPT_CD,1,1) EXPT_TP_CD, SUBSTR(EXPT_CD,1,3) EXPT_TP_DTL_CD, EXPT_CD_NM EXPT_TP_DTL_NM FROM SCE_EXPT_CD WHERE EXPT_CD LIKE '%00000' AND ACT_FLG = 'Y' AND EXPT_CD NOT LIKE '%0000000' ) DTP,  
( SELECT SUBSTR(TOP.EXPT_CD,1,1) EXPT_TP_CD, SUBSTR(TOP.EXPT_CD,1,3) EXPT_TP_DTL_CD,TOP.COP_EXPT_TP_DTL_DESC EXPT_TP_DTL_DESC   
		,SUBSTR(TOP.EXPT_CD,5,2) FM_ACT_EXPT_CD ,SUBSTR(TOP.EXPT_CD,7,2) TO_ACT_EXPT_CD  
  	,TOP.EXPT_CD_NM TO_ACT_CD, TOP.EXPT_CD_DESC TO_ACT_NM, TOP.UPD_USR_ID, TOP.UPD_DT, TOP.ACT_FLG, TOP.EXPT_CD  
  FROM   SCE_EXPT_CD TOP  
  WHERE  TOP.EXPT_CD NOT LIKE '%00000'  
	AND    (SUBSTR(TOP.EXPT_CD,4,1) = '2' OR (SUBSTR(TOP.EXPT_CD,1,1) BETWEEN '2' AND '3'  AND SUBSTR(TOP.EXPT_CD,7,2) = '00')) ) TOA,  
	(SELECT SUBSTR(FMP.EXPT_CD,1,1) EXPT_TP_CD, SUBSTR(FMP.EXPT_CD,1,3) EXPT_TP_DTL_CD  
		,SUBSTR(FMP.EXPT_CD,5,2) FM_ACT_EXPT_CD   
		,FMP.EXPT_CD_NM FM_ACT_CD, FMP.EXPT_CD_DESC FM_ACT_NM, FMP.EXPT_CD  
	FROM   SCE_EXPT_CD FMP  
	WHERE  FMP.EXPT_CD NOT LIKE '%00000'  
	AND    SUBSTR(FMP.EXPT_CD,4,1) = '1' ) FMA  
WHERE  TP.EXPT_TP_CD = TOA.EXPT_TP_CD  
AND    DTP.EXPT_TP_DTL_CD = TOA.EXPT_TP_DTL_CD  
AND    FMA.EXPT_TP_DTL_CD = TOA.EXPT_TP_DTL_CD  
AND    FMA.FM_ACT_EXPT_CD = TOA.FM_ACT_EXPT_CD
AND    TP.EXPT_TP_CD = @[f_expt_tp]			]]></sql>
			<params>
				<param name="f_expt_tp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
