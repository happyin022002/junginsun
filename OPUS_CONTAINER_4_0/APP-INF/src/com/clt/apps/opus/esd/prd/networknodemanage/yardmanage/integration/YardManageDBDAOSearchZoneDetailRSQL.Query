<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="YardManageDBDAOSearchZoneDetailRSQL">
			<desc><![CDATA[SearchZoneDetail]]></desc>
			<sql><![CDATA[
SELECT 
	-- location
	loc_cd location_code
	, (SELECT loc_nm FROM mdm_location WHERE loc_cd=a.loc_cd AND NVL(delt_flg,'N') <> 'Y') location_name
	
	-- zone
	, zn_cd zone_code, zn_nm zone_name
	, cgo_hndl_tm_hrs cargo_handling_time

	-- representative cy
	, rep_yd_cd representative_code
	, (SELECT yd_nm FROM mdm_yard WHERE yd_cd=a.rep_yd_cd AND NVL(delt_flg,'N') <> 'Y') representative_name
	, lnk_dist distance, tztm_hrs tt
FROM   mdm_zone a
WHERE  zn_cd = @[node_code]
AND    nvl(delt_flg,'N') <> 'Y'			]]></sql>
			<params>
				<param name="node_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
