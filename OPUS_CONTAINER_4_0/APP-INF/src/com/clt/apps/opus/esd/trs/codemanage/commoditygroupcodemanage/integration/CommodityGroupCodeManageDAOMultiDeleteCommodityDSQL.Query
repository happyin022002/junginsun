<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDAOMultiDeleteCommodityDSQL">
			<desc><![CDATA[Rail Vendor별 Group Commodity를 재 정의한 데이터를 삭제]]></desc>
			<sql><![CDATA[
delete 	trs_cmdt_grp_cz				        
WHERE 	trsp_grp_cmdt_cd	= @[trsp_grp_cmdt_cd]			   							
		AND vndr_seq	 	= @[vndr_seq]			]]></sql>
			<params>
				<param name="trsp_grp_cmdt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
