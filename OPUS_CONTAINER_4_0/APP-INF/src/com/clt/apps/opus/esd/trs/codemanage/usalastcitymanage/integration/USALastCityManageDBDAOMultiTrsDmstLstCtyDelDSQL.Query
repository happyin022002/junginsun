<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USALastCityManageDBDAOMultiTrsDmstLstCtyDelDSQL">
			<desc><![CDATA[Trucker에게 중간 경유지를 지정해 주기 위한 USA Last City를 삭제 상태로 변경]]></desc>
			<sql><![CDATA[
UPDATE trs_dmst_lst_cty
SET delt_flg = @[delt_flg]
WHERE	org_loc_cd  = @[org_loc_cd]
AND 	dest_loc_cd = @[dest_loc_cd]
AND 	lst_loc_cd  = @[lst_loc_cd]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="org_loc_cd" type="12" value="" out="N"/>
				<param name="dest_loc_cd" type="12" value="" out="N"/>
				<param name="lst_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
