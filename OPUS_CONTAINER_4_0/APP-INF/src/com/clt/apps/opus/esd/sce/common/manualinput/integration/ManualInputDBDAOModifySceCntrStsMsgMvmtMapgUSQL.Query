<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOModifySceCntrStsMsgMvmtMapgUSQL">
			<desc><![CDATA[ModifySceCntrStsMsgMvmtMapg]]></desc>
			<sql><![CDATA[
UPDATE SCE_CNTR_STS_MSG_MVMT_MAPG
   SET STND_EDI_STS_CD = @[stnd_edi_sts_cd],
       CSM_DESC = @[csm_desc],
       EFF_FM_DT = @[eff_fm_dt],
       EFF_TO_DT = @[eff_to_dt],
       DELT_FLG = @[delt_flg],
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = sysdate
 WHERE ACT_STS_MAPG_CD = @[act_sts_mapg_cd]
  AND  STND_EDI_STS_CD = @[stnd_edi_sts_cd]
  AND  CSM_CNT_CD 	   = @[csm_cnt_cd]			]]></sql>
			<params>
				<param name="stnd_edi_sts_cd" type="12" value="" out="N"/>
				<param name="csm_desc" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="act_sts_mapg_cd" type="12" value="" out="N"/>
				<param name="csm_cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
