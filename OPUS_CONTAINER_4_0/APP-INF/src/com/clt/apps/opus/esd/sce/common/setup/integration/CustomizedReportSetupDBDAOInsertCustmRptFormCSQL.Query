<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomizedReportSetupDBDAOInsertCustmRptFormCSQL">
			<desc><![CDATA[Customized Report Form Setup 정보 등록]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_PG_STUP_MST (EXE_USR_ID     
                     ,MOFC_ID
                     ,PGM_NO        
                     ,RPT_INFO_CTNT
                     ,CRE_USR_ID     
                     ,CRE_DT  
                     ,UPD_USR_ID     
                     ,UPD_DT )       
              VALUES (@[user_id]              
                     ,@[user_ofc_cd]
                     ,@[pgm_no]              
                     ,@[save_list] 
                     ,@[user_id]              
                     ,SYSDATE             
                     ,@[user_id]              
                     ,SYSDATE )			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="pgm_no" type="12" value="" out="N"/>
				<param name="save_list" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
