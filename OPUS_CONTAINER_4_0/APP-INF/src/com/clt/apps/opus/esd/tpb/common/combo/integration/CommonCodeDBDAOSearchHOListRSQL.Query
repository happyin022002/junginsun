 SELECT OFC_CD
       ,OFC_CD AS OFC_CD2
   FROM MDM_ORGANIZATION
  WHERE 1 = 1
    AND OFC_CD IN ( SELECT OFC_CD FROM MDM_OFC_GRP_MAPG WHERE OFC_GRP_ID = '000003' AND SUB_SYS_CD='TPB' )			]]></sql>
