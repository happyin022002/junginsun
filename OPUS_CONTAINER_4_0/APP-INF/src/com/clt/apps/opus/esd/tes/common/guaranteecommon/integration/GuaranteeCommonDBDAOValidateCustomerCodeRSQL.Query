<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeCommonDBDAOValidateCustomerCodeRSQL">
			<desc><![CDATA[Customer Code Validate]]></desc>
			<sql><![CDATA[
SELECT  
        CASE WHEN COUNT(CUST_SEQ) > 0 
             THEN 'Y'
             ELSE 'N'
        END CUST_SEQ_EXIST
		, CUST_CNT_CD || LPAD( CUST_SEQ, 6, 0) CUST_CD
		, CUST_LGL_ENG_NM
FROM    MDM_CUSTOMER
WHERE   CUST_CNT_CD = @[cust_cnt_cd]
AND     CUST_SEQ    = @[cust_seq]
GROUP BY CUST_LGL_ENG_NM
		, CUST_CNT_CD
		, CUST_SEQ			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
