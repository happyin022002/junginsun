                    VALUES(@[splc_cd] ,@[splc_vndr_nm] ,@[yd_cd] ,@[splc_abbr_loc_nm] ,@[ste_cd]
                          ,@[cuty_nm] ,@[splc_loc_nm] ,@[splc_dflt_flg] ,@[loc_cd]
                          ,@[upd_usr_id] ,sysdate ,@[upd_usr_id] ,sysdate)			]]></sql>
			<params>
				<param name="splc_cd" type="12" value="" out="N"/>
				<param name="splc_vndr_nm" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="splc_abbr_loc_nm" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="cuty_nm" type="12" value="" out="N"/>
				<param name="splc_loc_nm" type="12" value="" out="N"/>
				<param name="splc_dflt_flg" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
