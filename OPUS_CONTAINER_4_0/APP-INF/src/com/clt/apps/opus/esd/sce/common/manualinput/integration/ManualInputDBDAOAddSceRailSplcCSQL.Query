		<query name="ManualInputDBDAOAddSceRailSplcCSQL">
			<desc><![CDATA[AddSceRailSplc]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_RAIL_SPLC(SPLC_CD ,SPLC_VNDR_NM ,YD_CD ,SPLC_ABBR_LOC_NM ,STE_CD
                          ,CUTY_NM ,SPLC_LOC_NM ,SPLC_DFLT_FLG ,LOC_CD
