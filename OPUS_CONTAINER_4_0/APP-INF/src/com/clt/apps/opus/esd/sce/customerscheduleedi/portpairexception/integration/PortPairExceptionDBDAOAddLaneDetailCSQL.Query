<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairExceptionDBDAOAddLaneDetailCSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_CUST_EDI_ADJ (
	CUST_TRD_PRNR_ID
	,POL_CD
	,POD_CD
	,SLAN_CD
	,ADJ_SEQ
	,ETD_ADJ_DY
	,ETD_ADJ_HRMNT
	,DCT_ADJ_TP_NM
	,DCT_ADJ_DY
	,DCT_ADJ_ETB_DYS
	,DCT_ADJ_ETD_DYS
	,DCT_ADJ_HRMNT
	,CCT_ADJ_TP_NM
	,CCT_ADJ_DY
	,CCT_ADJ_ETB_DYS
	,CCT_ADJ_ETD_DYS
	,CCT_ADJ_HRMNT
	,DELT_FLG
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
) VALUES (
	 @[cust_trd_prnr_id]
	,@[pol_cd]
	,@[pod_cd]
	,@[slan_cd]
	,(
		SELECT NVL(MAX(ADJ_SEQ),0) + 1
  		FROM SCE_CUST_EDI_ADJ
 		WHERE CUST_TRD_PRNR_ID = @[cust_trd_prnr_id]
	 	AND POL_CD = @[pol_cd]
 	 	AND POD_CD	= @[pod_cd]
	 	AND SLAN_CD= @[slan_cd]
	  )
	,@[etd_adj_dy]
	,@[etd_adj_hrmnt]
	,@[dct_adj_tp_nm]
	,@[dct_adj_dy]
	,@[dct_adj_etb_dys]
	,@[dct_adj_etd_dys]
	,@[dct_adj_hrmnt]
	,@[cct_adj_tp_nm]
	,@[cct_adj_dy]
	,@[cct_adj_etb_dys]
	,@[cct_adj_etd_dys]
	,@[cct_adj_hrmnt]
	,'N'
	,@[cre_usr_id]
	,SYSDATE
	,@[upd_usr_id]
	,SYSDATE
)			]]></sql>
			<params>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="etd_adj_dy" type="12" value="" out="N"/>
				<param name="etd_adj_hrmnt" type="12" value="" out="N"/>
				<param name="dct_adj_tp_nm" type="12" value="" out="N"/>
				<param name="dct_adj_dy" type="12" value="" out="N"/>
				<param name="dct_adj_etb_dys" type="12" value="" out="N"/>
				<param name="dct_adj_etd_dys" type="12" value="" out="N"/>
				<param name="dct_adj_hrmnt" type="12" value="" out="N"/>
				<param name="cct_adj_tp_nm" type="12" value="" out="N"/>
				<param name="cct_adj_dy" type="12" value="" out="N"/>
				<param name="cct_adj_etb_dys" type="12" value="" out="N"/>
				<param name="cct_adj_etd_dys" type="12" value="" out="N"/>
				<param name="cct_adj_hrmnt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
