<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceProviderCategoryManageDBDAOSearchVndrSeqRSQL">
			<desc><![CDATA[SearchVndrSeq]]></desc>
			<sql><![CDATA[
SELECT LPAD(VNDR_SEQ, 6, '0') as  VNDR_SEQ, 
			  VNDR_LGL_ENG_NM  as VNDR_ABBR_NM,
			VNDR_LGL_ENG_NM  
    FROM MDM_VENDOR
   WHERE 1 = 1 
    AND NVL(DELT_FLG, 'N') <> 'Y'
    AND VNDR_SEQ = TO_NUMBER(@[vndr_seq])			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
