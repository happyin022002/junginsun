<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchIsTPBOfficeRSQL">
			<desc><![CDATA[SearchIsTPBOffice]]></desc>
			<sql><![CDATA[
SELECT DECODE( SIGN(COUNT(0)), 1, 'Y', 'N') AS is_tpboffice
  FROM TPB_HNDL_OFC A
 WHERE 1=1
   AND n3pty_ofc_tp_cd = 'T'
   AND delt_flg = 'N'
   AND ofc_cd = @[s_ofc_cd]			]]></sql>
			<params>
				<param name="s_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
