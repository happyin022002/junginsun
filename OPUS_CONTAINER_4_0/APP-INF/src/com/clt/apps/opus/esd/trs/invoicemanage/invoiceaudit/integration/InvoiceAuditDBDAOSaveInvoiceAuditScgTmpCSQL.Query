<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSaveInvoiceAuditScgTmpCSQL">
			<desc><![CDATA[Surcharge Temp 정보 입력]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_SCG_DTL_TMP							
 (	INV_FNE_CUZ_DESC	
 ,	LGS_COST_CD			
 ,	WO_PRV_GRP_SEQ		
 ,	TRSP_SO_OFC_CTY_CD	
 ,	TRSP_SO_SEQ			
 ,	N3PTY_DESC	
 ,	CRE_USR_ID
 ,	CRE_DT
 ,	UPD_USR_ID
 ,	UPD_DT
 ,	LOCL_CRE_DT
 ,	LOCL_UPD_DT		
 ) VALUES					
 (	'TRS_SPP_INV_SAV_PRG'
 ,	'SPP'				
 ,	TRS_TRSP_SCG_DTL_TMP_SEQ1.NEXTVAL					
 ,	@[trsp_so_ofc_cty_cd]					
 ,	@[trsp_so_seq]					
 ,	@[n3pty_desc]					
 		||(SELECT '/BZC_AMT:'	|| @[bzc_amt]						
 			|| '/FUEL_SCG_AMT:'	|| @[fuel_scg_amt]						
 			|| '/ETC_ADD_AMT:'	|| @[etc_add_amt]						
 			|| '/NEGO_AMT:'	|| @[nego_amt]							
 			FROM			
 				TRS_TRSP_SVC_ORD							
 			WHERE			
 				TRSP_SO_OFC_CTY_CD 	= @[trsp_so_ofc_cty_cd]						
 			AND TRSP_SO_SEQ			= @[trsp_so_seq]						
 			)	
 , @[FORM_CRE_USR_ID]
 , SYSDATE	
 , @[FORM_CRE_USR_ID]	
 , SYSDATE	
 , GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[FORM_USR_OFC_CD])
 , GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[FORM_USR_OFC_CD])	
 )			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
				<param name="n3pty_desc" type="12" value="" out="N"/>
				<param name="bzc_amt" type="6" value="" out="N"/>
				<param name="fuel_scg_amt" type="6" value="" out="N"/>
				<param name="etc_add_amt" type="6" value="" out="N"/>
				<param name="nego_amt" type="6" value="" out="N"/>
				<param name="FORM_CRE_USR_ID" type="12" value="" out="N"/>
				<param name="FORM_USR_OFC_CD" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
