<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOCheckCopToBeTroMapgAliveRSQL">
			<desc><![CDATA[container detach 될 때 호출하여 TRO MAPG가 살아있으면 COP cancel 체크를 skip한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CHK_RSLT
FROM SCE_COP_HDR A, SCE_TRO_MAPG B
WHERE A.COP_STS_CD != 'X'
AND A.COP_NO = B.COP_NO
AND A.BKG_NO = B.BKG_NO
AND A.COP_NO = @[cop_no]			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
