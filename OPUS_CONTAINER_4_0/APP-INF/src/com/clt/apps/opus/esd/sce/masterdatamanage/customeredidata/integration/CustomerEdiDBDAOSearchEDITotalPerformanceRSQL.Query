                              #end /*if-11e*/
                            #end 
                       ) 
				) v   			
			where d.bkg_no = v.bkg_no	
#elseif(${cntr_no} != '')
 			where d.CNTR_NO in (
                                   
                            #foreach($ele in ${cntr_no})
                              #if($velocityCount == 1) /*if-12s*/
                                   '$ele'
                              #else
