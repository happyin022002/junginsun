		        and d.edi_snd_knt = 1  	

#elseif(${bl_no}   != '')
               , (   select bkg_no from bkg_booking	
				    where (bl_no) 
                       in (
                            #foreach($ele in ${bl_no})
                              #if($velocityCount == 1) /*if-11s*/
