  								--O/B 구간 Rail이 아닌 Shipment  
 								(substr(rail_chk, 1, 1) <> 'I'  and a_edi_sts_cd in ('ALO','RLO','ARO','URO','FOTRDO','FOTRARN')) or  
 								--I/B 구간 Rail이 아닌 Shipment  
  								(substr(rail_chk, 2, 1) <> 'I' and  a_edi_sts_cd in ('ARN','RLN','ARN','URN','FITRARN','FITRDO','AVN','ACN','AFN','AON','NT')) or 
 								--I/B 구간 Rail Shipment  
  								(substr(rail_chk, 2, 1) = 'I' and  a_edi_sts_cd in ('AVL','ACL','AFL','AOL')) or  
  								--DEL이 ‘US’가 아닌 것   
  								(substr(del_cd, 1, 2) <> 'US' and  a_edi_sts_cd in ('CT','CC','CU','HR','PA','PQ','AVN','AVL','ACN','ACL')) or  
 								--DEL이 ‘CA,MX’가 아닌 것  
  								(substr(del_cd, 1, 2) <> 'CA' and substr(del_cd, 1, 2) <> 'MX' and a_edi_sts_cd in ('OB','AFN','AFL','AON','AOL')) or 
 								--DEL이 ‘US,CA,MX’가 아닌 것  
  								(substr(del_cd, 1, 2) <> 'US' and substr(del_cd, 1, 2) <> 'CA' and substr(del_cd, 1, 2) <> 'MX' and a_edi_sts_cd in ('NFT','FTR')) 
  								then  'EXP' else 'N/A' end t_cnt, 
								trunc(abs(nvl(d.GMT_DT  - d.act_dt, 0))) diff   
  					   from   (  
  							select  
