  				and a.a_edi_sts_cd    = d.EDI_STS_CD(+)  
  				and a.a_cust_edi_sts_cd = d.EDI_SUB_STS_CD(+)  
  			) tar  
 		where t_cnt = 'N/A')  tar_1    
  ) final  
/*repeat-e*/

