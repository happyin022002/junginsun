<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOAddSceEdiLogCSQL">
			<desc><![CDATA[AddSceEdiLog]]></desc>
			<sql><![CDATA[
insert into sce_edi_log
(
 CNTR_NO
,BKG_NO
,EDI_LOG_SEQ
,EDI_RCV_TP_CD
,EDI_SND_RMK
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
)
values
(
 @[cntr_no]					--CNTR_NO
,@[bkg_no]					--BKG_NO
,sce_edi_log_seq1.nextval	--EDI_LOG_SEQ
,@[edi_rcv_tp_cd]			--EDI_RCV_TP_CD
,@[edi_snd_rmk]				--EDI_SND_RMK
,nvl(@[cre_usr_id],'-')		--CRE_USR_ID
,sysdate					--CRE_DT
,to_char(sysdate,'yyyymmdd')--UPD_USR_ID
,sysdate					--UPD_DT
)			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="edi_rcv_tp_cd" type="12" value="" out="N"/>
				<param name="edi_snd_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
