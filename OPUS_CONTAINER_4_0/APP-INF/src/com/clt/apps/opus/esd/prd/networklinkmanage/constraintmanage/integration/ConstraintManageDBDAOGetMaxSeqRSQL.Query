<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOGetMaxSeqRSQL">
			<desc><![CDATA[GetMaxSeq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(ROUT_CNST_SEQ), 0) + 1 maxS 
	FROM PRD_ROUT_CNST 
	WHERE TRNK_LANE_CD = @[s_trunk_lane] 
	AND POL_NOD_CD = @[s_pol_node] 
	AND POD_NOD_CD LIKE @[s_pod] ||'%'			]]></sql>
			<params>
				<param name="s_trunk_lane" type="12" value="" out="N"/>
				<param name="s_pol_node" type="12" value="" out="N"/>
				<param name="s_pod" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
