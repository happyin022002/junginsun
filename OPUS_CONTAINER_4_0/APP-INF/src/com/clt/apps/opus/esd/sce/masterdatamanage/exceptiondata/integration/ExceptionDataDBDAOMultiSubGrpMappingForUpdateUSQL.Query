<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExceptionDataDBDAOMultiSubGrpMappingForUpdateUSQL">
			<desc><![CDATA[update group map]]></desc>
			<sql><![CDATA[
update SCE_EXPT_SUBSC_MST_GRP
set COP_EXPT_SUBSC_GRP_CD = @[r_subseq_grp]
	,SUBSC_GRP_NTFD_PTY_CD = @[r_noti_prty]
	,ACT_FLG = @[r_act]
	,UPD_USR_ID = @[usr_id]
	,UPD_DT = sysdate
where 1=1

#if(${r_expt_tp} == '10000000' || ${r_expt_tp} == '40000000')
and EXPT_CD = @[r_to_act]
#else
and EXPT_CD = @[r_fm_act]
#end

and COP_EXPT_SUBSC_CS_SEQ = @[cop_expt_subsc_cs_seq]			]]></sql>
			<params>
				<param name="r_subseq_grp" type="12" value="" out="N"/>
				<param name="r_noti_prty" type="12" value="" out="N"/>
				<param name="r_act" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="r_to_act" type="12" value="" out="N"/>
				<param name="r_fm_act" type="12" value="" out="N"/>
				<param name="cop_expt_subsc_cs_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
