<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualInputDBDAOSelectSceRailSplcRSQL">
			<desc><![CDATA[SelectSceRailSplc]]></desc>
			<sql><![CDATA[
SELECT SPLC_CD
      ,SPLC_VNDR_NM
      ,YD_CD
      ,SPLC_ABBR_LOC_NM
      ,STE_CD
      ,CUTY_NM
      ,SPLC_LOC_NM
      ,SPLC_DFLT_FLG
      ,LOC_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
 FROM SCE_RAIL_SPLC
WHERE 1 = 1
  
#if (${frm_splc_cd} != '') 
  AND SPLC_CD = @[frm_splc_cd]
#end

#if (${frm_yd_cd} != '') 
  AND YD_CD = @[frm_yd_cd]
#end

#if (${frm_splc_vndr_nm} != '') 
  AND SPLC_VNDR_NM like @[frm_splc_vndr_nm] || '%'
#end			]]></sql>
			<params>
				<param name="frm_splc_cd" type="12" value="" out="N"/>
				<param name="frm_yd_cd" type="12" value="" out="N"/>
				<param name="frm_splc_vndr_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
