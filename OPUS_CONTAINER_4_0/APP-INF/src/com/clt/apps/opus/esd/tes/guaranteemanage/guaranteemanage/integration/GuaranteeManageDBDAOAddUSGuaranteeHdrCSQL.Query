<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeManageDBDAOAddUSGuaranteeHdrCSQL">
			<desc><![CDATA[Guarantee Header정보 추가]]></desc>
			<sql><![CDATA[
INSERT INTO TES_GNTE_HDR GH (
		  GNTE_NO
		, OFC_CD
		, GNTE_TP_CD
		, CURR_CD
		, VNDR_SEQ
		, GNTE_CUST_CD
		, BKG_STS_CD
        , YD_CD
		, TTL_AMT
		, PIC_PHN_NO
		, PIC_FAX_NO
		, DEPT_NO
		, GNTE_RMK
		, DELT_FLG
		, DMY_FLG
		, CRE_USR_ID
		, CRE_DT
		, UPD_USR_ID
		, UPD_DT
		)
VALUES	(
		  @[gnte_no]
		, @[ofc_cd]
		, @[gnte_tp_cd]
		, @[curr_cd]
		, @[vndr_seq]
		, @[gnte_cust_cd]
		, @[bkg_sts_cd]
        , @[yd_cd]
		, @[ttl_amt]
		, @[pic_phn_no]
		, @[pic_fax_no]
		, @[dept_no]
		, @[gnte_rmk]
		, @[delt_flg]
		, @[dmy_flg]	 --// Y인 경우는 IRR생성으로 DUMMY GUARANTEE를 생성해야하는 경우
		, @[cre_usr_id]
		, SYSDATE
		, @[cre_usr_id]
		, SYSDATE
		)			]]></sql>
			<params>
				<param name="gnte_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="gnte_tp_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="gnte_cust_cd" type="12" value="" out="N"/>
				<param name="bkg_sts_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="ttl_amt" type="6" value="" out="N"/>
				<param name="pic_phn_no" type="12" value="" out="N"/>
				<param name="pic_fax_no" type="12" value="" out="N"/>
				<param name="dept_no" type="12" value="" out="N"/>
				<param name="gnte_rmk" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="dmy_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
