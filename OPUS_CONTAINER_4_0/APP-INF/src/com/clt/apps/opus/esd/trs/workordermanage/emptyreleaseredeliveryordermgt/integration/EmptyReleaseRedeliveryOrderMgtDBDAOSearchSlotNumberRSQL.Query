<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyReleaseRedeliveryOrderMgtDBDAOSearchSlotNumberRSQL">
			<desc><![CDATA[EmptyReleaseRedeliveryOrderMgtDBDAOSearchSlotNumber]]></desc>
			<sql><![CDATA[
WITH V AS
 (select rownum r
        ,V1.ABC || V2.ABC ABC
    from (SELECT chr(ASCII('A') + LEVEL - 1) ABC FROM DUAL connect by level <= 26) V1
        ,(SELECT chr(ASCII('A') + LEVEL - 1) ABC FROM DUAL connect by level <= 26) V2)
SELECT SUBSTR(@[wo_no], -5) || V.ABC AS SLOT_NUM FROM V WHERE R = @[slot_cnt]			]]></sql>
			<params>
				<param name="wo_no" type="12" value="" out="N"/>
				<param name="slot_cnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
