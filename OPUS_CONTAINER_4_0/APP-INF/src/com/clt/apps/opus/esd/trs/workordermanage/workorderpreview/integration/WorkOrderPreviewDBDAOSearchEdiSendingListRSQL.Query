<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdiSendingListRSQL">
			<desc><![CDATA[SearchEdiSendingList]]></desc>
			<sql><![CDATA[
SELECT 
    B.COP_NO as cop_no,
    B.COST_ACT_GRP_SEQ as cost_act_grp_seq,
    B.VNDR_SEQ as vndr_seq
FROM TRS_TRSP_WRK_ORD_PRV_TMP A, TRS_TRSP_SVC_ORD B
WHERE A.WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
    AND A.WO_ISS_NO = @[wo_iss_no]
	AND A.WO_CXL_FLG = 'N'
	AND B.COP_NO IS NOT NULL
    AND (B.TRSP_SO_OFC_CTY_CD, B.TRSP_SO_SEQ) IN ((A.TRSP_SO_OFC_CTY_CD, A.TRSP_SO_SEQ))
			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
