<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchCheckOfficeRSQL">
			<desc><![CDATA[SearchCheckOffice]]></desc>
			<sql><![CDATA[
SELECT ofc_cd, 
       ofc_cd
  FROM MDM_ORGANIZATION
 WHERE ofc_cd = @[s_if_ofc_cd]			]]></sql>
			<params>
				<param name="s_if_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
