<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOSearch07RailSoManageRSQL">
			<desc><![CDATA[US Rail O/B Verify SQL]]></desc>
			<sql><![CDATA[
SELECT 
	EQ_NO, 
	MAX(VERIFY_RESULT) VERIFY_RESULT, 
	MAX(VERIFY_YN) VERIFY_YN 
FROM ( 
	SELECT 
		EQ_NO, 
		SUBSTR(  TO_CHAR(LOCL_CRE_DT, 'YYYYMMDD')|| 'COMPANY - CNTR_NO: ' 
		||EQ_NO||' S/O created( '||TO_CHAR(LOCL_CRE_DT, 'YYYYMMDD') 
		||DECODE(TRSP_BND_CD,'O', ' : Full )', ' : Mty )'||' ROUTE : ' 
		||FM_NOD_CD||'-->'||TO_NOD_CD),9,100) VERIFY_RESULT, 
		CASE WHEN SO.TRSP_BND_CD = 'O'   THEN 'Y' 
			WHEN SO.TRSP_BND_CD IS NULL 
			THEN DECODE(SIGN(TRUNC(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[sofficeCd]) - LOCL_CRE_DT )-3), -1, 'Y', 0, 'Y', 'N') 
		END VERIFY_YN 
	FROM 
	   TRS_TRSP_RAIL_BIL_ORD SO 
	WHERE (TRSP_BND_CD = 'O' OR TRSP_BND_CD IS NULL) 
	AND   NVL(DELT_FLG, 'N') = 'N' 

#if(${eqNoVerify} != "")
		AND EQ_NO IN (
#foreach($key IN ${eqNoVerify})	
#if($velocityCount == 1)		
	'$key'
#else
	, '$key'
#end
#end
		)
#end     
		
	AND LOCL_CRE_DT BETWEEN (GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[sofficeCd]) - 15) AND GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[sofficeCd]) 
	UNION ALL 
	SELECT 
		EQ_NO, 
		SUBSTR(  TO_CHAR(LOCL_CRE_DT, 'YYYYMMDD')|| 'COMPANY - CNTR_NO: ' 
		||EQ_NO||' S/O created( '||TO_CHAR(LOCL_CRE_DT, 'YYYYMMDD') 
		||DECODE(TRSP_BND_CD,'O', ' : Full )', ' : Mty )'||' ROUTE : ' 
		||FM_NOD_CD||'-->'||TO_NOD_CD),9,100) VERIFY_RESULT, 
		'Y' VERIFY_YN 
	FROM 
	   TRS_TRSP_RAIL_BIL_ORD SO 
	WHERE SO.TRSP_BND_CD = 'I' 
	AND   NVL(DELT_FLG, 'N') = 'N' 
	
#if(${eqNoVerify} != "")
		AND EQ_NO IN (
#foreach($key IN ${eqNoVerify})	
#if($velocityCount == 1)		
	'$key'
#else
	, '$key'
#end
#end
		)
#end     
		
	AND LOCL_CRE_DT BETWEEN (GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[sofficeCd]) - 5) AND GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[sofficeCd]) 

) 
GROUP BY 
    EQ_NO			]]></sql>
			<params>
				<param name="sofficeCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
