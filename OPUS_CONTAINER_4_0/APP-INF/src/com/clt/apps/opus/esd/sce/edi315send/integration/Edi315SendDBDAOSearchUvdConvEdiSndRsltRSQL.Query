<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchUvdConvEdiSndRsltRSQL">
			<desc><![CDATA[Edi315SendDBDAOSearchUvdConvEdiSndRsltRSQL]]></desc>
			<sql><![CDATA[
SELECT count(*) uvd_conv_cnt  
    FROM SCE_EDI_SND_RSLT   
    WHERE 1=1   
    AND   EDI_GRP_CD     = @[v_edi_group_cd]  
    AND   EDI_STS_CD = 'UVD'  
    AND   NOD_CD         = @[v_org_yd_cd]
    AND   BKG_NO         = @[v_bkg_no]  
    and   CNTR_NO        = @[v_cntr_no]  
			]]></sql>
			<params>
				<param name="v_edi_group_cd" type="12" value="" out="N"/>
				<param name="v_org_yd_cd" type="12" value="" out="N"/>
				<param name="v_bkg_no" type="12" value="" out="N"/>
				<param name="v_cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
