<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OtherSOManageDBDAOAddTrsSvcConvAmtUsdCSQL">
			<desc><![CDATA[USD 통화변환 금액을 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_EXPN_CONV_AMT (												
 	TRSP_SO_OFC_CTY_CD								,								
 	TRSP_SO_SEQ										,								
 	CURR_CD											,								
 	BZC_AMT											,								
 	FUEL_SCG_AMT									,								
 	OVR_WGT_SCG_AMT									,								
 	ETC_ADD_AMT										,								
 	CRE_OFC_CD										,								
 	CRE_USR_ID										,								
 	CRE_DT											,								
 	UPD_USR_ID										,								
 	UPD_DT																			
 )																				
 VALUES																			
 (																				
 @[TRSP_SO_OFC_CTY_CD]								,								
 @[TRSP_SO_SEQ]										,								
 'EUR'												,								
 (																				
 	SELECT																			
 	ROUND(( TO_NUMBER(@[BZC_AMT]) / USD.USD_LOCL_XCH_RT)*EUR.USD_LOCL_XCH_RT,2) BZC_EUR		
 	FROM   GL_MON_XCH_RT USD, GL_MON_XCH_RT EUR										
 	WHERE  USD.CURR_CD								= @[CURR_CD]								
 	AND    EUR.CURR_CD								= 'EUR'							
 	AND    USD.ACCT_XCH_RT_LVL						= '1'							
 	AND    USD.ACCT_XCH_RT_LVL						= EUR.ACCT_XCH_RT_LVL			
 	AND    USD.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 	AND    USD.ACCT_XCH_RT_YRMON					= EUR.ACCT_XCH_RT_YRMON			
 )												,								
 (																				
 	SELECT 																			
 	ROUND((TO_NUMBER(@[FUEL_SCG_AMT]) / USD.USD_LOCL_XCH_RT)*EUR.USD_LOCL_XCH_RT,2) FUEL_EUR		
 	FROM   GL_MON_XCH_RT USD, GL_MON_XCH_RT EUR										
 	WHERE  USD.CURR_CD								= @[CURR_CD]
 	AND    EUR.CURR_CD								= 'EUR'							
 	AND    USD.ACCT_XCH_RT_LVL						= '1'							
 	AND    USD.ACCT_XCH_RT_LVL						= EUR.ACCT_XCH_RT_LVL			
 	AND    USD.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 	AND    USD.ACCT_XCH_RT_YRMON					= EUR.ACCT_XCH_RT_YRMON			
 )												,								
 (																				
 	SELECT 																			
 	ROUND((TO_NUMBER(@[OVR_WGT_SCG_AMT]) / USD.USD_LOCL_XCH_RT)*EUR.USD_LOCL_XCH_RT,2) OVR_EUR		
 	FROM   GL_MON_XCH_RT USD, GL_MON_XCH_RT EUR										
 	WHERE  USD.CURR_CD								= @[CURR_CD]
 	AND    EUR.CURR_CD								= 'EUR'							
 	AND    USD.ACCT_XCH_RT_LVL						= '1'							
 	AND    USD.ACCT_XCH_RT_LVL						= EUR.ACCT_XCH_RT_LVL			
 	AND    USD.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 	AND    USD.ACCT_XCH_RT_YRMON					= EUR.ACCT_XCH_RT_YRMON			
 )												,								
 (																				
 	SELECT																			
 	ROUND((TO_NUMBER(@[ETC_ADD_AMT]) / USD.USD_LOCL_XCH_RT)*EUR.USD_LOCL_XCH_RT,2) ETC_EUR		
 	FROM   GL_MON_XCH_RT USD, GL_MON_XCH_RT EUR										
 	WHERE  USD.CURR_CD								= @[CURR_CD]
 	AND    EUR.CURR_CD								= 'EUR'							
 	AND    USD.ACCT_XCH_RT_LVL						= '1'							
 	AND    USD.ACCT_XCH_RT_LVL						= EUR.ACCT_XCH_RT_LVL			
 	AND    USD.ACCT_XCH_RT_YRMON					= TO_CHAR(SYSDATE,'YYYYMM')		
 	AND    USD.ACCT_XCH_RT_YRMON					= EUR.ACCT_XCH_RT_YRMON			
 )												,								
 @[FORM_USR_OFC_CD]								,								
 @[FORM_CRE_USR_ID]								,								
 GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC( @[FORM_USR_OFC_CD]),												
 @[FORM_CRE_USR_ID]								,								
 GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC( @[FORM_USR_OFC_CD])												
 )			]]></sql>
			<params>
				<param name="TRSP_SO_OFC_CTY_CD" type="12" value="" out="N"/>
				<param name="TRSP_SO_SEQ" type="12" value="" out="N"/>
				<param name="BZC_AMT" type="12" value="" out="N"/>
				<param name="CURR_CD" type="12" value="" out="N"/>
				<param name="FUEL_SCG_AMT" type="12" value="" out="N"/>
				<param name="OVR_WGT_SCG_AMT" type="12" value="" out="N"/>
				<param name="ETC_ADD_AMT" type="12" value="" out="N"/>
				<param name="FORM_USR_OFC_CD" type="12" value="" out="N"/>
				<param name="FORM_CRE_USR_ID" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
