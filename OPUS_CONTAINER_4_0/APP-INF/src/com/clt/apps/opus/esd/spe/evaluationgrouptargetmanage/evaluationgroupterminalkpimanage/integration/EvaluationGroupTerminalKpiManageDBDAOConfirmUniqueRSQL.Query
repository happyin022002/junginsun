<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupTerminalKpiManageDBDAOConfirmUniqueRSQL">
			<desc><![CDATA[기 등록된 항목이 있는지 체크]]></desc>
			<sql><![CDATA[
SELECT EG_ID,EG_ID_SEQ,SP_KPI_CD,EV_YR,YD_CD
FROM  SPE_EV_GRP_TML_KPI_TGT_RTO
WHERE EG_ID = @[eg_id]
AND EG_ID_SEQ = TO_NUMBER(@[eg_id_seq])
AND EV_YR = @[ev_yr]
AND YD_CD = UPPER(@[yd_cd])
AND VNDR_SEQ = TO_NUMBER(@[vndr_seq])			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="eg_id_seq" type="12" value="" out="N"/>
				<param name="ev_yr" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
