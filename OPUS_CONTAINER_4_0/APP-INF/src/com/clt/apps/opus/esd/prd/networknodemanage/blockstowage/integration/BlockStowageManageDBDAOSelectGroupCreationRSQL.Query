<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlockStowageManageDBDAOSelectGroupCreationRSQL">
			<desc><![CDATA[SelectGroupCreation]]></desc>
			<sql><![CDATA[
SELECT
	DECODE(delt_flg, 'Y', '1', '0') del_flag
	, port_cd pod_code
	, hub_loc_cd hub_code
	, vsl_slan_cd lane_code
	, blck_stwg_cd group_code
	, cre_usr_id c_user_id
	, to_char(cre_dt, 'yyyy-mm-dd') c_date
	, upd_usr_id u_user_id
	, to_char(upd_dt, 'yyyy-mm-dd') u_date
FROM prd_blck_stwg
WHERE
	port_cd LIKE @[pod_code] || '%'
	AND hub_loc_cd LIKE @[hub_code] || '%'
	AND vsl_slan_cd LIKE @[lane_code] || '%'
	AND blck_stwg_cd LIKE @[group_code] || '%'
	AND delt_flg LIKE DECODE(@[del_flag], 'A', '%', @[del_flag])			]]></sql>
			<params>
				<param name="pod_code" type="12" value="" out="N"/>
				<param name="hub_code" type="12" value="" out="N"/>
				<param name="lane_code" type="12" value="" out="N"/>
				<param name="group_code" type="12" value="" out="N"/>
				<param name="del_flag" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
