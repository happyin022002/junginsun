<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOcheckCntCdRSQL">
			<desc><![CDATA[POL,POD가 CNT_CD ='CN' 인지 check]]></desc>
			<sql><![CDATA[
SELECT COUNT(0)
FROM MDM_LOCATION
WHERE 1=1
AND LOC_CD = @[search_cd]
AND CNT_CD = 'CN'			]]></sql>
			<params>
				<param name="search_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
