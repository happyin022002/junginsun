<sqls>
	<querys>
		<query name="LaneServiceDBDAOModifyPortPairUUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE SCE_PORT_PAIR_DTL
   SET VSL_SLAN_CD = @[vsl_slan_cd],
	   CUST_TRD_PRNR_ID = @[cust_trd_prnr_id],
	   UPD_USR_ID = @[upd_usr_id],
	   UPD_DT = SYSDATE
 WHERE ROUT_RCV_DT = @[rout_rcv_dt]
   AND ROUT_SEQ = @[rout_seq]			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rout_rcv_dt" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
