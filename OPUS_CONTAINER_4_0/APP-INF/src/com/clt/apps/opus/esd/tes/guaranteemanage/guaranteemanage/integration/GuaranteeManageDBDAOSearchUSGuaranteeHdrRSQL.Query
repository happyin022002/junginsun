<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeManageDBDAOSearchUSGuaranteeHdrRSQL">
			<desc><![CDATA[Guarantee Header정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
		  GNTE_NO
		, OFC_CD
		, GNTE_TP_CD
		, CURR_CD
		, LPAD(VNDR_SEQ, 6, '0') VNDR_SEQ
		, (	SELECT VNDR_LGL_ENG_NM 
			FROM MDM_VENDOR
			WHERE VNDR_SEQ = GH.VNDR_SEQ) VNDR_SEQ_NAME
		, GNTE_CUST_CD
		, (	SELECT CUST_LGL_ENG_NM 
			FROM MDM_CUSTOMER 
			WHERE CUST_CNT_CD = SUBSTR(GH.GNTE_CUST_CD, 0, 2)
			AND CUST_SEQ = SUBSTR(GH.GNTE_CUST_CD, 3, 6) ) GNTE_CUST_CD_NAME
		, BKG_STS_CD
        , YD_CD
		, (	SELECT YD_NM  
			FROM MDM_YARD
			WHERE YD_CD = GH.YD_CD ) YD_NM
		, TTL_AMT
		, PIC_PHN_NO
		, PIC_FAX_NO
		, DEPT_NO
		, GNTE_RMK
		, NVL(DELT_FLG, 'N') DELT_FLG
		, CRE_USR_ID
		, TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC((SELECT OFC_CD FROM TABLE (COM_OfficeCodeMgr_PKG.COM_getOfficeCodeList_FNC('000001', 'TES'))), CRE_DT, OFC_CD), 'YYYY-MM-DD') CRE_DT
		, NVL(GH.DMY_FLG,'N') AS DMY_FLG
FROM	TES_GNTE_HDR GH
WHERE	1	= 1
AND		NVL(GH.DMY_FLG,'N') <> 'Y'
AND		GH.GNTE_NO = @[gnte_no]			]]></sql>
			<params>
				<param name="gnte_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
