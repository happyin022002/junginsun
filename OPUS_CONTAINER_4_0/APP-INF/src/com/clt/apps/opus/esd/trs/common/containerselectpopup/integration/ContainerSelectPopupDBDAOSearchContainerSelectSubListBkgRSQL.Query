<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSelectPopupDBDAOSearchContainerSelectSubListBkgRSQL">
			<desc><![CDATA[Booking, Container 정보 조회]]></desc>
			<sql><![CDATA[
-- NOT SPLIT
SELECT         
     A.BKG_NO         
 ,   A.BKG_STS_CD          
 ,   B.CNTR_NO      AS EQ_NO
 ,   B.CNTR_TPSZ_CD AS EQ_TPSZ_CD      
 ,   ''             AS DUP_CHECK
 ,   ''             AS APPLY_SO_SEQ
 ,   ''             AS ORG_APPLY_SO_SEQ
 FROM
     BKG_BOOKING      A       
 ,   BKG_CONTAINER    B       
 ,   TRS_TRSP_SVC_ORD D       
 WHERE 1=1
#if($bkgNo.size() > 0)
AND A.BKG_NO IN (
	#foreach($code IN ${bkgNo})
		#if($velocityCount == 1)
			'$code'
		#else
			,'$code'
		#end
	#end
	)
#end
 AND A.BKG_NO       = B.BKG_NO         (+)       
 AND 'X'           <> A.BKG_STS_CD     (+)       
 AND B.BKG_NO       = D.BKG_NO         (+)       
 AND B.CNTR_NO      = D.EQ_NO          (+)       
 AND 'N'= D.DELT_FLG        (+)       
 AND 'OD'          = SUBSTR(D.COST_ACT_GRP_CD(+),1,2)
UNION -- IF SPLIT CASE, MAIN BKG
SELECT         
     A.BKG_NO         
 ,   A.BKG_STS_CD          
 ,   B.CNTR_NO      AS EQ_NO
 ,   B.CNTR_TPSZ_CD AS EQ_TPSZ_CD      
 ,   ''             AS DUP_CHECK
 ,   ''             AS APPLY_SO_SEQ
 ,   ''             AS ORG_APPLY_SO_SEQ
 FROM
     BKG_BOOKING      A    
 ,   BKG_CONTAINER    B       
 ,   TRS_TRSP_SVC_ORD D       
 WHERE 1=1
#if($bkgNo.size() > 0)
AND A.FM_BKG_NO IN (
	#foreach($code IN ${bkgNo})
		#if($velocityCount == 1)
			'$code'
		#else
			,'$code'
		#end
	#end
	)
#end         
 AND A.BKG_CRE_TP_CD = 'S'
 AND A.BKG_NO        = B.BKG_NO         (+)       
 AND 'X'<> A.BKG_STS_CD     (+)       
 AND B.BKG_NO        = D.BKG_NO         (+)       
 AND B.CNTR_NO       = D.EQ_NO(+)       
 AND 'N'= D.DELT_FLG        (+)       
 AND 'OD'            = SUBSTR(D.COST_ACT_GRP_CD(+),1,2)
UNION -- IF SPLIT CASE, SPLIT BKG
SELECT         
     A.BKG_NO         
 ,   A.BKG_STS_CD          
 ,   B.CNTR_NO      AS EQ_NO
 ,   B.CNTR_TPSZ_CD AS EQ_TPSZ_CD      
 ,   ''             AS DUP_CHECK
 ,   ''             AS APPLY_SO_SEQ
 ,   ''             AS ORG_APPLY_SO_SEQ
 FROM
     BKG_BOOKING      A
 ,   BKG_BOOKING      C     
 ,   BKG_CONTAINER    B       
 ,   TRS_TRSP_SVC_ORD D       
 WHERE 1=1
#if($bkgNo.size() > 0)
AND C.BKG_NO IN (
	#foreach($code IN ${bkgNo})
		#if($velocityCount == 1)
			'$code'
		#else
			,'$code'
		#end
	#end
	)
#end         
 AND C.BKG_CRE_TP_CD = 'S'
 AND C.FM_BKG_NO     = A.FM_BKG_NO
 AND A.BKG_NO        = B.BKG_NO         (+)       
 AND 'X'<> A.BKG_STS_CD     (+)       
 AND B.BKG_NO        = D.BKG_NO         (+)       
 AND B.CNTR_NO       = D.EQ_NO(+)       
 AND 'N'= D.DELT_FLG        (+)       
 AND 'OD'            = SUBSTR(D.COST_ACT_GRP_CD(+),1,2)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
