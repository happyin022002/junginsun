<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOModifyOfficeMTTransportationVoUSQL">
			<desc><![CDATA[MT Office Transfer update]]></desc>
			<sql><![CDATA[
UPDATE EQR_REPO_EXE_SO_IF
       SET TRNS_RQST_OFC_CD = @[ctrl_ofc_cd],
       TRNS_RQST_USR_ID     = @[upd_usr_id],
       TRNS_RQST_RSN        = @[new_trns_rqst_rsn],
       EQ_CTRL_OFC_CD       = @[new_trns_rqst_ofc_cd],
       UPD_DT               = SYSDATE,
       UPD_USR_ID           = @[upd_usr_id]
WHERE  REPO_PLN_ID          = @[repo_pln_id]
AND    PLN_YRWK  = @[pln_yrwk]
AND    REF_ID    = @[ref_id]
AND    REF_SEQ   = @[ref_seq]			]]></sql>
			<params>
				<param name="ctrl_ofc_cd" type="12" value="1" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="new_trns_rqst_rsn" type="12" value="1" out="N"/>
				<param name="new_trns_rqst_ofc_cd" type="12" value="1" out="N"/>
				<param name="repo_pln_id" type="12" value="1" out="N"/>
				<param name="pln_yrwk" type="12" value="1" out="N"/>
				<param name="ref_id" type="12" value="1" out="N"/>
				<param name="ref_seq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
