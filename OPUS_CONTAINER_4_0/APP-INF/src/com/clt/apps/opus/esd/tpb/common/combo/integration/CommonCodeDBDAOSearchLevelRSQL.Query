<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchLevelRSQL">
			<desc><![CDATA[SearchLevel]]></desc>
			<sql><![CDATA[
SELECT LVL
      ,OFC_CD
  FROM (
          SELECT OFC_CD, 'H' AS LVL FROM MDM_OFC_GRP_MAPG WHERE OFC_GRP_ID = '000003' AND SUB_SYS_CD='TPB'
          UNION
          SELECT OFC_CD, 'R' AS LVL FROM MDM_OFC_GRP_MAPG WHERE OFC_GRP_ID = '000004' AND SUB_SYS_CD='COM'
       )
 WHERE OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
