<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchContiCdRSQL">
			<desc><![CDATA[contiCd 목록을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT loc.conti_cd
  FROM trs_trsp_svc_ord so
	  ,mdm_location loc
 WHERE SUBSTR(so.fm_nod_cd,1,5) = loc.loc_cd
   AND so.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
   AND so.trsp_so_seq = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
