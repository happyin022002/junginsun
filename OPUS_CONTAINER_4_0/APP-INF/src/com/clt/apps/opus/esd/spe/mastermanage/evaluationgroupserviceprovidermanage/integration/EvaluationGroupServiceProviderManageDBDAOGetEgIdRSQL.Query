<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupServiceProviderManageDBDAOGetEgIdRSQL">
			<desc><![CDATA[EG_ID 쿼리]]></desc>
			<sql><![CDATA[
SELECT EG_ID||TO_CHAR(EG_ID_SEQ ,'000')EG_ID
FROM  SPE_EV_GRP
WHERE EG_RHQ_CD   = @[eg_rhq_cd]
 AND EG_CTY_CD    = UPPER(@[eg_cty_cd])
 AND SVC_CATE_CD  = @[svc_cate_cd]			]]></sql>
			<params>
				<param name="eg_rhq_cd" type="12" value="" out="N"/>
				<param name="eg_cty_cd" type="12" value="" out="N"/>
				<param name="svc_cate_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
