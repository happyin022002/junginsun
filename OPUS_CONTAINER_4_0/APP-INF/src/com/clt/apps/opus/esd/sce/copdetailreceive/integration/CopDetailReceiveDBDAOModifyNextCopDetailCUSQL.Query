<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyNextCopDetailCUSQL">
			<desc><![CDATA[ModifyNextCopDetailC]]></desc>
			<sql><![CDATA[
update SCE_COP_DTL 																									
SET 	 ACT_STS_CD  	= 'C'
WHERE COP_NO = @[cop_no] 
AND COP_DTL_SEQ = (SELECT MIN(COP_DTL_SEQ) FROM SCE_COP_DTL WHERE COP_NO = @[cop_no] AND COP_DTL_SEQ = TO_NUMBER(@[cop_dtl_seq]))
			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
