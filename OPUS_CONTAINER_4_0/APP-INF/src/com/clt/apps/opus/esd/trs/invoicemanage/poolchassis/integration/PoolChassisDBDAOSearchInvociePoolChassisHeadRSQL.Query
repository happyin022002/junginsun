<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisDBDAOSearchInvociePoolChassisHeadRSQL">
			<desc><![CDATA[Inovice Pool chassis의 Invoice Work  정보를 조회]]></desc>
			<sql><![CDATA[
SELECT  INV_NO
	    ,INV_VNDR_SEQ
	    ,TRSP_INV_AUD_STS_CD
	    ,INV_CURR_CD
	    ,INV_BZC_AMT
	    ,INV_TTL_AMT
	    ,TO_CHAR(INV_RCV_DT, 'YYYY-MM-DD')INV_RCV_DT
	    ,TO_CHAR(INV_ISS_DT, 'YYYY-MM-DD')INV_ISS_DT
	    ,INV_VAT_AMT
	    ,POOL_CHSS_COST_YRMON
	    ,CHSS_POOL_CD
	    ,DELT_FLG
	    ,CRE_OFC_CD
	    ,CRE_USR_ID
	    ,TO_CHAR(CRE_DT, 'YYYYMMDD') CRE_DT
	    ,RGST_NO
	    ,(SELECT CHSS_POOL_NM FROM TRS_TRSP_CHSS_POOL WHERE CHSS_POOL_CD = W.CHSS_POOL_CD AND NVL(DELT_FLG, 'N') <> 'Y') AS CHSS_POOL_NM
FROM TRS_TRSP_INV_WRK W
WHERE INV_NO = @[inv_no]
AND INV_VNDR_SEQ = @[paymt_sp_cd]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="paymt_sp_cd" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
