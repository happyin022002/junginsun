<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOSearch06RailSoManageUSQL">
			<desc><![CDATA[US Rail O/B SO 대상 수정 SQL]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_RAIL_BIL_ORD_TMP X
	SET (X.CUST_CNT_CD, X.CUST_SEQ, X.SHPR_CUST_NM) = 
                   (SELECT MAX(U.CUST_CNT_CD),
                           MAX(U.CUST_SEQ),
                           MIN(REPLACE(REPLACE(U.CUST_NM, CHR (13),' '), CHR (10), ' '))
                    FROM
			BKG_CUSTOMER U
                    WHERE U.BKG_NO = X.BKG_NO
                    AND   U.BKG_CUST_TP_CD = 'S')
	    ,(X.CNEE_CUST_NM) = 
                   (SELECT MIN(REPLACE(REPLACE(U.CUST_NM, CHR (13),' '), CHR (10), ' '))
                    FROM 
			BKG_CUSTOMER U
                    WHERE U.BKG_NO = X.BKG_NO
                    AND   U.BKG_CUST_TP_CD = 'C')
WHERE X.TRSP_RAIL_TMP_SEQ = @[trsp_rail_tmp_seq]			]]></sql>
			<params>
				<param name="trsp_rail_tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
