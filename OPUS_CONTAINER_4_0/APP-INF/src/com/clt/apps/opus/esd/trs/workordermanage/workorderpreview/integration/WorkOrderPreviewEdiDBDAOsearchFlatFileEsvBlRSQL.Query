<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewEdiDBDAOsearchFlatFileEsvBlRSQL">
			<desc><![CDATA[NYK-Hawk_FFile(ESV_N)-JOEDI_EU_OUTBOUND
{BL_NUMBER_LIST]]></desc>
			<sql><![CDATA[
#if(${f_type} == 'L')
SELECT SO.BL_NO
  FROM TRS_TRSP_SVC_ORD SO
 WHERE SO.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND SO.TRSP_SO_SEQ        = @[trsp_so_seq]
#elseif(${f_type} == 'D')
SELECT '' AS BL_NO
  FROM DUAL
#end			]]></sql>
			<params>
