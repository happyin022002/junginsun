<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOModifyProvCntrByWRSUSQL">
			<desc><![CDATA[WRS (Web Railbilling System) 에서 prov_Cntr_no, prov_cntr_tpsz_cd 를 update 하기 위해 호출한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_COP_HDR SET	          
		PROV_CNTR_NO       =  @[prov_cntr_no],   
		PROV_CNTR_TPSZ_CD  =  @[prov_cntr_tpsz_cd],
		UPD_DT	= sysdate
WHERE COP_NO             = @[cop_no]			]]></sql>
			<params>
				<param name="prov_cntr_no" type="12" value="" out="N"/>
				<param name="prov_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
