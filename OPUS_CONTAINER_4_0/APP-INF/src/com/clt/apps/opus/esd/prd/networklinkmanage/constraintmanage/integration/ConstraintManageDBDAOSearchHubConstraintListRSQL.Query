<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOSearchHubConstraintListRSQL">
			<desc><![CDATA[ConstraintManageDBDAOSearchHubConstraintList]]></desc>
			<sql><![CDATA[
SELECT H.PORT_CD
      ,H.HUB_LOC_CD
      ,H.NOD_CD
      ,H.IO_BND_CD
      ,H.CNST_LANE_CD
      ,H.SPCL_CGO_CNTR_TP_CD
      ,H.SVC_USE_FLG
      ,H.HUB_LOC_CNST_RMK
      ,U.OFC_CD AS CRE_OFC_CD
      ,H.CRE_USR_ID
      ,TO_CHAR(H.CRE_DT, 'YYYY-MM-DD') AS CRE_DT
      ,H.UPD_USR_ID
      ,TO_CHAR(H.UPD_DT, 'YYYY-MM-DD') AS UPD_DT
      ,H.DIR_CD
      ,H.CNTR_TP_CD
      ,H.CNTR_SZ_CD
  FROM PRD_HUB_LOC_CNST_MGMT H
      ,COM_USER              U
 WHERE NVL2(@[hport_cd], H.PORT_CD, 'XX') = NVL(@[hport_cd], 'XX')
   AND NVL2(@[hhub_loc_cd], H.HUB_LOC_CD, 'XX') = NVL(@[hhub_loc_cd], 'XX')
   AND NVL2(@[hnod_cd], H.NOD_CD, 'XX') = NVL(@[hnod_cd], 'XX')
   AND H.CRE_USR_ID = U.USR_ID(+)			]]></sql>
			<params>
				<param name="hport_cd" type="12" value="" out="N"/>
				<param name="hhub_loc_cd" type="12" value="" out="N"/>
				<param name="hnod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
