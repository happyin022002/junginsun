<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOUpdateSceCopDtlActualDateByTrsUSQL">
			<desc><![CDATA[Edi315SendDBDAOUpdateSceCopDtlActualDateByTrs]]></desc>
			<sql><![CDATA[
UPDATE SCE_COP_DTL T
   SET ACT_DT       = TO_DATE(@[act_dt], 'YYYYMMDDHH24MISS')
      ,T.UPD_USR_ID = @[upd_usr_id]
	  ,T.ACT_RCV_TP_CD = '10' -- CD00141 
      ,T.UPD_DT     = SYSDATE
 WHERE T.COP_NO = @[cop_no]
   AND T.ACT_CD = DECODE(@[bnd_cd], 'I', 'FITZAD', 'O', 'MOTZAD')			]]></sql>
			<params>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
