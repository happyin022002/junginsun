<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewEdiDBDAOsearchFlatFileEsvAwkardMeasureRSQL">
			<desc><![CDATA[NYK-Hawk_FFile(ESV_N)-JOEDI_EU_OUTBOUND
{AWKARD_MEASUREMENT]]></desc>
			<sql><![CDATA[
SELECT T1.*
  FROM (
	WITH A AS
	 (SELECT LEVEL LEV
			,DECODE(LEVEL, 1, 'OverLengthFront', 2, 'OverLengthBack', 3, 'OverWidthLeft', 4, 'OverWidthRight', 5, 'OverHeight', 6, 'OverWeight') COL
		FROM DUAL
	  CONNECT BY LEVEL < 7)
	SELECT A.COL AS AK_TYPE
		  ,DECODE(A.LEV, 1, UNIT_CD, 2, UNIT_CD, 3, UNIT_CD, 4, UNIT_CD, 5, UNIT_CD, 6, '') AS AK_UNIT
		  ,DECODE(A.LEV, 1, OVR_FWRD_LEN, 2, OVR_BKWD_LEN, 3, OVR_LF_LEN, 4, OVR_RT_LEN, 5, OVR_HGT, 6, OVR_WGHT) AS AK_QTY
	  FROM (SELECT ROUND(B.OVR_FWRD_LEN / 100, 3) OVR_FWRD_LEN
				  ,ROUND(B.OVR_BKWD_LEN / 100, 3) OVR_BKWD_LEN
				  ,ROUND(B.OVR_LF_LEN / 100, 3) OVR_LF_LEN
				  ,ROUND(B.OVR_RT_LEN / 100, 3) OVR_RT_LEN
				  ,ROUND(B.OVR_HGT / 100, 3) OVR_HGT
				  ,'' OVR_WGHT
				  ,'M' UNIT_CD
			  FROM BKG_AWK_CGO B
			 WHERE B.BKG_NO = @[bkg_no]
			   AND B.AWK_CGO_SEQ = @[awk_cgo_seq])
		  ,A
 ) T1
 RIGHT OUTER JOIN DUAL
    ON 1 = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="awk_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
