<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetAVEventPodCheckRSQL">
			<desc><![CDATA[GetAVEventPodCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(D XPKSCE_COP_DTL) */
       SUBSTR(D.NOD_CD,1, 5) CURR_EVENT_YARD
  FROM SCE_COP_DTL D
 WHERE 1=1
   AND D.COP_NO = @[cop_no]
   AND D.ACT_CD LIKE 'FI%DO'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
