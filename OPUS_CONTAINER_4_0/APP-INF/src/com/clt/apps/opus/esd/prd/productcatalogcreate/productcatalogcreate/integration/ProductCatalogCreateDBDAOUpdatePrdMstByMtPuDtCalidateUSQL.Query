<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProductCatalogCreateDBDAOUpdatePrdMstByMtPuDtCalidateUSQL">
			<desc><![CDATA[UpdatePrdMstByMtPuDtCalidate]]></desc>
			<sql><![CDATA[
UPDATE PRD_PROD_CTL_MST M
SET PCTL_NO = 'X'|| SUBSTR(PCTL_NO,2)
WHERE NOT EXISTS
(
    SELECT 'X' FROM PRD_PROD_CTL_ROUT_DTL D
    WHERE M.PCTL_NO = D.PCTL_NO
)
AND PCTL_NO = @[pctl_no]			]]></sql>
			<params>
				<param name="pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
