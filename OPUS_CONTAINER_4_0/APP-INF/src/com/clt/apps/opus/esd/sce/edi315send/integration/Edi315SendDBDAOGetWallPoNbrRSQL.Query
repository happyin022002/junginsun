<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetWallPoNbrRSQL">
			<desc><![CDATA[for getWallPoNbr]]></desc>
			<sql><![CDATA[
SELECT   DISTINCT REPLACE(PO_NO, CHR(13)||CHR(10) , '') wal_po_nbr  
FROM   BKG_CNTR_MF_DESC                          
WHERE   1=1
	and BKG_NO       = @[e_bkg_no]         
	and CNTR_NO      = @[e_cntr_no] 
	and PO_NO IS NOT NULL			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
				<param name="e_cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
