<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchWorkOrderPreviewEdiCondChkRSQL">
			<desc><![CDATA[Work Order Preview EDI 가능 조건 체크]]></desc>
			<sql><![CDATA[
SELECT EDI_CHK_SEQ AS WO_EDI_FLG
FROM TRS_TRSP_WRK_ORD_EDI
WHERE VNDR_SEQ = @[vndr_seq]
AND TRSP_COST_DTL_MOD_CD IS NULL
AND FM_NOD_CD IS NULL
AND TO_NOD_CD IS NULL
AND CONTI_CD IN ('A','E')

UNION

SELECT EDI_CHK_SEQ AS WO_EDI_FLG
FROM TRS_TRSP_WRK_ORD_EDI
WHERE VNDR_SEQ = @[vndr_seq]
AND TRSP_COST_DTL_MOD_CD = @[trsp_cost_dtl_mod_cd]
AND FM_NOD_CD IS NULL
AND TO_NOD_CD IS NULL
AND CONTI_CD IN ('A','E')

UNION

SELECT EDI_CHK_SEQ AS WO_EDI_FLG
FROM TRS_TRSP_WRK_ORD_EDI
WHERE VNDR_SEQ = @[vndr_seq]
AND TRSP_COST_DTL_MOD_CD = @[trsp_cost_dtl_mod_cd]
AND FM_NOD_CD = @[fm_nod_cd]
AND TO_NOD_CD = @[to_nod_cd]
AND CONTI_CD IN ('A','E')

UNION

SELECT EDI_CHK_SEQ AS WO_EDI_FLG
FROM TRS_TRSP_WRK_ORD_EDI
WHERE VNDR_SEQ IS NULL 
AND TRSP_COST_DTL_MOD_CD = @[trsp_cost_dtl_mod_cd]
AND FM_NOD_CD IS NULL
AND TO_NOD_CD IS NULL
AND CONTI_CD = @[conti_cd]
			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="trsp_cost_dtl_mod_cd" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="conti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
