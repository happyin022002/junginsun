<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupManageDBDAOConfirmLevelsRSQL">
			<desc><![CDATA[ConfirmLevels]]></desc>
			<sql><![CDATA[
SELECT *																	
FROM  SPE_EV_GRP                                            				
WHERE EG_RHQ_CD =@[eg_rhq_cd]                                                        
AND EG_CTY_CD = @[eg_cty_cd]                                                           
AND SVC_CATE_CD = DECODE(@[svc_cate_cd],'Truck','TR', 'Rail','RL',	 'ODCY','CY',		 'Terminal','TM',	 'Water','WT', 'EQ M&R','EQ')									
			]]></sql>
			<params>
				<param name="eg_rhq_cd" type="12" value="" out="N"/>
				<param name="eg_cty_cd" type="12" value="" out="N"/>
				<param name="svc_cate_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
