<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanLinkBackEndDBDAOOceanLinkUpdaeManualPfTzTmUSQL">
			<desc><![CDATA[정보 수정시, prd_pf_tz_tm에 해당 정보를 업데이트한다.]]></desc>
			<sql><![CDATA[
UPDATE prd_pf_tz_TM  
   SET FM_PORT_ETB_DY_CD= @[poletb] , 
       FM_PORT_ETD_DY_CD= @[poletd] , 
       TO_PORT_ETB_DY_CD= @[podetb] , 
       TO_PORT_ETD_DY_CD= @[podetd] , 
       TZTM_HRS= TO_NUMBER(@[fmt_tztm_hrs]) , 
       UPD_OFC_CD =@[cre_ofc_cd] , 
       UPD_USR_ID =@[upd_usr_id] , 
       UPD_DT = sysdate ,
       LNK_RMK = case when nvl(DELT_FLG,'N') = 'N' THEN @[lnk_rmk]
                      else @[upd_usr_id]||' has created on '||TO_CHAR(SYSDATE,'Mon-DD,YYYY', 'NLS_DATE_LANGUAGE=ENGLISH')||' : '|| @[lnk_rmk]
                 END ,
       DELT_FLG = 'N'
WHERE VSL_SLAN_CD= @[lanecd]  
AND FM_PORT_CD= @[polprot]  
AND TO_PORT_CD= @[podprot]
AND SKD_DIR_CD= @[dircd]			]]></sql>
			<params>
				<param name="poletb" type="12" value="" out="N"/>
				<param name="poletd" type="12" value="" out="N"/>
				<param name="podetb" type="12" value="" out="N"/>
				<param name="podetd" type="12" value="" out="N"/>
				<param name="fmt_tztm_hrs" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="lnk_rmk" type="12" value="" out="N"/>
				<param name="lanecd" type="12" value="" out="N"/>
				<param name="polprot" type="12" value="" out="N"/>
				<param name="podprot" type="12" value="" out="N"/>
				<param name="dircd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
