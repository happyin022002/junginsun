<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchItmUpdDtRSQL">
			<desc><![CDATA[item 테이블의 update date 취득]]></desc>
			<sql><![CDATA[
SELECT
    TO_CHAR (UPD_DT, 'YYYYMMDDHH24MISS') UPD_DT
FROM
    GEM_ITEM
WHERE
    GEN_EXPN_RQST_NO         = @[gen_expn_rqst_no]
    AND GEN_EXPN_RQST_SEQ    = @[gen_expn_rqst_seq]
    AND GEN_EXPN_TRNS_DIV_CD = 'F'			]]></sql>
			<params>
				<param name="gen_expn_rqst_no" type="12" value="" out="N"/>
				<param name="gen_expn_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
