<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InsuranceRecoveryDBDAOSearchInsuranceExistRSQL">
			<desc><![CDATA[Insurance 입력/수정 결정]]></desc>
			<sql><![CDATA[
SELECT
    'Y' AS CHK_EXIST
FROM
    DUAL
WHERE
    EXISTS
    (
        SELECT
            CGO_CLM_NO
        FROM
            CNI_CGO_CLM_INSUR
        WHERE
            CGO_CLM_NO = @[cgo_clm_no]
    )			]]></sql>
			<params>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
