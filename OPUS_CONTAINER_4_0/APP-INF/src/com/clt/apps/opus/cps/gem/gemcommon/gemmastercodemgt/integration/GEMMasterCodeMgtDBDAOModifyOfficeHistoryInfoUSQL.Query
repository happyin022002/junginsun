<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOModifyOfficeHistoryInfoUSQL">
			<desc><![CDATA[Office code history 수정]]></desc>
			<sql><![CDATA[
UPDATE GEM_OFC_HIS 
   set EXP_DT = @[exp_dt]
      ,BFR_OFC_CD = @[bfr_ofc_cd]
      ,BFR_CTR_CD = @[bfr_ctr_cd]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT     = sysdate
WHERE  OFC_CD = @[ofc_cd]
AND    OFC_HIS_SEQ = @[ofc_his_seq]			]]></sql>
			<params>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="bfr_ofc_cd" type="12" value="" out="N"/>
				<param name="bfr_ctr_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="ofc_his_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
