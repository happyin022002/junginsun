<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InsuranceDBDAOCustomPremiumInstallmentVOCSQL">
			<desc><![CDATA[Premium Installment 입력]]></desc>
			<sql><![CDATA[
INSERT INTO CNI_INSUR_PRM_INST (
	INSUR_TP_CD
,	INSUR_PLCY_YR
,   INSUR_CLM_PTY_NO
,	INSUR_PRM_TP_CD
,	INST_SEQ
,	INST_LOCL_AMT
,	INST_CURR_CD
,	INST_XCH_RT
,	INST_USD_AMT
,	INST_DUE_DT
,	INST_PAY_DT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[insur_tp_cd]
,	@[insur_plcy_yr]
,   @[insur_clm_pty_no]
,	@[insur_prm_tp_cd]
,	@[inst_seq]
,	REPLACE(@[inst_locl_amt],',')
,	@[inst_curr_cd]
,	REPLACE(@[inst_xch_rt],',')
,	REPLACE(@[inst_usd_amt],',')
,	REPLACE(@[inst_due_dt],'-')
,	REPLACE(@[inst_pay_dt],'-')
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
)			]]></sql>
			<params>
				<param name="insur_tp_cd" type="12" value="" out="N"/>
				<param name="insur_plcy_yr" type="12" value="" out="N"/>
				<param name="insur_clm_pty_no" type="12" value="" out="N"/>
				<param name="insur_prm_tp_cd" type="12" value="" out="N"/>
				<param name="inst_seq" type="2" value="" out="N"/>
				<param name="inst_locl_amt" type="12" value="" out="N"/>
				<param name="inst_curr_cd" type="12" value="" out="N"/>
				<param name="inst_xch_rt" type="12" value="" out="N"/>
				<param name="inst_usd_amt" type="12" value="" out="N"/>
				<param name="inst_due_dt" type="12" value="" out="N"/>
				<param name="inst_pay_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
