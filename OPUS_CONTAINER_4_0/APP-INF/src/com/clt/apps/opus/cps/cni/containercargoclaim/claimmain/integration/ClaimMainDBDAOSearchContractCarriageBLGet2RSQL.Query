<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOSearchContractCarriageBLGet2RSQL">
			<desc><![CDATA[SearchContractCarriageBLGet2]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(REPLACE(REPLACE(CUST_NM,CHR(13)||CHR(10),' '),CHR(12), ' '), 1, 50)  AS SHPR_NM
FROM BKG_CUSTOMER 
 WHERE BKG_NO     = @[bkg_no]
  AND BKG_CUST_TP_CD= 'S'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
