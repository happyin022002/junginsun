<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOModifyOfficeMatrixListByOfficeUSQL">
			<desc><![CDATA[Fm Office 에서 비용코드(Expense Code) 사용시 To Office 집계 되도록 정의된 정보]]></desc>
			<sql><![CDATA[
UPDATE GEM_OFC_EXPT
   SET DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE
WHERE  SND_OFC_CD = @[snd_ofc_cd]
AND    RCV_OFC_CD = @[rcv_ofc_cd]
AND    GEN_EXPN_CD = @[gen_expn_cd]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="snd_ofc_cd" type="12" value="" out="N"/>
				<param name="rcv_ofc_cd" type="12" value="" out="N"/>
				<param name="gen_expn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
