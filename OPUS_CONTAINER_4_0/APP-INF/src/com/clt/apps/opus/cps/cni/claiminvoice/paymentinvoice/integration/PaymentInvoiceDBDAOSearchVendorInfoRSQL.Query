<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PaymentInvoiceDBDAOSearchVendorInfoRSQL">
			<desc><![CDATA[Vendor 정보 취득]]></desc>
			<sql><![CDATA[
SELECT
    VNDR_SEQ
  , VNDR_CNT_CD
  , INV_CURR_CD
  , NVL(PAY_CURR_CD,  INV_CURR_CD) AS PAY_CURR_CD
  , GEN_PAY_TERM_CD  
  , VNDR_OFC_CD  
FROM
    MDM_VENDOR
WHERE VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
