<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOSearchLogInOfficeChangeByOfcCdDupCheckRSQL">
			<desc><![CDATA[GEM_CNG_OFC Table의 ofc_cd, cng_ofc_cd의 중복체크]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
FROM   GEM_CNG_OFC
WHERE  OFC_CD = @[ofc_cd]
AND    CNG_OFC_CD = @[cng_ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cng_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
