<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOSearchLogInOfficeChangeRSQL">
			<desc><![CDATA[GEM_CNG_OFC Table의 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
      ,CNG_OFC_CD
      ,DELT_FLG
      ,CRE_USR_ID
      ,TO_CHAR (CRE_DT, 'YYYY-MM-DD') CRE_DT
      ,UPD_USR_ID
      ,DECODE (DELT_FLG, 'N', '', TO_CHAR (UPD_DT, 'YYYY-MM-DD')) UPD_DT
      ,OFC_CNG_RMK
FROM   GEM_CNG_OFC
WHERE  1=1

#if(${sch_delt_flg} != '')
AND    DELT_FLG = @[sch_delt_flg]
#else
AND    DELT_FLG = 'N'
#end
#if(${sch_office_gbn} == 'Y')
AND    OFC_CD LIKE @[sch_office_code]||'%'
#end
#if(${sch_office_gbn} == 'N')
AND    CNG_OFC_CD LIKE @[sch_office_code]||'%'
#end			]]></sql>
			<params>
				<param name="sch_delt_flg" type="12" value="" out="N"/>
				<param name="sch_office_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
