<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOSearchContractCarriageCntrExistRSQL">
			<desc><![CDATA[SearchContractCarriageCntrExist]]></desc>
			<sql><![CDATA[
SELECT  

COUNT(CGO_CLM_REF_CNTR_NO)  AS  CNT
      
FROM CNI_CGO_CLM_CNTR_DTL 
WHERE CGO_CLM_NO = @[cgo_clm_no]
AND   CGO_CLM_REF_BL_NO = @[cgo_clm_ref_bl_no]
AND   CGO_CLM_REF_CNTR_NO   =  @[cgo_clm_ref_cntr_no]			]]></sql>
			<params>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
				<param name="cgo_clm_ref_bl_no" type="12" value="" out="N"/>
				<param name="cgo_clm_ref_cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
