<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOSearchHandlingCostListRSQL">
			<desc><![CDATA[HandlingCost 조회]]></desc>
			<sql><![CDATA[
SELECT
    COST.CGO_CLM_PAY_NO
  , COST.CGO_CLM_NO
  , COST.CLM_COST_TP_CD
  , COST.CLM_PTY_NO
  , COST.COST_DESC
  , COST.INV_NO
  , COST.INV_DT
  , COST.INV_AMT
  , COST.LOCL_CURR_CD
  , COST.INV_XCH_RT
  , COST.INV_USD_AMT
  , TO_CHAR (COST.PAY_DT, 'YYYYMMDD') AS PAY_DT
  , COST.INV_RMK
  , COST.INV_RGST_NO
  , PARTY.CLM_PTY_ABBR_NM
  , TO_CHAR (INV.AP_PAY_DT, 'YYYYMMDD') AS AP_PAY_DT
FROM
    CNI_CGO_CLM_COST COST
  , AP_PAY_INV INV
  , CNI_PARTY PARTY
WHERE
    COST.CGO_CLM_NO         = @[cgo_clm_no]
    AND COST.INV_RGST_NO  = INV.INV_RGST_NO(+)
    AND COST.CLM_PTY_NO     = PARTY.CLM_PTY_NO(+)
ORDER BY COST.CGO_CLM_PAY_NO			]]></sql>
			<params>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
