<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncidentSurveyDBDAOModifySurveyUSQL">
			<desc><![CDATA[Survey 업데이트]]></desc>
			<sql><![CDATA[
UPDATE
    CNI_CGO_CLM_SVEY
SET
    CLM_PTY_NO        = @[clm_pty_no]
  , SVYR_TP_CD        = @[svyr_tp_cd]
  , REF_SVYR_NO       = @[ref_svyr_no]
  , SVYR_APNT_DT      = @[svyr_apnt_dt]
  , SVEY_INP_DT       = @[svey_inp_dt]
  , SVYR_FEE_LOCL_AMT = @[svyr_fee_locl_amt]
  , SVYR_LOCL_CURR_CD = @[svyr_locl_curr_cd]
  , SVYR_XCH_RT       = @[svyr_xch_rt]
  , SVYR_FACT_FND_DESC = @[svyr_fact_fnd_desc]
  , UPD_USR_ID        = @[upd_usr_id]
  , UPD_DT            = CNI_GET_GMT_FNC(@[upd_usr_id])
WHERE
    CGO_CLM_NO = @[cgo_clm_no]			]]></sql>
			<params>
				<param name="clm_pty_no" type="12" value="" out="N"/>
				<param name="svyr_tp_cd" type="12" value="" out="N"/>
				<param name="ref_svyr_no" type="12" value="" out="N"/>
				<param name="svyr_apnt_dt" type="12" value="" out="N"/>
				<param name="svey_inp_dt" type="12" value="" out="N"/>
				<param name="svyr_fee_locl_amt" type="6" value="" out="N"/>
				<param name="svyr_locl_curr_cd" type="12" value="" out="N"/>
				<param name="svyr_xch_rt" type="6" value="" out="N"/>
				<param name="svyr_fact_fnd_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
