<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InsuranceDBDAOCustomPremiumVOUSQL">
			<desc><![CDATA[Premium 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE CNI_INSUR_PRM SET 
	TTL_LOCL_AMT    = REPLACE(@[ttl_locl_amt],',')   
,	TTL_CURR_CD     = @[ttl_curr_cd]    
,	TTL_XCH_RT      = REPLACE(@[ttl_xch_rt],',')     
,	TTL_USD_AMT     = REPLACE(@[ttl_usd_amt],',')    
,	TTL_DUE_DT      = REPLACE(@[ttl_due_dt],'-')     
,	TTL_PAY_DT      = REPLACE(@[ttl_pay_dt],'-')    
,	ADJ_LOCL_AMT    = REPLACE(@[adj_locl_amt],',')   
,	ADJ_CURR_CD     = @[adj_curr_cd]    
,	ADJ_XCH_RT      = REPLACE(@[adj_xch_rt],',')     
,	ADJ_USD_AMT     = REPLACE(@[adj_usd_amt],',')    
,	ADJ_DUE_DT      = REPLACE(@[adj_due_dt],'-')     
,	ADJ_PAY_DT      = REPLACE(@[adj_pay_dt],'-')     
,	RFND_LOCL_AMT   = REPLACE(@[rfnd_locl_amt],',')  
,	RFND_CURR_CD    = @[rfnd_curr_cd]   
,	RFND_XCH_RT     = REPLACE(@[rfnd_xch_rt],',')    
,	RFND_USD_AMT    = REPLACE(@[rfnd_usd_amt],',')   
,	RFND_DUE_DT     = REPLACE(@[rfnd_due_dt],'-')    
,	RFND_PAY_DT     = REPLACE(@[rfnd_pay_dt],'-')    
,	OTS_LOCL_AMT    = REPLACE(@[ots_locl_amt],',')   
,	OTS_CURR_CD     = @[ots_curr_cd]    
,	OTS_XCH_RT      = REPLACE(@[ots_xch_rt],',')     
,	OTS_USD_AMT     = REPLACE(@[ots_usd_amt],',')    
,	OTS_DUE_DT      = REPLACE(@[ots_due_dt],'-')     
,	OTS_PAY_DT      = REPLACE(@[ots_pay_dt],'-')     
,	DIFF_RMK        = @[diff_rmk]       
,	UPD_USR_ID 		= @[upd_usr_id]
,	UPD_DT 			= CNI_GET_GMT_FNC(@[upd_usr_id])
WHERE	INSUR_TP_CD = @[insur_tp_cd]
AND	INSUR_PLCY_YR 	= @[insur_plcy_yr]
AND	INSUR_CLM_PTY_NO = @[insur_clm_pty_no]
AND	INSUR_PRM_TP_CD = @[insur_prm_tp_cd]			]]></sql>
			<params>
				<param name="ttl_locl_amt" type="12" value="" out="N"/>
				<param name="ttl_curr_cd" type="12" value="" out="N"/>
				<param name="ttl_xch_rt" type="12" value="" out="N"/>
				<param name="ttl_usd_amt" type="12" value="" out="N"/>
				<param name="ttl_due_dt" type="12" value="" out="N"/>
				<param name="ttl_pay_dt" type="12" value="" out="N"/>
				<param name="adj_locl_amt" type="12" value="" out="N"/>
				<param name="adj_curr_cd" type="12" value="" out="N"/>
				<param name="adj_xch_rt" type="12" value="" out="N"/>
				<param name="adj_usd_amt" type="12" value="" out="N"/>
				<param name="adj_due_dt" type="12" value="" out="N"/>
				<param name="adj_pay_dt" type="12" value="" out="N"/>
				<param name="rfnd_locl_amt" type="12" value="" out="N"/>
				<param name="rfnd_curr_cd" type="12" value="" out="N"/>
				<param name="rfnd_xch_rt" type="12" value="" out="N"/>
				<param name="rfnd_usd_amt" type="12" value="" out="N"/>
				<param name="rfnd_due_dt" type="12" value="" out="N"/>
				<param name="rfnd_pay_dt" type="12" value="" out="N"/>
				<param name="ots_locl_amt" type="12" value="" out="N"/>
				<param name="ots_curr_cd" type="12" value="" out="N"/>
				<param name="ots_xch_rt" type="12" value="" out="N"/>
				<param name="ots_usd_amt" type="12" value="" out="N"/>
				<param name="ots_due_dt" type="12" value="" out="N"/>
				<param name="ots_pay_dt" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="insur_tp_cd" type="12" value="" out="N"/>
				<param name="insur_plcy_yr" type="12" value="" out="N"/>
				<param name="insur_clm_pty_no" type="12" value="" out="N"/>
				<param name="insur_prm_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
