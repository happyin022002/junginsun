<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeMgtDBDAOAddPartyCSQL">
			<desc><![CDATA[CNI Party Insert]]></desc>
			<sql><![CDATA[
INSERT INTO CNI_PARTY (
	CRE_OFC_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	PHN_NO
,	INTL_FAX_NO
,	FAX_NO
,	PTY_EML
,	PTY_ADDR
,	PTY_RMK
,	LOC_CD
,	ZIP_CD_CTNT
,	CNT_CD
,	CUST_SEQ
,	VNDR_SEQ
,	CLM_PTY_CLR_NO
,	DELT_FLG
,	CLM_PTY_ABBR_NM
,	PRNT_CLM_PTY_NO
,	PTY_NM
,	INTL_PHN_NO
,	CLM_PTY_NO
) VALUES( 
	@[cre_ofc_cd]
,	@[cre_usr_id]
,	sysdate
,	@[upd_usr_id]
,	sysdate
,	@[phn_no]
,	@[intl_fax_no]
,	@[fax_no]
,	@[pty_eml]
,	@[pty_addr]
,	@[pty_rmk]
,	@[loc_cd]
,	@[zip_cd_ctnt]
,	@[cnt_cd]
,	@[cust_seq]
,	@[vndr_seq]
,	@[clm_pty_clr_no]
,	'N'
,	@[clm_pty_abbr_nm]
,	@[prnt_clm_pty_no]
,	@[pty_nm]
,	@[intl_phn_no]
,	@[clm_pty_no]
)			]]></sql>
			<params>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="intl_fax_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="pty_eml" type="12" value="" out="N"/>
				<param name="pty_addr" type="12" value="" out="N"/>
				<param name="pty_rmk" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="zip_cd_ctnt" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="clm_pty_clr_no" type="2" value="" out="N"/>
				<param name="clm_pty_abbr_nm" type="12" value="" out="N"/>
				<param name="prnt_clm_pty_no" type="2" value="" out="N"/>
				<param name="pty_nm" type="12" value="" out="N"/>
				<param name="intl_phn_no" type="12" value="" out="N"/>
				<param name="clm_pty_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
