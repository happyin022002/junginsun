<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeMgtDBDAOModifyContactPointUSQL">
			<desc><![CDATA[Update]]></desc>
			<sql><![CDATA[
UPDATE CNI_CNTC_PNT SET 
	CLM_PTY_NO = @[clm_pty_no]
,	CNTC_PNT_NM = @[cntc_pnt_nm]
,	INTL_PHN_NO = @[intl_phn_no]
,	CNTC_PNT_PHN_NO = @[cntc_pnt_phn_no]
,	INTL_FAX_NO = @[intl_fax_no]
,	CNTC_PNT_FAX_NO = @[cntc_pnt_fax_no]
,	CNTC_PNT_EML = @[cntc_pnt_eml]
,	CNTC_PNT_RMK = @[cntc_pnt_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	CLM_CNTC_PNT_SEQ = @[clm_cntc_pnt_seq]
			]]></sql>
			<params>
				<param name="clm_pty_no" type="2" value="" out="N"/>
				<param name="cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="intl_phn_no" type="12" value="" out="N"/>
				<param name="cntc_pnt_phn_no" type="12" value="" out="N"/>
				<param name="intl_fax_no" type="12" value="" out="N"/>
				<param name="cntc_pnt_fax_no" type="12" value="" out="N"/>
				<param name="cntc_pnt_eml" type="12" value="" out="N"/>
				<param name="cntc_pnt_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="clm_cntc_pnt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
