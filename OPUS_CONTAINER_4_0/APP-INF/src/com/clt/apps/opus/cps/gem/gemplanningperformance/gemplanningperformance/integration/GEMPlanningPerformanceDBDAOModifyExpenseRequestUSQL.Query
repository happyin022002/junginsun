<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOModifyExpenseRequestUSQL">
			<desc><![CDATA[집행단위, 지역그룹|BU CTRL , 비용주관팀 , 사무국 에서 등록한 비용계획 수정]]></desc>
			<sql><![CDATA[
UPDATE GEM_REQUEST SET
UPD_DT = sysdate
#if (${pln_yrmon} != '') 
,	PLN_YRMON = @[pln_yrmon]
#end
#if (${gen_expn_rqst_tp_cd} != '') 
,	GEN_EXPN_RQST_TP_CD = @[gen_expn_rqst_tp_cd]
#end
#if (${rqst_ofc_cd} != '') 
,	RQST_OFC_CD = @[rqst_ofc_cd]
#end
#if (${upd_usr_id} != '') 
,	UPD_USR_ID = @[upd_usr_id]
#end
WHERE	GEN_EXPN_RQST_NO = @[gen_expn_rqst_no]			]]></sql>
			<params>
				<param name="pln_yrmon" type="12" value="" out="N"/>
				<param name="gen_expn_rqst_tp_cd" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="gen_expn_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
