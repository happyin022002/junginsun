<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchSlpIfRSQL">
			<desc><![CDATA[GEM_SLP_IF 테이블의 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SLP_TJ_NO
      ,SLP_SEQ_NO
      ,GL_EFF_DT
      ,ACCT_CD
      ,SLP_CURR_CD
      ,SLP_AMT
      ,SLP_CTR_CD
      ,SLP_DESC
      ,OFC_CD
      ,SLP_VNDR_CD
FROM   GEM_SLP_IF
WHERE  1=1
#if(${slp_tj_no} != '')
AND    SLP_TJ_NO = @[slp_tj_no]
#end
#if(${slp_seq_no} != '')
AND    SLP_SEQ_NO = (SELECT SUBSTR (@[slp_seq_no] + 10000, 2, 5) FROM DUAL)
#end
#if(${gl_eff_dt} != '')
AND    GL_EFF_DT LIKE @[gl_eff_dt]||'%'
#end
AND    SLP_IF_FLG = 'N'			]]></sql>
			<params>
				<param name="slp_tj_no" type="12" value="" out="N"/>
				<param name="slp_seq_no" type="12" value="" out="N"/>
				<param name="gl_eff_dt" type="12" value="200702" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
