<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchUserOfficeCdRSQL">
			<desc><![CDATA[로그인 OFFICE 를 Gem OFFICE로 검색]]></desc>
			<sql><![CDATA[
SELECT CNG_OFC_CD
FROM   GEM_CNG_OFC
WHERE  OFC_CD = @[ofc_cd]
AND    DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
