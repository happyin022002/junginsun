<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncidentSurveyDBDAOsearchLocationRSQL">
			<desc><![CDATA[location validation]]></desc>
			<sql><![CDATA[
SELECT
    A.LOC_CD
FROM
    MDM_LOCATION A
  , MDM_COUNTRY B
  , MDM_EQ_ORZ_CHT C
WHERE
    1            = 1
    AND A.CNT_CD = B.CNT_CD(+)
    AND A.SCC_CD = C.SCC_CD
    AND A.LOC_CD = @[loc_cd]
    AND NVL (A.DELT_FLG, 'N') <> 'Y'
    AND NVL (B.DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
