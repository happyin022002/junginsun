<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOSearchVesselNameRSQL">
			<desc><![CDATA[Vessel Name을 조회한다]]></desc>
			<sql><![CDATA[
SELECT VSL_ENG_NM
FROM (
		SELECT VSL_ENG_NM
		FROM   MDM_VSL_CNTR
		WHERE  VSL_CD   = @[vsl_cd]
        AND    DELT_FLG = 'N'
)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
