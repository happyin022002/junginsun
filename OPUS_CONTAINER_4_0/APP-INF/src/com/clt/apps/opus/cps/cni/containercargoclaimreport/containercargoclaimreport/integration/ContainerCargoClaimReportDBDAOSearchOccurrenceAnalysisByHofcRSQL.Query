<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerCargoClaimReportDBDAOSearchOccurrenceAnalysisByHofcRSQL">
			<desc><![CDATA[SearchOccurrenceAnalysisByHofc]]></desc>
			<sql><![CDATA[
SELECT
  
   REPORT_BY_CD     HOFC_REPORT_BY_CD
  , REPORT_BY_NM    HOFC_REPORT_BY_NM
  , HOFC
  
  , HOFC_CASE0
  , HOFC_PCT_CASE0 ||'%' HOFC_PCT_CASE0
  , HOFC_AMT0
  , HOFC_PCT_AMT0 ||'%' HOFC_PCT_AMT0
  
  , HOFC_CASE1
  , ROUND (DECODE (HOFC_CASE0, 0, 0, (HOFC_CASE1 / HOFC_CASE0) * 100), 2) ||'%' HOFC_PCT_CASE1
  , HOFC_AMT1
  , ROUND (DECODE (HOFC_AMT0, 0, 0, (HOFC_AMT1 / HOFC_AMT0) * 100), 2) ||'%' HOFC_PCT_AMT1
  
  , HOFC_CASE2
  , ROUND (DECODE (HOFC_CASE0, 0, 0, (HOFC_CASE2 / HOFC_CASE0) * 100), 2) ||'%' HOFC_PCT_CASE2
  , HOFC_AMT2
  , ROUND (DECODE (HOFC_AMT0, 0, 0, (HOFC_AMT2 / HOFC_AMT0) * 100), 2) ||'%' HOFC_PCT_AMT2
  
  , HOFC_CASE3
  , ROUND (DECODE (HOFC_CASE0, 0, 0, (HOFC_CASE3 / HOFC_CASE0) * 100), 2) ||'%' HOFC_PCT_CASE3
  , HOFC_AMT3
  , ROUND (DECODE (HOFC_AMT0, 0, 0, (HOFC_AMT3 / HOFC_AMT0) * 100), 2) ||'%' HOFC_PCT_AMT3
  
  , HOFC_CASE4
  , ROUND (DECODE (HOFC_CASE0, 0, 0, (HOFC_CASE4 / HOFC_CASE0) * 100), 2) ||'%' HOFC_PCT_CASE4
  , HOFC_AMT4
  , ROUND (DECODE (HOFC_AMT0, 0, 0, (HOFC_AMT4 / HOFC_AMT0) * 100), 2) ||'%' HOFC_PCT_AMT4
  
  , HOFC_CASE5
  , ROUND (DECODE (HOFC_CASE0, 0, 0, (HOFC_CASE5 / HOFC_CASE0) * 100), 2) ||'%' HOFC_PCT_CASE5
  , HOFC_AMT5
  , ROUND (DECODE (HOFC_AMT0, 0, 0, (HOFC_AMT5 / HOFC_AMT0) * 100), 2) ||'%' HOFC_PCT_AMT5
  
  , HOFC_CASE6
  , ROUND (DECODE (HOFC_CASE0, 0, 0, (HOFC_CASE6 / HOFC_CASE0) * 100), 2) ||'%' HOFC_PCT_CASE6
  , HOFC_AMT6
  , ROUND (DECODE (HOFC_AMT0, 0, 0, (HOFC_AMT6 / HOFC_AMT0) * 100), 2) ||'%' HOFC_PCT_AMT6

 
FROM
    (
        SELECT
            REPORT_BY_CD
          , REPORT_BY_NM
          , HOFC       
          , NVL (CASE0, 0)                      AS HOFC_CASE0
          , 100                                   AS HOFC_PCT_CASE0
          , NVL (AMT0, 0)                       AS HOFC_AMT0
          , 100                                   AS HOFC_PCT_AMT0
          
          , NVL (CASE1, 0)                      AS HOFC_CASE1
          , 0                                   AS HOFC_PCT_CASE1
          , NVL (AMT1, 0)                       AS HOFC_AMT1
          , 0                                   AS HOFC_PCT_AMT1
          
          , NVL (CASE2, 0)                      AS HOFC_CASE2
          , 0                                   AS HOFC_PCT_CASE2
          , NVL (AMT2, 0)                       AS HOFC_AMT2
          , 0                                   AS HOFC_PCT_AMT2
          
          , NVL (CASE3, 0)                      AS HOFC_CASE3
          , 0                                   AS HOFC_PCT_CASE3
          , NVL (AMT3, 0)                       AS HOFC_AMT3
          , 0                                   AS HOFC_PCT_AMT3
          
          , NVL (CASE4, 0)                      AS HOFC_CASE4
          , 0                                   AS HOFC_PCT_CASE4
          , NVL (AMT4, 0)                       AS HOFC_AMT4
          , 0                                   AS HOFC_PCT_AMT4
      
          , NVL (CASE5, 0)                      AS HOFC_CASE5
          , 0                                   AS HOFC_PCT_CASE5
          , NVL (AMT5, 0)                       AS HOFC_AMT5
          , 0                                   AS HOFC_PCT_AMT5
      
          , NVL (CASE6, 0)                      AS HOFC_CASE6
          , 0                                   AS HOFC_PCT_CASE6
          , NVL (AMT6, 0)                       AS HOFC_AMT6
          , 0                                   AS HOFC_PCT_AMT6
      
            
        FROM
            (
                SELECT

                    CLM_AREA_CD      AS REPORT_BY_CD
                  , MISC.CLM_MISC_NM AS REPORT_BY_NM
                  , HDLR_OFC_CD      AS HOFC
                  
                  , COUNT (CGO_CLM_NO)        AS CASE0
                  , SUM (CLMT_USD_AMT)        AS AMT0

                  , COUNT (DECODE (CGO_CLM_TP_CD, 'PHY', CGO_CLM_NO))    AS CASE1                  
				  , SUM (DECODE (CGO_CLM_TP_CD, 'PHY', CLMT_USD_AMT))    AS AMT1 

                  , COUNT (DECODE (CGO_CLM_TP_CD, 'WET', CGO_CLM_NO))    AS CASE2                  
				  , SUM (DECODE (CGO_CLM_TP_CD, 'WET', CLMT_USD_AMT))    AS AMT2  

                  , COUNT (DECODE (CGO_CLM_TP_CD, 'STG', CGO_CLM_NO))    AS CASE3                  
				  , SUM (DECODE (CGO_CLM_TP_CD, 'STG', CLMT_USD_AMT))    AS AMT3

                  , COUNT (DECODE (CGO_CLM_TP_CD, 'NDL', CGO_CLM_NO))    AS CASE4                 
				  , SUM (DECODE (CGO_CLM_TP_CD, 'NDL', CLMT_USD_AMT))    AS AMT4  

                  , COUNT (DECODE (CGO_CLM_TP_CD, 'RDL', CGO_CLM_NO))    AS CASE5                  
				  , SUM (DECODE (CGO_CLM_TP_CD, 'RDL', CLMT_USD_AMT))    AS AMT5

                  , COUNT (DECODE (CGO_CLM_TP_CD, 'OTR', CGO_CLM_NO))    AS CASE6                  
				  , SUM (DECODE (CGO_CLM_TP_CD, 'OTR', CLMT_USD_AMT))    AS AMT6     
                                    

                FROM
                    CNI_CLM_V CLM_V
                  , (
                        SELECT
                            CLM_MISC_NM
                          , CLM_MISC_CD
                        FROM
                            CNI_MISC_CD
                        WHERE
                            CLSS_CLM_MISC_CD = @[report_by]
                    )
                    MISC
                               

                WHERE CLM_AREA_CD      = MISC.CLM_MISC_CD
                AND CLM_AREA_CD IS NOT NULL
			  #if(${period} == 'DOU' && ${from_period} != '')
			      AND TO_CHAR(CLM_V.UPD_DT,'YYYYMMDD') BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOI' && ${from_period} != '')
			      AND CLM_V.INCI_OCCR_DT BETWEEN @[from_period]  AND @[to_period]  -- 나중확인
			  #end
			  #if(${period} == 'DOSV' && ${from_period} != '')
			      AND CLM_V.SVEY_INP_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end 
			  #if(${period} == 'DON' && ${from_period} != '')
			      AND CLM_V.PRLM_CLM_NTC_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOTB' && ${from_period} != '')
			      AND CLM_V.CLM_TM_BAR_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOTBLP' && ${from_period} != '')
			      AND CLM_V.LABL_TM_BAR_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOF' && ${from_period} != '')
			      AND CLM_V.FMAL_CLM_RCV_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOFF' && ${from_period} != '')
			      AND CLM_V.FACT_FND_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOSS' && ${from_period} != '')
			      AND CLM_V.SMNS_SVE_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOS' && ${from_period} != '')
			      AND CLM_V.CGO_CLM_STL_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DORLP' && ${from_period} != '')
			      AND CLM_V.LABL_PTY_RCVR_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DORINS' && ${from_period} != '')
			      AND CLM_V.INSUR_RCVR_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOR' && ${from_period} != '')
			      AND CLM_V.RCT_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOL' && ${from_period} != '')
			      AND CLM_V.LODG_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DOD' && ${from_period} != '')
			      AND CLM_V.DCHG_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
			  #if(${period} == 'DEL' && ${from_period} != '')
			      AND CLM_V.DE_DT BETWEEN @[from_period]  AND @[to_period] 
			  #end
                
                GROUP BY
                    CLM_AREA_CD
                  , MISC.CLM_MISC_NM
                  , HDLR_OFC_CD      
                  
                ORDER  BY CLM_MISC_NM, HDLR_OFC_CD          


            )
    )			]]></sql>
			<params>
				<param name="report_by" type="12" value="" out="N"/>
				<param name="from_period" type="12" value="" out="N"/>
				<param name="to_period" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
