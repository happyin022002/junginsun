<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOModifyExpenseApprovalStepUSQL">
			<desc><![CDATA[GEMPlanningPerformanceDB]]></desc>
			<sql><![CDATA[
UPDATE GEM_APRO_STEP SET
    UPD_DT = sysdate
#if (${gen_expn_apsts_cd} != '')
,	GEN_EXPN_APSTS_CD = @[gen_expn_apsts_cd]
#end
#if (${gen_expn_apro_auth_ofc_cd} != '')
,	GEN_EXPN_APRO_AUTH_OFC_CD = @[gen_expn_apro_auth_ofc_cd]
#end
#if (${jan_amt} != '')
,	JAN_AMT = @[jan_amt]
#end
#if (${feb_amt} != '')
,	FEB_AMT = @[feb_amt]
#end
#if (${mar_amt} != '')
,	MAR_AMT = @[mar_amt]
#end
#if (${apr_amt} != '')
,	APR_AMT = @[apr_amt]
#end
#if (${may_amt} != '')
,	MAY_AMT = @[may_amt]
#end
#if (${jun_amt} != '')
,	JUN_AMT = @[jun_amt]
#end
#if (${jul_amt} != '')
,	JUL_AMT = @[jul_amt]
#end
#if (${aug_amt} != '')
,	AUG_AMT = @[aug_amt]
#end
#if (${sep_amt} != '')
,	SEP_AMT = @[sep_amt]
#end
#if (${oct_amt} != '')
,	OCT_AMT = @[oct_amt]
#end
#if (${nov_amt} != '')
,	NOV_AMT = @[nov_amt]
#end
#if (${dec_amt} != '')
,	DEC_AMT = @[dec_amt]
#end
#if (${apro_opin_rmk} != '')
,	APRO_OPIN_RMK = @[apro_opin_rmk]
#end
,	UPD_USR_ID = @[upd_usr_id]
WHERE	GEN_EXPN_RQST_NO = @[gen_expn_rqst_no]
AND	OFC_CD = @[ofc_cd]
AND	GEN_EXPN_CD = @[gen_expn_cd]
AND	GEN_EXPN_ITM_NO = @[gen_expn_itm_no]
AND	GEN_EXPN_TRNS_DIV_CD = @[gen_expn_trns_div_cd]
AND	GEN_EXPN_RQST_SEQ = @[gen_expn_rqst_seq]
AND	GEN_EXPN_APRO_STEP_CD = @[gen_expn_apro_step_cd]			]]></sql>
			<params>
				<param name="gen_expn_apsts_cd" type="12" value="" out="N"/>
				<param name="gen_expn_apro_auth_ofc_cd" type="12" value="" out="N"/>
				<param name="jan_amt" type="6" value="" out="N"/>
				<param name="feb_amt" type="6" value="" out="N"/>
				<param name="mar_amt" type="6" value="" out="N"/>
				<param name="apr_amt" type="6" value="" out="N"/>
				<param name="may_amt" type="6" value="" out="N"/>
				<param name="jun_amt" type="6" value="" out="N"/>
				<param name="jul_amt" type="6" value="" out="N"/>
				<param name="aug_amt" type="6" value="" out="N"/>
				<param name="sep_amt" type="6" value="" out="N"/>
				<param name="oct_amt" type="6" value="" out="N"/>
				<param name="nov_amt" type="6" value="" out="N"/>
				<param name="dec_amt" type="6" value="" out="N"/>
				<param name="apro_opin_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="gen_expn_rqst_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="gen_expn_cd" type="12" value="" out="N"/>
				<param name="gen_expn_itm_no" type="12" value="" out="N"/>
				<param name="gen_expn_trns_div_cd" type="12" value="" out="N"/>
				<param name="gen_expn_rqst_seq" type="12" value="" out="N"/>
				<param name="gen_expn_apro_step_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
