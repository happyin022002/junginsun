<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchReqUpdDtRSQL">
			<desc><![CDATA[Request 테이블의 upd_dt 취득]]></desc>
			<sql><![CDATA[
SELECT
    TO_CHAR (UPD_DT, 'YYYYMMDDHH24MISS') UPD_DT
FROM
    GEM_REQUEST
WHERE
    GEN_EXPN_RQST_NO = @[gen_expn_rqst_no]			]]></sql>
			<params>
				<param name="gen_expn_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
