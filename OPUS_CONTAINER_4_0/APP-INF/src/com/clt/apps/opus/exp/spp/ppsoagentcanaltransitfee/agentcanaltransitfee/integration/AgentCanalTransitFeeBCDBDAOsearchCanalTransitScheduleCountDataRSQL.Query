<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgentCanalTransitFeeBCDBDAOsearchCanalTransitScheduleCountDataRSQL">
			<desc><![CDATA[Canal Transit Schedule inquiry Count]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM (
SELECT
		T5.SVC_SCP_BND_CD
		,DECODE(T5.SVC_SCP_BND_CD, 'N', 'North', 'S', 'South') BOUND
		,T2.VSL_CD
		,T2.SKD_DIR_CD
		,T2.VSL_CD||T2.SKD_VOY_NO||T2.SKD_DIR_CD VVD
		,T3.VSL_ENG_NM ||'('|| T3.VSL_CD ||')' VSL_ENG_NM
		,T1.VSL_SLAN_CD
		,T2.VPS_PORT_CD
		,T2.CLPT_SEQ
		,TO_CHAR(T2.VPS_ETA_DT,'YYYY-MM-DD HH24:MI') VPS_ETA_DT
		,TO_CHAR(T2.VPS_ETB_DT,'YYYY-MM-DD HH24:MI') VPS_ETB_DT
		,TO_CHAR(T2.VPS_ETD_DT,'YYYY-MM-DD HH24:MI') VPS_ETD_DT
        ,(SELECT VPS_PORT_CD
          FROM VSK_VSL_PORT_SKD
          WHERE 1=1
          AND VSL_CD=T2.VSL_CD
          AND SKD_VOY_NO=T2.SKD_VOY_NO
          AND SKD_DIR_CD=T2.SKD_DIR_CD
          AND CLPT_SEQ > T2.CLPT_SEQ
          AND VPS_ETA_DT > T2.VPS_ETA_DT
          AND NVL(SKD_CNG_STS_CD, 'X')<>'S'
          AND ROWNUM=1
        ) NXT_PORT_CD
        ,(SELECT CLPT_SEQ
          FROM VSK_VSL_PORT_SKD
          WHERE 1=1
          AND VSL_CD=T2.VSL_CD
          AND SKD_VOY_NO=T2.SKD_VOY_NO
          AND SKD_DIR_CD=T2.SKD_DIR_CD
          AND CLPT_SEQ > T2.CLPT_SEQ
          AND VPS_ETA_DT > T2.VPS_ETA_DT
          AND NVL(SKD_CNG_STS_CD, 'X')<>'S'
          AND ROWNUM=1
        ) NXT_PORT_CLPT_SEQ
        ,(SELECT TO_CHAR(VPS_ETA_DT,'YYYY-MM-DD HH24:MI') VPS_ETA_DT
          FROM VSK_VSL_PORT_SKD
          WHERE 1=1
          AND VSL_CD=T2.VSL_CD
          AND SKD_VOY_NO=T2.SKD_VOY_NO
          AND SKD_DIR_CD=T2.SKD_DIR_CD
          AND CLPT_SEQ > T2.CLPT_SEQ
          AND VPS_ETA_DT > T2.VPS_ETA_DT
          AND NVL(SKD_CNG_STS_CD, 'X')<>'S'
          AND ROWNUM=1
        ) NXT_PORT_ETA
        ,(SELECT 'Y' FROM PSO_TGT_VVD
          WHERE 1=1
          AND VSL_CD=T1.VSL_CD
          AND SKD_VOY_NO=T1.SKD_VOY_NO
          AND SKD_DIR_CD=T1.SKD_DIR_CD
          AND PSO_BZTP_CD='5'
          --AND BUD_SCNR_NO 		= '999912'
          ) TGT_VVD_EXIST_FLG
FROM   VSK_VSL_SKD T1
     , VSK_VSL_PORT_SKD T2
     , MDM_VSL_CNTR T3
     , MDM_VSL_SVC_LANE T4
     , MDM_VSL_SVC_LANE_DIR T5
WHERE  1=1
AND    T1.VSL_CD = T2.VSL_CD
AND    T1.SKD_VOY_NO = T2.SKD_VOY_NO
AND    T1.SKD_DIR_CD = T2.SKD_DIR_CD
AND    NVL(T2.SKD_CNG_STS_CD, 'X') <> 'S'
AND    (T2.VPS_PORT_CD = 'EGSUZ' OR T2.VPS_PORT_CD = 'PAPAC')
AND    T2.VPS_ETB_DT BETWEEN TO_DATE(@[tgt_yrmon], 'YYYYMM')
                     AND ADD_MONTHS(TO_DATE(@[tgt_yrmon], 'YYYYMM'), 1)
AND    T2.VSL_CD = T3.VSL_CD
AND    T3.CRR_CD = 'HJS'
AND    T1.VSL_SLAN_CD = T4.VSL_SLAN_CD
AND    T4.VSL_TP_CD = 'C' /*컨테이너선*/
	#if (${lane_cd} != '') 
AND    T1.VSL_SLAN_CD =  @[lane_cd]
	#end
	#if (${vsl_cd} != '') 
AND    T1.VSL_CD = @[vsl_cd]
	#end
AND		T1.VSL_SLAN_CD	= T5.VSL_SLAN_CD
AND		T1.SKD_DIR_CD	= T5.VSL_SLAN_DIR_CD
--AND		T5.SVC_SCP_BND_CD IS NOT NULL  
	#if (${vndr_seq} != '') 
AND		T4.CNL_AGN_VNDR_SEQ = TO_NUMBER(@[vndr_seq])
	#end
ORDER BY T2.VPS_ETB_DT ASC
       ) X			]]></sql>
			<params>
				<param name="tgt_yrmon" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
