<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOModifyCustomerInfoCustAddrUSQL">
			<desc><![CDATA[ModifyCustomerInfoCustAddr]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUST_ADDR
SET BZET_ADDR = @[bzet_addr]
  , UPD_USR_ID = @[user_id]
  , UPD_DT = SYSDATE
WHERE CUST_CNT_CD =SUBSTR(@[cust_cd],1,2)
AND   CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))
AND PRMRY_CHK_FLG ='Y'			]]></sql>
			<params>
				<param name="bzet_addr" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
