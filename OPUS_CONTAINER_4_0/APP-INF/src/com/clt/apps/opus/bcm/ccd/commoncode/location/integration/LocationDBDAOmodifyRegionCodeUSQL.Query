<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOmodifyRegionCodeUSQL">
			<desc><![CDATA[Region 정보를 수정합니다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_REGION
SET
	RGN_NM = @[rgn_nm]
	, CNT_CD = @[cnt_cd]
	, UPD_USR_ID = @[usr_id]
	, UPD_DT = SYSDATE
	, DELT_FLG = @[delt_flg]
WHERE
	RGN_CD = @[rgn_cd]			]]></sql>
			<params>
				<param name="rgn_nm" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
