<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAOSearchMdmUsrAuthVORSQL">
			<desc><![CDATA[MdmUsrAuthVO 리스트 정보 조회]]></desc>
			<sql><![CDATA[
SELECT USR_ID,
       (SELECT USR_NM FROM COM_USER U WHERE U.USR_ID = A.USR_ID) USR_NM,
       MST_DAT_SUBJ_CD,
       (SELECT U.OFC_CD
          FROM COM_USER U
         WHERE U.USR_ID = A.USR_ID) OFC_CD,
       AUTH_TP_CD,
       PGM_ID,
       CRE_USR_ID,
	   UPD_USR_ID,
       TO_CHAR(CRE_DT,'YYYY-MM-DD HH24:MI:SS') CRE_DT,
	   '' CRE_OFC_CD
  FROM MDM_USR_AUTH A
 WHERE 1=1

#if (${mst_dat_subj_cd} != '') 
	AND MST_DAT_SUBJ_CD = @[mst_dat_subj_cd]
#end

#if (${usr_id} != '')
	AND USR_ID = @[usr_id]
#end

#if (${auth_tp_cd} != '') 
	AND AUTH_TP_CD = @[auth_tp_cd]
#end

ORDER BY UPD_DT DESC			]]></sql>
			<params>
				<param name="mst_dat_subj_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="auth_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
