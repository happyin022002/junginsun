<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchScpRqstRSQL">
			<desc><![CDATA[Request scp 코드 조회]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
	  ,SVC_SCP_NM
      ,SVC_SCP_BND_CD
      ,CONF_FLG
      ,FMC_FILE_FLG
      ,TRF_PFX_CD
      ,TRF_NO
      ,DELT_FLG
      ,MODI_COST_CTR_CD
      ,MODI_SVC_GRP_CD
      ,DMNT_FLG
 FROM MDM_SVC_SCP_RQST
WHERE RQST_NO = @[rqst_no]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
