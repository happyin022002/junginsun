<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesRepresentativeDBDAOSearchSlsRepMaxSeqRSQL">
			<desc><![CDATA[cnt code에 해당하는 것 중 searchSlsRepMaxSeq +1 조회]]></desc>
			<sql><![CDATA[
SELECT LPAD(NVL(MAX(SREP_MAX_SEQ), 0) + 1, 3, '0') MAX_SEQ
  FROM MDM_SLS_REP_SEQ_MGMT
 WHERE CNT_CD = @[cnt_cd]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
