<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAOAddMdmDatProcVOCSQL">
			<desc><![CDATA[MDM_DAT_PROC 테이블에 레코드를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_DAT_PROC
(  RQST_NO,
   MST_DAT_SUBJ_CD,
   RQST_USR_ID,
   RQST_OFC_CD,
   APRO_USR_ID,
   PROC_TP_CD,
   RJCT_RMK,
   APRO_RMK,
   RQST_CRE_DT,
   RQST_UPD_DT,
   PROC_CRE_DT,
   PROC_UPD_DT,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   DELT_FLG
) VALUES (
   @[rqst_no],
   @[mst_dat_subj_cd],
   @[rqst_usr_id],
   @[rqst_ofc_cd],
   NULL,
   'O',
   NULL,
   NULL,
   NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[rqst_ofc_cd]), SYSDATE),
   NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[rqst_ofc_cd]), SYSDATE),
   NULL,
   NULL,
   @[rqst_usr_id],
   NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[rqst_ofc_cd]), SYSDATE),
   @[rqst_usr_id],
   NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[rqst_ofc_cd]), SYSDATE),
   'N'
)			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="mst_dat_subj_cd" type="12" value="" out="N"/>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
