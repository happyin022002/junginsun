<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAORemoveCustomerAddrInfoDSQL">
			<desc><![CDATA[RemoveCustomerAddrInfo]]></desc>
			<sql><![CDATA[
DELETE FROM MDM_CUST_ADDR
WHERE  CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]
AND ADDR_SEQ = @[addr_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="addr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
