<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OrganizationDBDAOSearchOfcAccGrpMapRSQL">
			<desc><![CDATA[입력한 Office Access Group Mapping(Pop-up) 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
	  ,SUB_SYS_CD
	  ,OFC_GRP_ID
      ,OFC_CD CUR_OFC_CD
FROM MDM_OFC_GRP_MAPG
WHERE 1=1
AND SUB_SYS_CD = @[sub_sys_cd]
AND OFC_GRP_ID = @[ofc_grp_id]			]]></sql>
			<params>
				<param name="sub_sys_cd" type="12" value="" out="N"/>
				<param name="ofc_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
