<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOModifyMvmtStsCodeUSQL">
			<desc><![CDATA[조회한 Movement Status Code의 상세 정보를 수정하는 쿼리]]></desc>
			<sql><![CDATA[
UPDATE MDM_MVMT_STS
   SET MVMT_STS_NM = @[mvmt_sts_nm]
      ,DEST_YD_FLG = @[dest_yd_flg]
      ,IO_BND_CD = @[io_bnd_cd]
      ,DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
 WHERE MVMT_STS_CD = @[mvmt_sts_cd]			]]></sql>
			<params>
				<param name="mvmt_sts_nm" type="12" value="Bare Chassis Gate In" out="N"/>
				<param name="dest_yd_flg" type="12" value="N" out="N"/>
				<param name="io_bnd_cd" type="12" value="I" out="N"/>
				<param name="delt_flg" type="12" value="N" out="N"/>
				<param name="user_id" type="12" value="PLMMGR" out="N"/>
				<param name="mvmt_sts_cd" type="12" value="BI" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
