<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchVndrRqstRSQL">
			<desc><![CDATA[입력된 Request Vendor 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   RQST_NO 
		,VNDR_SEQ
        ,VNDR_CNT_CD
        ,VNDR_LGL_ENG_NM
        ,VNDR_LOCL_LANG_NM
        ,VNDR_ABBR_NM
        ,LGS_FLG
        ,PROCU_FLG
        ,FINC_FLG
        ,TEAM_FLG
        ,INTER_CO_FLG
        ,OTR_FLG
        ,LOC_CD
        ,OFC_CD
        ,RGST_NO
        ,TAX_ID
        ,GEN_PAY_TERM_CD
        ,PAY_TERM_TP_CD
        ,PAY_MZD_CD
        ,PRNT_CNT_CD
        ,TRIM(TO_CHAR(PRNT_VNDR_SEQ,'000000')) PRNT_VNDR_SEQ
        ,INV_CURR_CD
        ,CNTC_PSON_NM
        ,CEO_NM
        ,BZCT_NM
        ,BZTP_NM
        ,USA_EDI_CD
        ,SVC_SCP_CD_NM
        ,SVC_PRD_TP_NM
        ,SVC_PRD_RMK
        ,DCGO_HNDL_FLG
        ,MTY_RRO_EDI_USE_FLG
        ,WO_ATCH_FILE_FLG
        ,WO_EDI_USE_FLG
        ,INV_EDI_USE_FLG
        ,RFND_PSDO_CUST_CD
        ,VNDR_OFC_CD
        ,SUBS_CO_CD
        ,ENG_ADDR
        ,ZIP_CD
        ,LOCL_LANG_ADDR
        ,CHK_DE_ADDR1
        ,CHK_DE_ADDR2
        ,CHK_DE_ADDR3
        ,CHK_DE_CTY_NM
        ,CHK_DE_STE_CD
        ,CHK_DE_ZIP_CD
        ,CHK_DE_CNT_CD
        ,LU_DELT_FLG
        ,DELT_FLG
        ,MODI_VNDR_CD
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
FROM MDM_VNDR_RQST
WHERE 1=1
AND RQST_NO = @[rqst_no]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
