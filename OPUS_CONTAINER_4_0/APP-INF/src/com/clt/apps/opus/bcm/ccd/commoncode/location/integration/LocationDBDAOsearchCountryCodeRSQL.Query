<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchCountryCodeRSQL">
			<desc><![CDATA[Country를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT CNT_NM
	  , SCONTI_CD
	  , CURR_CD
	  , CNT_ISO_CD
	  , DELT_FLG
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , UPD_DT
      , EU_CNT_FLG
FROM MDM_COUNTRY
WHERE CNT_CD = @[cnt_cd]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
