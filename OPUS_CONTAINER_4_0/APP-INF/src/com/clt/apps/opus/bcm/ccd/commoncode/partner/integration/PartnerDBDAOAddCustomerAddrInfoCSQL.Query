<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOAddCustomerAddrInfoCSQL">
			<desc><![CDATA[AddCustomerAddrInfo]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CUST_ADDR
    (CUST_CNT_CD,
     CUST_SEQ,
	 ADDR_TP_CD,
     ADDR_SEQ,
	 BZET_ADDR,
     PRMRY_CHK_FLG,
     DELT_FLG,
     CNT_CD,
     STE_CD,
     CTY_NM,
     ZIP_CD,
     CRE_USR_ID,
     UPD_USR_ID,
     CRE_DT,
     UPD_DT
    )
VALUES (@[cust_cnt_cd],
        @[cust_seq],
		@[addr_tp_cd],
        NVL((SELECT MAX(ADDR_SEQ)+1 
        FROM MDM_CUST_ADDR 
        WHERE CUST_CNT_CD = @[cust_cnt_cd]
        AND CUST_SEQ = @[cust_seq]
        AND ADDR_TP_CD = @[addr_tp_cd]
        GROUP BY CUST_CNT_CD,CUST_SEQ,ADDR_TP_CD ),1),
		@[bzet_addr],
        @[prmry_chk_flg],
        @[delt_flg],
        @[cnt_cd],
        @[ste_cd],
        @[cty_nm],
        @[zip_cd],
        @[cre_usr_id],
        @[upd_usr_id],
        SYSDATE,
        SYSDATE
       )			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="addr_tp_cd" type="12" value="" out="N"/>
				<param name="bzet_addr" type="12" value="" out="N"/>
				<param name="prmry_chk_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="cty_nm" type="12" value="" out="N"/>
				<param name="zip_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
