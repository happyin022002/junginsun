<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOModifyRepCommodityUSQL">
			<desc><![CDATA[2011.03.03 조인영 Rep Commodity 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_REP_CMDT SET
       REP_CMDT_NM        = @[rep_cmdt_nm]
      ,GRP_CMDT_CD        = @[grp_cmdt_cd]
      ,UPD_USR_ID         = @[upd_usr_id]     
      ,UPD_DT             = sysdate           
      ,DELT_FLG           = @[delt_flg]       
WHERE REP_CMDT_CD         = @[rep_cmdt_cd]			]]></sql>
			<params>
				<param name="rep_cmdt_nm" type="12" value="" out="N"/>
				<param name="grp_cmdt_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="rep_cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
