<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountDBDAOSearchChargeRqstRSQL">
			<desc><![CDATA[Request Charge Code 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CHG_CD
       ,CHG_NM
       ,FRT_CHG_TP_CD
       ,CO_CHG_ACCT_CD
       ,REP_CHG_CD
       ,CHG_REV_TP_CD
       ,CHG_APLY_TP_CD
       ,CHG_APLY_AREA_CD
       ,CY_RD_TERM_FLG
       ,CFS_RD_TERM_FLG
       ,DOR_RD_TERM_FLG
       ,NA_RD_TERM_FLG
       ,TKL_TML_FLG
       ,APLY_SVC_MOD_FLG
       ,USE_CO_TP_CD
       ,AUTO_RAT_FLG
       ,CHG_EDI_CD
       ,DP_SEQ
       ,CHG_STS_CD
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
       ,DELT_FLG
       ,EAI_EVNT_DT
       ,EAI_IF_ID
	   ,TAX_FLG
	   ,TAX_CNT_CD
 FROM MDM_CHG_RQST
 WHERE RQST_NO = @[rqst_no]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
