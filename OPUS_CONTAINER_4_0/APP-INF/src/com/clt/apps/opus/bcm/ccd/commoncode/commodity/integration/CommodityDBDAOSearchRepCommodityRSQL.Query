<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOSearchRepCommodityRSQL">
			<desc><![CDATA[2011.03.03 조인영 Rep Commodity 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT REP_CMDT_CD
      ,REP_CMDT_NM
      ,GRP_CMDT_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,DELT_FLG
FROM   MDM_REP_CMDT
WHERE  REP_CMDT_CD = @[rep_cmdt_cd]			]]></sql>
			<params>
				<param name="rep_cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
