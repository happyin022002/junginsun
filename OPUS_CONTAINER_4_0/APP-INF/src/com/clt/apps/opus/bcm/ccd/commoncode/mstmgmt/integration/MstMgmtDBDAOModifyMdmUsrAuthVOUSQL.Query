<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAOModifyMdmUsrAuthVOUSQL">
			<desc><![CDATA[MdmUsrAuthVO 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_USR_AUTH
   SET OFC_CD = @[ofc_cd],
	   AUTH_TP_CD = @[auth_tp_cd],
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
 WHERE USR_ID = @[usr_id]
   AND MST_DAT_SUBJ_CD = @[mst_dat_subj_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="auth_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="mst_dat_subj_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
