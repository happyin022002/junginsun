<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchCntrVndrClssRSQL">
			<desc><![CDATA[입력된 Vendor 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ
    ,VNDR_COST_CD
    ,CNTR_VNDR_SVC_CD
    ,DELT_FLG
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
FROM MDM_CNTR_VNDR_CLSS
WHERE 1=1
AND VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
