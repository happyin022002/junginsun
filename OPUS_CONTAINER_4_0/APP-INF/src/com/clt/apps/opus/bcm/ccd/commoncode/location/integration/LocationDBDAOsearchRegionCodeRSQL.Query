<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchRegionCodeRSQL">
			<desc><![CDATA[Region 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT RGN_CD
	  , RGN_NM
	  , CNT_CD
	  , DELT_FLG
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , UPD_DT
FROM MDM_REGION
WHERE RGN_CD = @[rgn_cd]			]]></sql>
			<params>
				<param name="rgn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
