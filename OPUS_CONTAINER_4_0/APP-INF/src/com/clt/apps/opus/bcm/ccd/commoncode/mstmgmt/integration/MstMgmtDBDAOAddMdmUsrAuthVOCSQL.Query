<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAOAddMdmUsrAuthVOCSQL">
			<desc><![CDATA[MdmUsrAuthVO 정보를 추가한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_USR_AUTH (
    USR_ID,    
    MST_DAT_SUBJ_CD,
    OFC_CD,
    AUTH_TP_CD,
    PGM_ID,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
) VALUES (
	@[usr_id],
    @[mst_dat_subj_cd],
    @[ofc_cd],
    @[auth_tp_cd],
    (SELECT INTG_CD_VAL_DESC
  	   FROM COM_INTG_CD_DTL
 	  WHERE INTG_CD_ID='CD20091'
   		AND INTG_CD_VAL_CTNT = @[mst_dat_subj_cd]),
    @[cre_usr_id],
    NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE),
    @[upd_usr_id],
    NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
)			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="mst_dat_subj_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="auth_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
