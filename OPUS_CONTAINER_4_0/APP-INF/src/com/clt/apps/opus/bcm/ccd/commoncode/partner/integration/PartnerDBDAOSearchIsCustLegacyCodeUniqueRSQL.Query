<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchIsCustLegacyCodeUniqueRSQL">
			<desc><![CDATA[Customer Legacy Code가 Unique한지 조회한다.]]></desc>
			<sql><![CDATA[
SELECT MODI_CUST_CD 
  FROM MDM_CUSTOMER
 WHERE 1=1
#if (${cust_cnt_cd} != '' && ${cust_seq} != '')
   AND CUST_CNT_CD||LPAD(CUST_SEQ, 6, '0')<> @[cust_cnt_cd]||LPAD(@[cust_seq], 6, '0')
#end
   AND MODI_CUST_CD = @[modi_cust_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="modi_cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
