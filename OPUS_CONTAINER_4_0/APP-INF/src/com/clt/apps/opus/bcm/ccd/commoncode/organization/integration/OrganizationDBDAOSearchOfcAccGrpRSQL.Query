<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OrganizationDBDAOSearchOfcAccGrpRSQL">
			<desc><![CDATA[입력한 Office Access Group Creation 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SUB_SYS_CD
      ,OFC_GRP_ID
      ,OFC_GRP_NM
      ,OFC_GRP_DESC
      ,CRE_USR_ID
	  ,TO_CHAR(CRE_DT,'YYYY-MM-DD') CRE_DT
      ,UPD_USR_ID
      ,TO_CHAR(UPD_DT,'YYYY-MM-DD') UPD_DT
FROM MDM_OFC_GRP
WHERE 1=1
AND SUB_SYS_CD = @[sub_sys_cd]
	#if(${ofc_grp_id} != '')
	AND OFC_GRP_ID = @[ofc_grp_id]
	#end

ORDER BY OFC_GRP_ID			]]></sql>
			<params>
				<param name="sub_sys_cd" type="12" value="" out="N"/>
				<param name="ofc_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
