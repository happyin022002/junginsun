<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOModifyTrdCodeUSQL">
			<desc><![CDATA[trd code 수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_TRADE
   SET TRD_NM = @[trd_nm]
      ,VSL_TP_CD = @[vsl_tp_cd]
      ,FM_CONTI_CD = @[fm_conti_cd]
      ,TO_CONTI_CD = @[to_conti_cd]
      ,OFC_CD = @[ofc_cd]
      ,ST_EFF_DT = TO_DATE(@[st_eff_dt], 'YYYY-MM-DD')
      ,END_EFF_DT = TO_DATE(@[end_eff_dt], 'YYYY-MM-DD')
	  ,MODI_COST_CTR_CD = @[modi_cost_ctr_cd]
      ,DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
 WHERE TRD_CD = @[trd_cd]			]]></sql>
			<params>
				<param name="trd_nm" type="12" value="AAA" out="N"/>
				<param name="vsl_tp_cd" type="12" value="" out="N"/>
				<param name="fm_conti_cd" type="12" value="" out="N"/>
				<param name="to_conti_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="st_eff_dt" type="12" value="" out="N"/>
				<param name="end_eff_dt" type="12" value="" out="N"/>
				<param name="modi_cost_ctr_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="ZZZ" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
