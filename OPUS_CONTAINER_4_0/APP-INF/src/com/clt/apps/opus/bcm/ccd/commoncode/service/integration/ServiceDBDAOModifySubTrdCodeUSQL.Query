<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOModifySubTrdCodeUSQL">
			<desc><![CDATA[sub trd code 수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_SUB_TRD
   SET SUB_TRD_NM = @[sub_trd_nm]
      ,TRD_CD = @[trd_cd]
      ,DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
 WHERE SUB_TRD_CD = @[sub_trd_cd]			]]></sql>
			<params>
				<param name="sub_trd_nm" type="12" value="AA" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="N" out="N"/>
				<param name="user_id" type="12" value="ZZZZ" out="N"/>
				<param name="sub_trd_cd" type="12" value="ZZ" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
