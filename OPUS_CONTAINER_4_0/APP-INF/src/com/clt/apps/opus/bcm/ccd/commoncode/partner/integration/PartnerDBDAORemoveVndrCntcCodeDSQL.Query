<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAORemoveVndrCntcCodeDSQL">
			<desc><![CDATA[입력된 Vendor Contact 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
 DELETE FROM MDM_VNDR_CNTC_PNT
 WHERE 1=1
 AND VNDR_SEQ = @[vndr_seq]
 AND VNDR_CNTC_PNT_SEQ = @[vndr_cntc_pnt_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vndr_cntc_pnt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
