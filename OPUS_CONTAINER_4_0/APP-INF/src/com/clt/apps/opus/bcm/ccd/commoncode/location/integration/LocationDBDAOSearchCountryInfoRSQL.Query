<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOSearchCountryInfoRSQL">
			<desc><![CDATA[Country Info Search]]></desc>
			<sql><![CDATA[
SELECT  RE.CNT_CD
       ,CO.SCONTI_CD
       ,SU.CONTI_CD
  FROM  MDM_REGION RE
       ,MDM_COUNTRY CO
       ,MDM_SUBCONTINENT SU
 WHERE  1 = 1
   AND  RE.RGN_CD = @[rgn_cd]
   AND  RE.CNT_CD = CO.CNT_CD
   AND  CO.SCONTI_CD = SU.SCONTI_CD			]]></sql>
			<params>
				<param name="rgn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
