<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OrganizationDBDAOAddOfcAccGrpCSQL">
			<desc><![CDATA[입력 및 조회된 Office Access Group Creation 정보를 추가 및 수정한다.(UI_CCD_0041)]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_OFC_GRP(
            SUB_SYS_CD
           ,OFC_GRP_ID 
           ,OFC_GRP_NM
           ,OFC_GRP_DESC
           ,CRE_USR_ID
           ,CRE_DT
           ,UPD_USR_ID
           ,UPD_DT
           )
    VALUES(@[sub_sys_cd]
   			,(SELECT NVL(TRIM(TO_CHAR(MAX(OFC_GRP_ID)+1,'000000')),'000001') 
   				FROM MDM_OFC_GRP
  			   WHERE SUB_SYS_CD = @[sub_sys_cd]
			)
          ,@[ofc_grp_nm]
          ,@[ofc_grp_desc]
          ,@[cre_usr_id]
          ,SYSDATE
          ,@[upd_usr_id]
          ,SYSDATE
          )			]]></sql>
			<params>
				<param name="sub_sys_cd" type="12" value="" out="N"/>
				<param name="ofc_grp_nm" type="12" value="" out="N"/>
				<param name="ofc_grp_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
