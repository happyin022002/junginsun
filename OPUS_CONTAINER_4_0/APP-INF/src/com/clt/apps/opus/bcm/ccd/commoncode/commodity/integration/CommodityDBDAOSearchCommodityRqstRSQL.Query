<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOSearchCommodityRqstRSQL">
			<desc><![CDATA[Request Commodity 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CMDT_CD 
      ,CMDT_NM
      ,REP_IMDG_LVL_CD
      ,REP_CMDT_CD
      ,GRP_CMDT_CD
      ,FMC_EXP_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,DELT_FLG
      ,EU_XPT_FLG
FROM   MDM_CMDT_RQST
WHERE  RQST_NO = @[rqst_no]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
