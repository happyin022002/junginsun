<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchScpCodeRSQL">
			<desc><![CDATA[scp 코드 조회]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
	  ,SVC_SCP_NM
      ,SVC_SCP_BND_CD
      ,CONF_FLG
      ,FMC_FILE_FLG
      ,TRF_PFX_CD
      ,TRF_NO
      ,DELT_FLG
	  ,MODI_COST_CTR_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,MODI_SVC_GRP_CD
      ,DMNT_FLG
 FROM MDM_SVC_SCP
WHERE SVC_SCP_CD = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="ACE" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
