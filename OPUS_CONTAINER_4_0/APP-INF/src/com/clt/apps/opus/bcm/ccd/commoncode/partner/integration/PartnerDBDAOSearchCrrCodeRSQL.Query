<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchCrrCodeRSQL">
			<desc><![CDATA[Carrier code를 조회한다]]></desc>
			<sql><![CDATA[
SELECT
	CRR_CD
, 	CRR_NM
,   BKG_EDI_CD
,   MAPG_CRR_CD
,   DELT_FLG
,   CRE_USR_ID
,   CRE_DT
,   UPD_USR_ID
,   UPD_DT
,   EAI_EVNT_DT
,   EAI_IF_ID
FROM MDM_CARRIER
WHERE CRR_CD = @[crr_cd]			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
