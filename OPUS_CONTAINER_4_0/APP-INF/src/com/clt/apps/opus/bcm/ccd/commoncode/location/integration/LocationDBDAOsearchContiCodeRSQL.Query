<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchContiCodeRSQL">
			<desc><![CDATA[Continent 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT CONTI_CD
	  , CONTI_NM
	  , DELT_FLG
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , UPD_DT
FROM MDM_CONTINENT
WHERE CONTI_CD = @[conti_cd]			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
