<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOModifyScpLaneCodeUSQL">
			<desc><![CDATA[scp code lane 정보 수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_SVC_SCP_LANE
   SET DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND VSL_SLAN_CD = @[vsl_slan_cd]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="N" out="N"/>
				<param name="user_id" type="12" value="ZZZZ" out="N"/>
				<param name="svc_scp_cd" type="12" value="ZZZ" out="N"/>
				<param name="vsl_slan_cd" type="12" value="AAA" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
