<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchSlaneCodeRSQL">
			<desc><![CDATA[VSL Code 상세 내용 조회]]></desc>
			<sql><![CDATA[
SELECT VSL.VSL_SLAN_CD 
	  ,VSL.VSL_SLAN_NM
      ,VSL.VSL_SVC_TP_CD
      ,VSL.VSL_TP_CD
      ,VSL.FDR_DIV_CD
      ,TO_CHAR(TO_DATE(VSL.ST_EFF_DT, 'YYYYMMDD'), 'YYYY-MM-DD') ST_EFF_DT
      ,TO_CHAR(TO_DATE(VSL.END_EFF_DT, 'YYYYMMDD'), 'YYYY-MM-DD') END_EFF_DT
      ,VSL.CO_CD
      ,VSL.DELT_FLG
      ,DIR.VSL_SLAN_DIR_CD
      ,DIR.VSL_SLAN_DIR_SEQ
      ,DIR.DELT_FLG DIR_DELT_FLG
      ,VSL.MODI_VSL_SLAN_CD
      ,VSL.MODI_COST_CTR_CD
      ,VSL.MODI_VSL_SLAN_CD2
      ,VSL.MODI_VIP_TEAM_CD
      ,DIR.MODI_VSL_SLAN_DIR_CD
#if(${dir_flg} == 'Y')
      ,DIR.CRE_USR_ID
      ,DIR.CRE_DT
      ,DIR.UPD_USR_ID
      ,DIR.UPD_DT
#else
      ,VSL.CRE_USR_ID
      ,VSL.CRE_DT
      ,VSL.UPD_USR_ID
      ,VSL.UPD_DT
#end
  FROM MDM_VSL_SVC_LANE VSL
      ,MDM_VSL_SVC_LANE_DIR DIR
 WHERE VSL.VSL_SLAN_CD = DIR.VSL_SLAN_CD(+)
   AND VSL.VSL_SLAN_CD = @[vsl_slan_cd]
 ORDER BY DIR_DELT_FLG, DIR.VSL_SLAN_DIR_SEQ			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="CGF" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
