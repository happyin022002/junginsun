<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOremoveZoneDtlCodeDSQL">
			<desc><![CDATA[zone code detail 정보를 삭제합니다.]]></desc>
			<sql><![CDATA[
DELETE FROM MDM_ZN_DTL
WHERE ZN_CD = @[zn_cd]
AND ZN_SEQ = @[zn_seq]			]]></sql>
			<params>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="zn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
