<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOModifyMoreInfoCrCustUSQL">
			<desc><![CDATA[ModifyMoreInfoCrCust]]></desc>
			<sql><![CDATA[
UPDATE MDM_CR_CUST
SET    CR_AMT = @[cr_amt]
     , CR_CLT_OFC_CD = @[cr_clt_ofc_cd]
     , IB_CR_TERM_DYS = @[ib_cr_term_dys]
     , OB_CR_TERM_DYS = @[ob_cr_term_dys]
     , UPD_USR_ID = @[user_id]
     , UPD_DT = SYSDATE
WHERE CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))			]]></sql>
			<params>
				<param name="cr_amt" type="12" value="" out="N"/>
				<param name="cr_clt_ofc_cd" type="12" value="" out="N"/>
				<param name="ib_cr_term_dys" type="12" value="" out="N"/>
				<param name="ob_cr_term_dys" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
