<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAORemoveScpLimitRqstDSQL">
			<desc><![CDATA[Request scp code limit 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MDM_SVC_SCP_LMT_RQST
 WHERE RQST_NO = @[rqst_no]
   AND RGN_CD = @[rgn_cd]
   AND ORG_DEST_CD = @[org_dest_cd]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="org_dest_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
