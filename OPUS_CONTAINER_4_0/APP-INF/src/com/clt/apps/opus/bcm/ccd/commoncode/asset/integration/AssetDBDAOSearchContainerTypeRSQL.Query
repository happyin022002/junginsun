<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AssetDBDAOSearchContainerTypeRSQL">
			<desc><![CDATA[2011.02.24 조인영 Container Type 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CNTR_TP_CD
       ,CNTR_TP_DESC
       ,MODI_CNTR_TP_CD
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
       ,DELT_FLG
       ,EAI_EVNT_DT
       ,EAI_IF_ID
FROM MDM_CNTR_TP
WHERE CNTR_TP_CD = @[cntr_tp_cd]			]]></sql>
			<params>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
