<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOModifyScpLaneRqstUSQL">
			<desc><![CDATA[Request scp code lane 정보 수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_SVC_SCP_LANE_RQST
   SET DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
 WHERE RQST_NO = @[rqst_no]
   AND VSL_SLAN_CD = @[vsl_slan_cd]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
