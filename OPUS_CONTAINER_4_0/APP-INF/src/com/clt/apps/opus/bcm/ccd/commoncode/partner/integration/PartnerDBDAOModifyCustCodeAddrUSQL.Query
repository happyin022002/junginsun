<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOModifyCustCodeAddrUSQL">
			<desc><![CDATA[MDM_CUST_ADDR 테이블에 정보 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUST_ADDR
SET 
	BZET_ADDR = @[bzet_addr]
,   BZET_NM   = @[cust_lgl_eng_nm]
,   CNT_CD    = @[cust_cnt_cd]
WHERE  CUST_CNT_CD    = @[cust_cnt_cd]
AND CUST_SEQ          = @[cust_seq]
AND ADDR_TP_CD        = '1'
AND ADDR_SEQ          = @[addr_seq]
AND PRMRY_CHK_FLG     = 'Y'			]]></sql>
			<params>
				<param name="bzet_addr" type="12" value="" out="N"/>
				<param name="cust_lgl_eng_nm" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="addr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
