<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOMergeMdmCustFrmVndrCSQL">
			<desc><![CDATA[PartnerDBDAOMergeMdmCustFrmVndrCSQL]]></desc>
			<sql><![CDATA[
MERGE INTO MDM_CUSTOMER A
 USING ( SELECT @[cust_cnt_cd] CUST_CNT_CD, @[cust_seq] CUST_SEQ FROM DUAL ) B
 ON (A.CUST_CNT_CD = B.CUST_CNT_CD AND A.CUST_SEQ = B.CUST_SEQ )
 WHEN MATCHED THEN
 UPDATE SET
	  CNTR_DIV_FLG = 'Y',
	  BLK_DIV_FLG = 'N',
	  UPD_USR_ID  = @[upd_usr_id],
	  UPD_DT      = SYSDATE,
	  CUST_RGST_NO = @[cust_rgst_no],
	  CUST_LGL_ENG_NM  = SUBSTRB(@[cust_lgl_eng_nm], 1, 100),
	  CUST_LOCL_LANG_NM  = @[cust_locl_lang_nm],
	  DELT_FLG = DECODE(NVL(@[ib_flag], 'U'), 'D', 'Y', 'N')
 WHEN NOT MATCHED THEN
 INSERT
	 (
		  CUST_CNT_CD,
		  CUST_SEQ,
		  CNTR_DIV_FLG,
		  BLK_DIV_FLG,
		  CUST_LGL_ENG_NM,
		  CUST_LOCL_LANG_NM,
		  OFC_CD,
		  CUST_RGST_NO,
		  LOC_CD,
		  VNDR_SEQ,
		  MODI_CUST_CNT_CD,
		  MODI_CUST_SEQ,
		  CRE_USR_ID,
		  CRE_DT,
		  UPD_USR_ID,
		  UPD_DT,
		  DELT_FLG
	 )
	 VALUES
	 (
		  @[cust_cnt_cd],
		  @[cust_seq],
		  'Y',
		  'N',
		  SUBSTRB(@[cust_lgl_eng_nm], 1, 100),
		  @[cust_locl_lang_nm],
		  @[ofc_cd],
		  @[cust_rgst_no],
		  @[loc_cd],
		  @[vndr_seq],
		  @[modi_cust_cnt_cd],
		  @[modi_cust_seq],
		  @[cre_usr_id],
		  SYSDATE,
		  @[upd_usr_id],
		  SYSDATE,
		  'N'
	 )			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cust_rgst_no" type="12" value="" out="N"/>
				<param name="cust_lgl_eng_nm" type="12" value="" out="N"/>
				<param name="cust_locl_lang_nm" type="12" value="" out="N"/>
				<param name="ib_flag" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="modi_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="modi_cust_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
