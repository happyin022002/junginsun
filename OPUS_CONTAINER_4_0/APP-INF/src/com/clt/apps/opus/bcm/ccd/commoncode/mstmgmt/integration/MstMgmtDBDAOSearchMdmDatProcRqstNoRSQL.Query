<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAOSearchMdmDatProcRqstNoRSQL">
			<desc><![CDATA[MDM_DAT_PROC 테이블의 RQST_NO 를 채번한다.]]></desc>
			<sql><![CDATA[
SELECT UPPER(@[mst_dat_subj_cd]) || 'C' || TO_CHAR(SYSDATE, 'YYYYMMDD') ||
        (
        SELECT LPAD( NVL(MAX(TO_NUMBER(SUBSTR(RQST_NO, 14))) + 1, 1) , 6, '0')
        FROM MDM_DAT_PROC
        WHERE INSTR(RQST_NO, UPPER(@[mst_dat_subj_cd])||'C'||TO_CHAR(SYSDATE, 'YYYYMMDD')) > 0
        ) RQST_NO
FROM DUAL			]]></sql>
			<params>
				<param name="mst_dat_subj_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
