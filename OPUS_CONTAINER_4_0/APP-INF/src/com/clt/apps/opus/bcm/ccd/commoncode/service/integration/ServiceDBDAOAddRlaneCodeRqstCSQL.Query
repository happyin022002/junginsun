<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOAddRlaneCodeRqstCSQL">
			<desc><![CDATA[Request rlane code 생성]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_REV_LANE_RQST 
           (RQST_NO
		   ,RLANE_CD
           ,RLANE_NM
           ,VSL_TP_CD
           ,REP_TRD_CD
           ,VSL_SLAN_CD
           ,CRE_USR_ID
           ,CRE_DT
           ,UPD_USR_ID
           ,UPD_DT
           ,DELT_FLG
           )
VALUES(@[rqst_no]
	  ,@[rlane_cd]
      ,@[rlane_nm]
      ,@[vsl_tp_cd]
      ,@[rep_trd_cd]
      ,@[vsl_slan_cd]
      ,@[user_id]
      ,SYSDATE
      ,@[user_id]
      ,SYSDATE
      ,@[delt_flg]
      )			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="rlane_nm" type="12" value="" out="N"/>
				<param name="vsl_tp_cd" type="12" value="" out="N"/>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
