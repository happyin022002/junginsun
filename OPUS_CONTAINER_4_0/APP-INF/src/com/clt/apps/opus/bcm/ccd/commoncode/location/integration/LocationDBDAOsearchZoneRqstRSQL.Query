<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchZoneRqstRSQL">
			<desc><![CDATA[Request Zone master 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT ZN_CD,
	   ZN_NM,
	   CGO_HNDL_TM_HRS,
	   TZTM_HRS,
	   REP_YD_CD,
	   LNK_DIST,
	   DIST_UT_CD,
	   DELT_FLG
FROM MDM_ZN_RQST
WHERE RQST_NO = @[rqst_no]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
