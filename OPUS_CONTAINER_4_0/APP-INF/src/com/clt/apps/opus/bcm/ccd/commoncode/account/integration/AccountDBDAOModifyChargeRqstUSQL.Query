<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountDBDAOModifyChargeRqstUSQL">
			<desc><![CDATA[Request Charge Code 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_CHG_RQST SET
       CHG_NM = @[chg_nm]
      ,FRT_CHG_TP_CD = @[frt_chg_tp_cd]
      ,CO_CHG_ACCT_CD = @[co_chg_acct_cd]
      ,REP_CHG_CD = @[rep_chg_cd]
      ,CHG_REV_TP_CD = @[chg_rev_tp_cd]
      ,CHG_APLY_TP_CD = @[chg_aply_tp_cd]
      ,CHG_APLY_AREA_CD = @[chg_aply_area_cd]
      ,CY_RD_TERM_FLG = @[cy_rd_term_flg]
      ,CFS_RD_TERM_FLG = @[cfs_rd_term_flg]
      ,DOR_RD_TERM_FLG = @[dor_rd_term_flg]
      ,NA_RD_TERM_FLG = @[na_rd_term_flg]
      ,TKL_TML_FLG = @[tkl_tml_flg]
      ,APLY_SVC_MOD_FLG = @[aply_svc_mod_flg]
      ,USE_CO_TP_CD = @[use_co_tp_cd]
      ,AUTO_RAT_FLG = @[auto_rat_flg]
      ,CHG_EDI_CD = @[chg_edi_cd]
      ,DP_SEQ = @[dp_seq]
      ,CHG_STS_CD = @[chg_sts_cd]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = sysdate 
      ,DELT_FLG = @[delt_flg]
      ,EAI_IF_ID = @[eai_if_id]
	  ,TAX_FLG = @[tax_flg]
	  ,TAX_CNT_CD = @[tax_cnt_cd]
WHERE  RQST_NO = @[rqst_no]			]]></sql>
			<params>
				<param name="chg_nm" type="12" value="" out="N"/>
				<param name="frt_chg_tp_cd" type="12" value="" out="N"/>
				<param name="co_chg_acct_cd" type="12" value="" out="N"/>
				<param name="rep_chg_cd" type="12" value="" out="N"/>
				<param name="chg_rev_tp_cd" type="12" value="" out="N"/>
				<param name="chg_aply_tp_cd" type="12" value="" out="N"/>
				<param name="chg_aply_area_cd" type="12" value="" out="N"/>
				<param name="cy_rd_term_flg" type="12" value="" out="N"/>
				<param name="cfs_rd_term_flg" type="12" value="" out="N"/>
				<param name="dor_rd_term_flg" type="12" value="" out="N"/>
				<param name="na_rd_term_flg" type="12" value="" out="N"/>
				<param name="tkl_tml_flg" type="12" value="" out="N"/>
				<param name="aply_svc_mod_flg" type="12" value="" out="N"/>
				<param name="use_co_tp_cd" type="12" value="" out="N"/>
				<param name="auto_rat_flg" type="12" value="" out="N"/>
				<param name="chg_edi_cd" type="12" value="" out="N"/>
				<param name="dp_seq" type="12" value="" out="N"/>
				<param name="chg_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="tax_flg" type="12" value="" out="N"/>
				<param name="tax_cnt_cd" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
