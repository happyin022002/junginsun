<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchScpLaneRqstRSQL">
			<desc><![CDATA[Request ScpLaneCode 조회]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
	  ,VSL_SLAN_CD
      ,DELT_FLG
  FROM MDM_SVC_SCP_LANE_RQST
 WHERE RQST_NO = @[rqst_no]
 ORDER BY DELT_FLG, VSL_SLAN_CD			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
