        ,ALTN_CURR_DIV_CD = @[altn_curr_div_cd]
		,MODI_COST_CTR_CD = @[modi_cost_ctr_cd]
        ,SPRT_EML_INV_FLG = @[sprt_eml_inv_flg]
WHERE 1 = 1
AND OFC_CD = @[ofc_cd]			]]></sql>
