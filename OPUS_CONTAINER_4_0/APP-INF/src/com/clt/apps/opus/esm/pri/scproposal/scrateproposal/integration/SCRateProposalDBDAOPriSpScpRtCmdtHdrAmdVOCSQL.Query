<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAOPriSpScpRtCmdtHdrAmdVOCSQL">
			<desc><![CDATA[SCRateProposalDBDAOPriSpScpRtCmdtHdrAmdVOCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO pri_sp_scp_rt_cmdt_hdr(

    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    GEN_SPCL_RT_TP_CD,
    CMDT_HDR_SEQ,
    N1ST_CMNC_AMDT_SEQ,
    BLET_DP_SEQ,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
SELECT
    prop_no             ,
    amdt_seq+1          ,
    svc_scp_cd          ,
    gen_spcl_rt_tp_cd   ,
    cmdt_hdr_seq        ,
    n1st_cmnc_amdt_seq	,
    blet_dp_seq			,
    @[cre_usr_id]       ,
    SYSDATE             ,
    @[upd_usr_id]       ,
    SYSDATE
FROM 
    pri_sp_scp_rt_cmdt_hdr hdr
WHERE
    prop_no     = @[prop_no]
AND amdt_seq    = @[amdt_seq]
AND EXISTS ( 
    SELECT 'OK'
    FROM    
        pri_sp_scp_rt
    WHERE   
        prop_no             = hdr.prop_no
    AND amdt_seq            = hdr.amdt_seq
    AND svc_scp_cd          = hdr.svc_scp_cd
    AND gen_spcl_rt_tp_cd   = hdr.gen_spcl_rt_tp_cd
    AND cmdt_hdr_seq        = hdr.cmdt_hdr_seq
	AND src_info_cd			<> 'AD'   
)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
