<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSetupDBDAOSearchCntrTpSzListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD,
       CNTR_TPSZ_DESC
  FROM MDM_CNTR_TP_SZ
 WHERE NVL (DELT_FLG, 'N') = 'N'
 UNION ALL
SELECT INTG_CD_VAL_CTNT AS CNTR_TPSZ_CD, 
       INTG_CD_VAL_DP_DESC AS CNTR_TPSZ_DESC 
  FROM COM_INTG_CD_DTL
 WHERE 1=1
   AND INTG_CD_ID = 'CD03155'
ORDER BY CNTR_TPSZ_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
