<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOSearchChangedDGRSQL">
			<desc><![CDATA[Check DG is changed or not]]></desc>
			<sql><![CDATA[
SELECT BKG_NO 

FROM BKG_DG_CGO
WHERE 0=0
AND BKG_NO = @[bkg_no]
AND DCGO_SEQ = @[dcgo_seq]

AND CNTR_VOL_QTY = @[cntr_vol_qty]
AND CNTR_TPSZ_CD = @[cntr_tpsz_cd]

#if (${imdg_clss_cd}== '')
AND IMDG_CLSS_CD IS NULL
#else
AND IMDG_CLSS_CD = @[imdg_clss_cd]
#end
#if (${imdg_un_no}== '')
AND IMDG_UN_NO IS NULL
#else
AND IMDG_UN_NO = @[imdg_un_no]
#end
#if (${imdg_un_no_seq}== '')
AND IMDG_UN_NO_SEQ IS NULL
#else
AND IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
#end
#if (${prp_shp_nm}== '')
AND PRP_SHP_NM IS NULL
#else
AND PRP_SHP_NM = @[prp_shp_nm]
#end
#if (${hzd_desc}== '')
AND HZD_DESC IS NULL
#else
AND HZD_DESC = @[hzd_desc]
#end
#if (${grs_wgt}== '')
AND GRS_WGT IS NULL
#else
AND GRS_WGT = @[grs_wgt]
#end
#if (${net_wgt}== '')
AND NET_WGT IS NULL
#else
AND NET_WGT = @[net_wgt]
#end
#if (${imdg_pck_grp_cd}== '')
AND IMDG_PCK_GRP_CD IS NULL
#else
AND IMDG_PCK_GRP_CD = @[imdg_pck_grp_cd]
#end
#if (${psa_no}== '')
AND PSA_NO IS NULL
#else
AND PSA_NO = @[psa_no]
#end
#if (${flsh_pnt_cdo_temp}== '')
AND FLSH_PNT_CDO_TEMP IS NULL
#else
AND FLSH_PNT_CDO_TEMP = @[flsh_pnt_cdo_temp]
#end
#if (${ems_no}== '')
AND EMS_NO IS NULL
#else
AND EMS_NO = @[ems_no]
#end
#if (${mrn_polut_flg}== '')
AND MRN_POLUT_FLG IS NULL
#else
AND MRN_POLUT_FLG = @[mrn_polut_flg]
#end
#if (${emer_cntc_phn_no_ctnt}== '')
AND EMER_CNTC_PHN_NO_CTNT IS NULL
#else
AND EMER_CNTC_PHN_NO_CTNT = @[emer_cntc_phn_no_ctnt]
#end
#if (${emer_cntc_pson_nm}== '')
AND EMER_CNTC_PSON_NM IS NULL
#else
AND EMER_CNTC_PSON_NM = @[emer_cntc_pson_nm]
#end
#if (${certi_no}== '')
AND CERTI_NO IS NULL
#else
AND CERTI_NO = @[certi_no]
#end
#if (${net_explo_wgt}== '')
AND NET_EXPLO_WGT IS NULL
#else
AND NET_EXPLO_WGT = @[net_explo_wgt]
#end
#if (${out_imdg_pck_qty1}== '')
AND OUT_IMDG_PCK_QTY1 IS NULL
#else
AND OUT_IMDG_PCK_QTY1 = @[out_imdg_pck_qty1]
#end
#if (${out_imdg_pck_cd1}== '')
AND OUT_IMDG_PCK_CD1 IS NULL
#else
AND OUT_IMDG_PCK_CD1 = @[out_imdg_pck_cd1]
#end
#if (${out_imdg_pck_qty2}== '')
AND OUT_IMDG_PCK_QTY2 IS NULL
#else
AND OUT_IMDG_PCK_QTY2 = @[out_imdg_pck_qty2]
#end
#if (${out_imdg_pck_cd2}== '')
AND OUT_IMDG_PCK_CD2 IS NULL
#else
AND OUT_IMDG_PCK_CD2 = @[out_imdg_pck_cd2]
#end
#if (${in_imdg_pck_qty1}== '')
AND IN_IMDG_PCK_QTY1 IS NULL
#else
AND IN_IMDG_PCK_QTY1 = @[in_imdg_pck_qty1]
#end
#if (${in_imdg_pck_cd1}== '')
AND IN_IMDG_PCK_CD1 IS NULL
#else
AND IN_IMDG_PCK_CD1 = @[in_imdg_pck_cd1]
#end
#if (${in_imdg_pck_qty2}== '')
AND IN_IMDG_PCK_QTY2 IS NULL
#else
AND IN_IMDG_PCK_QTY2 = @[in_imdg_pck_qty2]
#end
#if (${in_imdg_pck_cd2}== '')
	AND IN_IMDG_PCK_CD2 IS NULL
#else
	AND IN_IMDG_PCK_CD2 = @[in_imdg_pck_cd2]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dcgo_seq" type="12" value="" out="N"/>
				<param name="cntr_vol_qty" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
				<param name="prp_shp_nm" type="12" value="" out="N"/>
				<param name="hzd_desc" type="12" value="" out="N"/>
				<param name="grs_wgt" type="12" value="" out="N"/>
				<param name="net_wgt" type="12" value="" out="N"/>
				<param name="imdg_pck_grp_cd" type="12" value="" out="N"/>
				<param name="psa_no" type="12" value="" out="N"/>
				<param name="flsh_pnt_cdo_temp" type="12" value="" out="N"/>
				<param name="ems_no" type="12" value="" out="N"/>
				<param name="mrn_polut_flg" type="12" value="" out="N"/>
				<param name="emer_cntc_phn_no_ctnt" type="12" value="" out="N"/>
				<param name="emer_cntc_pson_nm" type="12" value="" out="N"/>
				<param name="certi_no" type="12" value="" out="N"/>
				<param name="net_explo_wgt" type="12" value="" out="N"/>
				<param name="out_imdg_pck_qty1" type="12" value="" out="N"/>
				<param name="out_imdg_pck_cd1" type="12" value="" out="N"/>
				<param name="out_imdg_pck_qty2" type="12" value="" out="N"/>
				<param name="out_imdg_pck_cd2" type="12" value="" out="N"/>
				<param name="in_imdg_pck_qty1" type="12" value="" out="N"/>
				<param name="in_imdg_pck_cd1" type="12" value="" out="N"/>
				<param name="in_imdg_pck_qty2" type="12" value="" out="N"/>
				<param name="in_imdg_pck_cd2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
