<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchKorMainSndCountRSQL">
			<desc><![CDATA[한국세관 Main 테이블에 해당 BKG에 대한 Data가 전송되었는지 확인한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM BKG_CSTMS_KR_BL
 WHERE BKG_NO = @[bkg_no]
  
   AND CSTMS_DECL_TP_CD = @[kcd_tp]
   AND DMST_PORT_CD = @[kt_port]
   AND TRNS_SEQ = @[kt_seq]
   AND MF_SND_DT IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="kcd_tp" type="12" value="" out="N"/>
				<param name="kt_port" type="12" value="" out="N"/>
				<param name="kt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
