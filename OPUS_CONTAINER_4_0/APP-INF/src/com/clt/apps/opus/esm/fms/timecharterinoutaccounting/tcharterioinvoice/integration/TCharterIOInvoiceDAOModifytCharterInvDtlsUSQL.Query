<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOModifytCharterInvDtlsUSQL">
			<desc><![CDATA[TCharterIOInvoiceDAOModifytCharterInvDtlsUSQL]]></desc>
			<sql><![CDATA[
update fms_inv_dtl
   set acct_cd = @[acct_cd],
	   acct_itm_seq = @[acct_itm_seq],
	   curr_cd = @[curr_cd],
	   inv_amt = @[inv_amt],
	   chtr_inv_dt = @[chtr_inv_dt],
	   inv_desc = @[inv_desc],
	   to_inv_no = @[to_inv_no],
	   vsl_cd = substr(@[bunker_vvd],1,4),
	   skd_voy_no = substr(@[bunker_vvd],5,4),
	   skd_dir_cd = substr(@[bunker_vvd],9,1),
	   rev_dir_cd = substr(@[bunker_vvd],10,1),
	   upd_usr_id = @[upd_usr_id],
	   upd_dt = sysdate
 where flet_ctrt_no = @[flet_ctrt_no]
   and flet_iss_tp_cd = 'CHT'
   and inv_seq = @[inv_seq]
   and inv_dtl_seq = @[inv_dtl_seq]			]]></sql>
			<params>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="acct_itm_seq" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="inv_amt" type="3" value="" out="N"/>
				<param name="chtr_inv_dt" type="12" value="" out="N"/>
				<param name="inv_desc" type="12" value="" out="N"/>
				<param name="to_inv_no" type="12" value="" out="N"/>
				<param name="bunker_vvd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="inv_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
