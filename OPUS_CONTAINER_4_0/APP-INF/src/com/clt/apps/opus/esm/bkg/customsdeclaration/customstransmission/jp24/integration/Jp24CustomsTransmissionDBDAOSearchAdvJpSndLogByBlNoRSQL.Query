<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24CustomsTransmissionDBDAOSearchAdvJpSndLogByBlNoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(SND XAK1BKG_CSTMS_ADV_JP_SND_LOG) */
       SND.BL_NO,
       '' AS HBL_NO,
       SND.VSL_CD||SND.SKD_VOY_NO||SND.SKD_DIR_CD AS VVD,
       SND.POL_CD,
       (SELECT LOC.LOC_NM
          FROM MDM_LOCATION LOC
         WHERE LOC.LOC_CD = SND.POL_CD) AS POL_NM,
       SND.POD_CD,
       (SELECT LOC.LOC_NM
          FROM MDM_LOCATION LOC
         WHERE LOC.LOC_CD = SND.POD_CD) AS POD_NM,
       SND.POR_CD,
       SND.EDI_SND_MSG_CTNT,
       (SELECT IB_CSSM_VOY_NO
          FROM BKG_CSTMS_ADV_JP_VSL_SKD
         WHERE VSL_CD = SND.VSL_CD
           AND SKD_VOY_NO = SND.SKD_VOY_NO
           AND SKD_DIR_CD = SND.SKD_DIR_CD
           AND POL_CD = SND.POL_CD) AS IB_CSSM_VOY_NO,
       (SELECT S.CUST_NM
          FROM BKG_CUSTOMER S,
               BKG_BOOKING BKG
         WHERE S.BKG_NO = BKG.BKG_NO
           AND BKG.BL_NO = SND.BL_NO
           AND S.BKG_CUST_TP_CD = 'S') AS SHPR_NM,
       (SELECT C.CUST_NM
          FROM BKG_CUSTOMER C,
               BKG_BOOKING BKG
         WHERE C.BKG_NO = BKG.BKG_NO
           AND BKG.BL_NO = SND.BL_NO
           AND C.BKG_CUST_TP_CD = 'C') AS CNEE_NM

  FROM BKG_CSTMS_ADV_JP_SND_LOG SND

 WHERE BL_NO = @[bl_no]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
