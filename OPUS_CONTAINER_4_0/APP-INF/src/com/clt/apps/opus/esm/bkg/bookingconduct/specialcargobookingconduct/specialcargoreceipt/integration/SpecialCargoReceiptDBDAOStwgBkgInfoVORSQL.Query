<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOStwgBkgInfoVORSQL">
			<desc><![CDATA[StwgBkgInfoVO]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y') 

SELECT BK.BKG_NO
,   BK.POR_CD
,   BK.DEL_CD
,	BK.RCV_TERM_CD
,	BK.DE_TERM_CD
,	BL.BDR_FLG
,	BL.CORR_NO
FROM BKG_BKG_HIS BK
,BKG_BL_DOC BL
WHERE BK.BKG_NO = @[bkg_no]
AND   BK.CORR_NO = 'TMP0000001'

AND BK.BKG_NO = BL.BKG_NO(+)

#else

SELECT BK.BKG_NO
,   BK.POR_CD
,   BK.DEL_CD
,	BK.RCV_TERM_CD
,	BK.DE_TERM_CD
,	BL.BDR_FLG
,	BL.CORR_NO
FROM BKG_BOOKING BK
,BKG_BL_DOC BL
WHERE BK.BKG_NO = @[bkg_no]

AND BK.BKG_NO = BL.BKG_NO(+)
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
