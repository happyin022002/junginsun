<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSearchTradeCheckRSQL">
			<desc><![CDATA[SearchTradeCheck]]></desc>
			<sql><![CDATA[
select DECODE(COUNT(*),0,'N','Y') TRADE_YN
from mdm_trade
WHERE TRD_CD=@[trd_cd]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
