<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusManageDBDAOCreateTransferCsqQtaPotnMgmtCSQL">
			<desc><![CDATA[Qta Transfer 시 연간 정보로 1Q CSQ_QTA_POTN_MGMT 생성]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_QTA_POTN_MGMT (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,QTA_STEP_CD
      ,QTA_VER_NO
      ,RHQ_CD
      ,RGN_OFC_CD
      ,TRD_CD
      ,RLANE_CD
      ,CONV_DIR_CD
      ,DIR_CD
      ,SUB_TRD_CD
      ,GID_LOD_POTN_RTO
      ,GID_REV_POTN_RTO
      ,LOD_POTN_RTO
      ,REV_POTN_RTO
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT 'Q'    AS BSE_TP_CD
      ,BSE_YR AS BSE_YR
      ,'1Q'   AS BSE_QTR_CD
      ,OFC_VW_CD
      ,QTA_STEP_CD
      ,SUBSTR(QTA_VER_NO, 1, 11) || '1Q' || SUBSTR(QTA_VER_NO, -2) AS QTA_VER_NO
      ,RHQ_CD
      ,RGN_OFC_CD
      ,TRD_CD
      ,RLANE_CD
      ,CONV_DIR_CD
      ,DIR_CD
      ,SUB_TRD_CD
      ,GID_LOD_POTN_RTO
      ,GID_REV_POTN_RTO
      ,LOD_POTN_RTO
      ,REV_POTN_RTO
      ,@[f_usr_id] AS CRE_USR_ID
      ,SYSDATE     AS CRE_DT
      ,@[f_usr_id] AS UPD_USR_ID
      ,SYSDATE     AS UPD_DT
  FROM CSQ_QTA_POTN_MGMT
 WHERE BSE_TP_CD  = 'Y'
   AND BSE_YR     = @[f_bse_yr]
   AND BSE_QTR_CD = '00'			]]></sql>
			<params>
				<param name="f_usr_id" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
