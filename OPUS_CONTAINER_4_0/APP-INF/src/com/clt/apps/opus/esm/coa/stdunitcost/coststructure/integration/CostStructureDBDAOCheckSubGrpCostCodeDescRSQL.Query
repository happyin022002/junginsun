<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOCheckSubGrpCostCodeDescRSQL">
			<desc><![CDATA[Sub Group Cost Code Desc 가 동일한 Sub Group Cost Code 있는지 check]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM COA_SUB_GRP_COST

 WHERE UPPER(SGRP_COST_CD_DESC) = UPPER(@[sgrp_cost_cd_desc])			]]></sql>
			<params>
				<param name="sgrp_cost_cd_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
