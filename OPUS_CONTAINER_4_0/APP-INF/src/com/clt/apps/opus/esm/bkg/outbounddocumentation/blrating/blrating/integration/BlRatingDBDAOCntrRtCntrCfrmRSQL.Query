<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCntrRtCntrCfrmRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
SELECT BKG_DOC_PROC_TP_CD
FROM (SELECT BKG_DOC_PROC_TP_CD
,            ROW_NUMBER() OVER(PARTITION BY BKG_NO ORDER BY UPD_DT DESC) ROW_NO
      FROM   BKG_DOC_PROC_SKD_HIS
      WHERE  BKG_NO = @[bkg_no]
      AND    CORR_NO = 'TMP0000001'
      AND    BKG_DOC_PROC_TP_CD IN ('CNTCFM', 'CNTRLS')
      AND    DOC_PERF_DELT_FLG = 'N')
WHERE  ROW_NO = 1
#else 
SELECT BKG_DOC_PROC_TP_CD
FROM (SELECT BKG_DOC_PROC_TP_CD
,            ROW_NUMBER() OVER(PARTITION BY BKG_NO ORDER BY UPD_DT DESC) ROW_NO
      FROM   BKG_DOC_PROC_SKD
      WHERE  BKG_NO = @[bkg_no]
      AND    BKG_DOC_PROC_TP_CD IN ('CNTCFM', 'CNTRLS')
      AND    DOC_PERF_DELT_FLG = 'N')
WHERE  ROW_NO = 1
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="KORY9025044" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
