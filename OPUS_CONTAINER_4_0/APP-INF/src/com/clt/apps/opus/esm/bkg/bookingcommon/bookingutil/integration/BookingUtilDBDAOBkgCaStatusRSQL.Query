<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOBkgCaStatusRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT decode(nvl(corr_no, 'N'),'N','N','Y') ca_flg
FROM bkg_bl_doc
WHERE	bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
