<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaManifestListDownloadDBDAOsearchFormSettingByOfcCdRSQL">
			<desc><![CDATA[INDIA 에서 세관에 적하목록을 신고하기 위한 Cover Form Setting 하는 operation]]></desc>
			<sql><![CDATA[
SELECT 
    OFC_CD
    ,HDR_CTNT
    ,FTR_CTNT
    ,DECL_ADDR
    ,BOD_CTNT
FROM BKG_CSTMS_IDA_PRN_FOM
WHERE OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
