<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOaddHoldByCntrCSQL">
			<desc><![CDATA[Container 단위로 Hold 하기 위한 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_IB_CGO_RLSE_HLD_CNTR
(
    BKG_NO
   ,CNTR_NO
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
) 
VALUES
(
       @[bkg_no]                --  AS BKG_NO
      ,@[cntr_no]               --  AS CNTR_NO
      ,@[cre_usr_id]            --  AS CRE_USR_ID
      ,SYSDATE                  --  AS CRE_DT
      ,@[upd_usr_id]            --  AS UPD_USR_ID
      ,SYSDATE                  --  AS UPD_DT   
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
