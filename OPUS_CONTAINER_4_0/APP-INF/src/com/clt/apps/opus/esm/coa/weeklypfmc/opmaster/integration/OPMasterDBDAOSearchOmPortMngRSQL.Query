<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPMasterDBDAOSearchOmPortMngRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT EFF_FM_DT
  	  ,EFF_TO_DT
  	  ,LOC_CD
      ,DECODE(RLANE_CD,'XXXXX','',RLANE_CD) RLANE_CD			--20150612.MOD	
      ,DECODE(DIR_CD,'X','',DIR_CD) DIR_CD
--,CRE_USR_ID		VO용 나중 지우기..
--,UPD_USR_ID
   FROM COA_MON_VVD_PORT_EXPT
  WHERE 1=1
#if (${eff_fm_dt} != '')
    AND @[eff_fm_dt] BETWEEN EFF_FM_DT AND EFF_TO_DT
     OR EFF_FM_DT >= @[eff_fm_dt]
#end
  ORDER BY RLANE_CD, DIR_CD, LOC_CD, EFF_FM_DT, EFF_TO_DT			]]></sql>
			<params>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
