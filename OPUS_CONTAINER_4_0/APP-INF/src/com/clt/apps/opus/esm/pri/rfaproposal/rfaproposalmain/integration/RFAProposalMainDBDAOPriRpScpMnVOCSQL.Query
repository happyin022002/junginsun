<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpScpMnVOCSQL">
			<desc><![CDATA[PRI RP SCP MN 생성]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_MN (
	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	EFF_DT
,	EXP_DT
,	PROP_SCP_SREP_CD
,	PROP_SCP_OFC_CD
,	PROP_SCP_STS_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,   TGT_MVC_QTY
,   CNTR_LOD_UT_CD
) VALUES( 
	@[prop_no]
,	@[amdt_seq]
,	@[svc_scp_cd]
,	TO_DATE(@[eff_dt],'YYYY-MM-DD')
,	TO_DATE(@[exp_dt],'YYYY-MM-DD')
,	@[prop_scp_srep_cd]
,	@[prop_scp_ofc_cd]
,	@[prop_scp_sts_cd]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	@[tgt_mvc_qty]
,	@[cntr_lod_ut_cd]
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="prop_scp_srep_cd" type="12" value="" out="N"/>
				<param name="prop_scp_ofc_cd" type="12" value="" out="N"/>
				<param name="prop_scp_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="tgt_mvc_qty" type="12" value="" out="N"/>
				<param name="cntr_lod_ut_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
