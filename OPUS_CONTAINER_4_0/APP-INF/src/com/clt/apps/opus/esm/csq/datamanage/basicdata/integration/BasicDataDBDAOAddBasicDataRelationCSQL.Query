<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOAddBasicDataRelationCSQL">
			<desc><![CDATA[CSQ_DAT_RLT Data Insert]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_DAT_RLT (
     BSE_TP_CD
    ,BSE_YR
    ,BSE_QTR_CD
    ,OFC_VW_CD
    ,TRD_CD
    ,CONV_DIR_CD
    ,RHQ_CD
    ,TEAM_CD
    ,OB_DIV_CD
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
) VALUES (
     @[bse_tp_cd]
    ,@[bse_yr]
    ,DECODE(@[bse_tp_cd], 'Y', '00', @[bse_qtr_cd])
    ,@[ofc_vw_cd]
    ,@[trd_cd]
    ,@[conv_dir_cd]
    ,@[rhq_cd]
    ,@[team_cd]
    ,@[ob_div_cd]
    ,@[cre_usr_id]
    ,SYSDATE
    ,@[upd_usr_id]
    ,SYSDATE
)			]]></sql>
			<params>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="ofc_vw_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="conv_dir_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="team_cd" type="12" value="" out="N"/>
				<param name="ob_div_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
