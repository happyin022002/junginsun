<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyCustomizedConditionDBDAOGetMonthlyCustomizedConditionLaneBoundStatus0163Tab02RSQL">
			<desc><![CDATA[status 조회]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_DESC AS STATUS       
  FROM COM_INTG_CD_DTL,                 
       (                                
         SELECT DISTINCT COND_STS_CD    
           FROM SAQ_MON_DIR_CONV     
          WHERE BSE_YR     = @[bse_yr]          
            AND BSE_QTR_CD = @[bse_qtr_cd]  )       
 WHERE INTG_CD_ID       = 'CD01387'     
   AND INTG_CD_VAL_CTNT = COND_STS_CD			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
