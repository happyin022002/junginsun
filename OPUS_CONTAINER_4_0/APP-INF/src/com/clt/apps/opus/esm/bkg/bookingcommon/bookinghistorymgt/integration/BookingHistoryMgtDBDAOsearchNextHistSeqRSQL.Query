<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOsearchNextHistSeqRSQL">
			<desc><![CDATA[Booking History Master 시퀀스를 조회한다]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(HIS_SEQ), 0) + 1 AS HIS_SEQ
  FROM BKG_HIS_MST
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
