<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchFACSeqRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchFACSeqRSQL]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(FAC_SEQ),0)+1 FAC_SEQ
  FROM ACM_FAC_COMM 
 WHERE  BKG_NO = @[bkg_no] 
	AND FAC_STS_CD = 'CM'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
