<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchOfmHeaderRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', sysdate, 'USNYC'),'ddmmrrhh24miss') ls_date,
 'USACUS'||RPAD(@[vvd],9,' ')||RPAD(@[pod],5,' ')||RPAD(@[ofc],6,' ')||RPAD(@[usr],10,' ')||
 LTRIM(TO_CHAR(bkg_cstm_edi_seq.nextval,'00009'),' ')||
 RPAD(TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', sysdate, 'USNYC'),'yyyymmddhh24miss'),14,' ')||RPAD(@[tmpstr3],2,' ')||'                       '||CHR(10) buf6
FROM DUAL			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
				<param name="ofc" type="12" value="" out="N"/>
				<param name="usr" type="12" value="" out="N"/>
				<param name="tmpstr3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
