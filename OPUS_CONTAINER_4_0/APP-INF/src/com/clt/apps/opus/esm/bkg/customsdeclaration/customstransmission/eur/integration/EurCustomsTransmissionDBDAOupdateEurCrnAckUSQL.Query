<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOupdateEurCrnAckUSQL">
			<desc><![CDATA[스페인 로컬 시스템에서 생성하여 사용 하는 CRN 값을 ALPS로 수신 받아 업데이트 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_EUR_CRN_RCV
SET  MF_NO = @[mf_no]
	,REF_GDS_ITM_NM = @[ref_gds_itm_nm]
    ,UPD_USR_ID = (
					SELECT NVL(MAX(MSG_FUNC_ID), 'U1')
					FROM BKG_CSTMS_EUR_CRN_RCV
				    WHERE CNT_CD = @[cnt_cd]
					AND MSG_SND_OFC_CD = @[msg_snd_ofc_cd]
					AND BL_NO = @[bl_no]
					AND MSG_FUNC_ID LIKE 'U%'
				  )
    ,UPD_DT = SYSDATE
WHERE CNT_CD = @[cnt_cd]
AND MSG_SND_OFC_CD = @[msg_snd_ofc_cd]
AND BL_NO = @[bl_no]
AND MSG_FUNC_ID = 'F'			]]></sql>
			<params>
				<param name="mf_no" type="12" value="" out="N"/>
				<param name="ref_gds_itm_nm" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="msg_snd_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
