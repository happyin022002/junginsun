<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManilaManifestListDownloadDBDAOsearchVvdDtlRSQL">
			<desc><![CDATA[ManilaSearchVvdDtlVO]]></desc>
			<sql><![CDATA[
SELECT REG_NUMBER,
       ETADT,
       VNAME,
       POLCD,
       DISCHARGE,
       G_TONG_WGT,
       N_TONG_WGT,
       ROWNUM AS SEQ

  FROM (SELECT DISTINCT
               (SELECT MDM.UN_LOC_CD
                  FROM MDM_LOCATION MDM
                 WHERE MDM.LOC_CD = (SELECT /*+ INDEX_DESC(SKD XAK7VSK_VSL_PORT_SKD) */
                                            SKD.VPS_PORT_CD
                                       FROM VSK_VSL_PORT_SKD SKD
                                      WHERE VPS.VSL_CD = SKD.VSL_CD
                                        AND VPS.SKD_VOY_NO = SKD.SKD_VOY_NO
                                        AND VPS.SKD_DIR_CD = SKD.SKD_DIR_CD
                                        AND VPS.CLPT_SEQ > SKD.CLPT_SEQ
                                        AND SKD.VPS_PORT_CD NOT LIKE 'PH%'
                                        AND ROWNUM = 1 )) AS POLCD,
               TO_CHAR(VPS.VPS_ETA_DT, 'YYYY-MM-DD') AS ETADT,
               '' AS DISCHARGE,
               RPAD(BKG.VSL_CD||BKG.SKD_VOY_NO||BKG.SKD_DIR_CD, 15, ' ') AS VNAME,
               VSL.GRS_RGST_TONG_WGT AS G_TONG_WGT,
               NET_RGST_TONG_WGT AS N_TONG_WGT,
               @[reg_no] AS REG_NUMBER

         FROM  VSK_VSL_PORT_SKD VPS,
               MDM_VSL_CNTR VSL,
               BKG_VVD BKG

         WHERE VSL.VSL_CD = BKG.VSL_CD
           AND VPS.VSL_CD = BKG.VSL_CD
           AND VPS.SKD_VOY_NO = BKG.SKD_VOY_NO
           AND VPS.SKD_DIR_CD = BKG.SKD_DIR_CD
#if (${vsl_cd}!= '')
           AND BKG.VSL_CD = @[vsl_cd]
#end
#if (${skd_voy_no}!= '')
           AND BKG.SKD_VOY_NO = @[skd_voy_no]
#end
#if (${skd_dir_cd}!= '')
           AND BKG.SKD_DIR_CD = @[skd_dir_cd]
#end
#if (${pol_cd}!= '')
           AND BKG.POL_CD LIKE @[pol_cd]
#end
#if (${pod_cd}!= '')
           AND BKG.POD_CD LIKE @[pod_cd]
           AND VPS.VPS_PORT_CD LIKE @[pod_cd]
#end
#if (${date_fm} != '' && ${date_to} != '')
           AND VPS.VPS_ETA_DT BETWEEN TO_DATE(@[date_fm], 'YYYY-MM-DD') AND TO_DATE(@[date_to], 'YYYY-MM-DD') + 0.9999
#end
       )			]]></sql>
			<params>
				<param name="reg_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="date_fm" type="12" value="" out="N"/>
				<param name="date_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
