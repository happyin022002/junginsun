<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltRtListVerticalExcelVORSQL">
			<desc><![CDATA[Rate 엑셀 조회]]></desc>
			<sql><![CDATA[
SELECT DECODE(B.ROUT_DP_SEQ, 1, A.BLET_DP_SEQ, NULL) AS CMDT_DP_SEQ
      ,A.PRC_CMDT_DEF_CD
      ,A.PRC_CMDT_DEF_NM
      ,A.CUST_SEQ
      ,A.CUST_LGL_ENG_NM
      ,B.ROUT_DP_SEQ
      ,B.ORG_ROUT_PNT_LOC_DEF_CD
      ,B.ORG_ROUT_PNT_LOC_DEF_NM
      ,B.ORG_RCV_DE_TERM_NM
      ,B.ORG_PRC_TRSP_MOD_NM
      ,B.ORG_ROUT_VIA_PORT_DEF_CD
      ,B.DEST_ROUT_VIA_PORT_DEF_CD
      ,B.DEST_ROUT_PNT_LOC_DEF_CD
      ,B.DEST_ROUT_PNT_LOC_DEF_NM
      ,B.DEST_RCV_DE_TERM_NM
      ,B.DEST_PRC_TRSP_MOD_NM
      ,B.RAT_UT_CD
      ,B.PRC_CGO_TP_CD
      ,B.CURR_CD
      ,B.PROP_FRT_RT_AMT
      ,0 BUC_USD_AMT
      ,0 IFC_USD_AMT
      ,0 PSC_USD_AMT
  FROM (SELECT R.BLET_DP_SEQ, S.CMDT_ROWKEY, B.PRC_CMDT_DEF_CD, B.PRC_CMDT_DEF_NM, C.CUST_SEQ, C.CUST_LGL_ENG_NM
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,CMDT_HDR_SEQ
                      ,LPAD(CMDT_HDR_SEQ, 5, '0') || '|' ||
                       LPAD(ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ ORDER BY NULL), 5, '0') AS CMDT_ROWKEY
                  FROM PRI_RP_SCP_RT_CMDT
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND SRC_INFO_CD <> 'AD'
                UNION
                SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,CMDT_HDR_SEQ
                      ,LPAD(CMDT_HDR_SEQ, 5, '0') || '|' ||
                       LPAD(ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ ORDER BY NULL), 5, '0') AS CMDT_ROWKEY
                  FROM PRI_RP_SCP_RT_ACT_CUST
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND SRC_INFO_CD <> 'AD') S
              ,PRI_RP_SCP_RT_CMDT_HDR R
              ,(SELECT T.CMDT_HDR_SEQ
                      ,T.PRC_CMDT_DEF_CD
                      ,DECODE(T.PRC_CMDT_TP_CD
                             ,'G'
                             ,(SELECT PRC_GRP_CMDT_DESC
                                FROM PRI_RP_SCP_GRP_CMDT
                               WHERE PROP_NO = T.PROP_NO
                                 AND AMDT_SEQ = T.AMDT_SEQ
                                 AND SVC_SCP_CD = T.SVC_SCP_CD
                                 AND PRC_GRP_CMDT_CD = T.PRC_CMDT_DEF_CD
                                 AND ROWNUM = 1)
                             ,'R'
                             ,(SELECT REP_CMDT_NM
                                FROM MDM_REP_CMDT
                               WHERE REP_CMDT_CD = T.PRC_CMDT_DEF_CD
                                 AND ROWNUM = 1)
                             ,'C'
                             ,(SELECT CMDT_NM
                                FROM MDM_COMMODITY
                               WHERE CMDT_CD = T.PRC_CMDT_DEF_CD
                                 AND ROWNUM = 1)) AS PRC_CMDT_DEF_NM
                      ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' ||
                       LPAD(ROW_NUMBER() OVER(PARTITION BY T.PROP_NO
                                ,T.AMDT_SEQ
                                ,T.SVC_SCP_CD
                                ,T.CMDT_HDR_SEQ ORDER BY DECODE(T.PRC_CMDT_TP_CD, 'G', 1, 'R', 2, 'C', 3, 99)
                                ,T.PRC_CMDT_DEF_CD)
                           ,5
                           ,'0') AS CMDT_ROWKEY
                  FROM PRI_RP_SCP_RT_CMDT T
                 WHERE T.PROP_NO = @[prop_no]
                   AND T.AMDT_SEQ = @[amdt_seq]
                   AND T.SVC_SCP_CD = @[svc_scp_cd]
                   AND T.SRC_INFO_CD <> 'AD') B
              ,(SELECT T.CMDT_HDR_SEQ
                      ,T.CUST_CNT_CD || LPAD(T.CUST_SEQ, 6, '0') AS CUST_SEQ
                      ,(SELECT CUST_LGL_ENG_NM
                          FROM MDM_CUSTOMER
                         WHERE CUST_CNT_CD = T.CUST_CNT_CD
                           AND CUST_SEQ = T.CUST_SEQ
                           AND ROWNUM = 1) AS CUST_LGL_ENG_NM
                      ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(ROW_NUMBER() OVER(PARTITION BY T.PROP_NO
                                                                       ,T.AMDT_SEQ
                                                                       ,T.SVC_SCP_CD
                                                                       ,T.CMDT_HDR_SEQ ORDER BY ACT_CUST_SEQ)
                                                                  ,5
                                                                  ,'0') AS CMDT_ROWKEY
                  FROM PRI_RP_SCP_RT_ACT_CUST T
                 WHERE T.PROP_NO = @[prop_no]
                   AND T.AMDT_SEQ = @[amdt_seq]
                   AND T.SVC_SCP_CD = @[svc_scp_cd]
                   AND T.SRC_INFO_CD <> 'AD') C
         WHERE S.PROP_NO = R.PROP_NO
           AND S.AMDT_SEQ = R.AMDT_SEQ
           AND S.SVC_SCP_CD = R.SVC_SCP_CD
           AND S.CMDT_HDR_SEQ = R.CMDT_HDR_SEQ
           AND S.CMDT_ROWKEY = B.CMDT_ROWKEY(+)
           AND S.CMDT_ROWKEY = C.CMDT_ROWKEY(+)
         ORDER BY S.CMDT_ROWKEY) A
  FULL OUTER JOIN (SELECT R.BLET_DP_SEQ
                         ,LPAD(S.CMDT_HDR_SEQ, 5, '0') || '|' ||
                          LPAD(ROW_NUMBER() OVER(PARTITION BY S.CMDT_HDR_SEQ ORDER BY S.ROUT_ROWKEY), 5, '0') AS CMDT_ROWKEY
                         ,A.ROUT_DP_SEQ
                         ,B.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD
                         ,B.ROUT_PNT_LOC_DEF_NM AS ORG_ROUT_PNT_LOC_DEF_NM
                         ,B.RCV_DE_TERM_NM AS ORG_RCV_DE_TERM_NM
                         ,B.PRC_TRSP_MOD_NM AS ORG_PRC_TRSP_MOD_NM
                         ,C.ROUT_VIA_PORT_DEF_CD AS ORG_ROUT_VIA_PORT_DEF_CD
                         ,D.ROUT_VIA_PORT_DEF_CD AS DEST_ROUT_VIA_PORT_DEF_CD
                         ,E.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD
                         ,E.ROUT_PNT_LOC_DEF_NM AS DEST_ROUT_PNT_LOC_DEF_NM
                         ,E.RCV_DE_TERM_NM AS DEST_RCV_DE_TERM_NM
                         ,E.PRC_TRSP_MOD_NM AS DEST_PRC_TRSP_MOD_NM
                         ,F.RAT_UT_CD
                         ,F.PRC_CGO_TP_CD
                         ,F.CURR_CD
                         ,F.PROP_FRT_RT_AMT
                     FROM (SELECT PROP_NO
                                 ,AMDT_SEQ
                                 ,SVC_SCP_CD
                                 ,CMDT_HDR_SEQ
                                 ,ROUT_SEQ
                                 ,LPAD(CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER()
                                       OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY NULL)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_PNT
                            WHERE PROP_NO = @[prop_no]
                              AND AMDT_SEQ = @[amdt_seq]
                              AND SVC_SCP_CD = @[svc_scp_cd]
                              AND ORG_DEST_TP_CD = 'O'
                              AND SRC_INFO_CD <> 'AD'
                           UNION
                           SELECT PROP_NO
                                 ,AMDT_SEQ
                                 ,SVC_SCP_CD
                                 ,CMDT_HDR_SEQ
                                 ,ROUT_SEQ
                                 ,LPAD(CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER()
                                       OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY NULL)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_VIA
                            WHERE PROP_NO = @[prop_no]
                              AND AMDT_SEQ = @[amdt_seq]
                              AND SVC_SCP_CD = @[svc_scp_cd]
                              AND ORG_DEST_TP_CD = 'O'
                              AND SRC_INFO_CD <> 'AD'
                           UNION
                           SELECT PROP_NO
                                 ,AMDT_SEQ
                                 ,SVC_SCP_CD
                                 ,CMDT_HDR_SEQ
                                 ,ROUT_SEQ
                                 ,LPAD(CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER()
                                       OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY NULL)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_VIA
                            WHERE PROP_NO = @[prop_no]
                              AND AMDT_SEQ = @[amdt_seq]
                              AND SVC_SCP_CD = @[svc_scp_cd]
                              AND ORG_DEST_TP_CD = 'D'
                              AND SRC_INFO_CD <> 'AD'
                           UNION
                           SELECT PROP_NO
                                 ,AMDT_SEQ
                                 ,SVC_SCP_CD
                                 ,CMDT_HDR_SEQ
                                 ,ROUT_SEQ
                                 ,LPAD(CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER()
                                       OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY NULL)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_PNT
                            WHERE PROP_NO = @[prop_no]
                              AND AMDT_SEQ = @[amdt_seq]
                              AND SVC_SCP_CD = @[svc_scp_cd]
                              AND ORG_DEST_TP_CD = 'D'
                              AND SRC_INFO_CD <> 'AD'
                           UNION
                           SELECT PROP_NO
                                 ,AMDT_SEQ
                                 ,SVC_SCP_CD
                                 ,CMDT_HDR_SEQ
                                 ,ROUT_SEQ
                                 ,LPAD(CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER()
                                       OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY NULL)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT
                            WHERE PROP_NO = @[prop_no]
                              AND AMDT_SEQ = @[amdt_seq]
                              AND SVC_SCP_CD = @[svc_scp_cd]
                              AND SRC_INFO_CD <> 'AD') S
                         ,PRI_RP_SCP_RT_CMDT_HDR R
                         ,(SELECT ROW_NUMBER() OVER(PARTITION BY T.PROP_NO, T.AMDT_SEQ, T.SVC_SCP_CD, T.CMDT_HDR_SEQ ORDER BY T.ROUT_SEQ) AS ROUT_DP_SEQ
                                 ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(T.ROUT_SEQ, 5, '0') || '|' || '00001' AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_CMDT_ROUT T
                            WHERE T.PROP_NO = @[prop_no]
                              AND T.AMDT_SEQ = @[amdt_seq]
                              AND T.SVC_SCP_CD = @[svc_scp_cd]) A
                         ,(SELECT T.CMDT_HDR_SEQ
                                 ,T.ROUT_SEQ
                                 ,T.ROUT_PNT_LOC_DEF_CD
                                 ,DECODE(ROUT_PNT_LOC_TP_CD
                                        ,'G'
                                        ,(SELECT PRC_GRP_LOC_DESC
                                           FROM PRI_RP_SCP_GRP_LOC
                                          WHERE PROP_NO = T.PROP_NO
                                            AND AMDT_SEQ = T.AMDT_SEQ
                                            AND SVC_SCP_CD = T.SVC_SCP_CD
                                            AND PRC_GRP_LOC_CD = T.ROUT_PNT_LOC_DEF_CD
                                            AND ROWNUM = 1)
                                        ,'L'
                                        ,(SELECT LOC_NM
                                           FROM MDM_LOCATION
                                          WHERE LOC_CD = T.ROUT_PNT_LOC_DEF_CD
                                            AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM
                                 ,(SELECT INTG_CD_VAL_DP_DESC
                                     FROM COM_INTG_CD_DTL
                                    WHERE INTG_CD_ID = 'CD02138'
                                      AND INTG_CD_VAL_CTNT = T.RCV_DE_TERM_CD
                                      AND ROWNUM = 1) AS RCV_DE_TERM_NM
                                 ,(SELECT INTG_CD_VAL_DP_DESC
                                     FROM COM_INTG_CD_DTL
                                    WHERE INTG_CD_ID = 'CD01720'
                                      AND INTG_CD_VAL_CTNT = T.PRC_TRSP_MOD_CD
                                      AND ROWNUM = 1) AS PRC_TRSP_MOD_NM
                                 ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(T.ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER() OVER(PARTITION BY T.PROP_NO
                                           ,T.AMDT_SEQ
                                           ,T.SVC_SCP_CD
                                           ,T.CMDT_HDR_SEQ
                                           ,T.ROUT_SEQ ORDER BY DECODE(T.ROUT_PNT_LOC_TP_CD, 'G', 1, 'L', 2, 99)
                                           ,T.ROUT_PNT_LOC_DEF_CD)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_PNT T
                            WHERE T.PROP_NO = @[prop_no]
                              AND T.AMDT_SEQ = @[amdt_seq]
                              AND T.SVC_SCP_CD = @[svc_scp_cd]
                              AND T.ORG_DEST_TP_CD = 'O'
                              AND T.SRC_INFO_CD <> 'AD') B
                         ,(SELECT T.CMDT_HDR_SEQ
                                 ,T.ROUT_SEQ
                                 ,T.ROUT_VIA_PORT_DEF_CD
                                 ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(T.ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER() OVER(PARTITION BY T.PROP_NO
                                           ,T.AMDT_SEQ
                                           ,T.SVC_SCP_CD
                                           ,T.CMDT_HDR_SEQ
                                           ,T.ROUT_SEQ ORDER BY DECODE(T.ROUT_VIA_PORT_TP_CD, 'G', 1, 'L', 2, 99)
                                           ,T.ROUT_VIA_PORT_DEF_CD)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_VIA T
                            WHERE T.PROP_NO = @[prop_no]
                              AND T.AMDT_SEQ = @[amdt_seq]
                              AND T.SVC_SCP_CD = @[svc_scp_cd]
                              AND T.ORG_DEST_TP_CD = 'O'
                              AND T.SRC_INFO_CD <> 'AD') C
                         ,(SELECT T.CMDT_HDR_SEQ
                                 ,T.ROUT_SEQ
                                 ,T.ROUT_VIA_PORT_DEF_CD
                                 ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(T.ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER() OVER(PARTITION BY T.PROP_NO
                                           ,T.AMDT_SEQ
                                           ,T.SVC_SCP_CD
                                           ,T.CMDT_HDR_SEQ
                                           ,T.ROUT_SEQ ORDER BY DECODE(T.ROUT_VIA_PORT_TP_CD, 'G', 1, 'L', 2, 99)
                                           ,T.ROUT_VIA_PORT_DEF_CD)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_VIA T
                            WHERE T.PROP_NO = @[prop_no]
                              AND T.AMDT_SEQ = @[amdt_seq]
                              AND T.SVC_SCP_CD = @[svc_scp_cd]
                              AND T.ORG_DEST_TP_CD = 'D'
                              AND T.SRC_INFO_CD <> 'AD') D
                         ,(SELECT T.CMDT_HDR_SEQ
                                 ,T.ROUT_SEQ
                                 ,T.ROUT_PNT_LOC_DEF_CD
                                 ,DECODE(ROUT_PNT_LOC_TP_CD
                                        ,'G'
                                        ,(SELECT PRC_GRP_LOC_DESC
                                           FROM PRI_RP_SCP_GRP_LOC
                                          WHERE PROP_NO = T.PROP_NO
                                            AND AMDT_SEQ = T.AMDT_SEQ
                                            AND SVC_SCP_CD = T.SVC_SCP_CD
                                            AND PRC_GRP_LOC_CD = T.ROUT_PNT_LOC_DEF_CD
                                            AND ROWNUM = 1)
                                        ,'L'
                                        ,(SELECT LOC_NM
                                           FROM MDM_LOCATION
                                          WHERE LOC_CD = T.ROUT_PNT_LOC_DEF_CD
                                            AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM
                                 ,(SELECT INTG_CD_VAL_DP_DESC
                                     FROM COM_INTG_CD_DTL
                                    WHERE INTG_CD_ID = 'CD02139'
                                      AND INTG_CD_VAL_CTNT = T.RCV_DE_TERM_CD
                                      AND ROWNUM = 1) AS RCV_DE_TERM_NM
                                 ,(SELECT INTG_CD_VAL_DP_DESC
                                     FROM COM_INTG_CD_DTL
                                    WHERE INTG_CD_ID = 'CD01720'
                                      AND INTG_CD_VAL_CTNT = T.PRC_TRSP_MOD_CD
                                      AND ROWNUM = 1) AS PRC_TRSP_MOD_NM
                                 ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(T.ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER() OVER(PARTITION BY T.PROP_NO
                                           ,T.AMDT_SEQ
                                           ,T.SVC_SCP_CD
                                           ,T.CMDT_HDR_SEQ
                                           ,T.ROUT_SEQ ORDER BY DECODE(T.ROUT_PNT_LOC_TP_CD, 'G', 1, 'L', 2, 99)
                                           ,T.ROUT_PNT_LOC_DEF_CD)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT_ROUT_PNT T
                            WHERE T.PROP_NO = @[prop_no]
                              AND T.AMDT_SEQ = @[amdt_seq]
                              AND T.SVC_SCP_CD = @[svc_scp_cd]
                              AND T.ORG_DEST_TP_CD = 'D'
                              AND T.SRC_INFO_CD <> 'AD') E
                         ,(SELECT T.CMDT_HDR_SEQ
                                 ,T.ROUT_SEQ
                                 ,T.RAT_UT_CD
                                 ,T.PRC_CGO_TP_CD
                                 ,T.CURR_CD
                                 ,T.PROP_FRT_RT_AMT
                                 ,0 BUC_USD_AMT
                                 ,0 IFC_USD_AMT
                                 ,0 PSC_USD_AMT
                                 ,LPAD(T.CMDT_HDR_SEQ, 5, '0') || '|' || LPAD(T.ROUT_SEQ, 5, '0') || '|' ||
                                  LPAD(ROW_NUMBER() OVER(PARTITION BY T.PROP_NO, T.AMDT_SEQ, T.SVC_SCP_CD, T.CMDT_HDR_SEQ, T.ROUT_SEQ
                                                         ORDER BY DECODE(T.PRC_CGO_TP_CD, 'DR', 1, 'RF', 2, 'DG', 3, 'AK', 4, 99), T.RAT_UT_CD, T.CURR_CD)
                                      ,5
                                      ,'0') AS ROUT_ROWKEY
                             FROM PRI_RP_SCP_RT T
                            WHERE T.PROP_NO = @[prop_no]
                              AND T.AMDT_SEQ = @[amdt_seq]
                              AND T.SVC_SCP_CD = @[svc_scp_cd]
                              AND T.SRC_INFO_CD <> 'AD') F
                    WHERE S.PROP_NO = R.PROP_NO
                      AND S.AMDT_SEQ = R.AMDT_SEQ
                      AND S.SVC_SCP_CD = R.SVC_SCP_CD
                      AND S.CMDT_HDR_SEQ = R.CMDT_HDR_SEQ
                      AND S.ROUT_ROWKEY = A.ROUT_ROWKEY(+)
                      AND S.ROUT_ROWKEY = B.ROUT_ROWKEY(+)
                      AND S.ROUT_ROWKEY = C.ROUT_ROWKEY(+)
                      AND S.ROUT_ROWKEY = D.ROUT_ROWKEY(+)
                      AND S.ROUT_ROWKEY = E.ROUT_ROWKEY(+)
                      AND S.ROUT_ROWKEY = F.ROUT_ROWKEY(+)
                    ORDER BY S.ROUT_ROWKEY) B ON A.CMDT_ROWKEY = B.CMDT_ROWKEY
 ORDER BY COALESCE(A.BLET_DP_SEQ, B.BLET_DP_SEQ), COALESCE(A.CMDT_ROWKEY, B.CMDT_ROWKEY)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="SEL090016" out="N"/>
				<param name="amdt_seq" type="2" value="1" out="N"/>
				<param name="svc_scp_cd" type="12" value="TPE" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
