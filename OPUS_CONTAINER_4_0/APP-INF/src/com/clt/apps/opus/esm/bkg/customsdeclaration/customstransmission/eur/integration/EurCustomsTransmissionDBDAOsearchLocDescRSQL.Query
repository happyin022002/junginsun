<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchLocDescRSQL">
			<desc><![CDATA[loc_nm을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT LOC_NM
  FROM MDM_LOCATION
 WHERE LOC_CD = NVL(@[pol_cd], @[pod_cd])
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
