<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSpcDlyFcastCustDSQL">
			<desc><![CDATA[PC_DLY_FCAST_CUST의 지정된 ibflag 값에 따라 DB에 반영한다.(삭제)
2016.03.30 CTRT_NO 조건 추가]]></desc>
			<sql><![CDATA[
DELETE
  FROM SPC_DLY_FCAST_CUST 
 WHERE RLANE_CD         = @[rlane_cd]
   AND DIR_CD           = @[dir_cd]
   AND VSL_CD           = @[vsl_cd]
   AND SKD_VOY_NO       = @[skd_voy_no]
   AND SKD_DIR_CD       = @[skd_dir_cd]
   AND IOC_TS_CD        = @[ioc_ts_cd]
   AND SREP_USR_ID      = @[srep_usr_id]
   AND CUST_CNT_CD      = NVL(@[cust_cnt_cd], '00')
   AND CUST_SEQ         = NVL(@[cust_seq], 0)
   AND POL_YD_CD        = NVL(@[pol_yd_cd], '0000000')
   AND POD_YD_CD        = NVL(@[pod_yd_cd], '0000000')
   AND FCAST_OFC_CD     = @[fcast_ofc_cd]
   AND FCAST_CUST_TP_CD = NVL(@[fcast_cust_tp_cd], '0')
   --20160204.ADD, 20160211.MOD
   AND NVL(CTRT_CUST_CNT_CD, '00') = NVL(@[ctrt_cust_cnt_cd], '00')
   AND NVL(CTRT_CUST_SEQ, 0)       = NVL(@[ctrt_cust_seq], 0)
   AND NVL(CTRT_NO, '00') = NVL(@[ctrt_no], '00') --2016.03.30 ADD			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="ioc_ts_cd" type="12" value="" out="N"/>
				<param name="srep_usr_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="fcast_ofc_cd" type="12" value="" out="N"/>
				<param name="fcast_cust_tp_cd" type="12" value="" out="N"/>
				<param name="ctrt_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="ctrt_cust_seq" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
