<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModityNewVvdOopCdUSQL">
			<desc><![CDATA[VVD ASSIGN시 OOP CODE를 UPDATE한다]]></desc>
			<sql><![CDATA[
UPDATE BKG_VVD
   SET OP_CD = @[oop_cd]
	 , UPD_DT = SYSDATE
	 , UPD_USR_ID = @[usr_id]
 WHERE BKG_NO = @[bkg_no]
   AND VSL_CD     = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)			]]></sql>
			<params>
				<param name="oop_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
