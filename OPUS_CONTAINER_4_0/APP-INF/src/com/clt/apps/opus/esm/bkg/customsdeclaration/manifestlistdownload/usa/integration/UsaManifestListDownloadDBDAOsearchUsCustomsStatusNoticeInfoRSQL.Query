<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchUsCustomsStatusNoticeInfoRSQL">
			<desc><![CDATA[searchUsCustomsStatusNoticeInfo]]></desc>
			<sql><![CDATA[
SELECT HNDL_OFC_CD
, NVL(MAX(AUTO_SND_FLG),'Y') AUTO_SND_FLG_RADIO
, NVL(MAX(HNDL_OFC_ADDR),' ') HNDL_OFC_ADDR
, NVL(MAX(HNDL_OFC_EML),' ') HNDL_OFC_EML
, NVL(MAX(NTC_MSG_TP_CD),' ') NTC_MSG_TP_CD
, NVL(MAX(CSTMS_NTC_MSG),' ') CSTMS_NTC_MSG
, NVL(MAX(CASE WHEN NTC_MSG_TP_CD = '1R' THEN CSTMS_NTC_MSG END),' ') CSTMS_NTC_MSG_1R
, NVL(MAX(CASE WHEN NTC_MSG_TP_CD = '1S' THEN CSTMS_NTC_MSG END),' ') CSTMS_NTC_MSG_1S
, MAX(CRE_USR_ID) CRE_USR_ID
, MAX(CRE_DT) CRE_DT
, MAX(UPD_USR_ID) UPD_USR_ID
, MAX(UPD_DT) UPD_DT
, ' ' AUTO_SND_FLG
, ' ' NTC_MSG_TP_CD_1R
, ' ' NTC_MSG_TP_CD_1S
FROM BKG_CSTMS_NTC_STUP
WHERE HNDL_OFC_CD = @[hndl_ofc_cd]
GROUP BY HNDL_OFC_CD			]]></sql>
			<params>
				<param name="hndl_ofc_cd" type="12" value="ATLSC" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
