<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchYdInfoByEdoTransRSQLRSQL">
			<desc><![CDATA[Booking Container 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE( CSTMS_CLR_WH_CD, '0000000004', CSTMS_DCHG_LOC_WH_CD
                              , '0000000012', CSTMS_DCHG_LOC_WH_CD
                              , '0000000018', CSTMS_DCHG_LOC_WH_CD
                              , '0000000014', CSTMS_DCHG_LOC_WH_CD
                              , '0000000017', CSTMS_DCHG_LOC_WH_CD
                              , '0000000006', CSTMS_DCHG_LOC_WH_CD
                              , '0000000003', CSTMS_DCHG_LOC_WH_CD
                              , CSTMS_CLR_WH_CD) AS DISC_LOC2 -- DISC_LOC2
	 , CSTMS_DCHG_LOC_WH_CD AS DISC_LOC1                      -- DISC_LOC1
     , KR_CSTMS_BL_TP_CD    AS BL_TP                          -- BL_TP
     , BD_TP_CD             AS BND_TP                         -- BND_TP
  FROM BKG_CSTMS_KR_MF_SEQ_NO
 WHERE BKG_NO 	  = @[bkg_no]
   AND MF_REF_NO  = SUBSTR(@[mf_ref_no], 1, 10)
   AND MRN_CHK_NO = SUBSTR(@[mf_ref_no], 11, 1)

-- CSTMS_DCHG_LOC_WH_CD 컬럼 변경 USA_YD_CD
-- KT-NET 에서 배정장소가 없으면 발급 대기상태로 하기 때문에
-- 부두직통관(C), 입항전 신고(A), 차상반출(B), 내장통관(J) 4가지의 경우에
-- 하선장소를 배정장소에 자동 기입하기 위하여 Bonded Type 조회			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="mf_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
