<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RussiaManifestDownloadDBDAOmodifyCntrInfoCSQL">
			<desc><![CDATA[modifyCntrInfo]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_CSTMS_RU_CNTR RC
USING (
         SELECT  LPAD(@[bkg_no],12)     AS BKG_NO
               , @[cntr_no]             AS CNTR_NO
               , NVL(@[wgt1], 0)        AS WGT1
               , NVL(@[tare_wgt], 0)    AS TARE_WGT
               , NVL(@[total_wgt], 0)   AS TOTAL_WGT
               , @[cre_usr_id]          AS CRE_USR_ID
               , SYSDATE                AS CRE_DT
               , @[upd_usr_id]          AS UPD_USR_ID
               , SYSDATE                AS UPD_DT
         FROM    DUAL
      ) B
ON (RC.BKG_NO = B.BKG_NO
	AND RC.CNTR_NO = B.CNTR_NO 
)

WHEN MATCHED THEN

  UPDATE
  SET   CGO_WGT     = B.WGT1
      , TARE_WGT    = B.TARE_WGT
      , CNTR_WGT    = B.TOTAL_WGT
      , UPD_USR_ID  = B.UPD_USR_ID
      , UPD_DT      = B.UPD_DT


WHEN NOT MATCHED THEN
  INSERT ( BKG_NO
         , CNTR_NO
         , CGO_WGT
         , TARE_WGT
         , CNTR_WGT
         , CRE_USR_ID
         , CRE_DT
         , UPD_USR_ID
         , UPD_DT
  )
  VALUES ( B.BKG_NO
         , B.CNTR_NO
         , B.WGT1
         , B.TARE_WGT
         , B.TOTAL_WGT
         , B.CRE_USR_ID
         , B.CRE_DT
         , B.UPD_USR_ID
         , B.UPD_DT
  )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="wgt1" type="12" value="" out="N"/>
				<param name="tare_wgt" type="12" value="" out="N"/>
				<param name="total_wgt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
