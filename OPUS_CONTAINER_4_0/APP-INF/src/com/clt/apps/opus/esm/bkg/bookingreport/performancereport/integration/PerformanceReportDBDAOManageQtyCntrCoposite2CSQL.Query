<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOManageQtyCntrCoposite2CSQL">
			<desc><![CDATA[PerformanceReportDBDAOManageQtyCntrCoposite2CSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_DPCS_PROC_SMRY
                (BKG_NO
                ,BKG_DOC_DT
                ,DOC_KND_STS_CD
                ,DOC_PROC_RSLT_TP_CD
                ,SLAN_CD
                ,VSL_CD
                ,SKD_VOY_NO
                ,SKD_DIR_CD
                ,POL_CD
                ,POD_CD
                ,BKG_OFC_CD
                ,OB_SLS_OFC_CD
                ,DOC_USR_ID
                ,DPCS_SMRY_RMK
                ,CNTR_TEU_QTY
                ,CNTR_FEU_QTY
                ,CRE_USR_ID
                ,CRE_DT
                ,UPD_USR_ID
                ,UPD_DT)
              (
              SELECT        
               BK.BKG_NO V_BKG_NO
              ,TO_CHAR(SYSDATE , 'yyyymmdd')
              ,'8'
              ,BK.BKG_STS_CD
              ,BK.SLAN_CD V_BKG_LANE
              ,BK.VSL_CD V_VSL_CD
              ,BK.SKD_VOY_NO V_VOY_NO
              ,BK.SKD_DIR_CD V_VOY_DIR
              ,BK.POL_CD V_POL_LOC
              ,BK.POD_CD V_POD_LOC
              ,BK.BKG_OFC_CD V_BKG_OFC    
              ,BK.OB_SLS_OFC_CD V_SAL_OFC
              ,BK.DOC_USR_ID V_BKG_STF
              ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(COM_CONSTANTMGR_PKG.COM_GETBASELOCATIONCODE_FNC(), SYSDATE - 1, BK.POL_CD), 'yyyymmdd')  V_RMK
    		  ,SUM(decode(substr(B.CNTR_TPSZ_CD,-1),'2',NVL(B.OP_CNTR_QTY,0),0))
              ,SUM(decode(substr(B.CNTR_TPSZ_CD,-1),'2',0,NVL(B.OP_CNTR_QTY,0)))
	
              --,SUM(DECODE(GREATEST(2,SUBSTR(B.CNTR_TPSZ_CD,2,1)),2,NVL(B.OP_CNTR_QTY,0),0)) TEU_A
              --,SUM(DECODE(GREATEST(2,SUBSTR(B.CNTR_TPSZ_CD,2,1)),2,0,NVL(B.OP_CNTR_QTY,0))) TEU_B
              ,'System'
              ,sysdate
              ,'System'
              ,Sysdate
        FROM   BKG_BOOKING  BK
              ,BKG_QUANTITY B
        WHERE  BK.BKG_NO = B.BKG_NO 
        AND    BK.BKG_NO = @[bkg_no]
          GROUP BY 
               BK.BKG_NO 
              ,TO_CHAR( SYSDATE, 'yyyymmdd')
              ,BK.BKG_STS_CD
              ,BK.SLAN_CD
              ,BK.VSL_CD 
              ,BK.SKD_VOY_NO 
              ,BK.SKD_DIR_CD 
              ,BK.POL_CD 
              ,BK.POD_CD 
              ,BK.BKG_OFC_CD 
              ,BK.OB_SLS_OFC_CD
              ,BK.DOC_USR_ID 
              ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(COM_CONSTANTMGR_PKG.COM_GETBASELOCATIONCODE_FNC(), SYSDATE - 1, BK.POL_CD), 'yyyymmdd') 
              )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
