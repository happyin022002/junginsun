<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCfmAdjustmentDBDAOSearchMonthlyQuotaEditOfficeAdd0167ListRSQL">
			<desc><![CDATA[Quota Editing - Office Add Popup List 조회]]></desc>
			<sql><![CDATA[
SELECT 
          A.MQTA_RLSE_VER_NO    , 
          A.BSE_YR    , 
          A.BSE_QTR_CD,
          A.QTA_TGT_CD          ,
          B.BSE_MON   , 
          B.BSE_WK    ,
          A.VSL_CD              , 
          A.SKD_VOY_NO,
          A.SKD_DIR_CD,
          A.TRD_CD              ,
          A.DIR_CD    , 
          A.SUB_TRD_CD,
          A.RLANE_CD            , 
          A.RHQ_CD    , 
          A.AQ_CD     ,
          A.RGN_OFC_CD          , 
          A.ADD_TP_CD , 
          A.DELT_FLG  ,
          A.RLANE_CD     AS NEW_RLANE_CD,
          B.FNL_BSA_CAPA AS BSA_CAPA    ,
          TO_CHAR(B.LST_LODG_PORT_ETD_DT,'YYYY-MM-DD HH24:MI:SS') AS LST_LODG_PORT_ETD_DT, B.IOC_CD    , B.VVD_SEQ
     FROM SAQ_MON_CFM_QTA_OFC_ADD A,
          SAQ_MON_CFM_TGT_VVD     B
    WHERE A.MQTA_RLSE_VER_NO  = @[mqtaRlseVerNo]
      AND A.BSE_YR            = @[bse_yr]
      AND A.BSE_QTR_CD        = @[bse_qtr_cd]
      AND A.QTA_TGT_CD        = @[qtaTgtCd]
      AND A.TRD_CD            = @[trd_cd]
      AND A.DIR_CD            = @[dir_cd]
#if (${add_tp_cd} == 'O')
	#if (${rlane_cd} != '') 
      AND A.RLANE_CD         = @[rlane_cd]
	#end
#end

      AND A.ADD_TP_CD        = @[add_tp_cd]
      AND A.MQTA_RLSE_VER_NO = B.MQTA_RLSE_VER_NO
      AND A.BSE_YR           = B.BSE_YR
      AND A.BSE_QTR_CD       = B.BSE_QTR_CD
      AND A.TRD_CD           = B.TRD_CD
      AND A.DIR_CD           = B.DIR_CD
      AND A.VSL_CD           = B.VSL_CD
      AND A.SKD_VOY_NO       = B.SKD_VOY_NO
      AND A.SKD_DIR_CD       = B.SKD_DIR_CD
      AND A.SUB_TRD_CD       = B.SUB_TRD_CD
      AND A.RLANE_CD         = B.RLANE_CD
 ORDER BY RLANE_CD, RGN_OFC_CD, BSE_MON, BSE_WK, VSL_CD||SKD_VOY_NO||SKD_DIR_CD			]]></sql>
			<params>
				<param name="mqtaRlseVerNo" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="qtaTgtCd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="add_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
