<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOPndlmSvcRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT   DISTINCT
         VVD_CD
        ,EFF_FM_DT
        ,EFF_TO_DT
        ,SLAN_CD
        ,DIR_CD
    FROM COA_PNDLM_SVC 
   WHERE 1 = 1 
     AND SLAN_CD = NVL(@[f_slan_cd], SLAN_CD)
     AND DIR_CD = NVL(@[f_dir_cd], DIR_CD)
#if (${f_vsl_cd} != '')
     AND VVD_CD LIKE @[f_vsl_cd]||NVL(@[f_skd_voy_no],'')||NVL(@[f_dir],'')||'%'
#end
--20151126.MOD
#if (${f_from_dt} != '')
     AND @[f_from_dt] BETWEEN EFF_FM_DT AND NVL(EFF_TO_DT,'99991231')
#end
   ORDER BY SLAN_CD, DIR_CD, EFF_FM_DT			]]></sql>
			<params>
				<param name="f_slan_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
				<param name="f_skd_voy_no" type="12" value="" out="N"/>
				<param name="f_dir" type="12" value="" out="N"/>
				<param name="f_from_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
