<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchCust301PodEtaRSQL">
			<desc><![CDATA[searchCust301PodEta]]></desc>
			<sql><![CDATA[
