<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ThailandManifestListDownloadDBDAOsearchCustomsBlInfoRSQL">
			<desc><![CDATA[ThailandManifestListDownloadDBDAOsearchCustomsBlInfo]]></desc>
			<sql><![CDATA[
SELECT 
    B.BKG_NO, 
    C.CNTR_NO, 
    C.CNTR_TPSZ_CD,
	B.POR_CD,
	B.POL_CD,
	B.POD_CD,
    NVL( (SELECT REPLACE(S.CUST_NM,CHR(10),'  ') AS CUST_NM     FROM BKG_CUSTOMER S WHERE S.BKG_NO=B.BKG_NO AND S.BKG_CUST_TP_CD='S' AND ROWNUM=1 ), ' ' ) AS SH_CUST_NM,
    NVL( (SELECT REPLACE(S.CUST_ADDR,CHR(10),'  ') AS CUST_ADDR FROM BKG_CUSTOMER S WHERE S.BKG_NO=B.BKG_NO AND S.BKG_CUST_TP_CD='S' AND ROWNUM=1 ), ' ' ) AS SH_CUST_ADDR,
	NVL( (SELECT REPLACE(F.CUST_NM,CHR(10),'  ') AS CUST_NM     FROM BKG_CUSTOMER F WHERE F.BKG_NO=B.BKG_NO AND F.BKG_CUST_TP_CD='F' AND ROWNUM=1 ), ' ' ) AS FW_CUST_NM,
    NVL( (SELECT REPLACE(C.CUST_NM,CHR(10),'  ') AS CUST_NM     FROM BKG_CUSTOMER C WHERE C.BKG_NO=B.BKG_NO AND C.BKG_CUST_TP_CD='C' AND ROWNUM=1 ), ' ' ) AS CN_CUST_NM,
    NVL( (SELECT REPLACE(S.CUST_ADDR,CHR(10),'  ') AS CUST_ADDR FROM BKG_CUSTOMER S WHERE S.BKG_NO=B.BKG_NO AND S.BKG_CUST_TP_CD='C' AND ROWNUM=1 ), ' ' ) AS CN_CUST_ADDR,
    NVL( (SELECT REPLACE(N.CUST_NM,CHR(10),'  ') AS CUST_NM     FROM BKG_CUSTOMER N WHERE N.BKG_NO=B.BKG_NO AND N.BKG_CUST_TP_CD='N' AND ROWNUM=1 ), ' ' ) AS NO_CUST_NM,
    NVL( (SELECT REPLACE(S.CUST_ADDR,CHR(10),'  ') AS CUST_ADDR FROM BKG_CUSTOMER S WHERE S.BKG_NO=B.BKG_NO AND S.BKG_CUST_TP_CD='N' AND ROWNUM=1 ), ' ' ) AS NO_CUST_ADDR,
    NVL( (SELECT REPLACE(TO_CHAR(U.MK_DESC),CHR(10),'  ') AS MK_DESC     FROM BKG_BL_MK_DESC U WHERE U.BKG_NO=B.BKG_NO AND ROWNUM=1 ), ' ' ) AS MK_DESC, 
    NVL( D.PCK_QTY, 0.000 ) AS PCK_QTY, 
    NVL( D.PCK_TP_CD, ' ' ) AS PCK_TP_CD, 
    NVL( D.CNTR_MF_WGT, 0.000 ) AS CNTR_MF_WGT, 
    NVL( D.WGT_UT_CD, ' ' ) AS WGT_UT_CD, 
    NVL( D.MEAS_QTY, 0.000 ) AS MEAS_QTY, 
    NVL( D.MEAS_UT_CD, ' ' ) AS MEAS_UT_CD, 
    NVL( (SELECT REPLACE(TO_CHAR(U.CMDT_DESC),CHR(10),'  ') AS CMDT_DESC     FROM BKG_BL_MK_DESC U WHERE U.BKG_NO=B.BKG_NO AND ROWNUM=1 ), ' ' ) AS CMDT_DESC, 
    B.DEL_NOD_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_CONTAINER C, BKG_CNTR_MF_DESC D 
 WHERE 1=1 
   AND A.BKG_NO = B.BKG_NO 
   AND B.BKG_STS_CD != 'X' 
   AND A.BKG_NO = C.BKG_NO 
   AND C.BKG_NO = D.BKG_NO(+)
   AND C.CNTR_NO = D.CNTR_NO(+)
   AND A.VSL_CD = SUBSTR(@[s_vvd],1,4)
   AND A.SKD_VOY_NO = SUBSTR(@[s_vvd],5,4)
   AND A.SKD_DIR_CD = SUBSTR(@[s_vvd],9,1)
   AND B.VSL_CD = SUBSTR(@[trnk_vvd],1,4)
   AND B.SKD_VOY_NO = SUBSTR(@[trnk_vvd],5,4)
   AND B.SKD_DIR_CD = SUBSTR(@[trnk_vvd],9,1)
   AND B.POL_CD = @[pol_cd]
   AND B.POD_CD = @[pod_cd]
   AND B.DEL_CD = @[s_del_cd]
   AND B.DEL_NOD_CD like NVL(@[s_del_cd]||@[s_del_nod_cd]||'%','%')
ORDER BY A.BKG_NO, C.CNTR_NO, D.CNTR_MF_SEQ			]]></sql>
			<params>
				<param name="s_vvd" type="12" value="" out="N"/>
				<param name="trnk_vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="s_del_cd" type="12" value="" out="N"/>
				<param name="s_del_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
