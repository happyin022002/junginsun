<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOmodifyBlIssInfoHistoryForBlDocUSQL">
			<desc><![CDATA[modifyBlIssInfoHistoryForBlDoc]]></desc>
			<sql><![CDATA[
UPDATE 
	BKG_BL_DOC_HIS
SET   
	VSL_NM		= @[vessel_direction]
	,PRE_VSL_NM	= @[pre_carriage_by]
##	,POR_CD		= @[por_code]
	,POR_NM		= @[por_name]
##	,POL_CD		= @[pol_code]
	,POL_NM		= @[pol_name]
##	,POD_CD		= @[pod_code]
	,POD_NM		= @[pod_name]
##	,DEL_CD		= @[del_code]
	,DEL_NM		= @[del_name]
	,BL_MV_TP_NM	= @[move_type]
	,FNL_DEST_NM   = @[final_dest]
	,BL_OBRD_TP_CD = @[on_board_type]
#if ('000000'!=${on_board_date})
    ,BL_OBRD_DT		= TO_DATE(@[on_board_date],'YYYY-MM-DD HH24:MI:SS')
#else
    ,BL_OBRD_DT		= NULL
#end
	,UPD_USR_ID	= @[upd_usr_id]
	,UPD_DT		= sysdate
	,BL_MV_TP_PRN_FLG		= @[bl_mv_tp_prn_flg]
	,RCV_DE_TERM_PRN_FLG	= @[rcv_de_term_prn_flg]
WHERE  
	BKG_NO = @[bkg_no]
    AND CORR_NO = 'TMP0000001'			]]></sql>
			<params>
				<param name="vessel_direction" type="12" value="" out="N"/>
				<param name="pre_carriage_by" type="12" value="" out="N"/>
				<param name="por_code" type="12" value="" out="N"/>
				<param name="por_name" type="12" value="" out="N"/>
				<param name="pol_code" type="12" value="" out="N"/>
				<param name="pol_name" type="12" value="" out="N"/>
				<param name="pod_code" type="12" value="" out="N"/>
				<param name="pod_name" type="12" value="" out="N"/>
				<param name="del_code" type="12" value="" out="N"/>
				<param name="del_name" type="12" value="" out="N"/>
				<param name="move_type" type="12" value="" out="N"/>
				<param name="final_dest" type="12" value="" out="N"/>
				<param name="on_board_type" type="12" value="" out="N"/>
				<param name="on_board_date" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_mv_tp_prn_flg" type="12" value="" out="N"/>
				<param name="rcv_de_term_prn_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
