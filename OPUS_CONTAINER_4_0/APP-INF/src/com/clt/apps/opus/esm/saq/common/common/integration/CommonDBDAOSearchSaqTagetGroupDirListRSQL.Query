<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqTagetGroupDirListRSQL">
			<desc><![CDATA[Direction Code search in Target Group and Trade Code]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT         
        DIR_CD AS CODE,  
        DIR_CD AS TEXT   
  FROM  SAQ_TGT_GRP_TRD    
 WHERE  1 = 1
   AND  SAQ_TGT_GRP_CD = NVL(@[targetGrp], SAQ_TGT_GRP_CD)
   AND  TRD_CD = @[trdCd]
ORDER BY TRD_CD			]]></sql>
			<params>
				<param name="targetGrp" type="12" value="" out="N"/>
				<param name="trdCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
