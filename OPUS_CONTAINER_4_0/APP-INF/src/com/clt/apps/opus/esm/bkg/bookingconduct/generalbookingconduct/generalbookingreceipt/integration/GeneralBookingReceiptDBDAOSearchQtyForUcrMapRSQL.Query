<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchQtyForUcrMapRSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOSearchQtyForUcrMapRSQL]]></desc>
			<sql><![CDATA[
SELECT 
    CASE WHEN OP_CNTR_QTY <= 1 THEN 'Y'
    ELSE 'N'
    END FLG
    FROM BKG_QTY_DTL Q
WHERE BKG_NO = @[bkg_no]
AND EXISTS (SELECT COUNT(1) FROM BKG_QTY_DTL WHERE BKG_NO = Q.BKG_NO HAVING COUNT(1) = 1 )
AND ROWNUM =1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
