<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOModifyControllingPartyUSQL">
			<desc><![CDATA[modify Controlling Party]]></desc>
			<sql><![CDATA[
UPDATE BKG_CTRL_PTY
    SET CTRL_PTY_NM = @[ctrl_pty_nm]
        ,CTRL_PTY_DESC = @[ctrl_pty_desc]
        ,UPD_USR_ID = @[upd_usr_id]
        ,UPD_DT = SYSDATE
WHERE CTRL_PTY_SEQ = @[ctrl_pty_seq]			]]></sql>
			<params>
				<param name="ctrl_pty_nm" type="12" value="" out="N"/>
				<param name="ctrl_pty_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ctrl_pty_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
