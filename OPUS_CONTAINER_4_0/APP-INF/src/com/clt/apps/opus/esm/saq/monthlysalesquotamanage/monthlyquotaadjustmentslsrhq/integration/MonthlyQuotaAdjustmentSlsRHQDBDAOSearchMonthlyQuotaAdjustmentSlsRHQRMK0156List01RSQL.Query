<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentSlsRHQDBDAOSearchMonthlyQuotaAdjustmentSlsRHQRMK0156List01RSQL">
			<desc><![CDATA[Remark 목록 조회]]></desc>
			<sql><![CDATA[
SELECT
     MQTA_STEP_CD ,
     BSE_YR       ,
     BSE_QTR_CD   ,
     TRD_CD       ,
     DIR_CD       ,
     MQTA_VER_NO  ,
     RLANE_CD || '-' || SPRT_GRP_CD || BSA_GRP_CD    AS RLANE_GRP,
     RLANE_CD     ,
     SPRT_GRP_CD  ,
     BSA_GRP_CD   ,
     SLS_RGN_OFC_CD  ,
     POL_CD ,
     POD_CD ,
     BSE_MON      ,
     CRE_SEQ      ,
     SUBJ_CTNT    ,
     CRE_OFC_CD   ,
     CMT_CTNT     ,
     SAQ_STS_CD,
     RMK_CRE_GDT    ,
     RNK
 FROM (
     SELECT
         MQTA_STEP_CD ,
         BSE_YR       ,
         BSE_QTR_CD   ,
         TRD_CD       ,
         DIR_CD       ,
         MQTA_VER_NO  ,
         RLANE_CD     ,
         SPRT_GRP_CD  ,
         BSA_GRP_CD   ,
         SLS_RGN_OFC_CD  ,
         POL_CD ,
         POD_CD ,
         BSE_MON      ,
         CRE_SEQ      ,
         SUBJ_CTNT    ,
         CRE_OFC_CD   ,
         CMT_CTNT     ,
         SAQ_STS_CD,
         RMK_CRE_GDT    ,
         RANK()  OVER ( PARTITION BY RLANE_CD,SPRT_GRP_CD,BSA_GRP_CD,POL_CD,POD_CD,SLS_RGN_OFC_CD ORDER BY RMK_CRE_GDT DESC) RNK
      FROM (
         SELECT
             MQTA_STEP_CD ,
             BSE_YR       ,
             BSE_QTR_CD   ,
             TRD_CD       ,
             DIR_CD       ,
             MQTA_VER_NO  ,
             RLANE_CD     ,
             SPRT_GRP_CD  ,
             BSA_GRP_CD   ,
             SLS_RGN_OFC_CD  ,
             POL_CD ,
             POD_CD ,
             BSE_MON      ,
             CRE_SEQ      ,
             SUBJ_CTNT    ,
             CRE_OFC_CD   ,
             CMT_CTNT     ,
             TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('GMT', RMK_CRE_GDT, @[ofcCd] ), 'yyyy.mm.dd hh24:mi:ss') AS RMK_CRE_GDT,
             SAQ_STS_CD
         FROM   SAQ_MON_QTA_LOD_TGT_RMK
         WHERE MQTA_STEP_CD  = @[mQtaStepCd]
         AND   BSE_YR        = @[year]
         AND   BSE_QTR_CD    = @[bse_qtr_cd]
         AND   TRD_CD        = @[trade]
         AND   DIR_CD        = @[bound]
         AND   MQTA_VER_NO   = @[mQtaVerNo]
         AND   BSE_YR || BSE_MON       = @[targetMonth]
         UNION ALL
         SELECT
             RMK.MQTA_STEP_CD ,
             RMK.BSE_YR       ,
             RMK.BSE_QTR_CD   ,
             RMK.TRD_CD       ,
             RMK.DIR_CD       ,
             RMK.MQTA_VER_NO  ,
             RMK.RLANE_CD     ,
             RMK.SPRT_GRP_CD  ,
             RMK.BSA_GRP_CD   ,
             RMK.SLS_RGN_OFC_CD  ,
             RMK.POL_CD ,
             RMK.POD_CD ,
             RMK.BSE_MON      ,
             RMK.CRE_SEQ      ,
             RMK.SUBJ_CTNT    ,
             RMK.CRE_OFC_CD   ,
             RMK.CMT_CTNT     ,
             TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('GMT', RMK_CRE_GDT, @[ofcCd] ), 'yyyy.mm.dd hh24:mi:ss') AS RMK_CRE_GDT,
             RMK.SAQ_STS_CD
         FROM  SAQ_MON_QTA_STEP_VER VER,
               SAQ_MON_QTA_LOD_TGT_RMK RMK
         WHERE VER.MQTA_STEP_CD  <> @[mQtaStepCd]
         AND   VER.BSE_YR        = @[year]
         AND   VER.BSE_QTR_CD    = @[bse_qtr_cd]
         AND   VER.TRD_CD        = @[trade]
         AND   VER.DIR_CD        = @[bound]
         AND   VER.GLINE_VER_NO  = @[glineVerNo]
         AND   VER.SAQ_STS_CD IN
                         (SELECT A.INTG_CD_VAL_CTNT
                          FROM   COM_INTG_CD_DTL A,
                                 COM_INTG_CD_DTL B
                          WHERE A.INTG_CD_ID = 'CD00926'
                          AND   B.INTG_CD_ID = A.INTG_CD_ID
                          AND   B.INTG_CD_VAL_CTNT = 'DN'
                          AND   A.INTG_CD_VAL_DP_SEQ >= B.INTG_CD_VAL_DP_SEQ)
         AND   RMK.MQTA_STEP_CD  = VER.MQTA_STEP_CD
         AND   RMK.BSE_YR        = VER.BSE_YR
         AND   RMK.BSE_QTR_CD    = VER.BSE_QTR_CD
         AND   RMK.TRD_CD        = VER.TRD_CD
         AND   RMK.DIR_CD        = VER.DIR_CD
         AND   RMK.MQTA_VER_NO   = VER.MQTA_VER_NO
         AND   RMK.BSE_YR || RMK.BSE_MON       = @[targetMonth]
     )
 )
 WHERE RNK = 1			]]></sql>
			<params>
				<param name="ofcCd" type="12" value="" out="N"/>
				<param name="mQtaStepCd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="mQtaVerNo" type="12" value="" out="N"/>
				<param name="targetMonth" type="12" value="" out="N"/>
				<param name="glineVerNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
