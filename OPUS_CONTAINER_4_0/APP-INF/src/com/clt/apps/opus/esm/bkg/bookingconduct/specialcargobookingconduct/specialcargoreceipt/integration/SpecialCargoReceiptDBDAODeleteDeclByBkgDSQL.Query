<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAODeleteDeclByBkgDSQL">
			<desc><![CDATA[BKG_DG_DECL 데이터를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_DG_DECL DECL
WHERE DECL.BKG_NO = @[bkg_no]
AND NOT EXISTS (
	SELECT *
	FROM BKG_DG_CGO CGO
	WHERE CGO.BKG_NO = DECL.BKG_NO
	AND CGO.DG_CNTR_SEQ = DECL.DG_CNTR_SEQ
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
