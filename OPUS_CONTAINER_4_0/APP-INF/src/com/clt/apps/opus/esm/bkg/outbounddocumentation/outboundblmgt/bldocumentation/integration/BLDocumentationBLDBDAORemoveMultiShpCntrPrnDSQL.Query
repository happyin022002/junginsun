<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAORemoveMultiShpCntrPrnDSQL">
			<desc><![CDATA[Multi Shipment를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE BKG_CNTR_SHP
WHERE BKG_NO = @[bkg_no]
AND CNTR_MF_SEQ = @[cntr_mf_seq]
AND CNTR_SEQ = @[cntr_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_mf_seq" type="12" value="" out="N"/>
				<param name="cntr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
