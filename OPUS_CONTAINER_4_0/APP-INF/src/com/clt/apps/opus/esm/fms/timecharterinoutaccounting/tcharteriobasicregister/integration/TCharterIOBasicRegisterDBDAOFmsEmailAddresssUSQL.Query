<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDBDAOFmsEmailAddresssUSQL">
			<desc><![CDATA[E-mail List Registration Update]]></desc>
			<sql><![CDATA[
UPDATE FMS_SCR_EML_ADDR SET 
	   CNTC_PSON_NM = @[cntc_pson_nm],
   	   CNTC_PSON_EML = @[cntc_pson_eml],
	   UPD_USR_ID = @[upd_usr_id],
	   UPD_DT = SYSDATE
 WHERE EML_SEQ = @[eml_seq]			]]></sql>
			<params>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_pson_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eml_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
