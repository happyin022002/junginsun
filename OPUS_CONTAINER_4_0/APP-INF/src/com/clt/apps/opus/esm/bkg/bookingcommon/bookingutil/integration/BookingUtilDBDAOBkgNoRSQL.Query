<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOBkgNoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
select
	bkg_no
from bkg_booking
where	bl_no = @[bl_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
