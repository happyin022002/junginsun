<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOSearchDgCargoSeqRSQL">
			<desc><![CDATA[BKG_NO, DCGO_SEQ 에 대한 BKG_DG_CGO 또는 BKG_DG_CGO_HIS의 데이터 존재여부 체크 로직]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' 
       ELSE 'N' 
       END DCGO_YN
#if (${ca_flg} == 'Y')
FROM BKG_DG_CGO_HIS
#else
FROM BKG_DG_CGO
#end
WHERE BKG_NO = @[bkg_no]
#if (${ca_flg} == 'Y')
AND CORR_NO = 'TMP0000001'
#end
AND DCGO_SEQ = @[dcgo_seq]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dcgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
