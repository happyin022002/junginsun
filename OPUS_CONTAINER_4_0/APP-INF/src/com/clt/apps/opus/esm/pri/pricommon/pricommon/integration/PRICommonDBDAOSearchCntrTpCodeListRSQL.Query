<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOSearchCntrTpCodeListRSQL">
			<desc><![CDATA[retrieving MDM Container Type]]></desc>
			<sql><![CDATA[
SELECT 
    CNTR_TP_CD AS CD
   ,CNTR_TP_DESC AS NM
   ,(CNTR_TP_CD || '|' || CNTR_TP_DESC) AS ETC5
FROM MDM_CNTR_TP
WHERE DELT_FLG = 'N'
ORDER BY DECODE(CNTR_TP_CD, 'D',1,'R',2,'F',3,'A',4,'O',5,'S',6,7)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
