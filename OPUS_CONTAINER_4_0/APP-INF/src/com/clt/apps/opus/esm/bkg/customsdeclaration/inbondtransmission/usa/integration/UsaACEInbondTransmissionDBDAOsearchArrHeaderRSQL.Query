<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaACEInbondTransmissionDBDAOsearchArrHeaderRSQL">
			<desc><![CDATA[UsaACEInbondTransmissionDBDAOsearchArrHeaderRSQL]]></desc>
			<sql><![CDATA[
SELECT RPAD('ACR' || COM_ConstantMgr_PKG.COM_getScacCode_FNC() || RPAD(' ', 6, ' ') || 'HI', 80, ' ')||CHR(10)||
	   RPAD('M01' || COM_ConstantMgr_PKG.COM_getScacCode_FNC() || '10'||
	        RPAD(@[vsl_flag],2,' ')||
	        RPAD(' ', 23, ' ')||
	        NVL((SELECT CASE WHEN TO_DATE(ATTR_CTNT1,'YYYYMMDDHH24MISS') > SYSDATE
                             THEN SUBSTR(@[vvd], 6, 4) || ' '
                             ELSE SUBSTR(@[vvd], 5, 5)
                        END
                   FROM BKG_CSTMS_CD_CONV_CTNT
                  WHERE CNT_CD = 'US'
                    AND CSTMS_DIV_ID = 'US_M01_VOY_NO'
                    AND ROWNUM = 1
                ), SUBSTR(@[vvd], 5, 5))||
	        RPAD(' ', 5, ' ') || '000001' || ' ' ||@[vsl_lloyd],80,' ')||CHR(10)||
       RPAD('M02'||@[crr_bat_no], 80, ' ')||CHR(10) ||
	   RPAD('P01'||RPAD(@[tmpstr4],4,' ')||RPAD(@[vps_eta_dt],6,' ')||LPAD(' ',5,' '), 80, ' ')||CHR(10)
  FROM DUAL			]]></sql>
			<params>
				<param name="vsl_flag" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="vsl_lloyd" type="12" value="" out="N"/>
				<param name="crr_bat_no" type="12" value="" out="N"/>
				<param name="tmpstr4" type="12" value="" out="N"/>
				<param name="vps_eta_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
