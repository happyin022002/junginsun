<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaGuidelineDBDAOSaqMonQtaStepVerStatus0076USQL">
			<desc><![CDATA[SAQ_MON_QTA_STEP_VER	Update]]></desc>
			<sql><![CDATA[
UPDATE  SAQ_MON_QTA_STEP_VER
SET     SAQ_STS_CD  = @[newstscd],
        UPD_USR_ID  = @[user_id] ,
        UPD_DT      = SYSDATE
WHERE   MQTA_STEP_CD LIKE @[mqtastepcd]||'%'
AND     BSE_YR          = @[year]
AND     BSE_QTR_CD      = @[bse_qtr_cd]
AND     TRD_CD          IN (SELECT TRD_CD
                              FROM SAQ_TGT_GRP_TRD_V
                             WHERE SAQ_TGT_GRP_CD = @[target_grp])
AND     GLINE_VER_NO    = @[version]
AND     SAQ_STS_CD      = @[stscd]			]]></sql>
			<params>
				<param name="newstscd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="mqtastepcd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="target_grp" type="12" value="" out="N"/>
				<param name="version" type="12" value="" out="N"/>
				<param name="stscd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
