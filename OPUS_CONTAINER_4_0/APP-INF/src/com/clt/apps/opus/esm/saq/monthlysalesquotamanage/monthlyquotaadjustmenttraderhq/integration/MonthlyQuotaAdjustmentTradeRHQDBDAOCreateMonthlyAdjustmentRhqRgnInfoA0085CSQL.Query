<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentTradeRHQDBDAOCreateMonthlyAdjustmentRhqRgnInfoA0085CSQL">
			<desc><![CDATA[MonthlyQuotaAdjustmentTradeRHQ 에서 'FC' Confirm 처리시 STEP '03' 정보를 기준으로 SETP '04' (RHQ RGN 정보)를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_MON_QTA_STEP_VER (
    MQTA_STEP_CD,
    BSE_YR,
    BSE_QTR_CD,
    TRD_CD,
    DIR_CD,
    MQTA_VER_NO,
    SAQ_STS_CD,
    GLINE_VER_NO,
    QTA_MST_VER_NO,
    INCL_PORT_FLG,
    CRE_OFC_CD,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT )
SELECT
    '04' AS MQTA_STEP_CD,
    @[year] AS BSE_YR,
    @[bse_quarter] AS BSE_QTR_CD,
    @[trade] AS TRD_CD,
    @[bound] AS DIR_CD,
    @[ofcCd]||SUBSTR(@[glineVerNo], LENGTH(@[glineVerNo])-5)
       ||TO_CHAR(NVL(MAX(SUBSTR(STV.MQTA_VER_NO,
           LENGTH(STV.MQTA_VER_NO)-1)), 0)+1,'FM09') AS MQTA_VER_NO,
    '00' AS SAQ_STS_CD,
    @[glineVerNo] AS GLINE_VER_NO,
    SUBSTR(@[glineVerNo],-6) AS QTA_MST_VER_NO,
    'N' AS INCL_PORT_FLG,
    @[ofcCd] AS CRE_OFC_CD,
    @[userId] AS CRE_USR_ID,
    SYSDATE AS CRE_DT,
    @[userId] AS UPD_USR_ID,
    SYSDATE AS UPD_DT
FROM  SAQ_MON_QTA_STEP_VER STV
WHERE  STV.MQTA_STEP_CD(+) = '04'
AND    STV.BSE_YR(+) = @[year]
AND    STV.BSE_QTR_CD(+) = @[bse_quarter]
AND    STV.TRD_CD(+) = @[trade]
AND    STV.DIR_CD(+) = @[bound]
AND    STV.GLINE_VER_NO(+) = @[glineVerNo]
AND    STV.CRE_OFC_CD(+) = @[ofcCd]			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_quarter" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="ofcCd" type="12" value="" out="N"/>
				<param name="glineVerNo" type="12" value="" out="N"/>
				<param name="userId" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
