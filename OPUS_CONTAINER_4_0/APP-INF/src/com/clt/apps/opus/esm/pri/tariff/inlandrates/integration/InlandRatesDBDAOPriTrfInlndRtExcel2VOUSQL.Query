<sqls>
	<querys>
		<query name="InlandRatesDBDAOPriTrfInlndRtExcel2VOUSQL">
			<desc><![CDATA[엑셀 업로드시 amend 데이터를 amend delete로 변경]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRF_INLND_RT
   SET UPD_USR_ID                  = @[upd_usr_id]
     , UPD_DT                      = SYSDATE
	 , SRC_INFO_CD     			   = 'AD'
 WHERE TRF_PFX_CD		   		   = @[trf_pfx_cd]
   AND TRF_NO                      = @[trf_no]
   AND TRF_INLND_SEQ               = @[trf_inlnd_seq]
   AND AMDT_SEQ                    = @[amdt_seq]
   AND SRC_INFO_CD 				   = 'AM'
   AND N1ST_CMNC_AMDT_SEQ 		   = @[amdt_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_inlnd_seq" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
