<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOmodifyBkgCntrPkcMeasWgtUSQL">
			<desc><![CDATA[modifyBkgCntrPkcMeasWgt]]></desc>
			<sql><![CDATA[
UPDATE BKG_CONTAINER SET
       PCK_QTY = (SELECT SUM(PCK_QTY) FROM BKG_CONTAINER WHERE (BKG_NO = @[mst_bkg_no] OR BKG_NO = @[bkg_no]) AND CNTR_NO = @[cntr_no])
	, MEAS_QTY = (SELECT SUM(MEAS_QTY) FROM BKG_CONTAINER WHERE (BKG_NO = @[mst_bkg_no] OR BKG_NO = @[bkg_no]) AND CNTR_NO = @[cntr_no])
	, CNTR_WGT = (SELECT SUM(CNTR_WGT) FROM BKG_CONTAINER WHERE (BKG_NO = @[mst_bkg_no] OR BKG_NO = @[bkg_no]) AND CNTR_NO = @[cntr_no])
	, CNTR_VOL_QTY = (SELECT SUM(CNTR_VOL_QTY) FROM BKG_CONTAINER WHERE (BKG_NO = @[mst_bkg_no] OR BKG_NO = @[bkg_no]) AND CNTR_NO = @[cntr_no])
	, CNTR_PRT_FLG = CASE WHEN (SELECT SUM(CNTR_VOL_QTY) 
								  FROM BKG_CONTAINER 
								 WHERE (BKG_NO = @[mst_bkg_no] OR BKG_NO = @[bkg_no]) 
								   AND CNTR_NO = @[cntr_no]) = 1 THEN 'N' ELSE 'Y' END
    , UPD_USR_ID = @[usr_id]
    , UPD_DT = SYSDATE
 WHERE BKG_NO = @[mst_bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="mst_bkg_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
