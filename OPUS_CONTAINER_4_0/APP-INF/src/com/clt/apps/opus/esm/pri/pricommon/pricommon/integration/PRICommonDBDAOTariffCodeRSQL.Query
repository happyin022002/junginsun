<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOTariffCodeRSQL">
			<desc><![CDATA[Tariff Code 리스트 조회]]></desc>
			<sql><![CDATA[
SELECT TRF_PFX_CD || '-' || TRF_NO AS CD, TRF_NM AS NM
  FROM PRI_TARIFF
 WHERE DELT_FLG = 'N'
 ORDER BY TRF_PFX_CD, TRF_NO			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
