<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAORsltCreRFANoVORSQL">
			<desc><![CDATA[RFA NO 생성
Office Prefix 3자리 + 연도 2자리 + A (Approval) + Sequence 4자리]]></desc>
			<sql><![CDATA[
SELECT 
       CASE WHEN NVL(@[trf_ctrf_flg],'N') = 'Y' THEN
            'TRF'||SUBSTR(TO_CHAR(SYSDATE,'YYYY'),-2)||'A'||LPAD(NVL(MAX(SUBSTR(RFA_NO,-4)),0)+1,4,'0') 
       ELSE
            SUBSTR(@[ofc_cd],0,3)||SUBSTR(TO_CHAR(SYSDATE,'YYYY'),-2)||'A'||LPAD(NVL(MAX(SUBSTR(RFA_NO,-4)),0)+1,4,'0') 
        END RFA_NO
  FROM PRI_RP_HDR
 WHERE RFA_NO LIKE CASE WHEN NVL(@[trf_ctrf_flg],'N') = 'Y' THEN
                        'TRF'||SUBSTR(TO_CHAR(SYSDATE,'YYYY'),-2)||'%'
                   ELSE
                        SUBSTR(@[ofc_cd],0,3)||SUBSTR(TO_CHAR(SYSDATE,'YYYY'),-2)||'%'
                   END
   AND PRC_PROP_CRE_TP_CD <> 'I'			]]></sql>
			<params>
				<param name="trf_ctrf_flg" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
