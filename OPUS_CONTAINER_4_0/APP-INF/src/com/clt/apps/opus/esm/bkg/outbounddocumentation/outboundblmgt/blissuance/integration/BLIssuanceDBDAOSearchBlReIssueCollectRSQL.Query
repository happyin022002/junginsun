<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchBlReIssueCollectRSQL">
			<desc><![CDATA[SearchBlReIssueCollect]]></desc>
			<sql><![CDATA[
SELECT 
 BKG_NO
,HIS_SEQ
,ISS_RDEM_KNT
,OBL_RDEM_CFM_FLG
,EVNT_OFC_CD
,EVNT_USR_ID
,TO_CHAR(EVNT_DT, 'YYYY-MM-DD') EVNT_DT
 FROM BKG_DOC_ISS_RDEM
WHERE BKG_NO =@[bkg_no]
AND OBL_RDEM_CFM_FLG ='N'  			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
