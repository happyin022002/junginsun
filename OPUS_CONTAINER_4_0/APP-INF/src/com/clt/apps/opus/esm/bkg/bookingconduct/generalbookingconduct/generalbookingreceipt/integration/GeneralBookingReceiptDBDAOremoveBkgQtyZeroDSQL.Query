<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOremoveBkgQtyZeroDSQL">
			<desc><![CDATA[delete quantity in case of OP_CNTR_QTY = 0]]></desc>
			<sql><![CDATA[
DELETE BKG_QUANTITY
 WHERE BKG_NO = @[bkg_no]
   AND OP_CNTR_QTY = 0			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
