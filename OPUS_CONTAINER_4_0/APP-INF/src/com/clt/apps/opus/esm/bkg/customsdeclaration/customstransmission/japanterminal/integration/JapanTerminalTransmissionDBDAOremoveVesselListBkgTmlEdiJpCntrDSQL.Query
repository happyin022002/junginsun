<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOremoveVesselListBkgTmlEdiJpCntrDSQL">
			<desc><![CDATA[BKG_TML_EDI_JP_CNTR 에서 잘못 나간 VVD bkg 정보 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_TML_EDI_JP_CNTR
WHERE BKG_NO IN (SELECT BKG_NO FROM BKG_TML_EDI_JP_BL 
WHERE	1=1
--AND BKG_NO = 
AND VSL_CD = SUBSTR(@[vvd_cd],1,4)
AND SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
AND SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
AND POL_CD    =@[pol_cd]
AND POL_YD_CD = @[pol_yd_cd]
AND POR_CD    =@[por_cd]
AND POR_YD_CD = NVL(@[por_yd_cd], ''))			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="por_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
