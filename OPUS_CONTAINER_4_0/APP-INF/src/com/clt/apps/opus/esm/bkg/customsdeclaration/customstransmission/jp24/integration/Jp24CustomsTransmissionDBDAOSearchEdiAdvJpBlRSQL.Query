<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24CustomsTransmissionDBDAOSearchEdiAdvJpBlRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT -- 03 Service provider code
       RPAD(' ', 16, ' ') AS DATA00,
       -- 04 Call sign
--       RPAD(NVL(SUBSTR(DECODE(SUBSTR(VVD.POD_CD, 1,2), 'JP', VSL.CALL_SGN_NO, ''), 1, 9), ' '), 9, ' ') AS DATA01,
       RPAD(NVL(SUBSTR(JVSL.CALL_SGN_NO, 1, 9), ' '), 9, ' ') AS DATA01,
       -- 05 vessel name
       RPAD(NVL(SUBSTR(VSL.VSL_ENG_NM, 1, 35), ' '), 35, ' ') AS DATA02,
       -- 06 vessel contury
       RPAD(NVL(DECODE(SUBSTR(VSL.VSL_RGST_CNT_CD, 1, 2),'IM', 'GB', SUBSTR(VSL.VSL_RGST_CNT_CD, 1, 2)), ' '), 2, ' ') AS DATA03,
       -- 07 voyage no
       RPAD(DECODE(JVSL.IB_CSSM_VOY_NO, NULL, SUBSTR(@[vvd], 5, 5), JVSL.IB_CSSM_VOY_NO), 10, ' ') AS DATA04,
       -- 07_1 CONSORTIUM VOYAGE NUMBER
       RPAD(NVL(JVSL.IB_CSSM_VOY_NO, ' '), 10, ' ') AS DATA05,
       -- 08 company code
       COM_ConstantMgr_PKG.COM_getScacCode_FNC() AS DATA06,
       -- 09 loading loc
       CASE
          WHEN SUBSTR(L1.LOC_CD, 1, 2) = 'JP' THEN
             NVL(H1.ATTR_CTNT2, L1.LOC_CD)
          ELSE
             DECODE(H1.ATTR_CTNT2, NULL, DECODE(SUBSTR(L1.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L1.LOC_CD, 1, 2))||'ZZZ', H1.ATTR_CTNT2)
       END AS DATA07,
       -- 10 POL NAME
       RPAD(UPPER(L1.LOC_NM), 20, ' ') AS DATA08,
       -- 11 POL Split No (1)
--       RPAD(' ', 1, ' ') AS DATA09,
       DECODE(@[pol_split_no], '', ' ', @[pol_split_no]) AS DATA09,
       -- 12 B/L NO
       RPAD(COM_ConstantMgr_PKG.COM_getScacCode_FNC()||A.BL_NO||A.BL_SPLIT_NO, 35, ' ') AS DATA10,
       -- 13 출항예정 년월
--       TO_CHAR(POL_SKD.VPS_ETD_DT, 'YYYYMMDD') AS DATA11,
       TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(L1.LOC_CD, POL_SKD.VPS_ETD_DT, 'GMT'), 'YYYYMMDD') AS DATA11,
       -- 14 출항예정 시각
--       TO_CHAR(POL_SKD.VPS_ETD_DT, 'HH24MI') AS DATA12,
       TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(L1.LOC_CD, POL_SKD.VPS_ETD_DT,'GMT'), 'HH24MI') AS DATA12,
       -- 15 그리니치표준
--       DECODE(SIGN(L1.GMT_HRS), -1, '-', '+')||DECODE(TRUNC(ABS(L1.GMT_HRS)/60), 0, '00', LPAD(TRUNC(ABS(L1.GMT_HRS)/60), 2, '0'))||LPAD((MOD(ABS(L1.GMT_HRS), 60)), 2, '0') AS DATA13,
       '00000' AS DATA13,
       -- 16 완화조치예상지역
       RPAD(NVL(JVSL.JO_CD1, ' '), 1, ' ') AS DATA14,
       -- 17 Master B/L 식별
       --RPAD(' ', 1, ' ') AS DATA15,
       RPAD(DECODE(A.RVIS_CNTR_CUST_TP_CD, 'N', 'M', ' '), 1, ' ') AS DATA15,
       -- 18 통지처코드 (반복3)
       RPAD(' ', 5, ' ') AS DATA16,
       -- 18 통지처코드 (반복3)
       RPAD(' ', 5, ' ') AS DATA17,
       -- 18 통지처코드 (반복3)
       RPAD(' ', 5, ' ') AS DATA18,
       -- 19 discharging loc
       CASE
          WHEN SUBSTR(L2.LOC_CD, 1, 2) = 'JP' THEN
             NVL(H2.ATTR_CTNT2, L2.LOC_CD)
          ELSE
             DECODE(H2.ATTR_CTNT2, NULL, DECODE(SUBSTR(L2.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L2.LOC_CD, 1, 2))||'ZZZ', H2.ATTR_CTNT2)
       END AS DATA19,
       -- 20 POD Split No (1)
--       RPAD(' ', 1, ' ') AS DATA20,
       DECODE(@[pod_split_no], '', ' ', @[pod_split_no]) AS DATA20,
       -- 21 입항예정 년월
       TO_CHAR(POD_SKD.VPS_ETA_DT, 'YYYYMMDD') AS DATA21,
       -- 22 오퍼레이션회사 코드
       RPAD(' ', 5, ' ') AS DATA22,
       -- 23 booking POL 코드
       CASE
          WHEN SUBSTR(L3.LOC_CD, 1, 2) = 'JP' THEN
             NVL(H3.ATTR_CTNT2, L3.LOC_CD)
          ELSE
             DECODE(H3.ATTR_CTNT2, NULL, DECODE(SUBSTR(L3.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L3.LOC_CD, 1, 2))||'ZZZ', H3.ATTR_CTNT2)
       END AS DATA23,
       -- 24 pol 코드명
       RPAD(UPPER(L3.LOC_NM), 20, ' ') AS DATA24,
       -- 25 booking DEL 코드
       CASE
          WHEN SUBSTR(L4.LOC_CD, 1, 2) = 'JP' THEN
             NVL(H4.ATTR_CTNT2, L4.LOC_CD)
          ELSE
             DECODE(H4.ATTR_CTNT2, NULL, DECODE(SUBSTR(L4.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L4.LOC_CD, 1, 2))||'ZZZ', H4.ATTR_CTNT2)
       END AS DATA25,
       -- 26 del 코드명
       RPAD(UPPER(L4.LOC_NM), 20, ' ') AS DATA26,
       -- 27 최종 del 코드
       RPAD(' ', 5, ' ') AS DATA27,
       -- 28 최종 del 코드명
       RPAD(' ', 20, ' ') AS DATA28

  FROM BKG_CSTMS_ADV_JP_BL A,
       BKG_CSTMS_CD_CONV_CTNT H1,
       BKG_CSTMS_CD_CONV_CTNT H2,
       BKG_CSTMS_CD_CONV_CTNT H3,
       BKG_CSTMS_CD_CONV_CTNT H4,
       MDM_VSL_CNTR VSL,
       MDM_LOCATION L1,
       MDM_LOCATION L2,
       MDM_LOCATION L3,
       MDM_LOCATION L4,
       VSK_VSL_PORT_SKD POL_SKD,
       VSK_VSL_PORT_SKD POD_SKD,
       BKG_VVD VVD,
       BKG_BOOKING BKG,
       BKG_CSTMS_ADV_JP_VSL_SKD JVSL

 WHERE 1 = 1
   AND A.BL_NO = @[bl_no]
   AND A.BL_SPLIT_NO = NVL(@[bl_split_no], '  ')
   AND A.POL_CD = L1.LOC_CD(+)
   AND A.POD_CD = L2.LOC_CD(+)
   AND A.BKG_POL_CD = L3.LOC_CD(+)
   AND A.BKG_DEL_CD = L4.LOC_CD(+)
   AND H1.CNT_CD(+) = 'JP'
   AND H1.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.POL_CD = H1.ATTR_CTNT1(+)
   AND H2.CNT_CD(+) = 'JP'
   AND H2.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.POD_CD = H2.ATTR_CTNT1(+)
   AND H3.CNT_CD(+) = 'JP'
   AND H3.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.BKG_POL_CD = H3.ATTR_CTNT1(+)
   AND H4.CNT_CD(+) = 'JP'
   AND H4.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.BKG_DEL_CD = H4.ATTR_CTNT1(+)
   AND VSL.VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND POL_SKD.VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND POL_SKD.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND POL_SKD.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND POL_SKD.VPS_PORT_CD = A.POL_CD
   AND POL_SKD.CLPT_IND_SEQ = (SELECT MIN(P.CLPT_IND_SEQ) 
                                 FROM VSK_VSL_PORT_SKD P
                                WHERE 1=1
                                  AND P.VSL_CD = SUBSTR(@[vvd], 1, 4)
                                  AND P.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                  AND P.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
                                  AND P.VPS_PORT_CD = A.POL_CD
                                  AND NVL(P.SKD_CNG_STS_CD, 'X') <> 'S')    /* Skip */
   AND POD_SKD.VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND POD_SKD.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND POD_SKD.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND POD_SKD.VPS_PORT_CD = VVD.POD_CD
   AND POD_SKD.CLPT_IND_SEQ = (SELECT MIN(P.CLPT_IND_SEQ) 
                                 FROM VSK_VSL_PORT_SKD P
                                WHERE 1=1
                                  AND P.VSL_CD = SUBSTR(@[vvd], 1, 4)
                                  AND P.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                  AND P.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
                                  AND P.VPS_PORT_CD = VVD.POD_CD
                                  AND NVL(P.SKD_CNG_STS_CD, 'X') <> 'S')    /* Skip */
   AND A.BL_NO = BKG.BL_NO
   AND BKG.BKG_NO = VVD.BKG_NO
   AND A.POL_CD = VVD.POL_CD
   AND POL_SKD.VSL_CD = JVSL.VSL_CD
   AND POL_SKD.SKD_VOY_NO = JVSL.SKD_VOY_NO
   AND POL_SKD.SKD_DIR_CD = JVSL.SKD_DIR_CD
   AND POL_SKD.VPS_PORT_CD = JVSL.POL_CD
   AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_split_no" type="12" value="" out="N"/>
				<param name="pod_split_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
