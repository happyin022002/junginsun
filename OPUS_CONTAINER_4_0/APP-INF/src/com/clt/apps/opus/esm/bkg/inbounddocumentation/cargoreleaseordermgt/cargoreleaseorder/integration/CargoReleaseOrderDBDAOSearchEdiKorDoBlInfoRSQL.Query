<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchEdiKorDoBlInfoRSQL">
			<desc><![CDATA[Do Reference 정보 조회]]></desc>
			<sql><![CDATA[
SELECT CHR(10)
    ||'BL_NO:'          || NVL(BKGM.BL_NO,' ')                                                                              || CHR(10)
    || 'MSG_FUNC:'      || DECODE(@[do_type],'KDC','C','N')                                                                 || CHR(10)
    || 'DO_NBR:'        || DECODE(@[do_type],'KDL', NVL(BDO.DO_NO||BDO.DO_NO_SPLIT,'NONDO'), BDO.DO_NO||BDO.DO_NO_SPLIT )   || CHR(10)
    || 'MRN_NBR:'       || MSN.MF_REF_NO||MSN.MRN_CHK_NO                                                                    || CHR(10)
    || 'MSN_NBR:'       || MSN.MF_SEQ_NO                                                                                    || CHR(10)
    || 'VVD:'           || BVVD.VSL_CD||BVVD.SKD_VOY_NO||BVVD.SKD_DIR_CD                                                    || CHR(10)
    || 'VSL_CALLSIGN:'  || VSL.CALL_SGN_NO                                                                                  || CHR(10)
    || 'VSL_FULLNAME:'  || VSL.VSL_ENG_NM                                                                                   || CHR(10)
    || 'FINAL_DEST:'    || BKGM.DEL_CD                                                                                      || CHR(10)
    || 'FINAL_DESTNAME:'|| DEL.LOC_NM||'  '||CNT1.CNT_NM                                                                    || CHR(10)
    || 'DO_DATE:'       || NVL(TO_CHAR(DTL.EVNT_DT,'YYYYMMDD'),' ')                                                         || CHR(10)
    || 'DEMU_FROM:'     || NVL(TO_CHAR(NULL,'YYYYMMDDHHMM'),' ')                                                            || CHR(10)
    || 'DEMU_TO:'       || NVL(TO_CHAR(NULL,'YYYYMMDDHHMM'),' ')                                                            || CHR(10)
    || 'SHPR_CD:'       || SH.CUST_CNT_CD || SH.CUST_SEQ                                                                    || CHR(10)
    || 'SHPR_NAME1:'    || BKG_TOKEN_NL_FNC(SH.CUST_NM,   1, '')                                                            || CHR(10)
    || 'SHPR_NAME2:'    || BKG_TOKEN_NL_FNC(SH.CUST_NM,   2, '')                                                            || CHR(10)
    || 'SHPR_NAME3:'    || DECODE(SH.ADDR_PRN_FLG, 'Y', BKG_TOKEN_NL_FNC(SH.CUST_ADDR, 1, ''), '')                          || CHR(10)
    || 'SHPR_NAME4:'    || DECODE(SH.ADDR_PRN_FLG, 'Y', BKG_TOKEN_NL_FNC(SH.CUST_ADDR, 2, ''), '')                          || CHR(10)
    || 'SHPR_NAME5:'    || DECODE(SH.ADDR_PRN_FLG, 'Y', BKG_TOKEN_NL_FNC(SH.CUST_ADDR, 3, ''), '')                          || CHR(10)
    || 'CNEE_CD:'       || CN.CUST_CNT_CD || CN.CUST_SEQ                                                                    || CHR(10)
    || 'CNEE_NAME1:'    || BKG_TOKEN_NL_FNC(CN.CUST_NM,   1, '')                                                            || CHR(10)
    || 'CNEE_NAME2:'    || BKG_TOKEN_NL_FNC(CN.CUST_NM,   2, '')                                                            || CHR(10)
    || 'CNEE_NAME3:'    || BKG_TOKEN_NL_FNC(REPLACE(REPLACE(CN.CUST_ADDR,CHR(10),CHR(13)||' '),CHR(13)||CHR(13),CHR(13)),1, '') || CHR(10)
    || 'CNEE_NAME4:'    || BKG_TOKEN_NL_FNC(REPLACE(REPLACE(CN.CUST_ADDR,CHR(10),CHR(13)||' '),CHR(13)||CHR(13),CHR(13)),2, '') || CHR(10)
    || 'CNEE_NAME5:'    || BKG_TOKEN_NL_FNC(REPLACE(REPLACE(CN.CUST_ADDR,CHR(10),CHR(13)||' '),CHR(13)||CHR(13),CHR(13)),3, '') || CHR(10)
    || 'CNEE_REG_NO:'   || NVL(BDO.RCVR_BIZ_NO,'')                                                                          || CHR(10) /* 2007.06.11 */
    || 'NTFY_CD:'       || NF.CUST_CNT_CD || NF.CUST_SEQ                                                                    || CHR(10)
    || 'NTFY_NAME1:'    || BKG_TOKEN_NL_FNC(NF.CUST_NM, 1, '')                                                              || CHR(10)
    || 'NTFY_NAME2:'    || BKG_TOKEN_NL_FNC(NF.CUST_NM, 2, '')                                                              || CHR(10)
    || 'NTFY_FAX_NO:'   || NF.CUST_FAX_NO                                                                                   || CHR(10)
    || 'SELF_TRANS:'    || BDO.SELF_TRNS_FLG                                                                                || CHR(10)
    || 'MSN_DISCLOC:'   || MSN.CSTMS_DCHG_LOC_WH_CD                                                                         || CHR(10)
    || 'POD_LOC:'       || BKGM.POD_CD                                                                                      || CHR(10)
    || 'POD_NAME:'      || POD.LOC_NM||'  '||CNT2.CNT_NM                                                                    || CHR(10)
 FROM BKG_BOOKING BKGM
    , BKG_DO BDO
    , BKG_DO_DTL DTL
    , BKG_CSTMS_KR_MF_SEQ_NO MSN
    , BKG_VVD BVVD
    , MDM_VSL_CNTR VSL
    , MDM_LOCATION DEL
    , MDM_COUNTRY  CNT1
    , BKG_CUSTOMER SH
    , BKG_CUSTOMER CN
    , BKG_CUSTOMER NF
    , MDM_LOCATION POD
    , MDM_COUNTRY  CNT2
WHERE BKGM.BKG_NO         = @[bkg_no]
  AND BKGM.BKG_NO         = BDO.BKG_NO(+)
  AND BDO.BKG_NO          = DTL.BKG_NO(+)
  AND BDO.RLSE_SEQ        = DTL.RLSE_SEQ(+)
  AND DTL.RLSE_STS_CD(+)  = 'R'
  AND MSN.BKG_NO          = BKGM.BKG_NO 
  AND MSN.MF_CFM_FLG      = 'Y'
  AND MSN.MRN_BL_TS_CD    = 'I'
  AND MSN.CFM_DT          = ( SELECT MAX(SEQ.CFM_DT) 
                              FROM BKG_CSTMS_KR_MF_SEQ_NO SEQ
                              WHERE SEQ.BKG_NO        = @[bkg_no]
                              AND   SEQ.MF_CFM_FLG    = 'Y'
                              AND   SEQ.MRN_BL_TS_CD  = 'I' )  
  AND BVVD.BKG_NO         = BKGM.BKG_NO
  AND BVVD.POD_CD         = BKGM.POD_CD
  AND BVVD.VSL_PRE_PST_CD IN ('T','U')
  AND BVVD.VSL_CD         = VSL.VSL_CD(+)
  AND BKGM.DEL_CD         = DEL.LOC_CD(+)
  AND DEL.CNT_CD          = CNT1.CNT_CD(+)
  AND BKGM.BKG_NO         = SH.BKG_NO(+)
  AND SH.BKG_CUST_TP_CD   = 'S'
  AND BKGM.BKG_NO         = CN.BKG_NO(+)
  AND CN.BKG_CUST_TP_CD   = 'C'
  AND BKGM.BKG_NO         = NF.BKG_NO(+)
  AND NF.BKG_CUST_TP_CD   = 'N'
  AND BKGM.POD_CD         = POD.LOC_CD(+)
  AND POD.CNT_CD          = CNT2.CNT_CD(+)			]]></sql>
			<params>
				<param name="do_type" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
