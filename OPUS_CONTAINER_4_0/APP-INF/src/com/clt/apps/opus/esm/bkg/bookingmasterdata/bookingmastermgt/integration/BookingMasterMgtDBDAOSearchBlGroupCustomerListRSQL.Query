<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchBlGroupCustomerListRSQL">
			<desc><![CDATA[select Group Customer]]></desc>
			<sql><![CDATA[
SELECT 
    BL_GRP_SEQ,
	CUST_CNT_CD||LPAD(CUST_SEQ,6,'0') AS OLD_CUST_CD,
    CUST_CNT_CD,
    CUST_SEQ,
    CUST_CNT_CD||LPAD(CUST_SEQ,6,'0') AS CUST_CD,
    (SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER WHERE CUST_CNT_CD = A.CUST_CNT_CD AND CUST_SEQ = A.CUST_SEQ) AS CUST_NM,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
FROM BKG_CTRL_BL_GRP_CUST A
WHERE BL_GRP_SEQ = @[bl_grp_seq]			]]></sql>
			<params>
				<param name="bl_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
