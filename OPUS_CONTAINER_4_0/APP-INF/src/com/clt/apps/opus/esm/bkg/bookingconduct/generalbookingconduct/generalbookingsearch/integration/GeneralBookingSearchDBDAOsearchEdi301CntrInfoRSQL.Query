<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchEdi301CntrInfoRSQL">
			<desc><![CDATA[searchEdi301CntrInfo]]></desc>
			<sql><![CDATA[
SELECT '{CNTR_INFO'																|| CHR(10)
	|| 'CNTN:'					|| RPAD(NVL(Cntr.CNTR_NO, ' '), 11, ' ')		|| CHR(10)
	|| 'CNTT:'					|| NVL(CNTR.CNTR_TPSZ_CD,SCHC.CNTR_TPSZ_CD) 	|| CHR(10)
	|| 'OB_HAUL_TP:'			|| CASE WHEN POL.CONTI_CD <> 'E' THEN DECODE(cntr.RCV_TERM_CD,'D','C','M')
                                            ELSE NVL((SELECT ETRO.HLG_TP_CD FROM BKG_EUR_TRO ETRO WHERE ETRO.BKG_NO = BK.BKG_NO AND ETRO.IO_BND_CD = 'O' AND ETRO.CXL_FLG = 'N' AND ROWNUM = 1),DECODE(cntr.RCV_TERM_CD,'D','C','M')) 
                                            END	|| CHR(10)
	|| 'IB_HAUL_TP:'			|| CASE WHEN POD.CONTI_CD <> 'E' THEN DECODE(cntr.DE_TERM_CD,'D','C','M')
                                            ELSE NVL((SELECT ETRO.HLG_TP_CD FROM BKG_EUR_TRO ETRO WHERE ETRO.BKG_NO = BK.BKG_NO AND ETRO.IO_BND_CD = 'I' AND ETRO.CXL_FLG = 'N' AND ROWNUM = 1),DECODE(cntr.RCV_TERM_CD,'D','C','M'))
                                            END	|| CHR(10)
	|| 'SEAL:'					|| REPLACE(REPLACE(REPLACE(NVL(seal.CNTR_SEAL_NO,' '),CHR(32),''),CHR(10),''),CHR(13),'')	|| CHR(10)
	|| 'RIND:'                  || DECODE(Cntr.RC_FLG,'Y','1','0')				|| CHR(10)
	|| 'DIND:'                  || DECODE(Cntr.DCGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'AIND:'                  || DECODE(Cntr.AWK_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'BIND:'                  || DECODE(Cntr.BB_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'PKG_QTY:'               || cntr.pck_qty                                 || CHR(10)
	|| 'PKG_TP:'                || cntr.pck_tp_cd                               || CHR(10)
	|| 'MEA_QTY:'               || cntr.meas_qty                                || CHR(10)
	|| 'MEA_TP:'                || cntr.meas_ut_cd                              || CHR(10)
	|| 'WGT_QTY:'               || CASE WHEN NVL(Cntr.CNTR_WGT,0) = 0 THEN ROUND((SELECT BLD.ACT_WGT/NVL((SELECT SUM(BQ.OP_CNTR_QTY) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = BLD.BKG_NO),1) FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = BK.BKG_NO),3)
                                        ELSE Cntr.CNTR_WGT END 					|| CHR(10)
	|| 'WGT_TP:'                || CASE WHEN Cntr.WGT_UT_CD IS NULL THEN (SELECT BLD.WGT_UT_CD FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = BK.BKG_NO)
                                        ELSE Cntr.WGT_UT_CD END					|| CHR(10)
	|| 'NET_WGT_QTY:'           || Cntr.CNTR_WGT								|| CHR(10)
	|| 'NET_WGT_TP:'            || Cntr.WGT_UT_CD								|| CHR(10)
	|| 'TEMP:'																	|| CHR(10)
	|| 'TUN:'																	|| CHR(10)
	|| 'TEMP_C:'																|| CHR(10)
	|| 'TUN_C:'																	|| CHR(10)
	|| 'RF_VOLTAGE:'															|| CHR(10)
	|| 'VENT:'																	|| CHR(10)
    || 'VENT_UNIT:'																|| CHR(10)
    || 'O2_PC_LVL_QTY:'															|| CHR(10)
    || 'CO2_PC_LVL_QTY:'														|| CHR(10)
	|| 'HUMID:'																	|| CHR(10)
	|| 'GENSET:'																|| CHR(10)
	|| 'RF_REMARK:'																|| CHR(10)
	|| 'RFDRY_IND:'				||DECODE(cntr.RD_CGO_FLG,'N','0','Y','1',CNTR.RD_CGO_FLG)|| CHR(10)
	|| 'OVF:'																	|| CHR(10)
	|| 'OVR:'																	|| CHR(10)
	|| 'OVL_UNIT:'																|| CHR(10)
	|| 'OVH:'																	|| CHR(10)
    || 'OVH_UNIT:'																|| CHR(10)
	|| 'OVLW:'																	|| CHR(10)
	|| 'OVRW:'																	|| CHR(10)
    || 'OVW_UNIT:'																|| CHR(10)
	|| 'OVWGT:'																	|| CHR(10)
	|| 'VOID_SLOT:'																|| CHR(10)
	|| 'STWG_REQ:'																|| CHR(10)
	|| 'TTL_DIM_LEN:'															|| CHR(10)
    || 'TTL_DIM_LEN_UNIT:'														|| CHR(10)
	|| 'TTL_DIM_WDT:'															|| CHR(10)
    || 'TTL_DIM_WDT_UNIT:'														|| CHR(10)
	|| 'TTL_DIM_HGT:'															|| CHR(10)
    || 'TTL_DIM_HGT_UNIT:'														|| CHR(10)
	|| 'TRM_TYPE:'																|| CHR(10)
	|| 'TRM_WEIGHT:'															|| CHR(10)
	|| 'TRM_HAULAGE:'															|| CHR(10)
	|| 'TRM_HMODE:'																|| CHR(10)
	|| 'TRM_PICKUP_CY:'															|| CHR(10)
	|| 'TRM_RETURN_CY:'															|| CHR(10)
	|| 'TRM_INSTRUCTION:'														|| CHR(10)
	|| 'TRM_TRAN_DT:'															|| CHR(10)
	|| 'TRM_TRAN_OFFICE:'														|| CHR(10)
	|| 'TRM_TRAN_NO:'															|| CHR(10)
	|| 'USR_ID:'																|| CHR(10)
	|| 'CNTR_RCV_TERM:'			||CNTR.RCV_TERM_CD								|| CHR(10)
	|| 'CNTR_DLV_TERM:'			||CNTR.DE_TERM_CD								|| CHR(10)
	|| '{CNTR_TRO_DTL'															|| CHR(10)
    || 'TRD_DIR_IND:'															|| CHR(10)
	|| 'TRD_ADDR:'																|| CHR(10)
	|| 'TRD_DOOR_LOC:'															|| CHR(10)
	|| 'TRD_DOOR_POSTAL:'														|| CHR(10)
	|| 'TRD_DOOR_DT:'															|| CHR(10)
    || 'TRD_DOOR_DT_GTM:'														|| CHR(10)
	|| 'TRD_PERSON:'															|| CHR(10)
	|| 'TRD_TEL:'																|| CHR(10)
	|| 'TRD_FAX:'																|| CHR(10)
    || 'TRD_EMAIL:'																|| CHR(10)
    || 'TRD_DEPARTURE:'															|| CHR(10)
	|| 'TRD_ACTSHIP:'															|| CHR(10)
	|| 'TRD_REMARK:'															|| CHR(10)
	|| '}CNTR_TRO_DTL'															|| CHR(10)
	|| '}CNTR_INFO'																|| CHR(10) CNTR_INFO
  FROM BKG_BOOKING Bk
        , BKG_CONTAINER cntr
        , BKG_CNTR_SEAL_NO seal
        , MDM_LOCATION POL
        , MDM_LOCATION POD
        , SCE_COP_HDR SCHC
-- WHERE Bk.BKG_NO   	    = cntr.BKG_NO
--   AND cntr.BKG_NO      = seal.BKG_NO       (+)
--   AND cntr.CNTR_NO     = seal.CNTR_NO      (+)
--   AND 1                = seal.CNTR_SEAL_SEQ(+)
   WHERE BK.BKG_NO   	    = SCHC.BKG_NO
   AND SCHC.COP_STS_CD <> 'X'
   AND SCHC.BKG_NO      = CNTR.BKG_NO(+)
   AND SCHC.CNTR_NO     = CNTR.CNTR_NO(+)
   AND cntr.CNTR_NO     = seal.CNTR_NO      (+)
   AND 1                = seal.CNTR_SEAL_SEQ(+)

   AND cntr.DCGO_FLG(+)    = 'N'
   AND cntr.RC_FLG(+)      = 'N'
   AND cntr.AWK_CGO_FLG(+) = 'N'
   and Bk.BKG_NO        = @[bkg_no]
   AND BK.POL_CD = POL.LOC_CD
   AND BK.POD_CD = POD.LOC_CD
   AND ROWNUM <= (SELECT TTL
       -(SELECT COUNT(*) FROM BKG_RF_CGO RF WHERE RF.BKG_NO = BQ.BKG_NO AND RF.cntr_tpsz_cd is not null)
       -(SELECT COUNT(DISTINCT DG.DG_CNTR_SEQ) FROM BKG_DG_CGO DG WHERE DG.BKG_NO = BQ.BKG_NO AND DG.cntr_tpsz_cd is not null)
       -(SELECT COUNT(*) FROM BKG_AWK_CGO AWK WHERE AWK.BKG_NO = BQ.BKG_NO AND AWK.CNTR_TPSZ_CD NOT LIKE 'Q%')
        FROM (
            SELECT BQ.BKG_NO
                   ,SUM(BQ.OP_CNTR_QTY) TTL
            FROM BKG_QUANTITY BQ
            WHERE BQ.BKG_NO = @[bkg_no]
            AND BQ.CNTR_TPSZ_CD NOT LIKE 'Q%'
            GROUP BY BQ.BKG_NO) BQ)
UNION ALL
SELECT '{CNTR_INFO'																|| CHR(10)
	|| 'CNTN:'					|| RPAD(NVL(Cntr.CNTR_NO, ' '), 11, ' ')		|| CHR(10)
	|| 'CNTT:'					|| NVL(CNTR.CNTR_TPSZ_CD,SCHC.CNTR_TPSZ_CD) 	|| CHR(10)
	|| 'OB_HAUL_TP:'			|| CASE WHEN POL.CONTI_CD <> 'E' THEN DECODE(cntr.RCV_TERM_CD,'D','C','M')
                                            ELSE NVL((SELECT ETRO.HLG_TP_CD FROM BKG_EUR_TRO ETRO WHERE ETRO.BKG_NO = BK.BKG_NO AND ETRO.IO_BND_CD = 'O' AND ETRO.CXL_FLG = 'N' AND ROWNUM = 1),DECODE(cntr.RCV_TERM_CD,'D','C','M')) 
                                            END	|| CHR(10)
	|| 'IB_HAUL_TP:'			|| CASE WHEN POD.CONTI_CD <> 'E' THEN DECODE(cntr.DE_TERM_CD,'D','C','M')
                                            ELSE NVL((SELECT ETRO.HLG_TP_CD FROM BKG_EUR_TRO ETRO WHERE ETRO.BKG_NO = BK.BKG_NO AND ETRO.IO_BND_CD = 'I' AND ETRO.CXL_FLG = 'N' AND ROWNUM = 1),DECODE(cntr.RCV_TERM_CD,'D','C','M'))
                                            END	|| CHR(10)
	|| 'SEAL:'					|| REPLACE(REPLACE(REPLACE(NVL(seal.CNTR_SEAL_NO,' '),CHR(32),''),CHR(10),''),CHR(13),'')	|| CHR(10)
	|| 'RIND:'                  || DECODE(Cntr.RC_FLG,'Y','1','0')				|| CHR(10)
	|| 'DIND:'                  || DECODE(Cntr.DCGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'AIND:'                  || DECODE(Cntr.AWK_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'BIND:'                  || DECODE(Cntr.BB_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'PKG_QTY:'               || cntr.pck_qty                                 || CHR(10)
	|| 'PKG_TP:'                || cntr.pck_tp_cd                               || CHR(10)
	|| 'MEA_QTY:'               || cntr.meas_qty                                || CHR(10)
	|| 'MEA_TP:'                || cntr.meas_ut_cd                              || CHR(10)
	|| 'WGT_QTY:'               || CASE WHEN NVL(Cntr.CNTR_WGT,0) = 0 THEN ROUND((SELECT BLD.ACT_WGT/NVL((SELECT SUM(BQ.OP_CNTR_QTY) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = BLD.BKG_NO),1) FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = BK.BKG_NO),3)
                                        ELSE Cntr.CNTR_WGT END 					|| CHR(10)
	|| 'WGT_TP:'                || CASE WHEN Cntr.WGT_UT_CD IS NULL THEN (SELECT BLD.WGT_UT_CD FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = BK.BKG_NO)
                                        ELSE Cntr.WGT_UT_CD END					|| CHR(10)
	|| 'NET_WGT_QTY:'           || Cntr.CNTR_WGT								|| CHR(10)
	|| 'NET_WGT_TP:'            || Cntr.WGT_UT_CD								|| CHR(10)
	|| 'TEMP:'																	|| CHR(10)
	|| 'TUN:'																	|| CHR(10)
	|| 'TEMP_C:'																|| CHR(10)
	|| 'TUN_C:'																	|| CHR(10)
	|| 'RF_VOLTAGE:'															|| CHR(10)
	|| 'VENT:'																	|| CHR(10)
    || 'VENT_UNIT:'																|| CHR(10)
    || 'O2_PC_LVL_QTY:'															|| CHR(10)
    || 'CO2_PC_LVL_QTY:'														|| CHR(10)
	|| 'HUMID:'																	|| CHR(10)
	|| 'GENSET:'																|| CHR(10)
	|| 'RF_REMARK:'																|| CHR(10)
	|| 'RFDRY_IND:'				||DECODE(cntr.RD_CGO_FLG,'N','0','Y','1',CNTR.RD_CGO_FLG)|| CHR(10)
	|| 'OVF:'																	|| CHR(10)
	|| 'OVR:'																	|| CHR(10)
	|| 'OVL_UNIT:'																|| CHR(10)
	|| 'OVH:'																	|| CHR(10)
    || 'OVH_UNIT:'																|| CHR(10)
	|| 'OVLW:'																	|| CHR(10)
	|| 'OVRW:'																	|| CHR(10)
    || 'OVW_UNIT:'																|| CHR(10)
	|| 'OVWGT:'																	|| CHR(10)
	|| 'VOID_SLOT:'																|| CHR(10)
	|| 'STWG_REQ:'																|| CHR(10)
	|| 'TTL_DIM_LEN:'															|| CHR(10)
    || 'TTL_DIM_LEN_UNIT:'														|| CHR(10)
	|| 'TTL_DIM_WDT:'															|| CHR(10)
    || 'TTL_DIM_WDT_UNIT:'														|| CHR(10)
	|| 'TTL_DIM_HGT:'															|| CHR(10)
    || 'TTL_DIM_HGT_UNIT:'														|| CHR(10)
	|| 'TRM_TYPE:'																|| CHR(10)
	|| 'TRM_WEIGHT:'															|| CHR(10)
	|| 'TRM_HAULAGE:'															|| CHR(10)
	|| 'TRM_HMODE:'																|| CHR(10)
	|| 'TRM_PICKUP_CY:'															|| CHR(10)
	|| 'TRM_RETURN_CY:'															|| CHR(10)
	|| 'TRM_INSTRUCTION:'														|| CHR(10)
	|| 'TRM_TRAN_DT:'															|| CHR(10)
	|| 'TRM_TRAN_OFFICE:'														|| CHR(10)
	|| 'TRM_TRAN_NO:'															|| CHR(10)
	|| 'USR_ID:'																|| CHR(10)
	|| 'CNTR_RCV_TERM:'			||CNTR.RCV_TERM_CD								|| CHR(10)
	|| 'CNTR_DLV_TERM:'			||CNTR.DE_TERM_CD								|| CHR(10)
	|| '{CNTR_TRO_DTL'															|| CHR(10)
    || 'TRD_DIR_IND:'															|| CHR(10)
	|| 'TRD_ADDR:'																|| CHR(10)
	|| 'TRD_DOOR_LOC:'															|| CHR(10)
	|| 'TRD_DOOR_POSTAL:'														|| CHR(10)
	|| 'TRD_DOOR_DT:'															|| CHR(10)
    || 'TRD_DOOR_DT_GTM:'														|| CHR(10)
	|| 'TRD_PERSON:'															|| CHR(10)
	|| 'TRD_TEL:'																|| CHR(10)
	|| 'TRD_FAX:'																|| CHR(10)
    || 'TRD_EMAIL:'																|| CHR(10)
    || 'TRD_DEPARTURE:'															|| CHR(10)
	|| 'TRD_ACTSHIP:'															|| CHR(10)
	|| 'TRD_REMARK:'															|| CHR(10)
	|| '}CNTR_TRO_DTL'															|| CHR(10)
	|| '}CNTR_INFO'																|| CHR(10) CNTR_INFO
  FROM BKG_BOOKING Bk
        , BKG_CONTAINER cntr
        , BKG_CNTR_SEAL_NO seal
        , MDM_LOCATION POL
        , MDM_LOCATION POD
        , SCE_COP_HIS SCHC
-- WHERE Bk.BKG_NO   	    = cntr.BKG_NO
--   AND cntr.BKG_NO      = seal.BKG_NO       (+)
--   AND cntr.CNTR_NO     = seal.CNTR_NO      (+)
--   AND 1                = seal.CNTR_SEAL_SEQ(+)
   WHERE BK.BKG_NO   	    = SCHC.BKG_NO
   AND SCHC.COP_STS_CD = 'X'
   AND BK.BKG_STS_CD = 'X'
   AND NOT EXISTS (SELECT 'X' FROM SCE_COP_HDR HDR WHERE HDR.BKG_NO = SCHC.BKG_NO AND HDR.COP_STS_CD <> 'X')
   AND SCHC.ACCL_SEQ = (SELECT MAX(HIS.ACCL_SEQ) FROM SCE_COP_HIS HIS WHERE HIS.COP_NO = SCHC.COP_NO AND HIS.COP_STS_CD = 'X')
   AND SCHC.BKG_NO      = CNTR.BKG_NO(+)
   AND SCHC.CNTR_NO     = CNTR.CNTR_NO(+)
   AND cntr.CNTR_NO     = seal.CNTR_NO      (+)
   AND 1                = seal.CNTR_SEAL_SEQ(+)

   AND cntr.DCGO_FLG(+)    = 'N'
   AND cntr.RC_FLG(+)      = 'N'
   AND cntr.AWK_CGO_FLG(+) = 'N'
   and Bk.BKG_NO        = @[bkg_no]
   AND BK.POL_CD = POL.LOC_CD
   AND BK.POD_CD = POD.LOC_CD
   AND ROWNUM <= (SELECT TTL
       -(SELECT COUNT(*) FROM BKG_RF_CGO RF WHERE RF.BKG_NO = BQ.BKG_NO and rf.cntr_tpsz_cd is not null)
       -(SELECT COUNT(DISTINCT DG.DG_CNTR_SEQ) FROM BKG_DG_CGO DG WHERE DG.BKG_NO = BQ.BKG_NO AND DG.cntr_tpsz_cd is not null)
       -(SELECT COUNT(*) FROM BKG_AWK_CGO AWK WHERE AWK.BKG_NO = BQ.BKG_NO AND AWK.CNTR_TPSZ_CD NOT LIKE 'Q%')
        FROM (
            SELECT BQ.BKG_NO
                   ,SUM(BQ.OP_CNTR_QTY) TTL
            FROM BKG_QUANTITY BQ
            WHERE BQ.BKG_NO = @[bkg_no]
            AND BQ.CNTR_TPSZ_CD NOT LIKE 'Q%'
            GROUP BY BQ.BKG_NO) BQ)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
