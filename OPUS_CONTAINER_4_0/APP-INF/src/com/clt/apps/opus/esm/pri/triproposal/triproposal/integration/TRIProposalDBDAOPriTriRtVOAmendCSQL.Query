<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriRtVOAmendCSQL">
			<desc><![CDATA[PRI_TRI_RT Amend]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRI_RT
  (TRI_PROP_NO
  ,AMDT_SEQ
  ,RAT_UT_CD
  ,PRC_CGO_TP_CD
  ,CURR_CD
  ,PROP_FRT_RT_AMT
  ,NOTE_CTNT
  ,NOTE_CONV_MAPG_ID
  ,EFF_DT
  ,EXP_DT
  ,TRI_RQST_OFC_CD
  ,TRI_RQST_USR_ID
  ,TRI_APRO_OFC_CD
  ,PROP_STS_CD
  ,GRI_APPL_TP_CD
  ,GRI_APPL_AMT
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
  ,TRI_RMK )
  SELECT TRI_PROP_NO
        ,AMDT_SEQ + 1
        ,RAT_UT_CD
        ,PRC_CGO_TP_CD
        ,CURR_CD
        ,NULL
        ,NOTE_CTNT
        ,SYS_GUID() AS NOTE_CONV_MAPG_ID
        ,TO_DATE(TO_CHAR(SYSDATE, 'YYYYMMDD'), 'YYYY-MM-DD')
        ,EXP_DT
        ,@[tri_rqst_ofc_cd]
        ,@[tri_rqst_usr_id]
        ,NULL
        ,'I'
        ,'N'
        ,0
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[upd_usr_id]
        ,SYSDATE
        ,TRI_RMK
    FROM PRI_TRI_RT
   WHERE TRI_PROP_NO = @[tri_prop_no]
     AND AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="tri_rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="tri_rqst_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="tri_prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
