<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteProposalDBDAOPriSpScpNoteCtntMigHisCSQL">
			<desc><![CDATA[insert PRI_SP_SCP_NOTE_CTNT_MIG_HIS about demdet for note]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_NOTE_CTNT_MIG_HIS 
(
	 PROP_NO        
	,AMDT_SEQ       
	,SVC_SCP_CD     
	,NOTE_TP_CD     
	,NOTE_SEQ       
	,NOTE_CTNT_SEQ  
	,BKUP_DT
	,CHG_CD         
	,NOTE_CTNT      
	,NOTE_CONV_FLG  
	,DP_SEQ         
	,NOTE_CONV_MAPG_ID
	,NOTE_CHG_TP_CD 
	,PRC_PROG_STS_CD
	,SRC_INFO_CD    
	,N1ST_CMNC_AMDT_SEQ 
	,ACPT_USR_ID    
	,ACPT_OFC_CD    
	,ACPT_DT        
	,CRE_USR_ID     
	,CRE_DT         
	,UPD_USR_ID     
	,UPD_DT         
)
SELECT
      A.PROP_NO        
	, A.AMDT_SEQ       
	, A.SVC_SCP_CD     
	, A.NOTE_TP_CD     
	, A.NOTE_SEQ       
	, A.NOTE_CTNT_SEQ  
	, SYSDATE
	, A.CHG_CD         
	, A.NOTE_CTNT      
	, A.NOTE_CONV_FLG  
	, A.DP_SEQ         
	, A.NOTE_CONV_MAPG_ID
	, A.NOTE_CHG_TP_CD 
	, A.PRC_PROG_STS_CD
	, A.SRC_INFO_CD    
	, A.N1ST_CMNC_AMDT_SEQ 
	, A.ACPT_USR_ID    
	, A.ACPT_OFC_CD    
	, A.ACPT_DT        
	, A.CRE_USR_ID     
	, A.CRE_DT         
	, A.UPD_USR_ID     
	, A.UPD_DT 
  FROM PRI_SP_SCP_NOTE_CTNT A
     , PRI_SP_SCP_NOTE B
 WHERE A.PROP_NO    = B.PROP_NO
   AND A.AMDT_SEQ   = B.AMDT_SEQ
   AND A.SVC_SCP_CD = B.SVC_SCP_CD
   AND A.NOTE_TP_CD = B.NOTE_TP_CD
   AND A.NOTE_SEQ   = B.NOTE_SEQ
   AND A.PROP_NO    = @[prop_no]
   AND A.AMDT_SEQ   = @[amdt_seq]
   AND A.SVC_SCP_CD = @[svc_scp_cd]
   AND A.NOTE_TP_CD = 'P'
   AND B.NOTE_CLSS_CD = 'D'			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
