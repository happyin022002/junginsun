<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArgCustomsTransmissionDBDAOsearchTransmitCmListRSQL">
			<desc><![CDATA[searchTransmitCmList]]></desc>
			<sql><![CDATA[
SELECT CM.PCK_QTY          AS CM_PKG
      ,NVL((SELECT CSTMS_PCK_TP_CD
              FROM BKG_CSTMS_PCK_TP_CONV AA
             WHERE AA.CNT_CD = 'AR'
               AND AA.PCK_TP_CD = CM.PCK_TP_CD
           ),CM.PCK_TP_CD) AS CM_PKGU
      ,CM.CNTR_MF_WGT      AS CM_WGT
      ,CM.WGT_UT_CD        AS CM_WGT_UNIT
      ,CM.CMDT_HS_CD       AS CM_HSCODE
      ,SUBSTR(BKG_TOKEN_NL_FNC(CM.CNTR_MF_GDS_DESC,0, ''), 1, 80)  AS CM_DESC
      ,SUBSTR(BKG_TOKEN_NL_FNC(CM.CNTR_MF_MK_DESC, 0, ''), 1, 100) AS CM_MARKNO
      ,CNT.CNTR_NO         AS CM_CNTRNBR
      ,DECODE(SUBSTR(CNT.CNTR_TPSZ_CD, 2, 1), '2', '20', '40') AS CM_CNTRSIZE
  FROM BKG_CNTR_MF_DESC CM
      ,BKG_CONTAINER CNT
 WHERE CM.CNTR_NO(+) = CNT.CNTR_NO
   AND CM.BKG_NO(+) = CNT.BKG_NO
   AND CNT.BKG_NO  = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
