<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOBkgRevDrNoteProgVOAddSearch2CSQL">
			<desc><![CDATA[prog_seq 증가하여 복사]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_REV_DR_NOTE_PROG (
	RDN_NO
,	RVIS_SEQ
,	PROG_SEQ
,	RDN_STS_CD
,	RDN_RMK
,	CRE_OFC_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT 
	RDN_NO
,	RVIS_SEQ AS RVIS_SEQ
,	(PROG_SEQ+1) AS PROG_SEQ
,	@[rdn_sts_cd]
,	@[rdn_rmk]
,	CRE_OFC_CD
,	@[upd_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM  BKG_REV_DR_NOTE_PROG
WHERE RDN_NO = @[rdn_no]
AND	  RVIS_SEQ = @[rvis_seq]
AND	  PROG_SEQ = (SELECT MAX(PROG_SEQ)
					FROM BKG_REV_DR_NOTE_PROG
				   WHERE RDN_NO = @[rdn_no]
				     AND RVIS_SEQ = @[rvis_seq])			]]></sql>
			<params>
				<param name="rdn_sts_cd" type="12" value="" out="N"/>
				<param name="rdn_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rdn_no" type="12" value="" out="N"/>
				<param name="rvis_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
