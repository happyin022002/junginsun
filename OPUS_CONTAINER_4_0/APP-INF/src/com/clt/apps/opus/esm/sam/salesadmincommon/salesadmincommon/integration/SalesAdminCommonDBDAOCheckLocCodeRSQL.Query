<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesAdminCommonDBDAOCheckLocCodeRSQL">
			<desc><![CDATA[Location Code 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT LOC_CD 
FROM MDM_LOCATION 
WHERE LOC_CD = @[loc_cd]
AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
