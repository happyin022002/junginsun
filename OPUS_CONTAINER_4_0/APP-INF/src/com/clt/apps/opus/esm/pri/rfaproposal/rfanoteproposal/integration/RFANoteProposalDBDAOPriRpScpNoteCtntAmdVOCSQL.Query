<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFANoteProposalDBDAOPriRpScpNoteCtntAmdVOCSQL">
			<desc><![CDATA[RFANoteProposalDBDAOPriRpScpNoteCtntAmdVOCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_NOTE_CTNT(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    NOTE_TP_CD,
    NOTE_SEQ,
    NOTE_CTNT_SEQ,
    NOTE_CTNT,
    NOTE_CONV_FLG,
    DP_SEQ,
    NOTE_CONV_MAPG_ID,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    N1ST_CMNC_AMDT_SEQ,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
SELECT
    PROP_NO             ,
    AMDT_SEQ+1          ,
    SVC_SCP_CD          ,
    NOTE_TP_CD          ,
    NOTE_SEQ            ,
    NOTE_CTNT_SEQ       ,
    NOTE_CTNT           ,
    NOTE_CONV_FLG       ,
    DP_SEQ              ,
    DECODE(NOTE_CONV_MAPG_ID,'','', SYS_GUID())   ,
    PRC_PROG_STS_CD     ,
    SRC_INFO_CD         ,
    N1ST_CMNC_AMDT_SEQ        ,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    @[cre_usr_id]       ,
    SYSDATE             ,
    @[upd_usr_id]       ,
    SYSDATE	
FROM 
    PRI_RP_SCP_NOTE_CTNT
WHERE
    PROP_NO     = @[prop_no]
AND AMDT_SEQ    = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
