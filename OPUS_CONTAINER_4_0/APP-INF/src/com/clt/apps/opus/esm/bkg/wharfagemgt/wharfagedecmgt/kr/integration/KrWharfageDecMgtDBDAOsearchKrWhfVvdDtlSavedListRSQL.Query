<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOsearchKrWhfVvdDtlSavedListRSQL">
			<desc><![CDATA[searchKrWhfVvdDtlSavedList]]></desc>
			<sql><![CDATA[
SELECT PORT_SEQ,
       UNLD_TP_CD,
       TTL_TON_QTY,
       KR_WHF_DC_CD,
	   KR_WHF_DC_CD AS RATE,	
       KR_WHF_DC_RSN_CD,
       'U' AS UPD_FLG,
		UPD_USR_ID,
		TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI') UPD_DT
  FROM BKG_KR_WHF_VVD_DTL
 WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND PORT_CD    = @[port_cd]
   AND WHF_BND_CD = @[whf_bnd_cd]			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="whf_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
