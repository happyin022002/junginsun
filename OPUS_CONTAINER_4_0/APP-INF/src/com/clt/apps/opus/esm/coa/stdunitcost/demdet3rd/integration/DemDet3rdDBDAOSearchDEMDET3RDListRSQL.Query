<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDet3rdDBDAOSearchDEMDET3RDListRSQL">
			<desc><![CDATA[_DMDT_N3RD_PTY, _COST_SRC_ACCT, _STND_ACCT, COM_INTG_CD_DTL  테이블의 데이터 조회   ]]></desc>
			<sql><![CDATA[
SELECT	CNTR_TPSZ_CD
		, A.STND_COST_CD
		, C.STND_COST_NM
		, DECODE(A.COST_ASS_BSE_CD, 'F', A.UC_AMT, decode(NVL(A.BKG_VOL_QTY,0),0,0,NVL(A.TTL_DMDT_AMT,0)/NVL(A.BKG_VOL_QTY,0))) UC_AMT
		, A.TTL_DMDT_AMT
		, A.BKG_VOL_QTY
		, A.COST_ASS_BSE_CD
FROM 	COA_DMDT_N3RD_PTY A,
		COA_STND_ACCT C
WHERE	A.COST_YRMON  = @[f_cost_yrmon]
AND		A.STND_COST_CD = C.STND_COST_CD
AND 	A.STND_COST_CD != '75000000'
ORDER	BY CNTR_TPSZ_CD, C.STND_COST_NM			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
