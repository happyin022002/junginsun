<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOsearchCntrDgListRSQL">
			<desc><![CDATA[searchCntrDgList]]></desc>
			<sql><![CDATA[
SELECT	BB.BL_NO					AS BL_NO,
    	DC.DCGO_SEQ					AS DCGO_SEQ,
    	DC.IMDG_UN_NO				AS IMDG_UN_NO,
    	NVL(DC.IMDG_CLSS_CD, ' ')	AS IMDG_CLSS_CD,
    	NVL(DC.CNTR_NO, ' ')		AS CNTR_NO,
    	--NVL(DC.IMDG_PG_NO, ' ')		AS IMDG_PG_NO,
    	NVL(DC.IMDG_SUBS_RSK_LBL_CD1, ' ') AS IMDG_SUBS_RSK_LBL_CD,
    	NVL(TRIM(DC.EMER_CNTC_PHN_NO_CTNT),' ') AS CNTC_PSON_TELCM_NO,
    	NVL(TRIM(DC.EMER_CNTC_PSON_NM),' ') AS CNTC_PSON_NM,
		FLSH_PNT_CDO_TEMP,
		EMS_NO,
    	@[trans_mode]				AS CHN_MF_SND_IND_CD,
		@[usr_id]					AS CRE_USR_ID,
		@[usr_id]					AS UPD_USR_ID
FROM	BKG_DG_CGO DC,
        BKG_BOOKING BB
WHERE   1=1
AND ( #foreach($field_id in ${field_list})
      	#if($velocityCount > 1)
      	OR #end      BB.BKG_NO IN ( $field_id )
      #end
)
#if (${bkg_cgo_tp_cd} != '')
AND     DECODE(BB.BKG_CGO_TP_CD,'P','P','F') = @[bkg_cgo_tp_cd]
#end
AND	    BB.BKG_NO = DC.BKG_NO			]]></sql>
			<params>
				<param name="trans_mode" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_cgo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
