<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMonthlyQuotaRlseVersionListRSQL">
			<desc><![CDATA[ReleaseVersion 조회 ]]></desc>
			<sql><![CDATA[
SELECT MQTA_RLSE_VER_NO AS TEXT,  
       MQTA_RLSE_VER_NO AS CODE   
  FROM SAQ_MON_QTA_RLSE           
 WHERE QTA_RLSE_STS_CD = 'R'      
   AND BSE_YR          = @[year]        
   AND BSE_QTR_CD      = @[quarter]        			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="quarter" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
