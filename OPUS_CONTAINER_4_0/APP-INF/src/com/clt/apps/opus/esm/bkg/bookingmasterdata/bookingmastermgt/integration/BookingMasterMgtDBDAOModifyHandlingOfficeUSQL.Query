<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOModifyHandlingOfficeUSQL">
			<desc><![CDATA[E-BKG Handling Office 를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_HNDL_OFC_STUP SET
	POL_CD			=	@[pol_cd],
	HNDL_OFC_CD		=	@[hndl_ofc_cd],
	POR_CD			=	@[por_cd],
	VT_CUST_OFC_CD	=	@[vt_cust_ofc_cd],
	VT_CUST_CNT_CD	=	@[vt_cust_cnt_cd],
	VT_CUST_OFC_NM	=	@[vt_cust_ofc_nm],
	CUST_SEQ		=	@[cust_seq],
    UPD_USR_ID  	= 	@[upd_usr_id],
    UPD_DT      	= 	SYSDATE
 WHERE BKG_HNDL_OFC_SEQ = @[bkg_hndl_ofc_seq]			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="hndl_ofc_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="vt_cust_ofc_cd" type="12" value="" out="N"/>
				<param name="vt_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="vt_cust_ofc_nm" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_hndl_ofc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
