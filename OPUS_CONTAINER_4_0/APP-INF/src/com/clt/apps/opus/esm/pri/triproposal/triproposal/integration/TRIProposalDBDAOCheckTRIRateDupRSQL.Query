<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOCheckTRIRateDupRSQL">
			<desc><![CDATA[TRI Rate 중복된 데이터가 있는지 점검한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(@[srch_action], '906', SUBSTR(A.TRI_NO, 1, 6) || '-' || SUBSTR(A.TRI_NO, 7, 4) || '-' || SUBSTR(A.TRI_NO, 11, 3), A.TRI_PROP_NO) AS TRI_PROP_NO
  FROM PRI_TRI_MN A
      ,(SELECT TRI_PROP_NO
              ,MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_CD || RCV_DE_TERM_CD || PRC_TRSP_MOD_CD, '|')) AS ROUT_PNT_LOC_CD
          FROM (SELECT TRI_PROP_NO
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_CD
                      ,RCV_DE_TERM_CD
                      ,PRC_TRSP_MOD_CD
                      ,ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_PNT_LOC_CD) AS RN
                  FROM PRI_TRI_RT_ROUT_PNT
                 WHERE ORG_DEST_TP_CD = 'O')
         START WITH RN = 1
        CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
               AND PRIOR RN = RN - 1
         GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD) B
      ,(SELECT TRI_PROP_NO, MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_CD, '|')) AS ROUT_VIA_PORT_CD
          FROM (SELECT TRI_PROP_NO
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_CD
                      ,ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_VIA_PORT_CD) AS RN
                  FROM PRI_TRI_RT_ROUT_VIA
                 WHERE ORG_DEST_TP_CD = 'O')
         START WITH RN = 1
        CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
               AND PRIOR RN = RN - 1
         GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD) C
      ,(SELECT TRI_PROP_NO, MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_CD, '|')) AS ROUT_VIA_PORT_CD
          FROM (SELECT TRI_PROP_NO
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_CD
                      ,ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_VIA_PORT_CD) AS RN
                  FROM PRI_TRI_RT_ROUT_VIA
                 WHERE ORG_DEST_TP_CD = 'D')
         START WITH RN = 1
        CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
               AND PRIOR RN = RN - 1
         GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD) D
      ,(SELECT TRI_PROP_NO
              ,MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_CD || RCV_DE_TERM_CD || PRC_TRSP_MOD_CD, '|')) AS ROUT_PNT_LOC_CD
          FROM (SELECT TRI_PROP_NO
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_CD
                      ,RCV_DE_TERM_CD
                      ,PRC_TRSP_MOD_CD
                      ,ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_PNT_LOC_CD) AS RN
                  FROM PRI_TRI_RT_ROUT_PNT
                 WHERE ORG_DEST_TP_CD = 'D')
         START WITH RN = 1
        CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
               AND PRIOR RN = RN - 1
         GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD) E
      ,(SELECT T.TRI_PROP_NO, T.AMDT_SEQ, T.EFF_DT, T.EXP_DT, T.RAT_UT_CD, T.PRC_CGO_TP_CD, T.CURR_CD, T.PROP_STS_CD, T.NOTE_CTNT
          FROM PRI_TRI_RT T) F
 WHERE A.TRI_PROP_NO = B.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = C.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = D.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = E.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = F.TRI_PROP_NO
   AND A.TRF_PFX_CD = @[srch_trf_pfx_cd]
   AND A.TRF_NO = @[srch_trf_no]
   AND (@[srch_tri_prop_no] IS NULL OR A.TRI_PROP_NO <> @[srch_tri_prop_no])
   AND A.CMDT_CD = @[srch_cmdt_cd]
   AND B.ROUT_PNT_LOC_CD = @[srch_org_rout_pnt_loc_nm]
   AND ((@[srch_org_rout_via_port_nm] IS NULL AND C.ROUT_VIA_PORT_CD IS NULL) OR C.ROUT_VIA_PORT_CD = @[srch_org_rout_via_port_nm])
   AND ((@[srch_dest_rout_via_port_nm] IS NULL AND D.ROUT_VIA_PORT_CD IS NULL) OR D.ROUT_VIA_PORT_CD = @[srch_dest_rout_via_port_nm])
   AND E.ROUT_PNT_LOC_CD = @[srch_dest_rout_pnt_loc_nm]
   AND F.RAT_UT_CD = @[srch_rat_ut_cd]
   AND F.PRC_CGO_TP_CD = @[srch_prc_cgo_tp_cd]
   AND (F.EFF_DT BETWEEN TO_DATE(@[srch_eff_dt], 'YYYY-MM-DD') AND TO_DATE(@[srch_exp_dt], 'YYYY-MM-DD') OR
       F.EXP_DT BETWEEN TO_DATE(@[srch_eff_dt], 'YYYY-MM-DD') AND TO_DATE(@[srch_exp_dt], 'YYYY-MM-DD') OR
       (F.EFF_DT <= TO_DATE(@[srch_eff_dt], 'YYYY-MM-DD') AND F.EXP_DT >= TO_DATE(@[srch_exp_dt], 'YYYY-MM-DD')))
   AND ((@[srch_action] = '2' AND F.PROP_STS_CD = 'F') OR (@[srch_action] = '904' AND F.PROP_STS_CD <> 'I') OR (@[srch_action] = '906' AND F.PRC_CGO_TP_CD <> 'AK' AND F.PRC_CGO_TP_CD <> 'BB' AND A.TRI_NO IS NOT NULL))
   AND F.NOTE_CTNT = @[srch_note_ctnt]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="srch_action" type="12" value="" out="N"/>
				<param name="srch_trf_pfx_cd" type="12" value="" out="N"/>
				<param name="srch_trf_no" type="12" value="" out="N"/>
				<param name="srch_tri_prop_no" type="12" value="" out="N"/>
				<param name="srch_cmdt_cd" type="12" value="" out="N"/>
				<param name="srch_org_rout_pnt_loc_nm" type="12" value="" out="N"/>
				<param name="srch_org_rout_via_port_nm" type="12" value="" out="N"/>
				<param name="srch_dest_rout_via_port_nm" type="12" value="" out="N"/>
				<param name="srch_dest_rout_pnt_loc_nm" type="12" value="" out="N"/>
				<param name="srch_rat_ut_cd" type="12" value="" out="N"/>
				<param name="srch_prc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="srch_eff_dt" type="12" value="" out="N"/>
				<param name="srch_exp_dt" type="12" value="" out="N"/>
				<param name="srch_note_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
