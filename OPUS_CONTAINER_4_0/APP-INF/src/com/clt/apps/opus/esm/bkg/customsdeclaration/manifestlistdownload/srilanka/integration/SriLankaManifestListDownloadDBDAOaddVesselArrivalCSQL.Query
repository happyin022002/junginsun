<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SriLankaManifestListDownloadDBDAOaddVesselArrivalCSQL">
			<desc><![CDATA[스리랑카세관 신고용 Vessel Arrival 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_SRI_VVD
	(VSL_RGST_NO, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, PORT_CD, VSL_NM, 
	 ETA_DT, ETD_DT, CAP_NM, VSL_CNT_CD, SVC_PRE_PORT_CD, SHP_AGN_NM, 
	 LOCL_SHP_AGN_NM, CRE_DT, CRE_USR_ID,UPD_USR_ID,UPD_DT)
VALUES
	(@[vsl_rgst_no], @[vsl_cd], @[skd_voy_no],@[skd_dir_cd] ,@[port_cd], NVL(@[vsl_nm],''), 
	 TO_DATE(REPLACE(@[eta_dt],'-','')||REPLACE(@[eta_time],':',''),'YYYYMMDD'||'HH24MISS'), 
	 TO_DATE(REPLACE(@[etd_dt],'-','')||REPLACE(@[etd_time],':',''),'YYYYMMDD'||'HH24MISS'),
	 NVL(@[cap_nm],''), NVL(@[vsl_cnt_cd],''), NVL(@[svc_pre_port_cd],''), NVL(@[shp_agn_nm],''), NVL(@[locl_shp_agn_nm],''), SYSDATE, @[user_id], @[user_id],SYSDATE)			]]></sql>
			<params>
				<param name="vsl_rgst_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="vsl_nm" type="12" value="" out="N"/>
				<param name="eta_dt" type="12" value="" out="N"/>
				<param name="eta_time" type="12" value="" out="N"/>
				<param name="etd_dt" type="12" value="" out="N"/>
				<param name="etd_time" type="12" value="" out="N"/>
				<param name="cap_nm" type="12" value="" out="N"/>
				<param name="vsl_cnt_cd" type="12" value="" out="N"/>
				<param name="svc_pre_port_cd" type="12" value="" out="N"/>
				<param name="shp_agn_nm" type="12" value="" out="N"/>
				<param name="locl_shp_agn_nm" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
