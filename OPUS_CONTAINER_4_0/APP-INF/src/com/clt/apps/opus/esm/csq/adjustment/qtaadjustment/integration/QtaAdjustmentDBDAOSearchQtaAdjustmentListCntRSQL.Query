<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOSearchQtaAdjustmentListCntRSQL">
			<desc><![CDATA[[QTA Adjustment by VVD]의 [Data Count] 합니다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS CNT
  FROM CSQ_CFM_TGT_VVD
 WHERE BSE_TP_CD       = @[f_bse_tp_cd]
   AND BSE_YR          = @[f_bse_yr]
   AND BSE_QTR_CD      = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
   AND QTA_TGT_CD      = 'D'
   AND QTA_RLSE_VER_NO = ( SELECT QTA_RLSE_VER_NO
                             FROM CSQ_QTA_RLSE_VER
                            WHERE BSE_TP_CD      = @[f_bse_tp_cd]
                              AND BSE_YR         = @[f_bse_yr]
                              AND BSE_QTR_CD     = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
                              AND CSQ_VER_STS_CD = 'R' )
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
   AND TRD_CD          = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
   AND RLANE_CD        = @[f_rlane_cd]
#end
#if (${f_dir_cd} != '' && ${f_dir_cd} != 'All')
   AND DIR_CD          = @[f_dir_cd]
#end			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
