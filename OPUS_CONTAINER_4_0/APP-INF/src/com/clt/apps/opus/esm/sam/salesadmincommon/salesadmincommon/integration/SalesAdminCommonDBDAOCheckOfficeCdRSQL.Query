<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesAdminCommonDBDAOCheckOfficeCdRSQL">
			<desc><![CDATA[Office Code 확인용 쿼리]]></desc>
			<sql><![CDATA[
SELECT OFC_CD 
FROM MDM_ORGANIZATION 
WHERE OFC_CD = @[ofc_cd]
--AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
