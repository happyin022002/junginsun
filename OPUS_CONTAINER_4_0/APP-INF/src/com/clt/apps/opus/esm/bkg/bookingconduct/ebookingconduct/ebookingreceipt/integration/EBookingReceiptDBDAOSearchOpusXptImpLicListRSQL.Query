<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchOpusXptImpLicListRSQL">
			<desc><![CDATA[Opus의 export/import licens no를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   BKG_NO
       , IO_BND_CD     
       , XPT_IMP_SEQ
       , CNT_CD
       , XPT_LIC_NO
       , TS_REF_NO
       , PCK_QTY
       , PCK_TP_CD
       , MF_WGT
       , WGT_UT_CD
       , DIVD_FLG
       , DIVD_SEQ
       , DIVD_PCK_QTY
       , DIVD_PCK_TP_CD
       , DIVD_WGT
       , DIVD_WGT_UT_CD
       , SAM_PCK_ID
       , SAM_PCK_QTY
       , SAM_PCK_TP_CD
       , AES_TP_CD
       , AES_INLND_TRNS_PFX_CTNT
       , AES_INLND_TRNS_NO
       , AES_PTA_PFX_CTNT
       , AES_PTA_NO1
       , AES_PTA_NO2
       , TO_CHAR(AES_PTA_DT,'MM-DD-YYYY') AES_PTA_DT
       , AES_PTU_PFX_CTNT
       , AES_PTU_NO
       , TO_CHAR(AES_PTU_DT,'MM-DD-YYYY') AES_PTU_DT
       , AES_DWN_PFX_CTNT
       , AES_DWN_NO
       , TO_CHAR(AES_DWN_DT,'MM-DD-YYYY') AES_DWN_DT
       , AES_EXPT_ID
       , AES_EXPT_CTNT
       , CAED_TP_CD
       , CAED_PFX_CTNT
       , CAED_NO1
			||decode(nvl(CAED_NO2, 'x'), 'x', '', '-'||CAED_NO2)
			||decode(nvl(CAED_NO3, 'x'), 'x', '', '-'||CAED_NO3) CAED_CTNT
       , G7_EDI_PFX_CTNT
       , G7_EDI_NO1
			||decode(nvl(G7_EDI_NO2, 'x'), 'x', '', '-'||G7_EDI_NO2) G7_EDI_CTNT
       , B13A_XPT_PFX_CTNT
       , decode(nvl(B13A_XPT_NO1, 'x'), 'x', '',B13A_XPT_NO1)||decode(nvl(B13A_XPT_NO2, 'x'), 'x', '',B13A_XPT_NO2) B13A_XPT_CTNT
       , MF_SMRY_RPT_PFX_CTNT
       , MF_SMRY_RPT_NO
       , CGO_CTRL_PFX_CTNT
       , CGO_CTRL_NO
       , NDR_REF_PFX_CTNT
       , NDR_REF_ID
       , NDR_REF_CTNT
	   , MX_SHPR_TAX_ID
	   , MX_CNEE_TAX_ID
	   , MX_NTFY_TAX_ID
	   , TR_SHPR_TAX_ID
       , TR_CNEE_TAX_ID
       , TR_NTFY_TAX_ID
	   , IL_SHPR_TAX_ID
       , IL_CNEE_TAX_ID
       , IL_NTFY_TAX_ID
	   , LB_SHPR_TAX_ID
       , LB_CNEE_TAX_ID
       , LB_NTFY_TAX_ID
       , SHPR_TAX_NO AS BR_SHPR_TAX_ID    -- Brazil 추가 
       , CNEE_TAX_NO AS BR_CNEE_TAX_ID    -- Brazil 추가 
       , NTFY_TAX_NO AS BR_NTFY_TAX_ID    -- Brazil 추가 
       , BRZ_DECL_NO    -- Brazil 추가 
       , SHPR_TAX_CPY_DESC_FLG , CNEE_TAX_CPY_DESC_FLG, NTFY_TAX_CPY_DESC_FLG, BRZ_DECL_CPY_DESC_FLG 
  FROM BKG_XPT_IMP_LIC
 WHERE 1 = 1
   AND BKG_NO = @[bkg_no]
   AND IO_BND_CD = 'O'
#if (${cnt_cd} != '')
   AND CNT_CD = @[cnt_cd]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
