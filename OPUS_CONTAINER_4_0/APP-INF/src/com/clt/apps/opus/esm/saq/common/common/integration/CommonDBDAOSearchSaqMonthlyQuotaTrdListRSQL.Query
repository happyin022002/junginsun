<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMonthlyQuotaTrdListRSQL">
			<desc><![CDATA[Trade 조회 ]]></desc>
			<sql><![CDATA[
SELECT DISTINCT TRD.TRD_CD AS TEXT,                                                   
               TRD.TRD_CD AS CODE                                                    
 FROM SAQ_TGT_GRP     TGT,                                                           
      SAQ_TGT_GRP_TRD TRD                                                            
WHERE TGT.SAQ_TGT_GRP_CD = TRD.SAQ_TGT_GRP_CD                                        
#if (${userChekc} != 'ALL' ) 
    AND TGT.OFC_CD = @[ofcCd]
#end
ORDER BY TRD.TRD_CD  			]]></sql>
			<params>
				<param name="ofcCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
