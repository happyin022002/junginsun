<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostAssignDBDAOMultiCoaCopIfMgmtCSQL">
			<desc><![CDATA[일배치 테이블에 정보 등록]]></desc>
			<sql><![CDATA[
MERGE INTO COA_COP_IF_MGMT A1
USING (
       SELECT @[bkg_no]         AS BKG_NO
             ,'n'               AS COA_DY_BAT_CD
             ,0                 AS COA_DY_BAT_SEQ
             ,SYSDATE           AS COA_DY_BAT_DT
             ,@[cost_if_dy_rmk] AS COST_IF_DY_RMK
             ,'M_'||@[cre_usr_id]     AS CRE_USR_ID
             ,'M_'||@[upd_usr_id]     AS UPD_USR_ID
         FROM DUAL
      ) A2
ON (A1.BKG_NO = A2.BKG_NO)
WHEN MATCHED THEN
	UPDATE 
       SET A1.COST_IF_DY_RMK = A2.COST_IF_DY_RMK
          ,A1.COA_DY_BAT_DT  = A2.COA_DY_BAT_DT
          ,A1.UPD_USR_ID     = A2.UPD_USR_ID
          ,A1.UPD_DT         = SYSDATE
WHEN NOT MATCHED THEN
	INSERT(A1.BKG_NO
          ,A1.COA_DY_BAT_CD
          ,A1.COA_DY_BAT_SEQ
          ,A1.COA_DY_BAT_DT
          ,A1.COST_IF_DY_RMK
          ,A1.CRE_USR_ID
          ,A1.CRE_DT
          ,A1.UPD_USR_ID
          ,A1.UPD_DT) 
    VALUES(A2.BKG_NO
          ,A2.COA_DY_BAT_CD
          ,A2.COA_DY_BAT_SEQ
          ,A2.COA_DY_BAT_DT
          ,A2.COST_IF_DY_RMK
          ,A2.CRE_USR_ID
          ,SYSDATE
          ,A2.UPD_USR_ID
          ,SYSDATE)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cost_if_dy_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
