<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCdlLocDescRSQL">
			<desc><![CDATA[searchCdlLocDesc]]></desc>
			<sql><![CDATA[
SELECT	 LOC_NM
FROM	MDM_LOCATION
WHERE	LOC_CD = NVL(@[in_pod_cd],@[in_pol_cd])			]]></sql>
			<params>
				<param name="in_pod_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
