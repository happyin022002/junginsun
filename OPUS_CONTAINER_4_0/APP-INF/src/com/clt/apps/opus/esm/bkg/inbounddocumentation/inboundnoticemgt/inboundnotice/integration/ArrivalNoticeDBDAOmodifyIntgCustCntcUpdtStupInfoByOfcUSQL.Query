<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOmodifyIntgCustCntcUpdtStupInfoByOfcUSQL">
			<desc><![CDATA[Integrated Customer Data Update Setup 저장]]></desc>
			<sql><![CDATA[
UPDATE BKG_IB_CUST_CNTC_STUP
   SET AUTO_UPD_FLG = @[auto_upd_flg],
       UPD_LOCL_DT  = GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,GLOBALDATE_PKG.GET_LOCCD_FNC(@[ofc_cd])),
       UPD_LOCL_GDT = GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,'GMT'),
       UPD_USR_ID   = @[upd_usr_id],
       UPD_DT       = SYSDATE
 WHERE OFC_CD       = @[ofc_cd]			]]></sql>
			<params>
				<param name="auto_upd_flg" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
