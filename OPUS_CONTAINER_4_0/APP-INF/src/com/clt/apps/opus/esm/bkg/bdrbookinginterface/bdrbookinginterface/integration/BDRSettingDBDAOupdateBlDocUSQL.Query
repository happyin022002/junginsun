<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRSettingDBDAOupdateBlDocUSQL">
			<desc><![CDATA[updateBlDoc]]></desc>
			<sql><![CDATA[
UPDATE  BKG_BL_DOC
SET     BDR_FLG = 'Y',
        BDR_DT = GLOBALDATE_PKG.TIME_CONV_FNC(COM_CONSTANTMGR_PKG.COM_GETBASELOCATIONCODE_FNC(),SYSDATE-1,POL_CD),
		UPD_USR_ID = 'BDRBookingSetting',
		UPD_DT = SYSDATE
WHERE   BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
