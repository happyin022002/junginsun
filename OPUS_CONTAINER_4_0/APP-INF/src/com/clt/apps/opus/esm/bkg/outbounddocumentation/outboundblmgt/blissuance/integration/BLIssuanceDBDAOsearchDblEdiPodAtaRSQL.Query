<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiPodAtaRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT 'POD_ATA:' || NVL(TO_CHAR(ACT_ARR_DT, 'YYYYMMDDHHMMSS'), ' ') || CHR(10)
FROM   VSK_ACT_PORT_SKD A, (SELECT A.VSL_CD
,                                  A.SKD_VOY_NO
,                                  A.SKD_DIR_CD
,                                  A.POD_CD
                            FROM   BKG_VVD A, BKG_BOOKING B
                            WHERE  A.BKG_NO = @[bkg_no]
                               AND A.BKG_NO = B.BKG_NO
                               AND A.POD_CD = B.POD_CD) B
WHERE  A.VSL_CD = B.VSL_CD
   AND A.SKD_VOY_NO = B.SKD_VOY_NO
   AND A.SKD_DIR_CD = B.SKD_DIR_CD
   AND A.VPS_PORT_CD = B.POD_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
