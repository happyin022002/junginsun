<sqls>
	<querys>
		<query name="CommonDBDAOSearchMonthlyQtaStatusInquiryStsListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT					
    CODE, 				
    TEXT				
FROM					
    (					
    SELECT '0' AS CODE,		
	   ' ' AS TEXT		
    FROM DUAL       			
    UNION ALL				
    SELECT 				
	TO_CHAR(INTG_CD_VAL_DP_SEQ) AS  CODE,	
	INTG_CD_VAL_DP_DESC AS TEXT			
    					
    FROM COM_INTG_CD_DTL		
    					
    WHERE 				
	INTG_CD_ID = 'CD01213'			
    )					
    					
ORDER BY				
    code 				]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
