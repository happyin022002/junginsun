<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTSecurityDBDAOAgtFincOfcInfoVORSQL">
			<desc><![CDATA[Security & AR Office Inquiry   ]]></desc>
			<sql><![CDATA[
SELECT
           B.AGN_CD,
           B.AR_OFC_CD,
           B.CURR_CD,
           NVL(B.OFC_DELT_RSN, ' ') AS OFC_DELT_RSN,
           NVL(B.DELT_FLG, 'N')     AS DELT_FLG
      FROM MDM_ORGANIZATION A,
           AGT_FINC_OFC_INFO B
     WHERE A.AR_HD_QTR_OFC_CD = NVL(@[hqofccd], A.AR_HD_QTR_OFC_CD)
       AND A.OFC_CD = B.AR_OFC_CD
  ORDER BY 1			]]></sql>
			<params>
				<param name="hqofccd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
