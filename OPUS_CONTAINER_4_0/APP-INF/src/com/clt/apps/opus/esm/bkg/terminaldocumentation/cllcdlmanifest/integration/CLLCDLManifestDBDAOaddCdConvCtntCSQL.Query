<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOaddCdConvCtntCSQL">
			<desc><![CDATA[addCdConvCtnt]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_CD_CONV_CTNT (
	CNT_CD
,	CSTMS_DIV_ID
,	CSTMS_DIV_ID_SEQ
,	ATTR_CTNT1
,	ATTR_CTNT2
,	ATTR_CTNT3
,	ATTR_CTNT4
,	ATTR_CTNT5
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[cnt_cd]
,	@[cstms_div_id]
,	(SELECT NVL(MAX(CSTMS_DIV_ID_SEQ), 0) + 1 FROM BKG_CSTMS_CD_CONV_CTNT WHERE CNT_CD = @[cnt_cd] AND CSTMS_DIV_ID = @[cstms_div_id])
,	@[attr_ctnt1]
,	@[attr_ctnt2]
,	@[attr_ctnt3]
,	@[attr_ctnt4]
,	@[attr_ctnt5]
,	NVL(@[delt_flg],'N')
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_div_id" type="12" value="" out="N"/>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="attr_ctnt3" type="12" value="" out="N"/>
				<param name="attr_ctnt4" type="12" value="" out="N"/>
				<param name="attr_ctnt5" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
