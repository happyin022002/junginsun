<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltTradeCodeVORSQL">
			<desc><![CDATA[Trade Code를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT TRD_CD AS CD , TRD_NM AS NM 
FROM MDM_TRADE 
WHERE VSL_TP_CD = @[etc1]
AND DELT_FLG = 'N'
ORDER BY TRD_CD			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
