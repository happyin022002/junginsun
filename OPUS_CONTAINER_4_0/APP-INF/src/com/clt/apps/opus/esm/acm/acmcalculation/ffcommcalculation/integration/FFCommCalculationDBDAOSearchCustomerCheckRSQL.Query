<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCommCalculationDBDAOSearchCustomerCheckRSQL">
			<desc><![CDATA[FFCommCalculationDBDAOSearchCustomerCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT DECODE(A.CUST_NM, B.CUST_NM,1,0) CUSTOMER_CHK
  FROM BKG_CUSTOMER A, BKG_CUSTOMER B
 WHERE A.BKG_NO = @[bkg_no]
	  AND A.BKG_CUST_TP_CD = 'S'
	  AND A.BKG_NO = B.BKG_NO(+)
	  AND B.BKG_CUST_TP_CD = 'F'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
