<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqCfmTgtRhqListRSQL">
			<desc><![CDATA[SAQ_MON_CFM_TGT_VVD의 Rlane List 조회 ]]></desc>
			<sql><![CDATA[
#if (${qta_tgt_cd} == 'T' )
   SELECT SLS_RHQ_CD AS TEXT,                            
          SLS_RHQ_CD AS CODE                             
     FROM SAQ_MON_LOD_TGT_OFC LOD ,                      
          SAQ_MON_DIR_CONV    CONV                       
    WHERE 1=1                                            
      AND LOD.BSE_YR          = @[bse_year]                        
      AND LOD.BSE_QTR_CD      = @[bse_qtr_cd]                        
      AND LOD.TRD_CD          = @[trd_cd]                        
      AND LOD.DIR_CD          = NVL(CONV.CONV_DIR_CD, @[dir_cd]) 
      AND CONV.BSE_YR     (+) = LOD.BSE_YR               
      AND CONV.BSE_QTR_CD (+) = LOD.BSE_QTR_CD           
      AND CONV.TRD_CD     (+) = LOD.TRD_CD               
      AND CONV.DIR_CD     (+) = @[dir_cd]                        
      AND CONV.RLANE_CD   (+) = @[rlane_cd]                        
 ORDER BY SLS_RHQ_CD                                     
#else
   SELECT OFC_CD AS TEXT, OFC_CD AS CODE          
     FROM SAQ_ORGANIZATION_V                      
    WHERE LVL      = '2'                          
      AND DELT_FLG = 'N'                          
 ORDER BY OFC_CD                                  
#end			]]></sql>
			<params>
				<param name="bse_year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
