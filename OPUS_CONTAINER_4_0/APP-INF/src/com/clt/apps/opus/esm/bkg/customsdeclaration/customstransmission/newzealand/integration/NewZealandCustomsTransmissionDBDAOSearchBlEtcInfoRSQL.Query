<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandCustomsTransmissionDBDAOSearchBlEtcInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 'F' AS FP_IND,
       (SELECT COUNT(*)
          FROM BKG_CONTAINER CNTR
         WHERE CNTR.BKG_NO = DOC.BKG_NO) AS TOT_CNTR,
       DOC.PCK_QTY AS TOT_PKG

  FROM BKG_BL_DOC DOC

 WHERE DOC.BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
