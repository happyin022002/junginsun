<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOBkgCMCroChkListByHBLVORSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
WITH  BKG_VVD_TMP  AS (
     SELECT A.BKG_NO
         ,A.POL_CD
         ,A.POD_CD
         ,B.CNTR_NO
         ,C.CNTR_MF_NO
         ,C.HAMO_TRF_CD
         ,C.CMDT_HS_CD
         ,C.NCM_NO
         ,DECODE(C.CNTR_MF_MK_DESC, NULL, 'N', 'Y') CM_MK
         ,DECODE(C.CNTR_MF_GDS_DESC, NULL, 'N', 'Y') CM_DS
         ,DECODE(C.CNTR_MF_NO, NULL, 'N', 'Y') CM_AMS
         ,C.PCK_QTY CM_PCK
         ,C.CNTR_MF_WGT CM_WGT
         ,C.MEAS_QTY CM_MEAS
   FROM   (SELECT BV.BKG_NO
                 ,BV.POL_CD
                 ,BV.POD_CD
				 ,(	SELECT BK.USA_CSTMS_FILE_CD || ',' || BK.CND_CSTMS_FILE_CD
             		FROM   BKG_BOOKING BK 
             		WHERE  BK.BKG_NO = BV.BKG_NO ) CSTMS_FILE_CD
           FROM   BKG_VVD BV
           WHERE  0 = 0 
#if (${vsl_cd} != '') 
      		AND     BV.VSL_CD = @[vsl_cd]
      		AND     BV.SKD_VOY_NO = @[skd_voy_no]
      		AND     BV.SKD_DIR_CD = @[skd_dir_cd]
#end
#if (${pol_cd} != '') 
      		AND     BV.POL_CD LIKE @[pol_cd] || '%'
#end
#if (${pod_cd} != '') 
      		AND     BV.POD_CD LIKE @[pod_cd] || '%'
#end
			) A
         ,BKG_CONTAINER B
         ,BKG_CNTR_MF_DESC C
   WHERE  A.BKG_NO = B.BKG_NO(+) 
   AND    B.BKG_NO = C.BKG_NO(+) 
   AND    B.CNTR_NO = C.CNTR_NO(+)

     )
SELECT  *
FROM   (SELECT BKG.BKG_NO
              ,BKG.BL_NO                                           AS B_BL_NO
              ,VVD.POL_CD                                          AS B_POL_CD
              ,VVD.POD_CD                                          AS B_POD_CD
              ,DECODE(BKG.BL_TP_CD,'W','W','')                     AS BL_TP_CD
              ,BKG.BKG_CGO_TP_CD                                   AS BKG_CGO_TP_CD
              ,HBL.HBL_NO                                          AS B_HBL_NO
              ,HBL.CNTR_MF_NO                                      AS B_USA_CSTMS_FILE_NO 

---------------------	SHIPPER
              ,CASE WHEN NVL(H_S.CUST_NM,'N') <> 'N'  THEN  'Y' 
                    WHEN NVL(RUL_LOC.SHPR_NM_FLG, RUL_ALL.SHPR_NM_FLG) ='Y' THEN 'E' 
                    ELSE NVL(RUL_LOC.SHPR_NM_FLG, RUL_ALL.SHPR_NM_FLG) END  AS B_CUST_NM_S
                      
              ,CASE WHEN NVL(H_S.CUST_ADDR,'N') <> 'N'  THEN  'Y' 
                    WHEN NVL(RUL_LOC.SHPR_ADDR_FLG, RUL_ALL.SHPR_ADDR_FLG) ='Y' THEN 'E' 
                    ELSE NVL(RUL_LOC.SHPR_ADDR_FLG, RUL_ALL.SHPR_ADDR_FLG) END  AS B_CUST_ADDR_S

			  ,CASE WHEN NVL(H_S.CTY_NM,'N') <> 'N'  THEN  'Y'
					WHEN NVL(RUL_LOC.SHPR_CTY_FLG, RUL_ALL.SHPR_CTY_FLG) ='Y' THEN 'E'
					ELSE NVL(RUL_LOC.SHPR_CTY_FLG, RUL_ALL.SHPR_CTY_FLG) END  AS B_CUST_CT_S

			  ,CASE WHEN NVL(H_S.CUST_CNT_CD,'N') <> 'N'  THEN  'Y'
					WHEN NVL(RUL_LOC.SHPR_CNT_FLG, RUL_ALL.SHPR_CNT_FLG) ='Y' THEN 'E'
					ELSE NVL(RUL_LOC.SHPR_CNT_FLG, RUL_ALL.SHPR_CNT_FLG) END  AS B_CUST_CN_S

---------------------  CONSIGNEE	
			  ,CASE WHEN NVL(H_C.CUST_NM,'N') <> 'N'  THEN  'Y'
					WHEN NVL(RUL_LOC.CNEE_NM_FLG, RUL_ALL.CNEE_NM_FLG) ='Y' THEN 'E'
					ELSE NVL(RUL_LOC.CNEE_NM_FLG, RUL_ALL.CNEE_NM_FLG) END  AS B_CUST_NM_C

			  ,CASE WHEN NVL(H_C.CUST_ADDR,'N') <> 'N'  THEN  'Y'
					WHEN NVL(RUL_LOC.CNEE_ADDR_FLG, RUL_ALL.CNEE_ADDR_FLG) ='Y' THEN 'E'
					ELSE NVL(RUL_LOC.CNEE_ADDR_FLG, RUL_ALL.CNEE_ADDR_FLG) END  AS B_CUST_ADDR_C

			  ,CASE WHEN NVL(H_C.CTY_NM,'N') <> 'N'  THEN  'Y'
					WHEN NVL(RUL_LOC.CNEE_CTY_FLG, RUL_ALL.CNEE_CTY_FLG) ='Y' THEN 'E'
					ELSE NVL(RUL_LOC.CNEE_CTY_FLG, RUL_ALL.CNEE_CTY_FLG) END  AS B_CUST_CT_C

			  ,CASE WHEN NVL(H_C.STE_CD,'N') <> 'N'  THEN  'Y'
					WHEN NVL(RUL_LOC.CNEE_STE_FLG, RUL_ALL.CNEE_STE_FLG) ='Y' THEN 'E'
					ELSE NVL(RUL_LOC.CNEE_STE_FLG, RUL_ALL.CNEE_STE_FLG) END  AS B_CUST_ST_C

			  ,CASE WHEN NVL(H_C.CUST_CNT_CD,'N') <> 'N'  THEN  'Y'
					WHEN NVL(RUL_LOC.CNEE_CNT_FLG, RUL_ALL.CNEE_CNT_FLG) ='Y' THEN 'E'
					ELSE NVL(RUL_LOC.CNEE_CNT_FLG, RUL_ALL.CNEE_CNT_FLG) END  AS B_CUST_CN_C

---------------------    NOTIFY     
              ,CASE WHEN NVL(H_N.CUST_NM,'N') <> 'N'  THEN  'Y' 
                    WHEN NVL(RUL_LOC.NTFY_NM_FLG, RUL_ALL.NTFY_NM_FLG) ='Y' THEN 'E' 
                    ELSE NVL(RUL_LOC.NTFY_NM_FLG, RUL_ALL.NTFY_NM_FLG) END  AS B_CUST_NM_N
                            
              ,CASE WHEN NVL(H_N.CUST_ADDR,'N') <> 'N'  THEN  'Y' 
                    WHEN NVL(RUL_LOC.NTFY_ADDR_FLG, RUL_ALL.NTFY_ADDR_FLG) ='Y' THEN 'E' 
                    ELSE NVL(RUL_LOC.NTFY_ADDR_FLG, RUL_ALL.NTFY_ADDR_FLG) END  AS B_CUST_ADDR_N

              ,REPLACE(C_C.CUST_NM,CHR(10),' ')         AS CUST_NM
              ,REPLACE(C_S.CUST_NM,CHR(10),' ')         AS S_CUST_NM
              ,REPLACE(C_N.CUST_NM,CHR(10),' ')         AS N_CUST_NM

---------------------- House B/L Data				  
              ,NVL(HBL.PCK_QTY,0)                                  AS B_PCK_QTY_DA
              ,NVL(HBL.HBL_WGT,0)                                  AS B_HBL_WGT_DA
              ,NVL(HBL.CMDT_MEAS_QTY,0)                            AS B_MEAS_QTY_DA
              ,CASE WHEN HBL.PCK_QTY = CM_HBL.CM_PCK_HBL  THEN  'Y'
                    WHEN NVL(RUL_LOC.PCK_FLG, RUL_ALL.PCK_FLG) ='Y' THEN 'E' 
                    ELSE NVL(RUL_LOC.PCK_FLG, RUL_ALL.PCK_FLG) END  AS B_PCK_QTY_CHK
                
              ,CASE WHEN HBL.HBL_WGT  = CM_HBL.CM_WGT_HBL  THEN  'Y'
                    WHEN NVL(RUL_LOC.WGT_FLG, RUL_ALL.WGT_FLG) ='Y' THEN 'E' 
                    ELSE NVL(RUL_LOC.WGT_FLG, RUL_ALL.WGT_FLG) END  AS B_HBL_WGT_CHK
                            
              ,CASE WHEN HBL.CMDT_MEAS_QTY = CM_HBL.CM_MEAS_HBL  THEN  'Y'
                    WHEN NVL(RUL_LOC.MEAS_FLG, RUL_ALL.MEAS_FLG) ='Y' THEN 'E' 
                    ELSE NVL(RUL_LOC.MEAS_FLG, RUL_ALL.MEAS_FLG) END  AS B_MEAS_QTY_CHK

------------------------C/M(Container Manifest)
			  ,NVL(VVD.CM_PCK,0)                                AS B_PCK_QTY_CM
      		  ,NVL(VVD.CM_WGT,0)                                AS B_CNTR_WGT_CM
      		  ,NVL(VVD.CM_MEAS,0)                               AS B_MEAS_QTY_CM

          	  ,CASE WHEN NVL(VVD.CM_DS,'N') <> 'N'  THEN  'Y'
                	WHEN NVL(RUL_LOC.CNTR_MF_DESC_FLG, RUL_ALL.CNTR_MF_DESC_FLG) ='Y' THEN 'E'
                	ELSE NVL(RUL_LOC.CNTR_MF_DESC_FLG, RUL_ALL.CNTR_MF_DESC_FLG) END  AS B_CNTR_MF_GDS_DESC

			  ,CASE WHEN NVL(VVD.CM_MK,'N') <> 'N'  THEN  'Y'
                	WHEN NVL(RUL_LOC.CNTR_MF_MK_FLG, RUL_ALL.CNTR_MF_MK_FLG) ='Y' THEN 'E'
                	ELSE NVL(RUL_LOC.CNTR_MF_MK_FLG, RUL_ALL.CNTR_MF_MK_FLG) END  AS B_CNTR_MF_MK_DESC

			  ,CASE WHEN NVL(VVD.HAMO_TRF_CD,'N') <> 'N'  THEN  'Y'
                	WHEN NVL(RUL_LOC.CNTR_MF_CMDT_FLG, RUL_ALL.CNTR_MF_CMDT_FLG) ='Y' THEN 'E'
                	ELSE NVL(RUL_LOC.CNTR_MF_CMDT_FLG, RUL_ALL.CNTR_MF_CMDT_FLG) END  AS B_CNTR_MF_HTS
 
           	  ,CASE WHEN NVL(VVD.NCM_NO,'N') <> 'N'  THEN  'Y'
                	WHEN NVL(RUL_LOC.CNTR_MF_NCM_FLG, RUL_ALL.CNTR_MF_NCM_FLG) ='Y' THEN 'E'
                	ELSE NVL(RUL_LOC.CNTR_MF_NCM_FLG, RUL_ALL.CNTR_MF_NCM_FLG) END  AS B_CNTR_MF_NCM
          
           	  ,CASE WHEN NVL(VVD.CM_AMS,'N') <> 'N'  THEN  'Y'
                	WHEN NVL(RUL_LOC.XPT_IMP_REF_FLG1, RUL_ALL.XPT_IMP_REF_FLG1) ='Y' THEN 'E'
                	ELSE NVL(RUL_LOC.XPT_IMP_REF_FLG1, RUL_ALL.XPT_IMP_REF_FLG1) END  AS B_CNTR_MF_NO

------------------------- Container 
              ,CNT.CNTR_NO                                         AS B_CNTR_NO
              ,NVL(CNT.PCK_QTY,0)                                  AS B_PCK_QTY_CO
              ,NVL(CNT.CNTR_WGT,0)                                 AS B_CNTR_WGT_CO
              ,NVL(CNT.MEAS_QTY,0)                                 AS B_MEAS_QTY_CO
              ,CASE WHEN NVL(BKG_JOIN_FNC(CURSOR( SELECT /*+ INDEX(BKG_CNTR_SEAL_NO XAK1BKG_CNTR_SEAL_NO) */  --> 힌트 추가
                                       CNTR_SEAL_NO
                                FROM   BKG_CNTR_SEAL_NO
                                WHERE  BKG_NO = CNT.BKG_NO AND CNTR_NO = CNT.CNTR_NO)),'N') <> 'N' THEN 'Y'
      				WHEN  NVL(RUL_LOC.SEAL_NO_FLG, RUL_ALL.SEAL_NO_FLG) ='Y' THEN 'E'
      				ELSE  'N' END  AS B_CNTR_SEAL_SEQ

              ,'0' Seq

#if (${tab_item} != '') 
	          ,@[tab_item] TAB_ITEM
#end
        FROM   BKG_VVD_TMP      VVD
              ,BKG_BOOKING      BKG
              ,BKG_CUSTOMER     C_S
              ,BKG_CUSTOMER     C_C
              ,BKG_CUSTOMER     C_N
              ,BKG_HBL_CUST     H_S
              ,BKG_HBL_CUST     H_C
              ,BKG_HBL_CUST     H_N
              ,BKG_HBL          HBL
              ,BKG_BL_DOC       DOC
              ,BKG_CONTAINER    CNT
            
			  ,BKG_CSTMS_RULE_STUP RUL_LOC
			  ,BKG_CSTMS_RULE_STUP RUL_ALL

              ,(SELECT CM.BKG_NO
					  ,CM.CNTR_MF_NO
                      ,SUM(CM.PCK_QTY) AS CM_PCK_HBL
                      ,SUM(CM.CNTR_MF_WGT) AS CM_WGT_HBL
                      ,SUM(CM.MEAS_QTY) AS CM_MEAS_HBL
                FROM BKG_CNTR_MF_DESC CM
                WHERE EXISTS (SELECT *
                              FROM BKG_VVD_TMP VVD_TMP
                              WHERE CM.BKG_NO = VVD_TMP.BKG_NO)
                GROUP BY CM.BKG_NO
					  ,CM.CNTR_MF_NO

             ) CM_HBL

        WHERE BKG.BKG_NO(+) = VVD.BKG_NO
        AND BKG.BKG_NO = C_S.BKG_NO(+)
        AND BKG.BKG_NO = C_C.BKG_NO(+)
        AND BKG.BKG_NO = C_N.BKG_NO(+)
        AND BKG.BKG_NO = DOC.BKG_NO(+)

		AND VVD.BKG_NO = CNT.BKG_NO(+)
		AND VVD.CNTR_NO = CNT.CNTR_NO(+)
		AND VVD.BKG_NO = HBL.BKG_NO(+)  
		AND  VVD.CNTR_MF_NO = HBL.CNTR_MF_NO(+)


                     AND HBL.BKG_NO = H_S.BKG_NO(+)
                     AND HBL.HBL_SEQ = H_S.HBL_SEQ(+)
                     AND HBL.BKG_NO = H_C.BKG_NO(+)
                     AND HBL.HBL_SEQ = H_C.HBL_SEQ(+)
                     AND HBL.BKG_NO = H_N.BKG_NO(+)
                     AND HBL.HBL_SEQ = H_N.HBL_SEQ(+)

        AND C_S.BKG_CUST_TP_CD(+) = 'S'
        AND C_C.BKG_CUST_TP_CD(+) = 'C'
        AND C_N.BKG_CUST_TP_CD(+) = 'N'
        AND H_S.BKG_CUST_TP_CD(+) = 'S'
        AND H_C.BKG_CUST_TP_CD(+) = 'C'
        AND H_N.BKG_CUST_TP_CD(+) = 'N'
        AND BKG.BKG_STS_CD <> 'X'

		AND VVD.BKG_NO = CM_HBL.BKG_NO(+)
		AND VVD.CNTR_MF_NO = CM_HBL.CNTR_MF_NO(+)

#if (${pol_cd} != '') 
        AND SUBSTR(VVD.POL_CD,1,2) = RUL_LOC.CNT_CD (+)
        AND VVD.POL_CD = RUL_LOC.LOC_CD (+)
        AND 'E' = RUL_LOC.XPT_IMP_CD (+)

        AND SUBSTR(VVD.POL_CD,1,2) = RUL_ALL.CNT_CD (+)
        AND 'ALL' = RUL_ALL.LOC_CD (+)
        AND 'E' = RUL_ALL.XPT_IMP_CD (+)
#else
        AND SUBSTR(VVD.POD_CD,1,2) = RUL_LOC.CNT_CD (+)
        AND VVD.POD_CD = RUL_LOC.LOC_CD (+)
        AND 'I' = RUL_LOC.XPT_IMP_CD (+)

        AND SUBSTR(VVD.POD_CD,1,2) = RUL_ALL.CNT_CD (+)
        AND 'ALL' = RUL_ALL.LOC_CD (+)
        AND 'I' = RUL_ALL.XPT_IMP_CD (+)

#end
        AND 'CTM' = RUL_LOC.CSTMS_DIV_ID (+)
        AND 'Y' = RUL_LOC.FROB_FLG (+)
        AND 'H' = RUL_LOC.BL_TP_CD (+)
        AND 'N' = RUL_LOC.DELT_FLG (+)

        AND 'CTM' = RUL_ALL.CSTMS_DIV_ID (+)
        AND 'Y' = RUL_ALL.FROB_FLG (+)
        AND 'H' = RUL_ALL.BL_TP_CD (+)
        AND 'N' = RUL_ALL.DELT_FLG (+)



#if (${pol_yd_cd} != '') 
        AND BKG.POL_NOD_CD = @[pol_yd_cd]
#end
#if (${pod_yd_cd} != '') 
        AND BKG.POD_NOD_CD = @[pod_yd_cd]
#end
#if (${cust_cnt_cd} != '') 
        AND ( C_S.CUST_CNT_CD = @[cust_cnt_cd] AND C_S.CUST_SEQ = @[cust_seq] OR
              C_C.CUST_CNT_CD = @[cust_cnt_cd] AND C_C.CUST_SEQ = @[cust_seq] OR
              C_N.CUST_CNT_CD = @[cust_cnt_cd] AND C_N.CUST_SEQ = @[cust_seq] )
#end
#if (${usa_cstms_file_cd} != '' && ${cnd_cstms_file_cd} != '') 
		 AND (BKG.USA_CSTMS_FILE_CD = @[usa_cstms_file_cd]
			OR BKG.CND_CSTMS_FILE_CD = @[cnd_cstms_file_cd])
#else
	#if (${usa_cstms_file_cd} != '') 
        AND BKG.USA_CSTMS_FILE_CD = @[usa_cstms_file_cd]
	#end
	#if (${cnd_cstms_file_cd} != '') 
        AND BKG.CND_CSTMS_FILE_CD = @[cnd_cstms_file_cd]
	#end
#end

#if (${bkg_ofc_cd} != '') 
        AND BKG.BKG_OFC_CD = @[bkg_ofc_cd]
#end
#if (${cre_usr_id} != '') 
        AND BKG.CRE_USR_ID = @[cre_usr_id]
#end
#if (${ob_srep_cd} != '')
        AND BKG.OB_SREP_CD = @[ob_srep_cd]
#end
		
        ORDER BY BKG.BL_NO, BKG.BKG_NO, B_USA_CSTMS_FILE_NO,C_C.CUST_NM
       )
#if (${chk_err} != '' && ${chk_err} == '0')
  WHERE B_CUST_NM_S = 'E'
    OR B_CUST_ADDR_S = 'E'
	OR B_CUST_CT_S = 'E'
	OR B_CUST_CN_S = 'E'

    OR B_CUST_NM_C = 'E'
    OR B_CUST_ADDR_C = 'E'
	OR B_CUST_CT_C = 'E'
	OR B_CUST_ST_C = 'E'
	OR B_CUST_CN_C = 'E'

    OR B_CUST_NM_N = 'E'
    OR B_CUST_ADDR_N = 'E'

    OR B_PCK_QTY_CHK = 'E'
    OR B_HBL_WGT_CHK = 'E'
    OR B_MEAS_QTY_CHK = 'E'

	OR B_CNTR_MF_GDS_DESC  = 'E'
	OR B_CNTR_MF_MK_DESC = 'E'
    OR B_CNTR_MF_HTS = 'E'
	OR B_CNTR_MF_NCM = 'E'
	OR B_CNTR_MF_NO = 'E'

    OR B_CNTR_SEAL_SEQ = 'E'
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="tab_item" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="usa_cstms_file_cd" type="12" value="" out="N"/>
				<param name="cnd_cstms_file_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="ob_srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
