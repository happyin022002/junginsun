<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsCustomsTransmissionDBDAOsearchAwkCgoInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
    (OVR_FWRD_LEN + OVR_BKWD_LEN) AS AK_LGT
    ,OVR_HGT  AK_HGT
    ,(OVR_LF_LEN + OVR_RT_LEN) AS AK_WDT
FROM BKG_AWK_CGO
WHERE BKG_NO = @[bkg_no]
    AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
