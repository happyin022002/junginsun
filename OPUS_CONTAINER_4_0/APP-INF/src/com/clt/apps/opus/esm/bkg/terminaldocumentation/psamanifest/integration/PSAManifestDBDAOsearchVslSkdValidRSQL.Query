<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchVslSkdValidRSQL">
			<desc><![CDATA[Vessel SKD에 등록되어있는지 여부 확인 ( 미등록시 Return 시킴 msg : "Not Exists Vessel SKD "띄움)]]></desc>
			<sql><![CDATA[
SELECT 'X'
FROM   VSK_VSL_SKD
WHERE  VSL_CD     = SUBSTR(@[vvd],1,4)
AND    SKD_VOY_NO = SUBSTR(@[vvd],5,4)
AND    SKD_DIR_CD = SUBSTR(@[vvd],9,1)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
