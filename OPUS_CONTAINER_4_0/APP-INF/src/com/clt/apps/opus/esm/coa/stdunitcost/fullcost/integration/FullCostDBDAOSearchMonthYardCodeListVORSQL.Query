<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FullCostDBDAOSearchMonthYardCodeListVORSQL">
			<desc><![CDATA[PRD_PORT_TML_MTX, MDM_YARD 테이블의 데이터 조회 
]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
     YD_CD  AS NOD_CD
    ,YD_NM
    ,'' AS SLAN_CD
    ,'' AS SKD_DIR_CD
    ,'' AS SEL_CHECK
  FROM MDM_YARD
 WHERE YD_CD LIKE @[f_loc_cd]||'%'
   AND DELT_FLG = 'N'
 ORDER BY YD_CD
			]]></sql>
			<params>
				<param name="f_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
