<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteConversionProposalDBDAOCpPrevNoteCasCNoteCSQL">
			<desc><![CDATA[Amend Cancel시 이전 Seq Conversion을 Copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SC_NOTE_CONV
  (NOTE_CONV_MAPG_ID
  ,NOTE_CONV_SEQ
  ,NOTE_CONV_TP_CD
  ,SVC_SCP_CD
  ,NOTE_HDR_SEQ
  ,PROP_NO
  ,AMDT_SEQ
  ,CHG_RULE_TP_CD
  ,NOTE_CONV_CHG_CD
  ,NOTE_CONV_RULE_CD
  ,EFF_DT
  ,EXP_DT
  ,RULE_APPL_CHG_TP_CD
  ,RT_APPL_TP_CD
  ,RT_OP_CD
  ,CURR_CD
  ,FRT_RT_AMT
  ,PAY_TERM_CD
  ,GEN_SPCL_RT_TP_CD
  ,BKG_RAT_UT_CD
  ,BKG_PRC_CGO_TP_CD
  ,BKG_IMDG_CLSS_CD
  ,BKG_CMDT_TP_CD
  ,BKG_CMDT_DEF_CD
  ,BKG_SCG_GRP_CMDT_CD
  ,BKG_USA_SVC_MOD_CD
  ,BKG_POR_TP_CD
  ,BKG_POR_DEF_CD
  ,BKG_POL_TP_CD
  ,BKG_POL_DEF_CD
  ,BKG_POD_TP_CD
  ,BKG_POD_DEF_CD
  ,BKG_DEL_TP_CD
  ,BKG_DEL_DEF_CD
  ,BKG_ORG_TRSP_MOD_CD
  ,BKG_DEST_TRSP_MOD_CD
  ,BKG_RCV_TERM_CD
  ,BKG_DE_TERM_CD
  ,BKG_SLAN_CD
  ,BKG_VSL_CD
  ,BKG_SKD_VOY_NO
  ,BKG_SKD_DIR_CD
  ,BKG_SOC_FLG
  ,BKG_ACT_CUST_CNT_CD
  ,BKG_ACT_CUST_SEQ
  ,BKG_TS_PORT_TP_CD
  ,BKG_TS_PORT_DEF_CD
  ,BKG_DIR_CALL_FLG
  ,BKG_MST_HBL_TP_CD
  ,CONV_RAT_UT_CD
  ,CONV_PRC_CGO_TP_CD
  ,CONV_CMDT_TP_CD
  ,CONV_CMDT_DEF_CD
  ,CONV_ORG_LOC_TP_CD
  ,CONV_ORG_LOC_DEF_CD
  ,CONV_ORG_VIA_LOC_TP_CD
  ,CONV_ORG_VIA_LOC_DEF_CD
  ,CONV_DEST_VIA_LOC_TP_CD
  ,CONV_DEST_VIA_LOC_DEF_CD
  ,CONV_DEST_LOC_TP_CD
  ,CONV_DEST_LOC_DEF_CD
  ,CONV_PRC_RCV_TERM_CD
  ,CONV_PRC_DE_TERM_CD
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
  ,BKG_IO_GA_CD
  ,BKG_CNL_TZ_CD
  ,BKG_ESVC_TP_CD
)
  SELECT (SELECT NOTE_CONV_MAPG_ID
            FROM PRI_SP_SCP_RT_CNOTE S
           WHERE S.PROP_NO = B.PROP_NO
             AND S.AMDT_SEQ = B.AMDT_SEQ + 1
             AND S.SVC_SCP_CD = B.SVC_SCP_CD
             AND S.GEN_SPCL_RT_TP_CD = B.GEN_SPCL_RT_TP_CD
             AND S.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
             AND S.CMDT_NOTE_SEQ = B.CMDT_NOTE_SEQ)
        ,A.NOTE_CONV_SEQ
        ,A.NOTE_CONV_TP_CD
        ,A.SVC_SCP_CD
        ,A.NOTE_HDR_SEQ
        ,A.PROP_NO
        ,A.AMDT_SEQ + 1
        ,A.CHG_RULE_TP_CD
        ,A.NOTE_CONV_CHG_CD
        ,A.NOTE_CONV_RULE_CD
        ,A.EFF_DT
        ,DECODE(A.EXP_DT
               ,C.EXP_DT
               ,(SELECT S.EXP_DT
                   FROM PRI_SP_SCP_MN S
                  WHERE S.PROP_NO = B.PROP_NO
                    AND S.AMDT_SEQ = B.AMDT_SEQ + 1
                    AND S.SVC_SCP_CD = B.SVC_SCP_CD
                    AND ROWNUM = 1)
               ,A.EXP_DT)
        ,A.RULE_APPL_CHG_TP_CD
        ,A.RT_APPL_TP_CD
        ,A.RT_OP_CD
        ,A.CURR_CD
        ,A.FRT_RT_AMT
        ,A.PAY_TERM_CD
        ,A.GEN_SPCL_RT_TP_CD
        ,A.BKG_RAT_UT_CD
        ,A.BKG_PRC_CGO_TP_CD
        ,A.BKG_IMDG_CLSS_CD
        ,A.BKG_CMDT_TP_CD
        ,A.BKG_CMDT_DEF_CD
        ,A.BKG_SCG_GRP_CMDT_CD
        ,A.BKG_USA_SVC_MOD_CD
        ,A.BKG_POR_TP_CD
        ,A.BKG_POR_DEF_CD
        ,A.BKG_POL_TP_CD
        ,A.BKG_POL_DEF_CD
        ,A.BKG_POD_TP_CD
        ,A.BKG_POD_DEF_CD
        ,A.BKG_DEL_TP_CD
        ,A.BKG_DEL_DEF_CD
        ,A.BKG_ORG_TRSP_MOD_CD
        ,A.BKG_DEST_TRSP_MOD_CD
        ,A.BKG_RCV_TERM_CD
        ,A.BKG_DE_TERM_CD
        ,A.BKG_SLAN_CD
        ,A.BKG_VSL_CD
        ,A.BKG_SKD_VOY_NO
        ,A.BKG_SKD_DIR_CD
        ,A.BKG_SOC_FLG
        ,A.BKG_ACT_CUST_CNT_CD
        ,A.BKG_ACT_CUST_SEQ
        ,A.BKG_TS_PORT_TP_CD
        ,A.BKG_TS_PORT_DEF_CD
        ,A.BKG_DIR_CALL_FLG
        ,A.BKG_MST_HBL_TP_CD
        ,A.CONV_RAT_UT_CD
        ,A.CONV_PRC_CGO_TP_CD
        ,A.CONV_CMDT_TP_CD
        ,A.CONV_CMDT_DEF_CD
        ,A.CONV_ORG_LOC_TP_CD
        ,A.CONV_ORG_LOC_DEF_CD
        ,A.CONV_ORG_VIA_LOC_TP_CD
        ,A.CONV_ORG_VIA_LOC_DEF_CD
        ,A.CONV_DEST_VIA_LOC_TP_CD
        ,A.CONV_DEST_VIA_LOC_DEF_CD
        ,A.CONV_DEST_LOC_TP_CD
        ,A.CONV_DEST_LOC_DEF_CD
        ,A.CONV_PRC_RCV_TERM_CD
        ,A.CONV_PRC_DE_TERM_CD
        ,A.CRE_USR_ID
        ,A.CRE_DT
        ,A.UPD_USR_ID
        ,A.UPD_DT
  		,A.BKG_IO_GA_CD
  		,A.BKG_CNL_TZ_CD
  		,A.BKG_ESVC_TP_CD
    FROM PRI_SC_NOTE_CONV A, PRI_SP_SCP_RT_CNOTE B, PRI_SP_SCP_MN C
   WHERE B.NOTE_CONV_MAPG_ID = A.NOTE_CONV_MAPG_ID
     AND B.PROP_NO = C.PROP_NO
     AND B.AMDT_SEQ = C.AMDT_SEQ
     AND B.SVC_SCP_CD = C.SVC_SCP_CD
     AND B.PROP_NO = @[prop_no]
     AND B.AMDT_SEQ = @[amdt_seq] - 1
     AND B.SVC_SCP_CD = @[svc_scp_cd]
#if (${CASCADE_LEVEL} == "0") 

#elseif (${CASCADE_LEVEL} == "1") 
     AND B.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
     AND B.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#else 
     AND B.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
     AND B.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
     AND B.CMDT_NOTE_SEQ = @[cmdt_note_seq]
#end			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="cmdt_note_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
