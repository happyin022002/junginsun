<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOMultiCostCodeUSQL">
			<desc><![CDATA[UPDATE ]]></desc>
			<sql><![CDATA[
UPDATE COA_STND_ACCT SET
    DELT_FLG           = @[delt_flg]
   ,STND_COST_NM       = @[stnd_cost_nm]
   ,MGRP_COST_CD       = @[mgrp_cost_cd]
   ,SGRP_COST_CD       = @[sgrp_cost_cd]
   ,PERF_VW_CD         = @[perf_vw_cd]
   ,RA_STND_COST_DESC  = @[ra_stnd_cost_desc]
   ,RA_ACCT_CD         = @[ra_acct_cd]
   ,RA_MN_COST_CD      = @[ra_mn_cost_cd]
   ,RA_SGRP_COST_CD    = @[ra_sgrp_cost_cd]
   ,RA_PERF_VW_CD      = @[ra_perf_vw_cd]
   ,HIR_SCP_FLG        = @[hir_scp_flg]
   ,UPD_USR_ID         = @[upd_usr_id]
   ,ACCT_DP_SEQ        = @[acct_dp_seq]
   ,UPD_DT             = SYSDATE
  WHERE STND_COST_CD = @[stnd_cost_cd]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="Y"/>
				<param name="stnd_cost_nm" type="12" value="" out="Y"/>
				<param name="mgrp_cost_cd" type="12" value="" out="Y"/>
				<param name="sgrp_cost_cd" type="12" value="" out="Y"/>
				<param name="perf_vw_cd" type="12" value="" out="Y"/>
				<param name="ra_stnd_cost_desc" type="12" value="" out="N"/>
				<param name="ra_acct_cd" type="12" value="" out="Y"/>
				<param name="ra_mn_cost_cd" type="12" value="" out="Y"/>
				<param name="ra_sgrp_cost_cd" type="12" value="" out="Y"/>
				<param name="ra_perf_vw_cd" type="12" value="" out="Y"/>
				<param name="hir_scp_flg" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="acct_dp_seq" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
