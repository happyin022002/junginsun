<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchKorCllSppListRSQL">
			<desc><![CDATA[searchKorCllSppList]]></desc>
			<sql><![CDATA[
SELECT ATTR_CTNT1
     , ATTR_CTNT2
     , CSTMS_DIV_ID_SEQ
     , DELT_FLG
     , CSTMS_DIV_ID
     , CNT_CD
     , UPD_USR_ID
     , CRE_USR_ID
  FROM BKG_CSTMS_CD_CONV_CTNT
 WHERE DELT_FLG = @[delt_flg]  --'N'
   AND CNT_CD = @[cnt_cd] --'KR' 
   AND CSTMS_DIV_ID = @[cstms_div_id]--'CLL_SPP_CD'
#if(${attr_ctnt1} != 'ALL')
   AND ATTR_CTNT1 = @[attr_ctnt1]
#end			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="N" out="N"/>
				<param name="cnt_cd" type="12" value="KR" out="N"/>
				<param name="cstms_div_id" type="12" value="CLL_SPP_CD" out="N"/>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
