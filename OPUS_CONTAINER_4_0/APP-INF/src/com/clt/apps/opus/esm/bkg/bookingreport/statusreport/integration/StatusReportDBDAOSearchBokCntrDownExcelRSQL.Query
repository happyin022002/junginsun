           BKG_GET_TOKEN_FNC(TS_POD,1) AS TS_PORT_1,
           BKG_GET_TOKEN_FNC(TS_POD,2) AS TS_PORT_2,
           BKG_GET_TOKEN_FNC(TS_POD,3) AS TS_PORT_3,
           BKG_GET_TOKEN_FNC(TS_POD,4) AS TS_PORT_4,
           BKG_GET_TOKEN_FNC(TS_POD,5) AS TS_PORT_5,
           REGEXP_SUBSTR(CTRT_CUST,'[^◆-]+',1,1) AS CTRT_CUST_CD,
           REGEXP_SUBSTR(CTRT_CUST,'[^◆-]+',1,2) AS CTRT_CUST_NM,
           TRIM(TO_CHAR(TO_NUMBER(NVL(CNTR_WGT_KG,0))+TO_NUMBER(NVL(TARE_WGT_TMP,0)),'999,999,999,990.99')) AS GRS_WGT_KG,
           TRIM(TO_CHAR(TO_NUMBER(NVL(CNTR_WGT_LB,0))+TO_NUMBER(NVL(TARE_LBS_TMP,0)),'999,999,999,990.99')) AS GRS_WGT_LB,
--           TRIM(TO_CHAR(CNTR_WGT_KG,'999,999,999,990.99')) AS CGO_WGT_KG,
--           TRIM(TO_CHAR(CNTR_WGT_LB,'999,999,999,990.99')) AS CGO_WGT_LB,
           TRIM(TO_CHAR(TARE_WGT_TMP,'999,999,999,990.99')) AS TARE_WGT,
           TRIM(TO_CHAR(TARE_LBS_TMP,'999,999,999,990.99')) AS TARE_LBS,

           BKG_Z.*
      FROM (
            SELECT /*+ ORDERED INDEX_RS(VSK (VPS_ETD_DT)) INDEX_RS(N1ST_SKD (VSL_CD,SKD_VOY_NO,SKD_DIR_CD,VPS_PORT_CD,CLPT_IND_SEQ))  INDEX_RS(LAST_SKD (VSL_CD,SKD_VOY_NO,SKD_DIR_CD,VPS_PORT_CD,CLPT_IND_SEQ))
                       USE_NL(VSK, VVD, BK, ISS, DOC, S, C, F, FINV, EBRF, EBSH, EBFF, ESRF, ESSH, ESFF, RGBK, COA, VVD1, VVD2, VVD3, VVD4, VVD5, VVD6, VVD7, VVD8, TRUNK, N1ST_VVD_RID,N1ST_VVD, LAST_VVD_RID,LAST_VVD, N1ST_SKD, LAST_SKD, CNTR, CM, AWK, DG, RF, COP) */
                   TO_CHAR(ROW_NUMBER() OVER (PARTITION BY BK.BKG_NO ORDER BY CNTR.CNTR_NO)) AS BKG_1,
                   TO_CHAR(ROW_NUMBER() OVER (PARTITION BY BK.BKG_NO, CNTR.CNTR_NO ORDER BY CM.CNTR_MF_SEQ)) AS CNTR_1,
            --BKG & B/L Info
                   BK.BL_NO,
                   (SELECT USR.OFC_CD FROM BKG_DOC_PROC_SKD PROC, COM_USER USR
                     WHERE PROC.BKG_DOC_PROC_TP_CD = 'OBLREL'
                       AND PROC.BKG_NO = BK.BKG_NO
                       AND PROC.DOC_PERF_DELT_FLG ='N'
                       AND PROC.EVNT_USR_ID = USR.USR_ID) AS BL_RLSE_OFC,
                   TO_CHAR(BK.CRE_DT,'YYYY-MM-DD HH24:MI') AS CRE_DT,
                   (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01619' AND INTG_CD_VAL_CTNT = BK.XTER_BKG_RQST_CD) AS BKG_KND,
                   BK.BKG_NO,
                   BK.BKG_OFC_CD,
                   BK.CRE_USR_ID,
                   BK.BKG_STS_CD,
                   BK.CTRT_SREP_CD,
                   BK.OB_SREP_CD,
                   NVL((SELECT 'Complete' FROM BKG_CHG_RT WHERE BKG_NO = BK.BKG_NO AND ROWNUM=1),'Incomplete') AS RATE_STS,
                   (SELECT LOC_CD FROM MDM_ORGANIZATION WHERE OFC_CD = BK.BKG_OFC_CD) AS SLS_AREA,
                   BK.OB_SREP_CD AS SLS_CD,
                   BK.OB_SLS_OFC_CD,
                   BK.SCAC_CD,
                   BK.STWG_CD AS SPCL_HNDL_INST,
				   (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD02146' AND INTG_CD_VAL_CTNT = BK.STWG_CD) AS SPCL_HNDL_INST_DESC,	--20150326.ADD
                   BK.SPLIT_FLG,
                   BK.STOP_OFF_LOC_CD,
                   BK.STOP_OFF_CNTC_PHN_NO,
                   BK.STOP_OFF_CNTC_PSON_NM,
                   REPLACE(REPLACE(REPLACE(BK.STOP_OFF_DIFF_RMK,CHR(10),' '),CHR(9),' '),CHR(34),'') STOP_OFF_DIFF_RMK,
                   REPLACE(REPLACE(REPLACE(BK.XTER_RMK,CHR(10),' '),CHR(9),' '),CHR(34),'') AS XTER_RMK,
                   REPLACE(REPLACE(REPLACE(BK.INTER_RMK,CHR(10),' '),CHR(9),' '),CHR(34),'') AS INTER_RMK,
                   REPLACE(REPLACE(REPLACE(BK.VNDR_RMK,CHR(10),' '),CHR(9),' '),CHR(34),'') AS VNDR_RMK,
				   BKG_JOIN_FNC(CURSOR(SELECT CNTR_TPSZ_CD||'-'||TRIM(TO_CHAR(OP_CNTR_QTY,'999999990.99')) FROM BKG_QUANTITY WHERE BKG_NO = BK.BKG_NO),', ') BKG_QTY,
                   COP.COP_NO COP_NO,	
				   COP.CNTR_TPSZ_CD COP_CNTR_TPSZ_CD,
            --Cargo & Commodity
                   TO_CHAR(CM.MEAS_QTY) AS LCL_MEAS_QTY,
                   CM.MEAS_UT_CD AS LCL_MEAS_UT_CD,
                   NVL(SUBSTR(DECODE(BK.DCGO_FLG,'Y',',DG','')||DECODE(BK.RC_FLG,'Y',',RF','')||DECODE(BK.AWK_CGO_FLG,'Y',',AW','')||DECODE(BK.BB_CGO_FLG,'Y',',BB','')||DECODE(BK.RD_CGO_FLG,'Y',',RD',''),2),'DR') AS CGO_N,
--                   TO_CHAR(ROUND(DECODE(DOC.WGT_UT_CD, 'KGS', DOC.ACT_WGT, ROUND(NVL(DOC.ACT_WGT,0)*0.4536,3)),1)) AS CGO_WGT_KG,--BKG별 WEIGHT->CNTR WGT 으로 변경(2015.03.13)
--                   TO_CHAR(ROUND(DECODE(DOC.WGT_UT_CD, 'KGS', ROUND(NVL(DOC.ACT_WGT,0)*2.2046,3), DOC.ACT_WGT),2)) AS CGO_WGT_LB,
                   TRIM(TO_CHAR(TO_CHAR(ROUND(DECODE(CM.WGT_UT_CD, 'KGS', CM.CNTR_MF_WGT, ROUND(NVL(CM.CNTR_MF_WGT,0)*0.453592,3)),2)),'999,999,999,990.99')) AS CGO_WGT_KG,--CM별 WEIGHT
                   TRIM(TO_CHAR(TO_CHAR(ROUND(DECODE(CM.WGT_UT_CD, 'KGS', NVL(CM.CNTR_MF_WGT,0)/0.453592, CM.CNTR_MF_WGT),2)),'999,999,999,990.99')) AS CGO_WGT_LB,
                   DECODE(SUBSTR(BK.CMDT_CD,1,1),0,'''','')||BK.CMDT_CD AS CMDT_CD,
                   (SELECT CMDT_NM FROM MDM_COMMODITY WHERE CMDT_CD = BK.CMDT_CD) AS CMDT_DESC,
                   DECODE(SUBSTR(CM.CMDT_HS_CD,1,1),0,'''','')||CM.CMDT_HS_CD AS CMDT_HS_CD,
                   DECODE(SUBSTR(CM.CMDT_HS_CD,1,1),0,'''','')||SUBSTR(CM.CMDT_HS_CD,1,4) AS CMDT_HS_GRP_CD,
                   CM.HAMO_TRF_CD AS HTS_CD,
                   CM.NCM_NO,
                   TO_CHAR(CM.PCK_QTY) AS LCL_PCK_QTY,
                   DECODE(BK.RCV_TERM_CD, 'S', BK.POR_NOD_CD, '') AS OB_CFS_LOC,
                   REPLACE(REPLACE(REPLACE(CM.CNTR_MF_GDS_DESC,CHR(10),' '),CHR(9),' '),CHR(34),'') AS LCL_CNTR_MF_GDS_DESC,
                   CM.PCK_TP_CD AS LCL_PCK_TP_CD,
            --Container
				   (SELECT TO_CHAR(NVL(MNL_SET_DT,SYS_SET_DT),'YYYY-MM-DD HH24:MI') FROM BKG_CLZ_TM WHERE CLZ_TP_CD='R' AND BKG_NO=BK.BKG_NO) AS CGO_CUTOFF_DT,
                   TO_CHAR(ROUND(DECODE(CNTR.WGT_UT_CD, 'KGS', CNTR.CNTR_WGT, ROUND(NVL(CNTR.CNTR_WGT,0)*0.453592,3)),2)) AS CNTR_WGT_KG,--CNTR별 WEIGHT
                   TO_CHAR(ROUND(DECODE(CNTR.WGT_UT_CD, 'KGS', NVL(CNTR.CNTR_WGT,0)/0.453592, CNTR.CNTR_WGT),2)) AS CNTR_WGT_LB,
                   CNTR.CNTR_NO,
                   TO_CHAR(CNTR.CNTR_VOL_QTY) AS CNTR_VOL_QTY,
                   (SELECT TO_CHAR(SUM(DECODE(CNTR_SZ_CD, 2, 1, 2) * OP_CNTR_QTY))
                      FROM BKG_QUANTITY QTY,
                           MDM_CNTR_TP_SZ MDM
                     WHERE BKG_NO = BK.BKG_NO
                       AND QTY.CNTR_TPSZ_CD = MDM.CNTR_TPSZ_CD) AS TEU,
                   (SELECT TO_CHAR(SUM(DECODE(CNTR_SZ_CD, 2, 0.5, 1) * OP_CNTR_QTY))
                      FROM BKG_QUANTITY QTY,
                           MDM_CNTR_TP_SZ MDM
                     WHERE BKG_NO = BK.BKG_NO
                       AND QTY.CNTR_TPSZ_CD = MDM.CNTR_TPSZ_CD) AS FEU,
                   CNTR.CNTR_TPSZ_CD,
                   DECODE(BK.BKG_STS_CD,'X','',DECODE(COP.COP_STS_CD,'F','MT',(SELECT B.CNMV_STS_CD FROM MST_CONTAINER B WHERE B.CNTR_NO = CNTR.CNTR_NO))) CNMV_STS_CD,
                   (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD00252' 
                                                                 AND INTG_CD_VAL_CTNT = (DECODE(BK.BKG_STS_CD,'X','',DECODE(COP.COP_STS_CD,'F','MT',(SELECT B.CNMV_STS_CD FROM MST_CONTAINER B WHERE B.CNTR_NO = CNTR.CNTR_NO))))) AS CNMV_STS_DESC,
                   NVL((SELECT TO_CHAR(ROUND(B.TARE_WGT,2))
                          FROM MST_CONTAINER A
                              ,MST_CNTR_SPEC B
                         WHERE A.CNTR_SPEC_NO = B.CNTR_SPEC_NO
                           AND A.CNTR_NO = CNTR.CNTR_NO)
                     , (SELECT TO_CHAR(ROUND(CNTR_TPSZ_TARE_WGT,2))
                          FROM MDM_CNTR_TP_SZ MDM
                         WHERE MDM.CNTR_TPSZ_CD = CNTR.CNTR_TPSZ_CD)
                   ) AS TARE_WGT_TMP,
                   NVL((SELECT TO_CHAR(ROUND(B.TARE_WGT / 0.453592,2))
                          FROM MST_CONTAINER A
                              ,MST_CNTR_SPEC B
                         WHERE A.CNTR_SPEC_NO = B.CNTR_SPEC_NO
                           AND A.CNTR_NO = CNTR.CNTR_NO)
                     , (SELECT TO_CHAR(ROUND(CNTR_TPSZ_TARE_WGT / 0.453592,2))
                          FROM MDM_CNTR_TP_SZ MDM
                         WHERE MDM.CNTR_TPSZ_CD = CNTR.CNTR_TPSZ_CD)
                   ) AS TARE_LBS_TMP,
                   (SELECT PKUP_YD_CD FROM BKG_PKUP_NTC_PKUP_NO WHERE BKG_NO = BK.BKG_NO AND CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1) AS IB_PKUP_YD_CD,
                   (SELECT CSTMS_LOC_CD FROM BKG_CSTMS_ADV_BL WHERE BKG_NO = BK.BKG_NO AND BL_NO = BK.BL_NO AND CNT_CD = 'US') AS IB_CLR_LOC_CD,
                   NVL((SELECT DECODE(TRO.HLG_TP_CD, 'C', 'C', 'M')
                            FROM BKG_EUR_TRO TRO
                           WHERE TRO.BKG_NO = BK.BKG_NO
                             AND TRO.CNTR_NO = CNTR.CNTR_NO
                             AND TRO.IO_BND_CD = 'I'
                             AND TRO.CXL_FLG = 'N'
                             AND ROWNUM=1), DECODE(BK.DE_TERM_CD, 'D', 'C', 'M')) AS IB_HLG_BKG,
                   DECODE(BK.RCV_TERM_CD, 'D', 'C', 'M') AS OB_HLG_BKG,
                   CNTR.PCK_TP_CD,
                   TO_CHAR(CNTR.PCK_QTY) AS PCK_QTY,
                   BKG_JOIN_FNC(CURSOR(SELECT CNTR_SEAL_NO
                                         FROM BKG_CNTR_SEAL_NO
                                        WHERE BKG_NO = CNTR.BKG_NO
                                          AND CNTR_NO = CNTR.CNTR_NO
                                          AND CNTR_SEAL_SEQ > 0
                               ),',') AS CNTR_SEAL_NO,
                   (SELECT TO_CHAR(COUNT(CNTR_SEAL_SEQ))
                      FROM BKG_CNTR_SEAL_NO
                     WHERE BKG_NO = CNTR.BKG_NO
                       AND CNTR_NO = CNTR.CNTR_NO
                     GROUP BY BKG_NO, CNTR_NO) AS CNTR_SEAL_CNT,
                   BKG_JOIN_FNC(CURSOR(SELECT CNTR_TPSZ_CD||'-'||EQ_SUBST_CNTR_TPSZ_CD 
                                         FROM BKG_QUANTITY
                                        WHERE BKG_NO = BK.BKG_NO
                                          AND EQ_SUBST_CNTR_TPSZ_CD IS NOT NULL
                               ),',') AS EQ_SUBST_TP,
                   CNTR.ORG_YD_CD,
                   (SELECT TO_CHAR(MST.CNMV_DT, 'YYYY-MM-DD HH24:MI')
                      FROM MST_CONTAINER MST
                     WHERE MST.CNTR_NO = CNTR.CNTR_NO) CNMV_EVNT_DT,
                   CNTR.SOC_FLG,
                   BK.MTY_PKUP_YD_CD AS BKG_MTY_PKUP_YD_CD,
               	   (SELECT TO_CHAR(BKG_GET_VGM_CCT_FNC(SKD.POL_NOD_CD, SKD.SLAN_CD, SKD.SLAN_DIR_CD, 'AL', SKD.VVD, SKD.CLPT_IND_SEQ, SKD.VPS_ETB_DT, SKD.VPS_ETD_DT), 'YYYY-MM-DD HH24:MI') AS VGM_CCT
                  	  FROM (SELECT SKD.SLAN_CD SLAN_CD , VVD.SKD_DIR_CD SLAN_DIR_CD , CASE WHEN DCGO_FLG = 'Y' THEN 'DG'
 								                                                           WHEN RC_FLG = 'Y' THEN 'RF'
                                    								                       WHEN DCGO_FLG = 'N' AND RC_FLG = 'N' AND AWK_CGO_FLG = 'N' AND BB_CGO_FLG = 'N' THEN 'DR'
											                                               ELSE 'AL'
                                             										       END CGO_TP_CD ,
                                   VPS_ETB_DT ,  VPS_ETD_DT , POL_NOD_CD ,  NVL(VVD.POL_CLPT_IND_SEQ, 1) AS CLPT_IND_SEQ , VVD.VSL_CD||VVD.SKD_VOY_NO||VVD.SKD_DIR_CD AS VVD, BK.BKG_NO
                          	  FROM BKG_BOOKING BK, BKG_VVD VVD, VSK_VSL_PORT_SKD SKD
                         	 WHERE BK.BKG_NO = VVD.BKG_NO
                           	   AND BK.POL_CD = VVD.POL_CD
                          	   AND VVD.VSL_PRE_PST_CD IN ('S','T')
                           	   AND VVD.VSL_CD NOT IN ('COXX','COYY','COZZ')
                           	   AND VVD.VSL_CD = SKD.VSL_CD
                               AND VVD.SKD_VOY_NO = SKD.SKD_VOY_NO
                           	   AND VVD.SKD_DIR_CD = SKD.SKD_DIR_CD
                               AND VVD.POL_CD = SKD.VPS_PORT_CD
                               AND NVL(VVD.POL_CLPT_IND_SEQ, 1) = SKD.CLPT_IND_SEQ) SKD
                 	 WHERE SKD.BKG_NO = BK.BKG_NO AND ROWNUM = 1) VGM_CUT_OFF_TM,
               	   TO_CHAR(ROUND(DECODE(CNTR.VGM_WGT_UT_CD, 'KGS', CNTR.VGM_WGT, 'LBS', CNTR.VGM_WGT * 0.453592, NULL), 3)) VGM_WGT,
            --Customer
                   (SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER WHERE CUST_CNT_CD = C.CUST_CNT_CD AND CUST_SEQ = C.CUST_SEQ) AS C_CUST,
                   (SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER WHERE CUST_CNT_CD = F.CUST_CNT_CD AND CUST_SEQ = F.CUST_SEQ) AS F_CUST,
                   (SELECT CNTC_PSON_NM FROM MDM_CUST_ADDR WHERE CUST_CNT_CD = F.CUST_CNT_CD AND CUST_SEQ = F.CUST_SEQ AND PRMRY_CHK_FLG = 'Y' AND ROWNUM = 1) AS F_CNTC_NM,
                   (SELECT PHN_NO FROM MDM_CUST_CNTC_PNT WHERE CUST_CNT_CD = F.CUST_CNT_CD AND CUST_SEQ = F.CUST_SEQ AND ROWNUM = 1) AS F_CNTC_NO,
                   F.CUST_CNT_CD AS F_CNT_CD,
                   (SELECT STE_CD FROM MDM_CUST_ADDR WHERE CUST_CNT_CD = F.CUST_CNT_CD AND CUST_SEQ = F.CUST_SEQ AND PRMRY_CHK_FLG = 'Y' AND ROWNUM = 1) AS F_STE_CD,
                   (SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER WHERE CUST_CNT_CD = S.CUST_CNT_CD AND CUST_SEQ = S.CUST_SEQ) AS S_CUST_NM,
                   (SELECT CNTC_PSON_NM FROM MDM_CUST_ADDR WHERE CUST_CNT_CD = S.CUST_CNT_CD AND CUST_SEQ = S.CUST_SEQ AND PRMRY_CHK_FLG = 'Y' AND ROWNUM = 1) AS S_CNTC_NM,
                   (SELECT PHN_NO FROM MDM_CUST_CNTC_PNT WHERE CUST_CNT_CD = S.CUST_CNT_CD AND CUST_SEQ = S.CUST_SEQ AND ROWNUM = 1) AS S_CNTC_NO,
                   BKG_CTRL_PTY_CUST_CNT_CD||LPAD(BK.BKG_CTRL_PTY_CUST_SEQ,6,'0') AS CTRT_PTY_CD,
                   (SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER WHERE CUST_CNT_CD = BK.BKG_CTRL_PTY_CUST_CNT_CD AND CUST_SEQ = BK.BKG_CTRL_PTY_CUST_SEQ) AS CTRT_PTY_NM,
            --Reference No
                   EBFF.CUST_REF_NO_CTNT AS EBFF_REF_NO,
                   EBRF.CUST_REF_NO_CTNT AS EBRF_REF_NO,
                   EBSH.CUST_REF_NO_CTNT AS EBSH_REF_NO,
                   CASE WHEN (SELECT COUNT(1) FROM BKG_IMG_STO IMG WHERE IMG.BKG_NO = BK.BKG_NO AND IMG.RIDR_TP_CD = 'D') > 0 THEN 'Y' ELSE 'N' END AS DG_CERTI_FLG,
                   BKG_JOIN_FNC(CURSOR(SELECT EUR_BL.MVMT_REF_NO
										 FROM BKG_CSTMS_EUR_BL EUR_BL
										WHERE BK.BL_NO = EUR_BL.BL_NO),';') AS ENS_MRN,
                   DECODE(CNTR.CNTR_NO, NULL, 
                                          BKG_JOIN_FNC(CURSOR(SELECT CUST_REF_NO_CTNT
                                                                FROM BKG_REFERENCE CUCR
                                                               WHERE CUCR.BKG_NO = BK.BKG_NO
                                                                 AND CUCR.BKG_REF_TP_CD = 'CMRN'
                                                               ORDER BY CUCR.REF_SEQ
                                                              ),','),
                                          (SELECT CUST_REF_NO_CTNT
                                             FROM BKG_REFERENCE CUCR
                                            WHERE CUCR.BKG_NO = BK.BKG_NO
                                              AND CUCR.CNTR_NO = CNTR.CNTR_NO
                                              AND CUCR.BKG_REF_TP_CD = 'CMRN')) AS CMRN_REF_NO,
                   BKG_JOIN_FNC(CURSOR(SELECT EX_EUR_BL.MVMT_REF_NO
										 FROM BKG_CSTMS_EUR_IO_BL EX_EUR_BL
										WHERE BK.BL_NO = EX_EUR_BL.BL_NO),';') AS EXS_MRN,
                   FINV.CUST_REF_NO_CTNT AS FINV_REF_NO,
                   (SELECT DECODE(XPT_IMP.AES_TP_CD,'AE',XPT_IMP.AES_INLND_TRNS_PFX_CTNT||' '||XPT_IMP.AES_INLND_TRNS_NO,
                                                                   'PA',XPT_IMP.AES_PTA_PFX_CTNT||' '||XPT_IMP.AES_PTA_NO1||'-'||XPT_IMP.AES_PTA_NO2||' '||TO_CHAR(XPT_IMP.AES_PTA_DT,'MM/DD/YYYY'),
                                                                   'PU',XPT_IMP.AES_PTU_PFX_CTNT||' '||XPT_IMP.AES_PTU_NO||' '||TO_CHAR(XPT_IMP.AES_PTU_DT,'MM/DD/YYYY'),
                                                                   'DN',XPT_IMP.AES_DWN_PFX_CTNT||' '||XPT_IMP.AES_DWN_NO||' '||TO_CHAR(XPT_IMP.AES_DWN_DT,'MM/DD/YYYY'),
                                                                   'EX',NVL(XPT_IMP.AES_EXPT_CTNT, (SELECT INTG_CD_VAL_DESC
                                                                                                      FROM COM_INTG_CD_DTL
                                                                                                     WHERE INTG_CD_ID = 'CD02570'
                                                                                                       AND APLY_ST_DT <= TO_CHAR(SYSDATE, 'YYYYMMDD')
                                                                                                       AND APLY_END_DT >= TO_CHAR(SYSDATE, 'YYYYMMDD')
                                                                                                       AND INTG_CD_VAL_CTNT = XPT_IMP.AES_EXPT_ID)),' ')
					FROM BKG_XPT_IMP_LIC XPT_IMP
					WHERE BKG_NO = BK.BKG_NO 
					AND IO_BND_CD = 'O' 
					AND CNT_CD = 'US' 
					AND ROWNUM = 1
					) AS AES_ITN_NO,
                   CNTR.PO_NO,
                   RGBK.CUST_REF_NO_CTNT AS RGBK_REF_NO,
                   ESSH.CUST_REF_NO_CTNT AS ESSH_REF_NO,
                   ESFF.CUST_REF_NO_CTNT AS ESFF_REF_NO,
                   ESRF.CUST_REF_NO_CTNT AS ESRF_REF_NO,
                   BK.TWN_SO_NO,
                   BK.SI_FLG,
                   DECODE(CNTR.CNTR_NO, NULL, 
                                          BKG_JOIN_FNC(CURSOR(SELECT CUST_REF_NO_CTNT
                                                                FROM BKG_REFERENCE CUCR
                                                               WHERE CUCR.BKG_NO = BK.BKG_NO
                                                                 AND CUCR.BKG_REF_TP_CD = 'CUCR'
                                                               ORDER BY CUCR.REF_SEQ
                                                              ),','),
                                          (SELECT CUST_REF_NO_CTNT
                                             FROM BKG_REFERENCE CUCR
                                            WHERE CUCR.BKG_NO = BK.BKG_NO
                                              AND CUCR.CNTR_NO = CNTR.CNTR_NO
                                              AND CUCR.BKG_REF_TP_CD = 'CUCR'
                                              AND ROWNUM = 1)) AS CUCR_REF_NO,
                   (SELECT CAED_TP_CD FROM BKG_XPT_IMP_LIC WHERE BKG_NO = BK.BKG_NO AND IO_BND_CD = 'O' AND CNT_CD = 'CA' AND ROWNUM = 1) AS CAED_TP_CD,
                   (SELECT DECODE(XPT_IMP.CAED_TP_CD,'CE', XPT_IMP.CAED_PFX_CTNT       ||XPT_IMP.CAED_NO1       ||XPT_IMP.CAED_NO2    ||XPT_IMP.CAED_NO3,
                                                     'G7', XPT_IMP.G7_EDI_PFX_CTNT     ||XPT_IMP.G7_EDI_NO1     ||XPT_IMP.G7_EDI_NO2,
                                                     'SM', XPT_IMP.MF_SMRY_RPT_PFX_CTNT||XPT_IMP.MF_SMRY_RPT_NO,
                                                     'EX', XPT_IMP.B13A_XPT_PFX_CTNT   ||XPT_IMP.B13A_XPT_NO1   ||XPT_IMP.B13A_XPT_NO2 ||TO_CHAR(XPT_IMP.B13A_XPT_DT,'YYYYMMDDHHMM'),
                                                     'IT', XPT_IMP.CGO_CTRL_PFX_CTNT   ||XPT_IMP.CGO_CTRL_NO,
                                                     'ND', XPT_IMP.NDR_REF_PFX_CTNT    ||NDR_REF_ID,' ')
                      FROM BKG_XPT_IMP_LIC XPT_IMP
                     WHERE XPT_IMP.BKG_NO      = BK.BKG_NO 
                       AND XPT_IMP.IO_BND_CD   ='O'
                       AND XPT_IMP.CNT_CD = 'CA'
                       AND ROWNUM = 1
                   ) AS CAED_NO,
            --Route & Schedule
                   BK.BLCK_STWG_CD,
                   BK.VSL_CD||BK.SKD_VOY_NO||BK.SKD_DIR_CD AS T_VVD,
                   (SELECT VSL_ENG_NM FROM MDM_VSL_CNTR WHERE VSL_CD = BK.VSL_CD) AS T_VSL_NM,
                   COA.TRD_CD,
                   BK.SVC_SCP_CD,
                   BK.DE_TERM_CD,
                   BK.RCV_TERM_CD,
                   (SELECT CNT_CD FROM MDM_LOCATION WHERE LOC_CD = BK.POR_CD) AS POR_CNT_CD,
                   BK.POR_CD,
                   (SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = BK.POR_CD) AS POR_NM,
                   VVD5.POL_CD AS PRE1_POL_CD,
                   (SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = VVD5.POL_CD) AS PRE1_POL_NM,
