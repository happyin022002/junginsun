<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TargetGroupDBDAOSearchTargetGroupTrade0013ListRSQL">
			<desc><![CDATA[Target Group 과 Trade/Bound/Sub Trade 의 mapping 정보]]></desc>
			<sql><![CDATA[
SELECT A.TRD_CD                    
          , A.SUB_TRD_CD                
          , A.DIR_CD                    
          , A.SAQ_TGT_GRP_CD            
     FROM SAQ_TGT_GRP_TRD A             
    WHERE A.TRD_CD         LIKE @[trade]||'%'  
      AND A.SUB_TRD_CD     LIKE @[sub_trade]||'%'  
      AND A.SAQ_TGT_GRP_CD LIKE @[tgt_grp_cd]||'%'  
 ORDER BY A.TRD_CD                    
          , A.SUB_TRD_CD                
          , A.DIR_CD                    
          , A.SAQ_TGT_GRP_CD			]]></sql>
			<params>
				<param name="trade" type="12" value="" out="N"/>
				<param name="sub_trade" type="12" value="" out="N"/>
				<param name="tgt_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
