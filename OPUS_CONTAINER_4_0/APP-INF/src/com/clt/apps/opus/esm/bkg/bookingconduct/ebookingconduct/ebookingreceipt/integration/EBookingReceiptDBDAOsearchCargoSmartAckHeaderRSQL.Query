<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchCargoSmartAckHeaderRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
	'$$$MSGSTART:'||RPAD(LTRIM(ATTR_CTNT2),20)||RPAD(LTRIM(ATTR_CTNT1),20)||RPAD('APERAK',10)||@[apk_msg_no]||CHR(10)
	||'MSG_FLAG:VGM' || CHR(10)
	||'CUST_MSG_NO:' || @[rqst_no] || CHR(10)
	||'APK_MSG_NO:' || @[apk_msg_no] || CHR(10) AS HEADER_FILE
FROM BKG_HRD_CDG_CTNT
WHERE HRD_CDG_ID = @[hrd_cdg_id]
AND ATTR_CTNT1 = @[sndr_id]			]]></sql>
			<params>
				<param name="apk_msg_no" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="hrd_cdg_id" type="12" value="" out="N"/>
				<param name="sndr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
