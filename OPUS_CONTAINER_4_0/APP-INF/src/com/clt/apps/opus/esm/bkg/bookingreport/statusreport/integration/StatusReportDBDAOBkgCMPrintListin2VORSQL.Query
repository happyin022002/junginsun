<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOBkgCMPrintListin2VORSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
SELECT FAX.* FROM 
(
SELECT A.BL_NO
      ,A.BKG_NO
      ,A.POD_CD
      ,REPLACE(B.CNTC_PSON_NM,CHR(10),' ') CNTC_PSON_NM
      ,REPLACE(B.CNTC_PSON_FAX_NO,CHR(10),' ') CNTC_PSON_FAX_NO
      ,B.BKG_CNTC_PSON_TP_CD
      ,C.CNTR_NO
      ,C.CNTR_TPSZ_CD
      ,REPLACE(E.CNTR_MF_GDS_DESC,CHR(10),' ') CNTR_MF_GDS_DESC
      ,NVL(E.PCK_QTY,0) AS PCK_QTY
      ,NVL(E.CNTR_MF_WGT,0) AS ACT_WGT
      ,NVL(E.MEAS_QTY,0) AS MEAS_QTY
      ,BKG_JOIN_FNC(CURSOR( SELECT XPT_LIC_NO FROM BKG_XPT_IMP_LIC WHERE BKG_NO = C.BKG_NO)) XPT_LIC_NO
      ,REPLACE(H.CUST_NM,CHR(10),' ') CUST_NM
      ,BKG_JOIN_FNC(CURSOR( SELECT CNTR_SEAL_NO FROM BKG_CNTR_SEAL_NO WHERE BKG_NO = C.BKG_NO AND CNTR_NO = C.CNTR_NO)) CNTR_SEAL_NO
      ,'FAX' TAB_TP
  FROM BKG_BOOKING      A
      ,BKG_CNTC_PSON    B
      ,BKG_CONTAINER    C
      ,BKG_CNTR_MF_DESC E
      ,BKG_CUSTOMER     H
      ,BKG_VVD          I
 WHERE A.BKG_NO = B.BKG_NO(+)
   AND A.BKG_NO = C.BKG_NO(+)
   AND A.BKG_NO = E.BKG_NO(+)
   AND A.BKG_NO = H.BKG_NO(+)
   AND A.BKG_NO(+) = I.BKG_NO
   AND H.BKG_CUST_TP_CD(+) = 'S'
#if (${vsl_cd} != '') 
   AND I.VSL_CD = @[vsl_cd]
   AND I.SKD_VOY_NO = @[skd_voy_no]
   AND I.SKD_DIR_CD = @[skd_dir_cd]
#end
#if (${pol_cd} != '') 
   AND I.POL_CD = @[pol_cd]
#end
#if (${pol_yd_cd} != '') 
   AND A.POL_NOD_CD = @[pol_yd_cd]
#end
#if (${pod_cd} != '') 
   AND I.POD_CD = @[pod_cd]
#end
#if (${pod_yd_cd} != '') 
   AND A.POD_NOD_CD = @[pod_yd_cd]
#end
#if (${rcv_term_cd} != '') 
   AND A.RCV_TERM_CD = @[rcv_term_cd]
#end
#if (${de_term_cd} != '') 
   AND A.DE_TERM_CD = @[de_term_cd]
#end
#if (${bkg_ofc_cd} != '') 
   AND A.BKG_OFC_CD = @[bkg_ofc_cd]
#end
#if (${cre_usr_id} != '') 
   AND A.CRE_USR_ID = @[cre_usr_id]
#end
#if (${bkg_no} != '') 
   AND A.BKG_NO = @[bkg_no]
#end
#if (${bl_no} != '') 
   AND A.BL_NO = @[bl_no]
#end
#if (${ob_srep_cd} != '') 
   AND A.OB_SREP_CD = @[ob_srep_cd]
#end
#if (${cntr_no} != '') 
   AND C.CNTR_NO = @[cntr_no]
#end
#if (${cust_cnt_cd} != '') 
   AND H.CUST_CNT_CD(+) = @[cust_cnt_cd]
   AND H.CUST_SEQ(+)	= @[cust_seq]
#end
#if (${cust_nm} != '')
   AND H.CUST_NM LIKE '%' || @[cust_nm] || '%'
#end
   ORDER BY A.BL_NO, H.CUST_NM, B.CNTC_PSON_FAX_NO
)FAX
WHERE FAX.BKG_CNTC_PSON_TP_CD = DECODE(BKG_CNTC_PSON_TP_CD, 'BK', 'BK', DECODE(BKG_CNTC_PSON_TP_CD, 'SI', 'SI'), 'AL')			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="rcv_term_cd" type="12" value="" out="N"/>
				<param name="de_term_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="ob_srep_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
