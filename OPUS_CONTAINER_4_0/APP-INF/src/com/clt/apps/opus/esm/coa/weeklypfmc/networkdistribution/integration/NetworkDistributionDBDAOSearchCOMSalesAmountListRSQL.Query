<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOSearchCOMSalesAmountListRSQL">
			<desc><![CDATA[SearchCOMSalesAmountList search]]></desc>
			<sql><![CDATA[
SELECT
     A.TRD_CD                                  AS TRD_CD
    ,A.RLANE_CD                                AS RLANE_CD
    ,A.VSL_CD                                  AS VSL_CD
    ,A.SKD_VOY_NO                              AS SKD_VOY_NO
    ,A.DIR_CD                                  AS DIR_CD
    ,B.VOP_CD                                  AS VOP_CD
    ,B.VSL_CAPA                                AS VSL_CAPA
    ,B.BSA_CAPA                                AS BSA_CAPA
    ,B.BSA_OP_CD                               AS BSA_OP_CD
    ,DECODE(B.BSA_OP_CD, 'J','J/O', 'S','S/C') AS BSA_OP_NM
    ,B.FNL_CO_BSA_CAPA                         AS FNL_CO_BSA_CAPA
    ,B.CO_BSA_CAPA                             AS CO_BSA_CAPA
    ,B.CO_BSA_RTO*100                          AS CO_BSA_RTO
    ,B.CHTR_BSA_RTO*100                        AS CHTR_BSA_RTO
    ,C.TS_UC_AMT                               AS TS_UC_AMT
    ,C.AMT_1_01                                AS AMT_1_01 
    ,C.AMT_1_02                                AS AMT_1_02 
    ,C.AMT_1_03                                AS AMT_1_03 
    ,C.AMT_1_04                                AS AMT_1_04 
    ,C.AMT_1_05                                AS AMT_1_05 
    ,C.AMT_1_06                                AS AMT_1_06 
    ,C.AMT_1_07                                AS AMT_1_07 
    ,C.AMT_1_08                                AS AMT_1_08 
    ,C.AMT_1_09                                AS AMT_1_09 
    ,C.AMT_1_10                                AS AMT_1_10 
    ,C.AMT_1_11                                AS AMT_1_11 
    ,C.AMT_1_12                                AS AMT_1_12 
    ,C.AMT_1_13                                AS AMT_1_13 
    ,C.AMT_1_14                                AS AMT_1_14 
    ,C.AMT_1_15                                AS AMT_1_15 
    ,C.AMT_2_01                                AS AMT_2_01 
    ,C.AMT_2_02                                AS AMT_2_02 
    ,C.AMT_2_03                                AS AMT_2_03 
    ,C.AMT_2_04                                AS AMT_2_04 
    ,C.AMT_2_05                                AS AMT_2_05 
    ,C.AMT_2_06                                AS AMT_2_06 
    ,C.AMT_2_07                                AS AMT_2_07 
    ,C.AMT_2_08                                AS AMT_2_08 
    ,C.AMT_2_09                                AS AMT_2_09 
    ,C.AMT_2_10                                AS AMT_2_10 
    ,C.AMT_2_11                                AS AMT_2_11 
    ,C.AMT_2_12                                AS AMT_2_12 
    ,C.AMT_2_13                                AS AMT_2_13 
    ,C.AMT_2_14                                AS AMT_2_14 
    ,C.AMT_2_15                                AS AMT_2_15 
    ,C.AMT_3_01                                AS AMT_3_01 
    ,C.AMT_3_02                                AS AMT_3_02 
    ,C.AMT_3_03                                AS AMT_3_03 
    ,C.AMT_3_04                                AS AMT_3_04 
    ,C.AMT_3_05                                AS AMT_3_05 
    ,C.AMT_3_06                                AS AMT_3_06 
    ,C.AMT_3_07                                AS AMT_3_07 
    ,C.AMT_3_08                                AS AMT_3_08 
    ,C.AMT_3_09                                AS AMT_3_09 
    ,C.AMT_3_10                                AS AMT_3_10 
    ,C.AMT_3_11                                AS AMT_3_11 
    ,C.AMT_3_12                                AS AMT_3_12 
    ,C.AMT_3_13                                AS AMT_3_13 
    ,C.AMT_3_14                                AS AMT_3_14 
    ,C.AMT_3_15                                AS AMT_3_15 
   FROM  COA_MON_VVD A
        ,BSA_VVD_MST B
        ,(SELECT
              TRD_CD                                                    AS TRD_CD
             ,RLANE_CD                                                  AS RLANE_CD
             ,IOC_CD                                                    AS IOC_CD
             ,VSL_CD                                                    AS VSL_CD
             ,SKD_VOY_NO                                                AS SKD_VOY_NO
             ,DIR_CD                                                    AS DIR_CD
             ,SUM(TS_UC_AMT)                                            AS TS_UC_AMT
             ,SUM(DECODE(STND_COST_CD,'53101000',NTWK_HIR_COST_AMT,0))  AS AMT_1_01
             ,SUM(DECODE(STND_COST_CD,'53102000',NTWK_HIR_COST_AMT,0))  AS AMT_1_02
             ,SUM(DECODE(STND_COST_CD,'53200000',NTWK_HIR_COST_AMT,0))  AS AMT_1_03
             ,SUM(DECODE(STND_COST_CD,'54100000',NTWK_HIR_COST_AMT,0))  AS AMT_1_04
             ,SUM(DECODE(STND_COST_CD,'54250000',NTWK_HIR_COST_AMT,0))  AS AMT_1_05
             ,SUM(DECODE(STND_COST_CD,'54300000',NTWK_HIR_COST_AMT,0))  AS AMT_1_06
             ,SUM(DECODE(STND_COST_CD,'54200000',NTWK_HIR_COST_AMT,0))  AS AMT_1_07
             ,SUM(DECODE(STND_COST_CD,'54150000',NTWK_HIR_COST_AMT,0))  AS AMT_1_08
             ,SUM(DECODE(STND_COST_CD,'54450000',NTWK_HIR_COST_AMT,0))  AS AMT_1_09
             ,SUM(DECODE(STND_COST_CD,'54180000',NTWK_HIR_COST_AMT,0))  AS AMT_1_10
             ,SUM(DECODE(STND_COST_CD,'54550000',NTWK_HIR_COST_AMT,0))  AS AMT_1_11
             ,SUM(DECODE(STND_COST_CD,'54350000',NTWK_HIR_COST_AMT,0))  AS AMT_1_12
             ,SUM(DECODE(STND_COST_CD,'54400000',NTWK_HIR_COST_AMT,0))  AS AMT_1_13
             ,SUM(DECODE(STND_COST_CD,'54400010',NTWK_HIR_COST_AMT,0))  AS AMT_1_14
             ,SUM(DECODE(STND_COST_CD,'75000000',NTWK_HIR_COST_AMT,0))  AS AMT_1_15		--20150622.ADD
             ,SUM(DECODE(STND_COST_CD,'53101000',CO_SLS_AMT,0))         AS AMT_2_01
             ,SUM(DECODE(STND_COST_CD,'53102000',CO_SLS_AMT,0))         AS AMT_2_02
             ,SUM(DECODE(STND_COST_CD,'53200000',CO_SLS_AMT,0))         AS AMT_2_03
             ,SUM(DECODE(STND_COST_CD,'54100000',CO_SLS_AMT,0))         AS AMT_2_04
             ,SUM(DECODE(STND_COST_CD,'54250000',CO_SLS_AMT,0))         AS AMT_2_05
             ,SUM(DECODE(STND_COST_CD,'54300000',CO_SLS_AMT,0))         AS AMT_2_06
             ,SUM(DECODE(STND_COST_CD,'54200000',CO_SLS_AMT,0))         AS AMT_2_07
             ,SUM(DECODE(STND_COST_CD,'54150000',CO_SLS_AMT,0))         AS AMT_2_08
             ,SUM(DECODE(STND_COST_CD,'54450000',CO_SLS_AMT,0))         AS AMT_2_09
             ,SUM(DECODE(STND_COST_CD,'54180000',CO_SLS_AMT,0))         AS AMT_2_10
             ,SUM(DECODE(STND_COST_CD,'54550000',CO_SLS_AMT,0))         AS AMT_2_11
             ,SUM(DECODE(STND_COST_CD,'54350000',CO_SLS_AMT,0))         AS AMT_2_12
             ,SUM(DECODE(STND_COST_CD,'54400000',CO_SLS_AMT,0))         AS AMT_2_13
             ,SUM(DECODE(STND_COST_CD,'54400010',CO_SLS_AMT,0))         AS AMT_2_14
             ,SUM(DECODE(STND_COST_CD,'75000000',CO_SLS_AMT,0))         AS AMT_2_15		--20150622.ADD
             ,SUM(DECODE(STND_COST_CD,'53101000',CO_AMT,0))             AS AMT_3_01
             ,SUM(DECODE(STND_COST_CD,'53102000',CO_AMT,0))             AS AMT_3_02
             ,SUM(DECODE(STND_COST_CD,'53200000',CO_AMT,0))             AS AMT_3_03
             ,SUM(DECODE(STND_COST_CD,'54100000',CO_AMT,0))             AS AMT_3_04
             ,SUM(DECODE(STND_COST_CD,'54250000',CO_AMT,0))             AS AMT_3_05
             ,SUM(DECODE(STND_COST_CD,'54300000',CO_AMT,0))             AS AMT_3_06
             ,SUM(DECODE(STND_COST_CD,'54200000',CO_AMT,0))             AS AMT_3_07
             ,SUM(DECODE(STND_COST_CD,'54150000',CO_AMT,0))             AS AMT_3_08
             ,SUM(DECODE(STND_COST_CD,'54450000',CO_AMT,0))             AS AMT_3_09
             ,SUM(DECODE(STND_COST_CD,'54180000',CO_AMT,0))             AS AMT_3_10
             ,SUM(DECODE(STND_COST_CD,'54550000',CO_AMT,0))             AS AMT_3_11
             ,SUM(DECODE(STND_COST_CD,'54350000',CO_AMT,0))             AS AMT_3_12
             ,SUM(DECODE(STND_COST_CD,'54400000',CO_AMT,0))             AS AMT_3_13
             ,SUM(DECODE(STND_COST_CD,'54400010',CO_AMT,0))             AS AMT_3_14
             ,SUM(DECODE(STND_COST_CD,'75000000',CO_AMT,0))             AS AMT_3_15		--20150622.ADD
            FROM COA_VVD_HIR
           GROUP BY TRD_CD
                   ,RLANE_CD
                   ,IOC_CD
                   ,VSL_CD
                   ,SKD_VOY_NO
                   ,DIR_CD
        ) C
  WHERE  A.TRD_CD     = B.TRD_CD
    AND  A.RLANE_CD   = B.RLANE_CD
    AND  A.IOC_CD     = B.IOC_CD
    AND  A.VSL_CD     = B.VSL_CD
    AND  A.SKD_VOY_NO = B.SKD_VOY_NO
    AND  A.DIR_CD     = B.SKD_DIR_CD
    AND  A.TRD_CD     = C.TRD_CD
    AND  A.RLANE_CD   = C.RLANE_CD
    AND  A.IOC_CD     = C.IOC_CD
    AND  A.VSL_CD     = C.VSL_CD
    AND  A.SKD_VOY_NO = C.SKD_VOY_NO
    AND  A.DIR_CD     = C.DIR_CD
    AND  NVL(A.DELT_FLG,'N') = 'N'

    #if (${f_seltrade} != '')
       AND A.TRD_CD = @[f_seltrade]
    #end
    #if (${f_selrlane} != '')
       AND A.RLANE_CD = @[f_selrlane]
    #end
    #if (${f_selioc} != '')
       AND A.IOC_CD = @[f_selioc]
    #end
    #if (${f_vsl_cd} != '')
       AND A.VSL_CD = @[f_vsl_cd]
    #end
    #if (${f_skd_voy_no} != '')
       AND A.SKD_VOY_NO = @[f_skd_voy_no]
    #end
    #if (${f_dir_cd} != '')
       AND A.DIR_CD = @[f_dir_cd]
    #end
	#if (${f_chkprd} == 'M')
       #if (${f_year} != '')
           AND A.COST_YRMON BETWEEN @[f_year]||@[f_fm_mon] AND @[f_year]||@[f_to_mon]
       #else
           AND A.COST_YRMON LIKE @[f_cost_yr] || '%'
       #end
	#elseif (${f_chkprd} == 'W')
       AND A.SLS_YRMON LIKE @[f_year]||'%'
       #if (${f_fm_wk} != '')
           AND A.COST_WK BETWEEN @[f_fm_wk] AND @[f_to_wk]
       #end
    #end			]]></sql>
			<params>
				<param name="f_seltrade" type="12" value="" out="N"/>
				<param name="f_selrlane" type="12" value="" out="N"/>
				<param name="f_selioc" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
				<param name="f_skd_voy_no" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_year" type="12" value="" out="N"/>
				<param name="f_fm_mon" type="12" value="" out="N"/>
				<param name="f_to_mon" type="12" value="" out="N"/>
				<param name="f_cost_yr" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
