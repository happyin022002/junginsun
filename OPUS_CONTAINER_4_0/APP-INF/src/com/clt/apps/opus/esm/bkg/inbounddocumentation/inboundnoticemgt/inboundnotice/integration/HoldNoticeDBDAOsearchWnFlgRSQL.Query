<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOsearchWnFlgRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT CNT_CD
      ,CSTMS_DSPO_CD
      ,CSTMS_DSPO_NM
      ,DSPO_DESC
      ,CSTMS_PAIR_DSPO_CD
      ,DSPO_TP_CD
      ,OB_NTC_FLG
      ,DELT_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,AUTO_NTC_FLG
      ,DIFF_RMK
      ,USA_CSTMS_ENTR_FLG
      ,USA_CSTMS_RLSE_FLG
      ,USA_CSTMS_ENTR_ICRZ_FLG
      ,USA_CSTMS_RLSE_ICRZ_FLG
FROM   BKG_CSTMS_ADV_DSPO
WHERE  CNT_CD             = 'US'
AND    DELT_FLG           = 'N'
AND    CSTMS_DSPO_CD      = @[cstms_dspo_cd] -- Code
			]]></sql>
			<params>
				<param name="cstms_dspo_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
