<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOmodifyBlCntrSealUSQL">
			<desc><![CDATA[modifyBlCntrSeal]]></desc>
			<sql><![CDATA[
UPDATE  BKG_CSTMS_SEAL_NO
SET     SEAL_NO = @[seal_no],
		SEAL_KND_CD = TRIM(@[seal_knd_cd]),
        SEAL_PTY_TP_CD = TRIM(@[seal_pty_tp_cd]),
        SEAL_PTY_NM = TRIM(@[seal_pty_nm]),
    	UPD_USR_ID  = @[upd_usr_id],
    	UPD_DT      = SYSDATE
WHERE	CNT_CD  = @[cnt_cd]
AND     BL_NO   = @[bl_no]
AND	    CNTR_NO = @[cntr_no]
AND     SEAL_NO_SEQ = 1			]]></sql>
			<params>
				<param name="seal_no" type="12" value="" out="N"/>
				<param name="seal_knd_cd" type="12" value="" out="N"/>
				<param name="seal_pty_tp_cd" type="12" value="" out="N"/>
				<param name="seal_pty_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
