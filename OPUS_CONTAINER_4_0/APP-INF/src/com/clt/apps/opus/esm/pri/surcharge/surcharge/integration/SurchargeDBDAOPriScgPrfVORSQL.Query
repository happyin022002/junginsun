<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOPriScgPrfVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	SVC_SCP_CD
,	CHG_CD
,	FLT_PCT_TP_CD
,	PCT_BSE_CD
,	SUB_TRD_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN SUB_TRD_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS SUB_TRD_CNT
,	SLAN_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN VSL_SLAN_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS VSL_SLAN_CNT
,	POR_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN POR_DEF_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS POR_DEF_CNT
,	POL_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN POL_DEF_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS POL_DEF_CNT
,	POD_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN POD_DEF_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS POD_DEF_CNT
,	DEL_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN DEL_DEF_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS DEL_DEF_CNT
,	IMDG_CLSS_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN SCG_IMDG_CLSS_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS SCG_IMDG_CLSS_CNT
,	TS_PORT_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN TS_PORT_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS TS_PORT_CNT
,	TML_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN TML_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS TML_CNT
,	TRNS_MOD_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN ORG_TRSP_MOD_CD IS NULL AND DEST_TRSP_MOD_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS TRNS_MOD_CNT
,	USA_SVC_MOD_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN USA_SVC_MOD_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS USA_SVC_MOD_CNT
,	RCV_DE_TERM_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN PRC_RCV_TERM_CD IS NULL AND PRC_DE_TERM_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS RCV_DE_TERM_CNT
,	HNGR_BAR_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN PRC_HNGR_BAR_TP_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS HNGR_BAR_CNT
,	DIR_CALL_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN DIR_CALL_FLG IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS DIR_CALL_CNT
,	CGO_WGT_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN MIN_CGO_WGT IS NULL AND MAX_CGO_WGT IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS CGO_WGT_CNT
,	CMDT_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN CMDT_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS CMDT_CNT
,	GRI_CMDT_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN SCG_GRP_CMDT_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS GRI_CMDT_CNT
,	SOC_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN SOC_FLG IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS SOC_CNT
,   IO_GA_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN IO_GA_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT 
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS IG_GA_CNT
,   CNL_TZ_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN CNL_TZ_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS CNL_TZ_CNT
,	CRE_USR_ID
,	TO_CHAR(CRE_DT, 'YYYY-MM-DD') AS CRE_DT
,	UPD_USR_ID
,	TO_CHAR(UPD_DT, 'YYYY-MM-DD') AS UPD_DT
,   ESVC_USE_FLG
,   (SELECT NVL(SUM(CASE
                   WHEN BKG_ESVC_TP_CD IS NULL THEN 0
                   ELSE 1
                END), 0)
       FROM PRI_SCG_RT
      WHERE SVC_SCP_CD = @[svc_scp_cd] 
        AND CHG_CD = @[chg_cd]
        AND DELT_FLG = 'N') AS ESVC_CNT
FROM PRI_SCG_PRF
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	CHG_CD = @[chg_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
