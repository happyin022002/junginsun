<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCMQCProposalDBDAOPriSpSubMqcAmdVOCSQL">
			<desc><![CDATA[SCMQCProposalDBPriSpSubMqcAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SUB_MQC(
    PROP_NO,
    AMDT_SEQ,
    SUB_MQC_CTNT,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    PROP_NO,
    @[amdt_seq]+1,
    SUB_MQC_CTNT,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID		,
    ACPT_OFC_CD		,
    ACPT_DT			,
    @[cre_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ
FROM
    PRI_SP_SUB_MQC
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
