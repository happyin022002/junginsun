<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOMdmEurFlgBkgNoRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 'Y' AS EUR_FLG
FROM BKG_BOOKING BB
     ,MDM_COUNTRY MC
WHERE SUBSTR(BB.POL_CD,1,2) = MC.CNT_CD
AND   MC.SCONTI_CD LIKE 'E%'
AND   BB.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
