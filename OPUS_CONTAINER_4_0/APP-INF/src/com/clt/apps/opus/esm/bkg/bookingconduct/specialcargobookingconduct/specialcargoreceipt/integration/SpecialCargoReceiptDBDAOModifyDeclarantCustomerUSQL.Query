<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOModifyDeclarantCustomerUSQL">
			<desc><![CDATA[DG Shipper, DG Consignee 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_DG_DECL A
	SET CUST_CNT_CD = @[cust_cnt_cd] 
	,	CUST_SEQ = @[cust_seq]
	,	CUST_NM = @[cust_nm]
	,	CUST_ADDR = @[cust_addr]
	,	CUST_CTY_NM = @[cust_cty_nm]
	,	CUST_STE_CD = @[cust_ste_cd]
	,	CSTMS_DECL_CNT_CD = @[cstms_decl_cnt_cd]
	,	CUST_ZIP_ID = @[cust_zip_id]
	,	PHN_NO = @[phn_no]
	,	CUST_FAX_NO = @[cust_fax_no]
	,	CUST_EML = @[cust_eml]
	,	UPD_USR_ID = @[upd_usr_id]
	,	UPD_DT = SYSDATE
	,	DECL_NM = @[decl_nm]
#if (${ca_flg} == 'Y')
    ,   CNTR_NO = (SELECT CNTR_NO FROM BKG_DG_CGO_HIS WHERE BKG_NO = A.BKG_NO AND CORR_NO = 'TMP0000001' AND DG_CNTR_SEQ = A.DG_CNTR_SEQ AND ROWNUM=1)
#else
    ,   CNTR_NO = (SELECT CNTR_NO FROM BKG_DG_CGO WHERE BKG_NO = A.BKG_NO AND DG_CNTR_SEQ = A.DG_CNTR_SEQ AND ROWNUM=1)
#end
WHERE BKG_NO = @[bkg_no]
AND DG_CNTR_SEQ = @[dg_cntr_seq]
AND BKG_CUST_TP_CD = @[bkg_cust_tp_cd]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="cust_addr" type="12" value="" out="N"/>
				<param name="cust_cty_nm" type="12" value="" out="N"/>
				<param name="cust_ste_cd" type="12" value="" out="N"/>
				<param name="cstms_decl_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_zip_id" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="cust_fax_no" type="12" value="" out="N"/>
				<param name="cust_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="decl_nm" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dg_cntr_seq" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
