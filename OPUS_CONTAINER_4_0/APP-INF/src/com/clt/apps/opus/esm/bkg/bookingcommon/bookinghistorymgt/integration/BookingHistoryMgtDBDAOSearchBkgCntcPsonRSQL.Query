<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgCntcPsonRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgCntcPsonRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
        , BKG_CNTC_PSON_TP_CD
        , CNTC_PSON_NM
        , CNTC_PSON_PHN_NO
        , CNTC_PSON_MPHN_NO
        , CNTC_PSON_FAX_NO
        , CNTC_PSON_EML
        , DIFF_RMK
        , CRE_USR_ID
        , TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
        , UPD_USR_ID
        , TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
  FROM BKG_CNTC_PSON
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
