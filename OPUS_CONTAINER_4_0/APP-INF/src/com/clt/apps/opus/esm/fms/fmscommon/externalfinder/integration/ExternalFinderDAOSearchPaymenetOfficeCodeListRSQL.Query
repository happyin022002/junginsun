		<query name="ExternalFinderDAOSearchPaymenetOfficeCodeListRSQL">
			<desc><![CDATA[ExternalFinderDAOSearchPaymenetOfficeCodeListRSQL]]></desc>
			<sql><![CDATA[
SELECT FP.PPT_CTNT OFC_CD,
       FP.PPT_CTNT || ' ' || MO.OFC_LOCL_NM
  FROM FMS_PPT_SET FP, MDM_ORGANIZATION MO
 WHERE FP.PPT_CTNT = MO.OFC_CD
   AND FP.PPT_NM = 'OFFICE_CODE'		]]></sql>
			<params>
			</params>
		</query>
	</querys>
