<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAOPriSpScpRtActCustAmdVOCSQL">
			<desc><![CDATA[SCRateProposalDBDAOPriSpScpRtActCustAmdVOCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_RT_ACT_CUST(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    GEN_SPCL_RT_TP_CD,
    CMDT_HDR_SEQ,
    ACT_CUST_SEQ,
    CUST_CNT_CD,
    CUST_SEQ,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    PROP_NO             ,
    AMDT_SEQ+1          ,
    SVC_SCP_CD          ,
    GEN_SPCL_RT_TP_CD   ,
    CMDT_HDR_SEQ        ,
    ACT_CUST_SEQ        ,
    CUST_CNT_CD         ,
    CUST_SEQ            ,
    PRC_PROG_STS_CD     ,
    SRC_INFO_CD         ,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    @[cre_usr_id]       ,
    SYSDATE             ,
    @[upd_usr_id]       ,
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ
FROM 
    PRI_SP_SCP_RT_ACT_CUST CUST
WHERE
    PROP_NO     = @[prop_no]
AND AMDT_SEQ    = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'
AND EXISTS ( 
    SELECT 'OK'
    FROM    
        PRI_SP_SCP_RT
    WHERE   
        PROP_NO             = CUST.PROP_NO
    AND AMDT_SEQ            = CUST.AMDT_SEQ
    AND SVC_SCP_CD          = CUST.SVC_SCP_CD
    AND GEN_SPCL_RT_TP_CD   = CUST.GEN_SPCL_RT_TP_CD
    AND CMDT_HDR_SEQ        = CUST.CMDT_HDR_SEQ
	AND SRC_INFO_CD			<> 'AD'   
)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
