			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_BL
	SET CSTMS_MF_TP_CD = @[command],
		MF_SND_DT = DECODE(@[command], 'MI', GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', sysdate, 'USNYC'), MF_SND_DT),
		AMDT_SND_DT = DECODE(@[command], 'AI', GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', sysdate, 'USNYC'), AMDT_SND_DT)
WHERE
	CNT_CD = 'US'
	AND BL_NO      = @[bl_no]			]]></sql>
