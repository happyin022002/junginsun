<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOBkgMdtItmVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
/* BkgMdtItmVO */
#if (${VO} == 'VO') 
SELECT
	'' MDT_ITM_SEQ,
	'' BKG_MDT_CATE_CD,
	'' CUST_GRP_ID,
	'' MDT_CUST_TP_CD,
	'' CUST_CNT_CD,
	'' CUST_SEQ,
	'' SC_NO,
	'' RFA_NO,
	'' SVC_SCP_CD,
	'' POR_CD,
	'' POL_CD,
	'' POD_CD,
	'' DEL_CD,
	'' MDT_ITM_RMK,
	'' CRE_USR_ID,
	'' CRE_DT,
	'' UPD_USR_ID,
	'' UPD_DT,
	'' ITM_CD_POB,
	'' ITM_NM_POB,
	'' ITM_CD_POC,
	'' ITM_NM_POC,
	'' ITM_CD_POM,
	'' ITM_NM_POM,
	'' ITM_CD_INV,
	'' ITM_NM_INV,
	'' ITM_CD_DEP,
	'' ITM_NM_DEP,
	'' ITM_CD_LC,
	'' ITM_NM_LC,
	'' ITM_CD_SHP,
	'' ITM_NM_SHP,
	'' ITM_CD_PAT,
	'' ITM_NM_PAT,
	'' ITM_CD_INC,
	'' ITM_NM_INC,
	'' BKG_MDT_ITM_CD,
	'' FCUST,
	'' SC_EXP_DT,
	'' RFA_EXP_DT
FROM DUAL
#else
SELECT
     A.MDT_ITM_SEQ
,    A.BKG_MDT_CATE_CD
,    A.CUST_GRP_ID
,    A.MDT_CUST_TP_CD
,    A.CUST_CNT_CD
,    A.CUST_SEQ
,    A.SC_NO
,    A.RFA_NO
,    A.SVC_SCP_CD
,    A.POR_CD
,    A.POL_CD
,    A.POD_CD
,    A.DEL_CD
,    A.MDT_ITM_RMK
,    A.CRE_USR_ID
,    A.CRE_DT
,    A.UPD_USR_ID
,    TO_CHAR(A.UPD_DT,'YYYY-MM-DD') UPD_DT
,    '' ITM_CD_POB
,    'P/O No.(by BKG)' ITM_NM_POB
,    '' ITM_CD_POC
,    'P/O No.(by CNTR)' ITM_NM_POC
,    '' ITM_CD_POM
,    'P/O No.(by Item)' ITM_NM_POM
,    '' ITM_CD_INV
,    'Invoice No.' ITM_NM_INV
,    '' ITM_CD_DEP
,    'Department No.' ITM_NM_DEP
,    '' ITM_CD_LC
,    'L/C No.' ITM_NM_LC
,    '' ITM_CD_SHP
,    'Ship ID' ITM_NM_SHP
,    '' ITM_CD_PAT
,    'Part No.' ITM_NM_PAT
,    '' ITM_CD_INC
,    'Incoterms' ITM_NM_INC
,    BKG_JOIN_FNC(CURSOR(SELECT BKG_MDT_ITM_CD  FROM BKG_MDT_ITM_DTL WHERE MDT_ITM_SEQ = A.MDT_ITM_SEQ)) AS BKG_MDT_ITM_CD
,	 MDT_CUST_TP_CD||CUST_CNT_CD||CUST_SEQ AS FCUST
,	(SELECT
    TO_CHAR(max(MN.EXP_DT), 'YYYY-MM-DD') AS TO_DT
    FROM   PRI_SP_HDR HD,
    PRI_SP_MN MN
    WHERE  1=1
    AND    HD.SC_NO =  A.SC_NO
    AND    HD.PROP_NO = MN.PROP_NO
    AND    PROP_STS_CD ='F' ) SC_EXP_DT
,	(SELECT 
	TO_CHAR(max(MN.EXP_DT), 'YYYY-MM-DD') TO_DT
	FROM   PRI_RP_HDR HD,
	PRI_RP_MN MN
	WHERE  1=1
	AND    HD.RFA_NO = A.RFA_NO
	AND    HD.PROP_NO = MN.PROP_NO
	AND    PROP_STS_CD ='A' /*상수*/) RFA_EXP_DT	
FROM BKG_MDT_ITM A
WHERE (@[cust_grp_id] IS NULL OR A.CUST_GRP_ID = @[cust_grp_id])
#if (${cust_cnt_cd} != '') 
AND   A.CUST_CNT_CD = @[cust_cnt_cd]
#end
#if (${cust_seq} != '') 
AND   A.CUST_SEQ = @[cust_seq]
#end
#if (${sc_no} != '') 
AND   A.SC_NO = @[sc_no]
#end
#if (${rfa_no} != '') 
AND   A.RFA_NO = @[rfa_no]
#end
#if (${svc_scp_cd} != '') 
AND   A.SVC_SCP_CD = @[svc_scp_cd]
#end
#if (${por_cd} != '') 
AND   A.POR_CD = @[por_cd]
#end
#if (${pod_cd} != '') 
AND   A.POD_CD = @[pod_cd]
#end
#end			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
