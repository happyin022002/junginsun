<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentTradeDBDAOCreateMonthlyAdjustmentRhqInfoB0048CSQL">
			<desc><![CDATA[MonthlyQuotaAdjustmentTrade 에서 Notify 처리시 SETP '02' (RHQ 정보)를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_MON_QTA_TRD (
    MQTA_STEP_CD,
    BSE_YR,
    BSE_QTR_CD,
    TRD_CD,
    DIR_CD,
    MQTA_VER_NO,
    RLANE_CD,
    SPRT_GRP_CD,
    BSA_GRP_CD,
    CTRT_RHQ_CD,
    BSE_MON,
    SUB_TRD_CD,
    LOD_QTY,
    GRS_RPB_REV,
    CM_UC_AMT,
    OPFIT_UC_AMT,
    RA_CM_UC_AMT,
    RA_OPFIT_UC_AMT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
     )
SELECT
    '02' AS MQTA_STEP_CD,
    TRD.BSE_YR,
    TRD.BSE_QTR_CD,
    TRD.TRD_CD,
    TRD.DIR_CD,
    VER.MQTA_VER_NO,
    TRD.RLANE_CD,
    TRD.SPRT_GRP_CD,
    TRD.BSA_GRP_CD,
    TRD.CTRT_RHQ_CD,
    TRD.BSE_MON,
    TRD.SUB_TRD_CD,
    TRD.LOD_QTY,
    TRD.GRS_RPB_REV,
    TRD.CM_UC_AMT,
    TRD.OPFIT_UC_AMT,
    TRD.RA_CM_UC_AMT,
    TRD.RA_OPFIT_UC_AMT,
    @[userId] AS CRE_USR_ID,
    SYSDATE AS CRE_DT,
    @[userId] AS UPD_USR_ID,
    SYSDATE AS UPD_DT
FROM SAQ_MON_QTA_TRD TRD,
    (
    SELECT CRE_OFC_CD,
           MAX(MQTA_VER_NO) AS MQTA_VER_NO
    FROM   SAQ_MON_QTA_STEP_VER STV
    WHERE  STV.MQTA_STEP_CD = '02'
    AND    STV.BSE_YR = @[year]
    AND    STV.BSE_QTR_CD = @[bse_quarter]
    AND    STV.TRD_CD = @[trade]
    AND    STV.DIR_CD = @[bound]
    AND    STV.GLINE_VER_NO = @[glineVerNo]
    GROUP BY CRE_OFC_CD ) VER
WHERE TRD.MQTA_STEP_CD = '01'
AND   TRD.BSE_YR = @[year]
AND   TRD.BSE_QTR_CD = @[bse_quarter]
AND   TRD.TRD_CD = @[trade]
AND   TRD.DIR_CD = @[bound]
AND   TRD.MQTA_VER_NO = @[mQtaVerNo]
AND   VER.CRE_OFC_CD = TRD.CTRT_RHQ_CD			]]></sql>
			<params>
				<param name="userId" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_quarter" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="glineVerNo" type="12" value="" out="N"/>
				<param name="mQtaVerNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
