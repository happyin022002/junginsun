<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOExistPerCodeRSQL">
			<desc><![CDATA[BookingUtilDB DAO ExistPerCode RSQL.Query]]></desc>
			<sql><![CDATA[
SELECT 
    CASE 
        WHEN COUNT(*)>0 THEN 'Y'
        ELSE 'N'
    END OUTPUT_TEXT
FROM PRI_RAT_UT
WHERE RAT_UT_CD = @[input_text]
AND DELT_FLG='N'			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
