<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOmodifyCaCgoRlseEdiUSQL">
			<desc><![CDATA[CargoReleaseOrderDBDAOmodifyCaCgoRlseEdiUSQL]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_CGO_RLSE A

USING (
       SELECT @[bl_no] AS BL_NO
         FROM DUAL
      ) B
ON (A.BL_NO = B.BL_NO)

WHEN MATCHED THEN

UPDATE 
   SET HBL_FLG	        = 'N',   
#if (${frt_clt_flg} != '') 
       FRT_CLT_FLG		= @[frt_clt_flg],
       FRT_CLT_LST_DT	= DECODE(@[frt_clt_flg],FRT_CLT_FLG,FRT_CLT_LST_DT,GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,'CATOR')),
#end

#if (${obl_rdem_flg} != '' )
       OBL_RDEM_FLG	    = @[obl_rdem_flg],
       OBL_RDEM_LST_DT	= DECODE(@[obl_rdem_flg],OBL_RDEM_FLG,OBL_RDEM_LST_DT,GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,'CATOR')),
#end

       UPD_USR_ID		= @[usr_id],
       UPD_DT			= SYSDATE,
       BL_RCV_KNT       = DECODE(NVL(TO_NUMBER(@[bl_rcv_knt]),0),0,NULL,
                              DECODE(TO_NUMBER(@[bl_rcv_knt]),BL_RCV_KNT,BL_RCV_KNT,@[bl_rcv_knt])),
       BL_RCV_OFC_CD    = DECODE(NVL(TO_NUMBER(@[bl_rcv_knt]),0),0,NULL,
                              DECODE(TO_NUMBER(@[bl_rcv_knt]),BL_RCV_KNT,BL_RCV_OFC_CD,@[usr_ofc_cd])),
       BL_RCV_USR_ID    = DECODE(NVL(TO_NUMBER(@[bl_rcv_knt]),0),0,NULL,
                              DECODE(TO_NUMBER(@[bl_rcv_knt]),BL_RCV_KNT,BL_RCV_USR_ID,@[usr_id])),
       BL_RCV_DT        = TO_DATE(DECODE(NVL(TO_NUMBER(@[bl_rcv_knt]),0),0,NULL,
                              DECODE(TO_NUMBER(@[bl_rcv_knt]),BL_RCV_KNT,TO_CHAR(BL_RCV_DT,'YYYYMMDDHH24MISS'), 
                                     TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,'CATOR'),'YYYYMMDDHH24MISS'))),
                          'YYYYMMDDHH24MISS')

WHEN NOT MATCHED THEN

INSERT (
        BL_NO                   ,
        HBL_FLG                 ,
        FRT_CLT_FLG             ,
        FRT_CLT_LST_DT          ,
        OBL_RDEM_FLG            ,
        OBL_RDEM_LST_DT         ,
        CSTMS_CLR_CD            ,
        CSTMS_CLR_LST_DT        ,
        CSTMS_DSPO_CD           ,
        MRN_TML_EDI_SND_FLG     ,
        MRN_TML_EDI_SND_CD      ,
        MRN_TML_EDI_LST_SND_DT  ,
        MRN_TML_EDI_LST_SND_GDT ,
        MRN_TML_EDI_RCV_ID      ,
        MRN_TML_EDI_LST_MSG_ID  ,
        MRN_TML_EDI_LST_SCS_FLG ,
        INLND_YD_EDI_SND_FLG    ,
        INLND_YD_EDI_SND_CD     ,
        INLND_YD_EDI_LST_SND_DT ,
        INLND_YD_EDI_LST_SND_GDT,
        INLND_YD_EDI_RCV_ID     ,
        INLND_YD_EDI_LST_MSG_ID ,
        INLND_YD_EDI_LST_SCS_FLG,
        CRE_USR_ID              ,
        CRE_DT                  ,
        UPD_USR_ID              ,
        UPD_DT                  )
VALUES (@[bl_no],
        'N'           ,
        NVL(@[frt_clt_flg],'N'),
        GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,'CATOR'),
        NVL(@[obl_rdem_flg],'N'),  
        GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,'CATOR'),
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        NULL          ,
        @[usr_id]     ,
        SYSDATE       ,
        @[usr_id]     ,
        SYSDATE       )			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="frt_clt_flg" type="12" value="" out="N"/>
				<param name="obl_rdem_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bl_rcv_knt" type="12" value="" out="N"/>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
