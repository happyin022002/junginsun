<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchShprNameRSQL">
			<desc><![CDATA[Shipper Name 조회]]></desc>
			<sql><![CDATA[
SELECT SUBSTR( LTRIM( REPLACE( CUST_NM, CHR(10), ' ' ) ), 1   , 35)  SHPR_NM1
     , NVL(LTRIM(  SUBSTR( LTRIM( REPLACE( CUST_NM, CHR(10), ' ' ) ), 36  , 35) ),' ') SHPR_NM2
  FROM BKG_CUSTOMER
 WHERE BKG_NO          =   @[bkg_no]
   AND BKG_CUST_TP_CD  =   'S'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
