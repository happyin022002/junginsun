<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAORemoveCustCoverInfoDSQL">
			<desc><![CDATA[RemoveCustCoverInfo]]></desc>
			<sql><![CDATA[
DELETE FROM SAM_CUST_SLS_REP_INFO
WHERE CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = TO_NUMBER(@[cust_seq])
AND SREP_CD = @[pre_srep_cd]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="pre_srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
