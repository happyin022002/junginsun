<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchChnBkgNoExistRSQL">
			<desc><![CDATA[china agent bkg no로 미리 채번하였는지 조회한다.]]></desc>
			<sql><![CDATA[
select bkg_no_use_flg
  from BKG_CHN_BKG_NO_GEN
 where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
