<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOmodifyBkgStwgInfoUSQL">
			<desc><![CDATA[modifyBkgStwgInfo]]></desc>
			<sql><![CDATA[
UPDATE BKG_BOOKING SET 
#if (${apro_cd}== 'Y')
	STWG_CD  = (SELECT STWG_CD 
				FROM BKG_STWG_CGO 
				WHERE BKG_NO = @[bkg_no]
				AND	  STWG_SEQ = @[stwg_seq])
,	STWG_RMK = (SELECT STWG_RMK 
				FROM BKG_STWG_CGO 
				WHERE BKG_NO = @[bkg_no]
				AND	  STWG_SEQ = @[stwg_seq])

#else
	STWG_CD = NULL
,	STWG_RMK = NULL

#end
WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="stwg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
