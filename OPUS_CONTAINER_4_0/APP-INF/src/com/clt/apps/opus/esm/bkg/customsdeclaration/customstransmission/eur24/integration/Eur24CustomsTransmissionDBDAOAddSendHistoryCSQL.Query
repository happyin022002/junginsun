<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24CustomsTransmissionDBDAOAddSendHistoryCSQL">
			<desc><![CDATA[BKG_CSTMS_ADV_EUR_SND에 데이터 저장]]></desc>
			<sql><![CDATA[
INSERT
INTO BKG_CSTMS_ADV_EUR_SND ( 
	EUR_EDI_MSG_TP_ID, 	MSG_SND_NO, 
	SND_DT, 			SND_GDT, 		SND_USR_ID, 
	VSL_CD, 			SKD_VOY_NO, 	SKD_DIR_CD, 		MSG_FUNC_ID,
	BL_NO, 				CSTMS_PORT_CD, 	EDI_SND_MSG_CTNT, 	SND_USR_OFC_CD,
	CRE_USR_ID, 		CRE_DT, 		UPD_USR_ID, 		UPD_DT
)VALUES( 
    @[eur_edi_msg_tp_id],   @[msg_snd_no],
    SYSDATE,              	GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', sysdate, 'GBLON'),             @[snd_usr_id],
    @[vsl_cd],   			@[skd_voy_no],   		@[skd_dir_cd],			@[msg_func_id],
    @[bl_no],               @[cstms_port_cd],       @[edi_snd_msg_ctnt],	(SELECT OFC_CD FROM COM_USER WHERE USR_ID = @[upd_usr_id]),
    @[cre_usr_id],          SYSDATE,                @[upd_usr_id],          SYSDATE
)			]]></sql>
			<params>
				<param name="eur_edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="msg_snd_no" type="12" value="" out="N"/>
				<param name="snd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="msg_func_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
				<param name="edi_snd_msg_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
