<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOProcessAgtErrLogSelectRSQL">
			<desc><![CDATA[processAgtErrLogSelect]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(MAX(ERR_LOG_SEQ),1) AS ERR_LOG_SEQ
FROM AGT_ERR_LOG
WHERE BKG_NO = @[bkg_no]
    AND ERR_LOG_FLG = @[err_log_flg]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="Y"/>
				<param name="err_log_flg" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
