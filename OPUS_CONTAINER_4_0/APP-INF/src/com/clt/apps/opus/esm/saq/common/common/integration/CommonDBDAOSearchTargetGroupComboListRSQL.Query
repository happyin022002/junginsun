<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchTargetGroupComboListRSQL">
			<desc><![CDATA[searchTargetGroupComboList 목록 조회]]></desc>
			<sql><![CDATA[
SELECT A.SAQ_TGT_GRP_CD GRP_CD, A.SAQ_TGT_GRP_DESC GRP_DESC
FROM SAQ_TGT_GRP A 
WHERE 1=1
AND A.DELT_FLG IN ('N', @[del])
#if (${ofc_cd} != '' )
	    AND A.OFC_CD = @[ofc_cd] 
#end 
ORDER BY A.SAQ_TGT_GRP_CD, A.SAQ_TGT_GRP_DESC			]]></sql>
			<params>
				<param name="del" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
