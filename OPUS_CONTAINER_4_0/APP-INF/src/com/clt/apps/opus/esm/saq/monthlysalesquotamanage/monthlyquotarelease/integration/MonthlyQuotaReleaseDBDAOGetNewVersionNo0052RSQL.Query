<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaReleaseDBDAOGetNewVersionNo0052RSQL">
			<desc><![CDATA[Confirmation and Distribution  distribute]]></desc>
			<sql><![CDATA[
SELECT 
		      SUBSTR(@[year], 3, 2) 
		       ||  @[quarter] 
		       || LPAD(SUBSTR(NVL(MAX(MQTA_RLSE_VER_NO), '000000'), 5, 2) + 1, 2, '0') AS NEW_VERSION_NO 
		FROM   SAQ_MON_QTA_RLSE A 
		WHERE  BSE_YR = @[year] 
		AND	  BSE_QTR_CD = @[quarter]			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="quarter" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
