<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAORemoveStwgDSQL">
			<desc><![CDATA[BKG_STWG_CGO delete]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y') 

DELETE FROM BKG_STWG_CGO_HIS
WHERE	BKG_NO = @[bkg_no]
AND	STWG_SEQ = 1
AND CORR_NO = 'TMP0000001'

#else

DELETE FROM BKG_STWG_CGO
WHERE	BKG_NO = @[bkg_no]
AND	STWG_SEQ = 1

#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
