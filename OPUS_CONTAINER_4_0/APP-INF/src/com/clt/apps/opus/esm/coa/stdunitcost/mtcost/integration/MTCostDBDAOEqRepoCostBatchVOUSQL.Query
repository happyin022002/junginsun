<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MTCostDBDAOEqRepoCostBatchVOUSQL">
			<desc><![CDATA[@20141021.SJH : COA_MTY_REPO_UT_COST의 EFF_TO_YRMON 일괄 업데이트]]></desc>
			<sql><![CDATA[
MERGE INTO COA_MTY_REPO_UT_COST A
USING(
      SELECT COST_LOC_GRP_CD, CNTR_TPSZ_CD, TRD_CD, SCC_CD, EFF_FM_YRMON, TO_YRMON
      FROM (
              SELECT COST_LOC_GRP_CD, CNTR_TPSZ_CD, TRD_CD, SCC_CD, EFF_FM_YRMON, EFF_TO_YRMON
                   , TO_CHAR(ADD_MONTHS(TO_DATE(LEAD(EFF_FM_YRMON,1) OVER (PARTITION BY COST_LOC_GRP_CD, CNTR_TPSZ_CD, TRD_CD, SCC_CD ORDER BY EFF_FM_YRMON), 'YYYYMM'),-1), 'YYYYMM') TO_YRMON
              FROM COA_MTY_REPO_UT_COST
              ORDER BY COST_LOC_GRP_CD, CNTR_TPSZ_CD, TRD_CD, SCC_CD, EFF_FM_YRMON )
      WHERE TO_YRMON IS NOT NULL
        AND EFF_TO_YRMON IS NULL
      ) B
ON (  A.COST_LOC_GRP_CD = B.COST_LOC_GRP_CD
  AND A.CNTR_TPSZ_CD = B.CNTR_TPSZ_CD
  AND A.TRD_CD = B.TRD_CD
  AND A.SCC_CD = B.SCC_CD
  AND A.EFF_FM_YRMON = B.EFF_FM_YRMON)
WHEN MATCHED THEN UPDATE 
    SET EFF_TO_YRMON = B.TO_YRMON			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
