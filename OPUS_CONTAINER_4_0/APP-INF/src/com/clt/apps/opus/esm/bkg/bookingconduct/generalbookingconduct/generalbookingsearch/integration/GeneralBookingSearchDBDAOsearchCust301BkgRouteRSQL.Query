<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchCust301BkgRouteRSQL">
			<desc><![CDATA[searchCust301BkgRoute]]></desc>
			<sql><![CDATA[
SELECT 
       '{I_BKG_ROUTE'							        ||CHR(10)||
       'ROUTE_TP_CD:'		||DECODE(DTL.PCTL_IO_BND_CD,'O','OB'||CASE WHEN (SELECT MIN(INDTL.PCTL_SEQ)
                                                                         FROM PRD_PROD_CTL_ROUT_DTL INDTL 
                                                                         WHERE INDTL.PCTL_NO = DTL.PCTL_NO
                                                                         AND INDTL.TRSP_MOD_CD <> 'X') =  DTL.PCTL_SEQ
                                                                          THEN 'DR' ELSE 'IM' END
                                                   ,'I','IB'||CASE WHEN (SELECT MAX(INDTL.PCTL_SEQ)
                                                                         FROM PRD_PROD_CTL_ROUT_DTL INDTL 
                                                                         WHERE INDTL.PCTL_NO = DTL.PCTL_NO
                                                                         AND INDTL.TRSP_MOD_CD <> 'X') =  DTL.PCTL_SEQ
                                                                          THEN 'DR' ELSE 'IM' END
                                                   ,'T',CASE WHEN (SELECT MIN(INDTL.PCTL_SEQ)
                                                                         FROM PRD_PROD_CTL_ROUT_DTL INDTL 
                                                                         WHERE INDTL.PCTL_NO = DTL.PCTL_NO
                                                                         AND INDTL.TRSP_MOD_CD = 'VD') > DTL.PCTL_SEQ
                                                                          THEN 'OBIM'
                                                             WHEN (SELECT MAX(INDTL.PCTL_SEQ)
                                                                         FROM PRD_PROD_CTL_ROUT_DTL INDTL 
                                                                         WHERE INDTL.PCTL_NO = DTL.PCTL_NO
                                                                         AND INDTL.TRSP_MOD_CD = 'VD') < DTL.PCTL_SEQ
                                                                          THEN 'IBIM'
                                                        ELSE 'TRNK' END)				||CHR(10)||  
       'ROUTE_SEQ:'		    ||RANK() OVER (ORDER BY DTL.PCTL_SEQ)				||CHR(10)||
       'ORGFAC_CD:'		    ||DTL.ORG_NOD_CD			||CHR(10)||
       'ORGFAC_NM:'		    ||NVL(MYO.YD_NM,MZO.ZN_NM)  ||CHR(10)||
       'ORGFAC_ADDR1:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYO.YD_ADDR, 1, ''), '*', '-'), ':', '-')        		||CHR(10)||									
       'ORGFAC_ADDR2:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYO.YD_ADDR, 2, ''), '*', '-'), ':', '-') 				||CHR(10)||
       'ORGFAC_ADDR3:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYO.YD_ADDR, 3, ''), '*', '-'), ':', '-') 				||CHR(10)||
       'ORGFAC_ADDR4:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYO.YD_ADDR, 4, ''), '*', '-'), ':', '-') 				||CHR(10)||
       'ORGFAC_ADDR5:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYO.YD_ADDR, 5, ''), '*', '-'), ':', '-') 				||CHR(10)||
       'ORGFAC_CITY_NM:'	||MLO.LOC_NM		        ||CHR(10)||
       'ORGFAC_STAT_CD:'	||MLO.STE_CD				||CHR(10)||
       'ORGFAC_STAT_NM:'	||(SELECT MS.STE_NM FROM MDM_STATE MS WHERE MS.STE_CD = MLO.STE_CD AND ROWNUM = 1)		||CHR(10)||
       'ORGFAC_CON_NM:'		||MCO.CNT_NM				||CHR(10)||
       'ORGFAC_UNLOC_CD:'	||MLO.UN_LOC_CD				||CHR(10)||
       'ORGFAC_ZIP_CD:'		||MYO.ZIP_CD				||CHR(10)||
       'DESFAC_CD:'		    ||DTL.DEST_NOD_CD			||CHR(10)||
       'DESFAC_NM:'		    ||NVL(MYD.YD_NM,MZD.ZN_NM)  ||CHR(10)||
       'DESFAC_ADDR1:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYD.YD_ADDR, 1, ''), '*', '-'), ':', '-')     			||CHR(10)||
       'DESFAC_ADDR2:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYD.YD_ADDR, 2, ''), '*', '-'), ':', '-')				||CHR(10)||
       'DESFAC_ADDR3:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYD.YD_ADDR, 3, ''), '*', '-'), ':', '-')				||CHR(10)||
       'DESFAC_ADDR4:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYD.YD_ADDR, 4, ''), '*', '-'), ':', '-')				||CHR(10)||
       'DESFAC_ADDR5:'		||REPLACE(REPLACE(BKG_TOKEN_NL_FNC(MYD.YD_ADDR, 5, ''), '*', '-'), ':', '-')				||CHR(10)||
       'DESFAC_CITY_NM:'	||MLD.LOC_NM		        ||CHR(10)||
       'DESFAC_STAT_CD:'	||MLD.STE_CD				||CHR(10)||
       'DESFAC_STAT_NM:'	||(SELECT MS.STE_NM FROM MDM_STATE MS WHERE MS.STE_CD = MLD.STE_CD AND ROWNUM = 1)	    ||CHR(10)||
       'DESFAC_CON_NM:'		||MCD.CNT_NM				||CHR(10)||
       'DESFAC_UNLOC_CD:'	||MLD.UN_LOC_CD		  	    ||CHR(10)||
       'DESFAC_ZIP_CD:'		||MYD.ZIP_CD				||CHR(10)||
       CASE WHEN DTL.TRSP_MOD_CD = 'VD' OR (DTL.TRSP_MOD_CD = 'WD' AND DTL.VSL_CD IS NOT NULL) THEN
       'LD_PORT_NM:'		||MLO.LOC_NM				||CHR(10)||
       'DIS_PORT_NM:'		||MLD.LOC_NM				||CHR(10)
       END ||
       'TRANS_MODE_CD:'		||DECODE(SUBSTR(DTL.TRSP_MOD_CD,1,1),'W','F',SUBSTR(DTL.TRSP_MOD_CD,1,1))			    ||CHR(10)||
       'MOTHER_VSL_IND:'	||DECODE(BK.VSL_CD||BK.SKD_VOY_NO||BK.SKD_DIR_CD,BV1.VSL_CD||BV1.SKD_VOY_NO||BV1.SKD_DIR_CD,'Y','N')				||CHR(10)||
       CASE WHEN DTL.TRSP_MOD_CD = 'VD' OR (DTL.TRSP_MOD_CD = 'WD' AND DTL.VSL_CD IS NOT NULL) THEN
       'LD_SVCLP_CD:'		||BV1.SLAN_CD				||CHR(10)||
       'LD_VSL_CD:'		    ||BV1.VSL_CD				||CHR(10)||
       'LD_VSL_NM:'		    ||MVC1.VSL_ENG_NM			||CHR(10)||
       'LD_VSL_LOYD_CD:'	||MVC1.LLOYD_NO				||CHR(10)||
       'LD_VSL_VOY_NO:'		||BV1.SKD_VOY_NO			||CHR(10)||
       'LD_VSL_DIR:'		||BV1.SKD_DIR_CD			||CHR(10)||
       'LD_VSL_CALL_SIGN:'	||MVC1.CALL_SGN_NO			||CHR(10)||
       'LD_CONSORTIUM_VOY_NO:'		||VSK_COMMON_PKG.GET_CSSM_VOY_NO_FNC(BV1.vsl_cd,BV1.skd_voy_no,BV1.skd_dir_cd,BV1.POL_CD,'O')				||CHR(10)||
       'DIS_SVCLP_CD:'		||BV1.SLAN_CD				||CHR(10)||
       'DIS_VSL_CD:'		||BV1.VSL_CD				||CHR(10)||
       'DIS_VSL_NM:'		||MVC1.VSL_ENG_NM		||CHR(10)||
       'DIS_VSL_LOYD_CD:'	||MVC1.LLOYD_NO			||CHR(10)||
       'DIS_VSL_VOY_NO:'	||BV1.SKD_VOY_NO		||CHR(10)||
       'DIS_VSL_DIR:'		||BV1.SKD_DIR_CD		||CHR(10)||
       'DIS_VSL_CALL_SIGN:'	||MVC1.CALL_SGN_NO	||CHR(10)||
       'DIS_CONSORTIUM_VOY_NO:'		||VSK_COMMON_PKG.GET_CSSM_VOY_NO_FNC(BV1.vsl_cd,BV1.skd_voy_no,BV1.skd_dir_cd,BV1.POL_CD,'O')  				||CHR(10)
        END ||
       'ORG_ETD:'		    ||TO_CHAR(DTL.ARR_ST_DT, 'RRRRMMDDHH24MI')			    ||CHR(10)||
       'ORG_ETD_GTM:'		||TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(MLO.LOC_CD,DTL.ARR_ST_DT,'GMT'), 'RRRRMMDDHH24MI')				||CHR(10)||
       CASE WHEN DTL.TRSP_MOD_CD = 'VD' OR (DTL.TRSP_MOD_CD = 'WD' AND DTL.VSL_CD IS NOT NULL) THEN
       (SELECT 'DEST_ETA:'		    ||TO_CHAR(SKD.VPS_ETA_DT, 'RRRRMMDDHH24MI')  ||CHR(10)||
               'DEST_ETA_GTM:'		||TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(SKD.VPS_PORT_CD, SKD.VPS_ETA_DT,'GMT'), 'RRRRMMDDHH24MI')				||CHR(10)
        FROM VSK_VSL_PORT_SKD SKD 
        WHERE SKD.VSL_CD = BV1.VSL_CD AND SKD.SKD_VOY_NO = BV1.SKD_VOY_NO AND SKD.SKD_DIR_CD = BV1.SKD_DIR_CD 
        AND SKD.VPS_PORT_CD = BV1.POD_CD AND SKD.CLPT_IND_SEQ = BV1.POD_CLPT_IND_SEQ AND ROWNUM = 1)
       ELSE
       'DEST_ETA:'		    ||TO_CHAR(DTL.DEP_FSH_DT, 'RRRRMMDDHH24MI')				||CHR(10)||
       'DEST_ETA_GTM:'		||TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(MLD.LOC_CD, DTL.DEP_FSH_DT,'GMT'), 'RRRRMMDDHH24MI')				||CHR(10)
       END ||
       '}I_BKG_ROUTE'  I_BKG_ROUTE
FROM BKG_BOOKING BK
     ,BKG_VVD BV1, MDM_VSL_CNTR MVC1
     ,BKG_VVD BV2, MDM_VSL_CNTR MVC2
     ,PRD_PROD_CTL_ROUT_DTL DTL
     ,MDM_YARD MYO, MDM_LOCATION MLO, MDM_COUNTRY MCO
     ,MDM_YARD MYD, MDM_LOCATION MLD, MDM_COUNTRY MCD
     ,MDM_ZONE MZO, MDM_ZONE MZD
WHERE BK.BKG_NO = @[bkg_no]
AND BK.PCTL_NO = DTL.PCTL_NO
AND DTL.ORG_NOD_CD = MYO.YD_CD(+)
AND DTL.ORG_NOD_CD = MZO.ZN_CD(+)
AND SUBSTR(DTL.ORG_NOD_CD,1,5) = MLO.LOC_CD
AND SUBSTR(DTL.ORG_NOD_CD,1,2) = MCO.CNT_CD
AND DTL.DEST_NOD_CD = MYD.YD_CD(+)
AND DTL.DEST_NOD_CD = MZD.ZN_CD(+)
AND SUBSTR(DTL.DEST_NOD_CD,1,5)= MLD.LOC_CD
AND SUBSTR(DTL.DEST_NOD_CD,1,2) = MCD.CNT_CD
AND DTL.TRSP_MOD_CD <> 'X'
AND BV1.BKG_NO(+) = @[bkg_no]
AND DTL.ORG_NOD_CD = BV1.POL_YD_CD(+)
AND DTL.VSL_CD = BV1.VSL_CD(+)
AND BV1.VSL_CD = MVC1.VSL_CD(+)
AND BV2.BKG_NO(+) = @[bkg_no]
AND DTL.DEST_NOD_CD = BV2.POL_YD_CD(+)
AND BV2.VSL_CD = MVC2.VSL_CD(+)
ORDER BY DTL.PCTL_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
