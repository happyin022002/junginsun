<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltRLaneCodeVORSQL">
			<desc><![CDATA[Revenue Lane을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT RLANE_CD AS CD , RLANE_CD AS NM  from MDM_DTL_REV_LANE
WHERE TRD_CD = @[etc1]
#if( ${etc2} != '' ) then
    AND SUB_TRD_CD = @[etc2]
#end

 			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
