<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshCustomsReportDBDAOaddLicenseInfoCSQL">
			<desc><![CDATA[Bangladesh 세관에서 사용 하는 License정보를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_BD_FRT_FWRD_LIC
			(CNT_CD, CUST_SEQ, CUST_LIC_NO, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
VALUES (SUBSTR(@[cust_cd],1,2),TO_NUMBER(SUBSTR(@[cust_cd],3)),@[cust_lic_no], @[cre_usr_id], SYSDATE, @[upd_usr_id], SYSDATE)			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="cust_lic_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
