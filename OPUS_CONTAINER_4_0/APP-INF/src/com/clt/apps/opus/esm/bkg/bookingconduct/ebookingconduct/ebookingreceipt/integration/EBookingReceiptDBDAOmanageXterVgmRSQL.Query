<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOmanageXterVgmRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
  COUNT(*) AS CNT
FROM BKG_XTER_VGM
WHERE BKG_NO  = @[bkg_no] 
AND CNTR_NO   = @[cntr_no]
AND USR_ID    = @[usr_id]
AND VGM_SEQ   = @[xter_rqst_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
