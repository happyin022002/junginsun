<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchTmnlVslCdRSQL">
			<desc><![CDATA[searchTmnlVslCd]]></desc>
			<sql><![CDATA[
SELECT 
	ATTR_CTNT2
FROM 
	BKG_CSTMS_CD_CONV_CTNT 
WHERE CNT_CD = 'KR' 
AND CSTMS_DIV_ID = 'KTML_CD' 
AND ATTR_CTNT1 = @[vsl_cd] 
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
