<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOsearchCodRqstStsRSQL">
			<desc><![CDATA[해당 cod 진행건의 cod status를 조회한다.]]></desc>
			<sql><![CDATA[
select cod_sts_cd from bkg_cod
#if (${bkg_no} !='')
	where bkg_no = @[bkg_no]
#end

#if (${cod_rqst_seq} !='')
	and cod_rqst_seq = @[cod_rqst_seq]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
