<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSearchDailyForecastSrepAccountManageListRSQL">
			<desc><![CDATA[SearchDailyForecastSrepAccountManageList]]></desc>
			<sql><![CDATA[
SELECT T.SREP_CD    ,
       T.TRD_CD     ,
       T.SUB_TRD_CD , 
       T.RLANE_CD   ,
       T.DIR_CD     ,
       T.IOC_TS_CD  ,
       T.CUST_CNT_CD, 
       T.CUST_SEQ   ,
       C.CUST_LGL_ENG_NM AS CUST_NM,
       T.FCAST_CUST_TP_CD,
       T.FCAST_SEQ,
       T.CTRT_CUST_CNT_CD, 
       T.CTRT_CUST_SEQ,
       (SELECT CUST_LGL_ENG_NM 
		  FROM MDM_CUSTOMER MC 
		 WHERE MC.CUST_CNT_CD = T.CTRT_CUST_CNT_CD 
		   AND MC.CUST_SEQ = T.CTRT_CUST_SEQ) AS CTRT_CUST_NM
  FROM SPC_SLS_REP_CUST_MAPG T,
       MDM_CUSTOMER          C
 WHERE C.CUST_CNT_CD(+) = T.CUST_CNT_CD
   AND C.CUST_SEQ   (+) = T.CUST_SEQ
   AND T.SREP_CD        = @[salesRep]
   AND T.TRD_CD         = @[trade]
   AND T.SUB_TRD_CD     = @[subTrade]
   AND T.RLANE_CD       = @[lane]
   AND T.DIR_CD         = @[bound] 
   AND T.IOC_TS_CD      = @[ioc]

   #if (${fcastCustTpCd} != 'A') 
   AND T.FCAST_CUST_TP_CD = @[fcastCustTpCd]
   #end			]]></sql>
			<params>
				<param name="salesRep" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="subTrade" type="12" value="" out="N"/>
				<param name="lane" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="ioc" type="12" value="" out="N"/>
				<param name="fcastCustTpCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
