<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyCustomizedConditionDBDAOSearchLaneBoundPreviousCheckKey0163Tab02RSQL">
			<desc><![CDATA[Year, Quarter 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT BSE_YR, BSE_QTR_CD  					       
    FROM SAQ_MON_DIR_CONV                              
   WHERE ROWNUM = 1                                    
     AND BSE_YR||BSE_QTR_CD < @[bse_yr]||@[bse_qtr_cd] 
ORDER BY BSE_YR DESC, BSE_QTR_CD DESC			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
