<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommAuditDBDAOSearchAGNCommAuditConfirmNoRSQL">
			<desc><![CDATA[SearchAGNCommAuditConfirmNo]]></desc>
			<sql><![CDATA[
SELECT
    AUD_NO AS AUD_NO
FROM
(
    SELECT 
        1 AS NO,
        SUBSTRB (B.R_AR_OFC_CD, 1, 3)
        || TO_CHAR (SYSDATE, 'YYMMDD')
        || TRIM (TO_CHAR (SUBSTR (MAX (B.AUD_NO), 10, 2) + 1, '00'))
        || 'T' AS AUD_NO
     FROM (        
        
        SELECT AUD_NO, R_AR_OFC_CD
        FROM (
            SELECT AUD_NO, SUBSTRB (AR_OFC_CD,1,3) AS R_AR_OFC_CD
            FROM ACM_AGN_COMM AGN
            WHERE 1=1
            AND AGN.AGN_CD in (select agn_cd from acm_ofc_info where SUBSTR(ar_ofc_cd,1,3) = SUBSTR(@[ar_ofc_cd],1,3) GROUP BY agn_cd )
            AND AGN.AUD_DT  >= TRUNC(SYSDATE)
            AND AGN.AC_STS_CD
            IN
                (
                'AS', 'PS', 'IF'
                )
            UNION ALL
            SELECT AUD_NO, SUBSTRB (AR_OFC_CD,1,3) AS R_AR_OFC_CD
            FROM ACM_AGN_OTR_COMM OTR
            WHERE 1=1
            AND OTR.AGN_CD in (select agn_cd from acm_ofc_info where SUBSTR(ar_ofc_cd,1,3) = SUBSTR(@[ar_ofc_cd],1,3) GROUP BY agn_cd )
            AND OTR.AUD_DT  >= TRUNC(SYSDATE)
            AND OTR.AC_STS_CD
            IN
                (
                'AS', 'PS', 'IF'
                )
        )
    )B
    GROUP BY R_AR_OFC_CD
    UNION ALL
    SELECT
        2 AS No,
        SUBSTR (@[ar_ofc_cd], 1, 3)
        || TO_CHAR (SYSDATE,'YYMMDD')
        || '01T' AS AUD_NO     
    FROM DUAL
    ORDER BY NO
)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
