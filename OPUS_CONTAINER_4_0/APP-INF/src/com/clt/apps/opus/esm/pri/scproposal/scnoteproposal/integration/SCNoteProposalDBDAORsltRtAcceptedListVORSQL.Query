<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteProposalDBDAORsltRtAcceptedListVORSQL">
			<desc><![CDATA[SCNoteProposalDBDAORsltRtAcceptedListVORSQL]]></desc>
			<sql><![CDATA[
SELECT
    SUBSTR(NOTE.NOTE_TIT_NM,1,15)||'..' NOTE_SEQ, 
    SRC.NOTE_CTNT NOTE_CTNT
FROM
    PRI_SP_SCP_NOTE NOTE,
    (
        SELECT
			NOTE_SEQ , SUBSTR(NOTE_CTNT,1,15)||'..' NOTE_CTNT, DP_SEQ
        FROM
	        PRI_SP_SCP_NOTE_CTNT DTL
        WHERE
	        PROP_NO 			= @[prop_no]
        AND AMDT_SEQ 			= @[amdt_seq]
        AND SVC_SCP_CD 			= @[svc_scp_cd]
        AND NOTE_TP_CD      = 'P'
        AND PRC_PROG_STS_CD     = 'A'
        AND N1ST_CMNC_AMDT_SEQ  = @[amdt_seq]

    ) SRC
WHERE
    NOTE.PROP_NO 		= @[prop_no]
AND NOTE.AMDT_SEQ 		= @[amdt_seq]
AND NOTE.SVC_SCP_CD 	= @[svc_scp_cd]
AND NOTE.NOTE_TP_CD     = 'P'
AND NOTE.NOTE_SEQ = SRC.NOTE_SEQ
AND NOTE.NOTE_SEQ IN (
    #foreach($key IN ${txtArr}) 
    #if($velocityCount < $txtArr.size()) 
    '$key', 
    #else 
    '$key' 
    #end 
    #end 
)
ORDER BY 
    NOTE.DP_SEQ, 
    SRC.DP_SEQ			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
