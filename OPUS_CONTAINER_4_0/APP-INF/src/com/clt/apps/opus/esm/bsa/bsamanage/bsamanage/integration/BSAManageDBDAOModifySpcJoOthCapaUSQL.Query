<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOModifySpcJoOthCapaUSQL">
			<desc><![CDATA[ModifySpcJoOthCapa]]></desc>
			<sql><![CDATA[
UPDATE BSA_JNT_OP_CRR_CAPA A
    SET (A.CRR_BSA_CAPA, A.SPC_CTRL_SLT_CAPA) = (
                                                 SELECT CRR_BSA_CAPA, 
                                                        SPC_CTRL_SLT_CAPA
                                                 FROM BSA_JNT_OP_CRR_CAPA B
                                                 WHERE 1 = 1
                                                   AND A.RLANE_CD     = B.RLANE_CD
                                                   AND A.DIR_CD       = B.DIR_CD
                                                   AND A.TRD_CD       = B.TRD_CD
                                                   AND A.VOP_CD       = B.VOP_CD
                                                   AND A.VSL_CAPA     = B.VSL_CAPA
                                                   AND A.BSA_OP_CD    = B.BSA_OP_CD
                                                   AND A.BSA_OP_JB_CD = B.BSA_OP_JB_CD
                                                   AND A.CRR_CD       = B.CRR_CD
                                                   AND B.BSA_SEQ      = @[bsa_seq] -1
                                                )
 where A.BSA_SEQ      = @[bsa_seq]
   AND A.RLANE_CD     = @[rlane_cd]
   AND A.DIR_CD       = @[dir_cd]
   AND A.TRD_CD       = @[trd_cd]
   AND A.VOP_CD       = @[vop_cd]
   AND A.VSL_CAPA     = @[vsl_capa]
   AND A.BSA_OP_CD    = 'J'
   AND A.BSA_OP_JB_CD IN ('008', '010', '011','012','013','014','022')			]]></sql>
			<params>
				<param name="bsa_seq" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
				<param name="trd_cd" type="12" value="" out="Y"/>
				<param name="vop_cd" type="12" value="" out="Y"/>
				<param name="vsl_capa" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
