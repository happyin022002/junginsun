<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOModifyStwgUSQL">
			<desc><![CDATA[BKG_BOOKING stwg_cd, stwg_rmk update]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y') 

UPDATE BKG_STWG_CGO_HIS SET 
	STWG_CD = @[stwg_cd]
,	STWG_RMK = @[stwg_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no]
AND	STWG_SEQ = 1
AND CORR_NO = 'TMP0000001'

#else

UPDATE BKG_STWG_CGO SET 
	STWG_CD = @[stwg_cd]
,	STWG_RMK = @[stwg_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no]
AND	STWG_SEQ = 1

#end			]]></sql>
			<params>
				<param name="stwg_cd" type="12" value="" out="N"/>
				<param name="stwg_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
