<sqls>
	<querys>
		<query name="FFCommCalculationDBDAORemoveFFMasterDSQL">
			<desc><![CDATA[FFCommCalculationDBDAORemoveFFMasterDSQL]]></desc>
			<sql><![CDATA[
DELETE FROM ACM_FF_CMPN
 WHERE  BKG_NO = @[bkg_no]
   AND FF_CMPN_SEQ = @[ff_cmpn_seq]
   AND FF_CMPN_STS_CD <> 'IF'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ff_cmpn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
