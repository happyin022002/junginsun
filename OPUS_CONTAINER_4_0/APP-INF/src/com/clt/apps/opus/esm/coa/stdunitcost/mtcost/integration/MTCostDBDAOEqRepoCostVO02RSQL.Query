<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MTCostDBDAOEqRepoCostVO02RSQL">
			<desc><![CDATA[@SJH.20141001 : COA_FULL_SCC_IMBAL 조회
@SJH.20141106 : 필드, 조건절, 변수명 수정]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD
      ,SCC_CD
      ,COST_SRC_FM_YRMON
      ,COST_SRC_TO_YRMON
      ,ORG_NOD_CD
      ,DEST_NOD_CD
      ,TRSP_MTY_COST_MOD_NM			--SJH.20141127.MOD
      ,MTY_QTY
      ,MTY_UT_AMT
  FROM COA_MTY_COST_HIS
 WHERE 1=1 
   --SJH.20141106.MOD
   AND CNTR_TPSZ_CD       	= @[f_cntr_tpsz_cd]
   AND SCC_CD             	= @[f_scc_cd]
   AND COST_SRC_FM_YRMON  	= @[f_cost_src_fm_yrmon]
   AND COST_SRC_TO_YRMON  	= @[f_cost_src_to_yrmon]
ORDER BY CNTR_TPSZ_CD, SCC_CD, COST_SRC_FM_YRMON, COST_SRC_TO_YRMON, ORG_NOD_CD, DEST_NOD_CD, TRSP_MTY_COST_MOD_NM			]]></sql>
			<params>
				<param name="f_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="f_scc_cd" type="12" value="" out="N"/>
				<param name="f_cost_src_fm_yrmon" type="12" value="" out="N"/>
				<param name="f_cost_src_to_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
