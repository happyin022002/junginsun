<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesActivityManageDBDAOsearchSrepsManageCustListRSQL">
			<desc><![CDATA[Sales Rep에 맞는 Customer Code 조회]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD||CUST_SEQ CUS_CODE
FROM SAM_CUST_SLS_REP_INFO 
WHERE CUST_CNT_CD = SUBSTR(@[cus_code],1,2)
AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cus_code],3,8))
AND SREP_CD = @[sls_code]
AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="cus_code" type="12" value="" out="N"/>
				<param name="sls_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
