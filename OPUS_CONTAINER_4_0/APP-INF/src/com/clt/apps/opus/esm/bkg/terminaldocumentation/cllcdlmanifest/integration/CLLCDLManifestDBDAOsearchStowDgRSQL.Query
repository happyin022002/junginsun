<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchStowDgRSQL">
			<desc><![CDATA[searchStowDg]]></desc>
			<sql><![CDATA[
SELECT
    'X' STWG_EXIST
, 	STWG_CD
FROM
    BKG_BOOKING
WHERE BKG_NO = @[bkg_no]
AND DCGO_FLG = 'N'
AND STWG_CD IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
