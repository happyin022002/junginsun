<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchBkgCustRSQL">
			<desc><![CDATA[searchBkgCust]]></desc>
			<sql><![CDATA[
SELECT 
	A.BL_NO,  
	'  ' BL_NO_SPLIT,
	B.BKG_CUST_TP_CD, 
	B.CUST_SEQ,  
	B.CUST_CNT_CD,
	REPLACE(REPLACE(NVL(UPPER(B.CUST_NM),' '),CHR(180),CHR(39)),'’','''') CUST_NM,
	REPLACE(REPLACE(NVL(UPPER(B.CUST_ADDR),' '),CHR(180),CHR(39)),'’','''') CUST_ADDR
FROM   
	BKG_BOOKING A, 
	BKG_CUSTOMER B
WHERE  A.BKG_NO       = @[bkg_no]
AND  A.BKG_NO       = B.BKG_NO
AND  (B.BKG_CUST_TP_CD      = 'S'
OR    B.BKG_CUST_TP_CD      = 'C'
OR    B.BKG_CUST_TP_CD      = 'N'  )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
