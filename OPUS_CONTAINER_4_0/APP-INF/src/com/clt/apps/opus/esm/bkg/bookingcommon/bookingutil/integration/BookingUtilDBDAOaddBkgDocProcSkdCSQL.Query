<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOaddBkgDocProcSkdCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_DOC_PROC_SKD A USING(
    SELECT @[bkg_no] BKG_NO, @[bkg_doc_proc_tp_cd] BKG_DOC_PROC_TP_CD FROM DUAL
) B     
ON (A.BKG_NO = B.BKG_NO AND A.BKG_DOC_PROC_TP_CD = B.BKG_DOC_PROC_TP_CD)
WHEN MATCHED THEN
	UPDATE SET DOC_PERF_DELT_FLG = @[doc_perf_delt_flg]
WHEN NOT MATCHED THEN
INSERT (
	BKG_NO,
	BKG_DOC_PROC_TP_CD,
	DOC_PROC_SEQ,
	DOC_PERF_DELT_FLG,
	CRE_USR_ID,
	EVNT_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT,
	DIFF_RMK
) VALUES (
	@[bkg_no],
	@[bkg_doc_proc_tp_cd],
	@[doc_proc_seq],
	@[doc_perf_delt_flg],
	@[cre_usr_id],
	@[cre_usr_id],
	SYSDATE,
	@[cre_usr_id],
	SYSDATE,
	'E-BOOKING'
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_doc_proc_tp_cd" type="12" value="" out="N"/>
				<param name="doc_perf_delt_flg" type="12" value="" out="N"/>
				<param name="doc_proc_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
