<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOremoveBkgCodDSQL">
			<desc><![CDATA[bkg_Cod를 delete한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_COD
WHERE	BKG_NO = @[bkg_no]
AND	COD_RQST_SEQ = @[cod_rqst_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
