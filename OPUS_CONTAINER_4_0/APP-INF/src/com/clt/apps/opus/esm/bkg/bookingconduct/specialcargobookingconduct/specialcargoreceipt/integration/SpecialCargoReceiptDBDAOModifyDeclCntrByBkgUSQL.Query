<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOModifyDeclCntrByBkgUSQL">
			<desc><![CDATA[Container 정보를 BKG_DG_DECL에 UPDATE한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_DG_DECL
    SET CNTR_NO = @[cntr_no]
       ,DG_CNTR_SEQ = @[dg_cntr_seq]
WHERE BKG_NO = @[bkg_no]
AND DG_CNTR_SEQ = @[dg_cntr_seq_original]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="dg_cntr_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dg_cntr_seq_original" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
