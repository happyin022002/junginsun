<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOPndlmSvcDtlRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT YD_CD 
	#foreach(${keys} IN ${header})
	  ,SUM(DECODE(RLANE_CD,'$keys',NVL(PNDLM_RTO,0),0)) AS $keys
	#end
    FROM COA_PNDLM_SVC_DTL
   WHERE 1=1
     AND SLAN_CD   = @[f_selslane]
     AND DIR_CD    = @[f_seldir]
     AND EFF_FM_DT = @[f_from]
   GROUP BY YD_CD
   ORDER BY YD_CD			]]></sql>
			<params>
				<param name="f_selslane" type="12" value="" out="N"/>
				<param name="f_seldir" type="12" value="" out="N"/>
				<param name="f_from" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
