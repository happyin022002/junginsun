<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchAproNoFromWhRoutRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT APRO_NO,
       @[del_bnd] AS CSTMS_CD,
       (SELECT WH_NM
          FROM BKG_CSTMS_JP_WH
         WHERE CSTMS_CD = @[del_bnd]
           AND ROWNUM = 1) AS WH_NM,
       TRSP_MOD_CD

  FROM BKG_CSTMS_JP_WH_ROUT

 WHERE CSTMS_CD = @[pod_bnd]
   AND (CSTMS_CD1 = @[del_bnd] OR
        CSTMS_CD2 = @[del_bnd] OR
        CSTMS_CD3 = @[del_bnd] OR
        CSTMS_CD4 = @[del_bnd] OR
        CSTMS_CD5 = @[del_bnd])
			]]></sql>
			<params>
				<param name="del_bnd" type="12" value="" out="N"/>
				<param name="pod_bnd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
