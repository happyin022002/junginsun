<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaCustomsReportDBDAOsearchDelModeRSQL">
			<desc><![CDATA[중국 DEL 별 운송 Mode 조회.]]></desc>
			<sql><![CDATA[
SELECT POD_CD
      , AREA_NM
      , DEL_CD
      , CHN_CSTMS_TRSP_MOD_CD AS TRSP_MOD_CD
      , DIFF_RMK
FROM BKG_CSTMS_CHN_DE_MOD
WHERE POD_CD  LIKE    @[pod_cd]||'%'
AND   DEL_CD  LIKE	  @[del_cd]||'%'			]]></sql>
			<params>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
