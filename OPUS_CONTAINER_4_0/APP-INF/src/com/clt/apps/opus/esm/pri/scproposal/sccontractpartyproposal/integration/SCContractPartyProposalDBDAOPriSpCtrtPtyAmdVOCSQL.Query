<sqls>
	<querys>
		<query name="SCContractPartyProposalDBDAOPriSpCtrtPtyAmdVOCSQL">
			<desc><![CDATA[SCContractPartyProposalDB PriSpCtrtCustTpAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_CTRT_PTY(
    PROP_NO,
    AMDT_SEQ,
    PRC_CTRT_PTY_TP_CD,
    CUST_CNT_CD,
    CUST_SEQ,
    CTRT_CUST_VAL_SGM_CD,
    CTRT_CUST_SREP_CD,
    CTRT_CUST_SLS_OFC_CD,
    CTRT_PTY_NM,
    CTRT_PTY_ADDR,
    CTRT_PTY_SGN_NM,
    CTRT_PTY_SGN_TIT_NM,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    PROP_NO					,
    AMDT_SEQ+1				,
    PRC_CTRT_PTY_TP_CD		,
    CUST_CNT_CD				,
    CUST_SEQ				,
    CTRT_CUST_VAL_SGM_CD    ,
    CTRT_CUST_SREP_CD        ,
    CTRT_CUST_SLS_OFC_CD    ,
    CTRT_PTY_NM                ,
    CTRT_PTY_ADDR            ,    
    CTRT_PTY_SGN_NM            ,
    CTRT_PTY_SGN_TIT_NM        ,
    PRC_PROG_STS_CD            ,
    SRC_INFO_CD                ,
    ACPT_USR_ID                ,
    ACPT_OFC_CD                ,
    ACPT_DT                    ,
    @[cre_usr_id]            ,
    sysdate                    ,
    @[upd_usr_id]            ,
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ
FROM
    PRI_SP_CTRT_PTY
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
