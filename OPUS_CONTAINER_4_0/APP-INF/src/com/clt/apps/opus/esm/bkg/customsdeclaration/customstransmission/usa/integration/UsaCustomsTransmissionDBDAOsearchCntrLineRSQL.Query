<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCntrLineRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
       B.BL_NO,
       RPAD(
       'C01'||
       RPAD(C.CNTR_NO, 14, ' ')||
       RPAD(NVL(DECODE(C.CNTR_NO, S.CNTR_NO, S.SEAL_NO1), '999'), 15, ' ')||
	   RPAD(NVL(DECODE(C.CNTR_NO, S.CNTR_NO, S.SEAL_NO2), ' '), 15, ' ')||
	 ( SELECT DECODE(MAX(ATTR_CTNT2),'','  ',MAX(ATTR_CTNT2)) AS ATTR_CTNT2 FROM BKG_CSTMS_cD_cONV_CTNT
		WHERE CNT_CD = 'US'
		AND CSTMS_DIV_ID = 'ACE_CNTR_EQ_DESC_CD'
		AND ATTR_CTNT1 = C.CNTR_TPSZ_CD )||
	 ( SELECT LPAD(NVL(MAX(ATTR_CTNT2),'     '),5) AS ATTR_CTNT2 FROM BKG_CSTMS_CD_CONV_CTNT
		WHERE CNT_CD='US'
		AND CSTMS_DIV_ID='CNTR_SPEC_INFO'
		AND ATTR_CTNT1 = C.CNTR_TPSZ_CD)||
     ( SELECT LPAD(NVL(MAX(ATTR_CTNT3),'        '),8) AS ATTR_CTNT3 FROM BKG_CSTMS_CD_CONV_CTNT
        WHERE CNT_CD='US'
        AND CSTMS_DIV_ID='CNTR_SPEC_INFO'
        AND ATTR_CTNT1 = C.CNTR_TPSZ_CD)||
--     ( SELECT LPAD(NVL(MAX(ATTR_CTNT4),'        '),8) AS ATTR_CTNT4 FROM BKG_CSTMS_CD_CONV_CTNT
--        WHERE CNT_CD='US'
--        AND CSTMS_DIV_ID='CNTR_SPEC_INFO'
--        AND ATTR_CTNT1 = C.CNTR_TPSZ_CD)||
     '00000806' ||
     ( SELECT LPAD(NVL(MAX(ATTR_CTNT5),'    '),4) AS ATTR_CTNT5 FROM BKG_CSTMS_CD_CONV_CTNT
		WHERE CNT_CD='US'
		AND CSTMS_DIV_ID='CNTR_SPEC_INFO'
		AND ATTR_CTNT1 = C.CNTR_TPSZ_CD )||
       DECODE(B.FULL_MTY_CD, 'M', 'E', 'L')||
       DECODE(K.BB_CGO_FLG,
	      'Y', 'BB',
	      DECODE(NVL(B.RCV_TERM_CD,' ')||NVL(B.DE_TERM_CD,' '),
		     'YY','CY',
		     'YD','PH',
		     'DY','HP',
		     'DD','HH',
		     'SY','CS',
		     'YS','CS',
		     'SD','CS',
		     'DS','CS',
		     'MM','MD',
		     'TT','PP',
		     'HH','HL',
		     '  '))||
       '   ',80,' ')||CHR(10) BUF3,
       C.CNTR_NO CNTR_NO
  FROM 
	(
	SELECT A.BL_NO, A.CNTR_NO, 
		MAX(DECODE(A.RANK_SEAL, 1, BKG_SPCLCHAR_CONV_FNC(SEAL_NO ,'X'))) SEAL_NO1,
		MAX(DECODE(A.RANK_SEAL, 2, BKG_SPCLCHAR_CONV_FNC(SEAL_NO ,'X'))) SEAL_NO2
	FROM(
		SELECT A.BL_NO, CT.CNTR_NO, S.SEAL_NO,
    RANK() OVER(PARTITION BY A.BL_NO, CT.CNTR_NO ORDER BY A.BL_NO, S.CNTR_NO, S.SEAL_NO DESC) RANK_SEAL
		FROM (SELECT COLUMN_VALUE BL_NO FROM TABLE(BKG_SPLIT_CLOB_FNC(${bl_no},',')) WHERE COLUMN_VALUE IS NOT NULL) A,
    BKG_CSTMS_SEAL_NO S,
			BKG_CSTMS_ADV_CNTR CT
		WHERE A.BL_NO = CT.BL_NO (+)
			AND CT.CNT_CD(+) = 'US'
			AND CT.BL_NO = S.BL_NO(+)
			AND CT.CNTR_NO = S.CNTR_NO(+) 
			AND S.CNT_CD(+) = 'US'
			AND S.CSTMS_DIV_ID(+)= 'CTM'
		) A
	GROUP BY A.BL_NO, A.CNTR_NO
	) S,
	BKG_CSTMS_ADV_BL B, BKG_CSTMS_ADV_CNTR C, BKG_BOOKING K
 WHERE 
	B.CNT_CD = 'US'
	AND B.BL_NO 	= S.BL_NO
	AND C.CNTR_NO	= S.CNTR_NO
	AND B.CNT_CD    = C.CNT_CD
	AND B.BL_NO     = C.BL_NO
	AND B.BL_NO     = K.BL_NO (+)
	AND C.IBD_CNTR_STS_CD  = 'A'
			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
