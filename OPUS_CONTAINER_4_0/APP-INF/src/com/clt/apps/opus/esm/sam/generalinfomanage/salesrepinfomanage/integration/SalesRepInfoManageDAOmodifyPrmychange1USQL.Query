<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesRepInfoManageDAOmodifyPrmychange1USQL">
			<desc><![CDATA[원래 관리자 S.Rep.code의 'Y' 값을 N으로 변경]]></desc>
			<sql><![CDATA[
UPDATE SAM_CUST_SLS_REP_INFO
SET SREP_PRMRY_FLG = 'N'
WHERE CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]
AND SREP_PRMRY_FLG = 'Y'
AND UPD_USR_ID = @[usr_id],
AND UPD_DT = SYSDATE			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
