<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchMonthlyQuotaAdjustmentVVD0116ListRSQL">
			<desc><![CDATA[Monthly Quota VVD]]></desc>
			<sql><![CDATA[
SELECT BSE_MON, BSE_WK,						
		       VSL_CD||SKD_VOY_NO||SKD_DIR_CD AS VVD,
		       FNL_BSA_CAPA AS BSA                   
     #if (${tbl_gbn} == 'CONFIRMED') 
           FROM   SAQ_MON_CFM_TGT_VVD        
     #else 
           FROM   SAQ_MON_TGT_VVD_ADJ    
     #end
		WHERE  BSE_YR = @[bse_yr]                            
		AND    BSE_QTR_CD = @[bse_qtr_cd]
	 #if (${gline_ver_no} != '') 
        AND    GLINE_VER_NO = @[gline_ver_no]      
     #end
		AND    TRD_CD = @[trd_cd]                            
		AND    DIR_CD = @[dir_cd]                            
		AND    RLANE_CD = @[rlane_cd]                          
		AND    SPRT_GRP_CD = @[sprt_grp_cd]                       
		AND    BSA_GRP_CD = @[bsa_grp_cd]    
     #if (${bse_mon} != '') 
        AND    BSE_MON = @[bse_mon]       
     #end
		ORDER BY BSE_MON, BSE_WK, VVD			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="gline_ver_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="sprt_grp_cd" type="12" value="" out="N"/>
				<param name="bsa_grp_cd" type="12" value="" out="N"/>
				<param name="bse_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
