<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchFACCancelAmtRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchFACCancelAmtRSQL]]></desc>
			<sql><![CDATA[
SELECT ROUND (NVL (SUM (IF_AMT),0), 2) CANCEL_AMT,
       NVL (MAX (FAC_SEQ), 0) + 1 AS FAC_SEQ
  FROM ACM_FAC_COMM
 WHERE BKG_NO = @[bkg_no]
	AND FAC_STS_CD = 'CM'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
