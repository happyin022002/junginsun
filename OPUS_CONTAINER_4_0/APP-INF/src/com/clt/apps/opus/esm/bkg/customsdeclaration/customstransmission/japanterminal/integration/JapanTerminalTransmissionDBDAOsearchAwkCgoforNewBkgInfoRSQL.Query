<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchAwkCgoforNewBkgInfoRSQL">
			<desc><![CDATA[searchAwkCgoforNewBkgInfo]]></desc>
			<sql><![CDATA[
SELECT AC.BKG_NO,
       @[bkg_skd_seq] AS BKG_SKD_SEQ,
       AC.AWK_CGO_SEQ,
       AC.CNTR_TPSZ_CD,
       AC.CNTR_VOL_QTY,
       AC.CNTR_NO,
       AC.OVR_HGT,
       AC.OVR_LF_LEN,
       AC.OVR_RT_LEN,
       AC.OVR_FWRD_LEN,
       AC.OVR_BKWD_LEN,
       AC.OVR_VOID_SLT_QTY

  FROM BKG_AWK_CGO AC,
       BKG_VVD BV

 WHERE AC.BKG_NO LIKE SUBSTR(@[bkg_no], 1, 10)||'%' 
   AND BV.VSL_Cd = @[vsl_cd]
   AND BV.SKD_VOY_NO = @[skd_voy_no]
   AND BV.SKD_DIR_CD = @[skd_dir_cd]
   AND BV.POL_CD = @[pol_cd]
   AND AC.BKG_NO = BV.BKG_NO			]]></sql>
			<params>
				<param name="bkg_skd_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
