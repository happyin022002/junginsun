<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAORemoveAcmSpclCmpnDSQL">
			<desc><![CDATA[RemoveAcmSpclCmpn]]></desc>
			<sql><![CDATA[
DELETE ACM_SPCL_CMPN 
WHERE BKG_NO         = @[bkg_no]
AND SPCL_CMPN_STS_CD NOT IN ('RS','AS','PS','IF')
AND NVL(PAY_CHK_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
