<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOUpdatemanagePotnAdjustmentUSQL">
			<desc><![CDATA[[Portion Adjustment by Head Office]을 [저장] 합니다.]]></desc>
			<sql><![CDATA[
UPDATE CSQ_CFM_QTA_POTN_MGMT 
   SET LOD_POTN_RTO = @[lod_potn_rto] 
      ,REV_POTN_RTO = @[rev_potn_rto]
      ,APLY_FM_YRWK = @[bse_yr]||@[aply_fm_yrwk] 
      ,APLY_TO_YRWK = @[bse_yr]||@[aply_to_yrwk] 
      ,FM_VVD_CD    = @[fm_vvd_cd] 
      ,TO_VVD_CD    = @[to_vvd_cd] 
      ,UPD_USR_ID   = @[upd_usr_id]
      ,UPD_DT       = SYSDATE 
WHERE BSE_TP_CD     = @[bse_tp_cd]
  AND BSE_YR        = @[bse_yr]
  AND BSE_QTR_CD    = DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
  AND OFC_VW_CD     = @[ofc_vw_cd]
  AND QTA_STEP_CD   = @[qta_step_cd]
  AND RHQ_CD        = @[rhq_cd]
  AND RGN_OFC_CD    = DECODE(@[qta_step_cd],'02',@[rhq_cd],@[rgn_ofc_cd])
  AND TRD_CD        = @[trd_cd]
  AND DIR_CD        = @[dir_cd]
  AND RLANE_CD      = @[rlane_cd]
  AND APLY_FM_YRWK  = @[bse_yr]||@[org_fm_yrwk]
  AND APLY_TO_YRWK  = @[bse_yr]||@[org_to_yrwk]			]]></sql>
			<params>
				<param name="lod_potn_rto" type="12" value="" out="N"/>
				<param name="rev_potn_rto" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="aply_fm_yrwk" type="12" value="" out="N"/>
				<param name="aply_to_yrwk" type="12" value="" out="N"/>
				<param name="fm_vvd_cd" type="12" value="" out="N"/>
				<param name="to_vvd_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="ofc_vw_cd" type="12" value="" out="N"/>
				<param name="qta_step_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="org_fm_yrwk" type="12" value="" out="N"/>
				<param name="org_to_yrwk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
