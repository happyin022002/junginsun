<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCommonDBDAOGetAgnByRhqListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT AGN_CD AS VALUE0
  FROM (SELECT AGN_CD
          FROM ACM_OFC_INFO
         WHERE RHQ_CD = @[value0]
         GROUP BY AGN_CD
        UNION
        SELECT AGN_CD
          FROM ACM_OFC_INFO
         WHERE DP_GRP_NM IN (SELECT DP_GRP_NM
                               FROM ACM_OFC_INFO
                              WHERE RHQ_CD = @[value0]
                                AND DP_GRP_NM IS NOT NULL)
         GROUP BY AGN_CD)
 ORDER BY AGN_CD			]]></sql>
			<params>
				<param name="value0" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
