<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchFrtTermRSQL">
			<desc><![CDATA[SearchFrtTerm]]></desc>
			<sql><![CDATA[
SELECT FRT_TERM_CD
FROM BKG_RATE
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
