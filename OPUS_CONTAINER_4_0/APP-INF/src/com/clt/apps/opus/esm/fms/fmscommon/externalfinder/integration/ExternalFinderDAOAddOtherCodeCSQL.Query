<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDAOAddOtherCodeCSQL">
			<desc><![CDATA[OtherCode를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO FMS_PPT_SET  (
	PPT_NM,
	PPT_SEQ,
	PPT_CTNT,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT

) VALUES (
	@[ppt_nm],
	@[ppt_seq],
	@[ppt_ctnt],
	@[cre_usr_id],
	sysdate,
	@[cre_usr_id],
	sysdate

)			]]></sql>
			<params>
				<param name="ppt_nm" type="12" value="" out="N"/>
				<param name="ppt_seq" type="12" value="" out="N"/>
				<param name="ppt_ctnt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
