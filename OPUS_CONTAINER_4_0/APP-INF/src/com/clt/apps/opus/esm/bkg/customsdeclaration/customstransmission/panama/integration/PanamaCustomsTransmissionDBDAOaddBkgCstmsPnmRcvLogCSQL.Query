      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      )
VALUES (TO_DATE(@[rcv_dt], 'yyyymmddhh24miss')
      ,@[rcv_log_seq]
      ,@[vst_no]
      ,@[cstms_ack_cd]
      ,@[edi_rcv_msg_ctnt]
      ,@[crr_bat_no]
      ,'RCV'
      ,SYSDATE
      ,'RCV'
      ,SYSDATE
      )			]]></sql>
