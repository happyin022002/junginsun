				<param name="rcv_log_seq" type="12" value="" out="N"/>
				<param name="vst_no" type="12" value="" out="N"/>
				<param name="cstms_ack_cd" type="12" value="" out="N"/>
				<param name="edi_rcv_msg_ctnt" type="12" value="" out="N"/>
				<param name="crr_bat_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
