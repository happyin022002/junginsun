<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchIsoGroupTpCdRSQL">
			<desc><![CDATA[ISO Group type 코드 가져오는 쿼리]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_DP_DESC GRP_CD
FROM COM_INTG_CD_DTL
WHERE INTG_CD_ID = 'CD03035'
  AND SUBSTR(INTG_CD_VAL_CTNT,0,1) = SUBSTR(@[iso_cd],0,1)			]]></sql>
			<params>
				<param name="iso_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
