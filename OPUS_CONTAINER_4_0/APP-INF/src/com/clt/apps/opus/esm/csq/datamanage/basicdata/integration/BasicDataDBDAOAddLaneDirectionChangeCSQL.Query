<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOAddLaneDirectionChangeCSQL">
			<desc><![CDATA[Reverse Sailing 노선들의 변경된 Direction을 추가]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_DIR_CONV (  
             BSE_TP_CD
            ,BSE_YR
            ,BSE_QTR_CD
            ,TRD_CD
            ,RLANE_CD
            ,DIR_CD
            ,CONV_DIR_CD   
            ,CRE_USR_ID          
            ,CRE_DT              
            ,UPD_USR_ID         
            ,UPD_DT           )  
 VALUES (                       
             @[bse_tp_cd]
            ,@[bse_yr]
            ,DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
            ,@[trd_cd]
            ,@[rlane_cd]
            ,@[dir_cd]
            ,@[conv_dir_cd]                  
            ,@[cre_usr_id]                   
            ,SYSDATE             
            ,@[cre_usr_id]                 
            ,SYSDATE )			]]></sql>
			<params>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="conv_dir_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
