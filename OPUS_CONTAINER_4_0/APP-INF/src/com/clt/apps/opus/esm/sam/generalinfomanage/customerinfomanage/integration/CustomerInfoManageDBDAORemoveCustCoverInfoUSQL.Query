<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAORemoveCustCoverInfoUSQL">
			<desc><![CDATA[RemoveCustCoverInfo]]></desc>
			<sql><![CDATA[
UPDATE SAM_CUST_SLS_REP_INFO
SET SREP_PRMRY_FLG = 'N'
  , UPD_USR_ID = @[user_id]
  , UPD_DT = SYSDATE
  , DELT_FLG = 'Y'
WHERE CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = TO_NUMBER(@[cust_seq])
AND SREP_CD = @[srep_cd]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
