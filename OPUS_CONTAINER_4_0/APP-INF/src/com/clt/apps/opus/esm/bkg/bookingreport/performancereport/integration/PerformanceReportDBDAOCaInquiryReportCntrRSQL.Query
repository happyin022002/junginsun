<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOCaInquiryReportCntrRSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
SELECT CNT.CNTR_NO
      ,CNT.CNTR_TPSZ_CD
      ,BKG_JOIN_FNC(CURSOR( SELECT CNTR_SEAL_NO FROM BKG_CNTR_SEAL_NO WHERE BKG_NO = CNT.BKG_NO AND CNTR_NO = CNT.CNTR_NO)) CNTR_SEAL_NO
      ,CNT.CNTR_PRT_FLG
      ,CNT.RCV_TERM_CD
      ,CNT.DE_TERM_CD
      ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD00767' AND INTG_CD_VAL_CTNT = BKG.BKG_CGO_TP_CD) BKG_CGO_TP_CD
      ,CNT.DIFF_RMK
  FROM BKG_BOOKING          BKG
      ,BKG_CONTAINER        CNT
 WHERE BKG.BKG_NO = CNT.BKG_NO
   AND BKG.BL_NO = @[bl_no]
			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
