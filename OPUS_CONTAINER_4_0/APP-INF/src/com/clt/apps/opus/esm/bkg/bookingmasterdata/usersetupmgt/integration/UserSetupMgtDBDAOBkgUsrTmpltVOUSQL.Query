<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOBkgUsrTmpltVOUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
update bkg_usr_tmplt set
	tmplt_tp_cd = @[tmplt_tp_cd],
	tmplt_hdr_nm = @[tmplt_hdr_nm],
	tmplt_ctnt = @[tmplt_ctnt],
	upd_usr_id = @[upd_usr_id],
	upd_dt = sysdate
where	usr_id = @[usr_id]
and	tmplt_tp_cd = @[tmplt_tp_cd]
and	tmplt_seq = @[tmplt_seq]			]]></sql>
			<params>
				<param name="tmplt_hdr_nm" type="12" value="" out="N"/>
				<param name="tmplt_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="tmplt_tp_cd" type="12" value="" out="N"/>
				<param name="tmplt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
