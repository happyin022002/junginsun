<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOMstContainerRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO
,      CNTR_TPSZ_CD
,      CNTR_STS_CD
,      CNMV_CYC_NO
,      CNMV_STS_CD
,      DMG_FLG
,      ACIAC_DIV_CD
,      ECC_CD
,      SYS_AREA_GRP_ID
,	   IMDT_EXT_FLG
FROM   MST_CONTAINER
WHERE  CNTR_NO = @[cntr_no]
UNION 
SELECT '' CNTR_NO
,      'B4' CNTR_TPSZ_CD
,      '' CNTR_STS_CD
,      TO_NUMBER('')  CNMV_CYC_NO
,      '' CNMV_STS_CD
,      '' DMG_FLG
,      '' ACIAC_DIV_CD
,      '' ECC_CD
,      '' SYS_AREA_GRP_ID
,	   '' IMDT_EXT_FLG
FROM   DUAL
WHERE  'BREAKBULK' =  UPPER(@[cntr_no])			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
