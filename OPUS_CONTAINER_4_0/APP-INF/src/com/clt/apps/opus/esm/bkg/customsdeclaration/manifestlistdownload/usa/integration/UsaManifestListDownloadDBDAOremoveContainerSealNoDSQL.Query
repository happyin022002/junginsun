<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOremoveContainerSealNoDSQL">
			<desc><![CDATA[removeContainerSealNo]]></desc>
			<sql><![CDATA[
DELETE BKG_CSTMS_SEAL_NO
WHERE 1=1
AND   CNT_CD = NVL(@[cnt_cd],'US')
AND   CSTMS_DIV_ID = NVL(@[cstms_div_id],'CTM')
AND ( #foreach($field_id in ${field_list}) 
      	#if($velocityCount > 1)
      	OR #end      BL_NO IN ( $field_id )
      #end
)			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_div_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
