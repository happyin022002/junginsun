<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOAddBlCluzStupCSQL">
			<desc><![CDATA[Insert into KBG_BL_CLUZ_STUP]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BL_CLUZ_STUP ( 
	ITM_SEQ
	,ORG_CNT_CD
    ,POR_APPL_FLG
    ,POL_APPL_FLG
    ,POD_APPL_FLG
    ,DEL_APPL_FLG
    ,CMDT_DESC
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
) VALUES (
	NVL((SELECT MAX(ITM_SEQ)+1 FROM BKG_BL_CLUZ_STUP), 1)
	, @[org_cnt_cd]
	, DECODE(@[por_appl_flg], '1', 'Y', '')
	, DECODE(@[pol_appl_flg], '1', 'Y', '')
	, DECODE(@[pod_appl_flg], '1', 'Y', '')
	, DECODE(@[del_appl_flg], '1', 'Y', '')
	, @[cmdt_desc]
	, @[cre_usr_id]
	, SYSDATE
	, @[upd_usr_id]
	, SYSDATE
)			]]></sql>
			<params>
				<param name="org_cnt_cd" type="12" value="" out="N"/>
				<param name="por_appl_flg" type="12" value="" out="N"/>
				<param name="pol_appl_flg" type="12" value="" out="N"/>
				<param name="pod_appl_flg" type="12" value="" out="N"/>
				<param name="del_appl_flg" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
