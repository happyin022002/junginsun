<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqSRepTotalAmountListRSQL">
			<desc><![CDATA[분기 S.Rep : T/B/L/RGN Volumn & Rev & CM Total Amount 조회]]></desc>
			<sql><![CDATA[
SELECT  SUM(LOD_QTY) || '|' || ROUND(SUM(LOD_QTY * GRS_RPB_REV)) || '|' ||   			    
        ROUND(SUM(LOD_QTY * GRS_RPB_REV - LOD_QTY * RA_CM_UC_AMT))	AS  CODE ,			    
        SUM(LOD_QTY) || '|' || ROUND(SUM(LOD_QTY * GRS_RPB_REV)) || '|' ||   			    
        ROUND(SUM(LOD_QTY * GRS_RPB_REV - LOD_QTY * RA_CM_UC_AMT))	AS  TEXT  			    
FROM    SAQ_MON_CFM_QTA                                                                      
WHERE   1 = 1                                                                                
AND     MQTA_RLSE_VER_NO    = @[mqtaRlseVerNo]                                                              
AND     QTA_TGT_CD          = @[qtaTgtCd]                                                              
AND     TRD_CD              = @[trdCd]                                                              
AND     RLANE_CD 			LIKE @[rlaneCd] || '%'                                                   
AND     DIR_CD              = @[dirCd]                                                              
AND     RGN_OFC_CD          = @[rgnOfcCd]                                                              
GROUP BY                                                                                     
        BSE_YR      ,                                                                        
        BSE_QTR_CD  ,                                                                        
        TRD_CD      ,                                                                        
		#if (${rlaneCd} != '' )
		        RLANE_CD    ,                                                                        
		#end
        DIR_CD      ,                                                                        
        RGN_OFC_CD        			]]></sql>
			<params>
				<param name="mqtaRlseVerNo" type="12" value="" out="N"/>
				<param name="qtaTgtCd" type="12" value="" out="N"/>
				<param name="trdCd" type="12" value="" out="N"/>
				<param name="rlaneCd" type="12" value="" out="N"/>
				<param name="dirCd" type="12" value="" out="N"/>
				<param name="rgnOfcCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
