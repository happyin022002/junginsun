<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshCustomsTransmissionDBDAOsearchPrePortRSQL">
			<desc><![CDATA[방글라데시 세관 신고용 Manifest Pre Port 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(VPS_PORT_CD, ' ') VPS_PORT_CD
FROM   VSK_VSL_PORT_SKD
WHERE  VSL_CD        = @[vsl_cd]
AND     SKD_VOY_NO = @[skd_voy_no]
AND     SKD_DIR_CD    = @[skd_dir_cd]
AND    CLPT_SEQ  = (SELECT  MAX(CLPT_SEQ) -1
                    FROM    VSK_VSL_PORT_SKD
                    WHERE   VSL_CD        = @[vsl_cd]
                    AND     SKD_VOY_NO = @[skd_voy_no]
                    AND     SKD_DIR_CD    = @[skd_dir_cd]
                    AND     VPS_PORT_CD    = @[pod_cd]
                    AND     NVL(SKD_CNG_STS_CD,' ') <> 'S')			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
