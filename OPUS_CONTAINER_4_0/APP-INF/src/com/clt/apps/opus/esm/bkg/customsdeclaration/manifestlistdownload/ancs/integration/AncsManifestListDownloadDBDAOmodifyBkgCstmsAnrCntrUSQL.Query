<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsManifestListDownloadDBDAOmodifyBkgCstmsAnrCntrUSQL">
			<desc><![CDATA[modifyBkgCstmsAnrCntr]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ANR_CNTR SET 
	CNTR_TPSZ_CD = @[cntr_tpsz_cd]
,	PCK_QTY = NVL(@[pck_qty],0)
,	WGT_UT_CD = @[wgt_ut_cd]
,	CNTR_WGT = NVL(@[cntr_wgt],0)
,	ORG_RCV_TERM_CD = @[org_rcv_term_cd]
,	DEST_DE_TERM_CD = @[dest_de_term_cd]
,	PCK_TP_CD = @[pck_tp_cd]
,   UPD_USR_ID = @[upd_usr_id]
,   UPD_DT     = SYSDATE 
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	BKG_NO = @[bkg_no]
AND	CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="org_rcv_term_cd" type="12" value="" out="N"/>
				<param name="dest_de_term_cd" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
