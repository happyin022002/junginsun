<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgencyCommissionDBDAOCoaOtrCommVOCSQL">
			<desc><![CDATA[COA_OTR_COMM  테이블의 데이터 삽입]]></desc>
			<sql><![CDATA[
MERGE INTO COA_OTR_COMM B1
USING (
       SELECT '1' FROM DUAL 
	  ) B2	
ON (     B1.COST_YRMON = @[cost_yrmon]						
	 AND B1.CNTR_TPSZ_CD = @[cntr_tpsz_cd]						
	 AND B1.COMM_LOC_CD = @[comm_loc_cd]
     AND B1.COA_COST_SRC_CD = @[coa_cost_src_cd])	
WHEN MATCHED THEN
     UPDATE 
        SET OTR_COMM_TTL_AMT = @[otr_comm_ttl_amt]
           ,BKG_TTL_QTY = @[bkg_ttl_qty] 
           ,STND_COST_USD_AMT = @[stnd_cost_usd_amt] 
           ,UPD_USR_ID = @[cre_usr_id] 
           ,UPD_DT = SYSDATE    
WHEN NOT MATCHED THEN 
    INSERT  (
			 COST_YRMON
			,CNTR_TPSZ_CD
			,COMM_LOC_CD
			,COA_COST_SRC_CD 
            ,STND_COST_USD_AMT
			,BKG_TTL_QTY
			,OTR_COMM_TTL_AMT 
            ,CRE_USR_ID
			,CRE_DT
			,UPD_USR_ID
			,UPD_DT
			 )VALUES(
					 @[cost_yrmon]
       				,@[cntr_tpsz_cd]
       				,@[comm_loc_cd]
      				,@[coa_cost_src_cd]
       				,@[stnd_cost_usd_amt]
      				,@[bkg_ttl_qty]
     				,@[otr_comm_ttl_amt]
      				,@[cre_usr_id]
      				,SYSDATE
      				,@[cre_usr_id]
      				,SYSDATE)			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="comm_loc_cd" type="12" value="" out="N"/>
				<param name="coa_cost_src_cd" type="12" value="" out="N"/>
				<param name="otr_comm_ttl_amt" type="12" value="" out="N"/>
				<param name="bkg_ttl_qty" type="12" value="" out="N"/>
				<param name="stnd_cost_usd_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
