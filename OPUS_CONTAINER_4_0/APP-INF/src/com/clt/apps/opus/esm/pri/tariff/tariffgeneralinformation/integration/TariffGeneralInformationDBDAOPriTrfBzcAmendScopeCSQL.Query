<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffGeneralInformationDBDAOPriTrfBzcAmendScopeCSQL">
			<desc><![CDATA[Tariff Code의 General Information을 Amend하여 Scope을 Insert 한다..]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_BZC_ROUT_PNT
    (
    TRF_PFX_CD
   ,TRF_NO
   ,AMDT_SEQ
   ,ORG_DEST_TP_CD
   ,TRF_BZC_ROUT_PNT_SEQ
   ,TRF_BZC_ROUT_PNT_TP_CD
   ,TRF_BZC_ROUT_PNT_DEF_CD
   ,N1ST_CMNC_AMDT_SEQ
   ,SRC_INFO_CD
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
    )
SELECT  
        TRF_PFX_CD
       ,TRF_NO
       ,AMDT_SEQ + 1
       ,ORG_DEST_TP_CD
       ,TRF_BZC_ROUT_PNT_SEQ
       ,TRF_BZC_ROUT_PNT_TP_CD
       ,TRF_BZC_ROUT_PNT_DEF_CD
       ,N1ST_CMNC_AMDT_SEQ
       ,SRC_INFO_CD
       ,@[cre_usr_id]
       ,SYSDATE
       ,@[upd_usr_id]
       ,SYSDATE
  FROM  PRI_TRF_BZC_ROUT_PNT
 WHERE  1 = 1
   AND  TRF_PFX_CD = @[trf_pfx_cd]
   AND  TRF_NO     = @[trf_no]
   AND  AMDT_SEQ   = @[amdt_seq]
   AND  SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
