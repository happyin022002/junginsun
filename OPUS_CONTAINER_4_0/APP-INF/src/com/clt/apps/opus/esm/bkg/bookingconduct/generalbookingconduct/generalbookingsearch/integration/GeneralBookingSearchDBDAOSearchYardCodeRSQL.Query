<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchYardCodeRSQL">
			<desc><![CDATA[GeneralBookingSearchDBDAOSearchYardCode]]></desc>
			<sql><![CDATA[
SELECT  YD.LOC_CD
       ,LC.LOC_NM
       ,YD.YD_CD
       ,YD.YD_NM
       ,LC.EQ_CTRL_OFC_CD
       ,DECODE(YD.YD_FCTY_TP_CY_FLG,      'Y','O','') YD_FCTY_TP_CY_FLG
       ,DECODE(YD.YD_FCTY_TP_CFS_FLG,     'Y','O','') YD_FCTY_TP_CFS_FLG
       ,DECODE(YD.YD_FCTY_TP_RAIL_RMP_FLG,'Y','O','') YD_FCTY_TP_RAIL_RMP_FLG
       ,DECODE(YD.YD_FCTY_TP_PSDO_YD_FLG, 'Y','O','') YD_FCTY_TP_PSDO_YD_FLG
       ,DECODE(YD.YD_FCTY_TP_MRN_TML_FLG, 'Y','O','') YD_FCTY_TP_MRN_TML_FLG
       ,'' YD_ADDR
       ,'' ZN_CD
       ,'' ZN_NM
       ,'' PST_CD
       ,'' DSTR_NM
FROM    MDM_YARD YD
       ,MDM_LOCATION LC
WHERE   YD.LOC_CD = LC.LOC_CD     
#if (${country_cd} != '')    
AND     LC.CNT_CD = @[country_cd]
#end
#if (${loc_cd} != '')    
#if (${loc_cd2}!= '') 
AND     YD.YD_CD LIKE @[loc_cd]||'%'||@[loc_cd2]
#else
AND     YD.LOC_CD LIKE @[loc_cd]||'%'
#end
#else
#if (${loc_cd2}!= '') 
AND     YD.YD_CD LIKE '%'||@[loc_cd2]
#end
#end
#if (${eq_ctrl_ofc_cd}!= '') 
AND     LC.EQ_CTRL_OFC_CD = @[eq_ctrl_ofc_cd]
#end
#if (${marine_terminal}== 'Y') 
AND     YD.YD_FCTY_TP_MRN_TML_FLG = 'Y'  
#end
AND     YD.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="country_cd" type="12" value="US" out="Y"/>
				<param name="loc_cd" type="12" value="USLGB" out="N"/>
				<param name="loc_cd2" type="12" value="PT" out="N"/>
				<param name="eq_ctrl_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
