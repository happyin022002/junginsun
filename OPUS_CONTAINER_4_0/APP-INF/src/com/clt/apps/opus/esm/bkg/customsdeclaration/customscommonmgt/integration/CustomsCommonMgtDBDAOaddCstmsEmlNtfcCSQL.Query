<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOaddCstmsEmlNtfcCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_EML_NTFC

       (CNT_CD,
        EDI_MSG,
        EDI_MSG_TP_ID,
        POL_CD,
        POD_CD,
        TO_EML_CTNT,
        CC_EML_CTNT,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (@[cnt_cd],
        @[edi_msg],
        @[edi_msg_tp_id],
        NVL(@[pol_cd], ' '),
        NVL(@[pod_cd], ' '),
        REPLACE(@[to_eml_ctnt], ' ', ''),
        REPLACE(@[cc_eml_ctnt], ' ', ''),
        @[usr_id],
        SYSDATE,
        @[usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="edi_msg" type="12" value="" out="N"/>
				<param name="edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="to_eml_ctnt" type="12" value="" out="N"/>
				<param name="cc_eml_ctnt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
