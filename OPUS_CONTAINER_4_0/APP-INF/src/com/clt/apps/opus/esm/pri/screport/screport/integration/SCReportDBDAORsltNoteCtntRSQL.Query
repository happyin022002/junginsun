<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltNoteCtntRSQL">
			<desc><![CDATA[RFA/SC NOTE CTNT]]></desc>
			<sql><![CDATA[
#if (${is_sc} == 'Y' && ${note_gubun} == 'R')
SELECT BKG_JOIN_FULL_CLOB_FNC(CURSOR(
                SELECT  t.ROUT_NOTE_SEQ ||'. '||t.NOTE_CTNT NOTE_CTNT
                FROM    PRI_SP_SCP_RT_CMDT_RNOTE t
                WHERE   t.PROP_NO            = @[prop_no]
                AND     t.AMDT_SEQ           = @[amdt_seq]
                AND     t.SVC_SCP_CD         = @[svc_scp_cd]
                AND     t.GEN_SPCL_RT_TP_CD  = @[gen_spcl_rt_tp_cd]
                AND     t.CMDT_HDR_SEQ       = @[cmdt_hdr_seq]
                AND     t.ROUT_SEQ           = @[rout_seq]
                AND     t.SRC_INFO_CD        <> 'AD'
                ORDER BY t.ROUT_NOTE_SEQ
        ),CHR(13)||CHR(13)) AS NOTE_CTNT FROM DUAL

#end
#if (${is_sc} == 'Y' && ${note_gubun} == 'C')

SELECT BKG_JOIN_FULL_CLOB_FNC(CURSOR(
                SELECT  t.CMDT_HDR_SEQ ||'. '||t.NOTE_CTNT NOTE_CTNT
                FROM    PRI_SP_SCP_RT_CNOTE t
                WHERE   t.PROP_NO            = @[prop_no]
                AND     t.AMDT_SEQ           = @[amdt_seq]
                AND     t.SVC_SCP_CD         = @[svc_scp_cd]
                AND     t.GEN_SPCL_RT_TP_CD  = @[gen_spcl_rt_tp_cd]
                AND     t.CMDT_HDR_SEQ       = @[cmdt_hdr_seq]
                AND     t.SRC_INFO_CD        <> 'AD'
                ORDER BY t.CMDT_NOTE_SEQ
        ),CHR(13)||CHR(13)) AS NOTE_CTNT FROM DUAL
#end
#if (${is_sc} == 'Y' && ${note_gubun} == 'S')

SELECT BKG_JOIN_FULL_CLOB_FNC(CURSOR(
                SELECT SND.NOTE_CTNT
                FROM PRI_SP_SCP_NOTE SN, PRI_SP_SCP_NOTE_CTNT SND
                WHERE SN.PROP_NO = SND.PROP_NO
                AND SN.AMDT_SEQ = SND.AMDT_SEQ
                AND SN.SVC_SCP_CD = SND.SVC_SCP_CD
                AND SN.NOTE_TP_CD = SND.NOTE_TP_CD
                AND SN.NOTE_SEQ = SND.NOTE_SEQ
                AND SN.NOTE_TP_CD = 'P' --Special Note
                AND SN.NOTE_CLSS_CD <> 'D' --O:Surcharge Exceptions S:Fixed Surcharge D:DEM/DET
                AND SND.SRC_INFO_CD <> 'AD' --DELETE
                AND SN.PROP_NO = @[prop_no]
                AND SN.AMDT_SEQ = @[amdt_seq]
                AND SN.SVC_SCP_CD = @[svc_scp_cd]
                ORDER BY DECODE(SN.NOTE_CLSS_CD, 'O', 1, 'S', 2, 3), SND.DP_SEQ
                ),CHR(13)||CHR(13)) AS NOTE_CTNT FROM DUAL
#end
#if (${is_sc} != 'Y' && ${note_gubun} == 'R')

 SELECT BKG_JOIN_FULL_CLOB_FNC(CURSOR(
                SELECT  RN.ROUT_NOTE_SEQ ||'. '||RN.NOTE_CTNT NOTE_CTNT
                FROM    PRI_RP_SCP_RT_CMDT_RNOTE RN
                WHERE   RN.PROP_NO            = @[prop_no]
                AND     RN.AMDT_SEQ           = @[amdt_seq]
                AND     RN.SVC_SCP_CD         = @[svc_scp_cd]
                AND     RN.CMDT_HDR_SEQ       = @[cmdt_hdr_seq]
                AND     RN.ROUT_SEQ           = @[rout_seq]
                AND     RN.SRC_INFO_CD        <> 'AD'
                ORDER BY RN.ROUT_NOTE_SEQ
                ),CHR(13)||CHR(13)) AS NOTE_CTNT FROM DUAL
#end
#if (${is_sc} != 'Y' && ${note_gubun} == 'C')

SELECT BKG_JOIN_FULL_CLOB_FNC(CURSOR(
                SELECT  CN.CMDT_HDR_SEQ ||'. '||CN.NOTE_CTNT NOTE_CTNT
                FROM    PRI_RP_SCP_RT_CNOTE  CN
                WHERE   CN.PROP_NO            = @[prop_no]
                AND     CN.AMDT_SEQ           = @[amdt_seq]
                AND     CN.SVC_SCP_CD         = @[svc_scp_cd]
                AND     CN.CMDT_HDR_SEQ       = @[cmdt_hdr_seq]
                AND     CN.SRC_INFO_CD        <> 'AD'
                ORDER BY CN.CMDT_NOTE_SEQ
                ),CHR(13)||CHR(13)) AS NOTE_CTNT FROM DUAL
#end
#if (${is_sc} != 'Y' && ${note_gubun} == 'S')

SELECT BKG_JOIN_FULL_CLOB_FNC(CURSOR(
                SELECT NOTE_CTNT
                FROM PRI_RP_SCP_NOTE SN, PRI_RP_SCP_NOTE_CTNT SND
                WHERE SN.PROP_NO = SND.PROP_NO
                AND SN.AMDT_SEQ = SND.AMDT_SEQ
                AND SN.SVC_SCP_CD = SND.SVC_SCP_CD
                AND SN.NOTE_TP_CD = SND.NOTE_TP_CD
                AND SN.NOTE_SEQ = SND.NOTE_SEQ
                AND SN.NOTE_TP_CD = 'P' --Special Note
                AND SND.SRC_INFO_CD <> 'AD' --DELETE
                AND SN.PROP_NO = @[prop_no]
                AND SN.AMDT_SEQ = @[amdt_seq]
                AND SN.SVC_SCP_CD = @[svc_scp_cd]
                ORDER BY SND.NOTE_SEQ, SND.DP_SEQ
                ),CHR(13)||CHR(13)) AS NOTE_CTNT FROM DUAL
#end			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
