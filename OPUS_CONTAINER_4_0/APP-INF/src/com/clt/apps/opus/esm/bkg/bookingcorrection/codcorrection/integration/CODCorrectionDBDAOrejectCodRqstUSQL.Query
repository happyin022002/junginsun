<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOrejectCodRqstUSQL">
			<desc><![CDATA[해당 COD 요청 건에 대해서 거절 상태로 update한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_COD SET 
	COD_STS_CD = 'N'
  ,	COD_AUTH_FLG = @[cod_auth_flg]
  ,	COD_RJCT_CD = @[cod_rjct_cd]
  ,	COD_RJCT_RSN_RMK = @[cod_rjct_rsn_rmk]
  ,	UPD_USR_ID = @[usr_id]
  ,	UPD_DT = sysdate
  ,	DIFF_RMK = @[diff_rmk]
WHERE	BKG_NO = @[bkg_no]
AND	COD_RQST_SEQ = @[cod_rqst_seq]			]]></sql>
			<params>
				<param name="cod_auth_flg" type="12" value="" out="N"/>
				<param name="cod_rjct_cd" type="12" value="" out="N"/>
				<param name="cod_rjct_rsn_rmk" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
