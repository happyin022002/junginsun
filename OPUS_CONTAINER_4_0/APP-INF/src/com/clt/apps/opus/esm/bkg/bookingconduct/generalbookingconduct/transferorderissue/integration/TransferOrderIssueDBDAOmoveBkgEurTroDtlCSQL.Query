<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOmoveBkgEurTroDtlCSQL">
			<desc><![CDATA[combine시 tro를 옮겨 준다]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_EUR_TRO_dtl
(BKG_NO
,IO_BND_CD
,TRO_SEQ
,TRO_SUB_SEQ
,DOR_ADDR_TP_CD
,LOC_CD
,ZN_CD
,LOD_REF_NO
,DOR_ZIP_ID
,DOR_ADDR
,ARR_DT
,CNTC_PSON_NM
,CNTC_PHN_NO
,CNTC_EML
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT)
SELECT 
@[new_bkg_no]
,IO_BND_CD
,to_number(@[new_tro_seq])
,TRO_SUB_SEQ
,DOR_ADDR_TP_CD
,LOC_CD
,ZN_CD
,LOD_REF_NO
,DOR_ZIP_ID
,DOR_ADDR
,ARR_DT
,CNTC_PSON_NM
,CNTC_PHN_NO
,CNTC_EML
,@[usr_id]
,sysdate
,@[usr_id]
,sysdate
FROM BKG_EUR_TRO_dtl source
WHERE BKG_NO = @[org_bkg_no]
AND tro_seq = @[org_tro_seq]			]]></sql>
			<params>
				<param name="new_bkg_no" type="12" value="" out="N"/>
				<param name="new_tro_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="org_bkg_no" type="12" value="" out="N"/>
				<param name="org_tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
