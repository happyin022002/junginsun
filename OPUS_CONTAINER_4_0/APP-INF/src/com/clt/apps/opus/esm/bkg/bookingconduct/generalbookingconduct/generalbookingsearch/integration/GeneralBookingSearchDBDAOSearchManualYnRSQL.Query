<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchManualYnRSQL">
			<desc><![CDATA[SearchManualYn]]></desc>
			<sql><![CDATA[
SELECT 'Y' AS RST_YN
FROM BKG_CHN_BKG_NO_GEN BCB
WHERE BCB.BKG_NO_USE_FLG = 'N'
AND   NOT EXISTS (SELECT 'X' FROM BKG_BOOKING BB WHERE BB.BKG_NO = BCB.BKG_NO)
AND   BCB.BKG_NO = @[bkg_no]
AND   ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
