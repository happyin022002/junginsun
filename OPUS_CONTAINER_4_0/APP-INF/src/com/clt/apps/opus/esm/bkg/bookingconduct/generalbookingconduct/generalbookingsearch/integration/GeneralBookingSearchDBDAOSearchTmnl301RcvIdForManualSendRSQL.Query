<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchTmnl301RcvIdForManualSendRSQL">
			<desc><![CDATA[manual 전송시 수신 대상 ID를 조회한다.]]></desc>
			<sql><![CDATA[
WITH RCV_ID AS 
       (SELECT CASE WHEN MIN(KIND) LIKE MIN(SUBSTR(KIND,0,LENGTH(KIND)-2))||'%'
                    THEN SUBSTR(MIN(KIND),LENGTHB(MIN(KIND))-1,LENGTHB(MIN(KIND)))
                    END AS NTC_KND_CD
			 , EDI_RCV_ID
    		 , EDI_REF_CD
          FROM 
               (--BKG CONFIRM
                SELECT DISTINCT
					   EY.RCVR_TRD_PRNR_ID AS EDI_RCV_ID
                     , EY.PRNR_PORT_CD AS PORT_CD
                     , EY.PRNR_SUB_LNK_CD AS YD_CD
                     , CD.INTG_CD_VAL_DP_SEQ||CD.INTG_CD_VAL_CTNT AS KIND
            		 , EY.PRNR_SUB_LNK_CD AS EDI_REF_CD
                  FROM BKG_EDI_TRD_PRNR_SUB_LNK EY
                     , BKG_EDI_SUB_LNK_MSG MSG
                     , BKG_VVD VVD
                     , BKG_BOOKING BK
                     , COM_INTG_CD_DTL CD
                 WHERE BK.BKG_NO   = @[bkg_no]
                   AND BK.BKG_NO   = VVD.BKG_NO
                   AND EY.TRD_PRNR_SUB_LNK_SEQ = MSG.TRD_PRNR_SUB_LNK_SEQ
                   AND EY.PRNR_SUB_LNK_DIV_CD = '1'
				   AND MSG.MSG_TP_DESC		  = '1'
                   AND MSG.EDI_MSG_TP_ID      = '301'
--                   and EY.TRD_PRNR_SUB_LNK_SEQ in 
--                        (select TRD_PRNR_SUB_LNK_SEQ from BKG_EDI_SUB_LNK_MSG where EDI_MSG_IND_CD ='2') --2:manual
                   AND (   (EY.PRNR_SUB_LNK_CD = BK.POL_NOD_CD AND MSG.EDI_MSG_IND_CD IN ('1', '2')) --입력된 찾아진 pol과 같거나
                        OR (EY.PRNR_SUB_LNK_CD = VVD.POL_YD_CD AND VVD.VSL_PRE_PST_CD <> 'U' AND MSG.EDI_MSG_IND_CD = '6') --pre t/s port 지정 건이나
                        OR (EY.PRNR_SUB_LNK_CD = VVD.POL_YD_CD AND VVD.VSL_PRE_PST_CD =  'U' AND MSG.EDI_MSG_IND_CD = '7') --post t/s port 지정 건
                       )
                   AND 'CD01552' = CD.INTG_CD_ID
                   AND 'BT' = CD.INTG_CD_VAL_CTNT
                UNION ALL
                 --EMPTY RELEASE ORDER 대상
                SELECT DISTINCT
                	   EY.RCVR_TRD_PRNR_ID AS EDI_RCV_ID
                	 , EY.PRNR_PORT_CD AS PORT_CD
                     , EY.PRNR_SUB_LNK_CD AS YD_CD
                     , CD.INTG_CD_VAL_DP_SEQ||CD.INTG_CD_VAL_CTNT AS KIND
            		 , EY.PRNR_SUB_LNK_CD AS EDI_REF_CD
                  FROM BKG_EDI_TRD_PRNR_SUB_LNK EY
                	 , BKG_EDI_SUB_LNK_MSG MSG
                	 , BKG_BOOKING BK
                     , COM_INTG_CD_DTL CD
                     , MDM_LOCATION ML    
                 WHERE BK.BKG_NO = @[bkg_no]
                   AND 1 = 2 -- Manual 전송은 MTY 를 보내지 않는다
                   AND BK.MTY_PKUP_YD_CD = EY.PRNR_SUB_LNK_CD
                   AND EY.TRD_PRNR_SUB_LNK_SEQ = MSG.TRD_PRNR_SUB_LNK_SEQ
                   AND EDI_MSG_IND_CD 	 = '5'
				   AND MSG.MSG_TP_DESC	 = '1'
                   AND MSG.EDI_MSG_TP_ID = '301'
                   AND SUBSTR(BK.MTY_PKUP_YD_CD,1,5) = ML.LOC_CD       ----------
                   AND ML.CONTI_CD||RCV_TERM_CD <> 'ED'
--                   and EY.TRD_PRNR_SUB_LNK_SEQ in 
--                        (select TRD_PRNR_SUB_LNK_SEQ from BKG_EDI_SUB_LNK_MSG where EDI_MSG_IND_CD ='2') --2:manual
                   AND 'CD01552' = CD.INTG_CD_ID
                   AND 'CN' = CD.INTG_CD_VAL_CTNT
                UNION ALL
                --FULL RELEASE ORDER 대상   
                SELECT DISTINCT
					   EY.RCVR_TRD_PRNR_ID AS EDI_RCV_ID
                	 , EY.PRNR_PORT_CD AS PORT_CD
                     , EY.PRNR_SUB_LNK_CD AS YD_CD
                     , CD.INTG_CD_VAL_DP_SEQ||CD.INTG_CD_VAL_CTNT AS KIND
            		 , EY.PRNR_SUB_LNK_CD AS EDI_REF_CD
                  FROM BKG_EDI_TRD_PRNR_SUB_LNK EY
                	 , BKG_EDI_SUB_LNK_MSG MSG
                	 , BKG_BOOKING BK
                     , COM_INTG_CD_DTL CD
                     , MDM_LOCATION ML        
                 WHERE BK.BKG_NO = @[bkg_no]
                   AND BK.FULL_RTN_YD_CD = EY.PRNR_SUB_LNK_CD
                   AND EY.TRD_PRNR_SUB_LNK_SEQ = MSG.TRD_PRNR_SUB_LNK_SEQ
                   AND MSG.EDI_MSG_TP_ID 	= '301'
				   AND MSG.MSG_TP_DESC	 	= '1'
                   --AND EY.RCVR_TRD_PRNR_ID  = 'CN'
                   AND SUBSTR(BK.FULL_RTN_YD_CD,1,5) = ML.LOC_CD       ----------
                   AND ML.CONTI_CD = 'E'  
                   AND EDI_MSG_IND_CD 	 = '4'                            ----------
                   AND NOT EXISTS (SELECT 'X' 
                                   FROM BKG_EDI_SUB_LNK_MSG MSG, BKG_EDI_TRD_PRNR_SUB_LNK LNK
                                   WHERE EDI_MSG_IND_CD ='1' 
                                   AND MSG.TRD_PRNR_SUB_LNK_SEQ = LNK.TRD_PRNR_SUB_LNK_SEQ
                                   AND LNK.PRNR_SUB_LNK_CD = BK.POL_NOD_CD
                                   AND BK.POL_NOD_CD = BK.FULL_RTN_YD_CD)
--                   and EY.TRD_PRNR_SUB_LNK_SEQ in 
--                        (select TRD_PRNR_SUB_LNK_SEQ from BKG_EDI_SUB_LNK_MSG where EDI_MSG_IND_CD ='1') --1:auto
                   AND 'CD01552' = CD.INTG_CD_ID
                   AND 'FC' = CD.INTG_CD_VAL_CTNT
                )
       GROUP BY EDI_RCV_ID, EDI_REF_CD)
#if(${brac}=='M')
, SENT_ID AS (SELECT /*+INDEX_DESC(BKG_NTC_HIS XPKBKG_NTC_HIS)*/NTC_KND_CD, EDI_ID AS EDI_RCV_ID, TML_NTC_SND_STS_CD--TYPE, ID별 마지막 전송 기록
                FROM BKG_NTC_HIS
       	       WHERE BKG_NO      = @[bkg_no]
                 AND NTC_VIA_CD  = 'E'
                 AND NTC_KND_CD  IN ('BT','CN','FC')
				 AND ROWNUM      = 1)
--0702 화면에서 전송시 새로 보낼 곳이 기존에 보낸적이 없으면 New로 전송, 보낸적이 있으면 UPDATE로 전송
SELECT NTC_KND_CD AS KIND, EDI_RCV_ID, '' AS EDI_REF_CD, 'R' BRAC_CD--CANCEL로 전송
  FROM (SELECT NTC_KND_CD, EDI_RCV_ID--기존 전송처가 
          FROM SENT_ID 
         WHERE SENT_ID.TML_NTC_SND_STS_CD <> 'R'     --기존 전송처가 CANCE이면 제외
		   AND SENT_ID.TML_NTC_SND_STS_CD IS NOT NULL--기존 전송처의 STATUS가 불분명할 때도 제외
         MINUS
        SELECT NTC_KND_CD, EDI_RCV_ID--새 전송처에 없을 경우
          FROM RCV_ID)
 UNION ALL
SELECT NTC_KND_CD AS KIND, EDI_RCV_ID, '' AS EDI_REF_CD, 'N' BRAC_CD--NEW로 전송
  FROM (SELECT NTC_KND_CD, EDI_RCV_ID--새 전송처가 
          FROM RCV_ID
         MINUS
        SELECT NTC_KND_CD, EDI_RCV_ID--기존 전송처에 없을 경우
          FROM SENT_ID)        
 UNION ALL
SELECT DISTINCT RCV_ID.NTC_KND_CD AS KIND, RCV_ID.EDI_RCV_ID, RCV_ID.EDI_REF_CD, DECODE(SENT_ID.TML_NTC_SND_STS_CD, 'R', 'N', 'M', 'U', @[brac]) BRAC_CD --마지막 전송이 R일 경우 'N', 아니면 U나 B로 전송
  FROM RCV_ID, SENT_ID
 WHERE RCV_ID.NTC_KND_CD = SENT_ID.NTC_KND_CD--기존 전송처에
   AND RCV_ID.EDI_RCV_ID = SENT_ID.EDI_RCV_ID--재전송을 경우
#else
--0616 화면에서 전송시 전달 받은 값으로 전송  
SELECT NTC_KND_CD AS KIND_CD, EDI_RCV_ID, EDI_REF_CD, @[brac] BRAC_CD
  FROM RCV_ID
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="ATL000109400" out="N"/>
				<param name="brac" type="12" value="n" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
