<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchTempBkgStwgCgoRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchTempBkgStwgCgoRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO 
,STWG_SEQ
,STWG_RMK
,TO_CHAR(RQST_DT, 'YYYY-MM-DD HH24:MI:SS') AS RQST_DT
,TO_CHAR(RQST_GDT, 'YYYY-MM-DD HH24:MI:SS') AS RQST_GDT
,RQST_USR_ID
,APLY_NO
,SPCL_CGO_APRO_CD
,CRE_USR_ID
,TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') AS CRE_DT
,UPD_USR_ID
,TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') AS UPD_DT
,STWG_CD
  FROM BKG_STWG_CGO
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
