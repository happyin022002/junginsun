<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchDeclBaseInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT '89N' AS DOC_NAME,
       @[d_type] AS HANDLING,
       (SELECT DECODE(COUNT(*), 0, 'O', 'U') AS SEND_STATUS
          FROM BKG_CSTMS_AUS_DG_SND
         WHERE AUS_SND_LOG_ID = 'IFD'
           AND PSA_DG_DECL_TP_CD = @[d_type]
           AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
           AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
           AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
           AND PORT_CD = @[port_cd] ) AS STATUS,
       -- Cancel전송 클릭 시 'C', 최초전송이면 'O', BKG_CSTMS_EUR_DG_SND_LOG에 존재하면 'U'
       NVL(@[reason_resending], '') AS REASON,
       '' AS USER_REF,
       -- 전송로그 MSN_SND_NO를 사용함
       '' AS OLD_USER_REF,
       '' AS FIRST_USER_REF,
       '' AS SEC_FILE_NBR,
       '' AS FF_REF -- 확인 요망

  FROM DUAL
			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="reason_resending" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
