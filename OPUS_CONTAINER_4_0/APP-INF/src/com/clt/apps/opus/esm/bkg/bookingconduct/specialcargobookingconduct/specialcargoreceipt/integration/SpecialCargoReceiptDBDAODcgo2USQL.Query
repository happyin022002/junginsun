<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAODcgo2USQL">
			<desc><![CDATA[Dcgo2]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')

UPDATE BKG_DG_CGO_HIS SET
	AWK_CGO_SEQ = @[awk_cgo_seq]
,	AWK_CGO_FLG = 'Y'
WHERE BKG_NO = @[bkg_no]
AND DCGO_SEQ = @[awk_dcgo_seq]
AND CORR_NO = 'TMP0000001'

#else

UPDATE BKG_DG_CGO SET
	AWK_CGO_SEQ = @[awk_cgo_seq]
,	AWK_CGO_FLG = 'Y'
WHERE BKG_NO = @[bkg_no]
AND DCGO_SEQ = @[awk_dcgo_seq]

#end			]]></sql>
			<params>
				<param name="awk_cgo_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="awk_dcgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
