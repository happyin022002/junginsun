<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAORemoveAcmSimBkgInfoDSQL">
			<desc><![CDATA[RemoveAcmSimBkgInfo]]></desc>
			<sql><![CDATA[
DELETE ACM_SIM_BKG_INFO B
WHERE 1=1  
AND B.SIM_NO = @[sim_no]
AND B.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
