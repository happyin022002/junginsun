   OR CUST_TAX_ID_S = 'E'

   OR CUST_NM_C = 'E'
   OR CUST_ADDR_C = 'E'
   OR CUST_CTY_NM_C = 'E'
   OR CSTMS_DECL_CNT_CD_C = 'E'
   OR CUST_STE_CD_C = 'E'
   OR CUST_ZIP_ID_C = 'E'
   OR CUST_ST_NM_C = 'E'
   OR CUST_EORI_NO_C = 'E'
   OR CUST_TAX_ID_C = 'E'

   OR CUST_NM_N = 'E'
