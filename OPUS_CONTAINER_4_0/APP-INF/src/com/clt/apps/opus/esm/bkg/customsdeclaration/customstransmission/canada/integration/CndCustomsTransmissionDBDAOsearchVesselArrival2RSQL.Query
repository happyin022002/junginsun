<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndCustomsTransmissionDBDAOsearchVesselArrival2RSQL">
			<desc><![CDATA[searchVesselArrival2]]></desc>
			<sql><![CDATA[
SELECT  B.CVY_REF_NO
  FROM  BKG_CSTMS_ADV_BL A, BKG_CSTMS_CND_VSL B
 WHERE  A.VSL_CD = B.VSL_CD
   AND  A.SKD_VOY_NO = B.SKD_VOY_NO
   AND  A.SKD_DIR_CD = B.SKD_DIR_CD
   AND  A.CSTMS_PORT_CD = B.PORT_CD
   AND  A.CNT_CD = 'CA'
   AND  A.BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
