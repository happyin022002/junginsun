<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCllSpclCgoSumByPodRSQL">
			<desc><![CDATA[searchCllSpclCgoSumByPod]]></desc>
			<sql><![CDATA[
SELECT *
FROM (
    SELECT POD_CD GUBUN_CD2,
      '' GUBUN_CD,
      '' GUBUN_CD3,
      1 ORDER_GUBUN,
      DECODE(SUM(LOCAL_DG_20), 0, '', SUM(LOCAL_DG_20)) LOCAL_DG_20,
      DECODE(SUM(LOCAL_DG_40), 0, '', SUM(LOCAL_DG_40)) LOCAL_DG_40,
      DECODE(SUM(LOCAL_DG_40H + LOCAL_DG_45), 0, '', SUM(LOCAL_DG_40H + LOCAL_DG_45)) LOCAL_DG_40H,
      DECODE(SUM(LOCAL_RF_20), 0, '', SUM(LOCAL_RF_20)) LOCAL_RF_20,
      DECODE(SUM(LOCAL_RF_40), 0, '', SUM(LOCAL_RF_40)) LOCAL_RF_40,
      DECODE(SUM(LOCAL_RF_40H + LOCAL_RF_45), 0, '', SUM(LOCAL_RF_40H + LOCAL_RF_45)) LOCAL_RF_40H,
      DECODE(SUM(LOCAL_FR_20), 0, '', SUM(LOCAL_FR_20)) LOCAL_FR_20,
      DECODE(SUM(LOCAL_FR_40), 0, '', SUM(LOCAL_FR_40)) LOCAL_FR_40,
      DECODE(SUM(LOCAL_FR_40H + LOCAL_RF_45), 0, '', SUM(LOCAL_FR_40H + LOCAL_RF_45)) LOCAL_FR_40H,
      DECODE(SUM(LOCAL_OT_20), 0, '', SUM(LOCAL_OT_20)) LOCAL_OT_20,
      DECODE(SUM(LOCAL_OT_40), 0, '', SUM(LOCAL_OT_40)) LOCAL_OT_40,
      DECODE(SUM(LOCAL_OT_40H), 0, '', SUM(LOCAL_OT_40H)) LOCAL_OT_40H,
      DECODE(SUM(LOCAL_PCOD_20), 0, '', SUM(LOCAL_PCOD_20)) LOCAL_PCOD_20,
      DECODE(SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45), 0, '', SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45)) LOCAL_PCOD_40,
      DECODE(SUM(TS_DG_20), 0, '', SUM(TS_DG_20)) TS_DG_20,
      DECODE(SUM(TS_DG_40), 0, '', SUM(TS_DG_40)) TS_DG_40,
      DECODE(SUM(TS_DG_40H + TS_DG_45), 0, '', SUM(TS_DG_40H + TS_DG_45)) TS_DG_40H,
      DECODE(SUM(TS_RF_20), 0, '', SUM(TS_RF_20)) TS_RF_20,
      DECODE(SUM(TS_RF_40), 0, '', SUM(TS_RF_40)) TS_RF_40,
      DECODE(SUM(TS_RF_40H + TS_RF_45), 0, '', SUM(TS_RF_40H + TS_RF_45)) TS_RF_40H,
      DECODE(SUM(TS_FR_20), 0, '', SUM(TS_FR_20)) TS_FR_20,
      DECODE(SUM(TS_FR_40), 0, '', SUM(TS_FR_40)) TS_FR_40,
      DECODE(SUM(TS_FR_40H + TS_FR_45), 0, '', SUM(TS_FR_40H + TS_FR_45)) TS_FR_40H,
      DECODE(SUM(TS_OT_20), 0, '', SUM(TS_OT_20)) TS_OT_20,
      DECODE(SUM(TS_OT_40), 0, '', SUM(TS_OT_40)) TS_OT_40,
      DECODE(SUM(TS_OT_40H), 0, '', SUM(TS_OT_40H)) TS_OT_40H,
      DECODE(SUM(TS_PCOD_20), 0, '', SUM(TS_PCOD_20)) TS_PCOD_20,
      DECODE(SUM(TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45), 0, '', SUM(TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45)) TS_PCOD_40,
      DECODE(SUM(LOCAL_DG_20 + TS_DG_20), 0, '', SUM(LOCAL_DG_20 + TS_DG_20)) SUM_DG_20,
      DECODE(SUM(LOCAL_DG_40 + TS_DG_40), 0, '', SUM(LOCAL_DG_40 + TS_DG_40)) SUM_DG_40,
      DECODE(SUM(LOCAL_DG_40H + LOCAL_DG_45 + TS_DG_40H + TS_DG_45), 0, '', SUM(LOCAL_DG_40H + LOCAL_DG_45 + TS_DG_40H + TS_DG_45)) SUM_DG_40H,
      DECODE(SUM(LOCAL_RF_20 + TS_RF_20), 0, '', SUM(LOCAL_RF_20 + TS_RF_20)) SUM_RF_20,
      DECODE(SUM(LOCAL_RF_40 + TS_RF_40), 0, '', SUM(LOCAL_RF_40 + TS_RF_40)) SUM_RF_40,
      DECODE(SUM(LOCAL_RF_40H + LOCAL_RF_45 + TS_RF_40H + TS_RF_45), 0, '', SUM(LOCAL_RF_40H + LOCAL_RF_45 + TS_RF_40H + TS_RF_45)) SUM_RF_40H,
      DECODE(SUM(LOCAL_FR_20 + TS_FR_20), 0, '', SUM(LOCAL_FR_20 + TS_FR_20)) SUM_FR_20,
      DECODE(SUM(LOCAL_FR_40 + TS_FR_40), 0, '', SUM(LOCAL_FR_40 + TS_FR_40)) SUM_FR_40,
      DECODE(SUM(LOCAL_FR_40H + LOCAL_FR_45 + TS_FR_40H + TS_FR_45), 0, '', SUM(LOCAL_FR_40H + LOCAL_FR_45 + TS_FR_40H + TS_FR_45)) SUM_FR_40H,
      DECODE(SUM(LOCAL_OT_20 + TS_OT_20), 0, '', SUM(LOCAL_OT_20 + TS_OT_20)) SUM_OT_20,
      DECODE(SUM(LOCAL_OT_40 + TS_OT_40), 0, '', SUM(LOCAL_OT_40 + TS_OT_40)) SUM_OT_40,
      DECODE(SUM(LOCAL_OT_40H + TS_OT_40H), 0, '', SUM(LOCAL_OT_40H + TS_OT_40H)) SUM_OT_40H,
      DECODE(SUM(LOCAL_PCOD_20 + TS_PCOD_20), 0, '', SUM(LOCAL_PCOD_20 + TS_PCOD_20)) SUM_PCOD_20,
      DECODE(SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45 + TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45), 0, '', SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45 + TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45)) SUM_PCOD_40
    FROM (
        SELECT CLL.POD_CD,
          1,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0), 0) LOCAL_DG_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 0), 0) LOCAL_DG_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 0), 0) LOCAL_DG_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0)), 0), 0) LOCAL_DG_45,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_RF_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_RF_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 0), 0), 0) LOCAL_RF_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_RF_45,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_FR_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_FR_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 0), 0), 0) LOCAL_FR_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0)), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_FR_45,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'O', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 'S', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_OT_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'O', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 'S', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) LOCAL_OT_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'O', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 'S', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 0), 0), 0) LOCAL_OT_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0), 0), 0) LOCAL_PCOD_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0), 0), 0) LOCAL_PCOD_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0), 0), 0) LOCAL_PCOD_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.MTY_BKG_CD, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0), 0), 0) LOCAL_PCOD_45,

          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0))) TS_DG_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0))) TS_DG_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO)))) TS_DG_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(DG.CNTR_NO, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0))) TS_DG_45,

          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0)) TS_RF_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 0)) TS_RF_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 0)) TS_RF_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'R', DECODE(SUBSTR(CLL.CLL_RMK2, 1, 2), 'RD', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0)), 0)) TS_RF_45,

          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0)) TS_FR_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 0)) TS_FR_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, COUNT(CLL.BKG_NO))), 0)) TS_FR_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'F', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0)), 'A', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0)), 0)) TS_FR_45,

          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'O', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0)) TS_OT_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'O', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '4', COUNT(CLL.BKG_NO), 0)), 0)) TS_OT_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'O', DECODE(CLL.CLL_RMK2, 'INGU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, COUNT(CLL.BKG_NO))), 0)) TS_OT_40H,

          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0)) TS_PCOD_20,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0))) TS_PCOD_40,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0)) TS_PCOD_40H,
          DECODE(CLL.KR_CLL_TS_CD, NULL, 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0)) TS_PCOD_45

        FROM BKG_CSTMS_TML_KR_CLL CLL,
             BKG_DG_CGO DG
        WHERE CLL.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no], 2, 3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd], 3, 3)
          AND CLL.POD_CD IS NOT NULL
          AND NVL(CLL.POL_YD_CD, ' ') like @[in_pol_yd_cd]||'%'
          AND CLL.BKG_NO = DG.BKG_NO(+)
          AND CLL.CNTR_NO = DG.CNTR_NO(+)
#if (${in_cll_type} == 'LOCAL')
          AND CLL.KR_CLL_TS_CD IS NULL 
#elseif (${in_cll_type} == 'TS')
          AND CLL.KR_CLL_TS_CD IS NOT NULL 
#end
        GROUP BY CLL.POD_CD, KR_CLL_TS_CD, MTY_BKG_CD, CLL.CNTR_TPSZ_CD, DG.CNTR_NO, CLL_RMK2, KR_TML_PRCT_ID 
    )
    GROUP BY POD_CD 
)			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
