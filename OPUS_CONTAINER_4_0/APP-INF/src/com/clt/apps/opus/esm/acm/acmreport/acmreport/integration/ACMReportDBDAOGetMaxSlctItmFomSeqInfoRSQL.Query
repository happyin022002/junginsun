<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMReportDBDAOGetMaxSlctItmFomSeqInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(SLCT_ITM_FOM_SEQ), 0) + 1 AS SLCT_ITM_FOM_SEQ
  FROM AGT_RPT_ITM_INFO_MST
 WHERE CRE_USR_ID = @[usr_id]
			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
