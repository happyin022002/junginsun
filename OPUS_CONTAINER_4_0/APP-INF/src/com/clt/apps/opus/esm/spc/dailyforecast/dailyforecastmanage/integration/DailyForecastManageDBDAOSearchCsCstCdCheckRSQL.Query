<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSearchCsCstCdCheckRSQL">
			<desc><![CDATA[CUST_CNT_CD, CUST_SEQ 유효성을 체크한다.]]></desc>
			<sql><![CDATA[
SELECT 
       DECODE(COUNT(*),0,'N','Y') CUST_CNT_CD_YN 
  FROM 
       MDM_CUSTOMER 
 WHERE 1=1
       AND CUST_CNT_CD = @[cust_cnt_cd] 
       AND CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
