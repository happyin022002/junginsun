<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiIbNoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT MAX(DECODE(BKG_REF_TP_CD, 'ESRF', CUST_REF_NO_CTNT)) IB_NO
FROM   BKG_REFERENCE
WHERE  BKG_NO = @[bkg_no]
GROUP BY BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
