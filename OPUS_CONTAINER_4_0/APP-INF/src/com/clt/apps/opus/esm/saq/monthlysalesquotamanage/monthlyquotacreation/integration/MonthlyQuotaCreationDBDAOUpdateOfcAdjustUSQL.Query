<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOUpdateOfcAdjustUSQL">
			<desc><![CDATA[해당 버전의 Office Adjust Data를 수정한다.]]></desc>
			<sql><![CDATA[
MERGE INTO SAQ_MON_FCAST_TRNS 
USING DUAL 
ON ( MQTA_MDL_VER_NO = @[mqta_mdl_ver_no] 
       AND TRD_CD = @[trd_cd] 
       AND SUB_TRD_CD = @[sub_trd_cd] 
       AND IOC_CD = @[ioc_cd] 
       AND RLANE_CD = @[rlane_cd] 
       AND DIR_CD = @[dir_cd] 
       AND CTRT_OFC_CD = @[ctrt_ofc_cd] 
       AND SLS_OFC_CD = @[sls_ofc_cd]
       AND (FCAST_TRNS_STS_CD = '0' OR FCAST_TRNS_STS_CD IS NULL)) 
WHEN MATCHED THEN  
    UPDATE 
           SET LOD_QTY = @[lod_qty] 
         , MDL_ALOC_QTY = @[lod_qty] 
         , CM_UC_AMT = @[cm_uc_amt] 
         , GRS_RPB_REV = @[grs_rpb_rev] 
         , UPD_USR_ID = @[user_id] 
         , UPD_DT = SYSDATE			]]></sql>
			<params>
				<param name="mqta_mdl_ver_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="sls_ofc_cd" type="12" value="" out="N"/>
				<param name="lod_qty" type="12" value="" out="N"/>
				<param name="cm_uc_amt" type="12" value="" out="N"/>
				<param name="grs_rpb_rev" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
