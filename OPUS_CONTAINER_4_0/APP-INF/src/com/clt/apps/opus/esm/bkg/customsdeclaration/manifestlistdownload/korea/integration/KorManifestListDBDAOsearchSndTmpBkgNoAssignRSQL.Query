		<query name="KorManifestListDBDAOsearchSndTmpBkgNoAssignRSQL">
			<desc><![CDATA[생성된 BKG_NO 와 SEQ 결합]]></desc>
			<sql><![CDATA[
SELECT @[bkg_no] || LPAD(@[gen_seq], 4, 0) BKG_NO
  FROM DUAL			]]></sql>
