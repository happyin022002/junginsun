<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOHblCmTmpltRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT B.OFC_CD
,      B.PROF_SEQ
,      B.PROF_SUB_SEQ
,      B.CSTMS_DESC
,      B.HAMO_TRF_CD
,      B.DELT_FLG
,      B.CRE_USR_ID
,      B.CRE_DT
,      B.UPD_USR_ID
,      B.UPD_DT
FROM   BKG_NVOCC_PROF A, BKG_NVOCC_PROF_CNTR_MF B
WHERE  A.OFC_CD = B.OFC_CD
AND    A.PROF_SEQ = B.PROF_SEQ
AND    B.DELT_FLG = 'N'
AND    A.OFC_CD = @[ofc_cd]
AND    UPPER(A.ACT_SHPR_NM) like '%' || UPPER(@[act_shpr_nm]) || '%'
AND    UPPER(A.ULTI_CNEE_NM) like '%' || UPPER(@[ulti_cnee_nm]) || '%'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="act_shpr_nm" type="12" value="" out="N"/>
				<param name="ulti_cnee_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
