<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInquiryDBDAOFmsContractSumRSQL">
			<desc><![CDATA[Fleet Status Sum Select]]></desc>
			<sql><![CDATA[
SELECT '' FLET_CTRT_NO,
	   DECODE(AAAAA.NUM,1,'TI','') FLET_CTRT_TP_CD,
	   '' VSL_CD,
	   '' VSL_ENG_NM,
	   DECODE(AAAAA.NUM,1,'Count: '||DDDDD.OWNR_NM||'','') OWNR_NM,
	   '' VSL_CNT_CD,
	   '' VSL_BLD_DT,
	   DDDDD.VSL_DZND_CAPA,
	   DDDDD.BSE_14TON_VSL_CAPA,
	   '' EFF_DT, 
	   '' EXP_DT,
	   BBBBB.HIR_CURR_N1ST_CD,
	   BBBBB.HIR_RT_N1ST_AMT,
	   CCCCC.HIR_CURR_N2ND_CD,
	   CCCCC.HIR_RT_N2ND_AMT,
	   '' SLAN_CD,
	   '' GR_FLG,
	   '' RF_CNTR_PLG_QTY,
	   '' SHP_SPD_QTY,
	   DDDDD.DDWT_CGO_CAPA_QTY,
	   DDDDD.GRS_WGT,
	   DDDDD.NRT_WGT,
	   '' CHTR_PRD_OPT_CTNT,
	   '' RDE_RNG_CTNT,
	   '' RDE_NTC_CTNT
  FROM (SELECT 1 NUM FROM DUAL
	    UNION
	    SELECT 2 NUM FROM DUAL
	    UNION
	    SELECT 3 NUM FROM DUAL
	    UNION
	    SELECT 4 NUM FROM DUAL
	    UNION
	    SELECT 5 NUM FROM DUAL
	    UNION
	    SELECT 6 NUM FROM DUAL
	    UNION
	    SELECT 7 NUM FROM DUAL
	    UNION
	    SELECT 8 NUM FROM DUAL
	    UNION
	    SELECT 9 NUM FROM DUAL) AAAAA,
	   (SELECT ROWNUM NUM,
			   AAAA.HIR_CURR_N1ST_CD,
			   AAAA.HIR_RT_N1ST_AMT
		  FROM (SELECT DDD.HIR_CURR_N1ST_CD,
					   SUM(DDD.HIR_RT_N1ST_AMT) HIR_RT_N1ST_AMT
				  FROM (SELECT VSL_CD,SLAN_CD 
                        FROM 
                          (SELECT VSL_CD,SLAN_CD,ROW_NUMBER() OVER(PARTITION BY VSL_CD ORDER BY VPS_ETD_DT DESC) RK
                           FROM VSK_VSL_PORT_SKD
                           WHERE VPS_ETD_DT IS NOT NULL
#if (${period_flag} == 'date')
                             AND TO_CHAR(VPS_ETD_DT,'YYYYMMDD') <= @[sch_dt_to]
#elseif (${period_flag} == 'month')
                            AND TO_CHAR(VPS_ETD_DT,'YYYYMM') <= @[sch_dt_to]
#elseif (${period_flag} == 'year')
                            AND TO_CHAR(VPS_ETD_DT,'YYYY') <= @[sch_dt_to]
#end
                          ) 
                         WHERE RK = 1
				       ) AAA,
				       (SELECT AA.FLET_CTRT_NO,
					   		   AA.FLET_CTRT_TP_CD,
					   		   DECODE(DD.VSL_CD,NULL,AA.VSL_CD,DD.VSL_CD) VSL_CD,
							   DECODE(BB.OWNR_NM,NULL,CC.OWNR_NM,BB.OWNR_NM) OWNR_NM,
							   DECODE(BB.OWNR_SEQ,NULL,CC.OWNR_SEQ,BB.OWNR_SEQ) OWNR_SEQ,
							   AA.VSL_CNT_CD,
							   AA.VSL_BLD_DT,
							   AA.VSL_DZND_CAPA,
							   AA.BSE_14TON_VSL_CAPA,
							   TO_CHAR(AA.EFF_DT,'yyyymmdd') EFF_DT, 
							   TO_CHAR(AA.EXP_DT,'yyyymmdd') EXP_DT,
							   AA.GR_FLG,
							   AA.RF_CNTR_PLG_QTY,
							   AA.SHP_SPD_QTY,
							   AA.DDWT_CGO_CAPA_QTY,
							   AA.GRS_WGT,
							   AA.NRT_WGT,
							   AA.CHTR_PRD_OPT_CTNT,
							   AA.RDE_RNG_CTNT,
							   AA.RDE_NTC_CTNT
						  FROM FMS_CONTRACT AA,
							   (SELECT A.OWNR_SEQ, A.OWNR_NM, B.VNDR_SEQ
								  FROM FMS_OWNER A,
								  	   MDM_VENDOR B
								 WHERE A.OWNR_SEQ = B.FLET_MGMT_OWNR_VNDR_SEQ) BB,
							   (SELECT A.OWNR_SEQ, A.OWNR_NM, B.CUST_CNT_CD, B.CUST_SEQ
								  FROM FMS_OWNER A,
								  	   MDM_CUSTOMER B
								 WHERE A.OWNR_SEQ = B.FLET_MGMT_OWNR_CUST_SEQ) CC,
							   (SELECT FLET_CTRT_NO, VSL_CD, ROW_NUMBER() OVER(PARTITION BY FLET_CTRT_NO ORDER BY FLET_CTRT_NO DESC) ROW_NUM
							      FROM FMS_ID_VSL 
							     WHERE FLET_RPT_FLG = 'Y') DD
						 WHERE AA.VNDR_SEQ = BB.VNDR_SEQ(+)
						   AND AA.CUST_CNT_CD = CC.CUST_CNT_CD(+)
						   AND AA.CUST_SEQ = CC.CUST_SEQ(+)
						   AND AA.FLET_CTRT_TP_CD = 'TI'
						   AND NVL(AA.DELT_FLG,'N') = 'N'
						   AND AA.FLET_CTRT_FACT_CD = 'ACT'
						   AND AA.FLET_CTRT_NO = DD.FLET_CTRT_NO(+)
						   AND DD.ROW_NUM(+) = 1
#if (${period_flag} == 'date')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymmdd') AND TO_CHAR(AA.EXP_DT, 'yyyymmdd')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymmdd') AND TO_CHAR(AA.EXP_DT, 'yyyymmdd')
		        				OR (    AA.EFF_DT >= TO_DATE(@[sch_dt],'YYYYMMDD')
                    				AND AA.EXP_DT <= TO_DATE(@[sch_dt_to],'YYYYMMDD')))
#elseif (${period_flag} == 'month')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymm') AND TO_CHAR(AA.EXP_DT, 'yyyymm')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymm') AND TO_CHAR(AA.EXP_DT, 'yyyymm')
								OR (    TO_CHAR(AA.EFF_DT,'YYYYMM') >= @[sch_dt]
                    				AND TO_CHAR(AA.EXP_DT,'YYYYMM') <= @[sch_dt_to]))
#elseif (${period_flag} == 'year')
 						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyy') AND TO_CHAR(AA.EXP_DT, 'yyyy')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyy') AND TO_CHAR(AA.EXP_DT, 'yyyy'))
#end
#if (${flet_ctrt_tp_cd} != '')
						   AND AA.FLET_CTRT_TP_CD = @[flet_ctrt_tp_cd]
#end
#if (${vsl_size_flag} == 'max')
						   AND AA.VSL_DZND_CAPA BETWEEN @[vsl_size1] AND @[vsl_size2]
#elseif (${vsl_size_flag} == '14ton')
						   AND AA.BSE_14TON_VSL_CAPA BETWEEN @[vsl_size1] AND @[vsl_size2]
#end
#if (${gr_flg} != '')
						   AND AA.GR_FLG = @[gr_flg]
#end
					   ) BBB,
					   (SELECT AA.FLET_CTRT_NO,
						  	   AA.HIR_CURR_N1ST_CD,
							   AA.HIR_RT_N1ST_AMT,
							   AA.HIR_CURR_N2ND_CD,
							   AA.HIR_RT_N2ND_AMT
						  FROM (SELECT A.FLET_CTRT_NO,
						  			   A.HIR_CURR_N1ST_CD,
							   		   A.HIR_RT_N1ST_AMT,
							   		   A.HIR_CURR_N2ND_CD,
									   A.HIR_RT_N2ND_AMT,
							           RANK() OVER (PARTITION BY FLET_CTRT_NO ORDER BY (EXP_DT - EFF_DT) DESC) RK
						          FROM FMS_HIRE A
								 WHERE 1 = 1
								#if (${period_flag} == 'date')
								   AND (   @[sch_dt] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymmdd') AND TO_CHAR(A.EXP_DT, 'yyyymmdd')
										OR @[sch_dt_to] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymmdd') AND TO_CHAR(A.EXP_DT, 'yyyymmdd')
										OR (    A.EFF_DT >= TO_DATE(@[sch_dt],'YYYYMMDD')
											AND A.EXP_DT <= TO_DATE(@[sch_dt_to],'YYYYMMDD')))   
								#elseif (${period_flag} == 'month')
								   AND (   @[sch_dt] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymm') AND TO_CHAR(A.EXP_DT, 'yyyymm')
										OR @[sch_dt_to] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymm') AND TO_CHAR(A.EXP_DT, 'yyyymm')
										OR (    TO_CHAR(A.EFF_DT,'YYYYMM') >= @[sch_dt]
											AND TO_CHAR(A.EXP_DT,'YYYYMM') <= @[sch_dt_to]))
								#elseif (${period_flag} == 'year')
								   AND (   @[sch_dt] BETWEEN TO_CHAR(A.EFF_DT, 'yyyy') AND TO_CHAR(A.EXP_DT, 'yyyy')
										OR @[sch_dt_to] BETWEEN TO_CHAR(A.EFF_DT, 'yyyy') AND TO_CHAR(A.EXP_DT, 'yyyy'))
								#end
					           ) AA
					     WHERE AA.RK = 1
						   AND AA.HIR_CURR_N1ST_CD IS NOT NULL) DDD
				 WHERE BBB.VSL_CD = AAA.VSL_CD(+)
				   AND BBB.FLET_CTRT_NO = DDD.FLET_CTRT_NO
#if (${slan_cd} != '')
				   AND AAA.SLAN_CD = @[slan_cd]
#end
#if (${ownr_seq} != '')
				   AND BBB.OWNR_SEQ = @[ownr_seq]
#end
				 GROUP BY DDD.HIR_CURR_N1ST_CD
			   ) AAAA
       ) BBBBB,
	   (SELECT ROWNUM NUM,
			   AAAA.HIR_CURR_N2ND_CD,
			   AAAA.HIR_RT_N2ND_AMT
		  FROM (SELECT DDD.HIR_CURR_N2ND_CD,
					   SUM(DDD.HIR_RT_N2ND_AMT) HIR_RT_N2ND_AMT
				  FROM (SELECT VSL_CD,SLAN_CD 
                        FROM 
                          (SELECT VSL_CD,SLAN_CD,ROW_NUMBER() OVER(PARTITION BY VSL_CD ORDER BY VPS_ETD_DT DESC) RK
                           FROM VSK_VSL_PORT_SKD
                           WHERE VPS_ETD_DT IS NOT NULL
#if (${period_flag} == 'date')
                             AND TO_CHAR(VPS_ETD_DT,'YYYYMMDD') <= @[sch_dt_to]
#elseif (${period_flag} == 'month')
                            AND TO_CHAR(VPS_ETD_DT,'YYYYMM') <= @[sch_dt_to]
#elseif (${period_flag} == 'year')
                            AND TO_CHAR(VPS_ETD_DT,'YYYY') <= @[sch_dt_to]
#end
                          ) 
                         WHERE RK = 1
				       ) AAA,
				       (SELECT AA.FLET_CTRT_NO,
					   		   AA.FLET_CTRT_TP_CD,
					   		   DECODE(DD.VSL_CD,NULL,AA.VSL_CD,DD.VSL_CD) VSL_CD,
							   DECODE(BB.OWNR_NM,NULL,CC.OWNR_NM,BB.OWNR_NM) OWNR_NM,
							   DECODE(BB.OWNR_SEQ,NULL,CC.OWNR_SEQ,BB.OWNR_SEQ) OWNR_SEQ,
							   AA.VSL_CNT_CD,
							   AA.VSL_BLD_DT,
							   AA.VSL_DZND_CAPA,
							   AA.BSE_14TON_VSL_CAPA,
							   TO_CHAR(AA.EFF_DT,'yyyymmdd') EFF_DT, 
							   TO_CHAR(AA.EXP_DT,'yyyymmdd') EXP_DT,
							   AA.GR_FLG,
							   AA.RF_CNTR_PLG_QTY,
							   AA.SHP_SPD_QTY,
							   AA.DDWT_CGO_CAPA_QTY,
							   AA.GRS_WGT,
							   AA.NRT_WGT,
							   AA.CHTR_PRD_OPT_CTNT,
							   AA.RDE_RNG_CTNT,
							   AA.RDE_NTC_CTNT
						  FROM FMS_CONTRACT AA,
							   (SELECT A.OWNR_SEQ, A.OWNR_NM, B.VNDR_SEQ
								  FROM FMS_OWNER A,
								  	   MDM_VENDOR B
								 WHERE A.OWNR_SEQ = B.FLET_MGMT_OWNR_VNDR_SEQ) BB,
							   (SELECT A.OWNR_SEQ, A.OWNR_NM, B.CUST_CNT_CD, B.CUST_SEQ
								  FROM FMS_OWNER A,
								  	   MDM_CUSTOMER B
								 WHERE A.OWNR_SEQ = B.FLET_MGMT_OWNR_CUST_SEQ) CC,
							   (SELECT FLET_CTRT_NO, VSL_CD, ROW_NUMBER() OVER(PARTITION BY FLET_CTRT_NO ORDER BY FLET_CTRT_NO DESC) ROW_NUM
							      FROM FMS_ID_VSL 
							     WHERE FLET_RPT_FLG = 'Y') DD
						 WHERE AA.VNDR_SEQ = BB.VNDR_SEQ(+)
						   AND AA.CUST_CNT_CD = CC.CUST_CNT_CD(+)
						   AND AA.CUST_SEQ = CC.CUST_SEQ(+)
						   AND AA.FLET_CTRT_TP_CD = 'TI'
						   AND NVL(AA.DELT_FLG,'N') = 'N'
						   AND AA.FLET_CTRT_FACT_CD = 'ACT'
						   AND AA.FLET_CTRT_NO = DD.FLET_CTRT_NO(+)
						   AND DD.ROW_NUM(+) = 1
#if (${period_flag} == 'date')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymmdd') AND TO_CHAR(AA.EXP_DT, 'yyyymmdd')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymmdd') AND TO_CHAR(AA.EXP_DT, 'yyyymmdd')
		        				OR (    AA.EFF_DT >= TO_DATE(@[sch_dt],'YYYYMMDD')
                    				AND AA.EXP_DT <= TO_DATE(@[sch_dt_to],'YYYYMMDD')))
#elseif (${period_flag} == 'month')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymm') AND TO_CHAR(AA.EXP_DT, 'yyyymm')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymm') AND TO_CHAR(AA.EXP_DT, 'yyyymm')
								OR (    TO_CHAR(AA.EFF_DT,'YYYYMM') >= @[sch_dt]
                    				AND TO_CHAR(AA.EXP_DT,'YYYYMM') <= @[sch_dt_to]))
#elseif (${period_flag} == 'year')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyy') AND TO_CHAR(AA.EXP_DT, 'yyyy')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyy') AND TO_CHAR(AA.EXP_DT, 'yyyy'))
#end
#if (${flet_ctrt_tp_cd} != '')
						   AND AA.FLET_CTRT_TP_CD = @[flet_ctrt_tp_cd]
#end
#if (${vsl_size_flag} == 'max')
						   AND AA.VSL_DZND_CAPA BETWEEN @[vsl_size1] AND @[vsl_size2]
#elseif (${vsl_size_flag} == '14ton')
						   AND AA.BSE_14TON_VSL_CAPA BETWEEN @[vsl_size1] AND @[vsl_size2]
#end
#if (${gr_flg} != '')
						   AND AA.GR_FLG = @[gr_flg]
#end
					   ) BBB,
					   (SELECT AA.FLET_CTRT_NO,
						  	   AA.HIR_CURR_N1ST_CD,
							   AA.HIR_RT_N1ST_AMT,
							   AA.HIR_CURR_N2ND_CD,
							   AA.HIR_RT_N2ND_AMT
						  FROM (SELECT A.FLET_CTRT_NO,
						  			   A.HIR_CURR_N1ST_CD,
							   		   A.HIR_RT_N1ST_AMT,
							   		   A.HIR_CURR_N2ND_CD,
									   A.HIR_RT_N2ND_AMT,
							           RANK() OVER (PARTITION BY FLET_CTRT_NO ORDER BY (EXP_DT - EFF_DT) DESC, EFF_DT DESC) RK
						          FROM FMS_HIRE A
								 WHERE 1 = 1
								#if (${period_flag} == 'date')
								   AND (   @[sch_dt] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymmdd') AND TO_CHAR(A.EXP_DT, 'yyyymmdd')
										OR @[sch_dt_to] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymmdd') AND TO_CHAR(A.EXP_DT, 'yyyymmdd')
										OR (    A.EFF_DT >= TO_DATE(@[sch_dt],'YYYYMMDD')
											AND A.EXP_DT <= TO_DATE(@[sch_dt_to],'YYYYMMDD')))   
								#elseif (${period_flag} == 'month')
								   AND (   @[sch_dt] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymm') AND TO_CHAR(A.EXP_DT, 'yyyymm')
										OR @[sch_dt_to] BETWEEN TO_CHAR(A.EFF_DT, 'yyyymm') AND TO_CHAR(A.EXP_DT, 'yyyymm')
										OR (    TO_CHAR(A.EFF_DT,'YYYYMM') >= @[sch_dt]
											AND TO_CHAR(A.EXP_DT,'YYYYMM') <= @[sch_dt_to]))
								#elseif (${period_flag} == 'year')
								   AND (   @[sch_dt] BETWEEN TO_CHAR(A.EFF_DT, 'yyyy') AND TO_CHAR(A.EXP_DT, 'yyyy')
										OR @[sch_dt_to] BETWEEN TO_CHAR(A.EFF_DT, 'yyyy') AND TO_CHAR(A.EXP_DT, 'yyyy'))
								#end
					           ) AA
					     WHERE AA.RK = 1
						   AND AA.HIR_CURR_N2ND_CD IS NOT NULL) DDD
				 WHERE BBB.VSL_CD = AAA.VSL_CD(+)
				   AND BBB.FLET_CTRT_NO = DDD.FLET_CTRT_NO
#if (${slan_cd} != '')
				   AND AAA.SLAN_CD = @[slan_cd]
#end
#if (${ownr_seq} != '')
				   AND BBB.OWNR_SEQ = @[ownr_seq]
#end
				 GROUP BY DDD.HIR_CURR_N2ND_CD
			   ) AAAA
       ) CCCCC,
	   (SELECT ROWNUM NUM,
	   		   OWNR_NM,
			   VSL_DZND_CAPA,
			   BSE_14TON_VSL_CAPA,
			   DDWT_CGO_CAPA_QTY,
			   GRS_WGT,
			   NRT_WGT
		  FROM (SELECT COUNT(BBB.OWNR_SEQ) OWNR_NM,
					   SUM(BBB.VSL_DZND_CAPA) VSL_DZND_CAPA,
				 	   SUM(BBB.BSE_14TON_VSL_CAPA) BSE_14TON_VSL_CAPA,
				 	   SUM(BBB.DDWT_CGO_CAPA_QTY) DDWT_CGO_CAPA_QTY,
				 	   SUM(BBB.GRS_WGT) GRS_WGT,
				 	   SUM(BBB.NRT_WGT) NRT_WGT
				  FROM (SELECT VSL_CD,SLAN_CD 
                        FROM 
                          (SELECT VSL_CD,SLAN_CD,ROW_NUMBER() OVER(PARTITION BY VSL_CD ORDER BY VPS_ETD_DT DESC) RK
                           FROM VSK_VSL_PORT_SKD
                           WHERE VPS_ETD_DT IS NOT NULL
#if (${period_flag} == 'date')
                             AND TO_CHAR(VPS_ETD_DT,'YYYYMMDD') <= @[sch_dt_to]
#elseif (${period_flag} == 'month')
                            AND TO_CHAR(VPS_ETD_DT,'YYYYMM') <= @[sch_dt_to]
#elseif (${period_flag} == 'year')
                            AND TO_CHAR(VPS_ETD_DT,'YYYY') <= @[sch_dt_to]
#end
                          ) 
                         WHERE RK = 1
					   ) AAA,
					   (SELECT AA.FLET_CTRT_NO,
							   AA.FLET_CTRT_TP_CD,
							   DECODE(DD.VSL_CD,NULL,AA.VSL_CD,DD.VSL_CD) VSL_CD,
							   DECODE(BB.OWNR_NM,NULL,CC.OWNR_NM,BB.OWNR_NM) OWNR_NM,
							   DECODE(BB.OWNR_SEQ,NULL,CC.OWNR_SEQ,BB.OWNR_SEQ) OWNR_SEQ,
							   AA.VSL_CNT_CD,
							   AA.VSL_BLD_DT,
							   AA.VSL_DZND_CAPA,
							   AA.BSE_14TON_VSL_CAPA,
							   TO_CHAR(AA.EFF_DT,'yyyymmdd') EFF_DT, 
							   TO_CHAR(AA.EXP_DT,'yyyymmdd') EXP_DT,
							   AA.GR_FLG,
							   AA.RF_CNTR_PLG_QTY,
							   AA.SHP_SPD_QTY,
						       AA.DDWT_CGO_CAPA_QTY,
							   AA.GRS_WGT,
							   AA.NRT_WGT,
							   AA.CHTR_PRD_OPT_CTNT,
							   AA.RDE_RNG_CTNT,
							   AA.RDE_NTC_CTNT
						  FROM FMS_CONTRACT AA,
							   (SELECT A.OWNR_SEQ, A.OWNR_NM, B.VNDR_SEQ
								  FROM FMS_OWNER A,
									   MDM_VENDOR B
								 WHERE A.OWNR_SEQ = B.FLET_MGMT_OWNR_VNDR_SEQ) BB,
							   (SELECT A.OWNR_SEQ, A.OWNR_NM, B.CUST_CNT_CD, B.CUST_SEQ
								  FROM FMS_OWNER A,
									   MDM_CUSTOMER B
								 WHERE A.OWNR_SEQ = B.FLET_MGMT_OWNR_CUST_SEQ) CC,
							   (SELECT FLET_CTRT_NO, VSL_CD, ROW_NUMBER() OVER(PARTITION BY FLET_CTRT_NO ORDER BY FLET_CTRT_NO DESC) ROW_NUM
							      FROM FMS_ID_VSL 
							     WHERE FLET_RPT_FLG = 'Y') DD
						 WHERE AA.VNDR_SEQ = BB.VNDR_SEQ(+)
						   AND AA.CUST_CNT_CD = CC.CUST_CNT_CD(+)
						   AND AA.CUST_SEQ = CC.CUST_SEQ(+)
						   AND AA.FLET_CTRT_TP_CD = 'TI'
						   AND NVL(AA.DELT_FLG,'N') = 'N'
						   AND AA.FLET_CTRT_FACT_CD = 'ACT'
						   AND AA.FLET_CTRT_NO = DD.FLET_CTRT_NO(+)
						   AND DD.ROW_NUM(+) = 1
#if (${period_flag} == 'date')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymmdd') AND TO_CHAR(AA.EXP_DT, 'yyyymmdd')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymmdd') AND TO_CHAR(AA.EXP_DT, 'yyyymmdd')
		        				OR (    AA.EFF_DT >= TO_DATE(@[sch_dt],'YYYYMMDD')
                    				AND AA.EXP_DT <= TO_DATE(@[sch_dt_to],'YYYYMMDD')))
#elseif (${period_flag} == 'month')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymm') AND TO_CHAR(AA.EXP_DT, 'yyyymm')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyymm') AND TO_CHAR(AA.EXP_DT, 'yyyymm')
								OR (    TO_CHAR(AA.EFF_DT,'YYYYMM') >= @[sch_dt]
                    				AND TO_CHAR(AA.EXP_DT,'YYYYMM') <= @[sch_dt_to]))
#elseif (${period_flag} == 'year')
						   AND (   @[sch_dt] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyy') AND TO_CHAR(AA.EXP_DT, 'yyyy')
                				OR @[sch_dt_to] BETWEEN TO_CHAR(AA.EFF_DT, 'yyyy') AND TO_CHAR(AA.EXP_DT, 'yyyy'))
#end
#if (${flet_ctrt_tp_cd} != '')
						   AND AA.FLET_CTRT_TP_CD = @[flet_ctrt_tp_cd]
#end
#if (${vsl_size_flag} == 'max')
						   AND AA.VSL_DZND_CAPA BETWEEN @[vsl_size1] AND @[vsl_size2]
#elseif (${vsl_size_flag} == '14ton')
						   AND AA.BSE_14TON_VSL_CAPA BETWEEN @[vsl_size1] AND @[vsl_size2]
#end
#if (${gr_flg} != '')
						   AND AA.GR_FLG = @[gr_flg]
#end
					   ) BBB
