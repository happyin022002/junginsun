<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOmodifyCstmsCdConvSeqCtntUSQL">
			<desc><![CDATA[modifyCstmsCdConvCtnt]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_CD_CONV_CTNT
SET ATTR_CTNT1 = @[attr_ctnt1],
	ATTR_CTNT2 = @[attr_ctnt2],
	ATTR_CTNT3 = @[attr_ctnt3],
	ATTR_CTNT4 = @[attr_ctnt4],
	ATTR_CTNT5 = @[attr_ctnt5],
	UPD_USR_ID = @[user_id],
	UPD_DT = SYSDATE
WHERE CNT_CD = @[cnt_cd]
AND CSTMS_DIV_ID = @[cstms_div_id]
AND CSTMS_DIV_ID_SEQ = @[cstms_div_id_seq]

			]]></sql>
			<params>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="attr_ctnt3" type="12" value="" out="N"/>
				<param name="attr_ctnt4" type="12" value="" out="N"/>
				<param name="attr_ctnt5" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_div_id" type="12" value="" out="N"/>
				<param name="cstms_div_id_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
