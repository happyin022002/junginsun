<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOsearchIBUNLocCdRSQL">
			<desc><![CDATA[Inbound이기 때문에 POD에 대한 UNLOCODE 검색    ]]></desc>
			<sql><![CDATA[
SELECT NVL(UN_LOC_CD, LOC_CD) UN_POD_CD
  FROM MDM_LOCATION
 WHERE LOC_CD = @[pod_cd]			]]></sql>
			<params>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
