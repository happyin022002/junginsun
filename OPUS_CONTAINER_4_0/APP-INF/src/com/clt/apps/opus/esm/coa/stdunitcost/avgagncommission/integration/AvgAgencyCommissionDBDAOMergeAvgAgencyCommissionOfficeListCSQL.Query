<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AvgAgencyCommissionDBDAOMergeAvgAgencyCommissionOfficeListCSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
MERGE INTO ACM_COMM_OFC_UT_COST A
USING (
       SELECT '1' FROM DUAL 
	  ) A2	
ON (   A.COMM_YRMON         = @[comm_yrmon]
   AND A.OFC_CD             = @[ofc_cd]
   AND A.IO_BND_CD          = @[io_bnd_cd]
   AND A.AC_TP_CD           = @[ac_tp_cd]
   AND A.CNTR_TPSZ_CD       = @[cntr_tpsz_cd]
   AND A.COMM_STND_COST_CD  = @[comm_stnd_cost_cd]
   AND A.TRD_CD             = @[trd_cd]
   AND A.SUB_TRD_CD         = @[sub_trd_cd])	
WHEN MATCHED THEN
     UPDATE 
        SET A.BKG_QTY       = @[bkg_qty] 
          , A.COMM_TTL_AMT  = @[comm_ttl_amt] 
          , A.COMM_UT_AMT   = @[comm_ut_amt] 
          , A.UPD_USR_ID    = @[cre_usr_id] 
          , A.UPD_DT        = SYSDATE    
WHEN NOT MATCHED THEN 
    INSERT (
		   A.COMM_YRMON
         , A.OFC_CD
         , A.IO_BND_CD
         , A.AC_TP_CD
         , A.CNTR_TPSZ_CD
         , A.COMM_STND_COST_CD
         , A.TRD_CD
         , A.SUB_TRD_CD
         , A.BKG_QTY
         , A.COMM_TTL_AMT
         , A.COMM_UT_AMT
         , A.CRE_USR_ID
         , A.CRE_DT
         , A.UPD_USR_ID
         , A.UPD_DT
	    ) VALUES (
          @[comm_yrmon]
        , @[ofc_cd]
        , @[io_bnd_cd]
        , @[ac_tp_cd]
        , @[cntr_tpsz_cd]
        , @[comm_stnd_cost_cd]
        , @[trd_cd]
        , @[sub_trd_cd]
        , @[bkg_qty]
        , @[comm_ttl_amt]
        , @[comm_ut_amt]
        , @[cre_usr_id]
        , SYSDATE
        , @[cre_usr_id]
        , SYSDATE )			]]></sql>
			<params>
				<param name="comm_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ac_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="comm_stnd_cost_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="bkg_qty" type="12" value="" out="N"/>
				<param name="comm_ttl_amt" type="12" value="" out="N"/>
				<param name="comm_ut_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
