<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiFnlAta15RSQL">
			<desc><![CDATA[BLIssuanceDBDAOsearchDblEdiFnlAta15RSQL]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(MAX(ACT_DT), 'RRRRMMDDHH24MI', 'NLS_DATE_LANGUAGE=ENGLISH') AS DEL_ACT_ARRIVAL_DATE
  FROM SCE_COP_HDR HDR
      ,SCE_COP_DTL DTL
      ,BKG_BOOKING BK
 WHERE BK.BKG_NO = HDR.BKG_NO 
   AND HDR.COP_NO = DTL.COP_NO
   AND DTL.NOD_CD = BK.DEL_NOD_CD
   AND HDR.COP_STS_CD IN ('C','T','F')
   AND (   DTL.ACT_CD LIKE 'FI__A_'
        OR DTL.ACT_CD LIKE 'FU__U_') 
   AND BK.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
