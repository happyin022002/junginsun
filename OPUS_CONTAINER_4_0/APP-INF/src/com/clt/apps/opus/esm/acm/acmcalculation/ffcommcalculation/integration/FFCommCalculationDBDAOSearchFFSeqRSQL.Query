<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCommCalculationDBDAOSearchFFSeqRSQL">
			<desc><![CDATA[FFCommCalculationDBDAOSearchFFSeqRSQL]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(FF_CMPN_SEQ),0)+1 FF_CMPN_SEQ
FROM ACM_FF_CMPN
 WHERE  BKG_NO = @[bkg_no] 
	AND FF_CMPN_STS_CD = 'IF'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
