<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchBkgVvdRSQL">
			<desc><![CDATA[Select VVD of specified booking]]></desc>
			<sql><![CDATA[
SELECT LISTAGG(VSL_CD||SKD_VOY_NO||SKD_DIR_CD) 
       WITHIN GROUP (ORDER BY VSL_PRE_PST_CD,VSL_SEQ) 
       AS VVD 
FROM BKG_VVD
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
