<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchYrWkDuRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT
      MIN(COST_YR) AS COST_YR
    , MIN(COST_WK) AS COST_WK
    , COUNT(*)     AS CNT
  FROM COA_WK_PRD 
 WHERE 1=1
   #if (${in_mon_or_wk} == 'W') 
       AND COST_YR||COST_WK BETWEEN @[in_fm_wk] AND @[in_to_wk]
   #elseif (${in_mon_or_wk} == 'M') 
       AND SUBSTR(SLS_FM_DT,1,6) BETWEEN @[in_fm_mon] AND @[in_to_mon]
   #end			]]></sql>
			<params>
				<param name="in_fm_wk" type="12" value="" out="N"/>
				<param name="in_to_wk" type="12" value="" out="N"/>
				<param name="in_fm_mon" type="12" value="" out="N"/>
				<param name="in_to_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
