<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDBDAOSearchRevenueVvdListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.VSL_CD
     , A.SKD_VOY_NO
     , A.SKD_DIR_CD
     , A.REV_DIR_CD
     , A.REV_YRMON
     , A.SLAN_CD
     , A.RLANE_CD
     , A.VST_DT
     , A.VED_DT
     , A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD||A.REV_DIR_CD VVD_CD
     , A.CRE_USR_ID
     , A.UPD_USR_ID
     , NVL((SELECT 'Y'
              FROM MDM_REV_LANE M
             WHERE M.REP_TRD_CD = 'COM'
               AND M.RLANE_CD = A.RLANE_CD),'N') AS COM_VVD_FLG
  FROM FMS_VVD A
 WHERE A.REV_YRMON = REPLACE(@[rev_yrmon],'-')
   AND A.SLAN_CD LIKE @[slan_cd]||'%'
   AND A.RLANE_CD LIKE @[rlane_cd]||'%'
 ORDER BY A.REV_YRMON, A.SLAN_CD, A.VST_DT			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
