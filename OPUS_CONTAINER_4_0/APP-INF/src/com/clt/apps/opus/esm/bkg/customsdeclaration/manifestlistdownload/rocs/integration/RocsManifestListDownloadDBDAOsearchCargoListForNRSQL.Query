<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchCargoListForNRSQL">
			<desc><![CDATA[ROCS(ROTTERDAM) 세관 신고용 Booking No List를 조회한다. (B/L Creation Indicator = 'N')]]></desc>
			<sql><![CDATA[
SELECT BL.BKG_NO
FROM BKG_CSTMS_RTM_BL BL, BKG_VVD BV
WHERE 1=1
AND BL.BKG_NO = BV.BKG_NO
AND BL.VSL_CALL_REF_NO = @[crn_number]
AND BV.VSL_CD = @[vsl_cd]
AND BV.SKD_VOY_NO = @[skd_voy_no]
AND BV.SKD_DIR_CD = @[skd_dir_cd]
AND	(BV.POL_CD LIKE @[pol_cd]||'%' or BL.PRE_RLY_PORT_CD LIKE @[pol_cd]||'%')			]]></sql>
			<params>
				<param name="crn_number" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
