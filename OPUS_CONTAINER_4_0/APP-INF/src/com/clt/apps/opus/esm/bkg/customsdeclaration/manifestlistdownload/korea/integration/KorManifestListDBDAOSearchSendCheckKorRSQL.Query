<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchSendCheckKorRSQL">
			<desc><![CDATA[해당 seq로 조회된 VVD Info에서 Send여부를 check한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(MF_SND_DT,NULL,'N','Y') MF_SND_DT
  FROM BKG_CSTMS_KR_VVD_SMRY
 WHERE MRN_NO = @[new_mrn_no]
   AND MRN_CHK_NO = @[new_mrn_chk_no]
   AND VSL_CD = SUBSTR(@[vvd_cd],1,4)
   AND SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
   AND SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
   AND OB_DECL_TP_CD = @[createdtype]
   AND VVD_SEQ = @[kv_seq]
   AND DECODE(@[in_bound],'I',DECODE(LENGTH(@[pod_tml]),7,PORT_TML_CD,' '),DECODE(LENGTH(@[pol_tml]),7,PORT_TML_CD,' ')) = DECODE(@[in_bound],'I',DECODE(LENGTH(@[pod_tml]),7,@[pod_tml],' '),DECODE(LENGTH(@[pol_tml]),7,@[pol_tml],' '))			]]></sql>
			<params>
				<param name="new_mrn_no" type="12" value="" out="N"/>
				<param name="new_mrn_chk_no" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="createdtype" type="12" value="" out="N"/>
				<param name="kv_seq" type="12" value="" out="N"/>
				<param name="in_bound" type="12" value="" out="N"/>
				<param name="pod_tml" type="12" value="" out="N"/>
				<param name="pol_tml" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
