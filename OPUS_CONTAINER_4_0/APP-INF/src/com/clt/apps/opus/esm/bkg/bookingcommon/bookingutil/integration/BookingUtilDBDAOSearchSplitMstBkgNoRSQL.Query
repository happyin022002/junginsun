<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchSplitMstBkgNoRSQL">
			<desc><![CDATA[Master Booking번호를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  FM_BKG_NO
FROM    BKG_BOOKING
WHERE   BKG_NO = @[bkg_no]
AND     BKG_CRE_TP_CD = 'S'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
