<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchImpCgoManiPrtCNTRInfoRSQL">
			<desc><![CDATA[Import Cargo Manifest Print를 위한 Container Information 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(CNTR.CNTR_NO,' ')||'-'||NVL(CNTR.CNTR_TPSZ_CD,' ')||CHR(10)||'/'||NVL(SEAL.CNTR_SEAL_NO,' ') CNTR_INFO
  FROM BKG_CONTAINER CNTR, BKG_CNTR_SEAL_NO SEAL
 WHERE CNTR.BKG_NO  = @[bkg_no]
   AND CNTR.BKG_NO  = SEAL.BKG_NO(+)
   AND CNTR.CNTR_NO = SEAL.CNTR_NO(+)
 ORDER BY CNTR.CNTR_NO, SEAL.CNTR_SEAL_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
