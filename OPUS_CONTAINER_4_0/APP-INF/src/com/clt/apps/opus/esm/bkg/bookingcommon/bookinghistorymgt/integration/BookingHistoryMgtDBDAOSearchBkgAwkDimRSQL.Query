<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgAwkDimRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgAwkDimRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
, AWK_CGO_SEQ
, DIM_SEQ
, DIM_LEN
, DIM_WDT
, DIM_HGT
, CRE_USR_ID
, TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, UPD_USR_ID
, TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
  FROM BKG_AWK_DIM
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
