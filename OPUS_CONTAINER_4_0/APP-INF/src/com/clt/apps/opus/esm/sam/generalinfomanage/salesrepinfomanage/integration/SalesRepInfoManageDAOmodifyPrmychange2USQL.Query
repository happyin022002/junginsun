<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesRepInfoManageDAOmodifyPrmychange2USQL">
			<desc><![CDATA[prmy 값이 N -> Y, Y->N 으로 변경하는 쿼리]]></desc>
			<sql><![CDATA[
UPDATE SAM_CUST_SLS_REP_INFO
SET SREP_PRMRY_FLG = @[srep_prmry_flg]
, upd_usr_id = @[usr_id]
, upd_dt = sysdate
WHERE CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]
AND SREP_CD = @[srep_cd]			]]></sql>
			<params>
				<param name="srep_prmry_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
