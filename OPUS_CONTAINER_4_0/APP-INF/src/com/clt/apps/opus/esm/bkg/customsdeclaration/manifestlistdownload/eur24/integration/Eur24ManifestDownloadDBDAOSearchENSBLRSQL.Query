<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOSearchENSBLRSQL">
			<desc><![CDATA[Eur24ManifestDownloadDBDAOSearchENSBLRSQL]]></desc>
			<sql><![CDATA[
/* ENS BL테이블에 존재하는지 조회 */
SELECT *
  FROM BKG_CSTMS_EUR_BL 
 WHERE VSL_CD= SUBSTR(@[p_vvd_cd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[p_vvd_cd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[p_vvd_cd], 9, 1) 
   AND MVMT_REF_NO IS NOT NULL
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="p_vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
