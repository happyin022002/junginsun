<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchRocsRcvHistByBlRSQL">
			<desc><![CDATA[rcv history]]></desc>
			<sql><![CDATA[
SELECT	DECODE(A.RTM_EDI_MSG_TP_CD,'A','ACCEPT','R','REJECT') RTM_EDI_MSG_TP_CD	,
		A.MSG_SND_DT,
		A.BL_NO,
		A.RTM_EDI_ERR_ID,
		A.ERR_DESC,
		A.ERR_CTNT,
		A.OFC_CD,
		A.CRE_USR_ID,
		ROWNUM	ROWCNT,
		@[sheet_vvd_number] VVD_NUMBER,
		@[sheet_pol_cd] POL_CD,
		@[sheet_pod_cd] POD_CD,
		'H' HIST
  FROM	BKG_CSTMS_RTM_EDI_LOG A
 WHERE	A.BL_NO	= @[sheet_bl_no]
   AND 	RCV_SND_DIV_CD = 'R'
   AND	A.MSG_SND_DT <> TO_DATE(@[sheet_msg_snd_dt],'YYYYMMDDHH24:MI:SS')
ORDER BY A.MSG_SND_DT			]]></sql>
			<params>
				<param name="sheet_vvd_number" type="12" value="" out="N"/>
				<param name="sheet_pol_cd" type="12" value="" out="N"/>
				<param name="sheet_pod_cd" type="12" value="" out="N"/>
				<param name="sheet_bl_no" type="12" value="" out="N"/>
				<param name="sheet_msg_snd_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
