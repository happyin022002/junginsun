<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchVVDListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT VVD CODE, SLAN_CD TEXT
FROM (SELECT ROW_NUMBER() OVER (	
ORDER BY SLAN_CD	
, VSL_CD || SKD_VOY_NO || SKD_DIR_CD
, TO_NUMBER(CLPT_SEQ)
) NO,
SLAN_CD
, VSL_CD || SKD_VOY_NO || SKD_DIR_CD VVD	
, VPS_PORT_CD
, VPS_ETA_DT
, VPS_ETD_DT
FROM VSK_VSL_PORT_SKD
WHERE 1=1
AND VSL_CD || SKD_VOY_NO || SKD_DIR_CD = @[vvd_cd] 
AND NVL(CNG_STS_CD, ' ') <> 'S'
AND CLPT_IND_SEQ = '1'		
) A	
WHERE no BETWEEN 1 AND 50
			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
