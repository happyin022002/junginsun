<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MultiDimensionRPTDBDAOModifyPnlRptItemUSQL">
			<desc><![CDATA[Update Query for ESM_COA_2003]]></desc>
			<sql><![CDATA[
UPDATE COA_PFIT_LSS_RPT_ITM 
SET		 SGRP_COST_CD = @[sgrp_cost_cd]
		,RPT_ITM_DESC = @[rpt_itm_desc]
		,LOCL_RPT_ITM_DESC = @[locl_rpt_itm_desc]
		,SGRP_COST_CD_DESC = @[sgrp_cost_cd_desc]
		,SGRP_LOCL_DESC = @[sgrp_locl_desc]
		,RPT_DP_SEQ = @[rpt_dp_seq]
		,RPT_DP_SGRP_SEQ = @[rpt_dp_sgrp_seq]
		,RPT_ITM_COLR_FLG = @[rpt_itm_colr_flg]
		,UPD_USR_ID = @[upd_usr_id]
		,UPD_DT = SYSDATE
WHERE RPT_VW_CD = @[rpt_vw_cd] 
   AND STND_COST_CD = @[stnd_cost_cd]			]]></sql>
			<params>
				<param name="sgrp_cost_cd" type="12" value="" out="N"/>
				<param name="rpt_itm_desc" type="12" value="" out="N"/>
				<param name="locl_rpt_itm_desc" type="12" value="" out="N"/>
				<param name="sgrp_cost_cd_desc" type="12" value="" out="N"/>
				<param name="sgrp_locl_desc" type="12" value="" out="N"/>
				<param name="rpt_dp_seq" type="12" value="" out="N"/>
				<param name="rpt_dp_sgrp_seq" type="12" value="" out="N"/>
				<param name="rpt_itm_colr_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rpt_vw_cd" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
