<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOSearchDgForSpecialRequestCheckRSQL">
			<desc><![CDATA[Search DG data to check special request]]></desc>
			<sql><![CDATA[
SELECT   BKG.BKG_NO
		,BKG.VSL_CD
		,BKG.SKD_VOY_NO
		,BKG.SKD_DIR_CD
		,BKG.POL_CD
		,BKG.POD_CD
		,VSL.CRR_CD
		,'' SLAN_CD
		,'' OPT_CLSS
		,DG.IMDG_UN_NO
		,DG.IMDG_UN_NO_SEQ
		,DG.IMDG_CLSS_CD
		,DG.IMDG_EXPT_QTY_FLG
		,DG.IMDG_LMT_QTY_FLG
		,DG.IMDG_SEGR_GRP_NO
		,DG.DG_CNTR_SEQ 
		,DG.CNTR_CGO_SEQ
 
FROM BKG_BOOKING BKG
	,BKG_DG_CGO DG
	,MDM_VSL_CNTR VSL

WHERE 0=0
AND BKG.BKG_NO = @[bkg_no]
AND BKG.BKG_NO = DG.BKG_NO
AND BKG.VSL_CD = VSL.VSL_CD
AND NVL(DG.SPCL_CGO_APRO_CD, 'A') <> 'C'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
