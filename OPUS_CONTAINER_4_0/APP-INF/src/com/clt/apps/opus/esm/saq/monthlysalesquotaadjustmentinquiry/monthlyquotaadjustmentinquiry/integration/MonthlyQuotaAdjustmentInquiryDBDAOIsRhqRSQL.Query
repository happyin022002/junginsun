<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentInquiryDBDAOIsRhqRSQL">
			<desc><![CDATA[isRhq]]></desc>
			<sql><![CDATA[
SELECT
      DECODE(NVL(MIN(OFC_CD), 'X'), 'X', 'X', 'O') AS IS_RHQ
FROM  SAQ_ORGANIZATION_V
WHERE LVL = 2
AND   OFC_CD = @[orgCd]						]]></sql>
			<params>
				<param name="orgCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
