<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAORemoveBkgTroSpclCgoSeqDSQL">
			<desc><![CDATA[TransferOrderIssueDBDAORemoveBkgTroSpclCgoSeqDSQL]]></desc>
			<sql><![CDATA[
DELETE 
  FROM BKG_TRO_SPCL_CGO_SEQ
 WHERE BKG_NO      = @[bkg_no]
   AND IO_BND_CD   = @[io_bnd_cd]
   AND RTN_TRO_FLG = @[rtn_tro_flg]
#if (${tro_seq} != '') 
   AND TRO_SEQ     = @[tro_seq]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="rtn_tro_flg" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
