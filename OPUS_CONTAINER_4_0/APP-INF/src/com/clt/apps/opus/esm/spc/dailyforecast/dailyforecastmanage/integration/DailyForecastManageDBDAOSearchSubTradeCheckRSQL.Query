<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSearchSubTradeCheckRSQL">
			<desc><![CDATA[SearchSubTradeCheck]]></desc>
			<sql><![CDATA[
select DECODE(COUNT(*),0,'N','Y') SUB_TRADE_YN
from MDM_SUB_TRD
where SUB_TRD_CD=@[sub_trd_cd]
			]]></sql>
			<params>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
