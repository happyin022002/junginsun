<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFADurationProposalDBDAOPriRpScpDurAmdVOCSQL">
			<desc><![CDATA[RFADurationProposalDBPriRpScpDurAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_DUR(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    CTRT_EFF_DT,
    CTRT_EXP_DT,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    PROP_NO			,
    amdt_seq+1		,
    SVC_SCP_CD		,
    CTRT_EFF_DT		,
    CTRT_EXP_DT, 
	PRC_PROG_STS_CD	 ,
    SRC_INFO_CD	,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    @[cre_usr_id]	,
    sysdate			,
    @[upd_usr_id]	,
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ
FROM
    PRI_RP_SCP_DUR
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
