<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOCheckYardValidRSQL">
			<desc><![CDATA[Full Container Release Order Email 전송을 위한 YD 유요성 검사 작업을 수행한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y') AS IS_YD
FROM MDM_YARD
WHERE YD_CD= @[yd_cd]			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
