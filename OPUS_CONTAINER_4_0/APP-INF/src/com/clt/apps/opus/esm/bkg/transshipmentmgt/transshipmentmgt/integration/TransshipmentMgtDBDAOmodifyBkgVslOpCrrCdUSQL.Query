<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransshipmentMgtDBDAOmodifyBkgVslOpCrrCdUSQL">
			<desc><![CDATA[bkg_vsl_op_crr_cd에 update한다.]]></desc>
			<sql><![CDATA[
UPDATE bkg_vsl_op_crr_cd SET 
	op_nm = @[op_nm]
,	upd_usr_id = @[upd_usr_id]
,	upd_dt = sysdate
WHERE	op_cd = @[op_cd]			]]></sql>
			<params>
				<param name="op_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="op_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
