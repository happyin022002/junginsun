<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HongKongCustomsTransmissionDBDAOsearchCntrListRSQL">
			<desc><![CDATA[홍콩세관 신고용 Manifest Container List 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  CNTR.CNTR_NO cntr_no  ,
	NVL(MIN(SEAL.CNTR_SEAL_NO),'') CNTR_SEAL_NO,
	NVL(CNTR.CNTR_TPSZ_CD,'') CNTR_TPSZ_CD,
	NVL(BKG.BKG_CGO_TP_CD,'') BKG_CGO_TP_CD,
	DECODE(nvl(BKG.RC_FLG,'N'),'Y',1,'N','0') RC_FLG
FROM BKG_BOOKING BKG, BKG_VVD VVD, BKG_CONTAINER CNTR, BKG_CNTR_SEAL_NO SEAL
WHERE VVD.VSL_CD     = @[vsl_cd]
AND   VVD.SKD_VOY_NO = @[skd_voy_no]
AND   VVD.SKD_DIR_CD = @[skd_dir_cd]
AND   BKG.BKG_NO = VVD.BKG_NO 
AND   BKG.BKG_NO IN (${ibflag})
#if (${pol_cd}!= '')
              AND VVD.POL_CD = @[pol_cd]
#end
#if (${pod_cd}!= '')
              AND VVD.POD_CD = @[pod_cd]
#end  
AND   BKG.BKG_NO = CNTR.BKG_NO
AND   CNTR.BKG_NO = SEAL.BKG_NO(+)
AND   CNTR.CNTR_NO = SEAL.CNTR_NO(+)
--AND   SEAL.CNTR_SEAL_SEQ(+) = '1'
AND   (BKG.BKG_STS_CD = 'F' OR BKG.BKG_STS_CD = 'W')
AND   BKG.BKG_CGO_TP_CD <> 'P'
AND   BKG.BL_NO IS NOT NULL
group by CNTR.CNTR_NO, CNTR.CNTR_TPSZ_CD, BKG.BKG_CGO_TP_CD, BKG.RC_FLG
order by CNTR.CNTR_NO			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
