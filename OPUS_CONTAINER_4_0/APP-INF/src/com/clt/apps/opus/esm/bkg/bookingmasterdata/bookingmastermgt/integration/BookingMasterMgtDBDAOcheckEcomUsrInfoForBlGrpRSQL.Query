<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOcheckEcomUsrInfoForBlGrpRSQL">
			<desc><![CDATA[Check the customer is still included in BL Group in ECOM]]></desc>
			<sql><![CDATA[
SELECT 'X'
FROM DUAL
WHERE EXISTS (SELECT 1 
                FROM BKG_XTER_USR_INFO
               WHERE 1=1
                 AND CUST_CNT_CD = @[cust_cnt_cd]
                 AND CUST_SEQ = @[cust_seq]
                 AND BL_GRP_SEQ = @[bl_grp_seq])			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="bl_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
