<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchKorCllListRSQL">
			<desc><![CDATA[searchKorCllList]]></desc>
			<sql><![CDATA[
SELECT 
	ROWNUM SEQ,
	VSL_CD,
	SKD_VOY_NO,
	SKD_DIR_CD,
	POL_CD,
	BKG_POL_CD,
	CNTR_NO,
	BL_NO,
	BKG_NO,
	CNTR_TPSZ_CD,
	MTY_BKG_CD,
	SEAL_NO,
	BL_WGT, 
	WGT_UT_CD,
	RCV_TERM_CD,
	TS_FLG,
	IMDG_CLSS_CD,
	IMDG_UN_NO,
	CDO_TEMP,
	CNTR_VENT_CD,
	HAMO_TRF_CD,
	CMDT_HS_CD,
	CLL_RMK1,
	CLL_RMK2,
	POD_CD,
	BLCK_STWG_CD,
	TS_VSL_CD||TS_SKD_VOY_NO||TS_SKD_DIR_CD TS_VVD_CD,
	(SELECT VSL_ENG_NM||' '||TS_SKD_VOY_NO||TS_SKD_DIR_CD FROM MDM_VSL_CNTR WHERE VSL_CD = TS_VSL_CD) TS_VSL_NM,
	OVR_LEN_QTY,
	OVR_WGT_QTY,
	OVR_HGT_QTY,
	MIN_TEMP,
	MAX_TEMP,
	KR_TML_PRCT_ID,
	POD_YD_CD,
	POL_YD_CD,
	MRN_POLUT_FLG,
	STWG_CD,
	XTER_RMK,
	A_POD_CD,
	T_VSL_CD,
	BKG_POL_CD,
    DECODE(POD_CD, 'SADMM', 'SADMM',
	    DECODE(KR_CLL_POD_CD,NULL,DECODE(BLCK_STWG_CD,NULL,POD_CD,DECODE(LENGTH(BLCK_STWG_CD),2,SUBSTR(POD_CD,1,3)||BLCK_STWG_CD,1,SUBSTR(POD_CD,1,4)||BLCK_STWG_CD)),
		    DECODE(BLCK_STWG_CD,NULL,POD_CD,DECODE(LENGTH(BLCK_STWG_CD),2,SUBSTR(KR_CLL_POD_CD,1,3)||BLCK_STWG_CD,1,SUBSTR(KR_CLL_POD_CD,1,4)||BLCK_STWG_CD))
    )) EDI_POD_CD,
	VGM_WGT,
	VGM_WGT_UT_CD,
	ESIG_CO_NM,
	VGM_DOC_TP_CD
FROM
	(	
SELECT 
	--CNTR_LIST_NO,
	A.VSL_CD,
	A.SKD_VOY_NO,
	A.SKD_DIR_CD,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.POL_CD),A.POL_CD) POL_CD,
	--A.POL_CD,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.BKG_POL_CD),A.POL_CD) BKG_POL_CD,
	--A.BKG_POL_CD,
	A.CNTR_NO,
	A.BL_NO,
	A.BKG_NO,
	A.CNTR_TPSZ_CD,
	A.MTY_BKG_CD,
	A.SEAL_NO,
	A.BL_WGT,
	A.WGT_UT_CD,
	SUBSTR(NVL(A.RCV_TERM_CD,' ')||NVL(A.DE_TERM_CD,' '),1,2) RCV_TERM_CD,
	A.KR_CLL_TS_CD TS_FLG,
	DECODE(A.MTY_BKG_CD,'M','2',DECODE(A.KR_CLL_TS_CD, 'TS', '3', 'TT', '4', '1')) AA,
	--UN_LOC_CD,
	A.IMDG_CLSS_ID IMDG_CLSS_CD ,
	A.TML_IMDG_ID IMDG_UN_NO,
	A.CDO_TEMP,
	A.CNTR_VENT_ID CNTR_VENT_CD,
	CM.HAMO_TRF_CD,
	CM.CMDT_HS_CD,
	A.CLL_RMK1,
	A.CLL_RMK2,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.POD_CD),A.POD_CD) POD_CD,
	--A.POD_CD,
	A.BLCK_STWG_CD,
	--HOT_FLG,
	A.TS_VSL_CD,
	A.TS_SKD_VOY_NO,
	A.TS_SKD_DIR_CD,
	--CUST_CNT_CD,
	A.OVR_LEN_QTY,
	A.OVR_WGT_QTY,
	A.OVR_HGT_QTY,
	A.MIN_TEMP,
	A.MAX_TEMP,
	--UPD_DT,
	A.KR_TML_PRCT_ID,
	A.POD_YD_CD,
	A.POL_YD_CD,
	'' MRN_POLUT_FLG,
	--B.STWG_CD,
	--DECODE(A.CLL_RMK1, 'INGU' , '' , B.STWG_CD) STWG_CD,
	A.STWG_CD STWG_CD,
	'' XTER_RMK,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.BKG_POD_CD),A.BKG_POD_CD) A_POD_CD,
	--A.BKG_POD_CD A_POD_CD,
	'' T_VSL_CD,
	'' VVD_CD_NM,	
	'' POL_CD_PRINT,
	'' VPS_ETD,
	(SELECT ATTR_CTNT2 FROM BKG_CSTMS_CD_CONV_CTNT WHERE CNT_CD = 'KR' AND CSTMS_DIV_ID = 'KR_CLL_EDI_POD_CD' AND ATTR_CTNT1 = A.POD_CD) KR_CLL_POD_CD,
	A.VGM_WGT,
	A.VGM_WGT_UT_CD,
	DECODE(C.XTER_SNDR_ID,'WEB',(SELECT ESIG_CO_NM 
                                   FROM BKG_XTER_VGM VGM
		                          WHERE VGM.BKG_NO = C.XTER_VGM_DOC_ID 
                                    AND VGM.CNTR_NO = C.CNTR_NO 
                                    AND VGM.VGM_SEQ = C.XTER_VGM_RQST_SEQ
                                    AND VGM.USR_ID = C.XTER_VGM_USR_ID
									AND VGM.VGM_CRE_GDT = (SELECT MAX(VGM_CRE_GDT)
															 FROM BKG_XTER_VGM
															WHERE BKG_NO = VGM.BKG_NO
														      AND CNTR_NO = VGM.CNTR_NO
															  AND ACT_TP_CD = 'I')
								    AND ROWNUM = 1)
                               ,(SELECT VGM_CUST_CNTC_NM 
                                   FROM BKG_XTER_VGM_CUST 
                                  WHERE XTER_SNDR_ID = C.XTER_SNDR_ID 
                                    AND XTER_VGM_DOC_ID = C.XTER_VGM_DOC_ID 
                                    AND XTER_VGM_RQST_SEQ = C.XTER_VGM_RQST_SEQ 
                                    AND CNTR_NO = C.CNTR_NO
                                    AND VGM_CUST_CNTC_TP_CD = 'RP'
									AND ROWNUM = 1)
           ) ESIG_CO_NM,
    NVL(DECODE(C.XTER_SNDR_ID,'WEB','SM1'
                               ,(SELECT VGM_DOC_TP_CD 
                                   FROM BKG_XTER_VGM_CUST 
                                  WHERE XTER_SNDR_ID = C.XTER_SNDR_ID 
                                    AND XTER_VGM_DOC_ID = C.XTER_VGM_DOC_ID 
                                    AND XTER_VGM_RQST_SEQ = C.XTER_VGM_RQST_SEQ
									AND CNTR_NO = C.CNTR_NO
									AND VGM_DOC_TP_CD IN ('SM1','SM2')
                                    AND ROWNUM = 1)
          ),'SM1') VGM_DOC_TP_CD
FROM 
	BKG_CSTMS_TML_KR_CLL A,BKG_BOOKING B, BKG_CNTR_MF_DESC CM
	,BKG_CONTAINER C
#if (${in_pol_yd_cd} == '') 
WHERE A.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no],2,3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd],3,3)
#elseif (${in_pol_yd_cd} != '') 
WHERE A.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no],2,3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd],3,3)
AND A.POL_YD_CD = @[in_pol_yd_cd]
#end
AND A.BKG_NO = B.BKG_NO(+)
#if (${in_bkg_sts_cd}!= '') 			
AND B.BKG_STS_CD = @[in_bkg_sts_cd]
#end
AND A.BKG_NO = C.BKG_NO(+)
AND A.CNTR_NO = C.CNTR_NO(+)	
#if (${in_cntr_cfm_flg}!= '') 		
AND C.CNTR_CFM_FLG = @[in_cntr_cfm_flg]
#end
AND A.BKG_NO = CM.BKG_NO (+)
AND A.CNTR_NO = CM.CNTR_NO (+)
AND CM.CNTR_MF_SEQ (+) = 1
#if (${in_cll_type} == 'TS')
AND A.KR_CLL_TS_CD IS NOT NULL
AND A.MTY_BKG_CD = 'F'
#elseif (${in_cll_type} == 'LOCAL')
AND A.KR_CLL_TS_CD IS NULL
AND A.MTY_BKG_CD = 'F'
#elseif (${in_cll_type} == 'EMPTY')
AND A.MTY_BKG_CD = 'M'
#end
#if (${in_sort_type} == '' ) 
order by AA, A.POL_CD, A.POD_CD, A.CNTR_NO
#elseif (${in_sort_type} == '1' ) 
order by AA, A.POL_CD, A.POD_CD, A.CNTR_NO
#elseif (${in_sort_type} == '2' ) 
order by AA, A.POL_CD, A.POD_CD, A.BKG_POD_CD
#elseif (${in_sort_type} == '3' ) 
order by AA, A.TS_VSL_CD||A.TS_SKD_VOY_NO||A.TS_SKD_DIR_CD
#elseif (${in_sort_type} == '4' ) 
order by AA, DECODE(A.BLCK_STWG_CD,'LOC','11','ONE','12','TWO','13','THR','14','FOR','15','FIV', '15.5', 'HOT','16','TRS','17', '18')
#elseif (${in_sort_type} == '5' ) 
order by AA, A.POD_CD, DECODE(A.BLCK_STWG_CD,'LOC','11','ONE','12','TWO','13','THR','14','FOR','15', 'FIV', '15.5', 'HOT','16','TRS','17', '18'), A.CNTR_TPSZ_CD, A.CNTR_NO
#elseif (${in_sort_type} == '6' ) 
order by A.CNTR_NO
#elseif (${in_sort_type} == '7' ) 
order by TO_NUMBER(A.BL_WGT,'99990')
#end
) T			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
				<param name="in_bkg_sts_cd" type="12" value="" out="N"/>
				<param name="in_cntr_cfm_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
