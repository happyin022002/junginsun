<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAOSearchCustomerGroupCodeDetailRSQL">
			<desc><![CDATA[Group Code에 해당하는 Customer Detail 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD
     , CUST_SEQ
     , CUST_CNT_CD||TRIM(TO_CHAR(CUST_SEQ,'000000')) CUST_CD
     , CUST_LGL_ENG_NM
     , LOC_CD
     , SREP_CD
     , OFC_CD
FROM MDM_CUSTOMER
WHERE 1=1
AND CUST_GRP_ID = @[cust_grp_id]			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
