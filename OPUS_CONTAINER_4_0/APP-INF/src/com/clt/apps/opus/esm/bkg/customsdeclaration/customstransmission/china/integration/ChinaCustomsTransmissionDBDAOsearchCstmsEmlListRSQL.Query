<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaCustomsTransmissionDBDAOsearchCstmsEmlListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
       TO_EML_CTNT,
       CC_EML_CTNT

  FROM BKG_CSTMS_EML_NTFC

 WHERE CNT_CD = 'CN'
   AND EDI_MSG = @[edi_msg]
   AND EDI_MSG_TP_ID = @[edi_msg_tp_id]
   AND (POL_CD = ' ' OR POL_CD = @[pol_cd])
   AND (POD_CD = ' ' OR POD_CD = @[pod_cd])			]]></sql>
			<params>
				<param name="edi_msg" type="12" value="" out="N"/>
				<param name="edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
