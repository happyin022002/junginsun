<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOSearchApPayInvDtlRSQL">
			<desc><![CDATA[SearchApPayInvDtl 정보를 가져옴]]></desc>
			<sql><![CDATA[
SELECT
	'I'							 AS IBFLAG,
    AA.ACCT                         AS ACCT_CD,
    AA.ACCT                         AS LGS_COST_CD,
    SUBSTR(AA.VVD,1,4)           AS VSL_CD,
    SUBSTR(AA.VVD,5,4)           AS SKD_VOY_NO,
    SUBSTR(AA.VVD,9,1)           AS SKD_DIR_CD,
    SUBSTR(AA.VVD,10,1)          AS REV_DIR_CD,
    AA.REV_VVD                      AS ACT_VVD_CD,
    AA.YARD                         AS YD_CD,
    AA.TPSZ                         AS CNTR_TPSZ_CD,
    AA.INV_AMT                      AS INV_AMT,
    CASE
    WHEN AA.QTY = '2'
    THEN AA.QTY
    ELSE NULL
    END                             AS SO_20FT_QTY,
    CASE
    WHEN AA.QTY = '2'
    THEN NULL
    ELSE AA.QTY
    END                             AS SO_40FT_QTY,    
    @[cre_usr_id]                   AS CRE_USR_ID,
    SYSDATE                         AS CRE_DT,
    @[cre_usr_id]                   AS UPD_USR_ID,
    SYSDATE                         AS UPD_DT,
    AA.INV_DESC                     AS INV_DESC
FROM
(
    SELECT
           Y.CSR_NO,
           ROW_NUMBER() OVER (ORDER BY X.ATT1, X.CMPY, X.REGION, X.CENTER, X.ACCT, X.VVD) AS LINE_SEQ,
           DENSE_RANK() OVER (ORDER BY X.ATT1, X.CMPY, X.REGION, X.CENTER, X.ACCT, X.VVD) AS LINE_NUMBER,
           X.LOOKUP,
           X.INV_AMT,
           X.INV_DESC,
           X.TAX_CD,
           X.CMPY,
           X.REGION,
           X.CENTER,
           X.ACCT,
           X.VVD,
           X.INTR_CMPY,
           X.FUTR1,
           X.FUTR2,
           X.ATT_CTLG,
           X.ATT1,
           X.ATT2,
           X.ATT3,
           X.ATT4,
           X.ATT5,
           X.ATT6,
           X.ATT7,
           X.ATT8,
           X.ATT9,
           X.ATT10,
           X.ATT11,
           X.ATT12,
           X.ATT13,
           X.ATT14,
           X.ATT15,
           X.BKG_NO,
           X.TPSZ,
           X.REV_VVD,
           X.DIV_CD,
           X.CARRIER,
           X.YARD,
           X.COST_CODE,
           X.QTY,
           X.TMNL_CD,
           X.AGNT,
           X.SUB_FLG,
           SYSDATE                                                                       AS EAI_EVNT_DT,
           @[cre_usr_id]                                                                 AS CRE_USR_ID,
           SYSDATE                                                                       AS CRE_DT
      FROM
         ( /* General + Other Commission */
               SELECT
                      A.VNDR_SEQ AS VNDR,
                      'ITEM' AS LOOKUP,
                  CASE A.CURR_CD
                  WHEN 'JPY'
                  THEN ROUND(NVL(B.ACT_LOCL_COMM_AMT, A.ACT_IF_LOCL_COMM_AMT), 0)
                  WHEN 'TWD'
                  THEN ROUND(NVL(B.ACT_LOCL_COMM_AMT, A.ACT_IF_LOCL_COMM_AMT), 0)
                  ELSE ROUND(NVL(B.ACT_LOCL_COMM_AMT, A.ACT_IF_LOCL_COMM_AMT), 2)
                   END                                                                   AS INV_AMT,
                    (     SELECT
                                 SUBSTR (ACCT_ENG_NM, 1, 39)
                            FROM MDM_ACCOUNT
                           WHERE ACCT_CD = A.COMM_STND_COST_CD
                    )
                   || ' '
                   || SUBSTR (TRIM (A.OTR_COMM_ACCT_CTNT), 1, 200)                       AS INV_DESC,
                      ''                                                                 AS TAX_CD,
                      '01'  AS CMPY,
                    (     SELECT
                                 NVL(MAX(FINC_RGN_CD), '00')
                            FROM MDM_ORGANIZATION
                           WHERE OFC_CD = A.AP_OFC_CD
                    )                                                                    AS REGION,
                    (     SELECT
                                 AP_CTR_CD
                            FROM MDM_ORGANIZATION
                           WHERE OFC_CD = A.AP_OFC_CD
                    )                                                                    AS CENTER,
                      A.COMM_STND_COST_CD ACCT,
                    (     SELECT
                            CASE SUBSTR(REV_VVD_CD, 0, 2)
                            WHEN 'FD'
                            THEN 'CFDR'||SUBSTR(REV_VVD_CD, 3, 4)||'EE'
                            ELSE REV_VVD_CD
                             END
                            FROM AGT_COMM_BKG_INFO
                           WHERE BKG_NO = A.BKG_NO
                    )                                                                    AS VVD,
                    (     SELECT NVL(LTRIM(SUBS_CO_CD), '00')
                            FROM MDM_VENDOR
                           WHERE VNDR_SEQ = A.VNDR_SEQ
                    )                                                                    AS INTR_CMPY,
                      '000000'                                                           AS FUTR1,
                      '000000'                                                           AS FUTR2,
                      A.COMM_STND_COST_CD                                                AS ATT_CTLG,
                      NVL(A.INV_NO, A.COMM_APRO_NO)                                      AS ATT1,
                      SUBSTR(@[gl_dt], 0, 4)
                   || '/'
                   || SUBSTR(@[gl_dt], 5, 2)
                   || '/'
                   || SUBSTR(@[gl_dt], 7, 2)
                   || ' 00:00:00'                                                        AS ATT2,
                      A.COMM_OCCR_INFO_CD                                                AS ATT3,
                      ''                                                                 AS ATT4,
                      ''                                                                 AS ATT5,
                      ''                                                                 AS ATT6,
                      ''                                                                 AS ATT7,
                      ''                                                                 AS ATT8,
                      ''                                                                 AS ATT9,
                      ''                                                                 AS ATT10,
                      ''                                                                 AS ATT11,
                      ''                                                                 AS ATT12,
                      ''                                                                 AS ATT13,
                      ''                                                                 AS ATT14,
                      ''                                                                 AS ATT15,
                      A.BKG_NO,
                      B.CNTR_TPSZ_CD                                                     AS TPSZ,
                 CASE A.COMM_SLAN_CD||SUBSTR(A.COMM_VSL_CD, 0, 2)
                 WHEN 'RBCFD'
                 THEN 'CFDR'
                   || SUBSTR (A.COMM_VSL_CD, 3, 2)
                   || SUBSTR (COMM_SKD_VOY_NO, 0, 2)
                   || 'EE'
                 ELSE A.COMM_VSL_CD
                   || A.COMM_SKD_VOY_NO
                   || A.COMM_SKD_DIR_CD
                   || NVL(A.COMM_REV_DIR_CD, A.COMM_SKD_DIR_CD)
                  END                                                                    AS REV_VVD,
                      'C'                                                                AS DIV_CD,
                      ''                                                                 AS CARRIER,
                      ''                                                                 AS YARD,
                      ''                                                                 AS COST_CODE,
                      B.BKG_VOL_QTY QTY,
                      ''                                                                 AS TMNL_CD,
                      A.AGN_CD AGNT,
                      A.OFC_CHR_LVL                                                      AS SUB_FLG,
                      A.CSR_NO                                                           AS CSR_NO
                 FROM AGT_AGN_COMM     A,
                      AGT_AGN_COMM_DTL B
                WHERE A.AR_OFC_CD         = @[ar_ofc_cd]
                  AND A.AGN_CD            = @[agn_cd]
                  AND A.AC_IF_DT         IS NULL
                  AND A.COMM_PROC_STS_CD  = 'AS'
#if(${com_apr_nos} != '')
                  AND A.COMM_APRO_NO
                   IN
                    (
                      ${com_apr_nos}
                    )
#end
                  AND A.COMM_APRO_NO     IS NOT NULL
                  AND A.BKG_NO            = B.BKG_NO(+)
                  AND A.AGN_CD            = B.AGN_CD(+)
                  AND A.IO_BND_CD         = B.IO_BND_CD(+)
                  AND A.AC_TP_CD          = B.AC_TP_CD(+)
                  AND A.AC_SEQ            = B.AC_SEQ(+)
            UNION ALL /* 상계정산 대리점 */
               SELECT
                      A.VNDR_SEQ AS VNDR,
                      'ITEM' AS LOOKUP,
                 CASE MAX(A.CURR_CD)
                 WHEN 'JPY'
                 THEN SUM (-ROUND(NVL(B.ACT_LOCL_COMM_AMT, A.ACT_IF_LOCL_COMM_AMT), 0))
                 WHEN 'TWD'
                 THEN SUM (-ROUND(NVL(B.ACT_LOCL_COMM_AMT, A.ACT_IF_LOCL_COMM_AMT), 0))
                 ELSE SUM (-ROUND(NVL(B.ACT_LOCL_COMM_AMT, A.ACT_IF_LOCL_COMM_AMT), 2))
                  END                                                                    AS INV_AMT,
                    (     SELECT
                                 SUBSTR (ACCT_ENG_NM, 1, 39)
                            FROM MDM_ACCOUNT
                           WHERE ACCT_CD = '954113'
                    )
                   || ' '
                   || SUBSTR (TRIM (A.OTR_COMM_ACCT_CTNT), 1, 200)                       AS INV_DESC,
                      ''                                                                 AS TAX_CD,
                      '01'                                                               AS CMPY,
                    (     SELECT
                                 NVL(MAX(FINC_RGN_CD), '00')
                            FROM MDM_ORGANIZATION
                           WHERE OFC_CD = A.AP_OFC_CD
                    )                                                                    AS REGION,
                    (     SELECT
                                 AP_CTR_CD
                            FROM MDM_ORGANIZATION
                           WHERE OFC_CD = A.AP_OFC_CD
                    )                                                                    AS CENTER,
                      '954113'                                                           AS ACCT,
                      '0000000000'                                                       AS VVD,
                    (     SELECT NVL(LTRIM(SUBS_CO_CD), '00')
                            FROM MDM_VENDOR
                           WHERE VNDR_SEQ = A.VNDR_SEQ
                    )                                                                    AS INTR_CMPY,
                      '000000'                                                           AS FUTR1,
                      '000000'                                                           AS FUTR2,
                      '954113' AS ATT_CTLG,
                      ''                                                                 AS ATT1,
                      ''                                                                 AS ATT2,
                      A.COMM_OCCR_INFO_CD                                                AS ATT3,
                      ''                                                                 AS ATT4,
                      ''                                                                 AS ATT5,
                      ''                                                                 AS ATT6,
                      ''                                                                 AS ATT7,
                      ''                                                                 AS ATT8,
                      ''                                                                 AS ATT9,
                      ''                                                                 AS ATT10,
                      ''                                                                 AS ATT11,
                      ''                                                                 AS ATT12,
                      ''                                                                 AS ATT13,
                      ''                                                                 AS ATT14,
                      ''                                                                 AS ATT15,
                      ''                                                                 AS BKG_NO,
                      ''                                                                 AS TPSZ,
                      ''                                                                 AS REV_VVD,
                      ''                                                                 AS DIV_CD,
                      ''                                                                 AS CARRIER,
                      ''                                                                 AS YARD,
                      ''                                                                 AS COST_CODE,
                      0                                                                  AS QTY,
                      ''                                                                 AS TMNL_CD,
                      A.AGN_CD                                                           AS AGNT,
                      A.OFC_CHR_LVL                                                      AS SUB_FLG,
                      A.CSR_NO                                                           AS CSR_NO
                 FROM AGT_AGN_COMM     A,
                      AGT_AGN_COMM_DTL B
                WHERE A.AR_OFC_CD         = @[ar_ofc_cd]
                  AND A.AGN_CD            = @[agn_cd]
                  AND A.AC_IF_DT         IS NULL
                  AND A.COMM_PROC_STS_CD  = 'AS'
#if(${com_apr_nos} != '')
                  AND A.COMM_APRO_NO
                   IN
                    (
                      ${com_apr_nos}
                    )
#end
                  AND A.COMM_APRO_NO     IS NOT NULL
                  AND 'O'
                   IN
                    (     SELECT
                                 SO_IF_CD
                            FROM MDM_ORGANIZATION
                           WHERE OFC_CD = A.AGN_CD
                    )
                  AND A.BKG_NO            = B.BKG_NO(+)
                  AND A.AGN_CD            = B.AGN_CD(+)
                  AND A.IO_BND_CD         = B.IO_BND_CD(+)
                  AND A.AC_TP_CD          = B.AC_TP_CD(+)
                  AND A.AC_SEQ            = B.AC_SEQ(+)
             GROUP BY A.VNDR_SEQ,
                      A.AP_OFC_CD,
                      A.COMM_OCCR_INFO_CD,
                      A.AGN_CD,
                      A.OFC_CHR_LVL,
                      A.CSR_NO,
                      A.OTR_COMM_ACCT_CTNT
            UNION ALL /* VAT */
               SELECT
                      A.VNDR_SEQ                                                         AS VNDR,
                      'ITEM'                                                             AS LOOKUP,
                 CASE A.CURR_CD
                 WHEN 'JPY'
                 THEN ROUND (SUM (NVL (B.ACT_LOCL_COMM_AMT*NVL(A.INV_TAX_RT, 0)/100, A.ACT_IF_LOCL_COMM_AMT*NVL (A.INV_TAX_RT, 0)/100)), 0)
                 WHEN 'TWD'
                 THEN ROUND (SUM (NVL (B.ACT_LOCL_COMM_AMT*NVL(A.INV_TAX_RT, 0)/100, A.ACT_IF_LOCL_COMM_AMT*NVL (A.INV_TAX_RT, 0)/100)), 0)
                 ELSE ROUND (SUM (NVL (B.ACT_LOCL_COMM_AMT*NVL(A.INV_TAX_RT, 0)/100, A.ACT_IF_LOCL_COMM_AMT*NVL (A.INV_TAX_RT, 0)/100)), 2)
                  END                                                                    AS INV_AMT,
                      NVL (A.INV_NO, A.COMM_APRO_NO)
                   || ' '
                   || A.OTR_COMM_ACCT_CTNT                                               AS INV_DESC,
                      ''                                                                 AS TAX_CD,
                      '01'                                                               AS company,
                      @[finc_rgn_cd]                                                       AS REGION,
                      @[ap_ctr_cd]                                                       AS CENTER,
                      '111821'                                                           AS ACCT,
                      '0000000000'                                                       AS VVD,
                    (     SELECT
                                 NVL(LTRIM(SUBS_CO_CD), '00')
                            FROM MDM_VENDOR
                           WHERE VNDR_SEQ = A.VNDR_SEQ
                    )                                                                    AS INTR_CMPY,
                      '000000'                                                           AS FUTURE1,
                      '000000'                                                           AS FUTURE2,
                      '111821'                                                           AS ATT_CTLG,
                      NVL(A.INV_NO, A.COMM_APRO_NO)                                      AS ATT1,
                      SUBSTR (@[gl_dt], 0, 4)
                   || '/'
                   || SUBSTR (@[gl_dt], 5, 2)
                   || '/'
                   || SUBSTR (@[gl_dt], 7, 2)
                   || ' 00:00:00'                                                        AS ATT2,
                      A.COMM_OCCR_INFO_CD                                                AS ATT3,
                      ''                                                                 AS ATT4,
                      ''                                                                 AS ATT5,
                      ''                                                                 AS ATT6,
                      ''                                                                 AS ATT7,
                      ''                                                                 AS ATT8,
                      ''                                                                 AS ATT9,
                      ''                                                                 AS ATT10,
                      ''                                                                 AS ATT11,
                      ''                                                                 AS ATT12,
                      ''                                                                 AS ATT13,
                      ''                                                                 AS ATT14,
                      ''                                                                 AS ATT15,
                      ''                                                                 AS BKG_NO,
                      ''                                                                 AS TPSZ,
                      '0000000000'                                                       AS REV_VVD,
                      'C'                                                                AS DIV_CD,
                      ''                                                                 AS CARRIER,
                      ''                                                                 AS YARD,
                      ''                                                                 AS COST_CODE,
                      0                                                                  AS QTY,
                      ''                                                                 AS TMNL_CD,
                      A.AGN_CD AGNT,
                      A.OFC_CHR_LVL SUB_FLG,
                      A.CSR_NO CSR_NO
                 FROM AGT_AGN_COMM A,
                      AGT_AGN_COMM_DTL B
                WHERE A.AR_OFC_CD         = @[ar_ofc_cd]
                  AND A.AGN_CD            = @[agn_cd]
                  AND A.AC_IF_DT         IS NULL
                  AND A.COMM_PROC_STS_CD  = 'AS'
#if(${com_apr_nos} != '')
                  AND A.COMM_APRO_NO
                   IN
                    (
                      ${com_apr_nos}
                    )
#end
                  AND A.COMM_APRO_NO     IS NOT NULL
                  AND A.INV_TAX_RT       >  0
                  AND A.BKG_NO            = B.BKG_NO(+)
                  AND A.AGN_CD            = B.AGN_CD(+)
                  AND A.IO_BND_CD         = B.IO_BND_CD(+)
                  AND A.AC_TP_CD          = B.AC_TP_CD(+)
                  AND A.AC_SEQ            = B.AC_SEQ(+)
             GROUP BY A.CURR_CD,
                      VNDR_SEQ,
                      NVL (A.INV_NO, A.COMM_APRO_NO),
                      A.OTR_COMM_ACCT_CTNT,
                      TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS', SYSDATE, A.COMM_OCCR_INFO_CD), 'YYYYMMDD'),
                      A.COMM_OCCR_INFO_CD,
                      A.AGN_CD,
                      A.OFC_CHR_LVL,
                      A.CSR_NO
               HAVING SUM (NVL (B.ACT_LOCL_COMM_AMT*A.INV_TAX_RT, A.ACT_IF_COMM_AMT*A.INV_TAX_RT)) <> 0
         ) X,
         (
               SELECT
                      CSR_NO,
                      VNDR_NO
                 FROM AP_INV_HDR
                WHERE CSR_NO = @[csr_no]
         ) Y
     WHERE X.CSR_NO = Y.CSR_NO(+)
       AND X.VNDR   = Y.VNDR_NO(+)
)    AA			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="gl_dt" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="finc_rgn_cd" type="12" value="" out="N"/>
				<param name="ap_ctr_cd" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
