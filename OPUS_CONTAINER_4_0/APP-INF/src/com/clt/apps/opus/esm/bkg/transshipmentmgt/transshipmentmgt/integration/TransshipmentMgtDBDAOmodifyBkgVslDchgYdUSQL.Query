<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransshipmentMgtDBDAOmodifyBkgVslDchgYdUSQL">
			<desc><![CDATA[bkg_vsl_dchg_yd에 update한다.]]></desc>
			<sql><![CDATA[
UPDATE bkg_vsl_dchg_yd SET 
	yd_cd = @[yd_cd]
,	cvy_ref_no = @[cvy_ref_no]
,	uq_vsl_id_no = @[uq_vsl_id_no]
,	upd_usr_id = @[upd_usr_id]
,	upd_dt = sysdate
WHERE	vsl_cd = @[vsl_cd]
AND	skd_voy_no = @[skd_voy_no]
AND	skd_dir_cd = @[skd_dir_cd]
AND	clpt_ind_seq = @[skd_call_seq]
AND	port_cd = @[clpt_cd]			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="cvy_ref_no" type="12" value="" out="N"/>
				<param name="uq_vsl_id_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="skd_call_seq" type="12" value="" out="N"/>
				<param name="clpt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
