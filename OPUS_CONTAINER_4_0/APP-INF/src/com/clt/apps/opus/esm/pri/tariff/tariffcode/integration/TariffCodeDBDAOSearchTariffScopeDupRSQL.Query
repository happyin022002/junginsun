<sqls>
	<querys>
		<query name="TariffCodeDBDAOSearchTariffScopeDupRSQL">
			<desc><![CDATA[SearchTariffScopeDup]]></desc>
			<sql><![CDATA[
SELECT TRF_NO,SVC_SCP_CD,TRF_PFX_CD
FROM PRI_SVC_SCP_TRF
WHERE 	#if( $svc_scp_list.size() != 0 ) 
			(SVC_SCP_CD) IN (
		#foreach( ${key} in ${svc_scp_list}) 
			#if($velocityCount != 1 ) 
				UNION ALL
			#end
			SELECT 
					'$key'
			FROM DUAL
		#end
		)
	#end 			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
