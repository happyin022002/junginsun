<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchClptIndSeqRSQL">
			<desc><![CDATA[Select slpt_ind_seq of speciffied vvd and yard]]></desc>
			<sql><![CDATA[
SELECT  
	CLPT_IND_SEQ
	,CLPT_IND_SEQ_MATCH
FROM (
SELECT  CLPT_IND_SEQ
		,CASE WHEN CLPT_IND_SEQ = NVL(@[cltp_ind_seq], 'X') THEN '1'
              ELSE '0' END CLPT_IND_SEQ_MATCH
FROM VSK_VSL_PORT_SKD
WHERE 0=0
AND NVL(SKD_CNG_STS_CD,'A') <> 'S'
AND VSL_CD = SUBSTR(@[vvd], 1, 4)
AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
AND YD_CD = @[yd_cd]
)
ORDER BY CLPT_IND_SEQ_MATCH DESC 			]]></sql>
			<params>
				<param name="cltp_ind_seq" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
