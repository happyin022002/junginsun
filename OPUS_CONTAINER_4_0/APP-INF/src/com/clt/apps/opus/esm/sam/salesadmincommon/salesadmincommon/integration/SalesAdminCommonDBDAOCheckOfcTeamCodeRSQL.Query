<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesAdminCommonDBDAOCheckOfcTeamCodeRSQL">
			<desc><![CDATA[Office Team Code 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT OFC_TEAM_CD
FROM MDM_SLS_REP
WHERE OFC_TEAM_CD = @[ofc_team_cd]			]]></sql>
			<params>
				<param name="ofc_team_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
