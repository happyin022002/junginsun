<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPsaBkgCntrCntRSQL">
			<desc><![CDATA[PSA BKG CNTR의 CNTR Count를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  CNTR_KNT
FROM    BKG_CSTMS_PSA_CNTR
WHERE   BKG_NO      =   @[bkg_no]
AND     BKG_SEQ     =   @[bkg_seq]
AND     PSA_SER_NO  =   @[psa_ser_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="psa_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
