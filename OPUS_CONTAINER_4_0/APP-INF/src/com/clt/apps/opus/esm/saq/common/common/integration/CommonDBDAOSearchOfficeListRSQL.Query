<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchOfficeListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT OFC_CD CODE, OFC_ENG_NM TEXT 		
FROM (	
SELECT ROW_NUMBER() OVER (ORDER BY OFC_CD ASC) NO, OFC_CD, OFC_ENG_NM, OFC_KND_CD, LOC_CD 
FROM MDM_ORGANIZATION	
WHERE 1=1
AND OFC_CD LIKE @[ofc_cd] 
AND NVL(DELT_FLG, 'N') <> 'Y'          ) A	
WHERE NO BETWEEN 1 AND 50			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
