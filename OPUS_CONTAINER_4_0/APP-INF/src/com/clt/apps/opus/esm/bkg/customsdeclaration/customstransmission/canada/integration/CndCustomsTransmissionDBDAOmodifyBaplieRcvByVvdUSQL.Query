<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndCustomsTransmissionDBDAOmodifyBaplieRcvByVvdUSQL">
			<desc><![CDATA[수정]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_STWG_CNTR
SET ACK_RCV_STS_CD = 'A',
    RCV_DT = TO_DATE(@[rcv_dt],'YYYYMMDDHH24MISS'),
    UPD_DT = SYSDATE
WHERE VSL_CD   = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]
AND SND_DT IS NOT NULL
AND (RCV_DT <> TO_DATE(@[rcv_dt],'YYYYMMDDHH24MISS')  OR RCV_DT IS NULL) /* R 로 UPDATE 한 항목은 제외 */			]]></sql>
			<params>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
