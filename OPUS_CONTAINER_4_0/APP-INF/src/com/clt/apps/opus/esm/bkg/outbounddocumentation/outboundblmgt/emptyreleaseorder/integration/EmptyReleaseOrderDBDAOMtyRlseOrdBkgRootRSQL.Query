<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyReleaseOrderDBDAOMtyRlseOrdBkgRootRSQL">
			<desc><![CDATA[EmptyReleaseOrderDBDAOMtyRlseOrdBkgRootRSQL]]></desc>
			<sql><![CDATA[
SELECT    
		BB.BKG_NO
        ,TO_CHAR(BB.BKG_CRE_DT,'YYYYMMDDHH24MISS') AS BKG_CRE_DT
        ,DECODE(BB.BKG_STS_CD,'X','X',(SELECT DECODE(COUNT(*),0,'N','U') FROM BKG_HIS_MST BHM WHERE BHM.BKG_NO = BB.BKG_NO AND BHM.HIS_SEQ > 1)) AS Status
        ,BB.SLAN_CD
        ,VVD1.SLAN_CD AS SLAN_CD1
        ,VVD1.VSL_CD
        ,VSL.LLOYD_NO AS VVD_LLOYD_No
        ,REPLACE(VSL.VSL_ENG_NM,',',' ') AS VSL_ENG_NM
        ,VSL.CALL_SGN_NO AS VVD_Call_Sign
        ,VVD1.SKD_VOY_NO
        ,VVD1.SKD_DIR_CD
		,VSK_COMMON_PKG.GET_CSSM_VOY_NO_FNC(VVD1.VSL_CD,VVD1.SKD_VOY_NO,VVD1.SKD_DIR_CD,VVD1.POL_CD,'O') AS CONS_VOY
        ,TO_CHAR(VSK1.VPS_ETD_DT,'YYYYMMDDHH24MISS') AS VPS_ETD_DT
        ,TO_CHAR(VSK2.VPS_ETA_DT,'YYYYMMDDHH24MISS') AS VPS_ETA_DT
        ,BB.VSL_CD AS VSL_CD1
        ,TVSL.LLOYD_NO AS Trunk_Vessel_LLOYD_No
        ,REPLACE(TVSL.VSL_ENG_NM,',',' ') AS VSL_ENG_NM1
        ,TVSL.CALL_SGN_NO AS Trunk_Vessel_Call_Sign
        ,BB.SKD_VOY_NO AS SKD_VOY_NO1
        ,BB.SKD_DIR_CD AS SKD_DIR_CD_T
        ,REPLACE(POR.LOC_NM,',',' ') AS LOC_NM
        ,BB.POR_CD AS POR_OPSCODE
        ,POR.UN_LOC_CD
        ,BB.POR_NOD_CD
        ,REPLACE(POL.LOC_NM,',',' ') AS LOC_NM1
        ,POL.UN_LOC_CD AS UN_LOC_CD1
        ,BB.POL_NOD_CD
        ,TO_CHAR(VSK1.VPS_ETA_DT,'YYYYMMDDHH24MISS') AS VPS_ETA_DT1
        ,TO_CHAR(VSK2.VPS_ETD_DT,'YYYYMMDDHH24MISS') AS VPS_ETD_DT1
        ,(SELECT to_char(NVL(CLZ.MNL_SET_DT,CLZ.SYS_SET_DT) , 'YYYYMMDDHH24MISS')
                         FROM BKG_CLZ_TM CLZ 
                         WHERE CLZ.BKG_NO = BB.BKG_NO AND CLZ.CLZ_TP_CD = 'T' 
                         AND ROWNUM = 1) AS CUT_OFF_TIME_FIRST_POL
        ,REPLACE(POD.LOC_NM,',',' ') AS LOC_NM2
        ,POD.UN_LOC_CD AS UN_LOC_CD2
        ,BB.DEL_CD AS DEL_OPUS_CODE
        ,REPLACE(DEL.LOC_NM,',',' ') AS LOC_NM3
        ,DEL.UN_LOC_CD AS UN_LOC_CD3
        ,BB.RCV_TERM_CD||BB.DE_TERM_CD AS RCV_TERM_CD
        ,REPLACE(REPLACE(REPLACE(BB.XTER_RMK, CHR(10), ' '), CHR(13), ' '),',',' ') AS XTER_RMK
        ,REPLACE(REPLACE(REPLACE(BB.VNDR_RMK, CHR(10), ' '), CHR(13), ' '),',',' ') AS VNDR_RMK
        ,BB.CMDT_CD
        ,REPLACE(MC.CMDT_NM,',',' ') CMDT_NM
        ,BB.MTY_PKUP_YD_CD
        ,(SELECT REPLACE(REPLACE(REPLACE(MC.CUST_LGL_ENG_NM, CHR(10), ' '), CHR(13), ' '),',',' ') FROM MDM_CUSTOMER MC WHERE MC.CUST_CNT_CD = BCS.CUST_CNT_CD AND MC.CUST_SEQ = BCS.CUST_SEQ) AS CUST_NM
        ,(SELECT REPLACE(REPLACE(REPLACE(MC.CUST_LGL_ENG_NM, CHR(10), ' '), CHR(13), ' '),',',' ') FROM MDM_CUSTOMER MC WHERE MC.CUST_CNT_CD = BCF.CUST_CNT_CD AND MC.CUST_SEQ = BCF.CUST_SEQ) AS CUST_NM1
		,(SELECT REPLACE(REPLACE(REPLACE(MC.CUST_LGL_ENG_NM, CHR(10), ' '), CHR(13), ' '),',',' ') FROM MDM_CUSTOMER MC WHERE MC.CUST_CNT_CD = BCC.CUST_CNT_CD AND MC.CUST_SEQ = BCC.CUST_SEQ) AS CUST_NM2
        ,BCS.CUST_CNT_CD||BCS.CUST_SEQ AS CUST_CNT_CD1
        ,BCF.CUST_CNT_CD||BCF.CUST_SEQ AS CUST_CNT_CD2
        ,BCC.CUST_CNT_CD||BCC.CUST_SEQ AS CUST_CNT_CD3
        ,BB.BKG_STS_CD
        ,BB.RD_CGO_FLG 
        ,BB.RC_FLG
        ,BB.AWK_CGO_FLG
        ,BB.BB_CGO_FLG
        ,BB.HNGR_FLG
        ,REPLACE(REPLACE(REPLACE(DECODE(LENGTH((SELECT trim(BSC.STWG_RMK) FROM BKG_STWG_CGO BSC WHERE BSC.BKG_NO = BB.BKG_NO AND ROWNUM = 1)),0,'N',null,'N','Y'), CHR(10), ' '), CHR(13), ' '),',',' ') AS STWG_RMK
        ,(SELECT BSC.STWG_CD FROM BKG_STWG_CGO BSC WHERE BSC.BKG_NO = BB.BKG_NO AND ROWNUM = 1) AS STWG_CD
        ,BB.BKG_OFC_CD
        ,(SELECT REPLACE(BCP.CNTC_PSON_NM,',',' ') FROM BKG_CNTC_PSON BCP WHERE BB.BKG_NO = BCP.BKG_NO AND BCP.BKG_CNTC_PSON_TP_CD = 'BK' AND ROWNUM = 1) AS BKG_CONTACT_POINT
        ,BB.TWN_SO_NO
        ,BB.BLCK_STWG_CD
        ,to_char(BB.MTY_PKUP_DT, 'YYYYMMDDHH24MISS') AS MTY_PKUP_DATE
        ,(select to_char(COALESCE(mnl_set_dt, sys_set_dt, BB.mty_pkup_dt), 'YYYYMMDDHH24MISS')
            from bkg_clz_tm clz
           where clz.bkg_no = BB.bkg_no
             and CLZ_TP_CD = 'R') AS MTY_PKUP_DATE1
        ,SUBSTR(BB.MTY_PKUP_YD_CD,1,2) AS MTY_PKUP_YD_CD_T
        ,(SELECT REPLACE(MY.YD_NM,',',' ') FROM MDM_YARD MY WHERE MY.YD_CD = BB.MTY_PKUP_YD_CD AND ROWNUM = 1) AS FULL_RETURN_YARD_COUNTRY
        ,SUBSTR(BB.FULL_RTN_YD_CD,1,2) AS FULL_RTN_YD_CD
        ,(SELECT REPLACE(MY.YD_NM,',',' ') FROM MDM_YARD MY WHERE MY.YD_CD = BB.FULL_RTN_YD_CD AND ROWNUM = 1) AS FULL_RETURN_YARD_NAME
        ,REPLACE(REPLACE(REPLACE((SELECT BSC.STWG_RMK FROM BKG_STWG_CGO BSC WHERE BSC.BKG_NO = BB.BKG_NO AND ROWNUM = 1), CHR(10), ' '), CHR(13), ' '),',',' ') AS STOW_REMARK
        ,(select REPLACE(cust_ref_no_ctnt,',',' ') from bkg_reference refNo where BB.bkg_no = refNo.bkg_no and bkg_ref_tp_cd = 'XMRN') AS cust_ref_no_ctnt
        ,BB.SC_NO
        ,BB.RFA_NO
        ,NVL(BB.TAA_NO, ' ') AS TAA_NO
FROM BKG_BOOKING BB
     ,BKG_VVD VVD1
     ,MDM_VSL_CNTR VSL
     ,MDM_VSL_CNTR TVSL
     ,VSK_VSL_PORT_SKD VSK1
     ,VSK_VSL_PORT_SKD VSK2
     ,MDM_LOCATION POR
     ,MDM_LOCATION POL
     ,MDM_LOCATION POD
     ,MDM_LOCATION DEL
     ,MDM_COMMODITY MC
     ,BKG_CUSTOMER BCS
     ,BKG_CUSTOMER BCC
     ,BKG_CUSTOMER BCF
WHERE BB.BKG_NO = VVD1.BKG_NO
AND BB.POL_CD = VVD1.POL_CD
AND NVL(BB.EDI_HLD_FLG,'N') = 'N'
--AND BB.BKG_CGO_TP_CD IN ('F', 'R')
AND VVD1.VSL_CD = VSL.VSL_CD(+)
AND VVD1.VSL_CD = VSK1.VSL_CD(+)
AND VVD1.SKD_VOY_NO = VSK1.SKD_VOY_NO(+)
AND VVD1.SKD_DIR_CD = VSK1.SKD_DIR_CD(+)
AND VVD1.POL_CD = VSK1.VPS_PORT_CD(+)
AND VVD1.POL_CLPT_IND_SEQ = VSK1.CLPT_IND_SEQ(+)
AND VVD1.VSL_CD = VSK2.VSL_CD(+)
AND VVD1.SKD_VOY_NO = VSK2.SKD_VOY_NO(+)
AND VVD1.SKD_DIR_CD = VSK2.SKD_DIR_CD(+)
AND VVD1.POD_CD = VSK2.VPS_PORT_CD(+)
AND VVD1.POD_CLPT_IND_SEQ = VSK2.CLPT_IND_SEQ(+)
AND BB.VSL_CD = TVSL.VSL_CD(+)
AND BB.POR_CD = POR.LOC_CD
AND BB.POL_CD = POL.LOC_CD
AND BB.POD_CD = POD.LOC_CD
AND BB.DEL_CD = DEL.LOC_CD
AND BB.CMDT_CD = MC.CMDT_CD
AND BB.BKG_NO = BCS.BKG_NO
AND BCS.BKG_CUST_TP_CD = 'S'
AND BB.BKG_NO = BCC.BKG_NO(+)
AND BCC.BKG_CUST_TP_CD(+) = 'C'
AND BB.BKG_NO = BCF.BKG_NO(+)
AND BCF.BKG_CUST_TP_CD(+) = 'F'
#if (${datetype} == 'FTP')
    AND BB.BKG_CRE_DT > SYSDATE -61
    AND BB.BKG_OFC_CD = UPPER(@[bkg_ofc_cd])
    AND (EXISTS (SELECT 'X' 
    			FROM BKG_HIS_MST BHM
                WHERE BHM.CRE_DT BETWEEN to_date(@[from_dt] ,'yyyy-mm-dd hh24:mi:ss') AND to_date(@[end_dt] ,'yyyy-mm-dd hh24:mi:ss')
    			AND BHM.BKG_HIS_ISS_UI_ID IN (SELECT ATTR_CTNT1 FROM BKG_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'BKG_LDF_UI')
                AND BB.BKG_NO = BHM.BKG_NO)
      		OR EXISTS (SELECT 'X' FROM SCG_AUTHORIZATION SA WHERE SA.BKG_NO = BB.BKG_NO
                AND SA.SPCL_CGO_AUTH_CD = 'Y' 
                AND   SA.UPD_DT BETWEEN to_date(@[from_dt] ,'yyyy-mm-dd hh24:mi:ss') AND to_date(@[end_dt] ,'yyyy-mm-dd hh24:mi:ss')) 
    ) 

#else
    #if (${from_dt} != '')
      #if (${datetype} == 'BKG')
         AND BB.BKG_CRE_DT  BETWEEN to_date(@[from_dt] ,'yyyy-mm-dd hh24:mi') AND to_date(@[end_dt] ,'yyyy-mm-dd hh24:mi')
      #elseif(${datetype} == 'PUP')
         AND BB.MTY_PKUP_DT BETWEEN to_date(@[from_dt] ,'yyyy-mm-dd hh24:mi') AND to_date(@[end_dt] ,'yyyy-mm-dd hh24:mi')
      #end
    #end
    #if (${bkg_ofc_cd}!='')
       AND BB.BKG_OFC_CD = UPPER(@[bkg_ofc_cd])
    #end
    #if (${doc_usr_id}!='')
       AND UPPER(BB.DOC_USR_ID)  = UPPER(@[doc_usr_id])
    #end
    #if (${eq_ctrl_ofc_cd}!='')
       AND BB.EQ_CTRL_OFC_CD = UPPER(@[eq_ctrl_ofc_cd])
    #end
    #if (${vvd} !='')
       AND EXISTS (SELECT 'X' 
                   FROM BKG_VVD VVD 
                   WHERE BB.BKG_NO = VVD.BKG_NO
                   AND   VVD.VSL_PRE_PST_CD IN ('S','T')
                   AND VVD.VSL_CD = UPPER(substr(@[vvd],0,4))
                   AND VVD.SKD_VOY_NO = UPPER(substr(@[vvd],5,4))
                   AND VVD.SKD_DIR_CD = UPPER(substr(@[vvd],9,1))
                  )
    #end
    #if (${por_cd} !='')
       AND BB.POR_CD   = UPPER(@[por_cd])
    #end
    #if (${pol_cd} !='')
       AND BB.POL_CD   = UPPER(@[pol_cd])
    #end
    #if (${pod_cd}!='')
       AND BB.POD_CD   = UPPER(@[pod_cd])
    #end
    #if (${mty_pkup_yd_cd}!='')
       AND BB.MTY_PKUP_YD_CD = UPPER(@[mty_pkup_yd_cd])
    #end
    #if (${full_rtn_yd_cd}!='')
       AND BB.FULL_RTN_YD_CD = UPPER(@[full_rtn_yd_cd])
    #end
    #if (${bkg_no}!='')
       AND BB.BKG_NO   = UPPER(@[bkg_no])
    #end
    #if (${eq_confirm}=='Y')
       AND EXISTS (SELECT 1 FROM BKG_DOC_PROC_SKD WHERE BKG_NO = BB.BKG_NO AND BKG_DOC_PROC_TP_CD = 'CNTCFM' AND DOC_PERF_DELT_FLG = 'N')
    #end
    #if (${eq_confirm}=='N')
       AND NOT EXISTS (SELECT 1 FROM BKG_DOC_PROC_SKD WHERE BKG_NO = BB.BKG_NO AND BKG_DOC_PROC_TP_CD = 'CNTCFM' AND DOC_PERF_DELT_FLG = 'N')
    #end
#end

ORDER BY BB.BKG_NO			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="end_dt" type="12" value="" out="N"/>
				<param name="doc_usr_id" type="12" value="" out="N"/>
				<param name="eq_ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="mty_pkup_yd_cd" type="12" value="" out="N"/>
				<param name="full_rtn_yd_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
