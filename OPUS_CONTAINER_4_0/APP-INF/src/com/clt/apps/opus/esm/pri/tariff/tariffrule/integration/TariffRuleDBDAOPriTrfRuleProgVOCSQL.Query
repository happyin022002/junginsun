<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffRuleDBDAOPriTrfRuleProgVOCSQL">
			<desc><![CDATA[Tariff Rule Progress Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_RULE_PROG
(
       TRF_PFX_CD
     , TRF_NO
     , TRF_RULE_NO
     , AMDT_SEQ
     , TRF_RULE_PROG_SEQ
     , TRF_RULE_STS_CD
     , PROG_USR_ID
     , PROG_OFC_CD
     , PROG_DT
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
)
SELECT @[trf_pfx_cd]
     , @[trf_no]
     , @[trf_rule_no]
     , @[amdt_seq]
     , NVL(MAX(TRF_RULE_PROG_SEQ)+1, 1)
     , @[trf_rule_sts_cd]
     , @[prog_usr_id]
     , @[prog_ofc_cd]
     , SYSDATE
     , @[cre_usr_id]
     , SYSDATE
     , @[cre_usr_id]
     , SYSDATE
  FROM PRI_TRF_RULE_PROG 
 WHERE TRF_PFX_CD	= @[trf_pfx_cd]
   AND TRF_NO		= @[trf_no]
   AND TRF_RULE_NO	= @[trf_rule_no]
   AND AMDT_SEQ		= @[amdt_seq]			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_rule_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="trf_rule_sts_cd" type="12" value="" out="N"/>
				<param name="prog_usr_id" type="12" value="" out="N"/>
				<param name="prog_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
