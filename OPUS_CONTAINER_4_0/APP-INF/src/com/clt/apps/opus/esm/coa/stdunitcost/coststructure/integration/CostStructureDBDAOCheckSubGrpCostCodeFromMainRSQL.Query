<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOCheckSubGrpCostCodeFromMainRSQL">
			<desc><![CDATA[Sub group code Check query for ESM_COA_2001]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM COA_SUB_GRP_COST
 WHERE MGRP_COST_CD = @[mgrp_cost_cd]			]]></sql>
			<params>
				<param name="mgrp_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
