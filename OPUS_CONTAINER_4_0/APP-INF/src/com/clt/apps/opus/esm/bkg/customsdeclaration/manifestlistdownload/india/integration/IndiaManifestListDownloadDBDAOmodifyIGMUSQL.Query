<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaManifestListDownloadDBDAOmodifyIGMUSQL">
			<desc><![CDATA[인도세관 테이블에 BL정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_IDA_BL SET 
	IDA_LINE_NO = @[ida_line_no]
,	IDA_CSTMS_BL_TP_CD = @[bl_cust_tp]
,	BL_DECL_TP_CD = @[entry_tp]
,	IDA_DEST_CD = @[dest_cd]
,	IDA_CFS_ID = @[cfs_cd]
,	IAL_RGN_CD = @[ial]
,	IDA_TRSP_CD = @[mode_trans]
,   BD_AREA_CD = @[bd_area_cd]
,   TRNS_OPR_ID = @[trns_opr_id]
,	IDA_CSTMS_ITM_TP_CD = @[item_tp]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
AND	SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
AND	SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
AND	POD_CD = @[pod_cd]
AND	BL_NO = @[bl_no]
AND	NVOCC_REF_NO = @[hbl_no]			]]></sql>
			<params>
				<param name="ida_line_no" type="12" value="" out="N"/>
				<param name="bl_cust_tp" type="12" value="" out="N"/>
				<param name="entry_tp" type="12" value="" out="N"/>
				<param name="dest_cd" type="12" value="" out="N"/>
				<param name="cfs_cd" type="12" value="" out="N"/>
				<param name="ial" type="12" value="" out="N"/>
				<param name="mode_trans" type="12" value="" out="N"/>
				<param name="bd_area_cd" type="12" value="" out="N"/>
				<param name="trns_opr_id" type="12" value="" out="N"/>
				<param name="item_tp" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="hbl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
