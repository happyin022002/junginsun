<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesActivityManageDBDAOModifyCustSatisFactionUSQL">
			<desc><![CDATA[Sales Activity Report 화면의 3번째 탭 ( Cust.SatisFaction ) 수정 후 저장용 쿼리]]></desc>
			<sql><![CDATA[
UPDATE SAM_CUST_SATSFC
SET CUST_SATSFC_GRD_CD = @[grd]
, CUST_SATSFC_RSN = @[rsn]
WHERE SREP_CD = @[srep_cd]
AND CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]
AND SLS_ACT_SEQ = @[sls_act_seq]
AND CUST_SATSFC_ITM_CD = @[cust_satsfc_itm_cd]			]]></sql>
			<params>
				<param name="grd" type="12" value="" out="N"/>
				<param name="rsn" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="sls_act_seq" type="12" value="" out="N"/>
				<param name="cust_satsfc_itm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
