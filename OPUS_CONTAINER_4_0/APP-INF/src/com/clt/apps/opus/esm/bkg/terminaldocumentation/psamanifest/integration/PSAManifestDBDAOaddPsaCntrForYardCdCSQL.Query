<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOaddPsaCntrForYardCdCSQL">
			<desc><![CDATA[Yard Assign By CNTR화면에서 Row add시 insert한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_PSA_CNTR
    (       BKG_NO          ,
            BKG_SEQ         ,
            PSA_SER_NO      ,
            PSA_IF_CD       ,
            DCGO_FLG        ,
            RC_FLG          ,
            RD_CGO_FLG      ,
            OVR_HGT_FLG     ,
            OVR_WDT_FLG     ,
            OVR_LEN_FLG     ,
            CRE_DT          ,
            CRE_USR_ID      ,
            UPD_DT          ,
            UPD_USR_ID   )
    VALUES
    (       @[bkg_no]       ,
            @[bkg_seq] + 1  ,
            1               ,
            'N'             ,
            'N'             ,
            'N'             ,
            'N'             ,
            'N'             ,
            'N'             ,
            'N'             ,
            SYSDATE         ,
            @[usr_id]       ,
            SYSDATE         ,
            @[usr_id]       )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
