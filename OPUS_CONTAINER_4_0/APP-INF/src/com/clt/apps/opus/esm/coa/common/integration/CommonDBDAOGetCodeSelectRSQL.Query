<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOGetCodeSelectRSQL">
			<desc><![CDATA[COA에서 사용하는 목록성 코드조회
@SJH.20140818.ADD : sokeupStatus 추가
@SJH.20141224.ADD : searchSubTradeList2 추가]]></desc>
			<sql><![CDATA[
#if (${methodname} == '')
	SELECT	'' AS CODE
	,		'' AS NAME
	
	,		'' AS KEY
	,		'' AS RTN_DATE
	,		'' AS ETD_DT 
	,		'' AS OFC_CD
	,		'' AS PRNT_OFC_CD
	,		'' AS SIM_NO
	,		'' AS OFC_LEVEL
	,		'' AS COST_YR
	,		'' AS COST_WK 
	,		'' AS TEAM_CODE
	,		'' AS RT
	,		'' AS SORT_KEY
	FROM	DUAL
#elseif (${methodname} == 'searchRevLaneList')

/*revenue lane의 정보를 가져온다*/

	SELECT DISTINCT RLANE_CD NAME
          ,RLANE_CD CODE 
	  FROM COA_LANE_RGST 
	 WHERE TRD_CD = NVL(@[str_trd_cd],TRD_CD) 
       AND SUB_TRD_CD = NVL(@[str_sub_trd_cd],SUB_TRD_CD) 
       AND NVL(DELT_FLG,'N') = 'N' 
	 ORDER BY RLANE_CD

#elseif (${methodname} == 'searchRevLaneList2')

/*revenue lane의 정보를 가져온다*/

	SELECT DISTINCT RLANE_CD NAME
          ,RLANE_CD CODE 
	  FROM COA_LANE_RGST 
     WHERE NVL(DELT_FLG,'N') = 'N' 
	 ORDER BY RLANE_CD 

#elseif (${methodname} == 'searchRLaneList')

/*revenue lane의 정보를 가져온다*/

	SELECT DISTINCT TRD_CD KEY
          ,RLANE_CD NAME
		  ,RLANE_CD CODE 
	  FROM COA_LANE_RGST 
	 WHERE NVL(DELT_FLG,'N') = 'N' 
 	   AND TRD_CD = NVL(@[trd_cd], TRD_CD) 
	 ORDER BY KEY
     		,NAME 

#elseif (${methodname} == 'searchSMUSubTradeList')

/*SMU Sub Trade 정보를 가져온다*/

	SELECT DISTINCT  SUB_TRD_CD CODE
          ,SUB_TRD_CD NAME 
	  FROM MDM_DTL_REV_LANE                           
	 WHERE SUB_TRD_CD IS NOT NULL                     
	   AND TRD_CD = NVL(@[trd_cd], TRD_CD)                
	   AND SUB_TRD_CD <> 'IP'                         
	 ORDER BY SUB_TRD_CD 

#elseif (${methodname} == 'searchSLaneList')

/*service lane의 정보를 가져온다*/

	SELECT DISTINCT TRD_CD KEY
          ,SLAN_CD NAME
          ,SLAN_CD CODE 
	  FROM COA_LANE_RGST 
	 WHERE TRD_CD = NVL(@[trd_cd], TRD_CD) 
  	   AND NVL(DELT_FLG,'N') = 'N' 
	 ORDER BY TRD_CD
        	 ,SLAN_CD  

#elseif (${methodname} == 'searchLccList')

/*LCC의 목록을 가져온다*/

	#if(${code} == '') 
   		SELECT DISTINCT LCC_CD NAME
	          ,LCC_CD CODE 
    	  FROM COA_LOCATION_V 
    	 ORDER BY LCC_CD 			
	#else
    	SELECT DISTINCT RCC_CD KEY
			  ,LCC_CD NAME
			  ,LCC_CD CODE 
    	  FROM COA_LOCATION_V 
    	 WHERE RCC_CD = @[code] 
   	     ORDER BY RCC_CD, LCC_CD  
	#end 

#elseif (${methodname} == 'searchEccList')

/*ECC의 목록을 가져온다*/

	#if (${code} == '') 
		SELECT DISTINCT ECC_CD NAME
			  ,ECC_CD CODE 
		FROM COA_LOCATION_V 
		ORDER BY ECC_CD 
  #else 
		SELECT DISTINCT RCC_CD || LCC_CD KEY
			  ,ECC_CD NAME
			  ,ECC_CD CODE 
		  FROM COA_LOCATION_V 
         WHERE RCC_CD = @[rcc_cd] 
		   AND LCC_CD = @[lcc_cd]
		 ORDER BY RCC_CD
				 ,LCC_CD  
   #end  

#elseif (${methodname} == 'searchSubOFCList')

/*선택한 office Level에 해당하는 office code목록을 조회한다*/

	SELECT DISTINCT DECODE(@[sel_ofc_lvl]
                      	,'1', OFC_N1ST_LVL_CD
                      	,'2', OFC_N2ND_LVL_CD
                      	,'3', OFC_N3RD_LVL_CD
                      	,'4', OFC_N4TH_LVL_CD
                      	,'5', OFC_N5TH_LVL_CD
                      	,'6', OFC_N6TH_LVL_CD
                      	,'7', OFC_N7TH_LVL_CD
                      	) CODE
               	,DECODE(@[sel_ofc_lvl]
                      	,'1', OFC_N1ST_LVL_CD
                      	,'2', OFC_N2ND_LVL_CD
                      	,'3', OFC_N3RD_LVL_CD
                      	,'4', OFC_N4TH_LVL_CD
                      	,'5', OFC_N5TH_LVL_CD
                      	,'6', OFC_N6TH_LVL_CD
                      	,'7', OFC_N7TH_LVL_CD
                      	) NAME
    FROM COA_OFC_LVL
   WHERE 1 = 1
		
			/*COST_MON이 들어온 경우, 2009년 이상 데이터는 월별관리*/
		
		  		AND @[cost_yr]||@[cost_mon] BETWEEN OFC_APLY_FM_YRMON AND OFC_APLY_TO_YRMON /*월별관리*/
	
		
     			AND DECODE(@[usr_ofc_lvl]
               		,'1', OFC_N1ST_LVL_CD
               		,'2', OFC_N2ND_LVL_CD
                    ,'3', OFC_N3RD_LVL_CD
                    ,'4', OFC_N4TH_LVL_CD
                    ,'5', OFC_N5TH_LVL_CD
                    ,'6', OFC_N6TH_LVL_CD
                    ,'7', OFC_N7TH_LVL_CD
                    ) IN (@[usr_ofc_cd], '', '', '')

     AND DECODE(@[sel_ofc_lvl]
               ,'1', OFC_N1ST_LVL_CD
               ,'2', OFC_N2ND_LVL_TP_CD
               ,'3', OFC_N3RD_LVL_TP_CD
               ,'4', OFC_N4TH_LVL_TP_CD                        
               ,'5', OFC_N5TH_LVL_TP_CD
               ,'6', OFC_N6TH_LVL_TP_CD
               ,'7', OFC_N7TH_LVL_TP_CD
               ,' '
               ) IS NOT NULL
  ORDER BY CODE	

#elseif (${methodname} == 'searchReportItem')

/*Report Item Infomation 목록을 조회한다*/

	SELECT SLCT_ITM_FOM_SEQ CODE
	      ,SLCT_ITM_FOM_DESC NAME
  	  FROM COA_RPT_ITM_INFO_MST
 	 WHERE CRE_USR_ID = @[code]

#elseif (${methodname} == 'checkBKGNo')

/*Booking Number 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
     	 		  SELECT '1' 
     	 			FROM COA_RGST_BKG 
      			   WHERE BKG_NO = @[bkgno] 
				  )

#elseif (${methodname} == 'checkLocationCode')

/*Location Code 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
      			   SELECT '1' 
      			     FROM MDM_LOCATION 
      			    WHERE LOC_CD = @[loccd]  
				   ) 

#elseif (${methodname} == 'checkVesselCode')

/*Vessel Code 유무를 확인*/

	SELECT 'X' 
 	  FROM DUAL 
 	 WHERE EXISTS ( 
               	   SELECT '1' 
                     FROM MDM_VSL_CNTR 
                    WHERE VSL_CD = @[vslcd]  
                  )   

#elseif (${methodname} == 'checkVVDCode')

/*VVD Code 유무를 확인*/

	SELECT 'X' 
      FROM DUAL 
     WHERE EXISTS ( 
      			   SELECT '1' 
      			     FROM VSK_VSL_SKD 
     			    WHERE VSL_CD = @[vslcd] 
        		      AND SKD_VOY_NO = @[skdvoyno]
                      AND SKD_DIR_CD = @[skddircd] 
    			   ) 

#elseif (${methodname} == 'checkOfficeCode')

/*Office Code 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
      			   SELECT '1' 
      				 FROM MDM_ORGANIZATION 
      				WHERE OFC_CD = @[ofccd] 
     			   ) 

#elseif (${methodname} == 'checkRevLaneCode')

/*Revenue Lane Code 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
      			   SELECT '1' 
      				 FROM MDM_REV_LANE 
      			    WHERE RLANE_CD = @[rlanecd] 
      				  AND NVL(DELT_FLG,'*') = 'N' 
     			   ) 

#elseif (${methodname} == 'checkNodeCode')

/*Port or Node Code 유무를 확인*/

	SELECT 'X' 
      FROM DUAL 
     WHERE EXISTS ( 
                   SELECT '1' 
                     FROM MDM_YARD 
                    WHERE YD_CD = @[tmnlcd] 
	               ) 

#elseif (${methodname} == 'checkSLaneCode')

/*Service Lane Code 유무를 확인*/

	SELECT 'X' 
  	  FROM DUAL 
 	 WHERE EXISTS ( 
               	   SELECT '1' 
                 	 FROM MDM_VSL_SVC_LANE 
                	WHERE DELT_FLG      = 'N'
                  	  AND VSL_SLAN_CD   = @[slancd] 
              	   ) 
#elseif (${methodname} == 'searchOFCLevelList')

/*office Level List 목록을 조회한다*/

	SELECT DISTINCT OFC_LVL CODE
		  ,OFC_LVL_DESC NAME 
  	  FROM   COA_RPT_AUTH_MGMT 
 	 WHERE OFC_LVL >= @[code] 
 	 ORDER BY OFC_LVL 

#elseif (${methodname} == 'searchDatePeriod')

/*검색 조건에 맞는 DatePriod을 가져온다. - 입력파라메터의 경우의 수 1. Year, Month 2. Year, Week 3. Year, Month, Week : Week가 우선권을 가진다. 
															 4. Year, FromMonth, ToMonth 5. Year, FromWeek, ToWeek 6. Year, FromMonth, ToMonth, FromWeek, ToWeek*/
	#if(${gubun}=='1')

	/*searchDatePeriod : YEAR, MONTH, WEEK인경우*/
		SELECT TO_CHAR(TO_DATE(DECODE(FM_CHK,'Y',SLS_FM_DT, @[year]||@[month]||'01'),'YYYYMMDD'),'YYYY-MM-DD') || ' ~ '|| 
    	   					   DECODE(TO_CHK,'Y',TO_CHAR(TO_DATE(SLS_TO_DT,'YYYYMMDD'),'YYYY-MM-DD')
			  ,TO_CHAR(LAST_DAY(TO_DATE(SLS_FM_DT,'YYYYMMDD')),'YYYY-MM-DD')) RTN_DATE
		  FROM ( 
				SELECT SLS_FM_DT,SLS_TO_DT 
    	   			  ,DECODE(SUBSTR(SLS_FM_DT,1,6), @[year]||@[month],'Y','N') FM_CHK 
       	   			  ,DECODE(SUBSTR(SLS_TO_DT,1,6), @[year]||@[month],'Y','N') TO_CHK 
	  			  FROM  COA_WK_PRD 
			     WHERE COST_YR = @[year] 
  				   AND COST_WK = @[week] 
      	) 
	#elseif(${gubun}=='2')

	/*log.info('searchDatePeriod : YEAR, MONTH인경우')*/
		SELECT TO_CHAR(TO_DATE(@[year]||@[month],'YYYYMM'),'YYYY-MM-DD') || ' ~ ' || 
       	   	   TO_CHAR(LAST_DAY(TO_DATE(@[year]||@[month],'YYYYMM')),'YYYY-MM-DD') RTN_DATE
	 	  FROM DUAL 

	
	#elseif(${gubun}=='3')			

	/*log.info('searchDatePeriod : YEAR, WEEK인경우')*/
		SELECT TO_CHAR(TO_DATE(SLS_FM_DT, 'YYYYMMDD'),'YYYY-MM-DD') || ' ~ '|| 
       	       TO_CHAR(TO_DATE(SLS_TO_DT, 'YYYYMMDD'),'YYYY-MM-DD') RTN_DATE 
	 	  FROM  COA_WK_PRD 
	     WHERE COST_YR = @[year] 
  	       AND COST_WK = @[week]

	#elseif(${gubun}=='4')	

/*log.info('searchDatePeriod : year,from month,to_month,from_week,to_week인경우')*/
		SELECT TO_CHAR(TO_DATE(DECODE(SIGN(TO_NUMBER(MN_FM_DT) - TO_NUMBER(WK_TO_DT)),  1, '' 
                              ,DECODE(SIGN(TO_NUMBER(MN_TO_DT) - TO_NUMBER(WK_FM_DT)),  -1, '' 
                              ,DECODE(SIGN(TO_NUMBER(MN_FM_DT)-TO_NUMBER(WK_FM_DT)), 1, MN_FM_DT, WK_FM_DT) )), 'YYYYMMDD'),'YYYY-MM-DD')  || '~' || 
      		   TO_CHAR(TO_DATE(DECODE(SIGN(TO_NUMBER(MN_TO_DT) - TO_NUMBER(WK_FM_DT)), -1, '', 
                               DECODE(SIGN(TO_NUMBER(MN_FM_DT) - TO_NUMBER(WK_TO_DT)),  1, '', 
                               DECODE(SIGN(TO_NUMBER(MN_TO_DT)-TO_NUMBER(WK_TO_DT)), -1, MN_TO_DT, WK_TO_DT) )), 'YYYYMMDD'),'YYYY-MM-DD') RTN_DATE  
		 FROM ( 
     		   SELECT MAX(WK_FM_DT) WK_FM_DT
					 ,MAX(WK_TO_DT) WK_TO_DT
					 ,MAX(MN_FM_DT) MN_FM_DT
					 ,MAX(MN_TO_DT) MN_TO_DT 
     			 FROM ( 
          			   SELECT MIN(SLS_FM_DT) WK_FM_DT
							 ,MAX(SLS_TO_DT) WK_TO_DT
							 ,'' MN_FM_DT
							 ,'' MN_TO_DT 
          FROM  COA_WK_PRD 
          WHERE COST_YR = @[year] 
            AND COST_WK BETWEEN @[frmweek] AND @[toweek] 
          UNION ALL 
          SELECT '','',TO_CHAR(TO_DATE(@[year]||@[frmmonth],'YYYYMM'),'YYYYMMDD'), 
                  TO_CHAR(LAST_DAY(TO_DATE(@[year]||@[tomonth],'YYYYMM')),'YYYYMMDD') 
           FROM DUAL 
      ) 
 ) 

#elseif(${gubun}=='5')

/*log.debug('searchDatePeriod : year,from month,to month인경우')*/
	SELECT TO_CHAR(TO_DATE(@[year]||@[frmmonth],'YYYYMM'),'YYYY-MM-DD') || ' ~ ' ||        
       	   TO_CHAR(LAST_DAY(TO_DATE(@[year]||@[tomonth],'YYYYMM')),'YYYY-MM-DD') RTN_DATE 
	  FROM DUAL                                                           

#elseif(${gubun}=='6')

/*log.debug('searchDatePeriod : year,from week,to week인경우')*/
	SELECT MIN(TO_CHAR(TO_DATE(SLS_FM_DT, 'YYYYMMDD'),'YYYY-MM-DD')) ||' ~ ' || 
       	   MAX(TO_CHAR(TO_DATE(SLS_TO_DT, 'YYYYMMDD'),'YYYY-MM-DD')) RTN_DATE 
	  FROM COA_WK_PRD 
	 WHERE COST_YR = @[year] 
  	   AND COST_WK BETWEEN @[frmweek] AND @[toweek] 
#end




#elseif (${methodname} == 'searchFirstEtd')

/*VVD에 해당하는 etd date를 리턴한다*/

	SELECT TO_CHAR(MIN(VPS_ETD_DT),'YYYYMMDD') ETD_DT 
	  FROM VSK_VSL_PORT_SKD 
	 WHERE VSL_CD      = @[vsl_cd] 
  	   AND SKD_VOY_NO  = @[skd_voy_no] 
  	   AND SKD_DIR_CD  = @[skd_dir_cd]
  	   /*AND NVL(CNG_STS_CD,'*') <> 'S'*/ 

#elseif (${methodname} == 'searchKeyAcctInDvlList')	

/*Group ID = code인 Key Account List*/

	SELECT CUST_CNT_CD||CUST_SEQ CODE 
       	  ,CUST_LGL_ENG_NM NAME 
  	  FROM MDM_CUSTOMER 
 	 WHERE DELT_FLG     = 'N'  
   	   AND KEY_ACCT_FLG = 'Y' 
   	   AND (NMD_CUST_FLG IS NULL OR NMD_CUST_FLG = 'N') 
   	   	#if(${keyacctgrp} != '' && ${keyacctgrp} != 'All')
   	    	AND CUST_GRP_ID = @[keyacctgrp]
   	    #end 
 	 ORDER BY CUST_LGL_ENG_NM

#elseif (${methodname} == 'searchMltTrdInDvlList')	

/*Group ID = code인 M/A List*/

	SELECT CUST_CNT_CD||CUST_SEQ CODE 
       	  ,CUST_LGL_ENG_NM NAME 
  	  FROM MDM_CUSTOMER 
 	 WHERE DELT_FLG     = 'N'  
   	   AND mlt_trd_acct_flg = 'Y' 
   	   AND (NMD_CUST_FLG IS NULL OR NMD_CUST_FLG = 'N') 
   	   	#if(${mlttrdgrp} != '' && ${mlttrdgrp} != 'All')
   	    	AND CUST_GRP_ID = @[mlttrdgrp]
   	    #end 
 	 ORDER BY CUST_LGL_ENG_NM

#elseif (${methodname} == 'searchChgOffice')

    SELECT OFC_CD
		  ,PRNT_OFC_CD                               
      FROM MDM_ORGANIZATION     
     WHERE 1=1 
   	   AND OFC_KND_CD = '2'    
   	   AND OFC_TP_CD  <> 'AQ'

#elseif (${methodname} == 'searchOFCLevel')

/*office code에 해당하는 Level를 리턴한다*/

	SELECT COA_GET_OFC_LEVEL_FNC(@[ofc_cd]) OFC_LEVEL
  	  FROM DUAL

#elseif (${methodname} == 'searchPrevWkPrd')

/*전주를 구한다*/

	SELECT  COST_YR
		   ,COST_WK 
  	  FROM  COA_WK_PRD 
 	 WHERE  1=1 
   	   AND  (TO_CHAR(SYSDATE-7, 'YYYYMMDD') 
   BETWEEN  SLS_FM_DT AND SLS_TO_DT)

#elseif (${methodname} == 'searchPreWeek')

/*현재주보다 작은 주를 반환한다*/

	SELECT MAX(COST_YR || COST_WK)
  	  FROM COA_WK_PRD 
 	 WHERE COST_YR || COST_WK < @[year]||@[week]

#elseif (${methodname} == 'getUSDAmt')

/*환율 변환*/

	SELECT ROUND(@[lcl_amt]/USD_LOCL_XCH_RT, 3) RT     
      FROM GL_MON_XCH_RT 			
     WHERE ACCT_XCH_RT_YRMON = @[cost_yrmon] 		
       AND ACCT_XCH_RT_LVL   = '1' 		
       AND CURR_CD = @[locl_curr_cd]
#elseif (${methodname} == 'searchTradeList')

/*Trade콤보의 목록을 가져온다*/
/*2009.09.07 TRADE 코드중 COM 이 나오지 않도록 쿼리 수정*/

	SELECT TRD_CD CODE
		  ,TRD_CD NAME 
	  FROM MDM_TRADE 
	 WHERE VSL_TP_CD = 'C' 
	   AND DELT_FLG  = 'N' 
	   AND TRD_CD    <> 'COM' 
	 ORDER BY TRD_CD 

#elseif (${methodname} == 'searchSubTradeList')

/*Sub Trade 정보를 가져온다*/

		SELECT DISTINCT  TRD_CD KEY
			  ,SUB_TRD_CD CODE
			  ,SUB_TRD_CD NAME 
		  FROM MDM_DTL_REV_LANE 
		 WHERE SUB_TRD_CD IS NOT NULL 
		   AND TRD_CD     = NVL(@[trd_cd], TRD_CD) 
		 ORDER BY TRD_CD
				 ,SUB_TRD_CD

#elseif (${methodname} == 'searchSccList')

/*SCC의 목록을 가져온다*/

		SELECT DISTINCT ECC_CD KEY, SCC_CD NAME, SCC_CD CODE 
		  FROM COA_LOCATION_V 
		 WHERE ECC_CD = @[code] 
		 ORDER BY ECC_CD, SCC_CD 


#elseif (${methodname} == 'searchVslCapaList')

/*VESSEL CAPA 목록을 조회한다*/

		SELECT DISTINCT vsl_clss_capa code
			  ,vsl_clss_capa name
		  FROM COA_VSL_RGST 
		 WHERE vsl_tp_cd     = 'C' 
		   AND vsl_clss_capa > 0 
		 ORDER BY vsl_clss_capa 

#elseif (${methodname} == 'searchCurrencyList')

/*Currency 목록을 조회한다*/

		SELECT curr_cd code
			  ,curr_cd name 
		  FROM MDM_CURRENCY 
		 WHERE NVL(delt_flg,'N') = 'N' 

#elseif (${methodname} == 'searchRepCMDTList')

/*Commodity List 목록을 조회한다*/

		SELECT REP_CMDT_CD CODE
			  ,REP_CMDT_NM NAME
		  FROM MDM_REP_CMDT                       
		 WHERE NVL(DELT_FLG,'N') = 'N'      

#elseif (${methodname} == 'searchRHQList')

/*Regional Headquarter: RHQ 목록을 조회한다*/

		SELECT ofc_cd code
			  ,ofc_cd name 
		  FROM mdm_organization 
		 WHERE ofc_knd_cd = '2' 
		 ORDER BY name   

#elseif (${methodname} == 'searchLocList')

/*Location Code 목록을 가져온다*/

		#if(${param0} == 'ECC')	
			 SELECT DISTINCT ECC_CD KEY
			       ,LOC_CD NAME
				   ,LOC_CD CODE 
			      FROM COA_LOCATION_V 
			      WHERE ECC_CD = @[param1]
			      ORDER BY ECC_CD
						  ,LOC_CD 
		#else    /*SCC*/
			  SELECT DISTINCT SCC_CD KEY
					,LOC_CD NAME
					,LOC_CD CODE 
			      FROM COA_LOCATION_V 
			      WHERE SCC_CD = @[param1]
			      ORDER BY SCC_CD, LOC_CD 
		#end 

#elseif (${methodname} == 'searchEQRepoTpSzList')  

/*컨테이너 Reposition Flag가 Y인 Type Size 목록을 조회한다*/ 

		SELECT CNTR_TPSZ_CD NAME
			  ,CNTR_TPSZ_CD CODE 
		  FROM COA_SPCL_REPO_CNTR_RGST 
		 WHERE NVL(REPO_FLG,'N') = 'Y' 
		   AND NVL(DELT_FLG,'N') = 'N' 
         ORDER BY CNTR_TPSZ_CD

#elseif (${methodname} == 'searchRAMainGroupList') 

/*RA Main Group 항목을 가져온다*/

		SELECT INTG_CD_VAL_CTNT CODE 
			  ,INTG_CD_VAL_DP_DESC NAME  
		  FROM COM_INTG_CD_DTL 
		 WHERE INTG_CD_ID = 'CD00895'  
		 ORDER BY INTG_CD_VAL_DP_SEQ

#elseif (${methodname} == 'searchRASubGroupList')

/*RA Sub Group 항목을 가져온다*/

		SELECT SUBSTR(INTG_CD_VAL_CTNT, 1,2) KEY
			  ,INTG_CD_VAL_CTNT CODE 
			  ,INTG_CD_VAL_DP_DESC NAME 
		FROM COM_INTG_CD_DTL 
		WHERE INTG_CD_ID = 'CD00894'
			#if (${code} != '')
		   		AND SUBSTR(INTG_CD_VAL_CTNT, 1,2) = @[code]
			#end
	    ORDER BY KEY

#elseif (${methodname} == 'searchRccList')

/*Rcc 의 목록을 가져온다*/

		SELECT DISTINCT RCC_CD NAME
			  ,RCC_CD CODE 
		FROM COA_LOCATION_V 
		ORDER BY RCC_CD 

#elseif (${methodname} == 'searchStndCostList')

/*COA_STND_ACCT에서 비용항목을 가져온다*/

		SELECT STND_COST_CD AS CODE 
		      ,STND_COST_NM AS NAME 
		FROM   COA_STND_ACCT 
		WHERE MGRP_COST_CD IN ('OV','OF','GE') 
		AND   NVL(DELT_FLG,'N') = 'N'
		ORDER BY  ACCT_DP_SEQ 

#elseif (${methodname} == 'searchOwnVesselList')

/*Vessel콤보의 목록을 가져온다*/

		SELECT DISTINCT VSL_CD CODE
			  ,VSL_CD NAME 
		FROM COA_VSL_RGST 
		WHERE VSL_TP_CD   = 'C' 
		  AND DELT_FLG    = 'N' 
		  AND VSL_OSHP_CD = 'OWN' 
		ORDER BY VSL_CD 

#elseif (${methodname} == 'searchChtVesselList')

/*Vessel콤보의 목록을 가져온다*/

		SELECT VSL_CD CODE
			  ,VSL_CD NAME 
		FROM COA_VSL_RGST 
		WHERE VSL_TP_CD   = 'C' 
		  AND DELT_FLG    = 'N' 
		  AND VSL_OSHP_CD = 'CHT' 
		  AND LST_FLG     = 'Y' 
