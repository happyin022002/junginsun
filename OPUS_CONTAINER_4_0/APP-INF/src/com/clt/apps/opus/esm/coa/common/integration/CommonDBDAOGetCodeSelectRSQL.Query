		  FROM COA_VSL_RGST 
		 WHERE 1=1
		   AND DELT_FLG = 'N' 
		UNION ALL
		SELECT DISTINCT VSL_CLSS_CAPA  
		  FROM COA_SIM_VSL_RGST 
		 WHERE 1=1
		   ) 
		ORDER BY  VSL_CLSS_CAPA

#elseif (${methodname} == 'searchSimSLaneList') 

/*S.Lane의 목록을 가져온다*/

		SELECT DISTINCT NAME
			  ,CODE 
		  FROM ( 
		        SELECT SLAN_CD NAME
					  ,SLAN_CD CODE 
		          FROM COA_LANE_RGST 
		         WHERE NVL(DELT_FLG,'N') = 'N' 
		         UNION ALL 
		        SELECT SLAN_CD NAME
					  ,SLAN_CD CODE 
		          FROM COA_SIM_INFO 
		       ) 
		ORDER BY CODE

#elseif (${methodname} == 'searchVOPCd') 

/*searchVOPCd*/

select COM_ConstantMgr_PKG.COM_getCompanyCode_FNC
from dual

#elseif (${methodname} == 'searchSokeupCodeList')

/*sokeupStatus : SJH.20140818.ADD */

	SELECT
		INTG_CD_VAL_CTNT CODE
		,INTG_CD_VAL_DP_DESC  NAME
	FROM COM_INTG_CD_DTL B
	WHERE INTG_CD_ID = 'CD00220'

#elseif (${methodname} == 'searchSvcScpCodeList')

/*searchSvcScpCodeList : PCM.20141218.ADD */

SELECT SVC_SCP_CD AS CODE
	  ,SVC_SCP_NM AS NAME
	  ,'' AS ETC1
      ,'' AS ETC2
      ,'' AS ETC3
      ,'' AS ETC4
      ,(SVC_SCP_CD || '|' || SVC_SCP_NM) AS ETC5
FROM MDM_SVC_SCP
WHERE DELT_FLG = 'N'
ORDER BY SVC_SCP_CD

#elseif (${methodname} == 'searchSubTradeList2')

/*Sub Trade 정보를 가져온다(중복없이) */

SELECT DISTINCT  
	   SUB_TRD_CD CODE
	  ,SUB_TRD_CD NAME 
  FROM MDM_DTL_REV_LANE 
 WHERE SUB_TRD_CD IS NOT NULL 
   AND TRD_CD     = NVL(@[trd_cd], TRD_CD) 
 ORDER BY SUB_TRD_CD

#elseif (${methodname} == 'searchLocListAll')

/* Location Code별 목록을 가져온다 (SJH.20150105.ADD) */

SELECT DISTINCT 
       DECODE(@[param0],'S',SCC_CD,'E',ECC_CD,'L',LCC_CD,'R',RCC_CD,LOC_CD) NAME
	  ,DECODE(@[param0],'S',SCC_CD,'E',ECC_CD,'L',LCC_CD,'R',RCC_CD,LOC_CD) CODE 
  FROM COA_LOCATION_V 
 ORDER BY CODE

#elseif (${methodname} == 'searchCostSourceGroupCodeList2')

/* so cost Code별 목록을 가져온다 (SJH.20150205.ADD) */

SELECT COA_COST_SRC_CD CODE 
     , COA_COST_SRC_CD_NM NAME 
  FROM COA_COST_SRC_ACCT 
 WHERE NVL(DELT_FLG, 'N')= 'N'
   AND SGRP_COST_CD 	 = @[code]
   AND COA_COST_SRC_CD LIKE @[cnum]||'%'
 ORDER BY STND_COST_CD, COA_COST_SRC_CD

#elseif (${methodname} == 'searchCntrTpszCdList')

/* so cost Code별 목록을 가져온다 (SJH.20150205.ADD) */

SELECT CNTR_TPSZ_CD CODE 
     , CNTR_TPSZ_CD NAME 
  FROM MDM_CNTR_TP_SZ 
 WHERE NVL(DELT_FLG, 'N')= 'N'
   AND CNTR_TP_CD 	 = @[param0]
 ORDER BY CNTR_TPSZ_CD

#elseif (${methodname} == 'searchAcmAccountTypeList')
/* avg agent commition account 구분 코드 - ac_tp_cd 2017.01.11 Add */
SELECT DISTINCT A.AC_TP_CD AS CODE
     , A.AC_CD_RMK AS NAME
  FROM ACM_COMM_TP_CD_MAPG A
 WHERE 1=1
   AND COMM_TP_CD   = 'C'
   AND AC_TP_CD     <> 'T'  
 ORDER BY A.AC_TP_CD

#elseif (${methodname} == 'searchAcmBoundList')
/* avg agent commition : bound 구분 코드 - io_bnd_cd 2017.01.11 Add */
SELECT DISTINCT A.IO_BND_CD AS CODE
     , A.IO_BND_CD AS NAME
  FROM ACM_COMM_TP_CD_MAPG A
 WHERE 1=1
   AND COMM_TP_CD   = 'C'
   AND AC_TP_CD     <> 'T'    
 ORDER BY A.IO_BND_CD

#elseif (${methodname} == 'searchAcmCostList')
/* avg agent commition : Cost Code 구분 코드 - coa_cost_src_cd 2017.01.11 Add */
SELECT A.COA_COST_SRC_CD AS CODE
     , A.COA_COST_SRC_CD_NM AS NAME
  FROM COA_COST_SRC_ACCT A
 WHERE A.SGRP_COST_CD   = 'CVAC'
   AND A.STND_COST_CD   <> '51401014'
 ORDER BY A.COA_COST_SRC_CD
#end			]]></sql>
