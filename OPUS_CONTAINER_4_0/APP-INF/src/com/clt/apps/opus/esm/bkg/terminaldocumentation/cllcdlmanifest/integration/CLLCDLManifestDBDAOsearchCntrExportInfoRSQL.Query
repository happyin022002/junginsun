<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCntrExportInfoRSQL">
			<desc><![CDATA[ExCntrTransmitVO]]></desc>
			<sql><![CDATA[
SELECT  TML.BKG_NO
       ,TML.EUR_TML_PURP_CD AS CODE_OPR
       ,TERM.PSA_TML_VSL_CD||TERM.TML_SKD_VOY_NO AS TERM_VVD
       ,TML.POL_CD AS TERM_POL
       ,TML.POD_CD AS TERM_POD
       ,VVD.VSL_CD||VVD.SKD_VOY_NO||VVD.SKD_DIR_CD AS NYK_VVD
       ,LOC1.LOC_NM AS NYK_POL
       ,LOC2.LOC_NM AS NYK_POD
       ,TML.FWRD_AGN_ID AS FWRD_AGN_CD
       ,TERM.EUR_TML_CD AS TMNL_BRTH_CD
       ,TML.HLG_TP_CD AS HAUL_MODE
       ,TML.BKG_TRSP_MOD_CD AS TRAN_MODE
       ,QTY.CNTR_TPSZ_CD AS CNTR_TYPE
       ,TPSZ.CNTR_TPSZ_TARE_WGT AS CNTR_SIZE
       ,QTY.OP_CNTR_QTY AS CNTR_QTY
	   ,QTY.QTY_GRS_WGT_DESC AS GRS_WGT
FROM    BKG_CSTMS_TML_EUR_ORG_LOCL TML
       ,BKG_CSTMS_VVD_TML TERM
       ,BKG_VVD VVD
       ,MDM_LOCATION LOC1
       ,MDM_LOCATION LOC2 
       ,BKG_QUANTITY QTY
       ,MDM_CNTR_TP_SZ TPSZ
WHERE	TML.BKG_NO	    = @[bkg_no]
AND	    VVD.POL_CD	    = @[pol_cd]
AND	    TML.BKG_NO	    = VVD.BKG_NO
AND	    VVD.VSL_CD	    = TERM.VSL_CD(+)
AND	    VVD.SKD_VOY_NO  = TERM.SKD_VOY_NO(+)
AND	    VVD.SKD_DIR_CD  = TERM.SKD_DIR_CD(+)
AND	    TERM.PORT_CD(+) = @[pol_cd]
AND	    TML.POL_CD	    = LOC1.LOC_CD(+)
AND	    TML.POD_CD	    = LOC2.LOC_CD(+)
AND	    TML.BKG_NO	    = QTY.BKG_NO
AND	    QTY.CNTR_TPSZ_CD != 'Q2'
AND	    QTY.CNTR_TPSZ_CD != 'Q4'
AND	    QTY.CNTR_TPSZ_CD = TPSZ.CNTR_TPSZ_CD(+)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
