<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaGuidelineDBDAOGetPreviousCheckKey0076RSQL">
			<desc><![CDATA[Tab 1(Trade Tab) 조회 전 Key 값 조회]]></desc>
			<sql><![CDATA[
SELECT  VER.MQTA_MDL_VER_NO		        ,
        MDL.SLS_FCAST_PUB_NO	        ,
        VER.SAQ_STS_CD		    AS  STS
FROM    SAQ_MON_QTA_GLINE_VER     VER ,
        SAQ_MON_QTA_MDL_EXE       MDL
WHERE   VER.BSE_YR          = @[year]
AND     VER.BSE_QTR_CD      = @[bse_qtr_cd]
AND     VER.SAQ_TGT_GRP_CD  = @[target_grp]
AND     VER.GLINE_VER_NO    = @[version]
AND     MDL.MQTA_MDL_VER_NO = VER.MQTA_MDL_VER_NO			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="target_grp" type="12" value="" out="N"/>
				<param name="version" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
