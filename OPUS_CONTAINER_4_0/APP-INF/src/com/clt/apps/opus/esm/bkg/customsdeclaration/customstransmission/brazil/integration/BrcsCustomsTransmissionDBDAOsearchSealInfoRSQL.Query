<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BrcsCustomsTransmissionDBDAOsearchSealInfoRSQL">
			<desc><![CDATA[Multi Seal 정보를 조회한다]]></desc>
			<sql><![CDATA[
SELECT SEAL_PTY_TP_CD AS SEAL_PT
      ,CNTR_SEAL_NO AS SEALNBR
FROM BKG_CNTR_SEAL_NO
WHERE 1=1
AND BKG_NO = @[bkg_no]
AND CNTR_NO = @[cntr_no]
ORDER BY CNTR_SEAL_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
