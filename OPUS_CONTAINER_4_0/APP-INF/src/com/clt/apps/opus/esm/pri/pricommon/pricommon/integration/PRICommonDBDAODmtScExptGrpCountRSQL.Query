<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAODmtScExptGrpCountRSQL">
			<desc><![CDATA[DMT S/C EXCEPTION GROUP의 prop_no존재유무 조회]]></desc>
			<sql><![CDATA[
SELECT COUNT(PROP_NO) CNT 
  FROM DMT_SC_EXPT_GRP
 WHERE PROP_NO = @[prop_no]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
