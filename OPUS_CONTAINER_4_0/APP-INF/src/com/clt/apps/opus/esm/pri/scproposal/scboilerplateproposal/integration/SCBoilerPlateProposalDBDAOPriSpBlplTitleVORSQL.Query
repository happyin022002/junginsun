<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateProposalDBDAOPriSpBlplTitleVORSQL">
			<desc><![CDATA[Boiler Plate Header Title Select]]></desc>
			<sql><![CDATA[
SELECT BLPL_NM
	  ,BLPL_REF_YR
	  ,BLPL_HDR_SEQ
FROM PRI_SG_BLPL_HDR
WHERE BLPL_HDR_SEQ = @[blpl_hdr_seq]
AND CFM_FLG = 'Y'			]]></sql>
			<params>
				<param name="blpl_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
