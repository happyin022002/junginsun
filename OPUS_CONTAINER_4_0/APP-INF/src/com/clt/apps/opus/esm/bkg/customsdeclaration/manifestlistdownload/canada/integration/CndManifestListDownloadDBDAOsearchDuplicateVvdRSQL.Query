<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndManifestListDownloadDBDAOsearchDuplicateVvdRSQL">
			<desc><![CDATA[searchDuplicateVvd]]></desc>
			<sql><![CDATA[
SELECT  VSL_CD || SKD_VOY_NO || SKD_DIR_CD AS VSL_CD
  FROM  BKG_CSTMS_CND_VSL
 WHERE  1=1
   AND  VSL_CD || SKD_VOY_NO NOT IN (SUBSTR(${vvd_cd},1,8))
   AND  CVY_REF_NO = @[cvy_ref_no]			]]></sql>
			<params>
				<param name="cvy_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
