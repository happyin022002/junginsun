<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupCommodityProposalDBDAOPriSpScpGrpCmdtDtlGlineCpVOCSQL">
			<desc><![CDATA[SCGroupCommodityProposalDBDAOPriSpScpGrpCmdtDtlGlineCpVOCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_GRP_CMDT_DTL(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    GRP_CMDT_SEQ,
    GRP_CMDT_DTL_SEQ,
    PRC_CMDT_TP_CD,
    PRC_CMDT_DEF_CD,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    N1ST_CMNC_AMDT_SEQ,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
SELECT 
      @[prop_no],
      @[amdt_seq],
      @[svc_scp_cd],
	  DENSE_RANK() OVER (ORDER BY CMDT.GRP_CMDT_SEQ) GRP_CMDT_SEQ,
	  DENSE_RANK() OVER (PARTITION BY CMDT.GRP_CMDT_SEQ ORDER BY CMDT.GRP_CMDT_DTL_SEQ) GRP_CMDT_DTL_SEQ,
      CMDT.PRC_CMDT_TP_CD,     
      CMDT.PRC_CMDT_DEF_CD,
      'I',
      'GC',  
      @[amdt_seq],
      '',
      '',
      '',
      @[cre_usr_id],
      SYSDATE,
      @[upd_usr_id],
      SYSDATE
FROM 
    PRI_SG_MN MN,
    PRI_SG_GRP_CMDT_DTL CMDT

WHERE 
    (MN.SVC_SCP_CD, MN.EFF_DT) = (
    SELECT  SVC_SCP_CD, MAX(EFF_DT) 
    FROM    PRI_SG_MN 
    WHERE   SVC_SCP_CD = @[svc_scp_cd]
    AND     (SELECT EFF_DT FROM PRI_SP_SCP_MN WHERE PROP_NO = @[prop_no] 
                         AND AMDT_SEQ = @[amdt_seq] AND SVC_SCP_CD = @[svc_scp_cd]) BETWEEN EFF_DT AND EXP_DT
    AND     CFM_FLG 		= 'Y'
    GROUP BY SVC_SCP_CD
    )
AND MN.SVC_SCP_CD 	= CMDT.SVC_SCP_CD
AND MN.GLINE_SEQ 	= CMDT.GLINE_SEQ
AND MN.CFM_FLG 		= 'Y'
AND CMDT.PRC_CUST_TP_CD = @[prc_prog_sts_cd]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prc_prog_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
