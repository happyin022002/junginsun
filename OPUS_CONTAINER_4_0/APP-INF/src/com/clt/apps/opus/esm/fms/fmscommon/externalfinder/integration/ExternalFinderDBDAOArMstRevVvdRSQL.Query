<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDBDAOArMstRevVvdRSQL">
			<desc><![CDATA[VVD Code Select]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
     , SKD_VOY_NO
     , SKD_DIR_CD
     , RLANE_DIR_CD
     , VOY_TP_CD
     , SLAN_CD
     , PORT_CHK_FLG
     , LOD_QTY
     , REV_PORT_CD
     , REV_YRMON
     , COM_VVD_FLG
     , VVD_COM_LVL
     , RLANE_CD
     , DELT_FLG
     , CRE_USR_ID
     , CRE_DT
     , EAI_EVNT_DT
     , COM_VVD_FLG||'|'||VSL_CD||SKD_VOY_NO||SKD_DIR_CD||RLANE_DIR_CD AS CHK_VVD
  FROM AR_MST_REV_VVD
 WHERE VSL_CD = SUBSTR(@[vvd_cd],1,4)
   AND SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
   AND SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
   AND RLANE_DIR_CD = SUBSTR(@[vvd_cd],10,1)
   AND DELT_FLG = 'N' 			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
