<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesRepInfoManageDAOSearchPrmyKeyRSQL">
			<desc><![CDATA[PrymyKey search]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD,SREP_CD,CUST_SEQ,SREP_PRMRY_FLG AS FLG
FROM SAM_CUST_SLS_REP_INFO
WHERE SREP_CD = @[srep_cd]
AND CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]
			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
