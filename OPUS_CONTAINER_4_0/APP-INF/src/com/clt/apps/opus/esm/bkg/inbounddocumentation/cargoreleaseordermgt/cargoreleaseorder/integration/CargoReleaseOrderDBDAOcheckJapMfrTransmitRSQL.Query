<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOcheckJapMfrTransmitRSQL">
			<desc><![CDATA[Japan DOR I/F 전에 MFR 전송 여부를 체크한다]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y') AS TRANS_FLG
FROM BKG_CSTMS_JP_SND_LOG
WHERE BL_NO=@[bl_no]
AND   JP_SND_LOG_ID='MFR'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
