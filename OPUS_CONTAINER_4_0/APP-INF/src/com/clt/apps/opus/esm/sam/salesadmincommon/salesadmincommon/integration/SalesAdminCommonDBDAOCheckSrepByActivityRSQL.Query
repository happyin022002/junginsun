<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesAdminCommonDBDAOCheckSrepByActivityRSQL">
			<desc><![CDATA[Customer의 Activity를 실행한 Sales Rep을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SREP_CD
FROM SAM_SLS_ACT
WHERE 1=1
AND CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]
AND SLS_ACT_SEQ = @[sls_act_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="sls_act_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
