<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCmdMarkRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT NVL(TRIM(BKG_SPCLCHAR_CONV_FNC(NVL(UPPER(MK_DESC),' '),'X')),'NO MARKS') cmd_mark_all
	  ,LENGTH(NVL(TRIM(BKG_SPCLCHAR_CONV_FNC(NVL(UPPER(MK_DESC),' '),'M')),'NO MARKS')) cmd_mark_len
      ,NVL(TRIM(BKG_SPCLCHAR_CONV_FNC(NVL(UPPER(MK_DESC),' '),'M')),'NO MARKS') MK_DESC
FROM  BKG_CSTMS_ADV_CNTR_MF
WHERE BL_NO        = @[bl_no]
	AND CNTR_NO    = TRIM(@[cntr_no])
	AND CMDT_GDS_SEQ    = TRIM(@[cmd_seq])
	AND CNT_CD = 'US'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cmd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
