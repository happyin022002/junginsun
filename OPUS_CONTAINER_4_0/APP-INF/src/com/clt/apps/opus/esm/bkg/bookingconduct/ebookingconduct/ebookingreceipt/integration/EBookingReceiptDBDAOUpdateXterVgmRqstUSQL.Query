<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOUpdateXterVgmRqstUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_VGM_RQST SET
#if (${res_tp_cd} == '1') 
	VGM_ACK_RSPN_CD = 'A'
#else
	VGM_ACK_RSPN_CD = 'R',
	VGM_RSPN_RSN_CD = @[res_tp_cd]
#end
WHERE EDI_MSG_PROC_ID = @[edi_no]
AND XTER_SNDR_ID = @[sndr_id]
AND XTER_VGM_RQST_SEQ = @[rqst_seq]			]]></sql>
			<params>
				<param name="res_tp_cd" type="12" value="" out="N"/>
				<param name="edi_no" type="12" value="" out="N"/>
				<param name="sndr_id" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
