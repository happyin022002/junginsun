<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCFlagAtResultCntrRSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신 outVO : None, 연관VO: UsaResultCntrVO]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(CSTMS_CLR_CD),'N') AS OLD_CNTR_CFLAG
      ,MAX(CSTMS_RMK1) AS OLD_HOLD_REMARK
      ,MAX(DECODE(C.CSTMS_DIV_ID, 'CARGO_RLS_H_CD', 'H', 'A')) AS HOLD
      ,MAX(DECODE(C.CSTMS_DIV_ID, 'CARGO_RLS_R_CD', 'R', 'A')) AS REMV
      ,'' AS IN_VVD
      ,'' AS IN_POD
      ,'' AS OLD_Cntr_C_Flag
      ,'' AS qty69
      ,'' AS irc_Qty
      ,'' AS in_Bl
      ,'' AS icr_Seq
  FROM (SELECT R.CNT_CD, R.CSTMS_CLR_CD, R.CSTMS_RMK1
          FROM BKG_CSTMS_ADV_CNTR_RSLT R
         WHERE R.CNT_CD = 'US'
           AND R.BL_NO = @[bl_no]
           AND R.CNTR_NO LIKE TRIM(@[in_cntr])||'%'
           AND R.CSTMS_SEQ = (SELECT MAX(CSTMS_SEQ)
                              FROM BKG_CSTMS_ADV_CNTR_RSLT
                             WHERE CNT_CD = 'US'
                               AND BL_NO = @[bl_no]
                               AND CNTR_NO LIKE TRIM(@[in_cntr])||'%'
                               AND CSTMS_CLR_CD IS NOT NULL
                            )
        UNION ALL
        SELECT 'US', NULL, NULL FROM DUAL
       ) R
      ,BKG_CSTMS_CD_CONV_CTNT C
 WHERE 1=1
   AND R.CNT_CD = C.CNT_CD(+)
   AND C.ATTR_CTNT3(+) = @[icr_code]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="in_cntr" type="12" value="" out="N"/>
				<param name="icr_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
