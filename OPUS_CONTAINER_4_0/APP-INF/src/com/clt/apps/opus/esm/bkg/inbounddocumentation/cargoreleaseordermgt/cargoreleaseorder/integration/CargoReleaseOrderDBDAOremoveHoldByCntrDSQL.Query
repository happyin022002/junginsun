<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOremoveHoldByCntrDSQL">
			<desc><![CDATA[Hold Removal 을 위해 테이블에서 데이터 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_IB_CGO_RLSE_HLD_CNTR
WHERE BKG_NO = @[bkg_no] 
  AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
