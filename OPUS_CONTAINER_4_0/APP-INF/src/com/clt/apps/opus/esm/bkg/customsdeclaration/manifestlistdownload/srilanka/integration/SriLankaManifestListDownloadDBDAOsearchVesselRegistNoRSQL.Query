<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SriLankaManifestListDownloadDBDAOsearchVesselRegistNoRSQL">
			<desc><![CDATA[Vessel Registration No 자동생성]]></desc>
			<sql><![CDATA[
SELECT 'V'||DECODE(SIGN(10 - YY), 1, TO_CHAR(YY), CHR(YY+55))||DECODE(MM,10,'A',11,'B',12,'C', TO_CHAR(MM))||DD||TO_CHAR(SYSDATE,'HH24MISS')  vessel_regist_no
	    	FROM   (SELECT TO_NUMBER(SUBSTR(TO_CHAR(SYSDATE,'YYYYMMDD'),3,2))YY,
	    		   TO_NUMBER(SUBSTR(TO_CHAR(SYSDATE,'YYYYMMDD'),5,2))MM,
	    		   SUBSTR(TO_CHAR(SYSDATE,'YYYYMMDD'),7,2) DD
	    	FROM   DUAL)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
