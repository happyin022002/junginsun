<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOSearchPayTermChngRSQL">
			<desc><![CDATA[BDRCorrectionDBDAOSearchPayTermChngRSQL]]></desc>
			<sql><![CDATA[
SELECT NVL(DECODE(COUNT(*), 0, 'N', 'Y'), 'N') CHN_FLG 
  FROM BKG_CHG_RT     BKGRT 
     , BKG_CHG_RT_HIS BKGRTHIS 
 WHERE BKGRT.BKG_NO      = BKGRTHIS.BKG_NO 
   AND BKGRT.RT_SEQ      = BKGRTHIS.RT_SEQ
   AND BKGRT.CHG_CD      = BKGRTHIS.CHG_CD
   AND BKGRT.FRT_TERM_CD != BKGRTHIS.FRT_TERM_CD 
   AND BKGRTHIS.CORR_NO  = 'TMP0000001' 
   AND BKGRT.BKG_NO      = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
