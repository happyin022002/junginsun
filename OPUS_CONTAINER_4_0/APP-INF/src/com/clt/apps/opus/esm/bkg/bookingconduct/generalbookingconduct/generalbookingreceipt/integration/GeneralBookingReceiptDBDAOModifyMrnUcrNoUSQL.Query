<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModifyMrnUcrNoUSQL">
			<desc><![CDATA[update bkg_reference (MRN, UCR)]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y')
	UPDATE BKG_REF_HIS
	    SET CUST_REF_NO_CTNT = @[cust_ref_no_ctnt]
	    ,BKG_REF_TP_CD = @[bkg_ref_tp_cd]
		,CNTR_NO = @[cntr_no]
	    ,UPD_USR_ID = @[upd_usr_id]
	    ,UPD_DT = sysdate
		,REF_NO = @[ref_no]
	WHERE BKG_NO = @[bkg_no]
	AND CORR_NO = 'TMP0000001'
	AND REF_SEQ = @[ref_seq]
#else
	UPDATE BKG_REFERENCE
	    SET CUST_REF_NO_CTNT = @[cust_ref_no_ctnt]
	    ,BKG_REF_TP_CD = @[bkg_ref_tp_cd]
		,CNTR_NO = @[cntr_no]
	    ,UPD_USR_ID = @[upd_usr_id]
	    ,UPD_DT = sysdate
		,REF_NO = @[ref_no]
	WHERE BKG_NO = @[bkg_no]
	AND REF_SEQ = @[ref_seq]
#end			]]></sql>
			<params>
				<param name="cust_ref_no_ctnt" type="12" value="" out="N"/>
				<param name="bkg_ref_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ref_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ref_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
