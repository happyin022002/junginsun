<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchEmptyCntrByBKGRSQL">
			<desc><![CDATA[Empty Repo Bkg에 속해 있는 Container No List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  CNTR_NO,
        CNTR_TPSZ_CD,
        MCNTR_BDL_NO,
        BDL_BTM_FLG
#if (${ca_flg} == 'Y') 
FROM    BKG_CNTR_HIS
#else 
FROM    BKG_CONTAINER
#end
WHERE   BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y')
AND   CORR_NO = 'TMP0000001'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
