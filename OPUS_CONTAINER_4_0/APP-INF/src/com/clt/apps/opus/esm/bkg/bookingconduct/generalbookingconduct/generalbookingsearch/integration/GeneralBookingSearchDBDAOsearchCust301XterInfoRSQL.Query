<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchCust301XterInfoRSQL">
			<desc><![CDATA[searchCust301XterInfo]]></desc>
			<sql><![CDATA[
SELECT	'{I_BKG_INFO'											||CHR(10)||
		'IB_EDI_ORG_ID:'||NVL(mst.ACT_CUST_REF_NO, ' ')			||CHR(10)||
		'IB_EDI_ID:'	||NVL(mst.xter_sndr_id, ' ')			||CHR(10)||
		'IB_POL_CD:'    ||NVL(mst.POL_CD, ' ')      			||CHR(10)||
		'IB_POD_CD:'    ||NVL(mst.POD_CD, ' ')      			||CHR(10)||
		'IB_PICKUP_DT:'	||TO_CHAR(mst.mty_pkup_DT, 'RRRRMMDD')	||CHR(10)||
		'IB_ARV_R_DT:'	||TO_CHAR(mst.RQST_ARR_DT, 'RRRRMMDD')	||CHR(10)||
		'IB_RCV_TERM:'	||MST.RCV_TERM_CD						||CHR(10)||
		'IB_DLV_TERM:'	||MST.DE_TERM_CD						||CHR(10)||
		'CNTRTS_CD:'	||NVL(qty.CNTR_tpsz_CD, ' ')			||CHR(10)||
		'IBI_POR_CD:'	||NVL(inst.POR_CTNT, ' ')				||CHR(10)||
		'IBI_POR_CD_TP_CD:'	||INST.XTER_POR_TP_CD				||CHR(10)||
		'IBI_POR_NM:'	||NVL(inst.POR_NM,   ' ')				||CHR(10)||
		'IBI_POL_CD:'	||NVL(inst.POL_CTNT, ' ')				||CHR(10)||
		'IBI_POL_CD_TP_CD:'	||INST.XTER_POL_TP_CD				||CHR(10)||
		'IBI_POL_NM:'	||NVL(inst.POL_NM,   ' ')				||CHR(10)||
		'IBI_POD_CD:'	||NVL(inst.POD_CTNT, ' ')				||CHR(10)||
		'IBI_POD_CD_TP_CD:'	||INST.XTER_POD_TP_CD				||CHR(10)||
		'IBI_POD_NM:'	||NVL(inst.POD_NM,   ' ')				||CHR(10)||
		'IBI_DEL_CD:'	||NVL(inst.DEL_CTNT, ' ')				||CHR(10)||
		'IBI_DEL_CD_TP_CD:'	||NVL(INST.XTER_DEL_TP_CD,   ' ')	||CHR(10)||
		'IBI_DEL_NM:'	||NVL(inst.DEL_NM,   ' ')				||CHR(10)||
		'IBI_TRANS_IND:'||NVL(inst.TRNS_IND_CTNT, ' ') 			||CHR(10)||
		'IBI_PARTNER_MSG_DT:'|| CASE WHEN SUBSTR(TO_CHAR(INST.PRNR_MSG_DT,'yyyymmddhh24miss'),9,6) = '000000' THEN TO_CHAR(INST.PRNR_MSG_DT,'yyyymmdd')
                                     WHEN SUBSTR(TO_CHAR(INST.PRNR_MSG_DT,'yyyymmddhh24miss'),13,2) = '00' THEN TO_CHAR(INST.PRNR_MSG_DT,'yyyymmddhh24mi')
                                ELSE TO_CHAR(PRNR_MSG_DT,'yyyymmddhh24miss')
                                END	||CHR(10)||
		'IBI_IB_BKG_IND:'|| CASE WHEN (SELECT COUNT(*) FROM BKG_NTC_HIS BNH WHERE BNH.BKG_NO = MST.bkg_no
                                                  AND BNH.NTC_VIA_CD = 'E' AND BNH.NTC_KND_CD = 'BK') = 0 THEN NVL(MST.XTER_BKG_RQST_STS_CD, ' ')
                            ELSE 
                            (SELECT DECODE(HIS.BKG_HIS_ISS_UI_ID,'ESM_BKG_0229',NVL(MST.XTER_BKG_RQST_STS_CD, ' '),' ')
                             FROM BKG_HIS_MST HIS
                             WHERE HIS.BKG_NO = MST.BKG_NO
                             AND HIS.HIS_SEQ = (SELECT MAX(HISD.HIS_SEQ) FROM BKG_HIS_MST HISD WHERE HISD.BKG_NO = HIS.BKG_NO AND HISD.BKG_HIS_ISS_UI_ID IS NOT NULL) AND ROWNUM = 1) END			||CHR(10)||
		'IBI_CUST_REMARK:'||  REPLACE(REPLACE(NVL(TO_CLOB(MST.XTER_BKG_RMK1)||TO_CLOB(MST.XTER_BKG_RMK2), ' '),CHR(10),' '),CHR(13),' ') ||CHR(10)||
		'}I_BKG_INFO' I_BKG_INFO
  FROM bkg_xter_rqst_mst mst
        , bkg_xter_QTY qty
        , bkg_xter_instr inst
 where mst.xter_sndr_id  = qty.xter_sndr_id  (+)
   and mst.xter_rqst_no  = qty.xter_rqst_no  (+)
   and mst.xter_rqst_seq = qty.xter_rqst_seq (+)
   and mst.xter_sndr_id  = inst.xter_sndr_id (+)
   and mst.xter_rqst_no  = inst.xter_rqst_no (+)
   and mst.xter_rqst_seq = inst.xter_rqst_seq(+)
   AND mst.xter_sndr_id  = @[sender_id]
   and mst.xter_rqst_no  = @[rqst_no]
   and mst.xter_rqst_seq = @[rqst_seq]			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
