<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandCustomsTransmissionDBDAOSearchDescInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT BKG_SPCLCHAR_CONV_FNC(SUBSTR(DOC.PCK_CMDT_DESC||DOC.CNTR_CMDT_DESC||MK.CMDT_DESC, 1, 279), 'Y') AS DESCRIPTION

  FROM BKG_BL_MK_DESC MK,
       BKG_BL_DOC DOC

 WHERE 1=1
   AND MK.BKG_NO = DOC.BKG_NO
   AND DOC.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
