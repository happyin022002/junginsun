<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchBkgForCopyRSQL">
			<desc><![CDATA[Copy용 Booking정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
		 BK.BKG_NO
	   , BK.VSL_CD||BK.SKD_VOY_NO||BK.SKD_DIR_CD BKG_TRUNK_VVD
	   , BK.POR_CD
	   , BK.POL_CD
	   , BK.POD_CD
	   , BK.DEL_CD
	   , BK.RFA_NO RFA_NO
	   , BK.RFA_NO RFA_NO_OLD
       , BK.SC_NO SC_NO
       , BK.SC_NO SC_NO_OLD
       , BK.TAA_NO TAA_NO
	   , BK.TAA_NO TAA_NO_OLD
       , BK.DCGO_FLG
       , BK.RC_FLG
       , BK.AWK_CGO_FLG
       , BK.BB_CGO_FLG
--       , DECODE(STWG_CD,NULL,'N','','N','Y') STOWAGE_FLG
       , DECODE(ST.STWG_CD,NULL,'N','','N','Y') STOWAGE_FLG
       , BK.HNGR_FLG
       , DECODE(BK.STOP_OFF_LOC_CD,NULL,'N','','N','Y') STOP_OFF_FLG
       , DECODE(BK.RAIL_BLK_CD,NULL,'N','','N','Y') BULK_RAIL_FLG
       , BK.HOT_DE_FLG
       , BK.SPCL_HIDE_FLG
       , BK.FD_GRD_FLG
       , BK.PRCT_FLG
       , DECODE(BK.XTER_RMK,NULL,'N','','N','Y') REMARK_FLG	
	   , '' PCTL_NO
	   , BK.BKG_CTRL_PTY_CUST_CNT_CD
	   , BK.BKG_CTRL_PTY_CUST_SEQ
	   , BK.BKG_WT_CHK_FLG
	   , BK.EDI_HLD_FLG
FROM	BKG_BOOKING BK
		,BKG_STWG_CGO ST
WHERE	BK.BKG_NO = @[bkg_no]
AND		BK.BKG_NO = ST.BKG_NO(+)
AND		1		  = ST.STWG_SEQ(+)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
