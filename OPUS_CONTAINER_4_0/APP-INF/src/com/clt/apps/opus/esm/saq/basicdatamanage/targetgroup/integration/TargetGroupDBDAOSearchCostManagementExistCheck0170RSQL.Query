<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TargetGroupDBDAOSearchCostManagementExistCheck0170RSQL">
			<desc><![CDATA[Description : Year/Month Set for Cost Management  Create Initial Data 버튼 활성화 유무]]></desc>
			<sql><![CDATA[
SELECT DISTINCT BSE_YR, 
				BSE_QTR_CD  
FROM SAQ_COST_APPL_BSE             
WHERE BSE_YR||BSE_QTR_CD < @[bse_yr]||@[bse_qtr_cd]
#if (${bse_qtr_cd} == '00') 
	AND BSE_QTR_CD = '00'             	
#else 
	AND BSE_QTR_CD != '00'            
#end
	AND ROWNUM = 1                   
ORDER BY BSE_YR DESC, 
		 BSE_QTR_CD DESC			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
