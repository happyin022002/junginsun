<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOHblCntrMfDescRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
SELECT B.BKG_NO
,      B.CNTR_MF_SEQ
,      B.CNTR_NO
,      B.PCK_QTY
,      B.PCK_TP_CD
,      B.CNTR_MF_WGT
,      B.WGT_UT_CD
,      B.MEAS_QTY
,      B.MEAS_UT_CD
,      B.DCGO_FLG
,      B.BB_CGO_FLG
,      B.AWK_CGO_FLG
,      B.RC_FLG
,      B.RD_CGO_FLG
,      B.HNGR_FLG
,      B.CNTR_MF_MK_DESC
,      B.CNTR_MF_GDS_DESC
,      B.HAMO_TRF_CD
,      B.CMDT_HS_CD
,      B.NCM_NO
,      B.CNTR_MF_NO
,      B.HBL_SEQ
FROM   BKG_HBL_HIS A, BKG_CNTR_MF_DESC_HIS B
WHERE  A.BKG_NO = B.BKG_NO
AND    A.CNTR_MF_NO = B.CNTR_MF_NO
AND    A.BKG_NO = @[bkg_no]
AND    A.CORR_NO = 'TMP0000001'
AND    B.CORR_NO = 'TMP0000001'
#else 
SELECT B.BKG_NO
,      B.CNTR_MF_SEQ
,      B.CNTR_NO
,      B.PCK_QTY
,      B.PCK_TP_CD
,      B.CNTR_MF_WGT
,      B.WGT_UT_CD
,      B.MEAS_QTY
,      B.MEAS_UT_CD
,      B.DCGO_FLG
,      B.BB_CGO_FLG
,      B.AWK_CGO_FLG
,      B.RC_FLG
,      B.RD_CGO_FLG
,      B.HNGR_FLG
,      B.CNTR_MF_MK_DESC
,      B.CNTR_MF_GDS_DESC
,      B.HAMO_TRF_CD
,      B.CMDT_HS_CD
,      B.NCM_NO
,      B.CNTR_MF_NO
,      B.HBL_SEQ
FROM   BKG_HBL A, BKG_CNTR_MF_DESC B
WHERE  A.BKG_NO = B.BKG_NO
AND    A.CNTR_MF_NO = B.CNTR_MF_NO
AND    A.BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
