<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaCustomsTransmissionDBDAOsearchIgmMaxSeqRSQL">
			<desc><![CDATA[Max Seq.를 조회해 온다.]]></desc>
			<sql><![CDATA[
SELECT 
    NVL(MAX(GEN_SEQ), 0) MAX_GEN_SEQ
FROM BKG_CSTMS_IDA_TP_RQST_SEQ
WHERE   IDA_SHPR_ID	 =	'S'
AND	    LOCL_AGN_ID	 =	@[ida_agn_id]
AND	    GEN_DT	     =	TO_DATE(@[gen_dt], 'YYMMDD')			]]></sql>
			<params>
				<param name="ida_agn_id" type="12" value="" out="N"/>
				<param name="gen_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
