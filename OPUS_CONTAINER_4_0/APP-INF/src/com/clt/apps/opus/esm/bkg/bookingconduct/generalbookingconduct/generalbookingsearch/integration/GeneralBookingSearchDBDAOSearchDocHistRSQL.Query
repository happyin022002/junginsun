<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchDocHistRSQL">
			<desc><![CDATA[GeneralBookingSearchDBDAOSearchDocHistRSQL]]></desc>
			<sql><![CDATA[
SELECT (SELECT DOC_PROC_DESC
		  FROM BKG_DOC_PROC_TP
	     WHERE BKG_DOC_PROC_TP_CD = HIS.BKG_DOC_PROC_TP_CD) ITEM
        , TO_CHAR(HIS.EVNT_DT, 'YYYY-MM-DD HH24:MI') CRE_DT
		, TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(BKG_COM_USER_LOC_FNC(HIS.UPD_USR_ID), HIS.EVNT_DT, 'GMT'), 'YYYY-MM-DD HH24:MI') GMT_DT
        , USR.USR_NM CRE_USR_ID
        , USR.OFC_CD OFFICE
        , '' REMARK 
        , '' REASON
  FROM BKG_DOC_PROC_SKD HIS 
     , COM_USER USR
 WHERE HIS.UPD_USR_ID = USR.USR_ID(+)
   AND BKG_NO = @[bkg_no]
 ORDER BY EVNT_DT			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
