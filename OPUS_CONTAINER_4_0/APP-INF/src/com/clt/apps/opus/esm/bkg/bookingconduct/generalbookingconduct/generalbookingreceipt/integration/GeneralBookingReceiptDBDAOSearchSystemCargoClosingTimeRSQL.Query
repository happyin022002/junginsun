<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchSystemCargoClosingTimeRSQL">
			<desc><![CDATA[SearchSystemCargoClosingTime]]></desc>
			<sql><![CDATA[
SELECT SYS_SET_DT 
  FROM BKG_CLZ_TM 
 WHERE BKG_NO = @[bkg_no]
   AND CLZ_TP_CD = @[clz_tp_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="clz_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
