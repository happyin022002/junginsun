<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOReportItemVORSQL">
			<desc><![CDATA[2010.09.10 김영철 [ ] VIP REPORT 부분 오류 수정 ( 반영일 : 2010.09.24 )]]></desc>
			<sql><![CDATA[
SELECT
A.TBL_NM
,	A.COL_NM
,	B.DP_NM  AS ITEM_NM
,	B.SQL_CTNT
,	A.RPT_ID
,	A.BKG_RPT_KND_CD
#if (${bkg_rpt_knd_cd} == 'V')
,	B.VIP_RPT_ORD_SEQ AS ORD_SEQ
#else
,	B.STS_RPT_ORD_SEQ AS ORD_SEQ
#end
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,	A.UPD_DT
FROM BKG_RPT_DFLT_DTL A
    ,BKG_TBL_COL      B
WHERE B.TBL_NM = A.TBL_NM
AND B.COL_NM = A.COL_NM
AND A.RPT_ID = @[rpt_id]
AND	A.BKG_RPT_KND_CD = @[bkg_rpt_knd_cd]
--ORDER BY B.VIP_RPT_ORD_SEQ ASC
ORDER BY A.ORD_SEQ ASC			]]></sql>
			<params>
				<param name="rpt_id" type="12" value="" out="N"/>
				<param name="bkg_rpt_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
