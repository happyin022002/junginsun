<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshCustomsReportDBDAOmodifyLicenseInfoUSQL">
			<desc><![CDATA[bangladesh 세관 customer License 정보 update 쿼리]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_BD_FRT_FWRD_LIC
   SET CUST_LIC_NO = @[cust_lic_no],
	   UPD_USR_ID = @[upd_usr_id],
	   UPD_DT = SYSDATE
 WHERE CNT_CD = SUBSTR(@[cust_cd],1,2)
   AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3)) 			]]></sql>
			<params>
				<param name="cust_lic_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
