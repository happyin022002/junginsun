<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOsearchBkgBookingByBlNoRSQL">
			<desc><![CDATA[bl_no 로 Booking 정보 조회]]></desc>
			<sql><![CDATA[
SELECT *
FROM   BKG_BOOKING
WHERE  BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
