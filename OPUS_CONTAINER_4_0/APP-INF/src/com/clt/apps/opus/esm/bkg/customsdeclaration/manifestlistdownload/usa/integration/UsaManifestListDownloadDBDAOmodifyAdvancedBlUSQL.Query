<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyAdvancedBlUSQL">
			<desc><![CDATA[미세관 응답메세지 수신 결과를 BL 테이블에 갱신한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_BL
   SET CSTMS_LOC_CD = @[cus_loc]
 WHERE CNT_CD = 'US'
   AND BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="cus_loc" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
