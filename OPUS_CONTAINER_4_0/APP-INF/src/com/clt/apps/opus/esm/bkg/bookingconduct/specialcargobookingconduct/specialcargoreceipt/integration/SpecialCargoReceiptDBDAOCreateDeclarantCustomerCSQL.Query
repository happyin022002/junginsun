<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOCreateDeclarantCustomerCSQL">
			<desc><![CDATA[DG Shipper , DG Consignee 정보를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_DG_DECL
(
	BKG_NO,
	DCGO_SEQ,
	DG_DECL_SEQ,
	DG_CNTR_SEQ, 
	CNTR_CGO_SEQ,
	CNTR_NO,
	BKG_CUST_TP_CD,
	CUST_CNT_CD,
	CUST_SEQ,
	CUST_NM,
	CUST_ADDR,
	CUST_CTY_NM,
	CUST_STE_CD,
	CSTMS_DECL_CNT_CD,
	CUST_ZIP_ID,
	PHN_NO,
	CUST_FAX_NO,
	CUST_EML,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT,
	DECL_NM
)VALUES
(
	@[bkg_no],
	1,
	(SELECT NVL(MAX(DG_DECL_SEQ),0)+1 FROM BKG_DG_DECL WHERE BKG_NO=@[bkg_no] AND DG_CNTR_SEQ=@[dg_cntr_seq]),
	@[dg_cntr_seq],
	@[cntr_cgo_seq],
#if (${ca_flg} == 'Y')
	(SELECT CNTR_NO FROM BKG_DG_CGO_HIS WHERE BKG_NO = @[bkg_no] AND CORR_NO = 'TMP0000001' AND DG_CNTR_SEQ = @[dg_cntr_seq] AND ROWNUM =1),
#else
	(SELECT CNTR_NO FROM BKG_DG_CGO WHERE BKG_NO = @[bkg_no] AND DG_CNTR_SEQ = @[dg_cntr_seq] AND ROWNUM = 1),
#end
	@[bkg_cust_tp_cd],
	@[cust_cnt_cd],
	@[cust_seq],
	@[cust_nm],
	@[cust_addr],
	@[cust_cty_nm],
	@[cust_ste_cd],
	@[cstms_decl_cnt_cd],
	@[cust_zip_id],
	@[phn_no],
	@[cust_fax_no],
	@[cust_eml],
	@[cre_usr_id],
	SYSDATE,
	@[upd_usr_id],
	SYSDATE,
	@[decl_nm]
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dg_cntr_seq" type="12" value="" out="N"/>
				<param name="cntr_cgo_seq" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="cust_addr" type="12" value="" out="N"/>
				<param name="cust_cty_nm" type="12" value="" out="N"/>
				<param name="cust_ste_cd" type="12" value="" out="N"/>
				<param name="cstms_decl_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_zip_id" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="cust_fax_no" type="12" value="" out="N"/>
				<param name="cust_eml" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="decl_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
