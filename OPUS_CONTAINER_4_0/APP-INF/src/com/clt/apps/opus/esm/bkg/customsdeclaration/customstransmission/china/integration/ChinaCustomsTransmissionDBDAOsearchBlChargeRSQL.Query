<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaCustomsTransmissionDBDAOsearchBlChargeRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(BCR.CHG_CD, ' ') AS FCCODE,
       NVL(BCR.RAT_AS_QTY, 0) AS QTY,
       NVL(BCR.CURR_CD, ' ') AS CURR_CD,
       NVL(BCR.CHG_UT_AMT, 0) AS RATE,
       NVL(BCR.CHG_AMT, 0) AS AMOUNT,
       NVL(BCR.FRT_TERM_CD, ' ') AS PRFLAG

  FROM BKG_CHG_RT BCR,
       BKG_BOOKING BKG

 WHERE BKG.BL_NO = @[bl_no]
   AND BCR.BKG_NO = BKG.BKG_NO
   AND BCR.FRT_INCL_XCLD_DIV_CD = 'N'
			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
