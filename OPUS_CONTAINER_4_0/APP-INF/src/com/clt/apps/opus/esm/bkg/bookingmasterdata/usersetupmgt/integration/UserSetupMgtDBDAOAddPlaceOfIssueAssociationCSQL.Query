<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOAddPlaceOfIssueAssociationCSQL">
			<desc><![CDATA[Add PlaceOfIssueAssociation]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BL_ESIG_OFC_ASGN (
	BL_ESIG_ASGN_SEQ
	, BL_ISS_OFC_CD
	, BL_ISS_OFC_NM
	, CNT_CD
	, AGN_CO_NM1
	, AGN_CO_NM2
	, AGN_CO_NM3
	, BL_ESIG_SEQ
	, BL_ESIG_RMK
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
	, BL_ESIG_FLG
	, BL_CPY_ESIG_FLG
	, BL_KNT_FLG
) VALUES (
	NVL((SELECT MAX(BL_ESIG_ASGN_SEQ)+1 FROM BKG_BL_ESIG_OFC_ASGN), 1)
	, @[bl_iss_ofc_cd]
	, @[bl_iss_ofc_nm]
	, @[cnt_cd]
	, @[agn_co_nm1]
	, @[agn_co_nm2]
	, @[agn_co_nm3]
	, @[bl_esig_seq]
	, @[bl_esig_rmk]
	, @[cre_usr_id]
	, SYSDATE
	, @[upd_usr_id]
	, SYSDATE
	, NVL(@[bl_esig_flg], 'N')
	, NVL(@[bl_cpy_esig_flg], 'N')
	, NVL(@[bl_knt_flg], 'N')
)			]]></sql>
			<params>
				<param name="bl_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_iss_ofc_nm" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="agn_co_nm1" type="12" value="" out="N"/>
				<param name="agn_co_nm2" type="12" value="" out="N"/>
				<param name="agn_co_nm3" type="12" value="" out="N"/>
				<param name="bl_esig_seq" type="12" value="" out="N"/>
				<param name="bl_esig_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_esig_flg" type="12" value="" out="N"/>
				<param name="bl_cpy_esig_flg" type="12" value="" out="N"/>
				<param name="bl_knt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
