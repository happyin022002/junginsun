<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOBkgCustomerAddrRSQL">
			<desc><![CDATA[Booking Customer 정보]]></desc>
			<sql><![CDATA[
SELECT 
	UPPER(REPLACE(bzet_addr, '@*', CHR(10))) AS ADDR,
	(SELECT UPPER(CUST_LGL_ENG_NM) FROM MDM_CUSTOMER WHERE CUST_CNT_CD = @[cust_cnt_cd] AND CUST_SEQ = @[cust_seq]) AS CUST_LGL_ENG_NM,
	(SELECT FRT_FWRD_FMC_NO FROM MDM_CUSTOMER WHERE CUST_CNT_CD = @[cust_cnt_cd] AND CUST_SEQ = @[cust_seq]) AS FMC_NO
FROM MDM_CUST_ADDR
WHERE CUST_CNT_CD   = @[cust_cnt_cd]
AND CUST_SEQ		= @[cust_seq]
AND PRMRY_CHK_FLG 	= 'Y'
AND ROWNUM  		= 1			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
