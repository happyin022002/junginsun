<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOBkgMdmCustomerExtRSQL">
			<desc><![CDATA[BkgMdmCustomerExt RSQL]]></desc>
			<sql><![CDATA[
SELECT 'C' as tro_act_cust_knd_cd, 
       cust_cnt_cd||cust_seq cust_cd, 
       cust_cnt_cd, 
       cust_seq, 
       cust_lgl_eng_nm, 
       ofc_cd,  
       LPAD(cust_seq, 6, '0') nvocc_co_scac_cd 
  FROM mdm_customer
 WHERE cust_cnt_cd = @[cust_cnt_cd]    
   AND CNTR_DIV_FLG = 'Y' 
   AND DELT_FLG = 'N' 
#if (${cust_seq} != '') 
  AND cust_seq = @[cust_seq] 
#end
#if (${cust_lgl_eng_nm} != '') 
  AND UPPER(cust_lgl_eng_nm) like UPPER(@[cust_lgl_eng_nm])||'%'
#end			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_lgl_eng_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
