<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchDoRefInfoRSQL">
			<desc><![CDATA[D/O Release를 위한 기본 Reference정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
,	CSTMS_REF_NM
,	CSTMS_REF_CTNT
,	CSTMS_ASGN_NM
,	CSTMS_ASGN_CTNT
,	INTER_RMK
,	DO_HLD_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	INFO_CGO_FLG
,	IDA_IMP_GEN_MF_NO
,	IDA_CGOR_ORD_YR
,	IDA_CSTMS_ASGN_LINE_NO
,	DO_SPLIT_FLG
,	CY_OP_CD
FROM BKG_DO_REF
WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
