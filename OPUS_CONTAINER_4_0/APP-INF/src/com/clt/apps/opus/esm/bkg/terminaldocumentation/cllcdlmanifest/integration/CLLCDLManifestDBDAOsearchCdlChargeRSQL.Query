<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCdlChargeRSQL">
			<desc><![CDATA[searchCdlCharge]]></desc>
			<sql><![CDATA[
SELECT	'{CHARGE'||CHR(10)||
	'FCTYPE:'||NVL(CHG_CD, ' ')||CHR(10)||
	'RATE:'||NVL(CHG_UT_AMT, 0)||CHR(10)||
	'REVENUETON:'||NVL(RAT_AS_QTY,0)||CHR(10)||
	DECODE(FRT_TERM_CD,'P','PPD:'||
	NVL(CHG_AMT,0),'PPD:'||0.0)||CHR(10)||
	DECODE(FRT_TERM_CD,'C','CCT:'||
	NVL(CHG_AMT,0),'CCT:'||0.0)||CHR(10)||
	'CURRENCYCODE:'||NVL(CURR_CD,' ')||CHR(10)||
	'EXCHRATE:'||CHR(10)||
	'TARIFF:'||NVL(TRF_ITM_NO,' ')||CHR(10)||
	'PERTYPE:'||NVL(RAT_UT_CD,' ')||CHR(10)||
	'RATEOFC:'||NVL(N3PTY_RCV_OFC_CD,' ')||CHR(10)||
	'}CHARGE'||CHR(10) CHARGE_INFO
FROM	BKG_CHG_RT
WHERE	BKG_NO = @[in_bkg_no]
AND	FRT_INCL_XCLD_DIV_CD = 'N'			]]></sql>
			<params>
				<param name="in_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
