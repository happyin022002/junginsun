<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAOGetBkgBdrFlgInfoRSQL">
			<desc><![CDATA[GetBkgBdrFlgInfo]]></desc>
			<sql><![CDATA[
SELECT NVL(BDR_FLG,'N') AS BDR_FLG 
FROM BKG_BL_DOC
WHERE bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
