<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCycleByCtmUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE  BKG_CONTAINER
SET     CNMV_CYC_NO   = 9999,
        CNMV_STS_CD   = 'MT',
        CNMV_EVNT_DT  = TO_DATE(@[cnmv_evnt_dt], 'ddMonrr hh24:mi'),
        DIFF_RMK      = 'MT repo VL deleted'
WHERE   BKG_NO        = @[bkg_no]
AND     CNTR_NO       = @[cntr_no]
AND     (CNMV_CYC_NO  = @[cnmv_cyc_no] OR CNMV_CYC_NO  = 9999)			]]></sql>
			<params>
				<param name="cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_cyc_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
