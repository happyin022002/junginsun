<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPSAPortListRSQL">
			<desc><![CDATA[EDI전송을 위해 Yard Code를 PSA Port로 Coversion을 위해 PSA Port조회]]></desc>
			<sql><![CDATA[
SELECT   LOC_CD, PSA_LOC_CD, substr(TML_CD,6,2) TML_CD
FROM     BKG_CSTMS_PSA_PORT
WHERE    LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
