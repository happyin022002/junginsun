<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDBDAOSearchVendorInfoByContractNoRSQL">
			<desc><![CDATA[Search Vendor Name]]></desc>
			<sql><![CDATA[
SELECT V.VNDR_SEQ
     , V.VNDR_LGL_ENG_NM
  FROM FMS_CONTRACT C
     , MDM_CUSTOMER M
     , MDM_VENDOR V
 WHERE C.CUST_CNT_CD = M.CUST_CNT_CD
   AND C.CUST_SEQ = M.CUST_SEQ
   AND M.VNDR_SEQ = V.VNDR_SEQ
   AND C.FLET_CTRT_NO = @[flet_ctrt_no]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
