<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOcopyBkgRateByBkgCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
insert into bkg_rate(BKG_NO 
,RT_BL_TP_CD 
,FRT_TERM_CD 
,BKG_CTRT_TP_CD 
,PPD_RCV_OFC_CD 
,PPD_PAYR_CNT_CD 
,PPD_PAYR_CUST_SEQ 
,CLT_OFC_CD 
,CLT_PAYR_CNT_CD 
,CLT_PAYR_CUST_SEQ  
,REV_DIV_CD 
,BKG_RT_WHF_EXPT_CD 
,WHF_SHPR_RGST_NO  
,RT_APLY_DT 
,CGO_RCV_DT 
,DIFF_RMK 
,AUD_STS_CD  
,CRE_USR_ID 
,CRE_DT 
,UPD_USR_ID 
,UPD_DT
,DOC_LOC_CD
)
select @[targetBkg] BKG_NO 
,RT_BL_TP_CD 
,FRT_TERM_CD 
,BKG_CTRT_TP_CD 
,PPD_RCV_OFC_CD 
,PPD_PAYR_CNT_CD 
,PPD_PAYR_CUST_SEQ 
,CLT_OFC_CD 
,CLT_PAYR_CNT_CD 
,CLT_PAYR_CUST_SEQ 
,REV_DIV_CD 
,BKG_RT_WHF_EXPT_CD 
,WHF_SHPR_RGST_NO  
,RT_APLY_DT 
,CGO_RCV_DT 
,DIFF_RMK 
,AUD_STS_CD  
,@[usr_id] CRE_USR_ID 
,sysdate CRE_DT 
,@[usr_id] UPD_USR_ID 
,sysdate UPD_DT
,DOC_LOC_CD
from bkg_rate
where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="targetBkg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
