<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOContractDAOFmsOtrExpnCSQL">
			<desc><![CDATA[TCharterIOContractDAOFmsOtrExpnCSQL]]></desc>
			<sql><![CDATA[
insert into fms_otr_expn (
	flet_ctrt_no,
	acct_cd,
	acct_itm_seq,
	eff_dt,
	exp_dt,
	curr_cd,
	otr_expn_amt,
	cre_usr_id,
	cre_dt,
	upd_usr_id,
	upd_dt
) values( 
	@[flet_ctrt_no],
	@[acct_cd],
	@[acct_itm_seq],
	TO_DATE(@[eff_dt],'YYYYMMDD'),
	TO_DATE(@[exp_dt],'YYYYMMDD'),
	@[curr_cd],
	@[otr_expn_amt],
	@[cre_usr_id],
	sysdate,
	@[upd_usr_id],
	sysdate
)			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="acct_itm_seq" type="2" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="otr_expn_amt" type="3" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
