<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOmodifyMoveTypeByBkgUSQL">
			<desc><![CDATA[Modify Move Type of BKG_BL_ISS by BKG RD Term]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')
UPDATE BKG_BL_DOC
SET BL_MV_TP_NM
= DECODE(@[rcv_term_cd]||@[de_term_cd],'YY',1 ,'YD',1 ,'YS',3 ,'YT',10,'YO',11,'YM',2 
								      ,'DY',1 ,'DD',1 ,'DS',3 ,'DT',10,'DO',11,'DM',2 
									  ,'SY',7 ,'SD',7 ,'SS',9 ,'ST',14,'SO',15,'SM',8 
									  ,'TY',16,'TD',16,'TS',18,'TT',19,'TO',20,'TM',17
									  ,'IY',21,'ID',21,'IS',23,'IT',24,'IO',25,'IM',22
									  ,'MY',4 ,'MD',4 ,'MS',6 ,'MT',12,'MO',13,'MM',5 )
WHERE BKG_NO = @[bkg_no]
  AND CORR_NO = 'TMP0000001'
#else
UPDATE BKG_BL_DOC
SET BL_MV_TP_NM
= DECODE(@[rcv_term_cd]||@[de_term_cd],'YY',1 ,'YD',1 ,'YS',3 ,'YT',10,'YO',11,'YM',2 
								      ,'DY',1 ,'DD',1 ,'DS',3 ,'DT',10,'DO',11,'DM',2 
									  ,'SY',7 ,'SD',7 ,'SS',9 ,'ST',14,'SO',15,'SM',8 
									  ,'TY',16,'TD',16,'TS',18,'TT',19,'TO',20,'TM',17
									  ,'IY',21,'ID',21,'IS',23,'IT',24,'IO',25,'IM',22
									  ,'MY',4 ,'MD',4 ,'MS',6 ,'MT',12,'MO',13,'MM',5 )
WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="rcv_term_cd" type="12" value="" out="N"/>
				<param name="de_term_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
