<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesActivityManageDBDAOAddCustSatisFactionCSQL">
			<desc><![CDATA[Sales Activity Report 화면의 3번째 탭 ( Cust.SatisFaction ) 생성 후 저장용 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO SAM_CUST_SATSFC (
                              CUST_CNT_CD
                            , CUST_SEQ
                            , SREP_CD
                            , SLS_ACT_SEQ
                            , CUST_SATSFC_ITM_CD
                            , CUST_SATSFC_GRD_CD
                            , CUST_SATSFC_RSN
                            , CRE_USR_ID
                            , CRE_DT
                            , UPD_USR_ID
                            , UPD_DT    
                            )
VALUES ( @[cust_cnt_cd]
       , @[cust_seq]
       , @[srep_cd]
       , @[sls_act_seq]
       , @[cust_satsfc_itm_cd]
       , @[grd]
       , @[rsn]
       , @[cre_usr_id]
       , sysdate
       , @[cre_usr_id]
       , sysdate
       )			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="sls_act_seq" type="12" value="" out="N"/>
				<param name="cust_satsfc_itm_cd" type="12" value="" out="N"/>
				<param name="grd" type="12" value="" out="N"/>
				<param name="rsn" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
