<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandCustomsTransmissionDBDAOSearchErrorReportRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(SND XPKBKG_CSTMS_NZL_SND_LOG) */
    '' MSG_ACK_RSLT
	,'' ERR_CODE
	,EDI_RCV_MSG_CTNT
	,RCV_KEY_DAT_CTNT
FROM BKG_CSTMS_NZL_SND_LOG SND
WHERE BL_NO = @[bl_no]
    AND VSL_CD = SUBSTR(@[vvd], 1, 4)
    AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
    AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
    AND POD_CD = @[vvd_pod_cd]
    AND CNTR_NO = @[cntr_no]
    AND LOG_FLG = 'Y'
    AND CSTMS_RQST_FLG = 'Y'
    AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="vvd_pod_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
