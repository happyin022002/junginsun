<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLaneBoundSwitchRlaneListRSQL">
			<desc><![CDATA[Rlane List 조회 ]]></desc>
			<sql><![CDATA[
  SELECT DISTINCT RLANE_CD AS TEXT,                       
                  RLANE_CD AS CODE                        
    FROM MDM_DTL_REV_LANE                                 
   WHERE DELT_FLG   = 'N'                                 
     AND TRD_CD     = @[trd_cd]                                   
     AND SUB_TRD_CD LIKE @[sub_trd_cd] || '%'                         
ORDER BY DECODE(TEXT,'RBCCO','ZZ',SUBSTR(TEXT,-2)), TEXT  			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
