<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOHblCustTmpltRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT 
	OFC_CD
,	PROF_SEQ
,	ACT_SHPR_NM
,	ACT_SHPR_ADDR
,	ULTI_CNEE_NM
,	ULTI_CNEE_ADDR
,	REF_NO
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_NVOCC_PROF
WHERE	OFC_CD = @[ofc_cd]
AND    UPPER(ACT_SHPR_NM) like '%' || UPPER(@[act_shpr_nm]) || '%'
AND    UPPER(ULTI_CNEE_NM) like '%' || UPPER(@[ulti_cnee_nm]) || '%'
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="act_shpr_nm" type="12" value="" out="N"/>
				<param name="ulti_cnee_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
