<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchCustomer2RSQL">
			<desc><![CDATA[UsaBlCustomerSecondVO]]></desc>
			<sql><![CDATA[
SELECT CNT_CD 
	  ,BL_NO
      ,BKG_CUST_TP_CD
      ,CUST_CNT_CD
      ,DECODE(CUST_SEQ, 0, '', CUST_SEQ) AS CUST_SEQ
      ,CUST_NM
      ,CUST_ADDR
      ,CUST_NM || CUST_ADDR AS CUST_NM_ADDR
      ,PHN_NO
      ,FAX_NO
FROM   BKG_CSTMS_ADV_CUST
WHERE  1=1   
AND    CNT_CD = @[cnt_cd]
AND    BL_NO = @[bl_no]
AND    BKG_CUST_TP_CD IN ('F','A')
ORDER BY BKG_CUST_TP_CD			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
