<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaCustomsReportDBDAOsearchLocationRSQL">
			<desc><![CDATA[Location Validation Check]]></desc>
			<sql><![CDATA[
SELECT LOC_CD
FROM MDM_LOCATION
WHERE CNT_CD = 'CN'
AND LOC_CD <> 'CNHKG'
AND LOC_CD = @[loc_cd]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
