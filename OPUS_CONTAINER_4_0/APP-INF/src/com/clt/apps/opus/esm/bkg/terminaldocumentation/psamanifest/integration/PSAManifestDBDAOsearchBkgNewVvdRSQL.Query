<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchBkgNewVvdRSQL">
			<desc><![CDATA[BKG VVD에서 POL=SGSIN인 Vessel정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD_CD
  FROM BKG_VVD
 WHERE BKG_NO = @[bkg_no]
   AND POL_CD = 'SGSIN'  			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
