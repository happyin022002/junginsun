<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsReportDBDAOsearchJpcusSendListRSQL">
			<desc><![CDATA[searchJpcusSendList]]></desc>
			<sql><![CDATA[
SELECT JP_SND_LOG_ID,
       SND_DT,
       SND_DT2,
       OFC_CD,
       UPD_USR_ID,
       VVD_CD,
       POD_CD,
       LOG_FLG,
       LOG_DT,
       LOG_DT2,
       LOG_SEQ,
       BL_NO,
       RN,
       TOTAL

  FROM (SELECT JP_SND_LOG_ID,
               SND_DT,
               SND_DT2,
               OFC_CD,
               UPD_USR_ID,
               VVD_CD,
               POD_CD,
               LOG_FLG,
               LOG_DT,
               LOG_DT2,
               LOG_SEQ,
               BL_NO,
               ROW_NUMBER() OVER(ORDER BY SND_DT) AS RN,
               COUNT(*) OVER() AS TOTAL

          FROM (SELECT JP_SND_LOG_ID,
                       TO_CHAR(SND_DT, 'YYYY-MM-DD') AS SND_DT,
                       TO_CHAR(SND_DT, 'HH24:MI:SS') AS SND_DT2,
                       OFC_CD,
                       UPD_USR_ID,
                       NVL(VSL_CD, ' ')||NVL(SKD_VOY_NO, ' ')||NVL(SKD_DIR_CD, ' ') AS VVD_CD,
                       POD_CD,
                       DECODE(LOG_FLG, 'Y', 'NACCS', '') AS LOG_FLG,
                       TO_CHAR(LOG_DT, 'YYYY-MM-DD') AS LOG_DT,
                       TO_CHAR(LOG_DT, 'HH24:MI') AS LOG_DT2,
                       LOG_SEQ,
                       NVL(BL_NO, ' ') AS BL_NO

                  FROM BKG_CSTMS_JP_SND_LOG

                 WHERE JP_SND_LOG_ID LIKE @[jp_snd_log_id]||'%'
                   AND NVL(VSL_CD, '%') LIKE NVL(SUBSTR(@[vvd_cd], 1, 4), '%')
                   AND NVL(SKD_VOY_NO, '%') LIKE NVL(SUBSTR(@[vvd_cd], 5, 4), '%')
                   AND NVL(SKD_DIR_CD, '%') LIKE NVL(SUBSTR(@[vvd_cd], 9, 1), '%')
                   AND NVL(OFC_CD, '%') LIKE @[ofc_cd]||'%'
                   AND NVL(UPD_USR_ID, '%') LIKE @[usr_id]||'%'
                   AND NVL(SND_FLG, ' ') <> 'N'
#if (${date_check} != '')
                   AND SND_DT BETWEEN TO_DATE(@[start_snd_dt], 'YYYY-MM-DD HH24:MI') AND TO_DATE(@[end_snd_dt], 'YYYY-MM-DD HH24:MI')
#end


                 UNION ALL


                SELECT DOR.FLT_FILE_REF_NO,
                       TO_CHAR(DOR.EDI_EVNT_DT, 'YYYY-MM-DD') AS SND_DT,
                       TO_CHAR(DOR.EDI_EVNT_DT, 'HH24:MI:SS') AS SND_DT2,
                       DOR.EDI_EVNT_OFC_CD,
                       DOR.EDI_EVNT_USR_ID, --DOR.UPD_USR_ID,
                       ' ' AS VVD_CD,
                       ' ' AS POD_CD,
                       ' ' AS LOG_FLG,
                       ' ' AS LOG_DT,
                       ' ' AS LOG_DT2,
                       DOR.EDI_SND_SEQ AS LOG_SEQ,
                       NVL(BKG.BL_NO, ' ') AS BL_NO

                  FROM BKG_IB_EDI_SND_LOG DOR,
                       BKG_BOOKING BKG

                 WHERE (@[jp_snd_log_id] IS NULL OR @[jp_snd_log_id] = 'DOR')
                   AND DOR.FLT_FILE_REF_NO LIKE 'DOR'
                   AND NVL(DOR.EDI_EVNT_OFC_CD, '%') LIKE @[ofc_cd]||'%'
                   AND NVL(DOR.EDI_EVNT_USR_ID, '%') LIKE @[usr_id]||'%'
                   AND DOR.BKG_NO = BKG.BKG_NO
#if (${date_check} != '')
                   AND DOR.EDI_EVNT_DT BETWEEN TO_DATE(@[start_snd_dt], 'YYYY-MM-DD HH24:MI') AND TO_DATE(@[end_snd_dt], 'YYYY-MM-DD HH24:MI')
#end
#if (${vvd_cd} != '')
                   AND BKG.BKG_NO IN (SELECT DISTINCT BKG_NO
                                                 FROM BKG_VVD
                                                WHERE VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
                                                  AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
                                                  AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1) )
#end
               )
       )

 WHERE RN BETWEEN @[startno] AND @[endno]			]]></sql>
			<params>
				<param name="jp_snd_log_id" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="start_snd_dt" type="12" value="" out="N"/>
				<param name="end_snd_dt" type="12" value="" out="N"/>
				<param name="startno" type="12" value="" out="N"/>
				<param name="endno" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
