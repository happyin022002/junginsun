<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchLocalTimeRSQL">
			<desc><![CDATA[SearchLocalTime]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(NVL(GLOBALDATE_PKG.TIME_CONV_FNC(COM_CONSTANTMGR_PKG.COM_GETBASELOCATIONCODE_FNC(), SYSDATE, @[usr_loc_cd]), SYSDATE), 'YYYY-MM-DD HH24:MI:SS') AS LCL_DT_TM
  FROM DUAL			]]></sql>
			<params>
				<param name="usr_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
