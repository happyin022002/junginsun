<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSimNoDescRSQL">
			<desc><![CDATA[simulation info 조회]]></desc>
			<sql><![CDATA[
SELECT SLAN_CD
	,SIM_DEPT_CD
	,SIM_DT
	,SIM_NO
	,CRE_USR_ID SIM_USR_ID
	,SIM_RMK 
FROM COA_SIM_INFO 
WHERE SLAN_CD = @[f_slan_cd]
	AND SIM_DT  = @[f_sim_dt]
	AND SIM_NO  = @[f_sim_no]			]]></sql>
			<params>
				<param name="f_slan_cd" type="12" value="" out="N"/>
				<param name="f_sim_dt" type="12" value="" out="N"/>
				<param name="f_sim_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
