<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCmpnAgreementDBDAOModifyFFAgreementUSQL">
			<desc><![CDATA[ModifyFFAgreement]]></desc>
			<sql><![CDATA[
UPDATE ACM_FF_AGMT 
  SET  FF_CNT_CD			= SUBSTR(@[ff_cnt_seq],0,2) , 
       FF_SEQ				= TO_NUMBER(SUBSTR(@[ff_cnt_seq],3)), 
       FF_AGMT_SEQ 			= @[ff_agmt_seq],
       SHPR_CNT_CD			= SUBSTR(@[shpr_cnt_seq],0,2),
       SHPR_SEQ				= TO_NUMBER(SUBSTR(@[shpr_cnt_seq],3)),
       POR_GRP_TP_CD		= @[por_grp_tp_cd], 
       POR_ROUT_CD			= @[por_rout_cd], 
       POL_GRP_TP_CD		= @[pol_grp_tp_cd], 
       POL_ROUT_CD			= @[pol_rout_cd], 
       POD_GRP_TP_CD		= @[pod_grp_tp_cd], 
       POD_ROUT_CD			= @[pod_rout_cd], 
       FM_EFF_DT			= @[fm_eff_dt], 
       TO_EFF_DT			= @[to_eff_dt], 
       SC_NO				= @[sc_no], 
       RFA_NO				= @[rfa_no], 
       CMDT_TP_CD			= @[cmdt_tp_cd], 
       CMDT_CD				= @[cmdt_cd], 
       FF_DIV_CD			= @[ff_div_cd], 
       FF_BKG_RT			= @[ff_bkg_rt], 
       FF_BX_AMT			= @[ff_bx_amt], 
       FF_TEU_AMT			= @[ff_teu_amt], 
       FF_FEU_AMT			= @[ff_feu_amt], 
       FF_RF_AMT			= @[ff_rf_amt], 
       FF_CHG_CTNT			= @[ff_chg_ctnt], 
       UPD_USR_ID 			= @[usr_id], 
       UPD_DT 				= SYSDATE 
WHERE FF_CNT_CD 			= @[ff_cnt_cd] 
  AND FF_SEQ 				= @[ff_seq] 
  AND FF_AGMT_SEQ 			= @[ff_agmt_seq] 			]]></sql>
			<params>
				<param name="ff_cnt_seq" type="12" value="" out="N"/>
				<param name="ff_agmt_seq" type="12" value="" out="N"/>
				<param name="shpr_cnt_seq" type="12" value="" out="N"/>
				<param name="por_grp_tp_cd" type="12" value="" out="N"/>
				<param name="por_rout_cd" type="12" value="" out="N"/>
				<param name="pol_grp_tp_cd" type="12" value="" out="N"/>
				<param name="pol_rout_cd" type="12" value="" out="N"/>
				<param name="pod_grp_tp_cd" type="12" value="" out="N"/>
				<param name="pod_rout_cd" type="12" value="" out="N"/>
				<param name="fm_eff_dt" type="12" value="" out="N"/>
				<param name="to_eff_dt" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="cmdt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="ff_div_cd" type="12" value="" out="N"/>
				<param name="ff_bkg_rt" type="12" value="" out="N"/>
				<param name="ff_bx_amt" type="12" value="" out="N"/>
				<param name="ff_teu_amt" type="12" value="" out="N"/>
				<param name="ff_feu_amt" type="12" value="" out="N"/>
				<param name="ff_rf_amt" type="12" value="" out="N"/>
				<param name="ff_chg_ctnt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ff_cnt_cd" type="12" value="" out="N"/>
				<param name="ff_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
