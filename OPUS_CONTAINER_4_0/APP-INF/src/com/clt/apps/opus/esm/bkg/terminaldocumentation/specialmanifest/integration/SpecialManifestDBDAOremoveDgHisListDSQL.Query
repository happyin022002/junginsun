<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOremoveDgHisListDSQL">
			<desc><![CDATA[위험물 정보들을 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CSTMS_EUR_DG_HIS
WHERE	EUR_DG_DECL_TP_CD in(@[d_type], 'T')
AND	VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
AND	SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
AND	SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
AND	BL_NO = @[bl_no]
AND	CNTR_NO = @[cntr_no]
AND SPCL_CNTR_SEQ = @[spcl_cntr_seq]
AND SPCL_CGO_SEQ = @[spcl_cgo_seq]
AND CGO_OPR_CD =@[cgo_opr_cd]			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="spcl_cntr_seq" type="12" value="" out="N"/>
				<param name="spcl_cgo_seq" type="12" value="" out="N"/>
				<param name="cgo_opr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
