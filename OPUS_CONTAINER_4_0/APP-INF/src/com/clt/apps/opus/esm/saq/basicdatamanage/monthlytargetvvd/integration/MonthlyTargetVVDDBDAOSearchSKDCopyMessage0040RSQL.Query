<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyTargetVVDDBDAOSearchSKDCopyMessage0040RSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT											
    DISTINCT A.SAQ_TGT_GRP_CD, 					
             A.TRD_CD, 						
             A.DIR_CD,						
             B.TGT_VVD_STS_CD,					
             ( SELECT TRIM(INTG_CD_VAL_DP_DESC) 			
           FROM COM_INTG_CD_DTL       				
           WHERE INTG_CD_VAL_CTNT = B.TGT_VVD_STS_CD 		
                 AND INTG_CD_ID = 'CD01216' ) STS_TEXT, 		
             DECODE(NVL(B.TGT_VVD_STS_CD,'0'),'0',1,0) CFM_FLG 				
FROM    								
    SAQ_TGT_GRP_TRD A,						
    (								
    SELECT DISTINCT 						
        TRD_CD, 							
        DIR_CD, 							
        TGT_VVD_STS_CD						
    FROM SAQ_MON_TGT_VVD 					
    WHERE 							
        BSE_YR = @[year] AND 					
        BSE_QTR_CD = @[quarter] AND 					
        TGT_VVD_STS_CD LIKE @[tgt_vvd_sts_cd] || '%' ) B 			
WHERE 								
    A.TRD_CD = B.TRD_CD(+) AND					
    A.DIR_CD = B.DIR_CD(+)					
ORDER BY 							
    TGT_VVD_STS_CD, 						
    SAQ_TGT_GRP_CD, 						
    DIR_CD						]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="quarter" type="12" value="" out="N"/>
				<param name="tgt_vvd_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
