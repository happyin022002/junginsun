<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPSAVVDRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT VPS.VSL_CD,
       VPS.SKD_VOY_NO,
       VPS.SKD_DIR_CD,
       PSA.PSA_VSL_NM,
       PSA.PSA_VOY_DIR_CD

  FROM VSK_VSL_PORT_SKD VPS,
       BKG_CSTMS_PSA_VVD PSA

 WHERE VPS.VPS_PORT_CD = @[port_cd]
   AND VPS.VPS_ETB_DT BETWEEN TO_DATE(@[etb_dt1], 'YYYY-MM-DD') AND TO_DATE(@[etb_dt2], 'YYYY-MM-DD')
   AND NVL(SKD_CNG_STS_CD, ' ') <> 'S'
   AND NVL(TURN_PORT_IND_CD, ' ') NOT IN ('D', 'V', 'F')
   AND VPS.VSL_CD = PSA.VSL_CD(+)
   AND VPS.SKD_VOY_NO = PSA.SKD_VOY_NO(+)
   AND VPS.SKD_DIR_CD = PSA.SKD_DIR_CD(+)
   AND VPS.CLPT_IND_SEQ = '1'

 GROUP BY VPS.VSL_CD,
       VPS.SKD_VOY_NO,
       VPS.SKD_DIR_CD,
       PSA.PSA_VSL_NM,
       PSA.PSA_VOY_DIR_CD
			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="etb_dt1" type="12" value="" out="N"/>
				<param name="etb_dt2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
