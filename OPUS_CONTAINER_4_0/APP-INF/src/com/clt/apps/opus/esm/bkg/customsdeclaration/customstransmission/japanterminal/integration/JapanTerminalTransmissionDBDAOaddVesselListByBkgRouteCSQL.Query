<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOaddVesselListByBkgRouteCSQL">
			<desc><![CDATA[BKG Route 로 조회한 후 정보를 저장]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_TML_EDI_JP_BAT_VVD_SKD (
VSL_CD,
SKD_VOY_NO,
SKD_DIR_CD,
JP_TML_VSL_NO,
POL_CD,
POL_YD_CD,
POR_CD,
POR_YD_CD,
CALL_SGN_NO,
OTR_NTFY_YD_CD,
BAT_SKD_PRD_FM_DT,
BAT_SKD_PRD_TO_DT,
EDI_SND_OFC_CD,
EDI_SND_USR_ID,
SKD_DELT_FLG,
DELT_USR_ID,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID,
UPD_DT
) VALUES(
	SUBSTR(@[vvd_cd],1,4)
,   SUBSTR(@[vvd_cd],5,4)
,   SUBSTR(@[vvd_cd],9,1)
,   @[jp_tml_vsl_no]
,	@[pol_cd]
,	@[pol_yd_cd]
,	@[por_cd]
#if(${por_yd_cd} != '')
,	@[por_yd_cd]
#else
,   ' '
#end
,   @[call_sgn_no]
,   @[otr_ntfy_yd_cd]
,   TO_DATE(@[bat_skd_prd_fm_dt],'YYYY-MM-DD')
,   TO_DATE(@[bat_skd_prd_to_dt],'YYYY-MM-DD')
,   @[ofc_cd]
,   @[cre_usr_id]
,   'N'
,   @[delt_usr_id]
,   @[cre_usr_id]
,   sysdate
,   @[cre_usr_id]
,   sysdate
)			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="jp_tml_vsl_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="por_yd_cd" type="12" value="" out="N"/>
				<param name="call_sgn_no" type="12" value="" out="N"/>
				<param name="otr_ntfy_yd_cd" type="12" value="" out="N"/>
				<param name="bat_skd_prd_fm_dt" type="12" value="" out="N"/>
				<param name="bat_skd_prd_to_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="delt_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
