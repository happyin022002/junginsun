<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAORsltScgGrpLocListVORSQL">
			<desc><![CDATA[Surcharge Group Location combo]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
	 , CHG_CD
     , GRP_LOC_SEQ
	 , SCG_GRP_LOC_CD CD
	 , SCG_GRP_LOC_DESC NM
  FROM PRI_SCG_GRP_LOC
 WHERE DELT_FLG = 'N'
   AND SVC_SCP_CD = @[svc_scp_cd]
   AND CHG_CD = @[chg_cd]
 ORDER BY SCG_GRP_LOC_CD ASC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
