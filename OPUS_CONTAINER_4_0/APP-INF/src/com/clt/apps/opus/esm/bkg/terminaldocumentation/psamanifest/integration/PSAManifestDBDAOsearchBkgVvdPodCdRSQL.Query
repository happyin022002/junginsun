<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchBkgVvdPodCdRSQL">
			<desc><![CDATA[POD 조회]]></desc>
			<sql><![CDATA[
    SELECT      POD_CD
    FROM        BKG_VVD
    WHERE       BKG_NO          =   @[bkg_no]
    ORDER BY    VSL_PRE_PST_CD, VSL_SEQ 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
