<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiChgBySwaTraxRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT '{CHARGE'                                                                                    || CHR(10)
         || 'FCTYPE:'        || B.CHG_CD                                                            || CHR(10)
         || 'RATE:'          || B.CHG_UT_AMT                                                        || CHR(10)
         || 'RATED_AS:'      || B.RAT_AS_QTY                                                        || CHR(10)
         || 'REVENUETON:'    || CHR(10)
         || 'DIF_AMT:'       || ROUND(B.CHG_AMT * DECODE(B.CURR_CD, 'USD', 1, C.INV_XCH_RT) , 2)    || CHR(10)
         || 'PPD:'               || CHR(10)
         || 'CCT:'               || CHR(10)
         || 'CURRENCYCODE:'  || B.CURR_CD                                                           || CHR(10)
         || 'TARIFF:'        || B.TRF_ITM_NO                                                        || CHR(10)
         || 'PERTYPE:'       || B.RAT_UT_CD                                                         || CHR(10)
         || 'EXRATE:'        || TO_CHAR(DECODE(B.CURR_CD, 'USD', 1, C.INV_XCH_RT),'FM9990.000000')  || CHR(10)
         || 'FRT_IND:'       || B.FRT_TERM_CD                                                       || CHR(10)
         || '}CHARGE'                                                                               || CHR(10)       
  FROM INV_AR_MN A
      ,BKG_CHG_RT B
      ,INV_CUST_AND_DLY_XCH_RT C
      ,BKG_RATE D
      ,MDM_LOCATION E
      ,BKG_VVD F
      ,VSK_VSL_PORT_SKD G
      ,(
         SELECT COUNT(G.VPS_PORT_CD) CNT
           FROM INV_AR_MN A, BKG_CHG_RT B, INV_CUST_AND_DLY_XCH_RT C, BKG_RATE D, MDM_LOCATION E, BKG_VVD F, VSK_VSL_PORT_SKD G
          WHERE B.BKG_NO = A.BKG_NO
            AND NVL(B.FRT_INCL_XCLD_DIV_CD, 'N') NOT IN ('I', 'E')
            AND B.BKG_NO = D.BKG_NO  
            AND DECODE(B.N3PTY_RCV_OFC_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_RCV_OFC_CD, D.CLT_OFC_CD) , B.N3PTY_RCV_OFC_CD) = 'SINBB'       
            AND DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CNT_CD, D.CLT_PAYR_CNT_CD) , B.N3PTY_CUST_CNT_CD) = 'SG'
            AND DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CUST_SEQ, D.CLT_PAYR_CUST_SEQ) , B.N3PTY_CUST_SEQ) IN (260, 11029)                                     
            AND A.AR_IF_NO =  ( SELECT MAX(AR_IF_NO) 
                                  FROM INV_AR_MN 
                                 WHERE BL_SRC_NO = @[bl_no]
                                   AND BL_INV_CFM_DT IS NOT NULL 
                                   AND AR_OFC_CD = 'SINBB'
                                   AND REV_TP_CD <> 'M'
                                   AND NVL(INV_DELT_DIV_CD, 'N') <> 'Y')
            AND C.CUST_CNT_CD = DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CNT_CD, D.CLT_PAYR_CNT_CD), B.N3PTY_CUST_CNT_CD)
            AND C.CUST_SEQ = DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CUST_SEQ, D.CLT_PAYR_CUST_SEQ), B.N3PTY_CUST_SEQ)
            AND A.IO_BND_CD = C.IO_BND_CD
            AND DECODE(B.CURR_CD, 'SGD', 'USD', B.CURR_CD) = C.CHG_CURR_CD
            AND C.LOCL_CURR_CD = 'SGD'
            AND C.AR_OFC_CD = 'SINBB'
            AND B.BKG_NO = D.BKG_NO
            AND A.POD_CD = E.LOC_CD
            AND F.BKG_NO = A.BKG_NO
            AND F.VSL_CD = G.VSL_CD
            AND F.SKD_VOY_NO = G.SKD_VOY_NO
            AND F.SKD_DIR_CD = G.SKD_DIR_CD
			AND 'Y' = CASE WHEN A.IO_BND_CD ='O' AND F.POL_CLPT_IND_SEQ = G.CLPT_IND_SEQ THEN 'Y'
						   WHEN A.IO_BND_CD ='I' AND F.POD_CLPT_IND_SEQ = G.CLPT_IND_SEQ THEN 'Y'
					  ELSE 'N'
					  END 
            AND G.VPS_PORT_CD = DECODE(A.IO_BND_CD , 'O', A.POL_CD, A.POD_CD)
            AND DECODE(A.IO_BND_CD , 'O' , (DECODE(E.CONTI_CD , 'M' , TO_CHAR(D.RT_APLY_DT, 'YYYYMMDD') , TO_CHAR(G.VPS_ETD_DT, 'YYYYMMDD') ) )
                                   , 'I' , TO_CHAR(G.VPS_ETA_DT, 'YYYYMMDD') ) BETWEEN C.FM_DT AND C.TO_DT
      ) P
WHERE B.BKG_NO = A.BKG_NO
  AND NVL(B.FRT_INCL_XCLD_DIV_CD, 'N') NOT IN ('I', 'E')
  AND B.BKG_NO = D.BKG_NO
  AND DECODE(B.N3PTY_RCV_OFC_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_RCV_OFC_CD, D.CLT_OFC_CD) , B.N3PTY_RCV_OFC_CD) = 'SINBB'       
  AND DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CNT_CD, D.CLT_PAYR_CNT_CD) , B.N3PTY_CUST_CNT_CD) = 'SG'
  AND DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CUST_SEQ, D.CLT_PAYR_CUST_SEQ) , B.N3PTY_CUST_SEQ) IN (260, 11029)                                     
  AND A.AR_IF_NO =  ( SELECT MAX(AR_IF_NO) 
                        FROM INV_AR_MN 
                       WHERE BL_SRC_NO = @[bl_no]
                         AND BL_INV_CFM_DT IS NOT NULL 
                         AND AR_OFC_CD = 'SINBB'
                         AND REV_TP_CD <> 'M'
                         AND NVL(INV_DELT_DIV_CD, 'N') <> 'Y')                          
  AND C.CUST_CNT_CD = DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CNT_CD, D.CLT_PAYR_CNT_CD), B.N3PTY_CUST_CNT_CD)
  AND C.CUST_SEQ = DECODE(B.N3PTY_CUST_CNT_CD, NULL, DECODE(B.FRT_TERM_CD, 'P', D.PPD_PAYR_CUST_SEQ, D.CLT_PAYR_CUST_SEQ), B.N3PTY_CUST_SEQ)
  AND A.IO_BND_CD = C.IO_BND_CD
  AND DECODE(B.CURR_CD, 'SGD', 'USD', B.CURR_CD) = C.CHG_CURR_CD
  AND C.LOCL_CURR_CD = 'SGD'
  AND C.AR_OFC_CD = 'SINBB'
  AND B.BKG_NO = D.BKG_NO
  AND A.POD_CD = E.LOC_CD
  AND F.BKG_NO = A.BKG_NO
  AND F.VSL_CD = G.VSL_CD
  AND F.SKD_VOY_NO = G.SKD_VOY_NO
  AND F.SKD_DIR_CD = G.SKD_DIR_CD
  AND 'Y' = CASE WHEN A.IO_BND_CD ='O' AND F.POL_CLPT_IND_SEQ = G.CLPT_IND_SEQ THEN 'Y'
				 WHEN A.IO_BND_CD ='I' AND F.POD_CLPT_IND_SEQ = G.CLPT_IND_SEQ THEN 'Y'
			ELSE 'N'
			END
  AND G.VPS_PORT_CD = DECODE(A.IO_BND_CD , 'O', A.POL_CD, A.POD_CD)
  AND G.VPS_PORT_CD = DECODE(B.FRT_TERM_CD, 'P', A.POL_CD, A.POD_CD)
  AND DECODE(A.IO_BND_CD , 'O' , (DECODE(E.CONTI_CD , 'M' , TO_CHAR(D.RT_APLY_DT, 'YYYYMMDD') , TO_CHAR(G.VPS_ETD_DT, 'YYYYMMDD') ) )
                         , 'I' , TO_CHAR(G.VPS_ETA_DT, 'YYYYMMDD') ) BETWEEN C.FM_DT AND C.TO_DT
  AND (    (G.CLPT_IND_SEQ = DECODE(P.CNT, 1, G.CLPT_IND_SEQ, 0))
        OR (1 =  CASE WHEN P.CNT = 1 AND NVL(G.SKD_CNG_STS_CD, ' ') = 'C' THEN 1
                      WHEN NVL(G.SKD_CNG_STS_CD, ' ') IN ('I', ' ', 'A') THEN 1
                      ELSE 0
                 END) )
 ORDER BY B.DP_SEQ			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
