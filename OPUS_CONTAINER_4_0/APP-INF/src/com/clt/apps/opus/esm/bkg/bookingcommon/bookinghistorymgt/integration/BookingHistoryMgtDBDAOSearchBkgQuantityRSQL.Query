<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgQuantityRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgQuantityRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
, CNTR_TPSZ_CD
, OP_CNTR_QTY
, ACT_CNTR_QTY
, DCGO_QTY
, AWK_CGO_QTY
, RC_QTY
, BB_CGO_QTY
, SOC_QTY
, EQ_SUBST_CNTR_TPSZ_CD
, EQ_SUBST_CGO_QTY
, MER_HNGR_QTY
, CRR_HNGR_QTY
, CRR_HNGR_SGL_BAR_QTY
, CRR_HNGR_DBL_BAR_QTY
, CRR_HNGR_TPL_BAR_QTY
, ORG_CNTR_QTY
, DEST_CNTR_QTY
, OB_TRO_QTY
, IB_TRO_QTY
, FLEX_HGT_FLG
, CRE_USR_ID
, TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, UPD_USR_ID
, TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
  FROM BKG_QUANTITY
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
