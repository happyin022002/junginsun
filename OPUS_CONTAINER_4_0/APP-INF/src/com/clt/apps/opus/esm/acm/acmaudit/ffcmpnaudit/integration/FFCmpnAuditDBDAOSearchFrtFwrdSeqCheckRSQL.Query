<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCmpnAuditDBDAOSearchFrtFwrdSeqCheckRSQL">
			<desc><![CDATA[SearchFrtFwrdSeqCheck]]></desc>
			<sql><![CDATA[
SELECT '*' CUSTOMER 
  FROM MDM_CUSTOMER 
WHERE CUST_CNT_CD = @[bkg_ff_cnt_cd]
	AND CUST_SEQ = SUBSTR(@[bkg_ff_seq],4)
	AND NVL(DELT_FLG,'N') = 'N'			]]></sql>
			<params>
				<param name="bkg_ff_cnt_cd" type="12" value="" out="N"/>
				<param name="bkg_ff_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
