<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandCustomsTransmissionDBDAOSearchGoodsInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DSC.CNTR_MF_SEQ AS CM_SEQ,
       DSC.PCK_QTY AS BLPKG,
       DECODE(CNV.CSTMS_PCK_TP_CD, NULL, DSC.PCK_TP_CD, CNV.CSTMS_PCK_TP_CD) AS BLPKGU,
       DSC.CNTR_MF_WGT AS BLWGT,
       DSC.WGT_UT_CD AS BLWGT_UNIT,
       DSC.MEAS_QTY AS BLMEA,
       DSC.MEAS_UT_CD AS BLMEA_UNIT,
       DSC.CMDT_HS_CD

  FROM BKG_CNTR_MF_DESC DSC,
       BKG_CSTMS_PCK_TP_CONV CNV

 WHERE BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]
   AND DSC.PCK_TP_CD = CNV.PCK_TP_CD(+)
   AND CNV.CNT_CD(+) = 'NZ'
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
