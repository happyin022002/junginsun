<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaManifestListDownloadDBDAOsearchVesselDetailAtVslRSQL">
			<desc><![CDATA[searchVesselDetailAtVsl - 운항쪽에서 조회]]></desc>
			<sql><![CDATA[
SELECT 
    @[vvd_cd] VVD_CD
    ,VSL_CD
    ,'' SKD_VOY_NO
    ,'' SKD_DIR_CD
    ,@[pod_cd] POD_CD
    ,'' IDA_DECL_VSL_NO
    ,'' VSL_DECL_DT
    ,VSL_ENG_NM VSL_NM
    ,CALL_SGN_NO
    ,'' IDA_LINE_NO
    ,'' IDA_AGN_ID
    ,VSL_RGST_CNT_CD CNT_CD
    ,'' PORT_CD
    ,'' ARR_DT
    ,'' ARR_DT2
    ,'' IBD_NO	-- TP Bond No
    ,'' CRR_AGN_CD
    ,'' IDA_MRN_LINE_OPR_CD
    ,'' IDA_CFS_ID
    ,'' BD_AREA_CD -- Bond Code
    ,'' IDA_VOY_NO
    ,'' IDA_YR_NO
    ,'' TRNS_OPR_ID
    ,'' CRE_USR_ID
    ,'' CRE_DT
    ,'' UPD_USR_ID
    ,'' UPD_DT
FROM MDM_VSL_CNTR
WHERE VSL_CD = SUBSTR(@[vvd_cd], 1, 4)			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
