<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchMiDiffRSQL">
			<desc><![CDATA[dwkim, 0233 전송시 Server Date < EDA on MI - 5 Days 체크]]></desc>
			<sql><![CDATA[
SELECT '*'
FROM DUAL
WHERE GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', sysdate, 'USNYC') < TO_DATE(@[eda_on_mi], 'YYYY-MM-DD') - 5			]]></sql>
			<params>
				<param name="eda_on_mi" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
