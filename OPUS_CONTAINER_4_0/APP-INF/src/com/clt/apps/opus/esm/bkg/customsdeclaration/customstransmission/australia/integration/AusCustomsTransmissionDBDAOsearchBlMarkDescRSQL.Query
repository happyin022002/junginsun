<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchBlMarkDescRSQL">
			<desc><![CDATA[호주세관 및 항만청으로 전송할 Manifest B/L Mark Description 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT	REPLACE(REGEXP_REPLACE(REPLACE(DOC.PCK_CMDT_DESC,CHR(10),'$$$$$'),'[^ a-zA-Z0-9~!@#$%^&*()-_+={}|:;,<.>/]',' ', 1, 0, 'im'),'$$$$$',CHR(10))||CHR(10)||
        REPLACE(REGEXP_REPLACE(REPLACE(DOC.CNTR_CMDT_DESC,CHR(10),'$$$$$'),'[^ a-zA-Z0-9~!@#$%^&*()-_+={}|:;,<.>/]',' ', 1, 0, 'im'),'$$$$$',CHR(10))||CHR(10)||
        REPLACE(REGEXP_REPLACE(REPLACE(BMD.CMDT_DESC,CHR(10),'$$$$$'),'[^ a-zA-Z0-9~!@#$%^&*()-_+={}|:;,<.>/]',' ', 1, 0, 'im'),'$$$$$',CHR(10)) CMDT_DESC,
        REPLACE(REGEXP_REPLACE(REPLACE(BMD.MK_DESC,CHR(10),'$$$$$'),'[^ a-zA-Z0-9~!@#$%^&*()-_+={}|:;,<.>/]',' ', 1, 0, 'im'),'$$$$$',CHR(10)) MK_DESC
FROM BKG_BL_MK_DESC BMD, BKG_BL_DOC DOC
WHERE	DOC.BKG_NO        = @[bkg_no]
AND     DOC.BKG_NO = BMD.BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
