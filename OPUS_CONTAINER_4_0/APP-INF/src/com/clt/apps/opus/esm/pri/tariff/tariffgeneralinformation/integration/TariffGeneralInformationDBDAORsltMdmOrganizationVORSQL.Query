<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffGeneralInformationDBDAORsltMdmOrganizationVORSQL">
			<desc><![CDATA[Office Type Code를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT OFC_TP_CD
FROM MDM_ORGANIZATION
WHERE  OFC_CD = @[ofc_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
