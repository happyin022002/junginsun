<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOMultiWeeklyTargetVVD2USQL">
			<desc><![CDATA[MultiWeeklyTargetVVD UPDATE2]]></desc>
			<sql><![CDATA[
UPDATE COA_MON_VVD SET
    COST_YRMON  = @[cost_yrmon]
   ,SLS_YRMON   = @[sls_yrmon]
   ,COST_WK     = LPAD(@[cost_wk],2,'0')
   ,WKY_TGT_FLG = @[wky_tgt_flg]
   ,WKY_MNL_FLG = @[wky_mnl_flg]
   ,DELT_FLG    = @[delt_flg]
   ,BSA_ZR_FLG  = @[bsa_zr_flg]
   ,UPD_USR_ID  = @[upd_usr_id]
   ,UPD_DT      = SYSDATE
WHERE 1=1
  AND TRD_CD     IN (@[trd_cd1],@[trd_cd2],@[trd_cd3],@[trd_cd4])
  AND RLANE_CD   = @[rlane_cd]
  AND VSL_CD     = @[vsl_cd]
  AND SKD_VOY_NO = @[skd_voy_no]
  AND DIR_CD     = @[dir_cd]			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="Y"/>
				<param name="sls_yrmon" type="12" value="" out="Y"/>
				<param name="cost_wk" type="12" value="" out="Y"/>
				<param name="wky_tgt_flg" type="12" value="" out="Y"/>
				<param name="wky_mnl_flg" type="12" value="" out="Y"/>
				<param name="delt_flg" type="12" value="" out="Y"/>
				<param name="bsa_zr_flg" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="trd_cd1" type="12" value="" out="Y"/>
				<param name="trd_cd2" type="12" value="" out="Y"/>
				<param name="trd_cd3" type="12" value="" out="N"/>
				<param name="trd_cd4" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="vsl_cd" type="12" value="" out="Y"/>
				<param name="skd_voy_no" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
