,CFM_DT
,DIFF_RMK
,CNTC_PSON_NM
,CNTC_FAX_NO
,CNTC_PHN_NO
,CNTC_MPHN_NO
,CXL_FLG
,SO_FLG
,SO_ACT_CUST_NO
,SO_ACT_CUST_SEQ
,PCTL_NO
,VSL_CD
,SKD_VOY_NO
,SKD_DIR_CD
,POL_CD
,POD_CD
,TRO_BKG_NO
,CRE_USR_ID
