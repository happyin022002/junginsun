,ACT_SHPR_CNT_CD
,ACT_SHPR_SEQ
,ACT_SHPR_NM
,ACT_SHPR_PHN_NO
,ACT_SHPR_ADDR
,ZN_CD
,DOR_LOC_CD
,DOR_PST_NO
,BIZ_RGST_NO
,CFM_FLG
,CFM_DT
,DIFF_RMK
,CNTC_PSON_NM
,CNTC_FAX_NO
,CNTC_PHN_NO
,CNTC_MPHN_NO
,CXL_FLG
,SO_FLG
,SO_ACT_CUST_NO
,SO_ACT_CUST_SEQ
,PCTL_NO
,VSL_CD
,SKD_VOY_NO
,SKD_DIR_CD
,POL_CD
,POD_CD
,TRO_BKG_NO
,@[usr_id]
,sysdate
,@[usr_id]
,sysdate
FROM BKG_TRO source
WHERE BKG_NO = @[org_bkg_no]
AND tro_seq = @[org_tro_seq]			]]></sql>
