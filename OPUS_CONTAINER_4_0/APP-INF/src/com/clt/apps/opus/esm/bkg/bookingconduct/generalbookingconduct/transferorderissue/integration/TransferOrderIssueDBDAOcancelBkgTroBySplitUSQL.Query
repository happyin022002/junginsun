<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOcancelBkgTroBySplitUSQL">
			<desc><![CDATA[cancelBkgTroBySplit]]></desc>
			<sql><![CDATA[
UPDATE BKG_TRO A
   SET CXL_FLG = 'Y'
	  ,CFM_FLG = 'N'
      ,UPD_USR_ID = @[usr_id]
	  ,UPD_DT = SYSDATE
 WHERE BKG_NO = @[source_bkg_no]
   AND (BKG_NO, IO_BND_CD, TRO_SEQ)
       NOT IN
       (
       SELECT BKG_NO
             ,IO_BND_CD
             ,TRO_SEQ
         FROM SCE_TRO_MAPG B
        WHERE A.BKG_NO = B.BKG_NO
          AND A.IO_BND_CD = B.IO_BND_CD
          AND A.TRO_SEQ = B.TRO_SEQ
       )
   AND CFM_FLG = 'Y'			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="source_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
