<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOmodifyEmpTransmitDateUSQL">
			<desc><![CDATA[전송일자 UPDATE]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_KR_BL 
SET MF_SND_DT = SYSDATE 
, MF_SND_USR_ID = @[usr_id] 
WHERE CSTMS_BL_NO = @[cstms_bl_no] 
AND CSTMS_DECL_TP_CD = @[cstms_decl_tp_cd] 
AND DMST_PORT_CD     = @[dmst_port_cd] 
AND TRNS_SEQ         = TO_NUMBER(SUBSTR(@[trns_seq],1,2)) 			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cstms_bl_no" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="dmst_port_cd" type="12" value="" out="N"/>
				<param name="trns_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
