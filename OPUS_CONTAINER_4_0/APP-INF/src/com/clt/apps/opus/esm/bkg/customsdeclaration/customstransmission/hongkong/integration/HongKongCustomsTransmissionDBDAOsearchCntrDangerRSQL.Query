<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HongKongCustomsTransmissionDBDAOsearchCntrDangerRSQL">
			<desc><![CDATA[홍콩세관 신고용 Manifest Container Danger 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  
   'UNNBR:'||NVL(DG.IMDG_UN_NO,'') UNNBR,
   'CLASS:'||NVL(UN.IMDG_CLSS_CD,'') IMDG_CLSS_CD,
   'DESC:'||NVL(DG.PRP_SHP_NM,'') DESCNM,
   'PHONE:'||NVL(DG.EMER_CNTC_PHN_NO_CTNT,'') PHONE,
   'PAGE:'||'' PAGE,
   'FLSH_TEMP:'||NVL(UN.FLSH_PNT_TEMP_CTNT,0) FLSH_TEMP,
   'FLSH_UNIT:'||'' FLSH_UNIT,
   'DG_REMARK:'||REPLACE(NVL(DG.DIFF_RMK,''),CHR(10),'') DG_REMARK,
   'EMSNO:'||NVL(UN.IMDG_EMER_NO,'') EMSNO,
   'PSACLS:'||NVL(DG.PSA_NO,'') PSACLS,
   'PKGGRP:'||NVL(DG.IMDG_PCK_GRP_CD,'') PKGGRP,
   'MFAG1:'||'' MFAG1,
   'MFAG2:'||'' MFAG2,
   'MAR_POLL:'||NVL(DG.MRN_POLUT_FLG,'') MAR_POLL,
   'LABEL_CD:'||'' LABEL_CD,
   'LABEL_DESC:' LABEL_DESC,
   'PKG:'||NVL(DG.OUT_IMDG_PCK_QTY1,0) PKG,
   'PKGUNIT:'||NVL(DG.OUT_IMDG_PCK_CD1,'') PKGUNIT,
   'NWGT:'||NVL(DG.NET_WGT,0) NWGT,
   'GWGT:'||DECODE(NVL(DG.WGT_UT_CD,''),'LBS',ROUND(NVL(DG.GRS_WGT,0)*0.4536,3),NVL(DG.GRS_WGT,0)) GWGT,
   'MEA:'||DECODE(NVL(DG.MEAS_UT_CD,''),'CBF',ROUND(NVL(DG.MEAS_QTY,0)*0.0283,3),NVL(DG.MEAS_QTY,0)) MEA,
   'HAZ_CONT:'||NVL(DG.HZD_DESC,'') HAZ_CONT,
   'STWG:'||NVL(DG.SPCL_STWG_RQST_DESC,'') STWG,
   'LABEL:'||'' LABEL    
FROM  BKG_DG_CGO DG, SCG_IMDG_UN_NO UN
WHERE DG.IMDG_UN_NO = UN.IMDG_UN_NO(+)
and   DG.IMDG_UN_NO_SEQ = UN.IMDG_UN_NO_SEQ(+)
and   DG.CNTR_NO(+)    = @[cntr_no]
and   DG.BKG_NO        = @[bkg_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
