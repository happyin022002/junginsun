<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApplicationDateRuleDBDAOsearchLocationInfoRSQL">
			<desc><![CDATA[LocationInfo를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD,
ORG_LOC_CD,
CONV_LOC_CD,
POR_APPL_FLG,
POL_APPL_FLG,
POD_APPL_FLG,
DEL_APPL_FLG,
PRE_RLY_PORT_APPL_FLG,
PST_RLY_PORT_APPL_FLG
FROM PRI_ROUT_LOC_CONV
WHERE 1=1
#if (${frm_org_loc_cd}!='')
    AND ORG_LOC_CD LIKE '%'||@[frm_org_loc_cd]||'%'
#end

#if (${frm_conv_loc_cd}!='')
    AND CONV_LOC_CD LIKE '%'||@[frm_conv_loc_cd]||'%'
#end

#if (${scp_cd}!='')
    AND SVC_SCP_CD LIKE '%'||@[scp_cd]||'%'
#end

ORDER BY SVC_SCP_CD,ORG_LOC_CD,CONV_LOC_CD			]]></sql>
			<params>
				<param name="frm_org_loc_cd" type="12" value="" out="N"/>
				<param name="frm_conv_loc_cd" type="12" value="" out="N"/>
				<param name="scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
