<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOmodifyArrNtcFormDtlUSQL">
			<desc><![CDATA[arrival notice detail을 update한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_ARR_NTC_WD_DTL 
 SET ADDR_CTNT = @[addr_ctnt]
 , IMPT_NTC_RMK = @[impt_ntc_rmk]
 , UPD_USR_ID = @[cre_usr_id]
 , UPD_DT = SYSDATE
WHERE AN_SEQ = @[an_seq]
 AND AN_FOM_CD = @[an_fom_cd]			]]></sql>
			<params>
				<param name="addr_ctnt" type="12" value="" out="N"/>
				<param name="impt_ntc_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="an_seq" type="12" value="" out="N"/>
				<param name="an_fom_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
