<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchCuscarDescRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT BKG_SPCLCHAR_CONV_FNC(TRIM(B.PCK_CMDT_DESC)||' '||TRIM(B.CNTR_CMDT_DESC)||' '||TRIM(M.CMDT_DESC), 'Y') AS DESCRIPTION

  FROM BKG_BL_MK_DESC M,
       BKG_BL_DOC B

 WHERE B.BKG_NO = @[bkg_no]
   AND M.BKG_NO = B.BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
