<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOSearchDocLocRSQL">
			<desc><![CDATA[Doc Loc 조회]]></desc>
			<sql><![CDATA[
SELECT 'DOC / XDO' AS CHARGE_CODE
	, DOC_LOC_CD 
#if (${ca_flg} == 'Y') 
FROM BKG_RT_HIS
#else
FROM BKG_RATE
#end
WHERE BKG_NO = @[bkg_no]
#if (${ca_flg} == 'Y') 
AND CORR_NO = 'TMP0000001'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
