<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchDstSvcRouteRSQL">
			<desc><![CDATA[DEST_TRNS_SVC_MOD_CD 조회]]></desc>
			<sql><![CDATA[
SELECT NVL((
            SELECT SVC_MOD_CD
              FROM COA_USA_SVC_MOD
             WHERE ORG_RGN_CD = (SELECT NVL(RGN_CD, ' ') FROM MDM_LOCATION WHERE LOC_CD = @[pod_cd])
               AND DEST_RGN_CD = (SELECT NVL(RGN_CD, ' ') FROM MDM_LOCATION WHERE LOC_CD = @[del_cd])
           ),
       NVL((
            SELECT 'ELO'
              FROM BKG_EQLZ_PORT
             WHERE SLAN_CD      = @[trunk_lane_cd]
               AND LOC_CD       = @[pod_cd]
               AND EQLZ_PORT_CD = @[del_cd]
               AND SKD_DIR_CD   = @[skd_dir_cd]
           ),
       NVL((
            SELECT DECODE(DEL.LOC_CHR_CD||POD.LOC_CHR_CD, 'PRTPRT', 'NLO', DECODE(@[del_cd], @[pod_cd], 'NLO', 'NIP'))
              FROM MDM_LOCATION DEL, MDM_LOCATION POD
             WHERE DEL.LOC_CD = @[del_cd]   
               AND DEL.DELT_FLG = 'N'
               AND POD.LOC_CD = @[pod_cd] 
               AND POD.DELT_FLG = 'N'
               AND NVL(@[pst_rly_port_cd], 'X') <> 'X'
           ),(
            SELECT DECODE(LENGTH(@[ocp_cd]), 5, 'COC',
                   DECODE(DEL.LOC_CHR_CD||POD.LOC_CHR_CD
                 , 'ECPGCP', 'CLO', 'GCPECP', 'CLO'
                 , 'ECPECP', 'CLO', 'WCPWCP', 'CLO', 'PRTPRT', 'CLO'
                 , DECODE(@[del_cd], @[pod_cd], 'CLO'
                 , DECODE(DEL.LOC_CHR_CD||POD.LOC_CHR_CD
                 , 'ECPWCP', 'CML', 'WCPECP', 'CML'
                 , 'GCPWCP', 'CML', 'WCPGCP', 'CML'
                 , 'CIP'))))
              FROM MDM_LOCATION DEL, MDM_LOCATION POD
             WHERE DEL.LOC_CD = @[del_cd]
               AND DEL.DELT_FLG = 'N'
               AND POD.LOC_CD = @[pod_cd]
               AND POD.DELT_FLG = 'N'
               AND NVL(@[pst_rly_port_cd], 'X') = 'X'
           )
       ))) AS DST_SVC_MODE
  FROM DUAL
			]]></sql>
			<params>
				<param name="pod_cd" type="12" value="USLGB" out="N"/>
				<param name="del_cd" type="12" value="USCLE" out="N"/>
				<param name="trunk_lane_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pst_rly_port_cd" type="12" value="" out="N"/>
				<param name="ocp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
