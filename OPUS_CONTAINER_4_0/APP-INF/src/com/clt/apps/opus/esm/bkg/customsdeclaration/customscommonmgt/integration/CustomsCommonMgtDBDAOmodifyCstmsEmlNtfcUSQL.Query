<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOmodifyCstmsEmlNtfcUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_EML_NTFC

   SET EDI_MSG = @[edi_msg],
       EDI_MSG_TP_ID = @[edi_msg_tp_id],
       POL_CD = NVL(@[pol_cd], ' '),
       POD_CD = NVL(@[pod_cd], ' '),
       TO_EML_CTNT = REPLACE(@[to_eml_ctnt], ' ', ''),
       CC_EML_CTNT = REPLACE(@[cc_eml_ctnt], ' ', ''),
       UPD_USR_ID = @[usr_id],
       UPD_DT = SYSDATE

 WHERE CNT_CD = @[cnt_cd]
   AND EDI_MSG = @[org_edi_msg]
   AND EDI_MSG_TP_ID = @[org_edi_msg_tp_id]
   AND POL_CD = NVL(@[org_pol_cd], ' ')
   AND POD_CD = NVL(@[org_pod_cd], ' ')			]]></sql>
			<params>
				<param name="edi_msg" type="12" value="" out="N"/>
				<param name="edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="to_eml_ctnt" type="12" value="" out="N"/>
				<param name="cc_eml_ctnt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="org_edi_msg" type="12" value="" out="N"/>
				<param name="org_edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="org_pol_cd" type="12" value="" out="N"/>
				<param name="org_pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
