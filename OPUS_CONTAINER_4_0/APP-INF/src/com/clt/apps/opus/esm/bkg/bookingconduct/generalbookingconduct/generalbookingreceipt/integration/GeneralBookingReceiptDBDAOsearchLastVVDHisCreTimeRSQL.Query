<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOsearchLastVVDHisCreTimeRSQL">
			<desc><![CDATA[Find last temporary creation time from booking VVD history]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(MIN(CASE WHEN UPD_DT > CRE_DT THEN UPD_DT ELSE CRE_DT END),'YYYYMMDDHH24MISS') CRE_DT
  FROM BKG_VVD_HIS 
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
