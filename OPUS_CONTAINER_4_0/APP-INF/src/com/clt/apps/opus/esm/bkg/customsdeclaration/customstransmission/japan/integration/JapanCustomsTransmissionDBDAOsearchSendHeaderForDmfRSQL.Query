<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOsearchSendHeaderForDmfRSQL">
			<desc><![CDATA[searchSendHeaderForDmf]]></desc>
			<sql><![CDATA[
SELECT 
	'JANCUS'||RPAD(@[in_vvd_cd],9,' ')||RPAD(@[in_pod_cd],5,' ')||
	RPAD(@[ofc_cd],6,' ')||RPAD(UPPER(@[usr_id]),10,' ')||
	SUBSTR(LTRIM(TO_CHAR(BKG_CSTM_EDI_SEQ.NEXTVAL,'000009'),' '),2)||
	TO_CHAR(TO_DATE(@[send_dt], 'YYYY-MM-DD HH24:MI:SS'), 'yyyymmddhh24miss')||
	RPAD('DMF',5,' ')||'                    ' HEADER,
	'   '||RPAD('DMF',5,' ')||
	RPAD(' ',21,' ')||
	'     '||'Z26'||  -- USER
	'3LQ2NZYK'||    -- PW
	RPAD(' ',174,' ')||
	RPAD(' ',26,' ')||
	RPAD(' ',8,' ')||
    UPPER(RPAD(@[usr_id],10,' '))||    --RPAD(' ',10,' ')||
	RPAD(' ',100,' ')||
	RPAD(' ',1,' ')||
	'2'||
	RPAD(' ',27,' ')||
	LPAD((length(@[vessel_info]) + 400),6,'0') HEADER2
FROM 
	DUAL			]]></sql>
			<params>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="in_pod_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="send_dt" type="12" value="" out="N"/>
				<param name="vessel_info" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
