<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndManifestListDownloadDBDAOmodifyBkgCstmsCndVslUSQL">
			<desc><![CDATA[modifyBkgCstmsCndVsl]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_CND_VSL 
   SET 
	CND_ACK_RSPN_CD = NVL(@[cnd_ack_rspn_cd], CND_ACK_RSPN_CD)
,	CND_ACK_SUB_CD = NVL(@[cnd_ack_sub_cd], CND_ACK_SUB_CD)
,	RCV_ERR_CTNT = NVL(@[rcv_err_ctnt], RCV_ERR_CTNT)
,	ACK_DT = TO_DATE(@[ack_dt],'YYYYMMDDHH24MISS')
,	CSTMS_RJCT_ID = NVL(@[cstms_rjct_id],CSTMS_RJCT_ID)
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	CND_ACK_CTRL_NO = @[cnd_ack_ctrl_no]			]]></sql>
			<params>
				<param name="cnd_ack_rspn_cd" type="12" value="" out="N"/>
				<param name="cnd_ack_sub_cd" type="12" value="" out="N"/>
				<param name="rcv_err_ctnt" type="12" value="" out="N"/>
				<param name="ack_dt" type="12" value="" out="N"/>
				<param name="cstms_rjct_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cnd_ack_ctrl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
