<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchCntrforNewBkgSeqRSQL">
			<desc><![CDATA[searchCntrforNewBkgSeq]]></desc>
			<sql><![CDATA[
SELECT MAX(BKG_SKD_SEQ+1) max_seq
  FROM BKG_TML_EDI_JP_CNTR
 WHERE BKG_NO = @[bkg_no]
--   AND CNTR_NO = [cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
