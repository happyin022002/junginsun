<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAONcmCodeDescRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT CMDT_DESC
FROM   BKG_CSTMS_CMDT
WHERE  CNT_CD = 'BR'
AND	   MF_CMDT_CD = @[mf_cmdt_cd]
AND    ACT_FLG = 'Y'			]]></sql>
			<params>
				<param name="mf_cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
