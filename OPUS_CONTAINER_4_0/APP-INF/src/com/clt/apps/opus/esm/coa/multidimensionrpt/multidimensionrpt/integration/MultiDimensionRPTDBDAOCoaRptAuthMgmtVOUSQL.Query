<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MultiDimensionRPTDBDAOCoaRptAuthMgmtVOUSQL">
			<desc><![CDATA[COA_RPT_AUTH_MGMT 테이블의 레포트 별 권한 수정]]></desc>
			<sql><![CDATA[
UPDATE COA_RPT_AUTH_MGMT                     
   SET PFIT_VW_DFLT_CD     = @[pfit_vw_dflt_cd]    
       ,PFIT_VW_SLCT_FLG   = @[pfit_vw_slct_flg]   
       ,OFC_VW_DFLT_CD     = @[ofc_vw_dflt_cd]   
       ,OFC_VW_SLCT_FLG    = @[ofc_vw_slct_flg]
       ,PFIT_LVL_DFLT_CD   = @[pfit_lvl_dflt_cd]   
       ,PFIT_LVL_SLCT_FLG  = @[pfit_lvl_slct_flg]  
       ,UPD_USR_ID         = @[upd_usr_id]    
       ,UPD_DT             = SYSDATE         
WHERE  RPT_SEQ = @[rpt_seq]                  
  AND  OFC_LVL = @[ofc_lvl]			]]></sql>
			<params>
				<param name="pfit_vw_dflt_cd" type="12" value="" out="N"/>
				<param name="pfit_vw_slct_flg" type="12" value="" out="N"/>
				<param name="ofc_vw_dflt_cd" type="12" value="" out="N"/>
				<param name="ofc_vw_slct_flg" type="12" value="" out="N"/>
				<param name="pfit_lvl_dflt_cd" type="12" value="" out="N"/>
				<param name="pfit_lvl_slct_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rpt_seq" type="12" value="" out="N"/>
				<param name="ofc_lvl" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
