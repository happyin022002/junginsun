<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOSearchLaneDirectionChangeListCntRSQL">
			<desc><![CDATA[Reverse Sailing 노선들의 Direction 조회 리스트를 count]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS CNT
  FROM CSQ_DIR_CONV
 WHERE BSE_TP_CD  = @[f_bse_tp_cd]
   AND BSE_YR     = @[f_bse_yr]
#if (${f_bse_tp_cd} == 'Y')
   AND BSE_QTR_CD = '00'
#else
   AND BSE_QTR_CD = @[f_bse_qtr_cd]
#end
			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
