<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOmodifyArrNtcSendIdByFaxUSQL">
			<desc><![CDATA[[381] SendFax 호출에서 나온 값을 Detail 의 SendId 에 입력]]></desc>
			<sql><![CDATA[
UPDATE BKG_ARR_NTC_DTL SET 
	FAX_NTC_SND_ID = @[fax_ntc_snd_id]
WHERE	BKG_NO = @[bkg_no]
AND	BKG_CUST_TP_CD = @[bkg_cust_tp_cd]
AND	CUST_CNTC_TP_CD = @[cust_cntc_tp_cd]			]]></sql>
			<params>
				<param name="fax_ntc_snd_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_cntc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
