<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOaddDnvoFileCSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지, NVOCC 입력]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_NVOCC_FILE
(
    SCAC_CD, NVOCC_BL_ID, NVOCC_CRR_BL_ID, NVOCC_DSPO_CD, NVOCC_PCK_QTY, NVOCC_VSL_NM
    , NVOCC_VSL_CD, NVOCC_SKD_VOY_NO, CSTMS_POD_CD, NVOCC_POD_CD, NVOCC_AMS_LOC_CD
    , CSTMS_LOC_CD, RCV_DT, NVOCC_CNTR_NO
    , MF_RSPN_RCV_ID, MF_RSPN_RCV_DT, CRE_USR_ID, UPD_USR_ID
)
VALUES
(
    @[in_snp], @[in_nvobl], TRIM(@[in_hjbl]), @[icr_code], to_number(@[icr_qty]), @[vsl_eng_nm_m]
    , @[vsl_cd_m], @[skd_voyage_no_m], @[pod_amsport_m], @[pod_loc_m], @[cus_amsport]
    , @[cus_loc], DECODE(TRIM(@[ir_date_m]),'',TO_DATE(@[icr_date],'RRMMDDHH24MI'),TO_DATE(@[ir_date_m],'RRMMDDHH24MISS')), @[in_cntr]
    , DECODE(@[icr_code],'69','69', ''), DECODE(@[icr_code],'69',TO_DATE(@[icr_date],'RRMMDDHH24MI'), NULL), 'SYSTEM', 'SYSTEM'
)			]]></sql>
			<params>
				<param name="in_snp" type="12" value="" out="N"/>
				<param name="in_nvobl" type="12" value="" out="N"/>
				<param name="in_hjbl" type="12" value="" out="N"/>
				<param name="icr_code" type="12" value="" out="N"/>
				<param name="icr_qty" type="12" value="" out="N"/>
				<param name="vsl_eng_nm_m" type="12" value="" out="N"/>
				<param name="vsl_cd_m" type="12" value="" out="N"/>
				<param name="skd_voyage_no_m" type="12" value="" out="N"/>
				<param name="pod_amsport_m" type="12" value="" out="N"/>
				<param name="pod_loc_m" type="12" value="" out="N"/>
				<param name="cus_amsport" type="12" value="" out="N"/>
				<param name="cus_loc" type="12" value="" out="N"/>
				<param name="ir_date_m" type="12" value="" out="N"/>
				<param name="icr_date" type="12" value="" out="N"/>
				<param name="in_cntr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
