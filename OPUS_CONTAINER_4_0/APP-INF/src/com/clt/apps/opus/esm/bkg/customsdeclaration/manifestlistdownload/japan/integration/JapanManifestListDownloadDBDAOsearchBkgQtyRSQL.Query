<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchBkgQtyRSQL">
			<desc><![CDATA[searchBkgQty]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(OP_CNTR_QTY,0) OP_CNTR_QTY, 
	SUBSTR(NVL(CNTR_TPSZ_CD,' '),2,1) CNTR_TPSZ_CD
FROM 
	BKG_QUANTITY
WHERE BKG_NO        = @[bkg_no]				]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
