<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchSADateOfTrunkDtRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchSADateOfTrunkDtRSQL]]></desc>
			<sql><![CDATA[
SELECT
		MAX(VPS_ETD_DT) AS VPS_ETD_DT,
		MAX(VPS_ETA_DT) AS VPS_ETA_DT
  FROM (SELECT TO_CHAR(T1.VPS_ETD_DT,'YYYYMMDDHH24MISS') VPS_ETD_DT,
			   '' VPS_ETA_DT
		  FROM VSK_VSL_PORT_SKD T1, 
     			( SELECT MIN(TO_NUMBER(CLPT_IND_SEQ)) CAL_IND 
        			FROM VSK_VSL_PORT_SKD 
       			   WHERE VSL_CD = @[vsl_cd]
          			 AND SKD_VOY_NO = @[skd_voy_no] 
          			 AND SKD_DIR_CD = @[skd_dir_cd] 
          			 AND VPS_PORT_CD = @[vsl_pol_cd]
          			 AND NVL(SKD_CNG_STS_CD,'*') <> 'S' 
          			 AND VT_ADD_CALL_FLG IS NULL
     			) T3 
 		 WHERE T1.VSL_CD = @[vsl_cd] 
   		   AND T1.SKD_VOY_NO = @[skd_voy_no] 
   		   AND T1.SKD_DIR_CD = @[skd_dir_cd]  
   		   AND T1.VPS_PORT_CD = @[vsl_pol_cd] 
   		   AND T1.CLPT_IND_SEQ = T3.CAL_IND 
   		   AND NVL(T1.SKD_CNG_STS_CD,' ') <> 'S' 
   		   AND T1.VT_ADD_CALL_FLG IS NULL
		 UNION ALL
	    SELECT '' VPS_ETD_DT,
	   		  TO_CHAR(T1.VPS_ETA_DT,'YYYYMMDDHH24MISS') VPS_ETA_DT 
		  FROM VSK_VSL_PORT_SKD T1, 
			 ( SELECT MIN(TO_NUMBER(CLPT_IND_SEQ)) CAL_IND 
        		 FROM VSK_VSL_PORT_SKD 
       			WHERE VSL_CD = @[vsl_cd]
          		  AND SKD_VOY_NO = @[skd_voy_no] 
          		  AND SKD_DIR_CD = @[skd_dir_cd] 
          		  AND VPS_PORT_CD = @[vsl_pod_cd]
          		  AND NVL(SKD_CNG_STS_CD,'*') <> 'S' 
          		  AND VT_ADD_CALL_FLG IS NULL
     			) T3 
 		 WHERE T1.VSL_CD = @[vsl_cd]
   		   AND T1.SKD_VOY_NO = @[skd_voy_no]  
   		   AND T1.SKD_DIR_CD = @[skd_dir_cd] 
   		   AND T1.VPS_PORT_CD = @[vsl_pod_cd]
   		   AND T1.CLPT_IND_SEQ = T3.CAL_IND 
   		   AND NVL(T1.SKD_CNG_STS_CD,' ') <> 'S' 
   		   AND T1.VT_ADD_CALL_FLG IS NULL
		)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vsl_pol_cd" type="12" value="" out="N"/>
				<param name="vsl_pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
