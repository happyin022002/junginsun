<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOsearchKrWhfDecBzcInfoForApIfRSQL">
			<desc><![CDATA[s]]></desc>
			<sql><![CDATA[
-- ERP 테스트 환경이 200906까지만 오픈되어 테스트를 위해 잠깐 HARD CODING
SELECT   TO_CHAR(SYSDATE, 'YYYYMMDD') AS GL_DT
       , CASE SUBSTR(@[whf_decl_ofc_cd], 1, 3) WHEN 'PUS' THEN '159289'
                                          WHEN 'INC' THEN '176654'
                                          ELSE '100045'
         END AS VNDR_NO
       , CASE SUBSTR(@[whf_decl_ofc_cd], 1, 3) WHEN 'PUS' THEN '0'
                                          WHEN 'INC' THEN 'O60'
                                          ELSE 'O60'
         END AS VNDR_TERM_NM
       , AP_CTR_CD
       , '211541' AS DTRB_COA_ACCT_CD
       , '211541' AS ATTR_CATE_NM
       , '211541' AS DTRB_ATTR_CTNT3
  FROM MDM_ORGANIZATION
 WHERE 1 = 1
   AND OFC_CD(+) = @[whf_decl_ofc_cd]			]]></sql>
			<params>
				<param name="whf_decl_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
