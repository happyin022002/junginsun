<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqSmryHdrMstVersionListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
#if (${tgt_orz_cd} == 'B' )
	SELECT DISTINCT  A.QTA_MST_VER_NO   AS CODE 
#else
	SELECT DISTINCT  A.QTA_MST_VER_NO ||'-'|| A.GLINE_VER_NO  AS CODE 
#end
       , A.QTA_MST_VER_NO AS TEXT 
FROM SAQ_MON_QTA_ADJ_SMRY_HDR A  
WHERE  
#if (${tgt_orz_cd} == 'B' )
	A.SAQ_TGT_GRP_CD LIKE @[targetGrp] || '%'  
#else
	A.SAQ_TGT_GRP_CD = @[targetGrp]  
#end			
       AND A.TGT_ORZ_CD = @[tgt_orz_cd] 
	   AND BSE_YR = @[year] 
       AND BSE_QTR_CD = @[bse_qtr_cd] 			
#if (${stage} == 'SQ' )
	       AND DAT_CRE_STEP_CD < '08' 	
#else
	       AND DAT_CRE_STEP_CD = '08' 	
#end			
       AND ( A.TGT_OFC_CD='000000' OR A.TGT_OFC_CD=@[tgt_ofc_cd1] OR A.TGT_OFC_CD=@[tgt_ofc_cd2]  OR DECODE(@[tgt_orz_cd],'B',1,0) =1) 
ORDER BY A.QTA_MST_VER_NO 			]]></sql>
			<params>
				<param name="targetGrp" type="12" value="" out="N"/>
				<param name="tgt_orz_cd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="tgt_ofc_cd1" type="12" value="" out="N"/>
				<param name="tgt_ofc_cd2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
