<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOToyotaBlNoCheckRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT
FROM BKG_HRD_CDG_CTNT BHCC
WHERE BHCC.HRD_CDG_ID = 'TOYOTA_BL'
AND (
      (@[bkg_por_cd] LIKE BHCC.ATTR_CTNT1||'%'
       AND   @[bkg_pol_cd] LIKE BHCC.ATTR_CTNT2||'%'
       AND   @[bkg_pod_cd] LIKE BHCC.ATTR_CTNT3||'%' 
       AND   @[bkg_del_cd] LIKE BHCC.ATTR_CTNT4||'%' 
       AND BHCC.ATTR_CTNT6 = @[s_cust_cnt_cd]||@[s_cust_seq]
      )
#if (${rfa_no} != 'DUM' && ${rfa_no} != '') 
       OR BHCC.ATTR_CTNT5 = @[rfa_no]
#elseif (${sc_no} != 'DUM' && ${sc_no} != '') 
       OR BHCC.ATTR_CTNT5 = @[sc_no]
#elseif (${taa_no} != 'DUM' && ${taa_no} != '') 
       OR BHCC.ATTR_CTNT5 = @[taa_no]
#end
)			]]></sql>
			<params>
				<param name="bkg_por_cd" type="12" value="" out="N"/>
				<param name="bkg_pol_cd" type="12" value="" out="N"/>
				<param name="bkg_pod_cd" type="12" value="" out="N"/>
				<param name="bkg_del_cd" type="12" value="" out="N"/>
				<param name="s_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="s_cust_seq" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="taa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
