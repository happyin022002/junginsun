<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOSearchCharterInvoiceExistRSQL">
			<desc><![CDATA[Search]]></desc>
			<sql><![CDATA[
SELECT FLET_CTRT_NO
  FROM FMS_INVOICE
 WHERE 1=1
   AND FLET_CTRT_NO = @[flet_ctrt_no]
   AND FLET_ISS_TP_CD = 'CHT'
   AND INV_SEQ = 1			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
