<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgTroDtlRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgTroDtlRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
, IO_BND_CD
, RTN_TRO_FLG
, TRO_SEQ
, TRO_SUB_SEQ
, CNTR_TPSZ_CD
, TRO_QTY
, CNTR_NO
, TO_CHAR(DOR_ARR_DT, 'YYYY-MM-DD HH24:MI') DOR_ARR_DT
, PKUP_LOC_CD
, PKUP_YD_CD
, RTN_LOC_CD
, RTN_YD_CD
, CMDT_CD
, PCTL_NO
, CXL_FLG
, CRE_USR_ID
, TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, UPD_USR_ID
, TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
  FROM BKG_TRO_DTL
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
