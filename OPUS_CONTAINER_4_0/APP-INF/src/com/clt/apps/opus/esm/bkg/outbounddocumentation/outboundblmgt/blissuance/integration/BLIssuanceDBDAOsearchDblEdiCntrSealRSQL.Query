<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiCntrSealRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT '{MULTI_SEAL_NO' || CHR (10) || 
       'CNTR_SEAL_NO:' || CNTR_SEAL_NO || CHR (10) || 
       '}MULTI_SEAL_NO' || CHR (10)
FROM   BKG_CNTR_SEAL_NO
WHERE 1=1
   AND BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
