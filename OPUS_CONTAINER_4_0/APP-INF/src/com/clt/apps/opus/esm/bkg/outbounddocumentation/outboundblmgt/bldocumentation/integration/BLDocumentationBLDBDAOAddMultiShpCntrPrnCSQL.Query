<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOAddMultiShpCntrPrnCSQL">
			<desc><![CDATA[Multi Shipment를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CNTR_SHP
(
    BKG_NO,
    CNTR_MF_SEQ,
    CNTR_SEQ,
    CNTR_NO,
    PCK_QTY,
    PCK_TP_CD,
    CNTR_MF_WGT,
    WGT_UT_CD,
    MEAS_QTY,
    MEAS_UT_CD,
    MK_DESC,
    CMDT_DESC,
    PRN_FLG,
    CNTR_SEAL_NO1,
    CNTR_SEAL_NO2,
    HAMO_TRF_CD,
    CMDT_HS_CD,
    NCM_NO,
    PO_NO,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)VALUES
(
    @[bkg_no],
	(SELECT NVL(MAX(CNTR_MF_SEQ),0)+1 FROM BKG_CNTR_SHP WHERE BKG_NO=@[bkg_no] AND CNTR_SEQ=@[cntr_seq]),
    @[cntr_seq],
    @[cntr_no],
    @[pck_qty],
    @[pck_tp_cd],
    @[cntr_mf_wgt],
    @[wgt_ut_cd],
    @[meas_qty],
    @[meas_ut_cd],
    @[mk_desc],
    @[cmdt_desc],
    DECODE(@[prn_flg],'1','Y','N'),
    @[cntr_seal_no1],
    @[cntr_seal_no2],
    @[hamo_trf_cd],
    @[cmdt_hs_cd],
    @[ncm_no],
    @[po_no],
    @[usr_id],
    sysdate,
    @[usr_id],
    sysdate
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_seq" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_mf_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="meas_ut_cd" type="12" value="" out="N"/>
				<param name="mk_desc" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="prn_flg" type="12" value="" out="N"/>
				<param name="cntr_seal_no1" type="12" value="" out="N"/>
				<param name="cntr_seal_no2" type="12" value="" out="N"/>
				<param name="hamo_trf_cd" type="12" value="" out="N"/>
				<param name="cmdt_hs_cd" type="12" value="" out="N"/>
				<param name="ncm_no" type="12" value="" out="N"/>
				<param name="po_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
