<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaCustomsReportDBDAOremoveDelModeDSQL">
			<desc><![CDATA[중국 DEL 별 운송 Mode 삭제.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CSTMS_CHN_DE_MOD
WHERE    POD_CD         = @[pod_cd]
AND      DEL_CD         = @[del_cd] 
			]]></sql>
			<params>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
