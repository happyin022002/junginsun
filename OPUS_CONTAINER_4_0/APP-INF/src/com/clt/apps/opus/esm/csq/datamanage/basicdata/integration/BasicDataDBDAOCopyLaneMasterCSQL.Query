<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOCopyLaneMasterCSQL">
			<desc><![CDATA[1]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_QTA_LANE_MGMT (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,TRD_CD
      ,RLANE_CD
	  ,SUB_TRD_CD
      ,LANE_DIR_CD
      ,CSQ_ACT_FLG
      ,IAS_SCTR_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT @[f_bse_tp_cd]
      ,@[f_bse_yr]
      ,DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
      ,TRD_CD
      ,RLANE_CD
	  ,SUB_TRD_CD
      ,LANE_DIR_CD
      ,CSQ_ACT_FLG
      ,IAS_SCTR_FLG
      ,@[cre_usr_id]
      ,SYSDATE
      ,@[cre_usr_id]
      ,SYSDATE
  FROM CSQ_QTA_LANE_MGMT
 WHERE BSE_YR||BSE_QTR_CD = ( SELECT /*+ INDEX_DESC(CSQ_QTA_LANE_MGMT XPKCSQ_QTA_LANE_MGMT) */
                                     BSE_YR || BSE_QTR_CD
                                FROM CSQ_QTA_LANE_MGMT
                               WHERE BSE_TP_CD = 'Q'
                                 AND BSE_YR || BSE_QTR_CD < @[f_bse_yr] || DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
                                 AND ROWNUM = 1 )			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
