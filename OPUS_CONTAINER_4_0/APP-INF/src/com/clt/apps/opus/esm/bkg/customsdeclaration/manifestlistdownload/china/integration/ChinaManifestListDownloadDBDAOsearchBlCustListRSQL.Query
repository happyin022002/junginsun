<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOsearchBlCustListRSQL">
			<desc><![CDATA[BkgCstmsChnCustVO]]></desc>
			<sql><![CDATA[
SELECT  BB.BL_NO					AS BL_NO,
        BC.BKG_CUST_TP_CD			AS BKG_CUST_TP_CD,
        NVL(BC.CUST_CNT_CD, ' ')	AS CNT_CD,
        NVL(BC.CUST_SEQ, 0)			AS CUST_SEQ,
        NVL(REPLACE(REPLACE(REPLACE(BC.CUST_NM ,CHR(34)),CHR(9),' '), CHR(10),' '), REPLACE(REPLACE(REPLACE(CNEE.CUST_NM ,CHR(34)),CHR(9),' '), CHR(10),' ')) AS CUST_NM,
		DECODE(BC.BKG_CUST_TP_CD,
				'S', NVL(REPLACE(REPLACE(REPLACE(BC.CUST_ADDR ,CHR(34)),CHR(9),' '), CHR(10),' '), ' ')
              , 'N', DECODE(TRIM(BC.CUST_ADDR), NULL, REPLACE(REPLACE(REPLACE(CNEE.CUST_ADDR ,CHR(34)),CHR(9),' '), CHR(10),' '), NVL(REPLACE(REPLACE(REPLACE(BC.CUST_ADDR ,CHR(34)),CHR(9),' '), CHR(10),' '), ' '))
              , 'C', DECODE(BB.CUST_TO_ORD_FLG, 'Y', 'TO ORDER', NVL(REPLACE(REPLACE(REPLACE(BC.CUST_ADDR ,CHR(34)),CHR(9),' '), CHR(10),' '), ' '))
              , NULL
      	) CUST_ADDR,
        @[trans_mode] 				AS CHN_MF_SND_IND_CD,
		@[usr_id]					AS CRE_USR_ID,
		@[usr_id]					AS UPD_USR_ID
FROM    BKG_CUSTOMER BC,
		BKG_CUSTOMER CNEE,
        BKG_BOOKING BB
WHERE   1=1
AND ( #foreach($field_id in ${field_list})
      	#if($velocityCount > 1)
      	OR #end      BB.BKG_NO IN ( $field_id )
      #end
)
#if (${bkg_cgo_tp_cd} != '')
AND     DECODE(BB.BKG_CGO_TP_CD,'P','P','F') = @[bkg_cgo_tp_cd]
#end
AND     BB.BKG_NO = BC.BKG_NO
AND     BB.BKG_NO = CNEE.BKG_NO
AND     BC.BKG_CUST_TP_CD IN ('S', 'C', 'N')
AND     CNEE.BKG_CUST_TP_CD = 'C'			]]></sql>
			<params>
				<param name="trans_mode" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_cgo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
