<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOInternetBookingRequestEmailInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT
	BKG.BKG_NO,
  	(SELECT BCP.CNTC_PSON_EML FROM BKG_CNTC_PSON BCP WHERE BCP.BKG_NO = BKG.BKG_NO AND BCP.BKG_CNTC_PSON_TP_CD = 'BK' AND ROWNUM = 1) AS BKG_CNTC_PSON_EML,
  	BKG.RC_FLG,
  	BKG.DCGO_FLG,
  	BKG.AWK_CGO_FLG,
  	BKG.XTER_RMK,
  	(SELECT COUNT(HIS.BKG_NO) FROM BKG_NTC_HIS HIS WHERE HIS.BKG_NO = BKG.BKG_NO AND HIS.NTC_KND_CD = 'BK' AND HIS.NTC_VIA_CD = 'M' ) AS XTER_RQST_SEQ
FROM BKG_BOOKING BKG, BKG_XTER_RQST_MST MST
WHERE BKG.BKG_NO = MST.XTER_RQST_NO
AND BKG.BKG_NO = @[bkg_no]
AND MST.XTER_RQST_SEQ  = (SELECT MAX(XTER_RQST_SEQ) FROM BKG_XTER_RQST_MST WHERE XTER_RQST_NO = @[bkg_no])
AND MST.DOC_TP_CD = 'B'
AND MST.AUTO_EML_FLG = 'Y'
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
