<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOdeleteDgCgoforNewBkgInfoDSQL">
			<desc><![CDATA[deleteDgCgoforNewBkgInfo]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_TML_EDI_JP_DG_CGO

WHERE BKG_NO LIKE SUBSTR(@[bkg_no], 1, 10)||'%'
  AND BKG_SKD_SEQ = @[bkg_skd_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_skd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
