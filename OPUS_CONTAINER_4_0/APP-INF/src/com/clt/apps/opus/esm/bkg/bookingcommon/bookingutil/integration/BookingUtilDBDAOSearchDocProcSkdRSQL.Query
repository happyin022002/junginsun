<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchDocProcSkdRSQL">
			<desc><![CDATA[SearchDocProcSkd]]></desc>
			<sql><![CDATA[
#if ('Y'==${ca_flg}) 
SELECT BKG_NO
       ,BKG_DOC_PROC_TP_CD
       ,DOC_PROC_SEQ
       ,EVNT_DT
       ,EVNT_GDT
       ,EVNT_USR_ID
       ,DIFF_RMK
       ,BKG_EVNT_CD1
       ,BKG_EVNT_CD2
       ,DOC_PERF_DELT_FLG
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
  FROM BKG_DOC_PROC_SKD_HIS
 WHERE @[bkg_no] = BKG_NO
   AND 'TMP0000001' = CORR_NO
   AND @[bkg_doc_proc_tp_cd] = BKG_DOC_PROC_TP_CD
   AND @[doc_perf_delt_flg] = DOC_PERF_DELT_FLG
   AND 1 = ROWNUM
#else
SELECT BKG_NO
       ,BKG_DOC_PROC_TP_CD
       ,DOC_PROC_SEQ
       ,EVNT_DT
       ,EVNT_GDT
       ,EVNT_USR_ID
       ,DIFF_RMK
       ,BKG_EVNT_CD1
       ,BKG_EVNT_CD2
       ,DOC_PERF_DELT_FLG
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
  FROM BKG_DOC_PROC_SKD
 WHERE @[bkg_no] = BKG_NO
   AND @[bkg_doc_proc_tp_cd] = BKG_DOC_PROC_TP_CD
	#if (${doc_perf_delt_flg} != '') 
	AND @[doc_perf_delt_flg] = DOC_PERF_DELT_FLG
	#end
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_doc_proc_tp_cd" type="12" value="" out="N"/>
				<param name="doc_perf_delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
