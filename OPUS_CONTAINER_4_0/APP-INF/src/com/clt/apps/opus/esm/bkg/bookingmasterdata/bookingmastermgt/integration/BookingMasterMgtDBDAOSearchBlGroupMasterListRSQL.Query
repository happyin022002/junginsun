<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchBlGroupMasterListRSQL">
			<desc><![CDATA[select BKG_CTRL_BL_GRP List]]></desc>
			<sql><![CDATA[
SELECT
    BL_GRP_SEQ,
    BL_GRP_NM,
    BL_GRP_DESC,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
FROM BKG_CTRL_BL_GRP CTRL
WHERE 1 = 1
#if (${bl_grp_nm} != '')
AND UPPER(BL_GRP_NM) LIKE '%'||UPPER(@[bl_grp_nm])||'%'
#end
#if (${bl_grp_desc} != '')
AND UPPER(BL_GRP_DESC) LIKE '%'||UPPER(@[bl_grp_desc])||'%'
#end
#if(${cust_cnt_cd} != '' && ${cust_seq} != '')
AND EXISTS (SELECT 'X' 
 FROM BKG_CTRL_BL_GRP_CUST CUST 
 WHERE CTRL.BL_GRP_SEQ = CUST.BL_GRP_SEQ 
 AND CUST.CUST_CNT_CD = @[cust_cnt_cd]
 AND CUST.CUST_SEQ = @[cust_seq]) 
#end			]]></sql>
			<params>
				<param name="bl_grp_nm" type="12" value="" out="N"/>
				<param name="bl_grp_desc" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
