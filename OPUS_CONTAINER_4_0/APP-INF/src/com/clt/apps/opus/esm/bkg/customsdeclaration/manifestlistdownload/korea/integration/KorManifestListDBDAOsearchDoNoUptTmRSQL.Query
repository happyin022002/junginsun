<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchDoNoUptTmRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT BKG.DO_NO,
       TO_CHAR(DTL.EVNT_DT,'YYYY-MM-DD HH24:MI') AS EVNT_DT

  FROM BKG_DO BKG,
       BKG_DO_DTL DTL

 WHERE 1 = 1
   AND BKG.BKG_NO = @[bkg_no]
   AND BKG.BKG_NO = DTL.BKG_NO
   AND DTL.RLSE_STS_CD = 'A'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
