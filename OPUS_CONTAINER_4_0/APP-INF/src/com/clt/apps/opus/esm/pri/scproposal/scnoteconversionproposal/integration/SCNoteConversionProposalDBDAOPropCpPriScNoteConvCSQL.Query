<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteConversionProposalDBDAOPropCpPriScNoteConvCSQL">
			<desc><![CDATA[Note Conversion Copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SC_NOTE_CONV (
	NOTE_CONV_MAPG_ID
,	NOTE_CONV_SEQ
,	NOTE_CONV_TP_CD
,	SVC_SCP_CD
,	NOTE_HDR_SEQ
,	PROP_NO
,	AMDT_SEQ
,	CHG_RULE_TP_CD
,	NOTE_CONV_CHG_CD
,	NOTE_CONV_RULE_CD
,	EFF_DT
,	EXP_DT
,	RULE_APPL_CHG_TP_CD
,	RT_APPL_TP_CD
,	RT_OP_CD
,	CURR_CD
,	FRT_RT_AMT
,	PAY_TERM_CD
,	GEN_SPCL_RT_TP_CD
,	BKG_RAT_UT_CD
,	BKG_PRC_CGO_TP_CD
,	BKG_IMDG_CLSS_CD
,	BKG_CMDT_TP_CD
,	BKG_CMDT_DEF_CD
,	BKG_SCG_GRP_CMDT_CD
,	BKG_USA_SVC_MOD_CD
,	BKG_POR_TP_CD
,	BKG_POR_DEF_CD
,	BKG_POL_TP_CD
,	BKG_POL_DEF_CD
,	BKG_POD_TP_CD
,	BKG_POD_DEF_CD
,	BKG_DEL_TP_CD
,	BKG_DEL_DEF_CD
,	BKG_ORG_TRSP_MOD_CD
,	BKG_DEST_TRSP_MOD_CD
,	BKG_RCV_TERM_CD
,	BKG_DE_TERM_CD
,	BKG_SLAN_CD
,	BKG_VSL_CD
,	BKG_SKD_VOY_NO
,	BKG_SKD_DIR_CD
,	BKG_SOC_FLG
,	BKG_ACT_CUST_CNT_CD
,	BKG_ACT_CUST_SEQ
,	BKG_TS_PORT_TP_CD
,	BKG_TS_PORT_DEF_CD
,	BKG_DIR_CALL_FLG
,	BKG_MST_HBL_TP_CD
,	CONV_RAT_UT_CD
,	CONV_PRC_CGO_TP_CD
,	CONV_CMDT_TP_CD
,	CONV_CMDT_DEF_CD
,	CONV_ORG_LOC_TP_CD
,	CONV_ORG_LOC_DEF_CD
,	CONV_ORG_VIA_LOC_TP_CD
,	CONV_ORG_VIA_LOC_DEF_CD
,	CONV_DEST_VIA_LOC_TP_CD
,	CONV_DEST_VIA_LOC_DEF_CD
,	CONV_DEST_LOC_TP_CD
,	CONV_DEST_LOC_DEF_CD
,	CONV_PRC_RCV_TERM_CD
,	CONV_PRC_DE_TERM_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	BKG_IO_GA_CD
,	BKG_CNL_TZ_CD
,	BKG_ESVC_TP_CD
)
WITH PRE_NOTE AS (
    SELECT @[new_prop_no] AS PROP_NO
         , 0 AS AMDT_SEQ
         , B.SVC_SCP_CD
         , B.NOTE_TP_CD
         , DENSE_RANK() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.NOTE_TP_CD
                              ORDER BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.NOTE_TP_CD, A.DP_SEQ, A.NOTE_SEQ) AS NOTE_SEQ
         , DENSE_RANK() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.NOTE_TP_CD, A.NOTE_SEQ
                              ORDER BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.NOTE_TP_CD, A.NOTE_SEQ, B.NOTE_CTNT_SEQ) AS NOTE_CTNT_SEQ
         , B.NOTE_CONV_MAPG_ID
    FROM PRI_SP_SCP_NOTE A
        ,PRI_SP_SCP_NOTE_CTNT B
    WHERE A.PROP_NO = @[prop_no]
    AND   A.AMDT_SEQ = @[amdt_seq]
    AND   A.SVC_SCP_CD = @[svc_scp_cd]
    AND   A.NOTE_TP_CD = 'P'
    AND   B.PROP_NO = A.PROP_NO
    AND   B.AMDT_SEQ = A.AMDT_SEQ
    AND   B.SVC_SCP_CD = A.SVC_SCP_CD
    AND   B.NOTE_TP_CD = A.NOTE_TP_CD
    AND   B.NOTE_SEQ = A.NOTE_SEQ
    AND   B.SRC_INFO_CD <> 'AD'
)
, NEW_CONV AS (
    SELECT C.PROP_NO
         , C.AMDT_SEQ
         , C.SVC_SCP_CD
         , C.NOTE_TP_CD
         , C.NOTE_SEQ
         , C.NOTE_CTNT_SEQ
         , C.NOTE_CONV_MAPG_ID AS OLD_CONV_MAPG_ID
         , D.NOTE_CONV_MAPG_ID AS NEW_CONV_MAPG_ID
    FROM PRE_NOTE C
       , PRI_SP_SCP_NOTE_CTNT D
    WHERE D.PROP_NO = C.PROP_NO
    AND   D.AMDT_SEQ = C.AMDT_SEQ
    AND   D.SVC_SCP_CD = C.SVC_SCP_CD
    AND   D.NOTE_TP_CD = C.NOTE_TP_CD
    AND   D.NOTE_SEQ = C.NOTE_SEQ
    AND   D.NOTE_CTNT_SEQ = C.NOTE_CTNT_SEQ
)
SELECT F.NEW_CONV_MAPG_ID AS NOTE_CONV_MAPG_ID
     , DENSE_RANK() OVER (PARTITION BY E.NOTE_CONV_MAPG_ID
                          ORDER BY E.NOTE_CONV_MAPG_ID, E.NOTE_CONV_SEQ) AS NOTE_CONV_SEQ
     , E.NOTE_CONV_TP_CD
     , F.SVC_SCP_CD
     , E.NOTE_HDR_SEQ
     , F.PROP_NO
     , F.AMDT_SEQ
     , E.CHG_RULE_TP_CD
     , E.NOTE_CONV_CHG_CD
     , E.NOTE_CONV_RULE_CD
     , TO_DATE('99991231', 'YYYYMMDD') AS EFF_DT
     , TO_DATE('99991231', 'YYYYMMDD') AS EXP_DT
     , E.RULE_APPL_CHG_TP_CD
     , E.RT_APPL_TP_CD
     , E.RT_OP_CD
     , E.CURR_CD
     , E.FRT_RT_AMT
     , E.PAY_TERM_CD
     , E.GEN_SPCL_RT_TP_CD
     , E.BKG_RAT_UT_CD
     , E.BKG_PRC_CGO_TP_CD
     , E.BKG_IMDG_CLSS_CD
     , E.BKG_CMDT_TP_CD
     , E.BKG_CMDT_DEF_CD
     , E.BKG_SCG_GRP_CMDT_CD
     , E.BKG_USA_SVC_MOD_CD
     , E.BKG_POR_TP_CD
     , E.BKG_POR_DEF_CD
     , E.BKG_POL_TP_CD
     , E.BKG_POL_DEF_CD
     , E.BKG_POD_TP_CD
     , E.BKG_POD_DEF_CD
     , E.BKG_DEL_TP_CD
     , E.BKG_DEL_DEF_CD
     , E.BKG_ORG_TRSP_MOD_CD
     , E.BKG_DEST_TRSP_MOD_CD
     , E.BKG_RCV_TERM_CD
     , E.BKG_DE_TERM_CD
     , E.BKG_SLAN_CD
     , E.BKG_VSL_CD
     , E.BKG_SKD_VOY_NO
     , E.BKG_SKD_DIR_CD
     , E.BKG_SOC_FLG
     , E.BKG_ACT_CUST_CNT_CD
     , E.BKG_ACT_CUST_SEQ
     , E.BKG_TS_PORT_TP_CD
     , E.BKG_TS_PORT_DEF_CD
     , E.BKG_DIR_CALL_FLG
     , E.BKG_MST_HBL_TP_CD
     , E.CONV_RAT_UT_CD
     , E.CONV_PRC_CGO_TP_CD
     , E.CONV_CMDT_TP_CD
     , E.CONV_CMDT_DEF_CD
     , E.CONV_ORG_LOC_TP_CD
     , E.CONV_ORG_LOC_DEF_CD
     , E.CONV_ORG_VIA_LOC_TP_CD
     , E.CONV_ORG_VIA_LOC_DEF_CD
     , E.CONV_DEST_VIA_LOC_TP_CD
     , E.CONV_DEST_VIA_LOC_DEF_CD
     , E.CONV_DEST_LOC_TP_CD
     , E.CONV_DEST_LOC_DEF_CD
     , E.CONV_PRC_RCV_TERM_CD
     , E.CONV_PRC_DE_TERM_CD
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
     , E.BKG_IO_GA_CD
     , E.BKG_CNL_TZ_CD
     , E.BKG_ESVC_TP_CD
FROM PRI_SC_NOTE_CONV E
   , NEW_CONV F
WHERE E.NOTE_CONV_MAPG_ID = F.OLD_CONV_MAPG_ID			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
