<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTOfficeAgreementInfoDBDAOCreateOldAgreementNoVendortoNewSelectRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT
           A.AGMT_OFC_CD AS AGMT_OFC_CD_VALUE,
           A.VNDR_CNT_CD AS VNDR_CNT_CD_VALUE,
           A.VNDR_SEQ AS VNDR_SEQ_VALUE,
         (     SELECT
                      NVL(MAX(AGN_AGMT_SEQ) + 1, 1) AS AGN_AGMT_SEQ
                 FROM AGT_AGN_AGMT_MST
         )                                          AS AGN_AGMT_SEQ,
         (     SELECT
                      NVL(MAX(AGN_AGMT_VER_SEQ), 1) AS AGN_AGMT_VER_SEQ
                 FROM AGT_AGN_AGMT
                WHERE AGMT_OFC_CD     = A.AGMT_OFC_CD
                  AND AGMT_OFC_CTY_CD = A.AGMT_OFC_CTY_CD
                  AND AGN_AGMT_SEQ    = A.AGN_AGMT_SEQ
                  AND VNDR_CNT_CD     = A.VNDR_CNT_CD
                  AND VNDR_SEQ        = A.VNDR_SEQ
         )                                          AS AGN_AGMT_VER_SEQ_VALUE,
           A.AGMT_OFC_CTY_CD 						AS AGMT_OFC_CTY_CD_VALUE,
           A.AGN_AGMT_SEQ                           AS AGN_AGMT_SEQ_VALUE,
           @[cre_usr_id]                            AS CRE_USR_ID,
           @[upd_usr_id]                            AS UPD_USR_ID,
           A.DELT_FLG
      FROM AGT_AGN_AGMT_MST A
     WHERE A.AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd_value]
       AND A.AGN_AGMT_SEQ    = @[agn_agmt_seq_value]
       AND A.DELT_FLG        = 'N'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="agmt_ofc_cty_cd_value" type="12" value="" out="N"/>
				<param name="agn_agmt_seq_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
