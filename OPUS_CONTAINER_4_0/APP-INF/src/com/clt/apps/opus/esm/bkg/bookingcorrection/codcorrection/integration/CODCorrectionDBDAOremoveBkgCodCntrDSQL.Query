<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOremoveBkgCodCntrDSQL">
			<desc><![CDATA[bkg_Cod_cntr를 delete한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_COD_CNTR
WHERE	BKG_NO = @[bkg_no]
AND	COD_RQST_SEQ = @[cod_rqst_seq]
#if (${cntr_no} !='')
	AND CNTR_NO = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
