<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOSearchBkgCngRSQL">
			<desc><![CDATA[BDRCorrectionDBDAOSearchBkgCngRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_A(@[bkg_no],@[corr_no]) RT_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_B(@[bkg_no],@[corr_no]) CHG_TERM_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_C(@[bkg_no],@[corr_no]) RCVDE_TERM_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_D(@[bkg_no],@[corr_no]) ROUT_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_E(@[bkg_no],@[corr_no]) CUST_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_F(@[bkg_no],@[corr_no]) QTY_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_G(@[bkg_no],@[corr_no]) MEAS_QTY_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_H(@[bkg_no],@[corr_no]) CMDT_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_I(@[bkg_no],@[corr_no]) TRNK_VSL_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_J(@[bkg_no],@[corr_no]) PRPST_VSL_CORR_FLG
	 , SUBSTR(BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_CLASS(@[bkg_no],@[corr_no]),1,1) RAT_FLG
	 , SUBSTR(BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_CLASS(@[bkg_no],@[corr_no]),2,1) EXPN_FLG
  FROM DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="corr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
