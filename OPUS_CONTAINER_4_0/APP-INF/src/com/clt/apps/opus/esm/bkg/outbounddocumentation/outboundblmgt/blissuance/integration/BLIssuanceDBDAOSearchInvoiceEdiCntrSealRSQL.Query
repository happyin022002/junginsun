<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchInvoiceEdiCntrSealRSQL">
			<desc><![CDATA[BLIssuanceDBDAOSearchInvoiceEdiCntrSeal]]></desc>
			<sql><![CDATA[
SELECT '{SEAL_INFO' || CHR (10) || 
	   'SEAL_PT_CD:' || SEAL_PTY_TP_CD || CHR(10) ||
       'SEAL_NO:' || CNTR_SEAL_NO || CHR (10) || 
       '}SEAL_INFO' || CHR (10)
FROM   BKG_CNTR_SEAL_NO
WHERE 1=1
   AND BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
