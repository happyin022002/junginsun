<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAOcreatePriMotFileRtDataCSQL">
			<desc><![CDATA[일별 데이터중에서 이전 Filing 되었던 것과 같은 건을 제거한 
금일 Filing 대상 데이터 생성
PRI_MOT_FILE_RT
PRI_MOT_FILE_RT_SCG_DTL]]></desc>
			<sql><![CDATA[
INSERT ALL 
WHEN    SCG_SEQ <= 1 THEN
INTO PRI_MOT_FILE_RT 
(BAT_EXE_DT, MOT_FILE_RT_SEQ, CTRT_NO, CTRT_HLD_NM, ACT_CUST_NM, MOT_FILE_LANE_CD, MOT_FILE_ORG_CD, MOT_FILE_DEST_CD, MOT_FILE_CNTR_TP_CD, 
 MOT_FILE_CMDT_TP_CD, MOT_FILE_CNTR_SZ_CD, MOT_FILE_RT_AMT, CTRT_EFF_DT, CTRT_EXP_DT, MOT_FILE_RMK, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT, SHPR_CNT_CD, SHPR_SEQ, 
 CNEE_NM, OP_AGN_NM, COMM_AMT, FWRD_CNT_CD, FWRD_CUST_SEQ)
VALUES 
( BAT_EXE_DT, MOT_FILE_RT_LOG_SEQ, CTRT_NO, CTRT_HLD_NM, ACT_CUST_NM, MOT_FILE_LANE_CD, MOT_FILE_ORG_CD, MOT_FILE_DEST_CD, MOT_FILE_CNTR_TP_CD, 
  MOT_FILE_CMDT_TP_CD, MOT_FILE_CNTR_SZ_CD, MOT_FILE_RT_AMT, CTRT_EFF_DT, CTRT_EXP_DT, MOT_FILE_RMK, 'SYSTEM', SYSDATE, 'SYSTEM', SYSDATE, SHPR_CNT_CD, SHPR_SEQ,
  CNEE_NM, OP_AGN_NM, COMM_AMT, FWRD_CNT_CD, FWRD_CUST_SEQ )
WHEN    SCG_SEQ >= 1 THEN
INTO PRI_MOT_FILE_RT_SCG_DTL (BAT_EXE_DT, MOT_FILE_RT_SEQ, SCG_SEQ, CHG_CD, RAT_UT_CD, MOT_FILE_CHG_AMT, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT, CURR_CD)
VALUES ( BAT_EXE_DT, MOT_FILE_RT_LOG_SEQ, SCG_SEQ, CHG_CD, RAT_UT_CD, MOT_FILE_CHG_AMT, 'SYSTEM', SYSDATE, 'SYSTEM', SYSDATE, CURR_CD )

SELECT  
       A.BAT_EXE_DT
     , A.MOT_FILE_RT_LOG_SEQ
     , NVL ( B.SCG_SEQ, 0 ) AS SCG_SEQ
     , A.SHPR_CNT_CD
     , A.SHPR_SEQ
     , A.CTRT_NO
     , A.CTRT_HLD_NM
     , A.ACT_CUST_NM
     , A.MOT_FILE_LANE_CD
     , A.MOT_FILE_ORG_CD
     , A.MOT_FILE_DEST_CD
     , A.MOT_FILE_CNTR_TP_CD
     , A.MOT_FILE_CMDT_TP_CD
     , A.MOT_FILE_CNTR_SZ_CD
     , A.MOT_FILE_RT_AMT
     , A.CTRT_EFF_DT
     , A.CTRT_EXP_DT
     , A.MOT_FILE_RMK
     , A.CNEE_NM
     , A.OP_AGN_NM
     , B.CHG_CD
     , B.RAT_UT_CD
     , B.MOT_FILE_CHG_AMT
     , B.CURR_CD
     , A.COMM_AMT
     , A.FWRD_CNT_CD
     , A.FWRD_CUST_SEQ
  FROM PRI_MOT_FILE_RT_LOG A
     , PRI_MOT_FILE_RT_LOG_DTL B
 WHERE   A.BAT_EXE_DT = TO_DATE ( @[exec_dt], 'YYYY-MM-DD' )
   AND EXISTS
                (SELECT 1
                   FROM PRI_MOT_FILE_RT_LOG_DTL D
                  WHERE D.BAT_EXE_DT          = A.BAT_EXE_DT
                    AND D.MOT_FILE_RT_LOG_SEQ = A.MOT_FILE_RT_LOG_SEQ 
                    AND ROWNUM <= 1 )
   AND EXISTS (
                 SELECT M.CTRT_NO
                      , M.CTRT_HLD_NM
                      , M.ACT_CUST_NM
                      , M.CNEE_NM
                      , M.MOT_FILE_LANE_CD
                      , M.MOT_FILE_ORG_CD
                      , M.MOT_FILE_DEST_CD
                      , M.MOT_FILE_CNTR_TP_CD
                      , M.MOT_FILE_CMDT_TP_CD
                      , M.MOT_FILE_CNTR_SZ_CD
                      , M.MOT_FILE_RT_AMT
                      , D.CHG_CD
                      , D.RAT_UT_CD
                   FROM PRI_MOT_FILE_RT_LOG M
                      , PRI_MOT_FILE_RT_LOG_DTL D
                  WHERE M.BAT_EXE_DT  = TO_DATE ( @[exec_dt], 'YYYY-MM-DD' )           
                    AND A.CTRT_NO             = M.CTRT_NO
                    AND A.CTRT_HLD_NM         = M.CTRT_HLD_NM
                    AND A.ACT_CUST_NM         = M.ACT_CUST_NM
                    AND NVL(A.CNEE_NM,'1')    = NVL(M.CNEE_NM,'1')
                    AND A.MOT_FILE_LANE_CD    = M.MOT_FILE_LANE_CD
                    AND A.MOT_FILE_ORG_CD     = M.MOT_FILE_ORG_CD
                    AND A.MOT_FILE_DEST_CD    = M.MOT_FILE_DEST_CD
                    AND NVL(A.MOT_FILE_CNTR_TP_CD,'1') = NVL(M.MOT_FILE_CNTR_TP_CD,'1')
                    AND A.MOT_FILE_CMDT_TP_CD = M.MOT_FILE_CMDT_TP_CD
                    AND A.MOT_FILE_CNTR_SZ_CD = M.MOT_FILE_CNTR_SZ_CD
                    AND A.MOT_FILE_RT_AMT     = M.MOT_FILE_RT_AMT
                    AND M.BAT_EXE_DT          = D.BAT_EXE_DT
                    AND M.MOT_FILE_RT_LOG_SEQ = D.MOT_FILE_RT_LOG_SEQ

                  MINUS 

                 SELECT M.CTRT_NO
                      , M.CTRT_HLD_NM
                      , M.ACT_CUST_NM
                      , M.CNEE_NM
                      , M.MOT_FILE_LANE_CD
                      , M.MOT_FILE_ORG_CD
                      , M.MOT_FILE_DEST_CD
                      , M.MOT_FILE_CNTR_TP_CD
                      , M.MOT_FILE_CMDT_TP_CD
                      , M.MOT_FILE_CNTR_SZ_CD
                      , M.MOT_FILE_RT_AMT
                      , D.CHG_CD
                      , D.RAT_UT_CD
                   FROM PRI_MOT_FILE_RT M
                      , PRI_MOT_FILE_RT_SCG_DTL D
                  WHERE M.BAT_EXE_DT          < TO_DATE ( @[exec_dt], 'YYYY-MM-DD' )           
                    AND A.CTRT_NO             = M.CTRT_NO
                    AND A.CTRT_HLD_NM         = M.CTRT_HLD_NM
                    AND A.ACT_CUST_NM         = M.ACT_CUST_NM
                    AND NVL(A.CNEE_NM,'1')    = NVL(M.CNEE_NM,'1')
                    AND A.MOT_FILE_LANE_CD    = M.MOT_FILE_LANE_CD
                    AND A.MOT_FILE_ORG_CD     = M.MOT_FILE_ORG_CD
                    AND A.MOT_FILE_DEST_CD    = M.MOT_FILE_DEST_CD
                    AND NVL(A.MOT_FILE_CNTR_TP_CD,'1') = NVL(M.MOT_FILE_CNTR_TP_CD,'1')
                    AND A.MOT_FILE_CMDT_TP_CD = M.MOT_FILE_CMDT_TP_CD
                    AND A.MOT_FILE_CNTR_SZ_CD = M.MOT_FILE_CNTR_SZ_CD  
                    AND A.MOT_FILE_RT_AMT     = M.MOT_FILE_RT_AMT
                    AND M.BAT_EXE_DT          = D.BAT_EXE_DT
                    AND M.MOT_FILE_RT_SEQ     = D.MOT_FILE_RT_SEQ
                )    
   AND A.BAT_EXE_DT = B.BAT_EXE_DT(+)
   AND A.MOT_FILE_RT_LOG_SEQ = B.MOT_FILE_RT_LOG_SEQ(+)
 ORDER BY 1,2,3			]]></sql>
			<params>
				<param name="exec_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
