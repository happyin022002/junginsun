<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOaddFIArrivalNoticeCSQL">
			<desc><![CDATA[핀란드 전송시 VESSEL 테이블에 없을 때 데이터 신규 추가]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_EUR_VSL( 
	VSL_CD,			SKD_VOY_NO,     SKD_DIR_CD,		CVY_REF_NO,			CAP_NM,
	N1ST_CLPT_CD,	LST_CLPT_CD,    NXT_CLPT_CD,	ARR_PORT_CD,		
	TML_CD,			RGST_NO,		RGST_PORT_CD,	GRS_RGST_TONG_WGT,	NET_RGST_TONG_WGT,
	CRE_USR_ID,		UPD_USR_ID,		ETA_DT,			ATA_DT,				ETD_DT,
	RGST_DT,		CRE_DT,			UPD_DT, 		CSTMS_PORT_CD, 		CSTMS_YD_CD,
    RVIS_N1ST_CLPT_CD, RVIS_CSTMS_YD_CD, 	DVS_RQST_SMT_FLG, UPD_OFC_CD, PORT_NET_NO

)VALUES(
	SUBSTR(@[vvd], 1, 4),SUBSTR(@[vvd], 5, 4),  SUBSTR(@[vvd], 9, 1),	@[cvy_ref_no],			@[cap_nm],
	@[n1st_clpt_cd],	@[lst_clpt_cd], 		@[nxt_clpt_cd],			@[arr_port_cd],			
	@[tml_cd],			@[rgst_no],				@[rgst_port_cd],		@[grs_rgst_tong_wgt],	@[net_rgst_tong_wgt],
	@[cre_usr_id],		@[upd_usr_id],	
	TO_DATE(@[eta_dt], 'YYYY-MM-DD HH24:MI'),		
	TO_DATE(@[ata_dt], 'YYYY-MM-DD HH24:MI'),			
	TO_DATE(@[etd_dt], 'YYYY-MM-DD HH24:MI'),	
	TO_DATE(@[rgst_dt], 'YYYY-MM-DD'),SYSDATE,	SYSDATE, 				@[cstms_port_cd], 		@[cstms_yd_cd],     
	@[rvis_n1st_clpt_cd],@[rvis_cstms_yd_cd], @[dvs_rqst_smt_flg], 		(SELECT OFC_CD FROM COM_USER WHERE USR_ID = @[upd_usr_id])
   ,@[port_net_no]
 )			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="cvy_ref_no" type="12" value="" out="N"/>
				<param name="cap_nm" type="12" value="" out="N"/>
				<param name="n1st_clpt_cd" type="12" value="" out="N"/>
				<param name="lst_clpt_cd" type="12" value="" out="N"/>
				<param name="nxt_clpt_cd" type="12" value="" out="N"/>
				<param name="arr_port_cd" type="12" value="" out="N"/>
				<param name="tml_cd" type="12" value="" out="N"/>
				<param name="rgst_no" type="12" value="" out="N"/>
				<param name="rgst_port_cd" type="12" value="" out="N"/>
				<param name="grs_rgst_tong_wgt" type="12" value="" out="N"/>
				<param name="net_rgst_tong_wgt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eta_dt" type="12" value="" out="N"/>
				<param name="ata_dt" type="12" value="" out="N"/>
				<param name="etd_dt" type="12" value="" out="N"/>
				<param name="rgst_dt" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
				<param name="cstms_yd_cd" type="12" value="" out="N"/>
				<param name="rvis_n1st_clpt_cd" type="12" value="" out="N"/>
				<param name="rvis_cstms_yd_cd" type="12" value="" out="N"/>
				<param name="dvs_rqst_smt_flg" type="12" value="" out="N"/>
				<param name="port_net_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
