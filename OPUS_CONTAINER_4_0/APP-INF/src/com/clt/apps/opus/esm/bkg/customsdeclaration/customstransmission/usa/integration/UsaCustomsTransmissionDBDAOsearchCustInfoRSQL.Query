<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCustInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT TB.*
      ,CASE WHEN LENGTH(S02_B) > 70 
            THEN RPAD('S02' || SUBSTR(S02_B, 1, 70), 80, ' ') || CHR(10)
            ELSE RPAD('S02' || S02_B, 80, ' ') || CHR(10)
        END S02
      ,CASE WHEN LENGTH(S02_B) > 70 
            THEN RPAD('S03' || SUBSTR(S02_B, 71) || ' ' || S03_B , 80, ' ') || CHR(10)
            WHEN S03_B IS NOT NULL
            THEN RPAD('S03' || S03_B, 80, ' ') || CHR(10)
            ELSE ''
        END S03
      ,CASE WHEN LENGTH(U02_B) > 70 
            THEN RPAD('U02' || SUBSTR(U02_B, 1, 70), 80, ' ') || CHR(10)
            ELSE RPAD('U02' || U02_B, 80, ' ') || CHR(10)
        END U02
      ,CASE WHEN LENGTH(U02_B) > 70 
            THEN RPAD('U03' || SUBSTR(U02_B, 71) || ' ' || U03_B , 80, ' ') || CHR(10)
            WHEN U03_B IS NOT NULL
            THEN RPAD('U03' || U03_B, 80, ' ') || CHR(10)
            ELSE ''
        END U03
      ,CASE WHEN LENGTH(N02_B) > 70 
            THEN RPAD('N02' || SUBSTR(N02_B, 1, 70), 80, ' ') || CHR(10)
            ELSE RPAD('N02' || N02_B, 80, ' ') || CHR(10)
        END N02
      ,CASE WHEN LENGTH(N02_B) > 70 
            THEN RPAD('N03' || SUBSTR(N02_B, 71) || ' ' || N03_B , 80, ' ') || CHR(10)
            WHEN N03_B IS NOT NULL
            THEN RPAD('N03' || N03_B, 80, ' ') || CHR(10)
            ELSE ''
        END N03
  FROM (
SELECT A.BL_NO
      ,RPAD('S01'||RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_NM),1, ''), 'X')),'.'),35,' ')||
                   RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_NM),2, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS S01
      ,RPAD('S02'||RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_ADDR),1, ''), 'X')),'.'),35,' ')||
                   RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_ADDR),2, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS S02_OLD
      ,DECODE(BKG_TOKEN_NL_FNC(A.CUST_ADDR, 3, ''), NULL, '',
              RPAD('S03'||RPAD(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(A.CUST_ADDR, 3, ''), 'X') || TRIM(A.PHN_NO) || TRIM(A.FAX_NO), 35, ' '),80,' ') || CHR(10)
              ) AS S03_OLD
      
      ,CASE WHEN A.CUST_CTY_NM IS NOT NULL AND A.CSTMS_DECL_CNT_CD IS NOT NULL
            THEN BKG_SPCLCHAR_CONV_FNC(A.EUR_CSTMS_ST_NM || ' ' || A.CUST_CTY_NM || ' ' || A.CUST_STE_CD || ' ' || A.CUST_ZIP_ID || ' ' || A.CSTMS_DECL_CNT_CD, 'X')
            ELSE RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_ADDR),1, ''), 'X')),'.'),35,' ')||
                 RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_ADDR),2, ''), 'X')),'.'),35,' ')
        END AS S02_B
      ,CASE WHEN A.PHN_NO IS NOT NULL OR A.FAX_NO IS NOT NULL
            THEN BKG_SPCLCHAR_CONV_FNC(TRIM(A.PHN_NO) || ' ' || TRIM(A.FAX_NO), 'X')
            ELSE NULL
        END AS S03_B
      
      ,RPAD('U01'||RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_NM),1, ''), 'X')),'.'),35,' ')||
                   RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_NM),2, ''),'X')),'.'),35,' '),80,' ') || CHR(10) AS U01
      ,RPAD('U02'||RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_ADDR),1, ''),'X')),'.'),35,' ')||
                   RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_ADDR),2, ''),'X')),'.'),35,' '),80,' ') || CHR(10) AS U02_OLD
      ,DECODE(BKG_TOKEN_NL_FNC(B.CUST_ADDR, 3, ''), NULL, '',
              RPAD('U03'||RPAD(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(B.CUST_ADDR, 3, ''), 'X') || TRIM(B.PHN_NO) || TRIM(B.FAX_NO), 35, ' '),80,' ') || CHR(10)
              ) AS U03_OLD

      ,CASE WHEN B.CUST_CTY_NM IS NOT NULL AND B.CSTMS_DECL_CNT_CD IS NOT NULL
            THEN BKG_SPCLCHAR_CONV_FNC(B.EUR_CSTMS_ST_NM || ' ' || B.CUST_CTY_NM || ' ' || B.CUST_STE_CD || ' ' || B.CUST_ZIP_ID || ' ' || B.CSTMS_DECL_CNT_CD, 'X')
            ELSE RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_ADDR),1, ''), 'X')),'.'),35,' ')||
                 RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_ADDR),2, ''), 'X')),'.'),35,' ')
        END AS U02_B
      ,CASE WHEN B.PHN_NO IS NOT NULL OR B.FAX_NO IS NOT NULL
            THEN BKG_SPCLCHAR_CONV_FNC(TRIM(B.PHN_NO) || ' ' || TRIM(B.FAX_NO), 'X')
            ELSE NULL
        END AS U03_B
              
      ,RPAD('N01'||RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(C.CUST_NM),1, ''),'X')),'SAME AS CONSIGNEE'),35,' ')||
                   RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(C.CUST_NM),2, ''),'X')),'.'),35,' '),80,' ') || CHR(10) AS N01
      ,RPAD('N02'||RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(C.CUST_ADDR),1, ''),'X')),'.'),35,' ')||
                   RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(C.CUST_ADDR),2, ''),'X')),'.'),35,' '),80,' ') || CHR(10) AS N02_OLD
      ,DECODE(BKG_TOKEN_NL_FNC(C.CUST_ADDR, 3, ''), NULL, '',
              RPAD('N03'||RPAD(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(C.CUST_ADDR, 3, ''), 'X') || TRIM(C.PHN_NO) || TRIM(C.FAX_NO), 35, ' '),80,' ') || CHR(10)
              ) AS N03_OLD
              
      ,CASE WHEN C.CUST_CTY_NM IS NOT NULL AND C.CSTMS_DECL_CNT_CD IS NOT NULL
            THEN BKG_SPCLCHAR_CONV_FNC(C.EUR_CSTMS_ST_NM || ' ' || C.CUST_CTY_NM || ' ' || C.CUST_STE_CD || ' ' || C.CUST_ZIP_ID || ' ' || C.CSTMS_DECL_CNT_CD, 'X')
            ELSE RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(C.CUST_ADDR),1, ''), 'X')),'.'),35,' ')||
                 RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(C.CUST_ADDR),2, ''), 'X')),'.'),35,' ')
        END AS N02_B
      ,CASE WHEN C.PHN_NO IS NOT NULL OR C.FAX_NO IS NOT NULL
            THEN BKG_SPCLCHAR_CONV_FNC(TRIM(C.PHN_NO) || ' ' || TRIM(C.FAX_NO), 'X')
            ELSE NULL
        END AS N03_B
              
      ,(SELECT 'X'
          FROM BKG_CSTMS_ADV_ORZ_PTY
         WHERE NVL(CUST_CNT_CD, 'XXX') = DECODE(CUST_CNT_CD, '', 'XXX', DECODE(Bk.CUST_TO_ORD_FLG, 'Y',  C.CUST_CNT_CD , B.CUST_CNT_CD )) -- TO ordre Flg ='Y' 이면 Notify 아니면, Consignee 의 Customer_cd와비교
           AND NVL(CUST_SEQ, 999999) = DECODE(CUST_SEQ, '', 999999, DECODE(Bk.CUST_TO_ORD_FLG, 'Y',  C.CUST_SEQ , B.CUST_SEQ ))
           AND NVL(SC_NO, 'XXX') IN ('XXX', BK.SC_NO)
           AND NVL(POD_CD, 'XXX') IN ('XXX',BL.POD_cD)
           AND NVL(DEL_CD, 'XXX') IN ('XXX',BL.DEL_cD)
           AND DELT_FLG = 'N'
           AND ROWNUM=1
       ) ORZ_PTY
  FROM BKG_CSTMS_ADV_CUST A
      ,BKG_CSTMS_ADV_CUST B
      ,BKG_CSTMS_ADV_CUST C
      ,BKG_CUSTOMER       BS
      ,BKG_CUSTOMER       BC
      ,BKG_CUSTOMER       BN
      ,BKG_CSTMS_ADV_BL   BL
      ,BKG_BOOKING        BK
 WHERE 1=1
   AND A.CNT_CD         = 'US'
   AND A.BL_NO          = @[bl_no]
   AND A.BKG_CUST_TP_CD = 'S'
   AND B.CNT_CD         = A.CNT_CD
   AND B.BL_NO          = A.BL_NO
   AND B.BKG_CUST_TP_CD = 'C'
   AND C.CNT_CD(+)      = A.CNT_CD
   AND C.BL_NO(+)       = A.BL_NO
   AND C.BKG_CUST_TP_CD(+) = 'N'
   AND BS.BKG_NO(+)        = BL.BKG_NO
   AND BS.BKG_CUST_TP_CD(+)= 'S'
   AND BC.BKG_NO(+)        = BL.BKG_NO
   AND BC.BKG_CUST_TP_CD(+)= 'C'
   AND BN.BKG_NO(+)     = BL.BKG_NO
   AND BN.BKG_CUST_TP_CD(+) = 'N'
   AND BL.BL_NO         = A.BL_NO
   AND BL.BKG_NO        = BK.BKG_NO(+)
)TB			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
