<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteProposalDBDAOPropCpPriSpScpNoteCSQL">
			<desc><![CDATA[Proposal Scope Copy PRI_SP_SCP_NOTE Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_NOTE (
      PROP_NO
    , AMDT_SEQ
    , SVC_SCP_CD
    , NOTE_TP_CD
    , NOTE_SEQ
    , NOTE_TIT_NM
    , NOTE_CLSS_CD
    , DP_SEQ
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT)
SELECT @[new_prop_no] AS PROP_NO
     , 0 AS AMDT_SEQ
     , SVC_SCP_CD
     , NOTE_TP_CD
     , DENSE_RANK() OVER (PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, NOTE_TP_CD
                          ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, NOTE_TP_CD, DP_SEQ, NOTE_SEQ) AS NOTE_SEQ
     , NOTE_TIT_NM
     , NOTE_CLSS_CD
     , DP_SEQ
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM PRI_SP_SCP_NOTE
WHERE PROP_NO = @[prop_no]
AND   AMDT_SEQ = @[amdt_seq]
AND   SVC_SCP_CD = @[svc_scp_cd]
AND   NOTE_TP_CD = 'P'			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
