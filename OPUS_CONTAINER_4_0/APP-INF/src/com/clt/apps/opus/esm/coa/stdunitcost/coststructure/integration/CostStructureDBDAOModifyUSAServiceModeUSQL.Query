<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOModifyUSAServiceModeUSQL">
			<desc><![CDATA[ModifyUSAServiceMode]]></desc>
			<sql><![CDATA[
UPDATE COA_USA_SVC_MOD SET 
	SVC_MOD_CD = @[svc_mod_cd],
	UPD_USR_ID = @[upd_usr_id],
	UPD_DT = SYSDATE
WHERE 1 = 1
	AND ORG_RGN_CD = @[org_rgn_cd]
	AND DEST_RGN_CD = @[dest_rgn_cd]
			]]></sql>
			<params>
				<param name="svc_mod_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="org_rgn_cd" type="12" value="" out="N"/>
				<param name="dest_rgn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
