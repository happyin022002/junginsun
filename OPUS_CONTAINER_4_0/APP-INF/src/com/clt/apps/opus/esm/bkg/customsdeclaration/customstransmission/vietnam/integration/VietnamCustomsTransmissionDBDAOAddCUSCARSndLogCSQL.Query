<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VietnamCustomsTransmissionDBDAOAddCUSCARSndLogCSQL">
			<desc><![CDATA[AddCUSCARSndLog]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_VN_SND_LOG (

     VN_MF_SND_IND_CD
    ,MF_SND_DT
    ,FLT_FILE_REF_NO
    ,MF_SND_SEQ
    ,VSL_CD
    ,SKD_VOY_NO
    ,SKD_DIR_CD
    ,POL_CD
    ,POD_CD
    ,VN_CSTMS_BND_CD
    ,OFC_CD
    ,BKG_NO
    ,CNTR_NO
    ,MF_RCVR_USR_ID
    ,TO_VSL_CD
    ,TO_SKD_VOY_NO
    ,TO_SKD_DIR_CD
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT

) VALUES (

     @[vn_mf_snd_ind_cd]
    ,SYSDATE
    ,@[flt_file_ref_no]
    ,(SELECT NVL(MAX(MF_SND_SEQ),0) + 1
      FROM   BKG_CSTMS_VN_SND_LOG
      WHERE  1 = 1
      AND    VN_MF_SND_IND_CD = @[vn_mf_snd_ind_cd]
      AND    FLT_FILE_REF_NO = @[flt_file_ref_no])
    ,SUBSTR(@[vvd],1,4)
    ,SUBSTR(@[vvd],5,4)
    ,SUBSTR(@[vvd],9,1)
    ,@[pol_cd]
    ,@[pod_cd]
    ,@[vn_cstms_bnd_cd]
    ,@[ofc_cd]
    ,@[bkg_no]
    ,@[cntr_no]
    ,@[usr_id]
    ,@[to_vsl_cd]
    ,@[to_skd_voy_no]
    ,@[to_skd_dir_cd]
    ,@[usr_id]
    ,SYSDATE
    ,@[usr_id]
    ,SYSDATE

)
			]]></sql>
			<params>
				<param name="vn_mf_snd_ind_cd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="vn_cstms_bnd_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="to_vsl_cd" type="12" value="" out="N"/>
				<param name="to_skd_voy_no" type="12" value="" out="N"/>
				<param name="to_skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
