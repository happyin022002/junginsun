<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArgCustomsTransmissionDBDAOaddBkgCstmsArgSndLogCSQL">
			<desc><![CDATA[addBkgCstmsArgSndLog]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_CSTMS_ARG_SND_LOG
      (
       BL_NO
      ,IO_BND_CD
      ,SND_DT
      ,EDI_SND_MSG_CTNT
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      )
 VALUES
      (
       @[bl_no]
      ,@[io_bnd_cd]
      ,NEW_TIME(SYS_EXTRACT_UTC(CURRENT_TIMESTAMP), 'GMT', 'EST')
      ,@[edi_snd_msg_ctnt]
      ,@[upd_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE
      )			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="edi_snd_msg_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
