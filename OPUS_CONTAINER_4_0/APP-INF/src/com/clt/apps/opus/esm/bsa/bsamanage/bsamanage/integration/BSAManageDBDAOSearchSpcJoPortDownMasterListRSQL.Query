<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOSearchSpcJoPortDownMasterListRSQL">
			<desc><![CDATA[SearchSpcJoPortDownMasterList SELECT]]></desc>
			<sql><![CDATA[
SELECT 
      A.BSA_SEQ
     ,A.TRD_CD
     ,A.RLANE_CD
     ,A.DIR_CD
     ,A.VOP_CD
     ,A.VVD_CD
     ,A.VSL_CAPA
     ,A.BSA_FM_DT
     ,A.BSA_TO_DT
     ,SUM(A.BSA_CAPA) AS BSA_CAPA
     ,SUM(A.FNL_CO_BSA_CAPA) AS FNL_CO_BSA_CAPA
     ,B.BSA_OP_CD
     ,DECODE(B.BSA_OP_JB_CD, '009' , '016' , B.BSA_OP_JB_CD) BSA_OP_JB_CD
     ,B.CRR_CD, C.STUP_FLG
     ,SUM(B.SPC_CTRL_SLT_CAPA) AS CRR_BSA_CAPA
FROM   
      BSA_JNT_OP_BZC      A
     ,BSA_JNT_OP_CRR_CAPA B
     ,COA_LANE_RGST       C
WHERE  
       A.BSA_SEQ   = B.BSA_SEQ
AND    A.TRD_CD    = B.TRD_CD
AND    A.RLANE_CD  = B.RLANE_CD
AND    A.DIR_CD    = B.DIR_CD
AND    A.VOP_CD    = B.VOP_CD
AND    A.VSL_CAPA  = B.VSL_CAPA
AND    A.TRD_CD    = C.TRD_CD
AND    A.RLANE_CD  = C.RLANE_CD
AND    A.DIR_CD    = C.DIR_CD
AND    NVL(C.STUP_FLG,'N') = 'Y'
AND    NVL(C.DELT_FLG,'N') = 'N'
AND    A.BSA_TO_DT >= @[txtsdate]
#if (${cobtrade} != '')
   AND A.TRD_CD = @[cobtrade]
#end
#if (${coblane} != '')
   AND A.RLANE_CD = @[coblane]
#end
#if (${cobdir} != '')
   AND A.DIR_CD = @[cobdir]
#end
AND    B.BSA_OP_CD = @[rdoopcd]
AND    B.BSA_OP_JB_CD = DECODE(@[rdotype],'016','009',@[rdotype])
AND    B.CRR_CD = @[cobCarrier]
GROUP BY 
        A.BSA_SEQ, 
        A.TRD_CD, 
        A.RLANE_CD, 
        A.DIR_CD, 
        A.VOP_CD, 
        A.VVD_CD, 
        A.VSL_CAPA,
        A.BSA_FM_DT,
        A.BSA_TO_DT, 
        B.BSA_OP_CD, 
        B.BSA_OP_JB_CD,
        B.CRR_CD, 
        C.STUP_FLG
ORDER BY 
        A.TRD_CD, 
        A.RLANE_CD, 
        A.DIR_CD, 
        A.VOP_CD, 
        A.VSL_CAPA, 
        A.BSA_SEQ			]]></sql>
			<params>
				<param name="txtsdate" type="12" value="" out="N"/>
				<param name="cobtrade" type="12" value="" out="N"/>
				<param name="coblane" type="12" value="" out="N"/>
				<param name="cobdir" type="12" value="" out="N"/>
				<param name="rdoopcd" type="12" value="" out="N"/>
				<param name="rdotype" type="12" value="" out="N"/>
				<param name="cobCarrier" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
