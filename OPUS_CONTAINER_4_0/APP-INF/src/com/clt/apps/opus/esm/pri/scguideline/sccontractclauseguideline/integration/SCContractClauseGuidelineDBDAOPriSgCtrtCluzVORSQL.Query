<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCContractClauseGuidelineDBDAOPriSgCtrtCluzVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	svc_scp_cd
,	gline_seq
,	ctrt_cluz_seq
,	note_clss_cd
,	cre_usr_id
,	cre_dt
,	upd_usr_id
,	upd_dt
FROM pri_sg_ctrt_cluz
WHERE	svc_scp_cd = @[svc_scp_cd]
AND	gline_seq = @[gline_seq]
ORDER BY note_clss_cd ASC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
