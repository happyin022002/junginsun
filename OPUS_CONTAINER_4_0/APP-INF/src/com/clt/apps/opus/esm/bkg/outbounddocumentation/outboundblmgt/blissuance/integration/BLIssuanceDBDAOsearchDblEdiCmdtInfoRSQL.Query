<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiCmdtInfoRSQL">
			<desc><![CDATA[BLIssuanceDBDAOsearchDblEdiCmdtInfoRSQL]]></desc>
			<sql><![CDATA[
SELECT '{CMDT_INFO' || CHR(10)
       || 'CMDT_SEQ:' || ROWNUM || CHR(10) 
       || 'CMDT_PKG_QTY:' || CM.PCK_QTY || CHR(10) 
       || 'CMDT_PKG_CD:' || CM.PCK_TP_CD || CHR(10) 
       || 'CMDT_PKG_CD_DESC:' ||CM.PCK_DES|| CHR(10) 
       || 'CMDT_WGT_QTY:' ||CM.CNTR_MF_WGT||CHR(10) 
       || 'CMDT_WGT_UNIT:' ||CM.WGT_UT_CD||CHR(10) 
       || 'CMDT_NET_WGT_QTY:' ||CM.NET_CNTR_MF_WGT|| CHR(10) 
       || 'CMDT_NET_WGT_UNIT:' ||CM.NET_WGT_UT_CD||CHR(10) 
       || 'CMDT_MEA_QTY:' ||CM.MEAS_QTY|| CHR(10) 
       || 'CMDT_MEA_UNIT:' ||CM.MEAS_UT_CD||CHR(10) 
       || 'CMDT_HTS_CD:' ||CM.HAMO_TRF_CD|| CHR(10) 
       || 'CMDT_HS_CD:' ||CM.CMDT_HS_CD||CHR(10) 
       || 'CMDT_NCM_CD:' ||CM.NCM_NO|| CHR(10) 
       || 'CMDT_DESC:' ||CM.CNTR_MF_GDS_DESC||CHR(10) 
       || 'CMDT_MARKNO:' ||CM.CNTR_MF_MK_DESC || CHR(10) 
       || '}CMDT_INFO' || CHR(10) 
  FROM (
            SELECT 
            SUM(PCK_QTY) AS PCK_QTY,
            PCK_TP_CD,
            (SELECT PCK_NM FROM MDM_PCK_TP WHERE PCK_CD = CM.PCK_TP_CD) PCK_DES,
            SUM(CNTR_MF_WGT) AS CNTR_MF_WGT,
            WGT_UT_CD WGT_UT_CD,
            SUM(CNTR_MF_WGT) NET_CNTR_MF_WGT,
            WGT_UT_CD NET_WGT_UT_CD,
            SUM(MEAS_QTY) AS MEAS_QTY,
            MEAS_UT_CD,
            HAMO_TRF_CD,
            CMDT_HS_CD,
            NCM_NO,
            REPLACE(CNTR_MF_GDS_DESC,CHR(10),' ') CNTR_MF_GDS_DESC,
            REPLACE(CNTR_MF_MK_DESC,CHR(10), ' ') CNTR_MF_MK_DESC
         FROM BKG_CNTR_MF_DESC CM
        WHERE BKG_NO = @[bkg_no]
        GROUP BY PCK_TP_CD, 
                 WGT_UT_CD,
                 MEAS_UT_CD,
                 HAMO_TRF_CD,
                 CMDT_HS_CD,
                 NCM_NO,
                 CNTR_MF_GDS_DESC,
                 CNTR_MF_MK_DESC
        ) CM			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
