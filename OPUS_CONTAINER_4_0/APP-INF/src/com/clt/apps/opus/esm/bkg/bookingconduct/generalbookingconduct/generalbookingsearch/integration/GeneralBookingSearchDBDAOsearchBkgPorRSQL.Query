<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchBkgPorRSQL">
			<desc><![CDATA[searchBkgPor]]></desc>
			<sql><![CDATA[
SELECT POR_CD
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
