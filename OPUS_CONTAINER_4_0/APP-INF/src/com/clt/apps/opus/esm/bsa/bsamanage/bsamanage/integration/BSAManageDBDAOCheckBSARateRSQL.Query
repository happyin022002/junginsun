<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOCheckBSARateRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(CASE WHEN BSA_FM_DT = @[bsa_fm_dt] AND BSA_TO_DT = @[bsa_to_dt]
                     AND DELT_FLG     = 'N' 
                    THEN 1 ELSE 0 END),0) AS CNT
      ,NVL(SUM(CASE WHEN @[bsa_fm_dt] BETWEEN BSA_FM_DT AND BSA_TO_DT
                     AND DELT_FLG     = 'N' 
                    THEN 1 
                    WHEN @[bsa_to_dt] BETWEEN BSA_FM_DT AND BSA_TO_DT
                     AND DELT_FLG     = 'N'                  
                    THEN 1 ELSE 0 END),0) AS CNT
   FROM BSA_HIGH_CUBIC_RT_MST 
  WHERE CNTR_TPSZ_CD = @[cntr_tpsz_cd]
    AND TRD_CD       = @[trd_cd]
    AND RLANE_CD     = @[rlane_cd]
    AND DIR_CD       = @[dir_cd]
    AND VOP_CD       = @[vop_cd]			]]></sql>
			<params>
				<param name="bsa_fm_dt" type="12" value="" out="N"/>
				<param name="bsa_to_dt" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vop_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
