			<sql><![CDATA[
SELECT BKG_NO
, BB_CGO_SEQ
, RCV_TERM_CD
, DE_TERM_CD
, PCK_QTY
, PCK_TP_CD
, GRS_WGT
, NET_WGT
, WGT_UT_CD
, DIM_LEN
, DIM_WDT
, DIM_HGT
, CGO_WGT
, STWG_INSTR_DESC
, BB_DCGO_SEQ
, CMDT_CD
, SLNG_PNT_FLG
, GRAV_CTR_DESC
, PCK_DTL_DESC
, CGO_LODG_MZD_CD
, SCR_DNG_CTNT
, SPCL_RQST_DESC
, DIFF_RMK
, TO_CHAR(RQST_DT, 'YYYY-MM-DD HH24:MI:SS') RQST_DT
, RQST_USR_ID
, SPCL_CGO_APRO_CD
, CRE_USR_ID
, TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, UPD_USR_ID
, TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
  FROM BKG_BB_CGO
 WHERE BKG_NO = @[bkg_no]			]]></sql>
