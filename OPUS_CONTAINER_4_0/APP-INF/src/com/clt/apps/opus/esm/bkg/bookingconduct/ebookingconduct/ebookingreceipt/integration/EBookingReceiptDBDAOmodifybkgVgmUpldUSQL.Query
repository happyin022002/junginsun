<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOmodifybkgVgmUpldUSQL">
			<desc><![CDATA[EBookingReceiptDBDAOmodifybkgVgmUpld]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_VGM
SET   UPLD_FLG =@[upld_flg]
	, IF_FLG = @[if_flg]
    , UPLD_DT = SYSDATE
WHERE BKG_NO = @[vgm_bkg_no]
AND CNTR_NO = @[cntr_no]
AND VGM_SEQ = @[vgm_seq]			]]></sql>
			<params>
				<param name="upld_flg" type="12" value="" out="N"/>
				<param name="if_flg" type="12" value="" out="N"/>
				<param name="vgm_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="vgm_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
