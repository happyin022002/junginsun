<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAODeleteEurDgPortListDSQL">
			<desc><![CDATA[Eur DG 관련 Berth Port Setup 내용을 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE 
  FROM BKG_CSTMS_EUR_DG_SND_STUP 
 WHERE 1=1 
       AND PORT_CD = @[port_cd] 
       AND CRR_CD = @[crr_cd]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
