<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RatingUnitDBDAOPriRatUtVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE pri_rat_ut SET 
 	rat_ut_desc = @[rat_ut_desc]
,	rat_ut_grp_cd = @[rat_ut_grp_cd]
,	cntr_sz_cd = @[cntr_sz_cd]
,	cntr_len = @[cntr_len]
,	cntr_wdt = @[cntr_wdt]
,	cntr_hgt = @[cntr_hgt]
,	cntr_wgt = @[cntr_wgt]
,	ctrt_use_ony_flg = DECODE(NVL(@[ctrt_use_ony_flg],'0'),'0','N','1','Y')
,	delt_flg = DECODE(NVL(@[delt_flg],'0'),'0','N','1','Y')
,	upd_usr_id = @[upd_usr_id]
,	upd_dt = sysdate
WHERE	rat_ut_cd = @[rat_ut_cd]			]]></sql>
			<params>
				<param name="rat_ut_desc" type="12" value="" out="N"/>
				<param name="rat_ut_grp_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
				<param name="cntr_len" type="2" value="" out="N"/>
				<param name="cntr_wdt" type="2" value="" out="N"/>
				<param name="cntr_hgt" type="2" value="" out="N"/>
				<param name="cntr_wgt" type="2" value="" out="N"/>
				<param name="ctrt_use_ony_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rat_ut_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
