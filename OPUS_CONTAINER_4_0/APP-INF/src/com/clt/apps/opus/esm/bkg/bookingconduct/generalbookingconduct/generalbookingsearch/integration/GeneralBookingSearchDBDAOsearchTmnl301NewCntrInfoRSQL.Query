                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_ML_ADDR
    FROM BKG_BOOKING BK
         , SCE_COP_HIS SCHC
         , BKG_CONTAINER CNTR
         , BKG_XTER_VGM_RQST BXVR
         , BKG_XTER_VGM ECOM
         , BKG_XTER_CNTR BXC
    WHERE BK.BKG_NO        = @[bkg_no]
    AND   BK.BKG_NO   	   = SCHC.BKG_NO
    AND   SCHC.COP_STS_CD  <> 'X'
    AND   BK.BKG_STS_CD    = 'X'
    AND   NOT EXISTS (SELECT 'X' FROM SCE_COP_HDR HDR WHERE HDR.BKG_NO = SCHC.BKG_NO )
    AND   SCHC.ACCL_SEQ = (SELECT MAX(HIS.ACCL_SEQ) FROM SCE_COP_HIS HIS WHERE HIS.COP_NO = SCHC.COP_NO AND SCHC.BKG_NO = HIS.BKG_NO)
    AND   SCHC.BKG_NO      = CNTR.BKG_NO(+)
    AND   SCHC.CNTR_NO     = CNTR.CNTR_NO(+)
    AND   CNTR.CNTR_NO = BXVR.CNTR_NO(+)
    AND   CNTR.XTER_SNDR_ID = BXVR.XTER_SNDR_ID(+)
    AND   CNTR.XTER_VGM_DOC_ID = BXVR.XTER_VGM_DOC_ID(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = BXVR.XTER_VGM_RQST_SEQ(+)
    AND   CNTR.BKG_NO = ECOM.BKG_NO(+)
    AND   CNTR.CNTR_NO = ECOM.CNTR_NO(+)
    AND   CNTR.XTER_VGM_DOC_ID = ECOM.BKG_NO(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = ECOM.VGM_SEQ(+)
	AND   CNTR.XTER_VGM_USR_ID = ECOM.USR_ID(+)
    AND   DECODE(CNTR.XTER_SNDR_ID,'WEB',CNTR.BKG_NO,'') = ECOM.BKG_NO(+)
    AND   CNTR.XTER_SNDR_ID = BXC.XTER_SNDR_ID(+)
    AND   CNTR.XTER_VGM_DOC_ID = BXC.XTER_RQST_NO(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = BXC.XTER_RQST_SEQ(+)
    AND   CNTR.CNTR_NO = BXC.CNTR_NO(+)
    ORDER BY SCHC.COP_NO
    ) CNTR

    ,(SELECT CASE WHEN RF.CNTR_NO IS NOT NULL THEN RF.CNTR_NO
             ELSE RF.CNTR_TPSZ_CD||'-'||row_number() over (partition by RF.CNTR_TPSZ_CD order by RF.CNTR_TPSZ_CD) 
             END RF_CNTR_NO
             ,RF.*
      FROM BKG_RF_CGO RF WHERE RF.BKG_NO = @[bkg_no] AND RF.CNTR_TPSZ_CD is not null
      ORDER BY RF.RC_SEQ) RF
    ,(SELECT CASE WHEN AWK.CNTR_NO IS NOT NULL THEN AWK.CNTR_NO
             ELSE AWK.CNTR_TPSZ_CD||'-'||row_number() over (partition by AWK.CNTR_TPSZ_CD order by AWK.CNTR_TPSZ_CD) 
             END AWK_CNTR_NO
             ,AWK.*
      FROM BKG_AWK_CGO AWK WHERE AWK.BKG_NO = @[bkg_no] AND AWK.CNTR_TPSZ_CD is not null
      ORDER BY AWK.AWK_CGO_SEQ) AK
    ,(SELECT CASE WHEN DG.CNTR_NO IS NOT NULL THEN DG.CNTR_NO
             ELSE DG.CNTR_TPSZ_CD||'-'||row_number() over (partition by DG.CNTR_TPSZ_CD order by DG.CNTR_TPSZ_CD) 
             END DG_CNTR_NO
             ,DG.*
             ,(select sum(nvl(bdg.grs_wgt,0)) from bkg_dg_cgo bdg where bdg.bkg_no = dg.bkg_no and bdg.dg_cntr_seq = dg.dg_cntr_seq) as dg_grs_wgt
             ,(select sum(nvl(bdg.net_wgt,0)) from bkg_dg_cgo bdg where bdg.bkg_no = dg.bkg_no and bdg.dg_cntr_seq = dg.dg_cntr_seq) as dg_net_wgt
      FROM BKG_DG_CGO DG WHERE DG.BKG_NO = @[bkg_no] AND DG.CNTR_TPSZ_CD is not null
                         AND DG.CNTR_CGO_SEQ = (SELECT MIN(DG1.CNTR_CGO_SEQ) FROM BKG_DG_CGO DG1 WHERE DG1.BKG_NO = DG.BKG_NO AND DG1.DG_CNTR_SEQ = DG.DG_CNTR_SEQ)
      ORDER BY DG.CNTR_CGO_SEQ) DG
    , BKG_CNTR_SEAL_NO seal
    , MDM_LOCATION POL
    , MDM_LOCATION POD
WHERE CNTR.RF_CNTR_NO = RF.RF_CNTR_NO(+)
AND   CNTR.BKG_NO = RF.BKG_NO(+)
AND   CNTR.DG_CNTR_NO = DG.DG_CNTR_NO(+)
AND   CNTR.BKG_NO = DG.BKG_NO(+)
AND   CNTR.AWK_CNTR_NO = AK.AWK_CNTR_NO(+)
AND   CNTR.BKG_NO = AK.BKG_NO(+)
AND   CNTR.BKG_NO      = SEAL.BKG_NO(+)
AND   CNTR.CNTR_NO     = seal.CNTR_NO      (+)
AND   1                = seal.CNTR_SEAL_SEQ(+)
AND   CNTR.POL_CD = POL.LOC_CD
AND   CNTR.POD_CD = POD.LOC_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
