<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchTmnl301NewCntrInfoRSQL">
			<desc><![CDATA[searchTmnl301CntrInfo]]></desc>
			<sql><![CDATA[
SELECT 
 '{CNTR_INFO'																    || CHR(10)
	|| 'CNTN:'					|| RPAD(NVL(CNTR.CNTR_NO, ' '), 11, ' ')		|| CHR(10)
    || 'CSN:'                   || bkg_get_slot_no_fnc(CNTR.COP_NO)             || CHR(10)
	|| 'CNTT:'					|| NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD)	|| CHR(10)
	|| 'MRN:'			  		|| (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CMRN' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1)	|| CHR(10)
    || 'UCR:'				  || CASE WHEN (SELECT SUM(BQ.OP_CNTR_QTY) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) = 1 THEN
                            NVL((SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1),
                            (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND ROWNUM = 1))
                              ELSE
                            (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1)
                         END
                         	|| CHR(10)
    || 'HS_CD:'					|| (SELECT BCMD.CMDT_HS_CD FROM BKG_CNTR_MF_DESC BCMD WHERE BCMD.BKG_NO = CNTR.BKG_NO AND BCMD.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1) || CHR(10)
	|| 'RIND:'          || DECODE(RF.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10)
	|| 'DIND:'          || DECODE(DG.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10)
	|| 'AIND:'          || DECODE(AK.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10) 
	|| 'BIND:'          || decode(CNTR.BB_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'WGT_QTY:'       || CASE WHEN NVL(CNTR.CNTR_WGT,0) <> 0 THEN CNTR.CNTR_WGT + NVL((SELECT NVL(MCS.TARE_WGT,0)
                                                                                    FROM MST_CONTAINER MC
                                                                                         ,MST_CNTR_SPEC MCS
                                                                                    WHERE MC.CNTR_SPEC_NO = MCS.CNTR_SPEC_NO
                                                                                    AND MC.CNTR_NO = CNTR.CNTR_NO
                                                                                    AND ROWNUM = 1)
																	,(SELECT NVL(MCTS.CNTR_TPSZ_TARE_WGT,0) FROM MDM_CNTR_TP_SZ MCTS WHERE MCTS.CNTR_TPSZ_CD = NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD)))
                                WHEN ((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0)) = 0 THEN 0
                                ELSE ROUND((SELECT (BLD.ACT_WGT-NVL((SELECT SUM(NVL(BCC.CNTR_WGT,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO),0))
                                     /((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = BLD.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0))
                                     FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO),3)
                                     + (SELECT NVL(MCTS.CNTR_TPSZ_TARE_WGT,0) FROM MDM_CNTR_TP_SZ MCTS WHERE MCTS.CNTR_TPSZ_CD = NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD) AND ROWNUM = 1)
                           END 					                || CHR(10)
	|| 'WGT_TP:'        || CASE WHEN Cntr.WGT_UT_CD IS NULL THEN (SELECT BLD.WGT_UT_CD FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO)
                                        ELSE Cntr.WGT_UT_CD END 					|| CHR(10)
    || 'NWGT_QTY:'      || CASE WHEN NVL(CNTR.CNTR_WGT,0) <> 0 THEN CNTR.CNTR_WGT
                              WHEN ((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0)) = 0 THEN 0
                              ELSE ROUND((SELECT (BLD.ACT_WGT-NVL((SELECT SUM(NVL(BCC.CNTR_WGT,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO),0))
                                     /((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = BLD.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0))
                                     FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO),3)
                         END 					|| CHR(10)
	|| 'NWGT_TP:'       || CASE WHEN Cntr.WGT_UT_CD IS NULL THEN (SELECT BLD.WGT_UT_CD FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO)
                         ELSE Cntr.WGT_UT_CD END 					                              || CHR(10)
	|| 'TEMP:'					|| RF.FDO_TEMP									                                  || CHR(10)
	|| 'TUN:'					  || 'F'											                                      || CHR(10)
	|| 'TEMP_C:'				|| RF.CDO_TEMP									                                  || CHR(10)
	|| 'TUN_C:'					|| 'C'											                                      || CHR(10)
 	|| 'PRECOOLING:'															                                        || CHR(10)
	|| 'RF_VOLTAGE:'		|| RF.VLTG_NO									                                    || CHR(10)
	|| 'VENT:'					|| CASE WHEN NVL(RF.VENT_RTO,0) = 0 THEN 'C'
										WHEN NVL(RF.VENT_RTO,0) > 0 AND NVL(RF.VENT_RTO,0) < 35 THEN 'Q'
										WHEN NVL(RF.VENT_RTO,0) >= 35 AND NVL(RF.VENT_RTO,0) < 65 THEN 'H'
										WHEN NVL(RF.VENT_RTO,0) >= 65 AND NVL(RF.VENT_RTO,0) < 100 THEN 'T'
										WHEN NVL(RF.VENT_RTO,0) = 100 THEN 'O' END		                      || CHR(10)
  	|| 'VENT_NUM:'			|| RF.VENT_RTO						        	                              || CHR(10)
	|| 'VENT_CMH:'			|| RF.CBM_PER_HR_QTY							                                || CHR(10)
	|| 'HUMID:'					|| RF.HUMID_NO									                                  || CHR(10)
	|| 'CAO2:'					|| RF.OXGN_RTO									                                  || CHR(10)
	|| 'CACO2:'					|| RF.CRBN_DXD_RTO									                                  || CHR(10)
	|| 'GENSET:'			  || RF.PWR_SPL_CBL_FLG                                             || CHR(10)
  	|| 'GENSET_CD:'																                                        || CHR(10)
  	|| 'GENSET_DESC:'															                                        || CHR(10)
	|| 'RF_REMARK:'			|| BKG_SPCLCHAR_CONV_FNC(REPLACE(RF.DIFF_RMK, chr(10), ' '),'E')	|| CHR(10)
	|| 'RFDRY_IND:'			|| DECODE(CNTR.RD_CGO_FLG,'N','0','Y','1','0')        || CHR(10)
	|| 'RF_DRAIN:'			|| decode(RF.CNTR_DRN_CD,'C','OFF','O','ON','N','N/A','')		      || CHR(10)
  	|| 'AK_UNIT:'				|| 'CM'											                                      || CHR(10)
	|| 'OVF:'			   		|| AK.OVR_FWRD_LEN								                                || CHR(10)
	|| 'OVR:'				  	|| AK.OVR_BKWD_LEN								                                || CHR(10)
	|| 'OVH:'					  || AK.OVR_HGT									                                    || CHR(10)
	|| 'OVLW:'					|| AK.OVR_LF_LEN								                                  || CHR(10)
	|| 'OVRW:'					|| AK.OVR_RT_LEN								                                  || CHR(10)
	|| 'OVWGT:'					|| ROUND(NVL(AK.GRS_WGT, 0) * DECODE(AK.WGT_UT_CD, 'LBS', 0.4536, 1), 3) || CHR(10)
	|| 'VOID_SLOT:'			|| AK.OVR_VOID_SLT_QTY							                              || CHR(10)
	|| 'STWG_REQ:'			|| REPLACE(AK.STWG_RQST_DESC, CHR(10), ' ')	                      || CHR(10)
  	|| 'MEMO:'					|| BKG_SPCLCHAR_CONV_FNC(REPLACE(AK.DIFF_RMK, CHR(10), ' '),'E')  || CHR(10)
	|| 'TTL_DIM_LEN:'		|| AK.TTL_DIM_LEN								                                  || CHR(10)
	|| 'TTL_DIM_WDT:'		|| AK.TTL_DIM_WDT								                                  || CHR(10)
	|| 'TTL_DIM_HGT:'		|| AK.TTL_DIM_HGT								                                  || CHR(10)
	|| 'TRM_TYPE:'																|| CHR(10)
	|| 'TRM_WEIGHT:'															|| CHR(10)
	|| 'TRM_HAULAGE:'															|| CHR(10)
	|| 'TRM_HMODE:'																|| CHR(10)
	|| 'TRM_PICKUP_CY:'														|| CHR(10)
	|| 'TRM_RETURN_CY:'														|| CHR(10)
	|| 'TRM_INSTRUCTION:'													|| CHR(10)
	|| 'TRM_TRAN_DT:'														|| CHR(10)
	|| 'TRM_TRAN_OFFICE:'													|| CHR(10)
	|| 'TRM_TRAN_NO:'														|| CHR(10)
	|| 'USR_ID:'															  || CHR(10)

	|| '{VGM'																  || CHR(10)
	|| 'DOC_ID:'		|| CNTR.DOC_ID									  	  || CHR(10)
	|| 'MEAS:'			|| CNTR.VGM_WGT										  || CHR(10)
	|| 'MEAS_UT:'		|| substr(CNTR.VGM_WGT_UT_CD,1,1)				  	  || CHR(10)
	|| 'DOC_TP:'		|| CNTR.DOC_TP										  || CHR(10)
	|| 'DATE_TP:'		|| DECODE(CNTR.VGM_WGT_UPD_DT,NULL,'','WAT')		  || CHR(10)
	|| 'DATE:'			|| TO_CHAR(CNTR.VGM_WGT_UPD_DT,'YYYYMMDDHH24MI')	  || CHR(10)
	|| 'CUST_CNTC_TP:'	|| CNTR.CUST_CNTC_TP								  || CHR(10)
	|| 'CUST_CNTC_NM:'	|| CNTR.CUST_CNTC_NM								  || CHR(10)
	|| 'CUST_FAX:'		|| CNTR.CUST_FAX								  	  || CHR(10)
	|| 'CUST_EML:'		|| CNTR.CUST_EML									  || CHR(10)
	|| 'CUST_PHN:'		|| CNTR.CUST_PHN									  || CHR(10)
	|| 'CUST_ML_ADDR:'	|| CNTR.CUST_ML_ADDR								  || CHR(10)
	|| '}VGM'                                                                 || CHR(10)

  	|| CASE WHEN (SELECT COUNT(*) FROM BKG_CNTR_SEAL_NO BCS WHERE CNTR.BKG_NO = BCS.BKG_NO AND CNTR.CNTR_NO = BCS.CNTR_NO) = 0 THEN '{CNTR_SEAL_NO'|| CHR(10) || 'SEAL_NO:'||CHR(10) ||'}CNTR_SEAL_NO'||CHR(10)
          ELSE
                 BKG_JOIN_FNC(CURSOR(SELECT '{CNTR_SEAL_NO'|| CHR(10) || 'SEAL_NO:'||BKG_SPCLCHAR_CONV_FNC(BCS.CNTR_SEAL_NO,'E')|| CHR(10)|| '}CNTR_SEAL_NO' 
                                     FROM BKG_CNTR_SEAL_NO BCS 
                                     WHERE CNTR.BKG_NO = BCS.BKG_NO AND CNTR.CNTR_NO = BCS.CNTR_NO) , CHR(10))||CHR(10)
     	END 
  	|| CASE WHEN (SELECT COUNT(*) FROM BKG_CNTR_MF_DESC BCM WHERE BCM.BKG_NO = CNTR.BKG_NO AND BCM.CNTR_NO = CNTR.CNTR_NO) = 0 THEN TO_CLOB('{CNTR_CARGO'|| CHR(10) || 'CARGO_DESC:'||CHR(10) ||'}CNTR_CARGO'||CHR(10))
          ELSE
                  BKG_JOIN_CLOB_FNC(CURSOR(SELECT '{CNTR_CARGO'|| CHR(10) || 'CARGO_DESC:'||REPLACE(BKG_SPCLCHAR_CONV_FNC(BCM.CNTR_MF_GDS_DESC,'E'),CHR(10),' ')|| CHR(10)|| '}CNTR_CARGO' 
                                      FROM BKG_CNTR_MF_DESC BCM 
                                      WHERE BCM.BKG_NO = CNTR.BKG_NO AND BCM.CNTR_NO = CNTR.CNTR_NO) , CHR(10))||CHR(10)
     	END 
  	|| '{REMARK'	    || CHR(10) 
  	|| 'REMARK_TP:' || CHR(10) 
  	|| 'REMARK:'    || CHR(10) 
  	|| '}REMARK'       || CHR(10) 
 	||CASE WHEN (SELECT COUNT(*) FROM BKG_HRD_CDG_CTNT BHCC WHERE 1=1
                 AND (BK_AWK_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = SCE_CNTR_TPSZ_CD)
                 OR (BK_BB_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = 'BK')
                 OR (BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 IN (SELECT BSC.STWG_CD FROM BKG_STWG_CGO BSC WHERE BSC.BKG_NO = CNTR.BKG_NO))) = 0
            THEN '{INSTRUCT'||CHR(10)||'SPCL_CD:'||CHR(10)||'SPCL_DESC:'||CHR(10)||'}INSTRUCT'
   		ELSE 
         BKG_JOIN_FNC(CURSOR((SELECT '{INSTRUCT'||CHR(10)||'SPCL_CD:'||BHCC.ATTR_CTNT2||CHR(10)||'SPCL_DESC:'||BHCC.ATTR_CTNT3||CHR(10)||'}INSTRUCT'
         FROM BKG_HRD_CDG_CTNT BHCC 
         WHERE 1=1
         AND (BK_AWK_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = SCE_CNTR_TPSZ_CD)
         OR (BK_BB_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = 'BK')
         OR (BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 IN (SELECT BSC.STWG_CD FROM BKG_STWG_CGO BSC WHERE BSC.BKG_NO = CNTR.BKG_NO))
         )), CHR(10))
   		END||CHR(10) 
	|| '{CNTR_TRO_DTL'														|| CHR(10)
	|| 'TRD_ADDR:'																|| CHR(10)
	|| 'TRD_DOOR_LOC:'														|| CHR(10)
	|| 'TRD_DOOR_POSTAL:'													|| CHR(10)
	|| 'TRD_DOOR_DT:'															|| CHR(10)
	|| 'TRD_PERSON:'															|| CHR(10)
	|| 'TRD_TEL:'																  || CHR(10)
	|| 'TRD_FAX:'																  || CHR(10)
	|| 'TRD_DEPARTURE:'														|| CHR(10)
	|| '}CNTR_TRO_DTL'														|| CHR(10)
  	|| TO_CLOB(BKG_JOIN_CLOB_FNC(CURSOR((SELECT '{CNTR_DANGER'														      || CHR(10)
                            || 'PROPERNAME:'    || BKG_SPCLCHAR_CONV_FNC(DG1.PRP_SHP_NM,'E')				  || CHR(10)
                            || 'TECHNAME:'      || BKG_SPCLCHAR_CONV_FNC(NVL(DG1.HZD_DESC, ' '),'E')	  	  || CHR(10)
                            || 'DG_APPROVAL:'   || CASE WHEN ML.CONTI_CD = 'E' THEN DG.DCGO_REF_NO 
                                                   ELSE
                                                              (SELECT   SA.APRO_REF_NO
                                      							FROM     SCG_APRO_RQST      SAR
                                      							      ,  SCG_VVD_APRO_RQST  SVAR
                                      							      ,  SCG_AUTHORIZATION  SA
                                      							      ,  BKG_VVD BV
                                      							WHERE    1 = 1
                                      							AND      SAR.BKG_NO           = SVAR.BKG_NO
                                      							AND      SAR.BKG_NO           = BV.BKG_NO
                                      							AND      SAR.SPCL_CGO_APRO_RQST_SEQ = SVAR.SPCL_CGO_APRO_RQST_SEQ
                                      							AND      SAR.SPCL_CGO_CATE_CD = 'DG'
                                      							AND      SAR.LST_RQST_DAT_FLG = 'Y'
                                      							AND      SVAR.BKG_NO           = SA.BKG_NO
                                      							AND      SVAR.SPCL_CGO_APRO_RQST_SEQ = SA.SPCL_CGO_APRO_RQST_SEQ
                                      							AND      SVAR.BKG_NO           = DG1.BKG_NO
                                      							AND      SVAR.VSL_CD           = BV.VSL_CD
                                      							AND      SVAR.SKD_VOY_NO       = BV.SKD_VOY_NO
                                      							AND      SVAR.SKD_DIR_CD       = BV.SKD_DIR_CD
                                      							AND      BV.VSL_PRE_PST_CD||BV.VSL_SEQ = (SELECT MIN(BV1.VSL_PRE_PST_CD||BV1.VSL_SEQ) FROM BKG_VVD BV1 WHERE BV1.BKG_NO = BV.BKG_NO)
                                      							AND      SA.SPCL_CGO_AUTH_SEQ= (SELECT   MAX(SAM.SPCL_CGO_AUTH_SEQ)
                                                                  							    FROM     SCG_AUTHORIZATION SAM
                                                                     								WHERE    SAM.BKG_NO         = SA.BKG_NO
                                                                     								AND      SAM.SPCL_CGO_APRO_RQST_SEQ = SA.SPCL_CGO_APRO_RQST_SEQ
                                                                     								)  
                                                                  AND      ROWNUM = 1) 	END	|| CHR(10)
                          	|| 'UNBR:'			|| DG1.IMDG_UN_NO                             || CHR(10)
                            || 'CLAS:'			|| DG1.IMDG_CLSS_CD									          || CHR(10)
                          	|| 'DESC:'			|| BKG_SPCLCHAR_CONV_FNC(DG1.PRP_SHP_NM,'E')	|| CHR(10)
                          	|| 'PHON:'			|| DG1.EMER_CNTC_PHN_NO_CTNT								  || CHR(10)
                            || 'PERSON:'		|| DG1.EMER_CNTC_PSON_NM								  	  || CHR(10)
                            || 'PAGE:'			                         								      || CHR(10)
                          	|| 'FPNT:'			|| DG1.FLSH_PNT_CDO_TEMP									    || CHR(10)
                          	|| 'FPUN:'			|| 'C'												                || CHR(10)
                          	|| 'DG_REMARK:'	|| BKG_SPCLCHAR_CONV_FNC(REPLACE(NVL(DG1.DIFF_RMK, ' '),CHR(10), ' '),'E')     || CHR(10)
                          	|| 'EMSNO:'			|| NVL(DG1.EMS_NO, ' ')		         					  || CHR(10)
                          	|| 'PSACLS:'		|| NVL(DG1.PSA_NO, ' ')								        || CHR(10)
                          	|| 'PKGGRP:'		|| NVL(DG1.IMDG_PCK_GRP_CD, ' ')							|| CHR(10)
                            || 'MFAG1:'			                                       				|| CHR(10)
                            || 'MFAG2:'			            							                    || CHR(10)
                          	|| 'MAR_POLL:'	|| NVL(DG1.MRN_POLUT_FLG,' ')							    || CHR(10)
                          	|| 'LABEL_CD:'	|| NVL((select LBL.IMDG_SUBS_RSK_LBL_CD IMO_SRL
                                                        from SCG_IMDG_SUBS_RSK_LBL LBL 
                                                       where DG1.IMDG_UN_NO      = LBL.IMDG_UN_NO 
                                                             and DG1.IMDG_UN_NO_SEQ = LBL.IMDG_UN_NO_SEQ
                                                             and rownum = 1), ' ') 				|| CHR(10)
                          	|| 'LABEL_DESC:'														                  || CHR(10)
                          	|| 'D_PKG:'			|| NVL(DG1.OUT_IMDG_PCK_QTY1, 0)							|| CHR(10)
                          	|| 'D_PKGUNIT:'	|| NVL(DG1.OUT_IMDG_PCK_CD1, ' ')			  			|| CHR(10)
                          	|| 'NWGT:'			|| NVL(DG1.NET_WGT, 0)								        || CHR(10)
                          	|| 'NWGT_UNIT:KGS'														                || CHR(10)
                          	|| 'GWGT:'			|| NVL(DG1.GRS_WGT, 0)							          || CHR(10)
                          	|| 'GWGT_UNIT:'	|| NVL(DG1.WGT_UT_CD, ' ')		  						  || CHR(10)
                          	|| 'MEA:'			  || NVL(DG1.MEAS_QTY, 0)							          || CHR(10)
                          	|| 'MEA_UNIT:'	|| NVL(DG1.MEAS_UT_CD, ' ')			  					  || CHR(10)
                          	|| 'HAZ_CONT:'	|| NVL(DG1.HZD_DESC, ' ')						  	      || CHR(10)
                          	|| 'STWG:'			|| NVL(DG1.SPCL_STWG_RQST_DESC, ' ')					|| CHR(10)
                            || 'LABEL1:'		|| NVL(DG1.IMDG_SUBS_RSK_LBL_CD1, ' ')				|| CHR(10)
                            || 'LABEL2:'		|| NVL(DG1.IMDG_SUBS_RSK_LBL_CD2, ' ')				|| CHR(10)
                            || 'LABEL3:'		|| NVL(DG1.IMDG_SUBS_RSK_LBL_CD3, ' ')				|| CHR(10)
                          	|| 'IMO_LIMIT:'     || NVL(DG1.IMDG_LMT_QTY_FLG, ' ')					|| CHR(10)
          	|| '}CNTR_DANGER'	||CHR(10)			      										  CNTR_DG_INFO
            FROM BKG_DG_CGO DG1
                 ,BKG_BOOKING BB
                 ,MDM_LOCATION ML
           WHERE DG1.BKG_NO		= CNTR.BKG_NO
             AND DG1.CNTR_TPSZ_CD is not null
             AND DG1.DG_CNTR_SEQ   = DG.DG_CNTR_SEQ
             AND DG1.BKG_NO = BB.BKG_NO
             AND BB.POL_CD = ML.LOC_CD
           --ORDER BY DG1.CNTR_CGO_SEQ
           )), '')
	|| '}CNTR_INFO'			)													|| CHR(10) CNTR_INFO
FROM (
    SELECT SCHC.COP_NO
           ,SCHC.CNTR_NO SCE_CNTR_NO
           ,SCHC.CNTR_TPSZ_CD SCE_CNTR_TPSZ_CD
           ,row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
           ,CASE WHEN CNTR.RC_FLG = 'Y' AND CNTR.CNTR_NO IS NOT NULL THEN CNTR.CNTR_NO
            ELSE SCHC.CNTR_TPSZ_CD||'-'||row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
            END RF_CNTR_NO
           ,CASE WHEN CNTR.DCGO_FLG = 'Y' AND CNTR.CNTR_NO IS NOT NULL THEN CNTR.CNTR_NO
            ELSE SCHC.CNTR_TPSZ_CD||'-'||row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
            END DG_CNTR_NO
           ,CASE WHEN CNTR.AWK_CGO_FLG = 'Y' AND CNTR.CNTR_NO IS NOT NULL THEN CNTR.CNTR_NO
            ELSE SCHC.CNTR_TPSZ_CD||'-'||row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
            END AWK_CNTR_NO
           ,CNTR.CNTR_NO
           ,CNTR.CNTR_TPSZ_CD
           ,CNTR.RC_FLG
           ,CNTR.DCGO_FLG
           ,CNTR.AWK_CGO_FLG
           ,CNTR.BB_CGO_FLG
           ,CNTR.pck_qty
           ,CNTR.pck_tp_cd
           ,CNTR.meas_qty
           ,CNTR.meas_ut_cd
           ,CNTR.CNTR_WGT
           ,CNTR.WGT_UT_CD
           ,CNTR.RD_CGO_FLG
           ,CNTR.RCV_TERM_CD
           ,CNTR.DE_TERM_CD
           ,CNTR.VGM_WGT
           ,BK.BKG_NO
           ,BK.POL_CD
           ,BK.POD_CD
           ,BK.AWK_CGO_FLG BK_AWK_CGO_FLG
           ,BK.BB_CGO_FLG BK_BB_CGO_FLG
          ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN ECOM.REF_ID
            ELSE BXVR.XTER_VGM_DOC_ID END AS DOC_ID
           ,CNTR.VGM_WGT_UT_CD
           ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN DECODE(ECOM.USR_ID,'COD','SM1','322','SM1','304',NVL(ECOM.VGM_DOC_TP_CD,'SM2'),DECODE(ECOM.WGT_TP_CD,'V','SM1','C','SM2'))
            ELSE NVL((SELECT VGM_DOC_TP_CD
                      FROM BKG_XTER_VGM_CUST
                      WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID
                                AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID
                                AND BXVR.CNTR_NO = CNTR_NO
                                AND VGM_DOC_TP_CD IN ('SM1', 'SM2')
                                AND ROWNUM = 1), 'SM1')END AS DOC_TP
           ,CNTR.VGM_WGT_UPD_DT
           ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN ECOM.VGM_CUST_CNTC_TP_CD
            ELSE (SELECT VGM_CUST_CNTC_TP_CD 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  )END AS CUST_CNTC_TP
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN DECODE(ECOM.ESIG_CO_NM, NULL, '', ESIG_CO_NM)
             ELSE (SELECT VGM_CUST_CNTC_NM 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_CNTC_NM
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN BXC.VGM_CUST_FAX_NO
             ELSE (SELECT VGM_CUST_FAX_NO 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_FAX
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN ECOM.CUST_EML
             ELSE (SELECT VGM_CUST_EML 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_EML
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN BXC.VGM_CUST_PHN_NO
             ELSE (SELECT VGM_CUST_PHN_NO 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_PHN
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN BXC.VGM_CUST_ADDR
             ELSE (SELECT VGM_CUST_ADDR 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_ML_ADDR
    FROM BKG_BOOKING BK
         , SCE_COP_HDR SCHC
         , BKG_CONTAINER CNTR
         , BKG_XTER_VGM_RQST BXVR
         , BKG_XTER_VGM ECOM
         , BKG_XTER_CNTR BXC
    WHERE BK.BKG_NO        = @[bkg_no]
    AND   BK.BKG_NO   	   = SCHC.BKG_NO
    AND   SCHC.COP_STS_CD  <> 'X'
    AND   SCHC.BKG_NO      = CNTR.BKG_NO(+)
    AND   SCHC.CNTR_NO     = CNTR.CNTR_NO(+)
    AND   CNTR.CNTR_NO = BXVR.CNTR_NO(+)
    AND   CNTR.XTER_SNDR_ID = BXVR.XTER_SNDR_ID(+)
    AND   CNTR.XTER_VGM_DOC_ID = BXVR.XTER_VGM_DOC_ID(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = BXVR.XTER_VGM_RQST_SEQ(+)
    AND   CNTR.BKG_NO = ECOM.BKG_NO(+)
    AND   CNTR.CNTR_NO = ECOM.CNTR_NO(+)
    AND   CNTR.XTER_VGM_DOC_ID = ECOM.BKG_NO(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = ECOM.VGM_SEQ(+)
	AND   CNTR.XTER_VGM_USR_ID = ECOM.USR_ID(+)
    AND   DECODE(CNTR.XTER_SNDR_ID,'WEB',CNTR.BKG_NO,'') = ECOM.BKG_NO(+)
    AND   CNTR.XTER_SNDR_ID = BXC.XTER_SNDR_ID(+)
    AND   CNTR.XTER_VGM_DOC_ID = BXC.XTER_RQST_NO(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = BXC.XTER_RQST_SEQ(+)
    AND   CNTR.CNTR_NO = BXC.CNTR_NO(+)

    ORDER BY SCHC.COP_NO
    ) CNTR

    ,(SELECT CASE WHEN RF.CNTR_NO IS NOT NULL THEN RF.CNTR_NO
             ELSE RF.CNTR_TPSZ_CD||'-'||row_number() over (partition by RF.CNTR_TPSZ_CD order by RF.CNTR_TPSZ_CD) 
             END RF_CNTR_NO
             ,RF.*
      FROM BKG_RF_CGO RF WHERE RF.BKG_NO = @[bkg_no] AND RF.CNTR_TPSZ_CD is not null
      ORDER BY RF.RC_SEQ) RF
    ,(SELECT CASE WHEN AWK.CNTR_NO IS NOT NULL THEN AWK.CNTR_NO
             ELSE AWK.CNTR_TPSZ_CD||'-'||row_number() over (partition by AWK.CNTR_TPSZ_CD order by AWK.CNTR_TPSZ_CD) 
             END AWK_CNTR_NO
             ,AWK.*
      FROM BKG_AWK_CGO AWK WHERE AWK.BKG_NO = @[bkg_no] AND AWK.CNTR_TPSZ_CD is not null
      ORDER BY AWK.AWK_CGO_SEQ) AK
    ,(SELECT CASE WHEN DG.CNTR_NO IS NOT NULL THEN DG.CNTR_NO
             ELSE DG.CNTR_TPSZ_CD||'-'||row_number() over (partition by DG.CNTR_TPSZ_CD order by DG.CNTR_TPSZ_CD) 
             END DG_CNTR_NO
             ,DG.*
             ,(select sum(nvl(bdg.grs_wgt,0)) from bkg_dg_cgo bdg where bdg.bkg_no = dg.bkg_no and bdg.dg_cntr_seq = dg.dg_cntr_seq) as dg_grs_wgt
             ,(select sum(nvl(bdg.net_wgt,0)) from bkg_dg_cgo bdg where bdg.bkg_no = dg.bkg_no and bdg.dg_cntr_seq = dg.dg_cntr_seq) as dg_net_wgt
      FROM BKG_DG_CGO DG WHERE DG.BKG_NO = @[bkg_no] AND DG.CNTR_TPSZ_CD is not null
                         AND DG.CNTR_CGO_SEQ = (SELECT MIN(DG1.CNTR_CGO_SEQ) FROM BKG_DG_CGO DG1 WHERE DG1.BKG_NO = DG.BKG_NO AND DG1.DG_CNTR_SEQ = DG.DG_CNTR_SEQ)
      ORDER BY DG.CNTR_CGO_SEQ) DG
    , BKG_CNTR_SEAL_NO seal
    , MDM_LOCATION POL
    , MDM_LOCATION POD
WHERE CNTR.RF_CNTR_NO = RF.RF_CNTR_NO(+)
AND   CNTR.BKG_NO = RF.BKG_NO(+)
AND   CNTR.DG_CNTR_NO = DG.DG_CNTR_NO(+)
AND   CNTR.BKG_NO = DG.BKG_NO(+)
AND   CNTR.AWK_CNTR_NO = AK.AWK_CNTR_NO(+)
AND   CNTR.BKG_NO = AK.BKG_NO(+)
AND   CNTR.BKG_NO      = SEAL.BKG_NO(+)
AND   CNTR.CNTR_NO     = seal.CNTR_NO      (+)
AND   1                = seal.CNTR_SEAL_SEQ(+)
AND   CNTR.POL_CD = POL.LOC_CD
AND   CNTR.POD_CD = POD.LOC_CD
UNION ALL
SELECT 
 '{CNTR_INFO'																    || CHR(10)
	|| 'CNTN:'					|| RPAD(NVL(CNTR.CNTR_NO, ' '), 11, ' ')		|| CHR(10)
    || 'CSN:'                   || bkg_get_slot_no_fnc(CNTR.COP_NO)             || CHR(10)
	|| 'CNTT:'					|| NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD)	|| CHR(10)
	|| 'MRN:'			  		|| (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CMRN' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1)	|| CHR(10)
    || 'UCR:'				  || CASE WHEN (SELECT SUM(BQ.OP_CNTR_QTY) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) = 1 THEN
                            NVL((SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1),
                            (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND ROWNUM = 1))
                              ELSE
                            (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1)
                         END
                         	|| CHR(10)
    || 'HS_CD:'					|| (SELECT BCMD.CMDT_HS_CD FROM BKG_CNTR_MF_DESC BCMD WHERE BCMD.BKG_NO = CNTR.BKG_NO AND BCMD.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1) || CHR(10)
	|| 'RIND:'          || DECODE(RF.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10)
	|| 'DIND:'          || DECODE(DG.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10)
	|| 'AIND:'          || DECODE(AK.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10) 
	|| 'BIND:'          || decode(CNTR.BB_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'WGT_QTY:'       || CASE WHEN NVL(CNTR.CNTR_WGT,0) <> 0 THEN CNTR.CNTR_WGT + NVL((SELECT NVL(MCS.TARE_WGT,0)
                                                                                    FROM MST_CONTAINER MC
                                                                                         ,MST_CNTR_SPEC MCS
                                                                                    WHERE MC.CNTR_SPEC_NO = MCS.CNTR_SPEC_NO
                                                                                    AND MC.CNTR_NO = CNTR.CNTR_NO
                                                                                    AND ROWNUM = 1)
																		,(SELECT NVL(MCTS.CNTR_TPSZ_TARE_WGT,0) FROM MDM_CNTR_TP_SZ MCTS WHERE MCTS.CNTR_TPSZ_CD = NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD)))
                                WHEN ((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0)) = 0 THEN 0
                                ELSE ROUND((SELECT (BLD.ACT_WGT-NVL((SELECT SUM(NVL(BCC.CNTR_WGT,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO),0))
                                     /((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = BLD.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0))
                                     FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO),3)
                                     + (SELECT NVL(MCTS.CNTR_TPSZ_TARE_WGT,0) FROM MDM_CNTR_TP_SZ MCTS WHERE MCTS.CNTR_TPSZ_CD = NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD) AND ROWNUM = 1)
                           END 					                || CHR(10)
	|| 'WGT_TP:'        || CASE WHEN Cntr.WGT_UT_CD IS NULL THEN (SELECT BLD.WGT_UT_CD FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO)
                                        ELSE Cntr.WGT_UT_CD END 					|| CHR(10)
    || 'NWGT_QTY:'      || CASE WHEN NVL(CNTR.CNTR_WGT,0) <> 0 THEN CNTR.CNTR_WGT
                              WHEN ((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0)) = 0 THEN 0
                              ELSE ROUND((SELECT (BLD.ACT_WGT-NVL((SELECT SUM(NVL(BCC.CNTR_WGT,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO),0))
                                     /((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = BLD.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0))
                                     FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO),3)
                         END 					|| CHR(10)
	|| 'NWGT_TP:'       || CASE WHEN Cntr.WGT_UT_CD IS NULL THEN (SELECT BLD.WGT_UT_CD FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO)
                         ELSE Cntr.WGT_UT_CD END 					                              || CHR(10)
	|| 'TEMP:'					|| RF.FDO_TEMP									                                  || CHR(10)
	|| 'TUN:'					  || 'F'											                                      || CHR(10)
	|| 'TEMP_C:'				|| RF.CDO_TEMP									                                  || CHR(10)
	|| 'TUN_C:'					|| 'C'											                                      || CHR(10)
  	|| 'PRECOOLING:'															                                        || CHR(10)
	|| 'RF_VOLTAGE:'		|| RF.VLTG_NO									                                    || CHR(10)
	|| 'VENT:'					|| CASE WHEN NVL(RF.VENT_RTO,0) = 0 THEN 'C'
										WHEN NVL(RF.VENT_RTO,0) > 0 AND NVL(RF.VENT_RTO,0) < 35 THEN 'Q'
										WHEN NVL(RF.VENT_RTO,0) >= 35 AND NVL(RF.VENT_RTO,0) < 65 THEN 'H'
										WHEN NVL(RF.VENT_RTO,0) >= 65 AND NVL(RF.VENT_RTO,0) < 100 THEN 'T'
										WHEN NVL(RF.VENT_RTO,0) = 100 THEN 'O' END		                      || CHR(10)
  	|| 'VENT_NUM:'			|| RF.VENT_RTO						        	                              || CHR(10)
	|| 'VENT_CMH:'			|| RF.CBM_PER_HR_QTY							                                || CHR(10)
	|| 'HUMID:'				|| RF.HUMID_NO									                                  || CHR(10)
	|| 'CAO2:'				|| RF.OXGN_RTO									                                  || CHR(10)
	|| 'CACO2:'				|| RF.CRBN_DXD_RTO								                                  || CHR(10)
	|| 'GENSET:'			  || RF.PWR_SPL_CBL_FLG                                             || CHR(10)
  	|| 'GENSET_CD:'																                                        || CHR(10)
  	|| 'GENSET_DESC:'															                                        || CHR(10)
	|| 'RF_REMARK:'			|| BKG_SPCLCHAR_CONV_FNC(REPLACE(RF.DIFF_RMK, chr(10), ' '),'E')	|| CHR(10)
	|| 'RFDRY_IND:'			|| DECODE(CNTR.RD_CGO_FLG,'N','0','Y','1','0')        || CHR(10)
	|| 'RF_DRAIN:'			|| decode(RF.CNTR_DRN_CD,'C','OFF','O','ON','N','N/A','')		      || CHR(10)
  	|| 'AK_UNIT:'				|| 'CM'											                                      || CHR(10)
	|| 'OVF:'			   		|| AK.OVR_FWRD_LEN								                                || CHR(10)
	|| 'OVR:'				  	|| AK.OVR_BKWD_LEN								                                || CHR(10)
	|| 'OVH:'					  || AK.OVR_HGT									                                    || CHR(10)
	|| 'OVLW:'					|| AK.OVR_LF_LEN								                                  || CHR(10)
	|| 'OVRW:'					|| AK.OVR_RT_LEN								                                  || CHR(10)
	|| 'OVWGT:'					|| ROUND(NVL(AK.GRS_WGT, 0) * DECODE(AK.WGT_UT_CD, 'LBS', 0.4536, 1), 3) || CHR(10)
	|| 'VOID_SLOT:'			|| AK.OVR_VOID_SLT_QTY							                              || CHR(10)
	|| 'STWG_REQ:'			|| REPLACE(AK.STWG_RQST_DESC, CHR(10), ' ')	                      || CHR(10)
  	|| 'MEMO:'					|| BKG_SPCLCHAR_CONV_FNC(REPLACE(AK.DIFF_RMK, CHR(10), ' '),'E')  || CHR(10)
	|| 'TTL_DIM_LEN:'		|| AK.TTL_DIM_LEN								                                  || CHR(10)
	|| 'TTL_DIM_WDT:'		|| AK.TTL_DIM_WDT								                                  || CHR(10)
	|| 'TTL_DIM_HGT:'		|| AK.TTL_DIM_HGT								                                  || CHR(10)
	|| 'TRM_TYPE:'																|| CHR(10)
	|| 'TRM_WEIGHT:'															|| CHR(10)
	|| 'TRM_HAULAGE:'															|| CHR(10)
	|| 'TRM_HMODE:'																|| CHR(10)
	|| 'TRM_PICKUP_CY:'														|| CHR(10)
	|| 'TRM_RETURN_CY:'														|| CHR(10)
	|| 'TRM_INSTRUCTION:'													|| CHR(10)
	|| 'TRM_TRAN_DT:'															|| CHR(10)
	|| 'TRM_TRAN_OFFICE:'													|| CHR(10)
	|| 'TRM_TRAN_NO:'															|| CHR(10)
	|| 'USR_ID:'																  || CHR(10)

	|| '{VGM'																  || CHR(10)
	|| 'DOC_ID:'		|| CNTR.DOC_ID									  	  || CHR(10)
	|| 'MEAS:'			|| CNTR.VGM_WGT										  || CHR(10)
	|| 'MEAS_UT:'		|| substr(CNTR.VGM_WGT_UT_CD,1,1)				  	  || CHR(10)
	|| 'DOC_TP:'		|| CNTR.DOC_TP										  || CHR(10)
	|| 'DATE_TP:'		|| DECODE(CNTR.VGM_WGT_UPD_DT,NULL,'','WAT')		  || CHR(10)
	|| 'DATE:'			|| TO_CHAR(CNTR.VGM_WGT_UPD_DT,'YYYYMMDDHH24MI')	  || CHR(10)
	|| 'CUST_CNTC_TP:'	|| CNTR.CUST_CNTC_TP								  || CHR(10)
	|| 'CUST_CNTC_NM:'	|| CNTR.CUST_CNTC_NM								  || CHR(10)
	|| 'CUST_FAX:'		|| CNTR.CUST_FAX								  	  || CHR(10)
	|| 'CUST_EML:'		|| CNTR.CUST_EML									  || CHR(10)
	|| 'CUST_PHN:'		|| CNTR.CUST_PHN									  || CHR(10)
	|| 'CUST_ML_ADDR:'	|| CNTR.CUST_ML_ADDR								  || CHR(10)
	|| '}VGM'                                                                 || CHR(10)

  	|| CASE WHEN (SELECT COUNT(*) FROM BKG_CNTR_SEAL_NO BCS WHERE CNTR.BKG_NO = BCS.BKG_NO AND CNTR.CNTR_NO = BCS.CNTR_NO) = 0 THEN '{CNTR_SEAL_NO'|| CHR(10) || 'SEAL_NO:'||CHR(10) ||'}CNTR_SEAL_NO'||CHR(10)
          ELSE
                 BKG_JOIN_FNC(CURSOR(SELECT '{CNTR_SEAL_NO'|| CHR(10) || 'SEAL_NO:'||BKG_SPCLCHAR_CONV_FNC(BCS.CNTR_SEAL_NO,'E')|| CHR(10)|| '}CNTR_SEAL_NO' 
                                     FROM BKG_CNTR_SEAL_NO BCS 
                                     WHERE CNTR.BKG_NO = BCS.BKG_NO AND CNTR.CNTR_NO = BCS.CNTR_NO) , CHR(10))||CHR(10)
     	END 
  	|| CASE WHEN (SELECT COUNT(*) FROM BKG_CNTR_MF_DESC BCM WHERE BCM.BKG_NO = CNTR.BKG_NO AND BCM.CNTR_NO = CNTR.CNTR_NO) = 0 THEN TO_CLOB('{CNTR_CARGO'|| CHR(10) || 'CARGO_DESC:'||CHR(10) ||'}CNTR_CARGO'||CHR(10))
          ELSE
                  BKG_JOIN_CLOB_FNC(CURSOR(SELECT '{CNTR_CARGO'|| CHR(10) || 'CARGO_DESC:'||REPLACE(BKG_SPCLCHAR_CONV_FNC(BCM.CNTR_MF_GDS_DESC,'E'),CHR(10),' ')|| CHR(10)|| '}CNTR_CARGO' 
                                      FROM BKG_CNTR_MF_DESC BCM 
                                      WHERE BCM.BKG_NO = CNTR.BKG_NO AND BCM.CNTR_NO = CNTR.CNTR_NO) , CHR(10))||CHR(10)
     	END 
  	|| '{REMARK'	    || CHR(10) 
  	|| 'REMARK_TP:' || CHR(10) 
  	|| 'REMARK:'    || CHR(10) 
  	|| '}REMARK'       || CHR(10) 
 	||CASE WHEN (SELECT COUNT(*) FROM BKG_HRD_CDG_CTNT BHCC WHERE 1=1
                 AND (BK_AWK_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = SCE_CNTR_TPSZ_CD)
                 OR (BK_BB_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = 'BK')
                 OR (BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 IN (SELECT BSC.STWG_CD FROM BKG_STWG_CGO BSC WHERE BSC.BKG_NO = CNTR.BKG_NO))) = 0
            THEN '{INSTRUCT'||CHR(10)||'SPCL_CD:'||CHR(10)||'SPCL_DESC:'||CHR(10)||'}INSTRUCT'
  		 ELSE 
         BKG_JOIN_FNC(CURSOR((SELECT '{INSTRUCT'||CHR(10)||'SPCL_CD:'||BHCC.ATTR_CTNT2||CHR(10)||'SPCL_DESC:'||BHCC.ATTR_CTNT3||CHR(10)||'}INSTRUCT'
         FROM BKG_HRD_CDG_CTNT BHCC 
         WHERE 1=1
         AND (BK_AWK_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = SCE_CNTR_TPSZ_CD)
         OR (BK_BB_CGO_FLG = 'Y' AND BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 = 'BK')
         OR (BHCC.HRD_CDG_ID = 'VNDR301_INSTRUCT' AND BHCC.ATTR_CTNT1 IN (SELECT BSC.STWG_CD FROM BKG_STWG_CGO BSC WHERE BSC.BKG_NO = CNTR.BKG_NO))
         )), CHR(10))
   		END||CHR(10) 
	|| '{CNTR_TRO_DTL'														|| CHR(10)
	|| 'TRD_ADDR:'																|| CHR(10)
	|| 'TRD_DOOR_LOC:'														|| CHR(10)
	|| 'TRD_DOOR_POSTAL:'													|| CHR(10)
	|| 'TRD_DOOR_DT:'															|| CHR(10)
	|| 'TRD_PERSON:'															|| CHR(10)
	|| 'TRD_TEL:'																  || CHR(10)
	|| 'TRD_FAX:'																  || CHR(10)
	|| 'TRD_DEPARTURE:'														|| CHR(10)
	|| '}CNTR_TRO_DTL'														|| CHR(10)
  	|| TO_CLOB(BKG_JOIN_CLOB_FNC(CURSOR((SELECT '{CNTR_DANGER'														      || CHR(10)
                            || 'PROPERNAME:'    || BKG_SPCLCHAR_CONV_FNC(DG1.PRP_SHP_NM,'E')				  || CHR(10)
                            || 'TECHNAME:'      || BKG_SPCLCHAR_CONV_FNC(NVL(DG1.HZD_DESC, ' '),'E')	  	  || CHR(10)
                            || 'DG_APPROVAL:'   || CASE WHEN ML.CONTI_CD = 'E' THEN DG.DCGO_REF_NO 
                                                   ELSE
                                                             (SELECT   SA.APRO_REF_NO
                                      							FROM     SCG_APRO_RQST      SAR
                                      							      ,  SCG_VVD_APRO_RQST  SVAR
                                      							      ,  SCG_AUTHORIZATION  SA
                                      							      ,  BKG_VVD BV
                                      							WHERE    1 = 1
                                      							AND      SAR.BKG_NO           = SVAR.BKG_NO
                                      							AND      SAR.BKG_NO           = BV.BKG_NO
                                      							AND      SAR.SPCL_CGO_APRO_RQST_SEQ = SVAR.SPCL_CGO_APRO_RQST_SEQ
                                      							AND      SAR.SPCL_CGO_CATE_CD = 'DG'
                                      							AND      SAR.LST_RQST_DAT_FLG = 'Y'
                                      							AND      SVAR.BKG_NO           = SA.BKG_NO
                                      							AND      SVAR.SPCL_CGO_APRO_RQST_SEQ = SA.SPCL_CGO_APRO_RQST_SEQ
                                      							AND      SVAR.BKG_NO           = DG1.BKG_NO
                                      							AND      SVAR.VSL_CD           = BV.VSL_CD
                                      							AND      SVAR.SKD_VOY_NO       = BV.SKD_VOY_NO
                                      							AND      SVAR.SKD_DIR_CD       = BV.SKD_DIR_CD
                                      							AND      BV.VSL_PRE_PST_CD||BV.VSL_SEQ = (SELECT MIN(BV1.VSL_PRE_PST_CD||BV1.VSL_SEQ) FROM BKG_VVD BV1 WHERE BV1.BKG_NO = BV.BKG_NO)
                                      							AND      SA.SPCL_CGO_AUTH_SEQ= (SELECT   MAX(SAM.SPCL_CGO_AUTH_SEQ)
                                                                  							    FROM     SCG_AUTHORIZATION SAM
                                                                     								WHERE    SAM.BKG_NO         = SA.BKG_NO
                                                                     								AND      SAM.SPCL_CGO_APRO_RQST_SEQ = SA.SPCL_CGO_APRO_RQST_SEQ
                                                                     								)  
                                                                  AND      ROWNUM = 1) 	END	|| CHR(10)
                          	|| 'UNBR:'			|| DG1.IMDG_UN_NO                             || CHR(10)
                            || 'CLAS:'			|| DG1.IMDG_CLSS_CD									          || CHR(10)
                          	|| 'DESC:'			|| BKG_SPCLCHAR_CONV_FNC(DG1.PRP_SHP_NM,'E')	|| CHR(10)
                          	|| 'PHON:'			|| DG1.EMER_CNTC_PHN_NO_CTNT								  || CHR(10)
                            || 'PERSON:'		|| DG1.EMER_CNTC_PSON_NM								  	  || CHR(10)
                            || 'PAGE:'			                         								      || CHR(10)
                          	|| 'FPNT:'			|| DG1.FLSH_PNT_CDO_TEMP									    || CHR(10)
                          	|| 'FPUN:'			|| 'C'												                || CHR(10)
                          	|| 'DG_REMARK:'	|| BKG_SPCLCHAR_CONV_FNC(REPLACE(NVL(DG1.DIFF_RMK, ' '),CHR(10), ' '),'E')     || CHR(10)
                          	|| 'EMSNO:'			|| NVL(DG1.EMS_NO, ' ')		         					  || CHR(10)
                          	|| 'PSACLS:'		|| NVL(DG1.PSA_NO, ' ')								        || CHR(10)
                          	|| 'PKGGRP:'		|| NVL(DG1.IMDG_PCK_GRP_CD, ' ')							|| CHR(10)
                            || 'MFAG1:'			                                       				|| CHR(10)
                            || 'MFAG2:'			            							                    || CHR(10)
                          	|| 'MAR_POLL:'	|| NVL(DG1.MRN_POLUT_FLG,' ')							    || CHR(10)
                          	|| 'LABEL_CD:'	|| NVL((select LBL.IMDG_SUBS_RSK_LBL_CD IMO_SRL
                                                        from SCG_IMDG_SUBS_RSK_LBL LBL 
                                                       where DG1.IMDG_UN_NO      = LBL.IMDG_UN_NO 
                                                             and DG1.IMDG_UN_NO_SEQ = LBL.IMDG_UN_NO_SEQ
                                                             and rownum = 1), ' ') 				|| CHR(10)
                          	|| 'LABEL_DESC:'														                  || CHR(10)
                          	|| 'D_PKG:'			|| NVL(DG1.OUT_IMDG_PCK_QTY1, 0)							|| CHR(10)
                          	|| 'D_PKGUNIT:'	|| NVL(DG1.OUT_IMDG_PCK_CD1, ' ')			  			|| CHR(10)
                          	|| 'NWGT:'			|| NVL(DG1.NET_WGT, 0)								        || CHR(10)
                          	|| 'NWGT_UNIT:KGS'														                || CHR(10)
                          	|| 'GWGT:'			|| NVL(DG1.GRS_WGT, 0)							          || CHR(10)
                          	|| 'GWGT_UNIT:'	|| NVL(DG1.WGT_UT_CD, ' ')		  						  || CHR(10)
                          	|| 'MEA:'			  || NVL(DG1.MEAS_QTY, 0)							          || CHR(10)
                          	|| 'MEA_UNIT:'	|| NVL(DG1.MEAS_UT_CD, ' ')			  					  || CHR(10)
                          	|| 'HAZ_CONT:'	|| NVL(DG1.HZD_DESC, ' ')						  	      || CHR(10)
                          	|| 'STWG:'			|| NVL(DG1.SPCL_STWG_RQST_DESC, ' ')					|| CHR(10)
                            || 'LABEL1:'		|| NVL(DG1.IMDG_SUBS_RSK_LBL_CD1, ' ')				|| CHR(10)
                            || 'LABEL2:'		|| NVL(DG1.IMDG_SUBS_RSK_LBL_CD2, ' ')				|| CHR(10)
                            || 'LABEL3:'		|| NVL(DG1.IMDG_SUBS_RSK_LBL_CD3, ' ')				|| CHR(10)
                          	|| 'IMO_LIMIT:'     || NVL(DG1.IMDG_LMT_QTY_FLG, ' ')					|| CHR(10)
          	|| '}CNTR_DANGER'	||CHR(10)			      										  CNTR_DG_INFO
            FROM BKG_DG_CGO DG1
                 ,BKG_BOOKING BB
                 ,MDM_LOCATION ML
           WHERE DG1.BKG_NO		= CNTR.BKG_NO
             AND DG1.CNTR_TPSZ_CD is not null
             AND DG1.DG_CNTR_SEQ   = DG.DG_CNTR_SEQ
             AND DG1.BKG_NO = BB.BKG_NO
             AND BB.POL_CD = ML.LOC_CD
           --ORDER BY DG1.CNTR_CGO_SEQ
           )), '')
	|| '}CNTR_INFO'						)										|| CHR(10) CNTR_INFO
FROM (
    SELECT SCHC.COP_NO
           ,SCHC.CNTR_NO SCE_CNTR_NO
           ,SCHC.CNTR_TPSZ_CD SCE_CNTR_TPSZ_CD
           ,row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
           ,CASE WHEN CNTR.RC_FLG = 'Y' AND CNTR.CNTR_NO IS NOT NULL THEN CNTR.CNTR_NO
            ELSE SCHC.CNTR_TPSZ_CD||'-'||row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
            END RF_CNTR_NO
           ,CASE WHEN CNTR.DCGO_FLG = 'Y' AND CNTR.CNTR_NO IS NOT NULL THEN CNTR.CNTR_NO
            ELSE SCHC.CNTR_TPSZ_CD||'-'||row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
            END DG_CNTR_NO
           ,CASE WHEN CNTR.AWK_CGO_FLG = 'Y' AND CNTR.CNTR_NO IS NOT NULL THEN CNTR.CNTR_NO
            ELSE SCHC.CNTR_TPSZ_CD||'-'||row_number() over (partition by SCHC.CNTR_TPSZ_CD order by SCHC.CNTR_TPSZ_CD)
            END AWK_CNTR_NO
           ,CNTR.CNTR_NO
           ,CNTR.CNTR_TPSZ_CD
           ,CNTR.RC_FLG
           ,CNTR.DCGO_FLG
           ,CNTR.AWK_CGO_FLG
           ,CNTR.BB_CGO_FLG
           ,CNTR.pck_qty
           ,CNTR.pck_tp_cd
           ,CNTR.meas_qty
           ,CNTR.meas_ut_cd
           ,CNTR.CNTR_WGT
           ,CNTR.WGT_UT_CD
           ,CNTR.RD_CGO_FLG
           ,CNTR.RCV_TERM_CD
           ,CNTR.DE_TERM_CD
           ,CNTR.VGM_WGT
           ,BK.BKG_NO
           ,BK.POL_CD
           ,BK.POD_CD
           ,BK.AWK_CGO_FLG BK_AWK_CGO_FLG
           ,BK.BB_CGO_FLG BK_BB_CGO_FLG
          ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN ECOM.REF_ID
            ELSE BXVR.XTER_VGM_DOC_ID END AS DOC_ID
           ,CNTR.VGM_WGT_UT_CD
           ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN DECODE(ECOM.USR_ID,'COD','SM1','322','SM1','304',NVL(ECOM.VGM_DOC_TP_CD,'SM2'),DECODE(ECOM.WGT_TP_CD,'V','SM1','C','SM2'))
            ELSE NVL((SELECT VGM_DOC_TP_CD
                      FROM BKG_XTER_VGM_CUST
                      WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID
                                AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID
                                AND BXVR.CNTR_NO = CNTR_NO
                                AND VGM_DOC_TP_CD IN ('SM1', 'SM2')
                                AND ROWNUM = 1), 'SM1')END AS DOC_TP
           ,CNTR.VGM_WGT_UPD_DT
           ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN ECOM.VGM_CUST_CNTC_TP_CD
            ELSE (SELECT VGM_CUST_CNTC_TP_CD 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  )END AS CUST_CNTC_TP
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN DECODE(ECOM.ESIG_CO_NM, NULL, '', ESIG_CO_NM)
             ELSE (SELECT VGM_CUST_CNTC_NM 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_CNTC_NM
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN BXC.VGM_CUST_FAX_NO
             ELSE (SELECT VGM_CUST_FAX_NO 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_FAX
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN ECOM.CUST_EML
             ELSE (SELECT VGM_CUST_EML 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_EML
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN BXC.VGM_CUST_PHN_NO
             ELSE (SELECT VGM_CUST_PHN_NO 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_PHN
            ,CASE WHEN CNTR.XTER_SNDR_ID = 'WEB' THEN BXC.VGM_CUST_ADDR
             ELSE (SELECT VGM_CUST_ADDR 
                  FROM BKG_XTER_VGM_CUST 
                  WHERE BXVR.XTER_SNDR_ID = XTER_SNDR_ID 
                  AND BXVR.XTER_VGM_DOC_ID = XTER_VGM_DOC_ID 
                  AND BXVR.XTER_VGM_RQST_SEQ = XTER_VGM_RQST_SEQ
                  AND BXVR.CNTR_NO = CNTR_NO
                  AND VGM_CUST_CNTC_TP_CD = 'RP'
                  AND ROWNUM = 1
                  ) END AS CUST_ML_ADDR
    FROM BKG_BOOKING BK
         , SCE_COP_HIS SCHC
         , BKG_CONTAINER CNTR
         , BKG_XTER_VGM_RQST BXVR
         , BKG_XTER_VGM ECOM
         , BKG_XTER_CNTR BXC
    WHERE BK.BKG_NO        = @[bkg_no]
    AND   BK.BKG_NO   	   = SCHC.BKG_NO
    AND   SCHC.COP_STS_CD = 'X'
    AND   BK.BKG_STS_CD = 'X'
    AND   NOT EXISTS (SELECT 'X' FROM SCE_COP_HDR HDR WHERE HDR.BKG_NO = SCHC.BKG_NO AND HDR.COP_STS_CD <> 'X')
    AND   SCHC.ACCL_SEQ = (SELECT MAX(HIS.ACCL_SEQ) FROM SCE_COP_HIS HIS WHERE HIS.COP_NO = SCHC.COP_NO AND HIS.COP_STS_CD = 'X')
    AND   SCHC.BKG_NO      = CNTR.BKG_NO(+)
    AND   SCHC.CNTR_NO     = CNTR.CNTR_NO(+)
    AND   CNTR.CNTR_NO = BXVR.CNTR_NO(+)
    AND   CNTR.XTER_SNDR_ID = BXVR.XTER_SNDR_ID(+)
    AND   CNTR.XTER_VGM_DOC_ID = BXVR.XTER_VGM_DOC_ID(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = BXVR.XTER_VGM_RQST_SEQ(+)
    AND   CNTR.BKG_NO = ECOM.BKG_NO(+)
    AND   CNTR.CNTR_NO = ECOM.CNTR_NO(+)
    AND   CNTR.XTER_VGM_DOC_ID = ECOM.BKG_NO(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = ECOM.VGM_SEQ(+)
	AND   CNTR.XTER_VGM_USR_ID = ECOM.USR_ID(+)
    AND   DECODE(CNTR.XTER_SNDR_ID,'WEB',CNTR.BKG_NO,'') = ECOM.BKG_NO(+)
    AND   CNTR.XTER_SNDR_ID = BXC.XTER_SNDR_ID(+)
    AND   CNTR.XTER_VGM_DOC_ID = BXC.XTER_RQST_NO(+)
    AND   CNTR.XTER_VGM_RQST_SEQ = BXC.XTER_RQST_SEQ(+)
    AND   CNTR.CNTR_NO = BXC.CNTR_NO(+)
    ORDER BY SCHC.COP_NO
    ) CNTR

    ,(SELECT CASE WHEN RF.CNTR_NO IS NOT NULL THEN RF.CNTR_NO
             ELSE RF.CNTR_TPSZ_CD||'-'||row_number() over (partition by RF.CNTR_TPSZ_CD order by RF.CNTR_TPSZ_CD) 
             END RF_CNTR_NO
             ,RF.*
      FROM BKG_RF_CGO RF WHERE RF.BKG_NO = @[bkg_no] AND RF.CNTR_TPSZ_CD is not null
      ORDER BY RF.RC_SEQ) RF
    ,(SELECT CASE WHEN AWK.CNTR_NO IS NOT NULL THEN AWK.CNTR_NO
             ELSE AWK.CNTR_TPSZ_CD||'-'||row_number() over (partition by AWK.CNTR_TPSZ_CD order by AWK.CNTR_TPSZ_CD) 
             END AWK_CNTR_NO
             ,AWK.*
      FROM BKG_AWK_CGO AWK WHERE AWK.BKG_NO = @[bkg_no] AND AWK.CNTR_TPSZ_CD is not null
      ORDER BY AWK.AWK_CGO_SEQ) AK
    ,(SELECT CASE WHEN DG.CNTR_NO IS NOT NULL THEN DG.CNTR_NO
             ELSE DG.CNTR_TPSZ_CD||'-'||row_number() over (partition by DG.CNTR_TPSZ_CD order by DG.CNTR_TPSZ_CD) 
             END DG_CNTR_NO
             ,DG.*
             ,(select sum(nvl(bdg.grs_wgt,0)) from bkg_dg_cgo bdg where bdg.bkg_no = dg.bkg_no and bdg.dg_cntr_seq = dg.dg_cntr_seq) as dg_grs_wgt
             ,(select sum(nvl(bdg.net_wgt,0)) from bkg_dg_cgo bdg where bdg.bkg_no = dg.bkg_no and bdg.dg_cntr_seq = dg.dg_cntr_seq) as dg_net_wgt
      FROM BKG_DG_CGO DG WHERE DG.BKG_NO = @[bkg_no] AND DG.CNTR_TPSZ_CD is not null
                         AND DG.CNTR_CGO_SEQ = (SELECT MIN(DG1.CNTR_CGO_SEQ) FROM BKG_DG_CGO DG1 WHERE DG1.BKG_NO = DG.BKG_NO AND DG1.DG_CNTR_SEQ = DG.DG_CNTR_SEQ)
      ORDER BY DG.CNTR_CGO_SEQ) DG
    , BKG_CNTR_SEAL_NO seal
    , MDM_LOCATION POL
    , MDM_LOCATION POD
WHERE CNTR.RF_CNTR_NO = RF.RF_CNTR_NO(+)
AND   CNTR.BKG_NO = RF.BKG_NO(+)
AND   CNTR.DG_CNTR_NO = DG.DG_CNTR_NO(+)
AND   CNTR.BKG_NO = DG.BKG_NO(+)
AND   CNTR.AWK_CNTR_NO = AK.AWK_CNTR_NO(+)
AND   CNTR.BKG_NO = AK.BKG_NO(+)
AND   CNTR.BKG_NO      = SEAL.BKG_NO(+)
AND   CNTR.CNTR_NO     = seal.CNTR_NO      (+)
AND   1                = seal.CNTR_SEAL_SEQ(+)
AND   CNTR.POL_CD = POL.LOC_CD
AND   CNTR.POD_CD = POD.LOC_CD
--combine
UNION ALL
SELECT 
 '{CNTR_INFO'																    || CHR(10)
	|| 'CNTN:'					|| RPAD(NVL(CNTR.CNTR_NO, ' '), 11, ' ')		|| CHR(10)
    || 'CSN:'                   || bkg_get_slot_no_fnc(CNTR.COP_NO)             || CHR(10)
	|| 'CNTT:'					|| NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD)	|| CHR(10)
	|| 'MRN:'			  		|| (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CMRN' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1)	|| CHR(10)
    || 'UCR:'				  || CASE WHEN (SELECT SUM(BQ.OP_CNTR_QTY) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) = 1 THEN
                            NVL((SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1),
                            (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND ROWNUM = 1))
                              ELSE
                            (SELECT BR.CUST_REF_NO_CTNT FROM BKG_REFERENCE BR WHERE BR.BKG_REF_TP_CD = 'CUCR' AND BR.BKG_NO = CNTR.BKG_NO AND BR.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1)
                         END
                         	|| CHR(10)
    || 'HS_CD:'					|| (SELECT BCMD.CMDT_HS_CD FROM BKG_CNTR_MF_DESC BCMD WHERE BCMD.BKG_NO = CNTR.BKG_NO AND BCMD.CNTR_NO = CNTR.CNTR_NO AND ROWNUM = 1) || CHR(10)
	|| 'RIND:'          || DECODE(RF.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10)
	|| 'DIND:'          || DECODE(DG.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10)
	|| 'AIND:'          || DECODE(AK.CNTR_TPSZ_CD,NULL,'0','1')			|| CHR(10) 
	|| 'BIND:'          || decode(CNTR.BB_CGO_FLG,'Y','1','0')			|| CHR(10)
		|| 'WGT_QTY:'       || CASE WHEN NVL(CNTR.CNTR_WGT,0) <> 0 THEN CNTR.CNTR_WGT + NVL((SELECT NVL(MCS.TARE_WGT,0)
                                                                                    FROM MST_CONTAINER MC
                                                                                         ,MST_CNTR_SPEC MCS
                                                                                    WHERE MC.CNTR_SPEC_NO = MCS.CNTR_SPEC_NO
                                                                                    AND MC.CNTR_NO = CNTR.CNTR_NO
                                                                                    AND ROWNUM = 1)
																		,(SELECT NVL(MCTS.CNTR_TPSZ_TARE_WGT,0) FROM MDM_CNTR_TP_SZ MCTS WHERE MCTS.CNTR_TPSZ_CD = NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD)))
                                WHEN ((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = CNTR.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0)) = 0 THEN 0
                                ELSE ROUND((SELECT (BLD.ACT_WGT-NVL((SELECT SUM(NVL(BCC.CNTR_WGT,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO),0))
                                     /((SELECT SUM(NVL(BQ.OP_CNTR_QTY,0)) FROM BKG_QUANTITY BQ WHERE BQ.BKG_NO = BLD.BKG_NO) - NVL((SELECT SUM(NVL(BCC.CNTR_VOL_QTY,0)) FROM BKG_CONTAINER BCC WHERE BCC.BKG_NO = CNTR.BKG_NO AND NVL(BCC.CNTR_WGT,0) <> 0),0))
                                     FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO),3)
                                     + (SELECT NVL(MCTS.CNTR_TPSZ_TARE_WGT,0) FROM MDM_CNTR_TP_SZ MCTS WHERE MCTS.CNTR_TPSZ_CD = NVL(CNTR.CNTR_TPSZ_CD,CNTR.SCE_CNTR_TPSZ_CD) AND ROWNUM = 1)
                           END 					                || CHR(10)
	|| 'WGT_TP:'        || CASE WHEN Cntr.WGT_UT_CD IS NULL THEN (SELECT BLD.WGT_UT_CD FROM BKG_BL_DOC BLD WHERE BLD.BKG_NO = CNTR.BKG_NO)
                                        ELSE Cntr.WGT_UT_CD END 					|| CHR(10)
    || 'NWGT_QTY:'      || CASE WHEN NVL(CNTR.CNTR_WGT,0) <> 0 THEN CNTR.CNTR_WGT
