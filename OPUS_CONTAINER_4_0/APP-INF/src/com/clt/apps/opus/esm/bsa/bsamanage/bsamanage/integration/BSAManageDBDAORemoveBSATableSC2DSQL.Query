<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAORemoveBSATableSC2DSQL">
			<desc><![CDATA[RemoveBSATableSC2]]></desc>
			<sql><![CDATA[
DELETE FROM BSA_SLT_CHTR_CRR_CAPA
 WHERE BSA_SEQ   = @[bsa_seq]
 AND   TRD_CD    = @[trd_cd]
 AND   RLANE_CD  = @[rlane_cd]
 AND   DIR_CD    = @[dir_cd]
 AND   VSL_SEQ   = @[vsl_seq]
 AND   BSA_OP_CD = 'S'			]]></sql>
			<params>
				<param name="bsa_seq" type="12" value="" out="Y"/>
				<param name="trd_cd" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
				<param name="vsl_seq" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
