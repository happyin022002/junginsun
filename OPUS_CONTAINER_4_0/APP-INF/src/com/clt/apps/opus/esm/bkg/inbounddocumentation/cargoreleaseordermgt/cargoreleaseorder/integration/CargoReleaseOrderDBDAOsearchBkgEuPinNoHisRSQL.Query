<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchBkgEuPinNoHisRSQL">
			<desc><![CDATA[Bkg_eu_pin_no 테이블에서 히스토리를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   A.BKG_NO
       , A.CNTR_NO
       , A.RLSE_ORD_SEQ
       , A.RLSE_ORD_SUB_SEQ
       , A.PIN_NO
       , A.SND_FLG
       , A.UPD_USR_ID
       , B.USR_NM AS UPD_USR_NM
       , TO_CHAR(A.UPD_DT, 'YYYY-MM-DD HH24:MI:SS') AS UPD_DT
--       , RANK() OVER(ORDER BY A.UPD_DT DESC) AS SEQ
  FROM BKG_EU_PIN_NO A, COM_USER B
 WHERE A.BKG_NO  = @[bkg_no]
   AND A.CNTR_NO = @[cntr_no]
   AND A.UPD_USR_ID = B.USR_ID (+)
 ORDER BY A.UPD_DT DESC			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
