<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchARAPOfcCdRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchARAPOfcCdRSQL]]></desc>
			<sql><![CDATA[
SELECT AR_OFC_CD, AP_OFC_CD,
       DECODE (DECODE (LTRIM (OFC_TP_CD), 'BA', 'Y', 'OT', 'Y', 'N'),
               'Y', VNDR_CNT_CD || TO_CHAR (VNDR_SEQ, 'FM000000'),
               ''
              ) VNDR_CNT_SEQ
  FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[ppd_ofc_cd]			]]></sql>
			<params>
				<param name="ppd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
