<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAOGetSpclAgmtCntRSQL">
			<desc><![CDATA[GetSpclAgmtCnt
계산 하는 대리점에 해당하는 Special Compensation 계약이 있는지 확인]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT
FROM ACM_SPCL_AGMT WHERE SPCL_OFC_CD = @[agn_cd]			]]></sql>
			<params>
				<param name="agn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
