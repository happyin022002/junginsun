<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentSlsRHQDBDAOModifyAllStepStatusQN0156USQL">
			<desc><![CDATA[trd_cd, dir_cd, glie_ver_no 에 해당하는 모든 STEP의 진행상태를 QN으로 변경.]]></desc>
			<sql><![CDATA[
UPDATE SAQ_MON_QTA_STEP_VER
SET SAQ_STS_CD = 'QN',
    UPD_USR_ID = @[userId],
    UPD_DT = SYSDATE
WHERE  BSE_YR = @[year]
AND    BSE_QTR_CD = @[bse_qtr_cd]
AND    QTA_MST_VER_NO = @[qtaMstVerNo]
AND    SAQ_STS_CD IN ('FN', 'FC')			]]></sql>
			<params>
				<param name="userId" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="qtaMstVerNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
