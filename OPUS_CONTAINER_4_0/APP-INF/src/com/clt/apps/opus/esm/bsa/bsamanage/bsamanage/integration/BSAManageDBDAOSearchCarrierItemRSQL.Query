<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOSearchCarrierItemRSQL">
			<desc><![CDATA[SearchCarrierItem SELECT]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
       CRR_CD
FROM 
      BSA_CRR_RGST
WHERE 
      APLY_FLG   = 'Y'
#if (${bsa_op_cd} != '') 
  AND BSA_OP_CD  = @[bsa_op_cd]
#end
  AND CRR_CD    != COM_ConstantMgr_PKG.COM_getCompanyCode_FNC
  AND BSA_OP_JB_CD IN (SELECT 
                              BSA_OP_JB_CD
                       FROM 
                            BSA_OP_JB
                       WHERE 
                            BSA_OP_JB_CD IN ('001','002','003','004','005')/*** 컬럼삭제 에따른 코드 하드코딩 bsa_op_mgmt_cd = '1' */
                       #if (${bsa_op_cd} != '') 
                         AND BSA_OP_CD  = @[bsa_op_cd]
                       #end
                        )
 ORDER BY 
      CRR_CD			]]></sql>
			<params>
				<param name="bsa_op_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
