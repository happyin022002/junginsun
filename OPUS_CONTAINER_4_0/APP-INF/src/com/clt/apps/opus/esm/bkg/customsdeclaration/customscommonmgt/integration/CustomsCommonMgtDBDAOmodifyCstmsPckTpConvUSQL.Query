<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOmodifyCstmsPckTpConvUSQL">
			<desc><![CDATA[modifyCstmsPckTpConv]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_PCK_TP_CONV
SET PCK_CD_DESC = @[pck_cd_desc]
WHERE CNT_CD = @[cnt_cd]
AND PCK_TP_CD = @[pck_tp_cd]
AND CSTMS_PCK_TP_CD = @[cstms_pck_tp_cd]
AND RCVR_ID = @[rcvr_id]			]]></sql>
			<params>
				<param name="pck_cd_desc" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="cstms_pck_tp_cd" type="12" value="" out="N"/>
				<param name="rcvr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
