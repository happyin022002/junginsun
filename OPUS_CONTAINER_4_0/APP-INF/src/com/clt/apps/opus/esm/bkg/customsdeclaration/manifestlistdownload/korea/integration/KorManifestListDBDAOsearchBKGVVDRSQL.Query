<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchBKGVVDRSQL">
			<desc><![CDATA[BKG VVD테이블에서 Lane과 Dir code를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT SLAN_CD
     , SKD_DIR_CD
     , NVL(SUBSTR(POD_YD_CD,6,2),' ') YARD
  FROM BKG_VVD 
 WHERE BKG_NO = @[bkg_no]
   AND POD_CD LIKE 'KR%'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
