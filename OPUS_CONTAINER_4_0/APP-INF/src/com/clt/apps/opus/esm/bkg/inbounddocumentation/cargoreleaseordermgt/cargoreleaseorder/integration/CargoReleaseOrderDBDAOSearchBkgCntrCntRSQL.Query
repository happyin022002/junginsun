<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchBkgCntrCntRSQL">
			<desc><![CDATA[Booking Container Count 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*)FROM BKG_CONTAINER WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
