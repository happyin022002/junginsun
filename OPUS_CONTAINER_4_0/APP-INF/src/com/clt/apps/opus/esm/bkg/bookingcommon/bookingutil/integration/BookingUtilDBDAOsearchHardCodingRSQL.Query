<sqls>
	<querys>
		<query name="BookingUtilDBDAOsearchHardCodingRSQL">
			<desc><![CDATA[SELECT]]></desc>
			<sql><![CDATA[
SELECT 
	HRD_CDG_ID
,	HRD_CDG_ID_SEQ
,	ATTR_CTNT1
,	ATTR_CTNT2
,	ATTR_CTNT3
,	ATTR_CTNT4
,	ATTR_CTNT5
,	ATTR_CTNT6
,	ATTR_CTNT7
,	ATTR_CTNT8
,	ATTR_CTNT9
,	ATTR_CTNT10
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_HRD_CDG_CTNT
WHERE	HRD_CDG_ID = @[hrd_cdg_id]
#if (${attr_ctnt1} != '') 
  AND  ATTR_CTNT1 = @[attr_ctnt1]
#end
#if (${attr_ctnt2} != '') 
  AND  ATTR_CTNT2 = @[attr_ctnt2]
#end
#if (${attr_ctnt3} != '') 
  AND  ATTR_CTNT3 = @[attr_ctnt3]
#end
#if (${attr_ctnt4} != '') 
  AND  ATTR_CTNT4 = @[attr_ctnt4]
#end
#if (${attr_ctnt5} != '') 
  AND  ATTR_CTNT5 = @[attr_ctnt5]
#end
#if (${attr_ctnt6} != '') 
  AND  ATTR_CTNT6 = @[attr_ctnt6]
#end			]]></sql>
			<params>
				<param name="hrd_cdg_id" type="12" value="" out="N"/>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
