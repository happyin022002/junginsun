<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOsearchCstmsCdConvDescListSeqRSQL">
			<desc><![CDATA[searchCstmsCdConvDescList]]></desc>
			<sql><![CDATA[
SELECT CTNT.ATTR_CTNT1 AS CNT_CD
      ,CTNT.ATTR_CTNT2 AS CSTMS_DIV_ID
      ,DES.HRD_CDG_DESC AS CSTMS_CD_DESC
      ,DES.ATTR_NM1 AS ATTR_NM1
      ,DES.ATTR_NM2 AS ATTR_NM2
	  ,DES.ATTR_NM3 AS ATTR_NM3
	  ,DES.ATTR_NM4 AS ATTR_NM4
	  ,DES.ATTR_NM5 AS ATTR_NM5
FROM BKG_HRD_CDG_CTNT CTNT, BKG_HRD_CDG_DESC DES
WHERE 1=1
AND CTNT.HRD_CDG_ID=DES.HRD_CDG_ID
AND CTNT.ATTR_CTNT1 = @[frm_cnt_cd]
#if (${frm_cstms_div_id} != '')
    AND UPPER(CTNT.ATTR_CTNT2) LIKE UPPER('%'||@[frm_cstms_div_id]||'%')
#end
#if (${frm_cstms_cd_desc} != '')
    AND UPPER(CTNT.ATTR_CTNT3) LIKE UPPER('%'||@[frm_cstms_cd_desc]||'%')
#end
ORDER BY CNT_CD, CSTMS_DIV_ID			]]></sql>
			<params>
				<param name="frm_cnt_cd" type="12" value="" out="N"/>
				<param name="frm_cstms_div_id" type="12" value="" out="N"/>
				<param name="frm_cstms_cd_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
