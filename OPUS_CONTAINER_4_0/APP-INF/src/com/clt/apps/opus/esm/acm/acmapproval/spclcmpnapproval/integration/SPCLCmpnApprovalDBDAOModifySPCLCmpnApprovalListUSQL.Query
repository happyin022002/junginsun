<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SPCLCmpnApprovalDBDAOModifySPCLCmpnApprovalListUSQL">
			<desc><![CDATA[Special Compensation Ex.Rate 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE ACM_SPCL_CMPN
   SET
     PAY_XCH_RT = @[pay_xch_rt]
   , PAY_PPD_AMT = ROUND(PPD_AMT * @[pay_xch_rt],3)
   , PAY_CRNT_AMT = ROUND(CRNT_AMT * @[pay_xch_rt],3)
   , PAY_IF_AMT = ROUND(IF_AMT * @[pay_xch_rt],3)
   , UPD_USR_ID = @[usr_id]
   , UPD_DT = SYSDATE
 WHERE 1=1
   AND BKG_NO = @[bkg_no] 
   AND SPCL_OFC_CD = @[spcl_ofc_cd]
   AND SPCL_CMPN_SEQ = @[spcl_cmpn_seq]
   AND CUST_CNT_CD   = SUBSTR(@[cust_cnt_seq], 0, 2)
   AND CUST_SEQ      = SUBSTR(@[cust_cnt_seq], 3, 6)
   AND SPCL_CMPN_STS_CD = 'CS' 
   AND NVL(PAY_CHK_FLG,'N') = 'Y'			]]></sql>
			<params>
				<param name="pay_xch_rt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_ofc_cd" type="12" value="" out="N"/>
				<param name="spcl_cmpn_seq" type="12" value="" out="N"/>
				<param name="cust_cnt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
