<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOCheckComUserRoleMtchRSQL">
			<desc><![CDATA[Check User Role. 'xtn_phn_no' parameter (ComUserVO parameter) is set to check the user role code.]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN NVL(COUNT(*),0) <= 0  THEN 'N' ELSE 'Y' END ISROLE
  FROM COM_USR_ROLE_MTCH
 WHERE USR_ID = @[usr_id]
#if (${xtn_phn_no} != '')
   AND USR_ROLE_CD = (SELECT ATTR_CTNT1
                        FROM PRI_PARA_CD_DTL
                       WHERE HRD_CDG_ID = @[xtn_phn_no])
#else
   AND USR_ROLE_CD = (SELECT ATTR_CTNT1
                        FROM PRI_PARA_CD_DTL
                       WHERE HRD_CDG_ID = 'PRICD0002')
#end			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="xtn_phn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
