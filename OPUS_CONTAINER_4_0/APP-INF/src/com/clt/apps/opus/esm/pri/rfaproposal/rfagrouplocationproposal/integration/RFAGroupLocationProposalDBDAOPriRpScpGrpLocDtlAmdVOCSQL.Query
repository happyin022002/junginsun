		<query name="RFAGroupLocationProposalDBDAOPriRpScpGrpLocDtlAmdVOCSQL">
			<desc><![CDATA[RFAGroupLocationProposalDBPriRpScpGrpLocDtlAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_GRP_LOC_DTL(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    GRP_LOC_SEQ,
    GRP_LOC_DTL_SEQ,
    LOC_CD,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    PROP_NO			,
    AMDT_SEQ+1		,
    SVC_SCP_CD		,
    GRP_LOC_SEQ		,
    GRP_LOC_DTL_SEQ	,
    LOC_CD			,
    PRC_PROG_STS_CD	,
    SRC_INFO_CD		,
    ''				,
    ''				,
    ''				,
    @[cre_usr_id]	,
    SYSDATE			,
    @[upd_usr_id]	,
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ
FROM
    PRI_RP_SCP_GRP_LOC_DTL
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
