<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiHpSwaRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT '{HPSWA_INFO' || CHR (10) || 
       'SHIP_ID:' || NVL (DE_NO, '') || CHR (10) || 
       'PART_NO:' || NVL (PRT_NO, '') || CHR (10) || 
       '}HPSWA_INFO' || CHR (10)
FROM   BKG_REF_DTL
WHERE  BKG_NO = @[bkg_no]
AND (DE_NO > ' ' OR PRT_NO > ' ')
ORDER BY REF_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
