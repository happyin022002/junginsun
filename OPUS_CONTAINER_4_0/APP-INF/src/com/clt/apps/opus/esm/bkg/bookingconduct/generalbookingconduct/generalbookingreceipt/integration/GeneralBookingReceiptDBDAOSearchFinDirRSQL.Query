<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchFinDirRSQL">
			<desc><![CDATA[financial direction을 찾는다.]]></desc>
			<sql><![CDATA[
SELECT dir REV_DIR_CD
FROM(
	SELECT 1 rank, substr(fin.RLANE_DIR_CD, 1, 1) dir
	FROM AR_FINC_DIR_CONV fin, mdm_LOCATION loc
	WHERE fin.SCONTI_CD = loc.SCONTI_CD 
  	AND loc.LOC_CD        = @[pol_cd]
   	AND fin.SLAN_CD       = @[trnk_lane_cd]
    AND fin.SLAN_DIR_CD   = @[skd_dir_cd]
	UNION ALL
	SELECT 2 rank, @[skd_dir_cd]
	FROM DUAL
)
WHERE DIR IS NOT NULL
AND ROWNUM = 1				]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="trnk_lane_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
