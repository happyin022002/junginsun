<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSetupDBDAOSearchChargeDdtSetListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT B.CHG_DDCT_GRP_NM,
       B.REP_CHG_CD,
       B.CHG_CD,
       B.ORG_CHG_CD,
       B.CHG_DDCT_GRP_NM AS ORG_CHG_DDCT_GRP_NM
  FROM (SELECT CHG_DDCT_GRP_NM,
               (ACM_JOIN_FNC(CURSOR(SELECT CHG_CD
                                      FROM ACM_AGN_SET_DDCT_CHG_GRP
                                     WHERE CHG_DDCT_GRP_NM = A.CHG_DDCT_GRP_NM
                                       AND CHG_DIV_CD = 'R'
                                     ORDER BY CHG_CD))) AS REP_CHG_CD,
               (ACM_JOIN_FNC(CURSOR(SELECT CHG_CD
                                      FROM ACM_AGN_SET_DDCT_CHG_GRP
                                     WHERE CHG_DDCT_GRP_NM = A.CHG_DDCT_GRP_NM
                                       AND CHG_DIV_CD = 'C'
                                       AND CHG_CD NOT IN (SELECT CHG_CD
                                                            FROM MDM_CHARGE
                                                           WHERE NVL (DELT_FLG, 'N') = 'N'
                                                             AND REP_CHG_CD IN (SELECT CHG_CD
                                                                                  FROM ACM_AGN_SET_DDCT_CHG_GRP
                                                                                 WHERE CHG_DDCT_GRP_NM = A.CHG_DDCT_GRP_NM
                                                                                   AND CHG_DIV_CD = 'R')) ORDER BY CHG_CD))) AS CHG_CD,
               (ACM_JOIN_FNC(CURSOR(SELECT CHG_CD
                                      FROM ACM_AGN_SET_DDCT_CHG_GRP
                                     WHERE CHG_DDCT_GRP_NM = A.CHG_DDCT_GRP_NM
                                       AND CHG_DIV_CD = 'C'
                                     ORDER BY CHG_CD))) AS ORG_CHG_CD
          FROM ACM_AGN_SET_DDCT_CHG_GRP A
         ORDER BY A.CHG_DDCT_GRP_NM) B

 GROUP BY B.CHG_DDCT_GRP_NM,
          B.REP_CHG_CD,
          B.CHG_CD,
          B.ORG_CHG_CD
 ORDER BY B.CHG_DDCT_GRP_NM,
          B.REP_CHG_CD,
          B.CHG_CD,
          B.ORG_CHG_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
