<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOCancelBkgStateFirmUSQL">
			<desc><![CDATA[BKG_DOC_PROC_SKD의 'BKGSTF' 상태값을 DOC_PERF_DELT_FLG='Y'로 설정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_DOC_PROC_SKD
   SET DOC_PERF_DELT_FLG = 'Y'
 WHERE BKG_NO             = @[bkg_no]
   AND BKG_DOC_PROC_TP_CD = 'BKGSTF'
   AND DOC_PERF_DELT_FLG = 'N'				]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
