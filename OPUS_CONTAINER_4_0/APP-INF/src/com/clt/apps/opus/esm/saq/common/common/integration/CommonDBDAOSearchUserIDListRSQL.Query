<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchUserIDListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT A.USR_ID CODE, A.USR_NM TEXT
FROM   COM_USER A
WHERE  A.USR_ID LIKE '%'||@[user_cd]||'%'			]]></sql>
			<params>
				<param name="user_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
