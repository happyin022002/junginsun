<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchJapanCYCodeRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT ATTR_CTNT2 AS CY_CD

  FROM BKG_CSTMS_CD_CONV_CTNT

WHERE CNT_CD = 'JP'
  AND CSTMS_DIV_ID = 'JP_TEMINAL_CY_CD'
  AND ATTR_CTNT1 = @[yd_cd]
  AND DELT_FLG = 'N'

ORDER BY CSTMS_DIV_ID_SEQ			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
