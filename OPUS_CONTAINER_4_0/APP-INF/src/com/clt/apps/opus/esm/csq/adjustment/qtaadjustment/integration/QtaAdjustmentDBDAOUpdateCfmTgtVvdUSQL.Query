<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOUpdateCfmTgtVvdUSQL">
			<desc><![CDATA[[QTA Adjustment by VVD]에서 확정데이터를 [수정] 합니다]]></desc>
			<sql><![CDATA[
UPDATE CSQ_CFM_TGT_VVD
   SET BSE_MON      = @[coa_bse_mon]
      ,BSE_WK       = @[coa_bse_wk]
      ,FNL_BSA_CAPA = @[coa_fnl_bsa_capa]
      ,LOD_QTY      = NVL(@[coa_lod_qty],0)
      ,GRS_REV      = NVL(@[coa_grs_rev],0)
      ,UPD_USR_ID   = @[usr_id]
      ,UPD_DT       = SYSDATE
 WHERE QTA_RLSE_VER_NO = @[qta_rlse_ver_no]
   AND BSE_TP_CD       = @[f_bse_tp_cd]
   AND BSE_YR          = @[f_bse_yr]
   AND BSE_QTR_CD      = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
   AND QTA_TGT_CD      = 'D'
   AND TRD_CD          = @[trd_cd]
   AND RLANE_CD        = @[rlane_cd]
   AND DIR_CD          = @[dir_cd]
   AND VSL_CD || SKD_VOY_NO || SKD_DIR_CD = @[vvd]			]]></sql>
			<params>
				<param name="coa_bse_mon" type="12" value="" out="N"/>
				<param name="coa_bse_wk" type="12" value="" out="N"/>
				<param name="coa_fnl_bsa_capa" type="12" value="" out="N"/>
				<param name="coa_lod_qty" type="12" value="" out="N"/>
				<param name="coa_grs_rev" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="qta_rlse_ver_no" type="12" value="" out="N"/>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
