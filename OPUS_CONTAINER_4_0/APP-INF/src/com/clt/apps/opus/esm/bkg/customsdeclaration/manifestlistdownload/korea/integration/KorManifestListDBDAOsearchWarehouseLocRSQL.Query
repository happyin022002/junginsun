<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchWarehouseLocRSQL">
			<desc><![CDATA[WareHouse Loc CD 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(LOC_CD,' ') LOC_CD
  FROM BKG_WAREHOUSE
 WHERE CSTMS_CD = @[cstms_cd]			]]></sql>
			<params>
				<param name="cstms_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
