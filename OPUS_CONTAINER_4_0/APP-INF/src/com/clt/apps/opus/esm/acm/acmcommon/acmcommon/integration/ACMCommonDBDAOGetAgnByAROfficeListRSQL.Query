<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCommonDBDAOGetAgnByAROfficeListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT AGN_CD AS VALUE0
  FROM (SELECT AGN_CD
          FROM ACM_OFC_INFO
         WHERE OFC_CD = @[value0]
           AND AGN_CD IN (SELECT AGN_CD
                            FROM ACM_OFC_INFO
                           WHERE @[value1] IN (OFC_CD, AGN_CD, RHQ_CD))
         GROUP BY AGN_CD
        UNION
		SELECT AGN_CD
          FROM ACM_OFC_INFO
         WHERE OFC_CD = @[value0]
		UNION
        SELECT AGN_CD
          FROM ACM_OFC_INFO
         WHERE DP_GRP_NM IN (SELECT DP_GRP_NM
                               FROM ACM_OFC_INFO
                              WHERE @[value0] IN (OFC_CD, AGN_CD)
                                AND DP_GRP_NM IS NOT NULL)
           AND AGN_CD IN (SELECT AGN_CD
                            FROM ACM_OFC_INFO
                           WHERE @[value1] IN (OFC_CD, AGN_CD, RHQ_CD))
         GROUP BY AGN_CD) 
 ORDER BY AGN_CD			]]></sql>
			<params>
				<param name="value0" type="12" value="" out="N"/>
				<param name="value1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
