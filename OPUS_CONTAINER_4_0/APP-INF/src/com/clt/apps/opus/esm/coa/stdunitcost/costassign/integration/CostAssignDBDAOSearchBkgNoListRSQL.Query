<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostAssignDBDAOSearchBkgNoListRSQL">
			<desc><![CDATA[BKG_BOOKING 테이블에서 BKG_NO 추출   ]]></desc>
			<sql><![CDATA[
SELECT 
	BKG_NO
FROM BKG_BOOKING
WHERE	BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
