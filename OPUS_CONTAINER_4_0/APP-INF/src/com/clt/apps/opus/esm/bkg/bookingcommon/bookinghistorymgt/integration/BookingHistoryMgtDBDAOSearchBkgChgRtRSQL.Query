			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgChgRtRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
, RT_SEQ
, DP_SEQ
, FRT_TERM_CD
, TRF_ITM_NO
, CGO_CATE_CD
, IMDG_CLSS_CD
, CHG_CD
, CURR_CD
, RAT_UT_CD
, BKG_QTY
, RAT_AS_QTY
, CHG_UT_AMT
, CHG_AMT
, RCV_TERM_CD
, DE_TERM_CD
, N3PTY_RCV_OFC_CD
, N3PTY_CUST_CNT_CD
, N3PTY_CUST_SEQ
--, INCL_OFT_FLG
, FRT_INCL_XCLD_DIV_CD
, INV_STS_CD
, PRN_HDN_FLG
, AUTO_RAT_CD
, APLY_XCH_RTO
, AGMT_RAT_UT_CD
, CRE_USR_ID
, TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, UPD_USR_ID
, TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
  FROM BKG_CHG_RT
 WHERE BKG_NO = @[bkg_no]			]]></sql>
