<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchVvdBdrRSQL">
			<desc><![CDATA[vvd, pol로 BDR 여부 조회(vvd, pol로 bkg_bdr_log를 확인하여 bdr이 걸렸으면(trnk_bdr_flg or bdr_bdr_flg) 중지한다)]]></desc>
			<sql><![CDATA[
SELECT 
	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	POL_CD
,	POD_CD
,	POD_CLPT_IND_SEQ
,	SLAN_CD
,	SLAN_TP_CD
,	TRNK_BDR_FLG
,	TRNK_ESTM_BDR_DT
,	TRNK_AUTO_BDR_FLG
,	TRNK_AUTO_BDR_DT
,	TRNK_MNL_BDR_FLG
,	TRNK_MNL_BDR_DT
,	TRNK_BDR_CRE_USR_ID
,	FDR_BDR_FLG
,	FDR_ESTM_BDR_DT
,	FDR_AUTO_BDR_FLG
,	FDR_AUTO_BDR_DT
,	FDR_MNL_BDR_FLG
,	FDR_MNL_BDR_DT
,	FDR_BDR_CRE_USR_ID
,	FDR_BDR_UPD_DT
,	BDR_VSL_CHK_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	NVL(TRNK_AUTO_BDR_FLG,FDR_AUTO_BDR_FLG) BDR_FLG
FROM BKG_VVD_BDR_LOG
WHERE	VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	POL_CD = @[pol_cd]
AND	POD_CD = NVL(@[pod_cd], POD_CD)
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
