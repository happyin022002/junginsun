<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCFlagTERSQL">
			<desc><![CDATA[C-Flag : T , E]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN BL.PCK_QTY = TO_NUMBER(@[icr_qty]) OR BL.PCK_QTY = TO_NUMBER(@[icr_qty]) THEN
         CASE WHEN IBD.IBD_TRSP_TP_CD = '62' AND BL.USA_LST_LOC_CD = @[cus_loc]
                   AND SUBSTR(IBD.IBD_TRSP_NO, 1,3) = BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)
                   AND SUBSTR(RS.ENTR_NO, 1,3) = BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)
                   AND SUBSTR(@[icr_et_no], 1,3) = BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)
                   THEN 'T'
              WHEN IBD.IBD_TRSP_TP_CD = '63' AND BL.CSTMS_POD_CD = @[cus_loc]
                   AND SUBSTR(IBD.IBD_TRSP_NO, 1,3) = BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)
                   AND RS.CNT_CD IS NULL 
                   AND SUBSTR(@[icr_et_no], 1,3) = BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)
                   THEN 'E'
               ELSE 'N'
          END 
        ELSE 'N'
        END C_FLAG
  FROM BKG_CSTMS_ADV_IBD IBD
      ,BKG_CSTMS_ADV_BL BL
      ,(SELECT /*+INDEX_DESC(R XPKBKG_CSTMS_ADV_RSLT)*/
               CNT_CD
              ,BL_NO
              ,ENTR_TP_NO
              ,ENTR_NO
          FROM BKG_CSTMS_ADV_RSLT R
         WHERE R.CNT_CD = 'US'
           AND R.BL_NO = @[bl_no]
           AND R.DSPO_CD IN ('12','13')
           AND NVL(RSND_FLG,'N') <> 'Y'
           AND BL_NVOCC_TP_CD = 'M'
           AND ROWNUM = 1 --EXP만(ARRIVAL 제외)
       ) RS
 WHERE IBD.CNT_CD = BL.CNT_CD
   AND IBD.BL_NO = BL.BL_NO
   AND IBD.CNT_CD = RS.CNT_CD(+)
   AND IBD.BL_NO = RS.BL_NO(+)
   AND IBD.CNT_CD = 'US'
   AND IBD.BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="icr_qty" type="12" value="" out="N"/>
				<param name="cus_loc" type="12" value="" out="N"/>
				<param name="icr_et_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
