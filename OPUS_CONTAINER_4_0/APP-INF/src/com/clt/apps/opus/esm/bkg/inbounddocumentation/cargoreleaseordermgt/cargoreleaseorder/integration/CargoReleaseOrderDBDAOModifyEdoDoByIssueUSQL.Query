<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOModifyEdoDoByIssueUSQL">
			<desc><![CDATA[EDI로  DO 발급신청 승인정보를 전송 한 후  관련 정보를 Update 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_EDO_DO
   SET EDO_ISS_OFC_CD  = @[edo_iss_ofc_cd]
 WHERE EDO_RQST_NO     = @[rqst_no]
   AND EDO_RQST_SEQ    = ( SELECT EDO_RQST_SEQ
                             FROM BKG_EDO_MST
                            WHERE EDO_RQST_NO = @[rqst_no]
                              AND EDO_TP_CD   = @[edo_tp_cd]
                              AND VTY_FLG     = 'Y'
                         )			]]></sql>
			<params>
				<param name="edo_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="edo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
