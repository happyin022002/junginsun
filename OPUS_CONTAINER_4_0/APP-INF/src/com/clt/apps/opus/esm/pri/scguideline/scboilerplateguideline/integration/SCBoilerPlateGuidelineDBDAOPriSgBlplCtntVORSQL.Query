<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateGuidelineDBDAOPriSgBlplCtntVORSQL">
			<desc><![CDATA[content search]]></desc>
			<sql><![CDATA[
select 
	blpl_hdr_seq,
	blpl_seq,
	blpl_ctnt_seq,
	blpl_ctnt,
	dp_seq,
	cre_usr_id,
	cre_dt,
	upd_usr_id,
	upd_dt
from pri_sg_blpl_ctnt
where	blpl_hdr_seq = @[blpl_hdr_seq]
and	blpl_seq = @[blpl_seq]
ORDER BY DP_SEQ			]]></sql>
			<params>
				<param name="blpl_hdr_seq" type="12" value="" out="N"/>
				<param name="blpl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
