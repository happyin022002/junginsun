<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchRLaneComboListRSQL">
			<desc><![CDATA[Lane Combo List 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT B.TRD_CD    ,
                  B.SUB_TRD_CD,
                  A.RLANE_CD  ,
                  A.RLANE_NM
    FROM MDM_REV_LANE     A,
         MDM_DTL_REV_LANE B
   WHERE A.RLANE_CD   = B.RLANE_CD
     AND A.VSL_TP_CD  = 'C'
     
#if (${ipc} != 'true')
     AND A.REP_TRD_CD = B.TRD_CD
#end

     AND B.DELT_FLG  IN ('N', @[del])
     AND B.TRD_CD    <> 'COM'
#if (${trade} != '')
	 AND B.TRD_CD     IN (@[trade]) 
#end
ORDER BY B.TRD_CD    ,
         B.SUB_TRD_CD,
         A.RLANE_CD			]]></sql>
			<params>
				<param name="del" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
