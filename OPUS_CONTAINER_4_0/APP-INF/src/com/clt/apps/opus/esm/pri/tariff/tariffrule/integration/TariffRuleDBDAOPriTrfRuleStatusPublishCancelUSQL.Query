<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffRuleDBDAOPriTrfRuleStatusPublishCancelUSQL">
			<desc><![CDATA[cancel the publish status for PriTrfRule]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRF_RULE
   SET EXP_DT				= NULL          
     , UPD_USR_ID			= @[upd_usr_id]                   
     , UPD_DT				= SYSDATE                         
 WHERE TRF_PFX_CD	= @[trf_pfx_cd]
   AND TRF_NO		= @[trf_no]
   AND TRF_RULE_NO	= @[trf_rule_no]
   AND AMDT_SEQ		= (TO_NUMBER(@[amdt_seq]) - 1)			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_rule_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
