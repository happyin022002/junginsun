<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MexCustomsTransmissionDBDAOsearchBlCmDescRSQL">
			<desc><![CDATA[dwkim, 0370, outVO : MxCmDescVO]]></desc>
			<sql><![CDATA[
SELECT
	-- '??'은 자료가 나오지 않는 문제가 발생 ( 2007.10.15 ) - Kim YC
	replace(replace(replace(CSTMS_DESC,'?','?'||'?'), CHR(39),'?'||CHR(39)), ':','?:') CSTMS_DESC
FROM
	BKG_BL_DOC
WHERE
	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
