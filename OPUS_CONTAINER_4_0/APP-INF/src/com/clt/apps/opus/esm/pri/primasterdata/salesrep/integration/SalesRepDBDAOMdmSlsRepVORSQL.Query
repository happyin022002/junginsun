<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesRepDBDAOMdmSlsRepVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT SREP_CD
     , OFC_CD
     , SREP_NM
  FROM MDM_SLS_REP
 WHERE DELT_FLG = 'N'
#if (${srep_cd} != '') 
   AND SREP_CD LIKE '%' || @[srep_cd] || '%'
#end
#if (${ofc_cd} != '') 
  AND OFC_CD LIKE @[ofc_cd] || '%'
#end			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
