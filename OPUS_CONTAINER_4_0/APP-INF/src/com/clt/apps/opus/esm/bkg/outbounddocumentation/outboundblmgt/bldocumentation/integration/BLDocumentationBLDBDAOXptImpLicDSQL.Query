<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOXptImpLicDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y')
DELETE BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO       = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
DELETE BKG_XPT_IMP_LIC
 WHERE BKG_NO       = @[bkg_no]
#end
   AND IO_BND_CD    = @[io_bnd_cd]   
   AND XPT_IMP_SEQ  = @[xpt_imp_seq]
   AND CNT_CD  		= @[cnt_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="xpt_imp_seq" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
