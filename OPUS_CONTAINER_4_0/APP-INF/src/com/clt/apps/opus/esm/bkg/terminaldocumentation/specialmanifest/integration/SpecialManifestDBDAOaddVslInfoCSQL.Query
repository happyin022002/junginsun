<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOaddVslInfoCSQL">
			<desc><![CDATA[VSL 정보 (ETA, ETD 포함)를 생성한다.(barge에 한해)]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_CSTMS_EUR_DG_VSL_SKD 
      (EUR_DG_DECL_TP_CD
      ,VSL_CD
      ,SKD_VOY_NO
      ,SKD_DIR_CD
      ,PORT_CD
      ,EUR_VSL_ID
      ,BRTH_YD_CD
      ,BRTH_YD_NM
      ,SVC_RQST_NO
      ,ETA_DT
      ,ETD_DT
      ,LLOYD_NO
      ,VSL_ENG_NM
      ,CALL_SGN_NO
      ,VSL_CNT_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      )
VALUES
      (@[d_type]
      ,SUBSTR(@[vvd_cd], 1, 4)
      ,SUBSTR(@[vvd_cd], 5, 4)
      ,SUBSTR(@[vvd_cd], 9, 1)
      ,@[port_cd]
      ,@[vsl_cd]
      ,@[brth_yd_cd]
      ,@[yd_nm]
      ,@[svc_rqst_no]
      ,TO_DATE(@[eta_d]||@[eta_t],'YYYYMMDDHH24MI')
      ,TO_DATE(@[etd_d]||@[etd_t],'YYYYMMDDHH24MI')
      ,@[lloyd_no]
      ,@[vsl_eng_nm]
      ,@[call_sgn_no]
      ,@[vsl_cnt_cd]
      ,@[upd_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE
      )			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="brth_yd_cd" type="12" value="" out="N"/>
				<param name="yd_nm" type="12" value="" out="N"/>
				<param name="svc_rqst_no" type="12" value="" out="N"/>
				<param name="eta_d" type="12" value="" out="N"/>
				<param name="eta_t" type="12" value="" out="N"/>
				<param name="etd_d" type="12" value="" out="N"/>
				<param name="etd_t" type="12" value="" out="N"/>
				<param name="lloyd_no" type="12" value="" out="N"/>
				<param name="vsl_eng_nm" type="12" value="" out="N"/>
				<param name="call_sgn_no" type="12" value="" out="N"/>
				<param name="vsl_cnt_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
