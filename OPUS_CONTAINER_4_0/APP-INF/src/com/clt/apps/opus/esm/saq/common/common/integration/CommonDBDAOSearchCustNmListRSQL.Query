<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchCustNmListRSQL">
			<desc><![CDATA[y & Multi Trade Acct 판매목표 수립관련]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT B.CUST_GRP_ID CODE           
       ,B.CUST_GRP_NM TEXT                    
FROM    MDM_CUSTOMER      C   ,               
        MDM_CUST_PERF_GRP B                   
WHERE   C.CUST_GRP_ID     = B.CUST_GRP_ID(+)  
AND     C.DELT_FLG        = 'N'               
AND     DECODE(@[qta_tgt_cd],'G',C.KEY_ACCT_FLG,'M',MLT_TRD_ACCT_FLG) = 'Y' 
AND     C.CUST_GRP_ID IS NOT NULL             
ORDER BY B.CUST_GRP_NM    			]]></sql>
			<params>
				<param name="qta_tgt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
