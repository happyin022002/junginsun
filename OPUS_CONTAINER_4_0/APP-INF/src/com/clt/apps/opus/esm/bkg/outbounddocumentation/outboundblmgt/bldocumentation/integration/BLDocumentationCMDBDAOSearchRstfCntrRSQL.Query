<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOSearchRstfCntrRSQL">
			<desc><![CDATA[BLDocumentationCMDBDAOSearchRstfCntrRSQL]]></desc>
			<sql><![CDATA[
SELECT XCH_CNTR_NO FROM CTM_MVMT_XCH
WHERE XCH_CNTR_NO = @[cntr_no]
AND EXISTS (SELECT 'Y' FROM CTM_MOVEMENT A WHERE A.CNTR_NO = @[cntr_no] AND BKG_NO = @[bkg_no] AND ROWNUM = 1 )
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
