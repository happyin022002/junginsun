<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MalaysiaManifestListDownloadDBDAOsearchVslNameRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(UPPER(TRIM(PSA_VSL_NM)),' ') PSA_VSL_NM
  FROM BKG_CSTMS_MY_VVD
 WHERE VSL_CD (+) = @[vsl_cd]
   AND ROWNUM =1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
