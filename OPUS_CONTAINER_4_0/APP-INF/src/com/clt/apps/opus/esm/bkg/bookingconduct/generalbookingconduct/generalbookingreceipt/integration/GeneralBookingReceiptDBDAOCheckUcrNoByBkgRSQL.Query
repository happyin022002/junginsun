<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOCheckUcrNoByBkgRSQL">
			<desc><![CDATA[Bkg에 해당하는 BKG_REFERENCE 중에 UCR 이 존재하는지 체크함.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y')
FROM BKG_REFERENCE 
WHERE BKG_NO = @[bkg_no]
AND BKG_REF_TP_CD = 'CUCR'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
