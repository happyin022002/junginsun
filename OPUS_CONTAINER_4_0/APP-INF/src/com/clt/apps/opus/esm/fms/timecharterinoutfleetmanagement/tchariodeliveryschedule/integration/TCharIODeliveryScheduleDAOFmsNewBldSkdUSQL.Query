<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharIODeliveryScheduleDAOFmsNewBldSkdUSQL">
			<desc><![CDATA[NB Delivery Schedule Creation Update]]></desc>
			<sql><![CDATA[
update fms_new_bld_skd set 
	shp_nm = @[shp_nm],
	shp_bld_nm = @[shp_bld_nm],
	vsl_dznd_capa = @[vsl_dznd_capa],
	bse_14ton_vsl_capa = @[bse_14ton_vsl_capa],
	rf_cntr_plg_qty = @[rf_cntr_plg_qty],
	shp_spd_qty = @[shp_spd_qty],
	flet_ctrt_dur_ctnt = @[flet_ctrt_dur_ctnt],
	trd_hus_nm = @[trd_hus_nm],
	vsl_de_dt = @[vsl_de_dt],
	yd_seq = @[yd_seq],
	ownr_seq = @[ownr_seq],
	upd_usr_id = @[upd_usr_id]
where	shp_de_seq = @[shp_de_seq]			]]></sql>
			<params>
				<param name="shp_nm" type="12" value="" out="N"/>
				<param name="shp_bld_nm" type="12" value="" out="N"/>
				<param name="vsl_dznd_capa" type="2" value="" out="N"/>
				<param name="bse_14ton_vsl_capa" type="2" value="" out="N"/>
				<param name="rf_cntr_plg_qty" type="2" value="" out="N"/>
				<param name="shp_spd_qty" type="3" value="" out="N"/>
				<param name="flet_ctrt_dur_ctnt" type="12" value="" out="N"/>
				<param name="trd_hus_nm" type="12" value="" out="N"/>
				<param name="vsl_de_dt" type="12" value="" out="N"/>
				<param name="yd_seq" type="2" value="" out="N"/>
				<param name="ownr_seq" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="shp_de_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
