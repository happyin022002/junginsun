<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOaddSendLogCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_JP_SND_LOG

       (JP_SND_LOG_ID,
        SND_DT,
        OFC_CD,
        LOG_SEQ,
        VSL_CD,
        SKD_VOY_NO,
        SKD_DIR_CD,
        POD_CD,
        LOG_FLG,
        LOG_DT,
        BL_NO,
        SND_FLG,
        EDI_SND_MSG_CTNT,
        CSTMS_RQST_FLG,
        EDI_REF_ID,
        CORR_RSN_CD,
        CORR_RSN,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (@[jp_snd_log_id],
        NVL(GLOBALDATE_PKG.TIME_LOCAL_FNC('JPTYO'), SYSDATE),
        @[ofc_cd],
        @[log_seq],
        @[vsl_cd],
        @[skd_voy_no],
        @[skd_dir_cd],
        @[pod_cd],
        @[log_flg],
        DECODE(@[snd_dt], NULL , SYSDATE, TO_DATE(@[snd_dt], 'YYYY-MM-DD HH24:MI:SS')),
        @[bl_no],
        'Y',
        @[edi_snd_msg_ctnt],
        'N',
        @[edi_ref_id],
        @[corr_rsn_cd],
        @[corr_rsn],
        @[cre_usr_id],
        SYSDATE,
        @[cre_usr_id],
        SYSDATE)
			]]></sql>
			<params>
				<param name="jp_snd_log_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="log_seq" type="2" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="log_flg" type="12" value="" out="N"/>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="edi_snd_msg_ctnt" type="12" value="" out="N"/>
				<param name="edi_ref_id" type="12" value="" out="N"/>
				<param name="corr_rsn_cd" type="12" value="" out="N"/>
				<param name="corr_rsn" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
