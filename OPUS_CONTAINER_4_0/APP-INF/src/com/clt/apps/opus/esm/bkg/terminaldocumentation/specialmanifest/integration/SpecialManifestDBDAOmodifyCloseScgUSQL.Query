<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOmodifyCloseScgUSQL">
			<desc><![CDATA[위험물 파트너 부킹을 마감한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_EUR_DG_VSL_SKD SET 
	SPCL_CGO_PRNR_CLZ_FLG = @[frm_spcl_cgo_prnr_clz_flg]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	EUR_DG_DECL_TP_CD = @[d_type]
AND	VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
AND	SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
AND	SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
AND	PORT_CD = @[port_cd]			]]></sql>
			<params>
				<param name="frm_spcl_cgo_prnr_clz_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
