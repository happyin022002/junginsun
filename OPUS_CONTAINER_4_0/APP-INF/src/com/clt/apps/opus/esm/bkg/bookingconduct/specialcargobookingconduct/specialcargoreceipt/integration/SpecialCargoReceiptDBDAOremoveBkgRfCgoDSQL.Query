<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOremoveBkgRfCgoDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_RF_CGO
WHERE	BKG_NO = @[bkg_no]
#if(${rc_seq} == '')
AND CNTR_NO = @[cntr_no]
#else
AND	RC_SEQ = @[rc_seq]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="rc_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
