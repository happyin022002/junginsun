							 WHERE BK.BKG_NO  = HDR.BKG_NO
							   AND HDR.COP_NO = DTL.COP_NO
							   AND DTL.NOD_CD = BK.DEL_NOD_CD
							   AND HDR.COP_STS_CD IN ('C', 'T', 'F')
