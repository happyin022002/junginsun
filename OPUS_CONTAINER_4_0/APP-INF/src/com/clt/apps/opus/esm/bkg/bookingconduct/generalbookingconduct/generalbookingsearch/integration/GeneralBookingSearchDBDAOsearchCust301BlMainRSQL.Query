                     		, vsk_vsl_port_skd skd, mdm_location loc
               			where vvd.pod_cd           = loc.loc_cd
                 		and vvd.vsl_cd           = skd.vsl_cd
                 		and vvd.skd_voy_no       = skd.skd_voy_no
