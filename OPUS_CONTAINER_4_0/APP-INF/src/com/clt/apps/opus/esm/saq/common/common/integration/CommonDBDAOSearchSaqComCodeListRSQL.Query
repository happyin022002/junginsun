<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqComCodeListRSQL">
			<desc><![CDATA[공통코드 조회]]></desc>
			<sql><![CDATA[
SELECT 
	   CASE WHEN @[code_cd] = 'CD01216' AND INTG_CD_VAL_CTNT ='0' THEN 
	       'X'
	   ELSE
		   INTG_CD_VAL_CTNT
	   END CODE_CD,
	   INTG_CD_VAL_DP_DESC CODE_NM
FROM  COM_INTG_CD_DTL									
WHERE INTG_CD_ID = @[code_cd]								
ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="code_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
