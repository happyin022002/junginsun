<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMonQtaAdjSmryWeekListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DISTINCT B.COST_WK  AS TEXT  
  , B.COST_WK   
  || '|'   
  || SUBSTR(B.SLS_FM_DT,1,4) || '.' || SUBSTR(B.SLS_FM_DT,5,2)  || '.' ||  SUBSTR(B.SLS_FM_DT,7,2)  
  || '|'   
  || SUBSTR(B.SLS_TO_DT,1,4) || '.' || SUBSTR(B.SLS_TO_DT,5,2)  || '.' ||  SUBSTR(B.SLS_TO_DT,7,2)  
  AS CODE  
FROM SAQ_MON_QTA_ADJ_SMRY_HDR HDR 
     ,SAQ_MON_QTA_ADJ_SMRY A  
     ,SAQ_MON_TGT_VVD_ADJ VVD  
     ,COA_WK_PRD B  
WHERE HDR.QTA_MST_VER_NO = @[qta_mst_ver_no]   
AND HDR.BSE_YR = @[bse_yr]   
AND HDR.BSE_QTR_CD = @[bse_qtr_cd]   
AND HDR.TRD_CD = @[trd_cd] AND A.DIR_CD = @[dir_cd]   
AND HDR.DAT_CRE_STEP_CD = @[dat_cre_step_cd]  
AND HDR.TGT_ORZ_CD = @[tgt_orz_cd]  
AND A.BSE_YR = HDR.BSE_YR 
AND A.BSE_QTR_CD = HDR.BSE_QTR_CD 
AND A.SAQ_TGT_GRP_CD = HDR.SAQ_TGT_GRP_CD 
AND A.GLINE_VER_NO = HDR.GLINE_VER_NO 
AND A.TRD_CD = HDR.TRD_CD 
AND A.DIR_CD = HDR.DIR_CD 
AND A.DAT_CRE_STEP_CD = HDR.DAT_CRE_STEP_CD 
AND A.BSE_YR = VVD.BSE_YR  
AND A.BSE_QTR_CD = VVD.BSE_QTR_CD  
AND A.GLINE_VER_NO = VVD.GLINE_VER_NO  
AND A.TRD_CD = VVD.TRD_CD  
AND A.RLANE_CD = VVD.RLANE_CD  
AND A.DIR_CD = VVD.DIR_CD  
AND A.VSL_CD = VVD.VSL_CD  
AND A.SKD_VOY_NO = VVD.SKD_VOY_NO  
AND A.SKD_DIR_CD = VVD.SKD_DIR_CD  
AND B.COST_YR = VVD.BSE_YR  
AND B.COST_WK = VVD.BSE_WK  
ORDER BY B.COST_WK      				]]></sql>
			<params>
				<param name="qta_mst_ver_no" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="dat_cre_step_cd" type="12" value="" out="N"/>
				<param name="tgt_orz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
