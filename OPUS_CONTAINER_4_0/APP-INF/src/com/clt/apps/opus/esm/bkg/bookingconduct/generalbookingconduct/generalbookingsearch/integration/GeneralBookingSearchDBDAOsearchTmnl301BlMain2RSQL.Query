<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchTmnl301BlMain2RSQL">
			<desc><![CDATA[searchTmnl301BlMain2]]></desc>
			<sql><![CDATA[
SELECT 'CANCEL_BIT:'		|| BB.BKG_STS_CD											|| CHR(10)
	|| 'CARGOTYPE:'			|| BB.BKG_CGO_TP_CD											|| CHR(10)
	|| 'DR_IND:'			|| decode(BB.DCGO_FLG,'Y','1','0')							|| CHR(10)
	|| 'RF_IND:'			|| decode(BB.RC_FLG,'Y','1','0')							|| CHR(10)
	|| 'AK_IND:'			|| decode(BB.AWK_CGO_FLG,'Y','1','0') 				 		|| CHR(10)
	|| 'BB_IND:'			|| decode(BB.BB_CGO_FLG,'Y','1','0')						|| CHR(10)
	|| 'HD_IND:'			|| decode(BB.SPCL_HIDE_FLG,'Y','1','0')				 		|| CHR(10)
	|| 'UD_IND:'			|| decode(nvl(length(trim(BB.STWG_CD)),0),0,'0','1')		|| CHR(10)
	|| 'UD_CD:'				|| BB.STWG_CD     									 		|| CHR(10)
	|| 'RD_IND:'			|| decode(BB.RD_CGO_FLG,'Y','1','0')				 		|| CHR(10)
	|| 'FG_IND:'			|| decode(BB.FD_GRD_FLG,'Y','1','0')						|| CHR(10)
	|| 'RF_CA:'				|| RF.CTRL_ATMS_FLG											|| CHR(10)
	|| 'RF_MA:'				|| RF.MODI_ATMS_FLG											|| CHR(10)
	|| 'SOC_IND:'			|| decode(BB.SOC_FLG,'Y','1','0')							|| CHR(10)
	|| 'HANGER_IND:'		|| decode(BB.HNGR_FLG,'Y','1','0')							|| CHR(10)
	|| 'EQSUB_IND:'			|| decode(BB.EQ_SUBST_FLG,'Y','1','0')						|| CHR(10)
	|| 'SALES_OFFICE:'		|| BB.OB_SLS_OFC_CD											|| CHR(10)
	|| 'SALES_NAME:'		|| BKG_SPCLCHAR_CONV_FNC(MSR.SREP_NM,'E')					|| CHR(10)
	|| 'BKG_STF:'	   	 	|| BB.DOC_USR_ID											|| CHR(10)
	|| 'BKG_STF_NAME:'		|| BKG_SPCLCHAR_CONV_FNC(CU.USR_NM,'E')						|| CHR(10)
	|| 'BKG_STF_TEL:'		|| CU.XTN_PHN_NO				                 	       	|| CHR(10)
	|| 'BKG_STF_FAX:'		|| CU.FAX_NO  											    || CHR(10)
	|| 'CONTACT_NAME:'		|| BKG_SPCLCHAR_CONV_FNC(BCP.CNTC_PSON_NM,'E')				|| CHR(10)
	|| 'CONTACT_TEL:'		|| BCP.CNTC_PSON_PHN_NO										|| CHR(10)
	|| 'CONTACT_MOBILE:'	|| BCP.CNTC_PSON_MPHN_NO									|| CHR(10)
	|| 'BOUND_IND:'			|| 'E'														|| CHR(10)
	|| 'REGIONAL_BKGNBR:'	|| BP_PSA.CUST_REF_NO_CTNT									|| CHR(10)
	|| 'CUST_REF_NO:'		|| BB.XTER_BKG_RQST_REF_NO									|| CHR(10)
	|| 'REF_VOYAGE:'	                                                    		    || CHR(10)
	|| 'SO_NO:'				|| BB.TWN_SO_NO											    || CHR(10)
	|| 'BLKSTWG:'   		||BB.BLCK_STWG_CD                           				|| CHR(10)
	|| 'EQPICKDT:'			||TO_CHAR(BB.MTY_PKUP_DT,'RRRRMMDDHH24MI')				    || CHR(10)
    || 'EQPICKDT_GMT:'		||TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR(MTY_PKUP_YD_CD,1,5),BB.MTY_PKUP_DT,'GMT'),'RRRRMMDDHH24MI') || CHR(10)
    || 'EQRTNDT:'	   		||NVL(TO_CHAR(BCT.MNL_SET_DT, 'yyyymmddhh24mi'),NVL(TO_CHAR(BCT.SYS_SET_DT, 'yyyymmddhh24mi'),'')) || CHR(10)
    || 'EQRTNDT_GMT:'		||NVL(TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR(BB.FULL_RTN_YD_CD,1,5),NVL(BCT.MNL_SET_DT,BCT.SYS_SET_DT),'GMT'), 'yyyymmddhh24mi'),'') || CHR(10)
	|| 'EQRTN:'	    		||BB.FULL_RTN_YD_CD						                    || CHR(10)
    || 'EQRTN_AMSQUAL:'	    || (SELECT DECODE(LENGTH(ML.LOC_AMS_PORT_CD), 4, 'D', 5, 'K', ' ') FROM MDM_LOCATION ML WHERE ML.LOC_CD = SUBSTR(BB.FULL_RTN_YD_CD,1,5))	|| CHR(10)
    || 'EQRTN_AMSPORT:'	    || (SELECT ML.LOC_AMS_PORT_CD FROM MDM_LOCATION ML WHERE ML.LOC_CD = SUBSTR(BB.FULL_RTN_YD_CD,1,5))	|| CHR(10)
	|| 'OLDEQRTN:'			||@[edi_old_rtn]											|| CHR(10)
    || 'OLDEQRTN_AMSQUAL:'	|| (SELECT DECODE(LENGTH(ML.LOC_AMS_PORT_CD), 4, 'D', 5, 'K', ' ') FROM MDM_LOCATION ML WHERE ML.LOC_CD = SUBSTR(@[edi_old_rtn],1,5))	|| CHR(10)
    || 'OLDEQRTN_AMSPORT:'	|| (SELECT ML.LOC_AMS_PORT_CD FROM MDM_LOCATION ML WHERE ML.LOC_CD = SUBSTR(@[edi_old_rtn],1,5))	|| CHR(10)
    || 'FINAL_CFM_DT:'		||(SELECT TO_CHAR(EVNT_DT, 'YYYYMMDDHH24MI')
								 FROM BKG_DOC_PROC_SKD SKD
								WHERE BKG_DOC_PROC_TP_CD = 'CNTCFM'
								  AND SKD.BKG_NO = BB.BKG_NO
								  AND SKD.DOC_PERF_DELT_FLG = 'N')						|| CHR(10)
	|| 'PUCY_CNT:'  		||PU_CY.N1ST_VNDR_CNT_CD									|| CHR(10)
	|| 'PUCY_NM:'   		||BKG_SPCLCHAR_CONV_FNC(replace(replace(PU_CY.YD_NM, chr(13), ' '), chr(10), ''),'E')	|| CHR(10)
	|| 'PUCY_ADDR1:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(PU_CY.yd_addr, chr(13), ' '), chr(10), ' '), 1, ''),'E') || CHR(10)
	|| 'PUCY_ADDR2:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(PU_CY.yd_addr, chr(13), ' '), chr(10), ' '), 2, ''),'E') || CHR(10)
	|| 'PUCY_ADDR3:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(PU_CY.yd_addr, chr(13), ' '), chr(10), ' '), 3, ''),'E') || CHR(10)
	|| 'PUCY_ADDR4:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(PU_CY.yd_addr, chr(13), ' '), chr(10), ' '), 4, ''),'E') || CHR(10)
	|| 'PUCY_ADDR5:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(PU_CY.yd_addr, chr(13), ' '), chr(10), ' '), 5, ''),'E') || CHR(10)
    || 'PUCY_VND:'		    ||''                                                   || CHR(10)
	|| 'RTCY_CNT:'  		||RT_CY.N1ST_VNDR_CNT_CD                                    || CHR(10)
	|| 'RTCY_NM:'   		||BKG_SPCLCHAR_CONV_FNC(replace(replace(RT_CY.YD_NM, chr(13), ' '), chr(10), ''),'E')  || CHR(10)
	|| 'RTCY_ADDR1:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(RT_CY.yd_addr, chr(13), ' '), chr(10), ' '), 1, ''),'E') || CHR(10)
	|| 'RTCY_ADDR2:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(RT_CY.yd_addr, chr(13), ' '), chr(10), ' '), 2, ''),'E') || CHR(10)
	|| 'RTCY_ADDR3:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(RT_CY.yd_addr, chr(13), ' '), chr(10), ' '), 3, ''),'E') || CHR(10)
	|| 'RTCY_ADDR4:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(RT_CY.yd_addr, chr(13), ' '), chr(10), ' '), 4, ''),'E') || CHR(10)
	|| 'RTCY_ADDR5:'		||BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(replace(replace(RT_CY.yd_addr, chr(13), ' '), chr(10), ' '), 5, ''),'E') || CHR(10)
	|| 'BL_PO_NO:'			||replace(replace(BP_PO.CUST_REF_NO_CTNT, chr(13), ''), chr(10), '')   || CHR(10)
	|| 'BL_SI_NO:'  		||BB.XTER_SI_REF_NO											|| CHR(10)
	|| 'DST_TRANS:' 		||CASE WHEN @[edi_type] = '301M' THEN 'T'
                              ELSE (SELECT DECODE(SUBSTR(MA.ACT_NM,1,1),'O','T',SUBSTR(MA.ACT_NM,1,1))
                                     FROM SCE_COP_HDR HDR
                                         ,SCE_COP_DTL DTL
                                         ,MDM_ACTIVITY MA
                                    WHERE HDR.BKG_NO = BB.BKG_NO
                                      AND HDR.COP_NO = DTL.COP_NO
                                      AND DTL.NOD_CD = BB.Full_Rtn_Yd_Cd
                                      AND DTL.ACT_CD = MA.ACT_CD
                                      AND HDR.COP_STS_CD <> 'X'
                                      AND DTL.ACT_CD NOT IN ('FLVMLO','FLVMDO','FORRLO','FORRDO','MITYAD','FITMDO','FOWMDO','FLWMDO','FOWMLO','FLWMLO')
                                      AND DTL.COP_DTL_SEQ = (SELECT max(DTL1.COP_DTL_SEQ) FROM SCE_COP_DTL DTL1 
                                                             WHERE DTL1.COP_NO = DTL.COP_NO AND DTL1.NOD_CD = DTL.NOD_CD 
                                                             AND DTL1.ACT_CD NOT IN ('FLVMLO','FLVMDO','FORRLO','FORRDO','MITYAD','FITMDO','FOWMDO','FLWMDO','FOWMLO','FLWMLO'))
                                      AND ROWNUM = 1	)	END								|| CHR(10)
	|| 'BKG_FLEX_IND:' 		||NVL(decode(BB.FLEX_HGT_FLG,'Y','1','0'),'0')				|| CHR(10) BL_MAIN2
FROM	BKG_BOOKING	BB,
        BKG_VVD VVD,
        VSK_VSL_PORT_SKD VVPS1,
		BKG_RF_CGO RF, 
		COM_USER CU, 
		MDM_YARD PU_CY, 
		MDM_YARD RT_CY,
		BKG_CNTC_PSON BCP, 
		BKG_REFERENCE BP_PSA, 
		BKG_REFERENCE BP_PO,
		MDM_SLS_REP MSR,
		BKG_CLZ_TM BCT
WHERE	BB.BKG_NO	   = @[bkg_no]
AND     BB.BKG_NO          = VVD.BKG_NO(+)
AND     BB.POL_CD          = VVD.POL_CD(+)	
AND     BB.BKG_NO          = RF.BKG_NO(+)	
AND     BB.BKG_NO          = BCP.BKG_NO(+)
AND     BCP.BKG_CNTC_PSON_TP_CD(+) = 'BK'	
AND     BB.BKG_NO          = BP_PSA.BKG_NO(+)
AND     BP_PSA.bkg_ref_tp_cd(+) = 'PSAN'
AND     BB.BKG_NO          = BP_PO.BKG_NO(+)
AND     BP_PO.bkg_ref_tp_cd(+) = 'BKPO'	
AND     VVD.VSL_CD		   = VVPS1.VSL_CD(+)
AND     VVD.SKD_VOY_NO	   = VVPS1.SKD_VOY_NO(+)
AND     VVD.SKD_DIR_CD	   = VVPS1.SKD_DIR_CD(+)
AND     VVD.POL_CD 		   = VVPS1.VPS_PORT_CD(+)
AND     VVD.POL_CLPT_IND_SEQ = VVPS1.CLPT_IND_SEQ(+)
AND     UPPER(BB.DOC_USR_ID) = UPPER(CU.USR_ID(+))
AND     BB.OB_SREP_CD   = MSR.SREP_CD(+)
AND     BB.MTY_PKUP_YD_CD = PU_CY.yd_cd(+)
AND     BB.FULL_RTN_YD_CD = RT_CY.yd_cd(+)
AND     BB.BKG_NO         = BCT.BKG_NO(+)
AND     BCT.CLZ_TP_CD(+)  = 'R'			]]></sql>
			<params>
				<param name="edi_old_rtn" type="12" value="" out="N"/>
				<param name="edi_type" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="SEL900684700" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
