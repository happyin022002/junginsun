<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLaneBoundSwitchTradeListRSQL">
			<desc><![CDATA[Lane 목록 조회]]></desc>
			<sql><![CDATA[
SELECT TRD_CD AS TEXT, TRD_CD AS CODE  
FROM SAQ_TGT_GRP_TRD_V                
WHERE SAQ_TGT_GRP_CD LIKE @[trade_group] || '%'     
ORDER BY TRD_CD           			]]></sql>
			<params>
				<param name="trade_group" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
