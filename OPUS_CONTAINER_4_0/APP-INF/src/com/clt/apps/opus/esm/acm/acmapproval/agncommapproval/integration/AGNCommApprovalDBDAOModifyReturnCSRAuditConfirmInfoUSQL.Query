<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommApprovalDBDAOModifyReturnCSRAuditConfirmInfoUSQL">
			<desc><![CDATA[ACM_AGN_COMM에 aud_no 를 업데이트 한다.]]></desc>
			<sql><![CDATA[
UPDATE ACM_AGN_COMM A
SET
 AC_STS_CD       = 'AS'
,AUD_NO          = @[aud_no]
,AUD_DT          = SYSDATE
,AUD_GDT         = GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS', SYSDATE, 'GMT')
,ACCL_FLG        = 'N'
,CSR_NO          = NULL
,APRO_USR_ID     = NULL
,APRO_DT         = NULL
,APRO_GDT        = NULL
,GL_DT           = NULL
,ASA_NO          = NULL
,INV_TAX_RT      = NULL
,IF_USR_ID       = NULL
,IF_DT           = NULL
,IF_GDT          = NULL
,AC_PROC_DESC    = NULL
,UPD_USR_ID      = @[usr_id]
,UPD_DT          = SYSDATE

WHERE CSR_NO     = @[csr_no]
AND BKG_NO NOT IN 
(
    SELECT DISTINCT A.BKG_NO FROM BKG_BOOKING B , ACM_AGN_COMM A
    WHERE 1=1
    AND A.CSR_NO = @[csr_no]
    AND A.AGN_CD = SUBSTR(@[csr_no],4,5)
    AND A.BKG_NO = B.BKG_NO
    AND B.BKG_STS_CD IN ('X','A')
)
AND BKG_NO NOT IN (
    SELECT DISTINCT A.BKG_NO FROM BKG_VVD V , ACM_AGN_COMM A
    WHERE 1=1
    AND A.CSR_NO = @[csr_no]
    AND A.AGN_CD = SUBSTR(@[csr_no],4,5)
    AND A.BKG_NO = V.BKG_NO
    AND V.VSL_CD IN ('HJXX','HJYY','HJZZ')
)			]]></sql>
			<params>
				<param name="aud_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
