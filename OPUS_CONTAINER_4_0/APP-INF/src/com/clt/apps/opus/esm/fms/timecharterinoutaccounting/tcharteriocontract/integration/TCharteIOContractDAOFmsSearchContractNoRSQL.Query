<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharteIOContractDAOFmsSearchContractNoRSQL">
			<desc><![CDATA[aa]]></desc>
			<sql><![CDATA[
SELECT MVC.VSL_CD,
	   FC.FLET_CTRT_NO, 
 	   MVC.VSL_ENG_NM 
  FROM FMS_CONTRACT FC, MDM_VSL_CNTR MVC 
 WHERE FC.VSL_CD = MVC.VSL_CD
   AND FC.FLET_CTRT_TP_CD = @[flet_ctrt_tp_cd]
   AND FC.VSL_CD LIKE '%' || @[vsl_cd] || '%'
   AND NVL(FC.DELT_FLG, 'N') = 'N'
   AND MVC.DELT_FLG = 'N'
#if(${ctrt_flag} != 'Y')
   AND FLET_CTRT_FACT_CD = 'ACT'
#end
 ORDER BY FC.CRE_DT DESC			]]></sql>
			<params>
				<param name="flet_ctrt_tp_cd" type="12" value="TO" out="N"/>
				<param name="vsl_cd" type="12" value="HJAA" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
