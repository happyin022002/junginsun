<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesAdminCommonDBDAOSearchActItemDtlListRSQL">
			<desc><![CDATA[Sub type 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT SLS_ACT_TP_CD
	   , SLS_ACT_SUB_TP_CD
	   , SLS_ACT_SUB_TP_DESC
FROM SAM_SLS_ACT_TP_DTL
WHERE SLS_ACT_TP_CD = @[sls_act_tp_cd]
ORDER BY SLS_ACT_SUB_TP_CD			]]></sql>
			<params>
				<param name="sls_act_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
