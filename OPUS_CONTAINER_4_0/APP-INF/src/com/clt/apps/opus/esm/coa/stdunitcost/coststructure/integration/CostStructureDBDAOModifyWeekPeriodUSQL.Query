<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOModifyWeekPeriodUSQL">
			<desc><![CDATA[ModifyWeekPeriod]]></desc>
			<sql><![CDATA[
UPDATE COA_WK_PRD SET 
	SLS_FM_DT = @[sls_fm_dt],
	SLS_TO_DT = @[sls_to_dt],
	EQ_WK = @[eq_wk],
	UPD_USR_ID = @[upd_usr_id],
	UPD_DT = SYSDATE
WHERE 1 = 1
	AND COST_YR = @[cost_yr]
	AND COST_WK = @[cost_wk]			]]></sql>
			<params>
				<param name="sls_fm_dt" type="12" value="" out="N"/>
				<param name="sls_to_dt" type="12" value="" out="N"/>
				<param name="eq_wk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yr" type="12" value="" out="N"/>
				<param name="cost_wk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
