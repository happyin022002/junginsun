<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchEdoTransLogRSQL">
			<desc><![CDATA[UI_BKG_0134 화면에서 사용하는 SQL]]></desc>
			<sql><![CDATA[
SELECT A.EDO_SEQ,
       B.BL_NO,
       A.MF_REF_NO,
       A.MF_SEQ_NO,
       A.EDO_RSLT_CD,
       A.ACK_MSG,
       A.EDO_RCV_DT
  FROM BKG_EDO_LOG A,
       BKG_BOOKING B
 WHERE A.BKG_NO = B.BKG_NO

#if (${rcv_fm_dt} != '')
   AND A.EDO_RCV_DT BETWEEN TO_DATE(@[rcv_fm_dt], 'YYYYMMDD') 
                       AND (TO_DATE(@[rcv_to_dt], 'YYYYMMDD') + 1)
#end

#if (${bl_no} != '')
   AND B.BL_NO  = @[bl_no]
#end			]]></sql>
			<params>
				<param name="rcv_fm_dt" type="12" value="" out="N"/>
				<param name="rcv_to_dt" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
