<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOsearchPkupNoVerifyResultListRSQL">
			<desc><![CDATA[철송 회사로 부터 e-mail을 받은 pick-up No upload 결과를 검증(Verify) 한다]]></desc>
			<sql><![CDATA[
-- PkupNoRptVO 생성
SELECT 
	@[chk] AS CHK
,	@[cntr_no] AS CNTR_NO
,	@[bl_no] AS BL_NO
,	@[pkup_no] AS PKUP_NO
,	@[pkup_aval_dt] AS PKUP_AVAL_DT
,	@[lst_free_dt] AS LST_FREE_DT
,	@[pkup_yd_cd] AS PKUP_YD_CD
,	@[remark] AS REMARK
,	DECODE(CNTR_USE_FLG, 0, 'N', 'Y')  AS CNTR_NO_CHK_FLG
,	DECODE(CNTR_USE_FLG, 0, 'Invalid~!', 'Clear~!')  AS CNTR_NO_CHK_MSG
,	DECODE(BL_CHECK_FLG, 0, 'N', 'Y')  AS BL_NO_CHK_FLG
,	DECODE(BL_CHECK_FLG, 0, 'Invalid~!', 'Clear~!')  AS BL_NO_CHK_MSG
,	DECODE(YD_CHECK_FLG, 0, 'N', 'Y')  AS PKUP_YD_CD_CHK_FLG
,	DECODE(YD_CHECK_FLG, 0, 'Invalid~!', 'Clear~!')  AS PKUP_YD_CD_CHK_MSG
,	DECODE(AVL_CHECK_FLG, 0, 'N', 'Y') AS PKUP_AVAL_DT_CHK_FLG
,	DECODE(AVL_CHECK_FLG, 0, 'Invalid~!', 'Clear~!') AS PKUP_AVAL_DT_CHK_MSG
,	DECODE(FRE_CHECK_FLG, 0, 'N', 'Y') AS LST_FREE_DT_CHK_FLG
,	DECODE(FRE_CHECK_FLG, 0, 'Invalid~!', 'Clear~!') AS LST_FREE_DT_CHK_MSG
  FROM (SELECT NVL(COUNT(1), 0) AS CNTR_USE_FLG
          FROM TRS_TRSP_RAIL_BIL_ORD
         WHERE BL_NO = @[bl_no]
           AND EQ_NO LIKE @[cntr_no] || '%'
           AND TRSP_BND_CD  = 'I'
           AND DELT_FLG     = 'N'
       ),
       (SELECT NVL(COUNT(1), 0) AS BL_CHECK_FLG
          FROM BKG_BOOKING   A
              ,(
                SELECT A.BKG_NO
                  FROM (
                        SELECT A.*
                              ,MAX(A.CNMV_CYC_NO) OVER() AS MAX_CYC_NO
                          FROM BKG_CONTAINER A
                              ,BKG_BOOKING   B
                         WHERE A.CNTR_NO = 
                               (SELECT EQ_NO
                                  FROM TRS_TRSP_RAIL_BIL_ORD
                                 WHERE BL_NO        = @[bl_no]
                                   AND EQ_NO        LIKE @[cntr_no] || '%'
                                   AND TRSP_BND_CD  = 'I'
                                   AND DELT_FLG     = 'N'
                                   AND ROWNUM       = 1
                               )  
                           AND A.BKG_NO = B.BKG_NO
                           AND B.BKG_STS_CD <> 'X'                               
                       ) A
                 WHERE A.MAX_CYC_NO = A.CNMV_CYC_NO
               ) B
         WHERE A.BL_NO              = @[bl_no]
           AND A.BKG_CGO_TP_CD      IN ('F','R','B')
         --  AND A.DEL_CD             LIKE 'US%'
           AND SUBSTR(A.BKG_NO,1,4) <> 'DCHI'
           AND SUBSTR(A.BKG_NO,1,4) <> 'TCHI'    
           AND B.BKG_NO             = A.BKG_NO
       ),
       (SELECT NVL(COUNT(1), 0) AS YD_CHECK_FLG
          FROM MDM_YARD
         WHERE YD_CD = @[pkup_yd_cd]),
       (SELECT NVL(COUNT(@[pkup_aval_dt]), 0) AS AVL_CHECK_FLG
          FROM DUAL),
       (SELECT NVL(COUNT(@[lst_free_dt]), 0) AS FRE_CHECK_FLG
          FROM DUAL)			]]></sql>
			<params>
				<param name="chk" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="pkup_no" type="12" value="" out="N"/>
				<param name="pkup_aval_dt" type="12" value="" out="N"/>
				<param name="lst_free_dt" type="12" value="" out="N"/>
				<param name="pkup_yd_cd" type="12" value="" out="N"/>
				<param name="remark" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
