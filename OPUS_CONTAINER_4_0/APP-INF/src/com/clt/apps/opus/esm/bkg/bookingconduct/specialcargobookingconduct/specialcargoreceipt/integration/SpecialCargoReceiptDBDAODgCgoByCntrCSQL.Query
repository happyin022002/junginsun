<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAODgCgoByCntrCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_DG_CGO
            (BKG_NO
,            DCGO_SEQ
,            DG_CNTR_SEQ
,            CNTR_CGO_SEQ
,            CNTR_NO
,            CNTR_TPSZ_CD
,            IMDG_UN_NO
,            IMDG_UN_NO_SEQ
,            IMDG_CLSS_CD
,            IMDG_SUBS_RSK_LBL_CD1
,            IMDG_SUBS_RSK_LBL_CD2
,            IMDG_SUBS_RSK_LBL_CD3
,            IMDG_LMT_QTY_FLG
,            IMDG_EXPT_QTY_FLG
,            NET_WGT
,            GRS_WGT
,            WGT_UT_CD
,            FLSH_PNT_CDO_TEMP
,            PRP_SHP_NM
,            HZD_DESC
,            MEAS_QTY
,            MEAS_UT_CD
,            CLOD_FLG
,            SPCL_STWG_RQST_DESC
,            DCGO_STS_CD
,            CGO_LCL_FLG
,            EMER_RSPN_GID_NO
,            EMER_RSPN_GID_CHR_NO
,            EMER_CNTC_PHN_NO_CTNT
,            EMER_CNTC_PSON_NM
,            EMER_TEMP_CTNT
,            CTRL_TEMP_CTNT
,            EMS_NO
,            IMDG_PCK_GRP_CD
,            MRN_POLUT_FLG
,            PSA_NO
,            CERTI_NO
,            IN_IMDG_PCK_QTY1
,            IN_IMDG_PCK_CD1
,            IN_IMDG_PCK_QTY2
,            IN_IMDG_PCK_CD2
,            OUT_IMDG_PCK_QTY1
,            OUT_IMDG_PCK_CD1
,            OUT_IMDG_PCK_QTY2
,            OUT_IMDG_PCK_CD2
,            MAX_IN_PCK_QTY
,            MAX_IN_PCK_TP_CD
,            CNEE_DTL_DESC
,            NET_EXPLO_WGT
,            RADA_SKD_NO
,            RADA_AMT
,            RADA_UT_CD
,            RADA_TRSP_NO
,            RC_FLG
,            AWK_CGO_FLG
,            BB_CGO_FLG
,            RC_SEQ
,            AWK_CGO_SEQ
,            BB_CGO_SEQ
,            HCDG_FLG
,            HCDG_DPND_QTY_FLG
,            RQST_DT
,            RQST_USR_ID
,            APLY_NO
,            SPCL_CGO_APRO_CD
,            DIFF_RMK
,            CRE_USR_ID
,            CRE_DT
,            UPD_USR_ID
,            UPD_DT
,            IMDG_COMP_GRP_CD
,            IMDG_SUBS_RSK_LBL_CD4
,            CNTR_VOL_QTY
,            DCGO_REF_NO
,            IMDG_AMDT_NO
            )
    SELECT @[tgt_bkg_no]
,          (
			SELECT NVL(MAX(DCGO_SEQ), 0)
            FROM   BKG_DG_CGO
            WHERE  BKG_NO = @[tgt_bkg_no]) + ROW_NUMBER() OVER (PARTITION BY BKG_NO ORDER BY DCGO_SEQ 
			) AS DCGO_SEQ
,          ((
			SELECT NVL(MAX(DG_CNTR_SEQ), 0)
            FROM   BKG_DG_CGO
            WHERE  BKG_NO = @[tgt_bkg_no]) + 1 
			) AS DG_CNTR_SEQ
,          CNTR_CGO_SEQ
,          CNTR_NO
,          CNTR_TPSZ_CD
,          IMDG_UN_NO
,          IMDG_UN_NO_SEQ
,          IMDG_CLSS_CD
,          IMDG_SUBS_RSK_LBL_CD1
,          IMDG_SUBS_RSK_LBL_CD2
,          IMDG_SUBS_RSK_LBL_CD3
,          IMDG_LMT_QTY_FLG
,          IMDG_EXPT_QTY_FLG
,          NET_WGT
,          GRS_WGT
,          WGT_UT_CD
,          FLSH_PNT_CDO_TEMP
,          PRP_SHP_NM
,          HZD_DESC
,          MEAS_QTY
,          MEAS_UT_CD
,          CLOD_FLG
,          SPCL_STWG_RQST_DESC
,          DCGO_STS_CD
,          CGO_LCL_FLG
,          EMER_RSPN_GID_NO
,          EMER_RSPN_GID_CHR_NO
,          EMER_CNTC_PHN_NO_CTNT
,          EMER_CNTC_PSON_NM
,          EMER_TEMP_CTNT
,          CTRL_TEMP_CTNT
,          EMS_NO
,          IMDG_PCK_GRP_CD
,          MRN_POLUT_FLG
,          PSA_NO
,          CERTI_NO
,          IN_IMDG_PCK_QTY1
,          IN_IMDG_PCK_CD1
,          IN_IMDG_PCK_QTY2
,          IN_IMDG_PCK_CD2
,          OUT_IMDG_PCK_QTY1
,          OUT_IMDG_PCK_CD1
,          OUT_IMDG_PCK_QTY2
,          OUT_IMDG_PCK_CD2
,          MAX_IN_PCK_QTY
,          MAX_IN_PCK_TP_CD
,          CNEE_DTL_DESC
,          NET_EXPLO_WGT
,          RADA_SKD_NO
,          RADA_AMT
,          RADA_UT_CD
,          RADA_TRSP_NO
,          RC_FLG
,          AWK_CGO_FLG
,          BB_CGO_FLG
,          RC_SEQ
,          AWK_CGO_SEQ
,          BB_CGO_SEQ
,          HCDG_FLG
,          HCDG_DPND_QTY_FLG
,          RQST_DT
,          RQST_USR_ID
,          APLY_NO
,          SPCL_CGO_APRO_CD
,          DIFF_RMK
,          @[cre_usr_id]
,          SYSDATE
,          @[cre_usr_id]
,          SYSDATE
,          IMDG_COMP_GRP_CD
,          IMDG_SUBS_RSK_LBL_CD4
,          @[tgt_cntr_vol]
#if(${copy_mode_cd} != 'M')
,          BKG_COMMON_PKG.BKG_NO_GEN_FNC('DGN', @[ofc_cd], @[cre_usr_id])
#else
,          DCGO_REF_NO
#end
,          IMDG_AMDT_NO
    FROM   BKG_DG_CGO
    WHERE  BKG_NO = @[src_bkg_no]
    AND    CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="tgt_bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="tgt_cntr_vol" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="src_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
