<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOBkgRevDrAmtVOAddSearchCSQL">
			<desc><![CDATA[amt 의 최근값을 조회하여 신규로 등록]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_REV_DR_AMT (
	RDN_NO
,	RVIS_SEQ
,	CURR_CD
,	DR_AMT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT 
	RDN_NO
,	(RVIS_SEQ+1) AS RVIS_SEQ
,	CURR_CD
,	DR_AMT
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM  BKG_REV_DR_AMT
WHERE RDN_NO = @[rdn_no]
AND	  RVIS_SEQ = (SELECT MAX(RVIS_SEQ)
					FROM BKG_REV_DR_AMT
				   WHERE RDN_NO = @[rdn_no])			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rdn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
