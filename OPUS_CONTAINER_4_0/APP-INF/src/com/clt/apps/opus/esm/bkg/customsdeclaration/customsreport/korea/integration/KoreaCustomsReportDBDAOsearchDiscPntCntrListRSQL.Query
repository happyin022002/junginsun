<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchDiscPntCntrListRSQL">
			<desc><![CDATA[CNTR List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(CNTR_NO,' ') CNTR_NO
  FROM BKG_CONTAINER 
 WHERE BKG_NO = @[bkg_no]
 ORDER BY CNTR_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
