<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOCreateLaneAdjustCSQL">
			<desc><![CDATA[기존 rlane 에 해당되는 Data를 새로운 rlane Data로 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_MON_FCAST_TRNS 
       ( 
          MQTA_MDL_VER_NO 
         ,SLS_FCAST_NO 
         ,ALTN_FCAST_SEQ 
         ,RLANE_CD 
         ,DIR_CD 
         ,VSL_CD 
         ,SKD_VOY_NO 
         ,SKD_DIR_CD 
         ,CUST_CNT_CD 
         ,CUST_SEQ 
         ,CTRT_OFC_CD 
         ,SLS_OFC_CD 
         ,POR_CD 
         ,DEL_CD 
         ,N1ST_POL_CD 
         ,LST_POD_CD 
         ,TRNK_POL_CD 
         ,TRNK_POD_CD 
         ,FCAST_CNTR_TPSZ_CD 
         ,LOD_QTY 
         ,CNTR_WGT 
         ,GRS_RPB_REV 
         ,CM_UC_AMT 
         ,OPFIT_UC_AMT 
         ,RA_CM_UC_AMT 
         ,RA_STP_PFIT_UT_AMT 
         ,RA_OPFIT_UC_AMT 
         ,MDL_RSLT_FLG 
         ,MDL_ALOC_QTY 
         ,REP_TRD_CD 
         ,REP_SUB_TRD_CD 
         ,TRD_CD 
         ,SUB_TRD_CD 
         ,IOC_CD 
         ,CTRT_RHQ_CD 
         ,CTRT_AQ_CD 
         ,CTRT_RGN_OFC_CD 
         ,CTRT_SREP_CD 
         ,SLS_RHQ_CD 
         ,SLS_AQ_CD 
         ,SLS_RGN_OFC_CD 
         ,SREP_CD 
         ,COST_ASGN_STEP_CD 
         ,LGS_COST_ASGN_STEP_CD 
         ,SVC_MOD_CD 
         ,SAQ_SVC_MOD_CD 
         ,FULL_STVG_UC_AMT 
         ,FULL_TRSP_UC_AMT 
         ,MTY_STVG_UC_AMT 
         ,MTY_TRSP_UC_AMT 
         ,CNTR_FX_UC_AMT 
         ,CHSS_FX_UC_AMT 
         ,AGN_COMM_UT_AMT 
         ,BIZ_ACT_UC_AMT 
         ,SLT_MGMT_UC_AMT 
         ,OWN_VOL_ACT_UC_AMT 
         ,STP_UC_AMT 
         ,EQ_HLD_UC_AMT 
         ,EQ_REPO_UC_AMT 
         ,EQ_SIM_UC_AMT 
         ,DMDT_UT_AMT 
         ,TML_VOL_INCNT_AMT 
         ,SAQ_MISC_REV_AMT 
         ,CUST_GRP_ID 
         ,DMND_FCAST_UT_NM 
         ,DMND_FCAST_GRP_NM 
         ,DMND_FCAST_EQ_TPSZ_NO 
         ,ST_DT 
         ,CRE_USR_ID 
         ,CRE_DT 
         ,UPD_USR_ID 
         ,UPD_DT 
         ,ORG_LOD_QTY 
         ,ORG_GRS_RPB_REV 
         ,ORG_CM_UC_AMT 
         ,FCAST_TRNS_STS_CD 
       ) 
SELECT MQTA_MDL_VER_NO 
     ,
       (SELECT MAX(SLS_FCAST_NO)
         FROM SAQ_MON_FCAST_TRNS 
        WHERE 1=1 
              AND MQTA_MDL_VER_NO = @[mqta_mdl_ver_no] 
       ) + ROWNUM AS SLS_FCAST_NO 
     ,ALTN_FCAST_SEQ 
     ,@[org_rlane_cd] AS RLANE_CD 
     ,DIR_CD 
     ,VSL_CD 
     ,SKD_VOY_NO 
     ,SKD_DIR_CD 
     ,CUST_CNT_CD 
     ,CUST_SEQ 
     ,CTRT_OFC_CD 
     ,SLS_OFC_CD 
     ,POR_CD 
     ,DEL_CD 
     ,N1ST_POL_CD 
     ,LST_POD_CD 
     ,TRNK_POL_CD 
     ,TRNK_POD_CD 
     ,FCAST_CNTR_TPSZ_CD 
     ,LOD_QTY 
     ,CNTR_WGT 
     ,GRS_RPB_REV 
     ,CM_UC_AMT 
     ,OPFIT_UC_AMT 
     ,RA_CM_UC_AMT 
     ,RA_STP_PFIT_UT_AMT 
     ,RA_OPFIT_UC_AMT 
     ,MDL_RSLT_FLG 
     ,MDL_ALOC_QTY 
     ,REP_TRD_CD 
     ,REP_SUB_TRD_CD 
     ,TRD_CD 
     ,SUB_TRD_CD 
     ,IOC_CD 
     ,CTRT_RHQ_CD 
     ,CTRT_AQ_CD 
     ,CTRT_RGN_OFC_CD 
     ,CTRT_SREP_CD 
     ,SLS_RHQ_CD 
     ,SLS_AQ_CD 
     ,SLS_RGN_OFC_CD 
     ,SREP_CD 
     ,COST_ASGN_STEP_CD 
     ,LGS_COST_ASGN_STEP_CD 
     ,SVC_MOD_CD 
     ,SAQ_SVC_MOD_CD 
     ,FULL_STVG_UC_AMT 
     ,FULL_TRSP_UC_AMT 
     ,MTY_STVG_UC_AMT 
     ,MTY_TRSP_UC_AMT 
     ,CNTR_FX_UC_AMT 
     ,CHSS_FX_UC_AMT 
     ,AGN_COMM_UT_AMT 
     ,BIZ_ACT_UC_AMT 
     ,SLT_MGMT_UC_AMT 
     ,OWN_VOL_ACT_UC_AMT 
     ,STP_UC_AMT 
     ,EQ_HLD_UC_AMT 
     ,EQ_REPO_UC_AMT 
     ,EQ_SIM_UC_AMT 
     ,DMDT_UT_AMT 
     ,TML_VOL_INCNT_AMT 
     ,SAQ_MISC_REV_AMT 
     ,CUST_GRP_ID 
     ,DMND_FCAST_UT_NM 
     ,DMND_FCAST_GRP_NM 
     ,DMND_FCAST_EQ_TPSZ_NO 
     ,ST_DT 
     ,CRE_USR_ID 
     ,CRE_DT 
     ,@[user_id] AS UPD_USR_ID 
     ,SYSDATE AS UPD_DT 
     ,ORG_LOD_QTY 
     ,ORG_GRS_RPB_REV 
     ,ORG_CM_UC_AMT 
     ,'0' AS FCAST_TRNS_STS_CD 
  FROM SAQ_MON_FCAST_TRNS 
 WHERE 1=1 
       AND MQTA_MDL_VER_NO = @[mqta_mdl_ver_no] 
       AND TRD_CD = @[trd_cd] 
       AND SUB_TRD_CD = @[sub_trd_cd] 
       AND RLANE_CD = @[rlane_cd]			]]></sql>
			<params>
				<param name="mqta_mdl_ver_no" type="12" value="" out="N"/>
				<param name="org_rlane_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
