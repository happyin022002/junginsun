<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiCntrPoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT '{MULTI_PO_NO' || CHR (10) || 
       'CNTR_PO_NO:' || CUST_REF_NO_CTNT || CHR (10) || 
       '}MULTI_PO_NO' || CHR (10)
FROM   BKG_REFERENCE
WHERE  BKG_REF_TP_CD = 'CTPO'
   AND BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
