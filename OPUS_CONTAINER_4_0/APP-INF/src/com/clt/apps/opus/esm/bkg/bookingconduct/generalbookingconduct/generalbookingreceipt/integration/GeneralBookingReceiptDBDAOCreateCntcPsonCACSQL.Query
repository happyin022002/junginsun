<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOCreateCntcPsonCACSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOCreateCntcPsonCACSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_CNTC_PSON (
    BKG_NO 
#else
INSERT INTO BKG_CNTC_PSON_HIS (
    BKG_NO 
    , CORR_NO 
#end
    , BKG_CNTC_PSON_TP_CD 
    , CNTC_PSON_NM 
    , CNTC_PSON_PHN_NO 
    , CNTC_PSON_MPHN_NO 
    , CNTC_PSON_FAX_NO 
    , CNTC_PSON_EML 
    , DIFF_RMK 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
        , BKG_CNTC_PSON_TP_CD 
        , CNTC_PSON_NM 
        , CNTC_PSON_PHN_NO 
        , CNTC_PSON_MPHN_NO 
        , CNTC_PSON_FAX_NO 
        , CNTC_PSON_EML 
        , DIFF_RMK 
        , CRE_USR_ID 
        , CRE_DT 
        , UPD_USR_ID 
        , sysdate
#if (${copy_type_cd} == 'BKG')
  FROM BKG_CNTC_PSON_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_CNTC_PSON
 WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
