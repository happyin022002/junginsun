<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchMakeHeaderPRSQL">
			<desc><![CDATA[Flat File Header 생성]]></desc>
			<sql><![CDATA[
SELECT '$$$MSGSTART:'||
    RPAD(NVL(NVL(DECODE(@[pod_cd],'AUBNE','HSABNE','AUMEL','HSAMEL','AUSYD','HSASYD'),DECODE(@[pol_cd],'AUBNE','HSABNE','AUMEL','HSAMEL','AUSYD','HSASYD',COM_ConstantMgr_PKG.COM_getScacCode_FNC())),' '),20,' ')||
    RPAD(NVL(NVL(DECODE(@[pod_cd],'AUBNE','BPC','AUMEL','MPC','AUSYD','SPC'),DECODE(@[pol_cd],'AUBNE','BPC','AUMEL','MPC','AUSYD','SPC','AUSCUS')),' '),20,' ')||
    RPAD(NVL(NVL(DECODE(@[pod_cd],'AUBNE','IFCSUM','AUMEL','IFCSUM','AUSYD','IFCSUM'),DECODE(@[pol_cd],'AUBNE','IFCSUM','AUMEL','IFCSUM','AUSYD','IFCSUM','IFTMCS')),' '),10,' ')||
    'BKC'||
    TO_CHAR(sysdate,'rrmmdd')||
			LTRIM(TO_CHAR(BKG_CSTM_EDI_SEQ.NEXTVAL,'000009'),' ') msg_header	
FROM DUAL			]]></sql>
			<params>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
