<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRatesDBDAOPriTrfInlndAmendVOCSQL">
			<desc><![CDATA[Inland Amend]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_INLND (
       TRF_PFX_CD
     , TRF_NO
     , TRF_INLND_SEQ
     , AMDT_SEQ
     , TRF_INLND_NM
	 , TRF_INLND_AMDT_TP_CD
     , EFF_DT
     , EXP_DT
     , PUB_DT
     , RQST_OFC_CD
     , APRO_OFC_CD
     , TRF_INLND_STS_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
  	 , ATCH_FILE_ID
)

SELECT TRF_PFX_CD
     , TRF_NO
     , TRF_INLND_SEQ
     , AMDT_SEQ + 1
     , TRF_INLND_NM
	 , NULL
     , TO_DATE(@[eff_dt], 'YYYYMMDD')
     , EXP_DT
     , NULL
     , @[rqst_ofc_cd]
     , APRO_OFC_CD
     , 'I'
     , @[cre_usr_id]
     , SYSDATE
     , @[cre_usr_id]
     , SYSDATE
  	 , ATCH_FILE_ID
  FROM PRI_TRF_INLND
 WHERE TRF_PFX_CD		= @[trf_pfx_cd]
   AND TRF_NO			= @[trf_no]
   AND AMDT_SEQ			= @[amdt_seq] - 1
   AND TRF_INLND_SEQ	= @[trf_inlnd_seq]			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="trf_inlnd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
