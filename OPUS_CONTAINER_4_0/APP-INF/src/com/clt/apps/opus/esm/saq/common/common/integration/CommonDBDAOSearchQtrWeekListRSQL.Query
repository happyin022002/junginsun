<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchQtrWeekListRSQL">
			<desc><![CDATA[해당년도, 분기의 주차 가져오기]]></desc>
			<sql><![CDATA[
SELECT COST_WK AS CODE, 
       COST_WK AS TEXT  
FROM   COA_WK_PRD 
WHERE  COST_YR    = @[year] 
AND    ( 
        (SLS_FM_DT >= @[year]||DECODE(@[quarter], '1Q', '01', '2Q', '04', '3Q', '07', '4Q', '10')||'01' AND 
         SLS_FM_DT <= TO_CHAR(LAST_DAY(TO_DATE(@[year]||DECODE(@[quarter], '1Q', '03', '2Q', '06', '3Q', '09', '4Q', '12'), 'YYYYMM')), 'YYYYMMDD')) OR 
        (SLS_TO_DT >= @[year]||DECODE(@[quarter], '1Q', '01', '2Q', '04', '3Q', '07', '4Q', '10')||'01' AND 
         SLS_TO_DT <= TO_CHAR(LAST_DAY(TO_DATE(@[year]||DECODE(@[quarter], '1Q', '03', '2Q', '06', '3Q', '09', '4Q', '12'), 'YYYYMM')), 'YYYYMMDD'))    
       ) 
AND    NVL(@[bseMon], '00') BETWEEN DECODE(NVL(@[bseMon], '00'), '00', '00', SUBSTR(SLS_FM_DT, 5, 2))  
                    AND     DECODE(NVL(@[bseMon], '00'), '00', '00', SUBSTR(SLS_TO_DT, 5, 2))  
			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="quarter" type="12" value="" out="N"/>
				<param name="bseMon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
