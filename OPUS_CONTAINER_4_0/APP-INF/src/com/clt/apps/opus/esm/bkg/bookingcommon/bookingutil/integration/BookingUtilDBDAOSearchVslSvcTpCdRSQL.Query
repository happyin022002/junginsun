<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchVslSvcTpCdRSQL">
			<desc><![CDATA[VVD의 service type code를 조회한다(cca feeder 구분용).]]></desc>
			<sql><![CDATA[
SELECT VSL_SVC_TP_CD RSLT
  FROM MDM_VSL_SVC_LANE
 WHERE VSL_SLAN_CD = (SELECT VSL_SLAN_CD
						FROM VSK_VSL_SKD
					   WHERE VSL_CD     = SUBSTR(@[vvd], 1, 4)
                         AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                         AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
					     AND rownum = 1)			]]></sql>
			<params>
				<param name="vvd" type="12" value="HJPO0001E" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
