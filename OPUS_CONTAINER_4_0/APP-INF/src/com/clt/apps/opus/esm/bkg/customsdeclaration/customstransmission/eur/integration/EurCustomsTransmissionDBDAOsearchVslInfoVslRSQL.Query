<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchVslInfoVslRSQL">
			<desc><![CDATA[Vessel ETA 정보 조회]]></desc>
			<sql><![CDATA[
SELECT BVVD
      ,BPOL
      ,(SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = X.POL_CD ) AS BPOL_NAME
      ,BPOD
      ,(SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = X.POD_CD ) AS BPOD_NAME
FROM (SELECT 
    VSL_CD || SKD_VOY_NO || SKD_DIR_CD AS BVVD
    ,POL_YD_CD AS BPOL
    ,POL_CD
    ,POD_YD_CD AS BPOD
    ,POD_CD
FROM BKG_VVD
WHERE BKG_NO = @[bkg_no]) X			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
