			<sql><![CDATA[
#if (${f_table_name} != 'COA_NOD_AVG_STND_COST') 				
DELETE FROM COA_LNK_AVG_STND_COST 	
 WHERE COST_YRMON =  @[cost_yrmon]						 
   AND LNK_FM_NOD_CD =  @[lnk_fm_nod_cd]					 
   AND LNK_TO_NOD_CD =  @[lnk_to_nod_cd]					
