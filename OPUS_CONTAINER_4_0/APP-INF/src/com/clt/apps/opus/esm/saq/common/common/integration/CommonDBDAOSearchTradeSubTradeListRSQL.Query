<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchTradeSubTradeListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT   DISTINCT        			
        SUB_TRD_CD AS CODE,  
        SUB_TRD_CD AS TEXT   
FROM  SAQ_TGT_GRP_TRD    
WHERE TRD_CD =  @[trade]  
ORDER BY SUB_TRD_CD 			]]></sql>
			<params>
				<param name="trade" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
