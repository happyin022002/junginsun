<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchBlCntClauseRSQL">
			<desc><![CDATA[Select BL Country Clause from BKG_BL_CLUZ_STUP]]></desc>
			<sql><![CDATA[
SELECT  
	LISTAGG(CMDT_DESC, CHR(13)||CHR(10)) WITHIN GROUP (ORDER BY ORD) AS CMDT_DESC
FROM (
    SELECT	CMDT_DESC
            ,CASE 	WHEN ORG_CNT_CD = @[por_cnt_cd] THEN 1
                    WHEN ORG_CNT_CD = @[pol_cnt_cd] THEN 2
                    WHEN ORG_CNT_CD = @[pod_cnt_cd] THEN 3
                    ELSE 4 END AS ORD
    FROM	BKG_BL_CLUZ_STUP
    WHERE	0=0
    AND		(
            (ORG_CNT_CD = @[por_cnt_cd] AND POR_APPL_FLG = 'Y')
        OR	(ORG_CNT_CD = @[pol_cnt_cd] AND POL_APPL_FLG = 'Y')
        OR	(ORG_CNT_CD = @[pod_cnt_cd] AND POD_APPL_FLG = 'Y')
        OR	(ORG_CNT_CD = @[del_cnt_cd] AND DEL_APPL_FLG = 'Y')
    )
)			]]></sql>
			<params>
				<param name="por_cnt_cd" type="12" value="" out="N"/>
				<param name="pol_cnt_cd" type="12" value="" out="N"/>
				<param name="pod_cnt_cd" type="12" value="" out="N"/>
				<param name="del_cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
