<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOmodifyVvdEtaUSQL">
			<desc><![CDATA[dwkim, BKG_CSTMS_ADV_VVD_ARR에 갱신.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_VVD_ARR V
SET
        EDA_UPD_DT = GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', sysdate, 'USNYC'),
        OFC_CD = @[snd_usr_ofc_cd],
        UPD_USR_ID = @[upd_usr_id],
		UPD_DT = SYSDATE
WHERE
	V.CNT_CD = 'US'
    AND V.VSL_CD = SUBSTR(@[vvd], 1, 4)
    AND V.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
    AND V.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
    AND V.POD_CD = @[pod_cd]
    AND V.ETA_DT = TO_DATE(@[eta_dt], @[eta_dt_format])			]]></sql>
			<params>
				<param name="snd_usr_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="eta_dt" type="12" value="" out="N"/>
				<param name="eta_dt_format" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
