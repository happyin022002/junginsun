<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOmodifyContractHdrDeleteHistoryUSQL">
			<desc><![CDATA[Add contract number information into contract delete history table]]></desc>
			<sql><![CDATA[
UPDATE PRI_CTRT_DELT_HIS
 SET PRC_CTRT_NO = @[rfa_no]
 WHERE PROP_NO = @[prop_no] AND AMDT_SEQ = @[amdt_seq] AND PRC_CTRT_TP_CD = 'R'			]]></sql>
			<params>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
