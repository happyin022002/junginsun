<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCntrVolByCntrUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE BKG_CONTAINER
SET    CNTR_CFM_FLG = 'N'
,      CNTR_PRT_FLG = decode(@[cntr_vol], '1', 'N', 'Y')
,      CNTR_VOL_QTY = @[cntr_vol]
WHERE	BKG_NO = @[bkg_no]
AND	CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_vol" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
