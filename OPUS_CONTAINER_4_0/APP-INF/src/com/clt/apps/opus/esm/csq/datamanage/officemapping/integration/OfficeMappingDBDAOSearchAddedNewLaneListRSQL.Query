<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeMappingDBDAOSearchAddedNewLaneListRSQL">
			<desc><![CDATA[추가된 LaneList를 조회한다]]></desc>
			<sql><![CDATA[
SELECT BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,TRD_CD
      ,RLANE_CD
      ,DIR_CD
      ,SRC_RLANE_CD
 FROM CSQ_QTA_NEW_LANE
WHERE BSE_TP_CD  = @[f_bse_tp_cd]
  AND BSE_YR     = @[f_bse_yr]
#if (${f_bse_tp_cd} == 'Y')
  AND BSE_QTR_CD = '00'
#else
  AND BSE_QTR_CD = @[f_bse_qtr_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
  AND TRD_CD     = @[f_trd_cd]
#end
#if (${f_dir_cd} != '' && ${f_dir_cd} != 'All')
  AND DIR_CD     = @[f_dir_cd]
#end
ORDER BY TRD_CD
        ,RLANE_CD
        ,DIR_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
