<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanningDBDAOInsertCfmTgtVvdForAddFreezingCSQL">
			<desc><![CDATA[Add Freezing 시 CSQ_CFM_TGT_VVD에 insert

* 2014.07.25 이혜민  QTA Set up by HO for IAS Sector_Add Freezing 시 Bound 삽입 DIR_CD = [@dir_cd] 조건 추가]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_CFM_TGT_VVD(QTA_RLSE_VER_NO, BSE_TP_CD, BSE_YR, BSE_QTR_CD, QTA_TGT_CD, TRD_CD, RLANE_CD, DIR_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, BSE_MON, BSE_WK, CONV_DIR_CD, SUB_TRD_CD, IOC_CD, FNL_BSA_CAPA, LOD_QTY, GRS_REV, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT )
SELECT SUBSTR(@[bse_yr], 3, 2) || A1.BSE_QTR_CD || LPAD(A3.CPY_NO,2,'0') AS QTA_RLSE_VER_NO
      ,A1.BSE_TP_CD
      ,A1.BSE_YR
      ,A1.BSE_QTR_CD
      ,'D' AS QTA_TGT_CD
      ,A1.TRD_CD
      ,A1.RLANE_CD
      ,A1.DIR_CD
      ,A1.VSL_CD
      ,A1.SKD_VOY_NO
      ,A1.SKD_DIR_CD
      ,A1.BSE_MON
      ,A1.BSE_WK
      ,NVL((SELECT S1.CONV_DIR_CD
              FROM CSQ_DIR_CONV S1
             WHERE S1.BSE_TP_CD  = A1.BSE_TP_CD
               AND S1.BSE_YR     = A1.BSE_YR
               AND S1.BSE_QTR_CD = A1.BSE_QTR_CD
               AND S1.TRD_CD     = A1.TRD_CD
               AND S1.RLANE_CD   = A1.RLANE_CD
               AND S1.DIR_CD     = A1.DIR_CD
            ),DIR_CD) AS CONV_DIR_CD
      ,A1.SUB_TRD_CD
      ,A1.IOC_CD
      ,A1.FNL_BSA_CAPA
      ,0 AS LOD_QTY
      ,0 AS GRS_REV
      ,@[usr_id] AS CRE_USR_ID
      ,SYSDATE AS CRE_DT
      ,@[usr_id] AS UPD_USR_ID
      ,SYSDATE AS UPD_DT
#if (${f_add_flg} == 'Y')
  FROM CSQ_SCTR_ADD_TGT_VVD A1
#else
  FROM CSQ_QTA_TGT_VVD A1
#end
      ,CSQ_QTA_LANE_MGMT A2
      ,COM_CPY_NO A3
 WHERE 1=1
   AND A1.BSE_TP_CD  = A2.BSE_TP_CD
   AND A1.BSE_YR     = A2.BSE_YR 
   AND A1.BSE_QTR_CD = A2.BSE_QTR_CD
   AND A1.TRD_CD     = A2.TRD_CD
   AND A1.RLANE_CD   = A2.RLANE_CD
   AND A1.DIR_CD     = NVL(A2.LANE_DIR_CD, A1.DIR_CD)
   AND A2.IAS_SCTR_FLG = 'Y'
   AND A1.BSE_TP_CD  = @[bse_tp_cd]
   AND A1.BSE_YR     = @[bse_yr]
   AND A1.BSE_QTR_CD = DECODE(@[bse_tp_cd], 'Y', '00', @[bse_qtr_cd])
#if (${f_add_flg} == 'Y')
   AND A1.RLANE_CD   = @[rlane_cd]
   AND A1.PF_GRP_CD  = @[pf_grp_cd]
   AND A1.DIR_CD     = NVL(@[dir_cd], A1.DIR_CD)
   AND A3.CPY_NO     BETWEEN 2 AND  2
#else
   AND A3.CPY_NO     BETWEEN 1 AND  2
#end			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="pf_grp_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
