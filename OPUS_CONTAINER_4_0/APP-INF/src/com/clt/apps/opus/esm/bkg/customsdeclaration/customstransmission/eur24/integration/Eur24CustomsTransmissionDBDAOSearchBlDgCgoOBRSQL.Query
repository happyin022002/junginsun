<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24CustomsTransmissionDBDAOSearchBlDgCgoOBRSQL">
			<desc><![CDATA[Eur24CustomsTransmissionDBDAOSearchBlDgCgoOBRSQL]]></desc>
			<sql><![CDATA[
/* Eur24BlDgCgoListVOs Eur24CustomsTransmissionDBDAOSearchBlDgCgoOB ( String blNo , String vslCd , String skdVoyNo , String skdDirCd ,String cstmsPortCd, String cntrNo) */
SELECT
   IMDG_UN_NO     AS UNNBR     /* 110 */
 , IMDG_CLSS_CD   AS CLASS_CD     /* 111 CLASS */
 , PCK_QTY        AS D_PKG     /* 112 */

 , NVL((SELECT CSTMS_PCK_TP_CD
	   FROM BKG_CSTMS_PCK_TP_CONV
	  WHERE CNT_CD ='EU'
        AND RCVR_ID = 'ENS'
        AND PCK_TP_CD = X.PCK_TP_CD),X.PCK_TP_CD)  AS D_PKGUNIT /* 113 */

 , GRS_WGT        AS GWGT      /* 114 */
 , WGT_UT_CD      AS GWGT_UNIT /* 115 */
 , MEAS_QTY       AS MEA       /* 116 */
 , MEAS_UT_CD     AS MEA_UNIT  /* 117 */

  ,VSL_CD,      SKD_VOY_NO,       SKD_DIR_CD,       BL_NO,       CSTMS_PORT_CD, CNTR_NO, DCGO_SEQ
  ,IMDG_UN_NO,  IMDG_UN_NO_SEQ,   GRS_WGT,         IMDG_CLSS_CD, PCK_QTY,       PCK_TP_CD
  ,CRE_USR_ID,  CRE_DT,           UPD_USR_ID,       UPD_DT
FROM BKG_CSTMS_EUR_IO_DG_CGO X
WHERE BND_TP_CD = 'O'
AND VSL_CD   = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]
AND BL_NO      = @[bl_no]
AND CSTMS_PORT_CD = @[cstms_port_cd]
AND CNTR_NO    = @[cntr_no]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
