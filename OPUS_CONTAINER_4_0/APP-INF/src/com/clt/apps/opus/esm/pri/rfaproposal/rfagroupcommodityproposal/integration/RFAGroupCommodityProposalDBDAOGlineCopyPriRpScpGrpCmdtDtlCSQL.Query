<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupCommodityProposalDBDAOGlineCopyPriRpScpGrpCmdtDtlCSQL">
			<desc><![CDATA[Guideline Copy PRI_RP_SCP_GRP_CMDT_DTL Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_GRP_CMDT_DTL (
      PROP_NO
    , AMDT_SEQ
    , SVC_SCP_CD
    , GRP_CMDT_SEQ
    , GRP_CMDT_DTL_SEQ
    , PRC_CMDT_TP_CD
    , PRC_CMDT_DEF_CD
    , PRC_PROG_STS_CD
    , SRC_INFO_CD
    , N1ST_CMNC_AMDT_SEQ
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT) 
SELECT @[prop_no] AS PROP_NO
     , @[amdt_seq] AS AMDT_SEQ
     , B.SVC_SCP_CD
     , DENSE_RANK() OVER(PARTITION BY B.SVC_SCP_CD, B.GLINE_SEQ
                         ORDER BY B.SVC_SCP_CD, B.GLINE_SEQ, B.GRP_CMDT_SEQ) AS GRP_CMDT_SEQ
     , DENSE_RANK() OVER(PARTITION BY B.SVC_SCP_CD, B.GLINE_SEQ, B.GRP_CMDT_SEQ
                         ORDER BY B.SVC_SCP_CD, B.GLINE_SEQ, B.GRP_CMDT_SEQ, B.GRP_CMDT_DTL_SEQ) AS GRP_CMDT_DTL_SEQ
     , B.PRC_CMDT_TP_CD
     , B.PRC_CMDT_DEF_CD
     , 'I' AS PRC_PROG_STS_CD
     , 'GC' AS SRC_INFO_CD
     , @[amdt_seq] AS N1ST_CMNC_AMDT_SEQ
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM PRI_RG_GRP_CMDT A
    ,PRI_RG_GRP_CMDT_DTL B
WHERE A.SVC_SCP_CD = @[svc_scp_cd]
AND   A.GLINE_SEQ = @[gline_seq]
AND   B.SVC_SCP_CD = A.SVC_SCP_CD
AND   B.GLINE_SEQ = A.GLINE_SEQ
AND   B.GRP_CMDT_SEQ = A.GRP_CMDT_SEQ			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
