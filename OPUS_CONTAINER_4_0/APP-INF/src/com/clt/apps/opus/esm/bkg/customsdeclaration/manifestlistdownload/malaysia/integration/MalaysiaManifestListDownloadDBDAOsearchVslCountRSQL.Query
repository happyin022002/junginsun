<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MalaysiaManifestListDownloadDBDAOsearchVslCountRSQL">
			<desc><![CDATA[에 존재하는 Vessel Count하여 조회건수가 없을 경우 VVD 등록을 먼저 하도록 메시지 뿌림]]></desc>
			<sql><![CDATA[
SELECT COUNT(VSL_CD) CNT
  FROM BKG_CSTMS_MY_VVD
 WHERE VSL_CD        = @[vsl_cd]
   AND SKD_VOY_NO    = @[skd_voy_no]
   AND SKD_DIR_CD    = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
