<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandManifestListDownloadDBDAOsearchCstmsMfListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT A.POL_CD AS POL,
       C.VPS_ETD_DT AS POL_ATD,
       A.POD_CD AS POD,
       DECODE(D.TRNK_AUTO_BDR_DT, NULL, DECODE(D.TRNK_MNL_BDR_DT, NULL, 'N', 'Y'), 'Y') AS BDR,
       DECODE(D.TRNK_AUTO_BDR_DT, NULL, DECODE(D.TRNK_MNL_BDR_DT, NULL, NULL, TO_CHAR(D.TRNK_MNL_BDR_DT, 'YYYY-MM-DD HH24:MI')), TO_CHAR(D.TRNK_AUTO_BDR_DT, 'YYYY-MM-DD HH24:MI')) AS BDR_DATE,
       COUNT(DISTINCT B.BKG_NO) AS BL,
       (SELECT COUNT(*)
          FROM BKG_CSTMS_NZL_SND_LOG
         WHERE (BL_NO, MSG_SND_NO) IN (SELECT BL_NO,
                                              MAX(MSG_SND_NO) OVER(PARTITION BY BL_NO, POL_CD, POD_CD)
                                         FROM BKG_CSTMS_NZL_SND_LOG
                                        WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
                                          AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                          AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
										)
            AND POD_CD = A.POD_CD
            AND POL_CD = A.POL_CD
) AS DNLD,
       (SELECT COUNT(*)
          FROM BKG_CSTMS_NZL_SND_LOG
         WHERE (BL_NO, MSG_SND_NO) IN (SELECT BL_NO,
                                              MAX(MSG_SND_NO) OVER(PARTITION BY BL_NO, POL_CD, POD_CD)
                                         FROM BKG_CSTMS_NZL_SND_LOG
                                        WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
                                          AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                          AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
										)
           AND POD_CD = A.POD_CD
           AND POL_CD = A.POL_CD
           AND CSTMS_RQST_FLG = 'N') AS ACPT,
       (SELECT COUNT(*)
          FROM BKG_CSTMS_NZL_SND_LOG
         WHERE (BL_NO, MSG_SND_NO) IN (SELECT BL_NO,
                                              MAX(MSG_SND_NO) OVER(PARTITION BY BL_NO, POL_CD, POD_CD)
                                         FROM BKG_CSTMS_NZL_SND_LOG
                                        WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
                                          AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                          AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
										)
           AND POD_CD = A.POD_CD
           AND POL_CD = A.POL_CD
           AND CSTMS_RQST_FLG = 'Y') AS DIFF,
       COUNT(DISTINCT H.CNTR_NO) AS CNTR,
       (SELECT COUNT(*)
          FROM BKG_CSTMS_NZL_SND_LOG
         WHERE (CNTR_NO, MSG_SND_NO) IN (SELECT CNTR_NO,
                                                MAX(MSG_SND_NO) OVER(PARTITION BY CNTR_NO, POL_CD, POD_CD)
                                           FROM BKG_CSTMS_NZL_SND_LOG
                                          WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
                                            AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                            AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
										)
           AND POD_CD = A.POD_CD
           AND POL_CD = A.POL_CD
) AS CNTR_DNLD,
       (SELECT COUNT(*)
          FROM BKG_CSTMS_NZL_SND_LOG
         WHERE (CNTR_NO, MSG_SND_NO) IN (SELECT CNTR_NO,
                                                 MAX(MSG_SND_NO) OVER(PARTITION BY CNTR_NO, POL_CD, POD_CD)
                                            FROM BKG_CSTMS_NZL_SND_LOG
                                           WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
                                             AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                             AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
										)
           AND POD_CD = A.POD_CD
           AND POL_CD = A.POL_CD
           AND CSTMS_RQST_FLG = 'N') AS CNTR_ACPT,
       (SELECT COUNT(*)
          FROM BKG_CSTMS_NZL_SND_LOG
         WHERE (CNTR_NO, MSG_SND_NO) IN (SELECT CNTR_NO,
                                                MAX(MSG_SND_NO) OVER(PARTITION BY CNTR_NO, POL_CD, POD_CD)
                                           FROM BKG_CSTMS_NZL_SND_LOG
                                          WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
                                            AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                                            AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
										)
           AND POD_CD = A.POD_CD
           AND POL_CD = A.POL_CD
           AND CSTMS_RQST_FLG = 'Y') AS CNTR_DIFF

  FROM BKG_VVD A,
       BKG_BOOKING B,
       VSK_VSL_PORT_SKD C,
       BKG_VVD_BDR_LOG D,
       BKG_CONTAINER H

 WHERE A.VSL_CD = SUBSTR( @[vvd], 1, 4)
   AND A.SKD_VOY_NO = SUBSTR( @[vvd], 5, 4)
   AND A.SKD_DIR_CD = SUBSTR( @[vvd], 9, 1) 
#if (${pol_cd} != '')
   AND A.POL_CD = @[pol_cd] 
#end
   AND A.POD_CD LIKE @[pod] || '%'
   AND B.BKG_NO = A.BKG_NO
   AND B.BKG_STS_CD NOT IN ('X', 'S')
   AND B.BKG_CGO_TP_CD IN ('F', 'P')
   AND C.VSL_CD = A.VSL_CD
   AND C.SKD_VOY_NO = A.SKD_VOY_NO
   AND C.SKD_DIR_CD = A.SKD_DIR_CD
   AND C.VPS_PORT_CD = A.POL_CD
   AND C.CLPT_IND_SEQ = A.POL_CLPT_IND_SEQ
   AND D.VSL_CD(+) = A.VSL_CD
   AND D.SKD_VOY_NO(+) = A.SKD_VOY_NO
   AND D.SKD_DIR_CD(+) = A.SKD_DIR_CD
   AND D.POL_CLPT_IND_SEQ(+) = A.POL_CLPT_IND_SEQ
   AND D.POD_CLPT_IND_SEQ(+) = A.POD_CLPT_IND_SEQ
   AND D.POL_CD(+) = A.POL_CD
   AND D.POD_CD(+) = A.POD_CD
   AND H.BKG_NO(+) = B.BKG_NO

 GROUP BY A.POL_CD,
       C.VPS_ETD_DT,
       A.POD_CD,
       DECODE(D.TRNK_AUTO_BDR_DT, NULL, DECODE(D.TRNK_MNL_BDR_DT, NULL, 'N', 'Y'), 'Y'),
       DECODE(D.TRNK_AUTO_BDR_DT, NULL, DECODE(D.TRNK_MNL_BDR_DT, NULL, NULL, TO_CHAR(D.TRNK_MNL_BDR_DT, 'YYYY-MM-DD HH24:MI')), TO_CHAR(D.TRNK_AUTO_BDR_DT, 'YYYY-MM-DD HH24:MI'))			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
