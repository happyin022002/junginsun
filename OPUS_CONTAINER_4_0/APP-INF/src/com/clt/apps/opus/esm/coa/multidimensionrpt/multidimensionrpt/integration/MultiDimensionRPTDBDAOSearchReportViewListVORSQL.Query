<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MultiDimensionRPTDBDAOSearchReportViewListVORSQL">
			<desc><![CDATA[   COA_RPT_AUTH_MGMT 테이블의 레포트 별 권한 조회]]></desc>
			<sql><![CDATA[
SELECT MIN(TO_NUMBER(RPT_SEQ)) AS RPT_SEQ
      ,OFC_LVL
      ,OFC_LVL_DESC
      ,MAX(DECODE(PFIT_VW_CD, 'P', PFIT_VW_SLCT_FLG)) AS PFIT_CD1
      ,MAX(DECODE(PFIT_VW_CD, 'R', PFIT_VW_SLCT_FLG)) AS PFIT_CD2
      ,MAX(DECODE(PFIT_VW_DFLT_CD,'Y', PFIT_VW_CD)) AS PFIT_DFLT
      ,MAX(DECODE(OFC_VW_CD, 'C', OFC_VW_SLCT_FLG)) AS OFC_CD1
      ,MAX(DECODE(OFC_VW_CD, 'L', OFC_VW_SLCT_FLG)) AS OFC_CD2
      ,MAX(DECODE(OFC_VW_DFLT_CD, 'Y', OFC_VW_CD)) AS OFC_DFLT
      ,MAX(DECODE(PFIT_LVL_CD, 'C', PFIT_LVL_SLCT_FLG)) AS LVL_CD1
      ,MAX(DECODE(PFIT_LVL_CD, 'O', PFIT_LVL_SLCT_FLG)) AS LVL_CD2
      ,MAX(DECODE(PFIT_LVL_DFLT_CD, 'Y', PFIT_LVL_CD)) AS LVL_DFLT
  FROM COA_RPT_AUTH_MGMT
 WHERE 1 = 1
GROUP BY OFC_LVL, OFC_LVL_DESC
ORDER BY OFC_LVL			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
