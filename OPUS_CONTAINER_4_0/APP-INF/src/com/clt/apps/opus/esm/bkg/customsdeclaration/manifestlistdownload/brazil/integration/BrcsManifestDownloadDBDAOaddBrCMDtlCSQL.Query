<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BrcsManifestDownloadDBDAOaddBrCMDtlCSQL">
			<desc><![CDATA[CNTR MF DETAIL 정보 생성 - BKG_CSTMS_BRZ_CNTR_MF_DTL]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_CSTMS_BRZ_CNTR_MF_DTL
      (BL_NO
      ,CNTR_NO
      ,CNTR_MF_SEQ
      ,MF_DTL_SEQ
      ,BRZ_CMDT_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,BKG_NO
      )
SELECT (SELECT BL_NO
        FROM BKG_BOOKING
        WHERE BKG_NO = @[bkg_no]) AS BL_NO
      ,CNTR_NO
      ,CNTR_MF_SEQ
      ,1
      ,NCM_NO
      ,@[cre_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE
      ,@[bkg_no]
  FROM BKG_CNTR_MF_DESC
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
