<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeMappingDBDAOCreateBasicCmcbForAddedOfcCSQL">
			<desc><![CDATA[추가된 Office에 대해서 CMCB정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_QTA_LANE_OFC_COST 
SELECT BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,TRD_CD
      ,RLANE_CD
      ,DIR_CD
      ,RGN_OFC_CD
      ,RHQ_CD
      ,0             GID_PA_CM_UC_AMT
      ,0             GID_RA_CM_UC_AMT
      ,0             PA_CM_UC_AMT
      ,0             RA_CM_UC_AMT
      ,'N'           ADD_FLG
      ,@[cre_usr_id] CRE_USR_ID
      ,SYSDATE       CRE_DT
      ,@[upd_usr_id] UPD_USR_ID
      ,SYSDATE       UPD_DT
  FROM CSQ_QTA_LANE_OFC
 WHERE 1=1
   AND BSE_TP_CD  = @[bse_tp_cd]
   AND BSE_YR     = @[bse_yr]
   AND BSE_QTR_CD = DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
   AND RGN_OFC_CD = @[rgn_ofc_cd]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
