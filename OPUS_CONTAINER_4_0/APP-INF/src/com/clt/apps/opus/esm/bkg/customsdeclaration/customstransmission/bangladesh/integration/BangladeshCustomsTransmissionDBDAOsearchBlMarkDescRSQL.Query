<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshCustomsTransmissionDBDAOsearchBlMarkDescRSQL">
			<desc><![CDATA[방글라데시 세관 신고용 Manifest B/L Marks Description 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   SUBSTR('{DESC'||CHR(10)||'DESC:' ||REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(DOC.PCK_CMDT_DESC||CHR(10)||'DESC:'||DOC.CNTR_CMDT_DESC||CHR(10)||'DESC:'||CMDT_DESC,'&',CHR(38)||'amp'||CHR(59)),'<',CHR(38)||'lt'||CHR(59)),'>',CHR(38)||'gt'||CHR(59)),'''',CHR(38)||'quot'||CHR(59)),CHR(39),CHR(38)||'apos'||CHR(59)),CHR(10),CHR(10)||'DESC:') ||CHR(10)||'}DESC'||CHR(10), 1, 4000) CMDT_DESC,
         DECODE(LENGTH(MK_DESC),NULL,'',SUBSTR('{MARK'||CHR(10)||'MARKNO:' ||REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(BKG_SPCLCHAR_CONV_FNC(MK_DESC, 'M'),'&',CHR(38)||'amp'||CHR(59)),'<',CHR(38)||'lt'||CHR(59)),'>',CHR(38)||'gt'||CHR(59)),'''',CHR(38)||'quot'||CHR(59)),CHR(39),CHR(38)||'apos'||CHR(59)),CHR(10),CHR(10)||'MARKNO:') ||chr(10)||'}MARK'||CHR(10), 1, 4000)  ) MK_DESC
        FROM   BKG_BL_MK_DESC MD, BKG_BL_DOC DOC
        WHERE  DOC.BKG_NO        = @[bkg_no]
		AND		DOC.BKG_NO		= MD.BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
