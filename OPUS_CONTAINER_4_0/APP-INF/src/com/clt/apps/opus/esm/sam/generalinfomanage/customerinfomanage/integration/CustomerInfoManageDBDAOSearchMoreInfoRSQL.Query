<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAOSearchMoreInfoRSQL">
			<desc><![CDATA[SearchMoreInfo]]></desc>
			<sql><![CDATA[
SELECT CUST.NVOCC_CO_SCAC_CD
     , CUST.NVOCC_LIC_NO
     , CUST.NVOCC_BD_NO
     , CUST.NVOCC_BD_AMT
     , CUST_SLA_FLG
     , CUST.BKG_ALT_RSN
     , CUST.BKG_ALT_MSG
     , TO_CHAR(CUST.BKG_ALT_FM_DT, 'yyyy-mm-dd') AS BKG_ALT_FM_DT
     , TO_CHAR(CUST.BKG_ALT_TO_DT, 'yyyy-mm-dd') AS BKG_ALT_TO_DT     
     , CUST.SPCL_REQ_DESC     
     , CUST.CMPT_DESC     
     , CUST.PRF_CNTR_TPSZ_CD
     , CUST.CUST_RMK
     , TO_CHAR(TO_DATE(CUST.NVOCC_BD_ST_EFF_DT, 'yyyy-mm-dd'), 'yyyy-mm-dd') AS NVOCC_BD_ST_EFF_DT
     , TO_CHAR(TO_DATE(CUST.NVOCC_BD_END_EFF_DT, 'yyyy-mm-dd'), 'yyyy-mm-dd') AS NVOCC_BD_END_EFF_DT 
     , CR.CR_AMT
     , CR.CR_CLT_OFC_CD
     , CR.IB_CR_TERM_DYS
     , CR.OB_CR_TERM_DYS
     , INFO.INDUS_TP_N1ST_DESC
     , INFO.INDUS_TP_N2ND_DESC
     , INFO.MJR_N1ST_TRD_CD
	 , INFO.MJR_N2ND_TRD_CD
     , INFO.PRF_N1ST_REP_CMDT_CD
     , INFO.PRF_N2ND_REP_CMDT_CD
     , INFO.YRY_VOL_QTY     
     , CNTC.CUST_URL
     , A.REP_CMDT_NM AS PRF_N1ST_CMDT_GRP_DTL
     , B.REP_CMDT_NM AS PRF_N2ND_CMDT_GRP_DTL
FROM MDM_CUSTOMER CUST
   , MDM_CR_CUST CR
   , SAM_CUST_OTR_INFO INFO
   , MDM_CUST_CNTC_PNT CNTC
   , MDM_REP_CMDT A 
   , MDM_REP_CMDT B
   , ( SELECT MAX(CUST_CNTC_PNT_SEQ) SEQ
         FROM MDM_CUST_CNTC_PNT
        WHERE 1=1
          AND CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
          AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))
     )CNTC_SEQ
WHERE 1=1 
AND CUST.CUST_CNT_CD = CR.CUST_CNT_CD (+)
AND CUST.CUST_SEQ = CR.CUST_SEQ (+)
AND CUST.CUST_CNT_CD = INFO.CUST_CNT_CD (+)
AND CUST.CUST_SEQ = INFO.CUST_SEQ (+)
AND CUST.CUST_CNT_CD = CNTC.CUST_CNT_CD
AND CUST.CUST_SEQ = CNTC.CUST_SEQ
AND CNTC.CUST_CNTC_PNT_SEQ = CNTC_SEQ.SEQ
AND CUST.CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND CUST.CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))
AND INFO.PRF_N1ST_REP_CMDT_CD  = A.REP_CMDT_CD (+)
AND INFO.PRF_N2ND_REP_CMDT_CD  = B.REP_CMDT_CD (+)			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
