<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchMaxInfoSeqRSQL">
			<desc><![CDATA[BKG_INET_BL_PRN_AUTH의 bkg별 max info seq 조회(벌룬 파라메터용)]]></desc>
			<sql><![CDATA[
SELECT MAX(INFO_SEQ) 
FROM BKG_INET_BL_PRN_AUTH 
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
