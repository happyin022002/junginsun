<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOModifyJapDorStsByTransUSQL">
			<desc><![CDATA[전송 대상 항목으로 상태로 변경한다. ( Locking )]]></desc>
			<sql><![CDATA[
UPDATE BKG_DO
   SET JP_DO_SND_STS_CD = 'S'
     , UPD_USR_ID       = @[upd_usr_id]
     , UPD_DT           = SYSDATE
 WHERE BKG_NO IN ( SELECT BKG_NO 
                     FROM BKG_JP_DO_IF 
                    WHERE JP_DO_GRP_NO = @[grp_no]
                    AND   JP_DO_SND_STS_CD ='T' )			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="grp_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
