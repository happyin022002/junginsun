		<query name="JapanTerminalTransmissionDBDAOdeleteCntrforNewBkgInfoDSQL">
			<desc><![CDATA[deleteCntrforNewBkgInfo]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_TML_EDI_JP_CNTR

WHERE BKG_NO  = @[bkg_no]
  AND BKG_SKD_SEQ = @[bkg_skd_seq]
			]]></sql>
			<params>
