<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOAddCoaMainGrpCostCodeCSQL">
			<desc><![CDATA[Main Group cost code 신규 추가]]></desc>
			<sql><![CDATA[
INSERT INTO COA_MN_GRP_COST
(
          STND_COST_TP_CD
         ,MGRP_COST_CD
         ,MGRP_COST_CD_DESC
         ,CRE_USR_ID
         ,CRE_DT
         ,UPD_USR_ID
         ,UPD_DT

)
VALUES
(        @[stnd_cost_tp_cd]
        ,@[mgrp_cost_cd]
        ,@[mgrp_cost_cd_desc]
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[upd_usr_id]
        ,SYSDATE
)			]]></sql>
			<params>
				<param name="stnd_cost_tp_cd" type="12" value="" out="N"/>
				<param name="mgrp_cost_cd" type="12" value="" out="N"/>
				<param name="mgrp_cost_cd_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
