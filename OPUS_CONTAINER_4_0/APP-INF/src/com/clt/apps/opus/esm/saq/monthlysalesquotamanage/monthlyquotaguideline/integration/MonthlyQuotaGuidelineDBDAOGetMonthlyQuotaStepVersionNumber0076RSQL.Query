<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaGuidelineDBDAOGetMonthlyQuotaStepVersionNumber0076RSQL">
			<desc><![CDATA[TRD_CD/DIR_CD/MQTA_VER_NO 별 조회]]></desc>
			<sql><![CDATA[
SELECT TRD_CD, DIR_CD, MAX(MQTA_VER_NO) AS MQTA_VER_NO
FROM   SAQ_MON_QTA_STEP_VER           
WHERE  MQTA_STEP_CD = @[step_cd]      
AND    BSE_YR       = @[year]         
AND    BSE_QTR_CD   = @[bse_qtr_cd]     
AND    GLINE_VER_NO = @[version]      
AND    CRE_OFC_CD   = @[ofc_cd]
GROUP BY TRD_CD, DIR_CD			]]></sql>
			<params>
				<param name="step_cd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="version" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
