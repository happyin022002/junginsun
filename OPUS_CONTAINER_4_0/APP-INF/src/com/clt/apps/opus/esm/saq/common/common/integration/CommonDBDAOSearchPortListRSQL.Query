<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchPortListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT A.LOC_CD CODE, A.LOC_NM TEXT
FROM   MDM_LOCATION A
WHERE  A.LOC_CD LIKE @[port_cd]
       AND NVL(A.PORT_INLND_CD, 'N') = 'Y'			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
