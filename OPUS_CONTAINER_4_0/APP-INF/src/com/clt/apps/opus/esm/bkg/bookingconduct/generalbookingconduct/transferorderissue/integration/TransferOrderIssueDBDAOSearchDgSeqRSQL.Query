<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOSearchDgSeqRSQL">
			<desc><![CDATA[ESM_BKG_0079_02A dq 콤보목록 조회]]></desc>
			<sql><![CDATA[
SELECT DCGO_SEQ DG_SEQ
       , 'UN No:'||IMDG_UN_NO||', IMO Class:'||IMDG_CLSS_CD
            ||DECODE(NVL(SPCL_CGO_APRO_CD, 'X'), 'X', '', ' ('||INTG_CD_VAL_DP_DESC||')') DG_TRO_RMK
       , DISPLAY_SEQ||'|CNTR seq:'||DG_CNTR_SEQ||', UN No:'||IMDG_UN_NO||', IMO Class:'||IMDG_CLSS_CD
            ||DECODE(NVL(SPCL_CGO_APRO_CD, 'X'), 'X', '', ' ('||INTG_CD_VAL_DP_DESC||')') DISPLAY_NM
  FROM ( SELECT DG_CNTR_SEQ,
                IMDG_UN_NO,
                ROW_NUMBER() OVER(PARTITION BY DG_CNTR_SEQ ORDER BY DCGO_SEQ) DISPLAY_SEQ,
                IMDG_CLSS_CD,
                DCGO_SEQ,
                CNTR_NO,
                INTG_CD_VAL_DP_DESC,
                SPCL_CGO_APRO_CD
           FROM BKG_DG_CGO dg, com_intg_cd_dtl code
          WHERE SPCL_CGO_APRO_CD = INTG_CD_VAL_CTNT(+)
            AND 'CD01955'        = INTG_CD_ID(+)
            AND BKG_NO 			= @[bkg_no] 
       )
 WHERE NVL(SPCL_CGO_APRO_CD, ' ') <> 'C'
 ORDER BY CNTR_NO,DCGO_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="FXT900004600" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
