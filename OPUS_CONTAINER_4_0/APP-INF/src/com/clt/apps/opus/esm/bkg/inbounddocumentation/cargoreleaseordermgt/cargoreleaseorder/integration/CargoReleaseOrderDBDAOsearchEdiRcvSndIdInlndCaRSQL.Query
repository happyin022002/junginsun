<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchEdiRcvSndIdInlndCaRSQL">
			<desc><![CDATA[Inland setting이 되어있을 때 Inland에도 CA전송을 하기위해 SND ID와 RCV ID 가져옴]]></desc>
			<sql><![CDATA[
--BKG_EDI_YD 테이블을 BKG_EDI_TRD_PRNR_SUB_LNK, BKG_EDI_SUB_LNK_MSG로 수정
SELECT 'CA' AS EDI_MSG_ID, C.RCVR_TRD_PRNR_ID AS EDI_RCV_ID, C.SNDR_TRD_PRNR_ID AS EDI_SND_ID
  FROM BKG_CGO_RLSE     A,
       BKG_BOOKING      B,
       BKG_EDI_TRD_PRNR_SUB_LNK C,
       BKG_EDI_SUB_LNK_MSG D

 WHERE A.BL_NO       = @[bl_no]
   AND A.BL_NO       = B.BL_NO
   AND C.PRNR_SUB_LNK_CD = ( DECODE(B.DE_TERM_CD,'Y',B.DEL_NOD_CD, 
                                                 'D',(SELECT MAX (COPD.NOD_CD) NOD_CD
                                                        FROM SCE_COP_HDR COPM
                                                           , SCE_COP_DTL COPD
                                                           , MDM_ACTIVITY MDM
                                                        WHERE 1=1 
                                                        --AND SUBSTR(BKGM.DEL_CD, 1,2) = 'US'
                                                          AND COPM.BKG_NO = (SELECT BKG_NO 
                                                                               FROM BKG_BOOKING 
                                                                              WHERE BL_NO = @[bl_no]
                                                                                AND ROWNUM = 1)
                                                          AND COPD.COP_NO = COPM.COP_NO
                                                          AND COPM.COP_STS_CD <> 'X'
                                                          AND COPD.ACT_CD IN ('FITRDO', 'FITYDO')  
                                                          AND MDM.ACT_CD = COPD.ACT_CD)               ,B.DEL_NOD_CD)) -- CY면 DEL_NOD_CD , DOOR면 [Truck Gate Out from I/B CY] or [Truck Gate Out from I/B Rail Ramp]
   AND D.EDI_MSG_IND_CD = 1   --inland
   AND C.TRD_PRNR_SUB_LNK_SEQ = D.TRD_PRNR_SUB_LNK_SEQ
   AND D.EDI_MSG_TP_ID = 'TDC315'
   AND (D.MSG_TP_DESC = '1' OR D.MSG_TP_DESC = '2')
   AND ROWNUM	      = 1
   AND 'CA1'    = @[edi_knd] 
UNION
SELECT 'CA' AS EDI_MSG_ID, C.RCVR_TRD_PRNR_ID AS EDI_RCV_ID, C.SNDR_TRD_PRNR_ID AS EDI_SND_ID
  FROM BKG_CGO_RLSE     A,
       BKG_BOOKING      B,
       BKG_EDI_TRD_PRNR_SUB_LNK C,
       BKG_EDI_SUB_LNK_MSG D
 WHERE A.BL_NO       = @[bl_no]
   AND A.BL_NO       = B.BL_NO
  AND C.PRNR_SUB_LNK_CD = ( DECODE(B.DE_TERM_CD,'Y',B.DEL_NOD_CD, 
                                                 'D',(SELECT MAX (COPD.NOD_CD) NOD_CD
                                                        FROM SCE_COP_HDR COPM
                                                           , SCE_COP_DTL COPD
                                                           , MDM_ACTIVITY MDM
                                                        WHERE 1=1 
                                                        --AND SUBSTR(BKGM.DEL_CD, 1,2) = 'US'
                                                          AND COPM.BKG_NO = (SELECT BKG_NO 
                                                                               FROM BKG_BOOKING 
                                                                              WHERE BL_NO = @[bl_no]
                                                                                AND ROWNUM = 1)
                                                          AND COPD.COP_NO = COPM.COP_NO
                                                          AND COPM.COP_STS_CD <> 'X'
                                                          AND COPD.ACT_CD IN ('FITRDO', 'FITYDO')  
                                                          AND MDM.ACT_CD = COPD.ACT_CD)               ,B.DEL_NOD_CD)) -- CY면 DEL_NOD_CD , DOOR면 [Truck Gate Out from I/B CY] or [Truck Gate Out from I/B Rail Ramp]
   AND D.EDI_MSG_IND_CD = 1
   AND C.TRD_PRNR_SUB_LNK_SEQ = D.TRD_PRNR_SUB_LNK_SEQ
   AND D.EDI_MSG_TP_ID = 'TDC315'
   AND D.MSG_TP_DESC = '2'
   AND ROWNUM	= 1
   AND 'CA2'    = @[edi_knd]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="edi_knd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
