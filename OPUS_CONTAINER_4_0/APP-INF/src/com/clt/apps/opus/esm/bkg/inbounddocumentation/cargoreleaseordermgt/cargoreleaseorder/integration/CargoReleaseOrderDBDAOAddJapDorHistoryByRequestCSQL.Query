<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOAddJapDorHistoryByRequestCSQL">
			<desc><![CDATA[Cargo Release Order에서 Event별로 발생 내역 정보에 대한 History를 기록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_DO_HIS (
     BKG_NO
   , DO_SEQ
   , DO_CNG_EVNT_CD
   , PRE_CTNT
   , CRNT_CTNT
   , EVNT_DT
   , EVNT_GDT
   , EVNT_USR_ID
   , EVNT_OFC_CD
   , CRE_USR_ID
   , CRE_DT
   , UPD_USR_ID
   , UPD_DT
) 
SELECT BDO.BKG_NO                 AS BKG_NO
     , NVL((SELECT /*+INDEX_DESC(T XPKBKG_DO_HIS) */ DO_SEQ
              FROM BKG_DO_HIS T
             WHERE T.BKG_NO = BDO.BKG_NO
               AND ROWNUM =1),0)+1 AS DO_SEQ  
      , @[do_cng_evnt_cd]          AS DO_CNG_EVNT_CD
      , @[pre_ctnt]                AS PRE_CTNT     
      , @[crnt_ctnt]               AS CRNT_CTNT       
    
     , GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,sysdate,GLOBALDATE_PKG.GET_LOCCD_FNC(@[evnt_ofc_cd])) AS EVNT_DT
     , GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,sysdate,'GMT') AS EVNT_GDT  
  

      , @[evnt_usr_id]             AS EVNT_USR_ID
      , @[evnt_ofc_cd]             AS EVNT_OFC_CD
      , @[cre_usr_id]              AS CRE_USR_ID
      , SYSDATE                    AS CRE_DT
      , @[upd_usr_id]              AS UPD_USR_ID
      , SYSDATE                    AS UPD_DT   
   FROM BKG_DO BDO
  WHERE BDO.BKG_NO   = @[bkg_no]
    AND BDO.RLSE_SEQ = @[rlse_seq]			]]></sql>
			<params>
				<param name="do_cng_evnt_cd" type="12" value="" out="N"/>
				<param name="pre_ctnt" type="12" value="" out="N"/>
				<param name="crnt_ctnt" type="12" value="" out="N"/>
				<param name="evnt_ofc_cd" type="12" value="" out="N"/>
				<param name="evnt_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rlse_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
