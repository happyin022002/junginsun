<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchContainerMrnUcrRSQL">
			<desc><![CDATA[MRN UCR No 의 container list 를 조회한다.]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y')
SELECT CNTR_NO,'N' AS CHK
FROM BKG_CNTR_HIS
WHERE BKG_NO = @[bkg_no]
AND CORR_NO = 'TMP0000001'
#else
SELECT CNTR_NO,'N' AS CHK
FROM BKG_CONTAINER
WHERE BKG_NO = @[bkg_no]
#end
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
