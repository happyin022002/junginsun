<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOEstimatedDBDAOGlEstmIfErpCSQL">
			<desc><![CDATA[GL_ESTM_IF_ERP Insert]]></desc>
			<sql><![CDATA[
INSERT INTO GL_ESTM_IF_ERP (
	  EXE_YRMON         /*EXE_YRMON*/
    , SYS_SRC_ID        /*CONSTANT : CDA*/
    , REV_YRMON         /*REV_YRMON*/
    , ACCT_CD           /*ACT_CD*/
    , ESTM_SEQ_NO       /*MAX(ESTM_SEQ_NO) + 1*/
    , AGMT_NO           /*FLET_CTRT_TP_CD : TI, TO*/
    , WO_NO             /*VST_DT|VED_DT : 20150801|20150819*/
    , BIZ_UT_ID         /*CONSTANT : CNTR  */
    , LOC_CD            /*MDM_ORGANIZATION.LOC_CD*/
    , VSL_CD            /*VSL_CD*/
    , SKD_VOY_NO        /*SKD_VOY_NO*/
    , SKD_DIR_CD        /*SKD_DIR_CD*/
    , REV_DIR_CD        /*REV_DIR_CD*/
    , SAIL_DYS          /*SAIL_DYS*/
    , VVD_DUR_DYS       /*ESTM_DYS*/
    , HIR_DT_AMT        /*HIRE_AMT*/
    , ESTM_AMT          /*EST_AMT*/
    , ACT_AMT           /*ACT_AMT*/
    , ACCL_AMT          /*ACC_AMT*/
    , ESTM_VVD_TP_CD    /*ESTM_VVD_TP_CD : RV*/
    , ESTM_IOC_DIV_CD   /*GL_ESTM_REV_VVD.ESTM_IOC_DIV_CD*/
    , ESTM_VVD_HDR_ID   /*GL_ESTM_REV_VVD.ESTM_VVD_HDR_ID*/
    , ESTM_BC_DIV_CD    /*GL_ESTM_REV_VVD.ESTM_BC_DIV_CD*/
    , LOCL_CURR_CD      /*CONSTANT : USD*/
    , ACT_DT            /*ACT_DT*/
    , SLAN_CD           /*SUBSTR(RLANE_CD,1,3)*/
    , VNDR_INV_NO       /*SLP_EFF_DT|SLP_EXP_DT*/
    , CRE_USR_ID        /*CRE_USR_ID*/
    , CRE_DT            /*SYSDATE*/
    , UPD_USR_ID        /*CRE_USR_ID*/
    , UPD_DT            /*SYSDATE*/
)
SELECT @[exe_yrmon]                         /*PK - EXE_YRMON*/
     , 'CDA'                                /*PK - SYS_SRC_ID*/
     , @[rev_yrmon]                         /*PK - REV_YRMON*/
     , @[acct_cd]                           /*PK - ACCT_CD*/ 
     , (SELECT NVL(MAX(ESTM_SEQ_NO), 1) + 1
          FROM GL_ESTM_IF_ERP E
         WHERE E.EXE_YRMON  = @[exe_yrmon]
           AND E.SYS_SRC_ID = 'CDA'
           AND E.REV_YRMON  = @[rev_yrmon]
           AND E.ACCT_CD    = @[acct_cd])   /*PK - ESTM_SEQ_NO*/
      , @[re_divr_cd]                       /*AGMT_NO : Expense/Revenue FLET_CTRT_TP_CD*/
      , @[vst_dt]||'|'||@[ved_dt]           /*WO_NO : VST_DT|VED_DT*/
      , 'CNTR'                              /*BIZ_UT_ID*/
      , (SELECT LOC_CD 
           FROM MDM_ORGANIZATION 
          WHERE OFC_CD = @[slp_ofc_cd])     /*LOC_CD*/
      , @[vsl_cd]                           /*VSL_CD*/
      , @[skd_voy_no]                       /*SKD_VOY_NO*/
      , @[skd_dir_cd]                       /*SKD_DIR_CD*/
      , @[rev_dir_cd]                       /*REV_DIR_CD*/
      , @[sail_dys]                         /*SAIL_DYS*/
      , @[estm_dys]                         /*VVD_DUR_DYS*/
      , @[hire_amt]                         /*HIR_DT_AMT*/
      , @[est_amt]                          /*ESTM_AMT*/
      , @[act_amt]                          /*ACT_AMT*/
      , @[acc_amt]                          /*ACCL_AMT*/
      , G.ESTM_VVD_TP_CD                    /*ESTM_VVD_TP_CD*/
      , G.ESTM_IOC_DIV_CD                   /*ESTM_IOC_DIV_CD*/
      , DECODE(G.ESTM_VVD_HDR_ID, 0, NULL, G.ESTM_VVD_HDR_ID) /*ESTM_VVD_HDR_ID*/
      , G.ESTM_BC_DIV_CD                    /*ESTM_BC_DIV_CD*/
      , 'USD'                               /*LOCL_CURR_CD*/
      , @[act_dt]                           /*ACT_DT*/
      , SUBSTR(G.RLANE_CD,1,3)              /*SLAN_CD*/

#if (${slp_eff_dt} != '' && ${slp_exp_dt}) 
      , @[slp_eff_dt]||'|'||@[slp_exp_dt]   /*VNDR_INV_NO*/
#else 
      , NULL   							    /*VNDR_INV_NO*/
#end
      , @[cre_usr_id]                       /*CRE_USR_ID*/
      , SYSDATE                             /*CRE_DT*/
      , @[cre_usr_id]                       /*UPD_USR_ID*/
      , SYSDATE                             /*UPD_DT*/
  FROM (SELECT ESTM_VVD_TP_CD
             , ESTM_IOC_DIV_CD
             , ESTM_VVD_HDR_ID
             , ESTM_BC_DIV_CD
             , RLANE_CD
          FROM GL_ESTM_REV_VVD
         WHERE EXE_YRMON    = @[exe_yrmon]
           AND REV_YRMON    = @[rev_yrmon]
           AND VSL_CD       = @[vsl_cd]
           AND SKD_VOY_NO   = @[skd_voy_no]
           AND SKD_DIR_CD   = @[skd_dir_cd]
           AND REV_DIR_CD   = @[rev_dir_cd]
         ORDER BY 1 DESC
       ) G
 WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="vst_dt" type="12" value="" out="N"/>
				<param name="ved_dt" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="sail_dys" type="12" value="" out="N"/>
				<param name="estm_dys" type="12" value="" out="N"/>
				<param name="hire_amt" type="12" value="" out="N"/>
				<param name="est_amt" type="12" value="" out="N"/>
				<param name="act_amt" type="12" value="" out="N"/>
				<param name="acc_amt" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="slp_eff_dt" type="12" value="" out="N"/>
				<param name="slp_exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
