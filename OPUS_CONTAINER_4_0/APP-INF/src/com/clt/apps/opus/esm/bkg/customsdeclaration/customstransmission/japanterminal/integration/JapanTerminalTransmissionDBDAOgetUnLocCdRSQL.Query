<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOgetUnLocCdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT
       CASE
          WHEN SUBSTR(@[loc_cd], 1, 2) = 'JP' THEN
             NVL(ATTR_CTNT2, @[loc_cd])
          ELSE
             DECODE(ATTR_CTNT2, NULL, DECODE(SUBSTR(@[loc_cd], 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(@[loc_cd], 1, 2))||'ZZZ', ATTR_CTNT2)
       END

  FROM BKG_CSTMS_CD_CONV_CTNT

 WHERE CNT_CD(+) = 'JP'
   AND CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND @[loc_cd] = ATTR_CTNT1(+)
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
