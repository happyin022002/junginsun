<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusManageDBDAOCreateTransferCsqQtaLodRevCSQL">
			<desc><![CDATA[Qta Transfer 시 연간 정보로 1Q CSQ_QTA_LOD_REV 생성]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_QTA_LOD_REV (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,QTA_STEP_CD
      ,QTA_VER_NO
      ,TRD_CD
      ,RLANE_CD
      ,VSL_CD
      ,SKD_VOY_NO
      ,SKD_DIR_CD
      ,CONV_DIR_CD
      ,DIR_CD
      ,SUB_TRD_CD
      ,FNL_BSA_CAPA
      ,LDF_RTO
      ,GRS_RPB_REV
      ,PA_CM_UC_AMT
      ,RA_CM_UC_AMT
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT 'Q'      AS BSE_TP_CD
      ,L.BSE_YR AS BSE_YR
      ,'1Q'     AS BSE_QTR_CD
      ,L.OFC_VW_CD
      ,L.QTA_STEP_CD
      ,SUBSTR(L.QTA_VER_NO, 1, 11) || '1Q' || SUBSTR(L.QTA_VER_NO, -2) AS QTA_VER_NO
      ,L.TRD_CD
      ,L.RLANE_CD
      ,L.VSL_CD
      ,L.SKD_VOY_NO
      ,L.SKD_DIR_CD
      ,L.CONV_DIR_CD
      ,L.DIR_CD
      ,L.SUB_TRD_CD
      ,L.FNL_BSA_CAPA
      ,L.LDF_RTO
      ,L.GRS_RPB_REV
      ,L.PA_CM_UC_AMT
      ,L.RA_CM_UC_AMT
      ,@[f_usr_id] AS CRE_USR_ID
      ,SYSDATE     AS CRE_DT
      ,@[f_usr_id] AS UPD_USR_ID
      ,SYSDATE     AS UPD_DT
  FROM CSQ_QTA_TGT_VVD T
      ,CSQ_QTA_LOD_REV L
 WHERE T.BSE_TP_CD  = 'Y'
   AND T.BSE_YR     = @[f_bse_yr]
   AND T.BSE_QTR_CD = '00'
   AND T.BSE_TP_CD  = L.BSE_TP_CD
   AND T.BSE_YR     = L.BSE_YR
   AND T.BSE_QTR_CD = L.BSE_QTR_CD
   AND T.TRD_CD     = L.TRD_CD
   AND T.RLANE_CD   = L.RLANE_CD
   AND T.DIR_CD     = L.DIR_CD
   AND T.VSL_CD     = L.VSL_CD 
   AND T.SKD_VOY_NO = L.SKD_VOY_NO
   AND T.SKD_DIR_CD = L.SKD_DIR_CD
   AND T.BSE_WK BETWEEN '00' AND '13'			]]></sql>
			<params>
				<param name="f_usr_id" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
