<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchKrXptLicNoRSQL">
			<desc><![CDATA[korea export licens no 조회]]></desc>
			<sql><![CDATA[
SELECT XPT_LIC_NO           XPT_LIC_NO2
        , DECODE(CGO_DIVD_FLG, 'Y', DIVD_PCK_QTY, PCK_QTY )          PCK_QTY2
        , DECODE(CGO_DIVD_FLG, 'Y', DIVD_PCK_TP_CD, PCK_TP_CD )        PCK_TP_CD2
        , DECODE(CGO_DIVD_FLG, 'Y', DIVD_WGT, CNTR_WGT )         MF_WGT2
        , DECODE(SUBSTR(DECODE(CGO_DIVD_FLG, 'Y', DIVD_WGT_UT_CD, WGT_UT_CD ),1,1), 'K', 'KGS', DECODE(CGO_DIVD_FLG, 'Y', DIVD_WGT_UT_CD, WGT_UT_CD ) )        WGT_UT_CD2
        , CGO_DIVD_FLG		CGO_DIVD_FLG2
        , DIVD_SEQ          DIVD_SEQ2
        , STY_ID            SAM_PCK_ID2
--        , DIVD_PCK_QTY      SAM_PCK_QTY2
--        , DIVD_PCK_TP_CD    SAM_PCK_TP_CD2
--        , DIVD_WGT			DIVD_WGT2
--        , DIVD_WGT_UT_CD	DIVD_WGT_UT_CD2
        , TO_CHAR(XPT_LIC_NO_FILE_DT, 'yyyy-mm-dd') XPT_LIC_NO_FILE_DT2
        , XTER_TRSP_STS_CD
  FROM BKG_XTER_XPT_LIC_NO MST
 WHERE MST.XTER_SNDR_ID  = @[sender_id]
   AND MST.XTER_RQST_NO  = @[rqst_no]
   AND MST.XTER_RQST_SEQ = @[rqst_seq]			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
