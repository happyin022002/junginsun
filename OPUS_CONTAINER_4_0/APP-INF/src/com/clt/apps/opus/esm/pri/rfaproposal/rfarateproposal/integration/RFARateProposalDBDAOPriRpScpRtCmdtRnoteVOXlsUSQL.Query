<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtCmdtRnoteVOXlsUSQL">
			<desc><![CDATA[UPDATE PRI_RP_SCP_RT_CMDT_RNOTE]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_RT_CMDT_RNOTE
   SET PRC_PROG_STS_CD = 'I'
     , SRC_INFO_CD = 'AD'
     , N1ST_CMNC_AMDT_SEQ = @[amdt_seq]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT = SYSDATE
WHERE PROP_NO           = @[prop_no]
  AND AMDT_SEQ          = @[amdt_seq]
  AND SVC_SCP_CD        = @[svc_scp_cd]
  AND CMDT_HDR_SEQ      = @[cmdt_hdr_seq]
  AND ROUT_SEQ          = @[rout_seq]			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
