<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchRHQComboListRSQL">
			<desc><![CDATA[RHQ Combo List 조회]]></desc>
			<sql><![CDATA[
  SELECT T.OFC_CD,
         T.OFC_ENG_NM
    FROM SAQ_ORGANIZATION_V T
   WHERE T.LVL = 2
     AND T.DELT_FLG IN ('N', @[del])
ORDER BY 1			]]></sql>
			<params>
				<param name="del" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
