<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOSearchAdvJpVslSkdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(ETB_DT, 'YYYY-MM-DD HH24:MI') AS VPS_DT,
       DECODE(JO_CD1, 'Y', '1', '') AS RLX_DIV,
       MF_SND_FLG,
       FNL_EDI_SND_FLG

  FROM BKG_CSTMS_ADV_JP_VSL_SKD

 WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND POL_CD = @[pol_cd]
			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
