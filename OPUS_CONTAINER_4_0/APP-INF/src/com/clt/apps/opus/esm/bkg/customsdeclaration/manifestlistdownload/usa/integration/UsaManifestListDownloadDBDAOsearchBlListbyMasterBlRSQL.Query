<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchBlListbyMasterBlRSQL">
			<desc><![CDATA[master bl_no로 master bl_no + house bl_no 리스트를 구한다.]]></desc>
			<sql><![CDATA[
SELECT BL_NO
FROM BKG_CSTMS_ADV_BL
WHERE CNT_CD = @[cnt_cd]
AND MF_NO = @[bl_no]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
