<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOAddBlESignatureCSQL">
			<desc><![CDATA[Add BlESignature]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BL_ESIG (
	BL_ESIG_SEQ
	, ESIG_N1ST_NM
	, ESIG_LST_NM
	, CNT_CD
	, ACT_FLG
	, ESIG_FILE_NM
	, ESIG_FILE_PATH_RMK
	, ESIG_FILE_SAV_ID
	, INIT_FILE_NM
	, INIT_FILE_PATH_RMK
	, INIT_FILE_SAV_ID
	, ESIG_DESC
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
) VALUES (
	NVL((SELECT MAX(BL_ESIG_SEQ)+1 FROM BKG_BL_ESIG), 1)
	, @[esig_n1st_nm]
	, @[esig_lst_nm]
	, @[cnt_cd]
	, @[act_flg]
	, @[esig_file_nm]
	, @[esig_file_path_rmk]
	, @[esig_file_sav_id]
	, @[init_file_nm]
	, @[init_file_path_rmk]
	, @[init_file_sav_id]
	, @[esig_desc]
	, @[cre_usr_id]
	, SYSDATE
	, @[upd_usr_id]
	, SYSDATE
)			]]></sql>
			<params>
				<param name="esig_n1st_nm" type="12" value="" out="N"/>
				<param name="esig_lst_nm" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="act_flg" type="12" value="" out="N"/>
				<param name="esig_file_nm" type="12" value="" out="N"/>
				<param name="esig_file_path_rmk" type="12" value="" out="N"/>
				<param name="esig_file_sav_id" type="12" value="" out="N"/>
				<param name="init_file_nm" type="12" value="" out="N"/>
				<param name="init_file_path_rmk" type="12" value="" out="N"/>
				<param name="init_file_sav_id" type="12" value="" out="N"/>
				<param name="esig_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
