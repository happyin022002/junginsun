<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOAddChgTaxRateCSQL">
			<desc><![CDATA[BKG_CHG_TAX_RT Data에 해당하는 정보를 추가한다]]></desc>
			<sql><![CDATA[
#if (${caflag} == 'Y') 

	 INSERT INTO BKG_CHG_TAX_RT_HIS (
	   BKG_NO,CORR_NO, RT_SEQ, DP_SEQ, 
	   FRT_TERM_CD, CGO_CATE_CD, 
	   CHG_CD, CURR_CD, 
	   RAT_UT_CD, RAT_AS_QTY, 
	   CHG_UT_AMT, CHG_AMT,  
	   FRT_INCL_XCLD_DIV_CD, PCT_BSE_CD, TAX_FLG,
	   CRE_USR_ID, CRE_DT, 
	   UPD_USR_ID, UPD_DT) 
	VALUES (
	   @[bkg_no], 'TMP0000001',
	   @[rt_seq],
	   (SELECT A.DP_SEQ FROM MDM_CHARGE A WHERE A.CHG_CD=@[chg_cd] AND DELT_FLG='N') , 
	   @[frt_term_cd], @[cgo_cate_cd], 
	   @[chg_cd], @[curr_cd], 
	   @[rat_ut_cd], @[rat_as_qty], 
	   @[chg_ut_amt], @[chg_amt],
	   @[incl_oft_flg], @[pct_bse_cd], @[tax_flg],
	   @[cre_usr_id], sysdate, 
	   @[upd_usr_id], sysdate
	   )
#else
	 INSERT INTO BKG_CHG_TAX_RT (
	   BKG_NO,RT_SEQ, DP_SEQ, 
	   FRT_TERM_CD, CGO_CATE_CD, 
	   CHG_CD, CURR_CD, 
	   RAT_UT_CD, RAT_AS_QTY, 
	   CHG_UT_AMT, CHG_AMT,  
	   FRT_INCL_XCLD_DIV_CD, PCT_BSE_CD, TAX_FLG,
	   CRE_USR_ID, CRE_DT, 
	   UPD_USR_ID, UPD_DT) 
	VALUES (
	   @[bkg_no],
	   @[rt_seq],
	   (SELECT A.DP_SEQ FROM MDM_CHARGE A WHERE A.CHG_CD=@[chg_cd] AND DELT_FLG='N') , 
	   @[frt_term_cd], @[cgo_cate_cd], 
	   @[chg_cd], @[curr_cd], 
	   @[rat_ut_cd], @[rat_as_qty], 
	   @[chg_ut_amt], @[chg_amt],
	   @[incl_oft_flg], @[pct_bse_cd], @[tax_flg],
	   @[cre_usr_id], sysdate, 
	   @[upd_usr_id], sysdate
	   )
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rt_seq" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="frt_term_cd" type="12" value="" out="N"/>
				<param name="cgo_cate_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="rat_ut_cd" type="12" value="" out="N"/>
				<param name="rat_as_qty" type="12" value="" out="N"/>
				<param name="chg_ut_amt" type="12" value="" out="N"/>
				<param name="chg_amt" type="12" value="" out="N"/>
				<param name="incl_oft_flg" type="12" value="" out="N"/>
				<param name="pct_bse_cd" type="12" value="" out="N"/>
				<param name="tax_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
