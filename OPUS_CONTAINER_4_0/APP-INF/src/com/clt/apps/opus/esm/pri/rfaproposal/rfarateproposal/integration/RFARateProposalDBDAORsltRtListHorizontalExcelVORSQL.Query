<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltRtListHorizontalExcelVORSQL">
			<desc><![CDATA[Rate Excel - Horizontal]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
       '' AS TYPE
     , CMDT.CMDT_HDR_SEQ
     , CMDT.PRC_CMDT_DEF_CD
     , CMDT.PRC_CMDT_DEF_NM
     , CUST.CUST_SEQ
     , CUST.CUST_LGL_ENG_NM
     , ROUT.ROUT_SEQ
     , ORG.ORG_ROUT_PNT_LOC_DEF_CD
     , ORG.ORG_ROUT_PNT_LOC_DEF_NM
     , ORG.ORG_RCV_DE_TERM_NM 
     , ORG.ORG_PRC_TRSP_MOD_NM
     , VORG.ORG_ROUT_VIA_PORT_DEF_CD
     , VDES.DEST_ROUT_VIA_PORT_DEF_CD
     , DES.DEST_ROUT_PNT_LOC_DEF_CD
     , DES.DEST_ROUT_PNT_LOC_DEF_NM
     , DES.DEST_RCV_DE_TERM_NM
     , DES.DEST_PRC_TRSP_MOD_NM
     , RATE.PREFIX AS PREFIX_NM
     , RATE.PRC_CGO_TP_CD
     , RATE.RATE_20
     , RATE.RATE_40
     , RATE.RATE_40HC
     , RATE.RATE_45
     , CNOTE.CNOTE_DESC
     , RNOTE.RNOTE_DESC
  FROM (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , LISTAGG (DECODE(A.LOC_GB, 1, A.PRC_CMDT_DEF_CD), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ)  AS PRC_CMDT_DEF_CD
     , LISTAGG (DECODE(A.LOC_GB, 1, A.PRC_CMDT_DEF_NM), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ)  AS PRC_CMDT_DEF_NM
  FROM (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.PRC_CMDT_DEF_CD
     , A.PRC_CMDT_DEF_NM
     , ROW_NUMBER() OVER (PARTITION BY A.CMDT_HDR_SEQ, A.PRC_CMDT_DEF_CD ORDER BY A.CMDT_HDR_SEQ) AS LOC_GB
  FROM (
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.PRC_CMDT_DEF_CD  AS PRC_CMDT_DEF_CD
     , B.CMDT_NM AS PRC_CMDT_DEF_NM
  FROM PRI_RP_SCP_RT_CMDT A
     , MDM_COMMODITY B
 WHERE A.PRC_CMDT_DEF_CD   = B.CMDT_CD
   AND A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND A.SRC_INFO_CD       <> 'AD'
 UNION ALL
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.PRC_CMDT_DEF_CD  AS PRC_CMDT_DEF_CD
     , B.PRC_GRP_CMDT_DESC AS PRC_CMDT_DEF_NM
  FROM PRI_RP_SCP_RT_CMDT A
     , PRI_RP_SCP_GRP_CMDT B
 WHERE A.PROP_NO = B.PROP_NO
   AND A.AMDT_SEQ = B.AMDT_SEQ
   AND A.SVC_SCP_CD = B.SVC_SCP_CD
   AND A.PRC_CMDT_DEF_CD = B.PRC_GRP_CMDT_CD   
   AND A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND A.SRC_INFO_CD       <> 'AD' ) A) A
 GROUP BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.CMDT_HDR_SEQ ) CMDT
     , (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , LISTAGG (DECODE(A.LOC_GB, 1, A.CUST_SEQ), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ)  AS CUST_SEQ
     , LISTAGG (DECODE(A.LOC_GB, 1, A.CUST_LGL_ENG_NM), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ)  AS CUST_LGL_ENG_NM
  FROM (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.CUST_SEQ
     , A.CUST_LGL_ENG_NM
     , ROW_NUMBER() OVER (PARTITION BY A.CMDT_HDR_SEQ, A.CUST_SEQ ORDER BY A.CMDT_HDR_SEQ) AS LOC_GB
  FROM (
 SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , (A.CUST_CNT_CD||A.CUST_SEQ)  AS CUST_SEQ
     , B.CUST_LGL_ENG_NM
  FROM PRI_RP_SCP_RT_ACT_CUST A
     , MDM_CUSTOMER B
 WHERE A.CUST_CNT_CD       = B.CUST_CNT_CD
   AND A.CUST_SEQ          = B.CUST_SEQ
   AND A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND A.SRC_INFO_CD       <> 'AD' ) A) A
 GROUP BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.CMDT_HDR_SEQ ) CUST
     , (
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
 WHERE A.PROP_NO             = @[prop_no]
   AND A.AMDT_SEQ            = @[amdt_seq]
   AND A.SVC_SCP_CD          = @[svc_scp_cd]) ROUT     
     , (

SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , LISTAGG (DECODE(A.LOC_GB, 1, A.ORG_ROUT_PNT_LOC_DEF_CD), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS ORG_ROUT_PNT_LOC_DEF_CD
     , LISTAGG (DECODE(A.LCNM_GB, 1, A.ORG_ROUT_PNT_LOC_DEF_NM), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS ORG_ROUT_PNT_LOC_DEF_NM
     , LISTAGG (DECODE(A.TERM_GB, 1, A.ORG_RCV_DE_TERM_NM), ';')      WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS ORG_RCV_DE_TERM_NM
     , LISTAGG (DECODE(A.TRSP_GB, 1, A.ORG_PRC_TRSP_MOD_NM), ';')     WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS ORG_PRC_TRSP_MOD_NM
  FROM (
SELECT        
       X.PROP_NO
     , X.AMDT_SEQ
     , X.SVC_SCP_CD
     , X.CMDT_HDR_SEQ
     , X.ROUT_SEQ
     , X.ORG_ROUT_PNT_LOC_DEF_CD
     , X.ORG_ROUT_PNT_LOC_DEF_NM
     , X.ORG_RCV_DE_TERM_NM
     , X.ORG_PRC_TRSP_MOD_NM
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.ORG_ROUT_PNT_LOC_DEF_CD ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS LOC_GB
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.ORG_ROUT_PNT_LOC_DEF_NM ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS LCNM_GB
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.ORG_RCV_DE_TERM_NM ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS TERM_GB
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.ORG_PRC_TRSP_MOD_NM ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS TRSP_GB
  FROM (
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD
     , C.LOC_NM              AS ORG_ROUT_PNT_LOC_DEF_NM
     , D.INTG_CD_VAL_DP_DESC AS ORG_RCV_DE_TERM_NM
     , E.INTG_CD_VAL_DP_DESC AS ORG_PRC_TRSP_MOD_NM
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_PNT B
     , MDM_LOCATION C
     , COM_INTG_CD_DTL D
     , COM_INTG_CD_DTL E
 WHERE A.PROP_NO             = B.PROP_NO
   AND A.AMDT_SEQ            = B.AMDT_SEQ
   AND A.SVC_SCP_CD          = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ        = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ            = B.ROUT_SEQ
   AND A.PROP_NO             = @[prop_no]
   AND A.AMDT_SEQ            = @[amdt_seq]
   AND A.SVC_SCP_CD          = @[svc_scp_cd]
   AND B.SRC_INFO_CD         <> 'AD'
   AND B.ORG_DEST_TP_CD      = 'O'
   AND B.ROUT_PNT_LOC_DEF_CD = C.LOC_CD
   AND D.INTG_CD_ID(+)       = 'CD02070'
   AND D.INTG_CD_VAL_CTNT(+) = B.RCV_DE_TERM_CD
   AND E.INTG_CD_ID(+)       = 'CD01720'
   AND E.INTG_CD_VAL_CTNT(+) = B.PRC_TRSP_MOD_CD
 UNION ALL
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD
     , C.PRC_GRP_LOC_DESC    AS ORG_ROUT_PNT_LOC_DEF_NM
     , D.INTG_CD_VAL_DP_DESC AS ORG_RCV_DE_TERM_NM
     , E.INTG_CD_VAL_DP_DESC AS ORG_PRC_TRSP_MOD_NM
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_PNT B
     , PRI_RP_SCP_GRP_LOC C
     , COM_INTG_CD_DTL D
     , COM_INTG_CD_DTL E
 WHERE A.PROP_NO             = B.PROP_NO
   AND A.AMDT_SEQ            = B.AMDT_SEQ
   AND A.SVC_SCP_CD          = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ        = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ            = B.ROUT_SEQ
   AND A.PROP_NO             = @[prop_no]
   AND A.AMDT_SEQ            = @[amdt_seq]
   AND A.SVC_SCP_CD          = @[svc_scp_cd]
   AND B.SRC_INFO_CD         <> 'AD'
   AND B.ORG_DEST_TP_CD      = 'O'
   AND A.PROP_NO             = C.PROP_NO
   AND A.AMDT_SEQ            = C.AMDT_SEQ
   AND A.SVC_SCP_CD          = C.SVC_SCP_CD
   AND B.ROUT_PNT_LOC_DEF_CD = C.PRC_GRP_LOC_CD
   AND D.INTG_CD_ID(+)       = 'CD02070'
   AND D.INTG_CD_VAL_CTNT(+) = B.RCV_DE_TERM_CD
   AND E.INTG_CD_ID(+)       = 'CD01720'
   AND E.INTG_CD_VAL_CTNT(+) = B.PRC_TRSP_MOD_CD ) X ) A
 GROUP BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ ) ORG
     , (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , LISTAGG (DECODE(A.LOC_GB, 1, A.DEST_ROUT_PNT_LOC_DEF_CD), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS DEST_ROUT_PNT_LOC_DEF_CD
     , LISTAGG (DECODE(A.LCNM_GB, 1, A.DEST_ROUT_PNT_LOC_DEF_NM), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS DEST_ROUT_PNT_LOC_DEF_NM
     , LISTAGG (DECODE(A.TERM_GB, 1, A.DEST_RCV_DE_TERM_NM), ';')      WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS DEST_RCV_DE_TERM_NM
     , LISTAGG (DECODE(A.TRSP_GB, 1, A.DEST_PRC_TRSP_MOD_NM), ';')     WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS DEST_PRC_TRSP_MOD_NM
  FROM (
SELECT 
       X.PROP_NO
     , X.AMDT_SEQ
     , X.SVC_SCP_CD
     , X.CMDT_HDR_SEQ
     , X.ROUT_SEQ
     , X.DEST_ROUT_PNT_LOC_DEF_CD
     , X.DEST_ROUT_PNT_LOC_DEF_NM
     , X.DEST_RCV_DE_TERM_NM
     , X.DEST_PRC_TRSP_MOD_NM
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.DEST_ROUT_PNT_LOC_DEF_CD ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS LOC_GB
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.DEST_ROUT_PNT_LOC_DEF_NM ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS LCNM_GB
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.DEST_RCV_DE_TERM_NM ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS TERM_GB
   , ROW_NUMBER() OVER (PARTITION BY X.CMDT_HDR_SEQ, X.ROUT_SEQ, X.DEST_PRC_TRSP_MOD_NM ORDER BY X.CMDT_HDR_SEQ, X.ROUT_SEQ) AS TRSP_GB
  FROM (
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD
     , C.LOC_NM              AS DEST_ROUT_PNT_LOC_DEF_NM
     , D.INTG_CD_VAL_DP_DESC AS DEST_RCV_DE_TERM_NM
     , E.INTG_CD_VAL_DP_DESC AS DEST_PRC_TRSP_MOD_NM
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_PNT B
     , MDM_LOCATION C
     , COM_INTG_CD_DTL D
     , COM_INTG_CD_DTL E
 WHERE A.PROP_NO             = B.PROP_NO
   AND A.AMDT_SEQ            = B.AMDT_SEQ
   AND A.SVC_SCP_CD          = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ        = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ            = B.ROUT_SEQ
   AND A.PROP_NO             = @[prop_no]
   AND A.AMDT_SEQ            = @[amdt_seq]
   AND A.SVC_SCP_CD          = @[svc_scp_cd]
   AND B.SRC_INFO_CD         <> 'AD'
   AND B.ORG_DEST_TP_CD      = 'D'
   AND B.ROUT_PNT_LOC_DEF_CD = C.LOC_CD
   AND D.INTG_CD_ID(+)       = 'CD02071'
   AND D.INTG_CD_VAL_CTNT(+) = B.RCV_DE_TERM_CD
   AND E.INTG_CD_ID(+)       = 'CD01720'
   AND E.INTG_CD_VAL_CTNT(+) = B.PRC_TRSP_MOD_CD
 UNION ALL
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD
     , C.PRC_GRP_LOC_DESC    AS DEST_ROUT_PNT_LOC_DEF_NM
     , D.INTG_CD_VAL_DP_DESC AS DEST_RCV_DE_TERM_NM
     , E.INTG_CD_VAL_DP_DESC AS DEST_PRC_TRSP_MOD_NM
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_PNT B
     , PRI_RP_SCP_GRP_LOC C
     , COM_INTG_CD_DTL D
     , COM_INTG_CD_DTL E
 WHERE A.PROP_NO             = B.PROP_NO
   AND A.AMDT_SEQ            = B.AMDT_SEQ
   AND A.SVC_SCP_CD          = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ        = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ            = B.ROUT_SEQ
   AND A.PROP_NO             = @[prop_no]
   AND A.AMDT_SEQ            = @[amdt_seq]
   AND A.SVC_SCP_CD          = @[svc_scp_cd]
   AND B.SRC_INFO_CD         <> 'AD'
   AND B.ORG_DEST_TP_CD      = 'D'
   AND A.PROP_NO             = C.PROP_NO
   AND A.AMDT_SEQ            = C.AMDT_SEQ
   AND A.SVC_SCP_CD          = C.SVC_SCP_CD
   AND B.ROUT_PNT_LOC_DEF_CD = C.PRC_GRP_LOC_CD
   AND D.INTG_CD_ID(+)       = 'CD02071'
   AND D.INTG_CD_VAL_CTNT(+) = B.RCV_DE_TERM_CD
   AND E.INTG_CD_ID(+)       = 'CD01720'
   AND E.INTG_CD_VAL_CTNT(+) = B.PRC_TRSP_MOD_CD ) X ) A
 GROUP BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ ) DES
     , (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , LISTAGG (DECODE(A.VIA_GB, 1, A.ORG_ROUT_VIA_PORT_DEF_CD), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS ORG_ROUT_VIA_PORT_DEF_CD
  FROM (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , A.ORG_ROUT_VIA_PORT_DEF_CD
     , ROW_NUMBER() OVER (PARTITION BY A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_ROUT_VIA_PORT_DEF_CD ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ) AS VIA_GB
  FROM (
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_VIA_PORT_DEF_CD AS ORG_ROUT_VIA_PORT_DEF_CD
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_VIA B
 WHERE A.PROP_NO           = B.PROP_NO
   AND A.AMDT_SEQ          = B.AMDT_SEQ
   AND A.SVC_SCP_CD        = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ      = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ          = B.ROUT_SEQ
   AND A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND B.SRC_INFO_CD       <> 'AD'
   AND B.ORG_DEST_TP_CD    = 'O'
 UNION ALL
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_VIA_PORT_DEF_CD  AS ORG_ROUT_VIA_PORT_DEF_CD
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_VIA B
 WHERE A.PROP_NO           = B.PROP_NO
   AND A.AMDT_SEQ          = B.AMDT_SEQ
   AND A.SVC_SCP_CD        = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ      = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ          = B.ROUT_SEQ
   AND A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND B.SRC_INFO_CD       <> 'AD'
   AND B.ORG_DEST_TP_CD    = 'O' ) A ) A
 GROUP BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ ) VORG
     , (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , LISTAGG (DECODE(A.VIA_GB, 1, A.DEST_ROUT_VIA_PORT_DEF_CD), ';') WITHIN GROUP (ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ)  AS DEST_ROUT_VIA_PORT_DEF_CD
  FROM (
SELECT
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , A.DEST_ROUT_VIA_PORT_DEF_CD
     , ROW_NUMBER() OVER (PARTITION BY A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.DEST_ROUT_VIA_PORT_DEF_CD ORDER BY A.CMDT_HDR_SEQ, A.ROUT_SEQ) AS VIA_GB
  FROM (
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_VIA_PORT_DEF_CD  AS DEST_ROUT_VIA_PORT_DEF_CD
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_VIA B
 WHERE A.PROP_NO           = B.PROP_NO
   AND A.AMDT_SEQ          = B.AMDT_SEQ
   AND A.SVC_SCP_CD        = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ      = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ          = B.ROUT_SEQ
   AND A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND B.SRC_INFO_CD       <> 'AD'
   AND B.ORG_DEST_TP_CD    = 'D'
 UNION ALL
SELECT 
       A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , B.ROUT_VIA_PORT_DEF_CD  AS DEST_ROUT_VIA_PORT_DEF_CD
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
     , PRI_RP_SCP_RT_ROUT_VIA B
 WHERE A.PROP_NO           = B.PROP_NO
   AND A.AMDT_SEQ          = B.AMDT_SEQ
   AND A.SVC_SCP_CD        = B.SVC_SCP_CD
   AND A.CMDT_HDR_SEQ      = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ          = B.ROUT_SEQ
   AND A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND B.SRC_INFO_CD       <> 'AD'
   AND B.ORG_DEST_TP_CD    = 'D' ) A ) A
 GROUP BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ ) VDES
     , (
 SELECT
       T2.PROP_NO 
     , T2.AMDT_SEQ 
     , T2.SVC_SCP_CD 
     , T2.CMDT_HDR_SEQ
     , T2.ROUT_SEQ
     , T2.PREFIX
     , T2.PRC_CGO_TP_CD
     , SUM(T2.RATE_20)   AS RATE_20
     , SUM(T2.RATE_40)   AS RATE_40
     , SUM(T2.RATE_40HC) AS RATE_40HC
     , SUM(T2.RATE_45)   AS RATE_45
  FROM (SELECT
               T1.PROP_NO 
             , T1.AMDT_SEQ 
             , T1.SVC_SCP_CD 
             , T1.CMDT_HDR_SEQ
             , T1.ROUT_SEQ
             , T1.PREFIX
             , T1.PRC_CGO_TP_CD
             , CASE WHEN T1.PREFIX_TYPE = '2' THEN T1.PROP_FRT_RT_AMT END RATE_20
             , CASE WHEN T1.PREFIX_TYPE = '4' THEN T1.PROP_FRT_RT_AMT END RATE_40
             , CASE WHEN T1.PREFIX_TYPE = '5' THEN T1.PROP_FRT_RT_AMT END RATE_40HC
             , CASE WHEN T1.PREFIX_TYPE = '7' THEN T1.PROP_FRT_RT_AMT END RATE_45
          FROM (SELECT 
                       A.PROP_NO 
                     , A.AMDT_SEQ 
                     , A.SVC_SCP_CD 
                     , A.CMDT_HDR_SEQ
                     , A.ROUT_SEQ
                     , A.RAT_UT_CD
                     , SUBSTR(A.RAT_UT_CD, LENGTH(A.RAT_UT_CD), 1)  AS PREFIX_TYPE
                     , SUBSTR(A.RAT_UT_CD, LENGTH(A.RAT_UT_CD)-1, 1) AS PREFIX
                     , A.PRC_CGO_TP_CD
                     , A.PROP_FRT_RT_AMT
                  FROM PRI_RP_SCP_RT A
                     , (SELECT
                               RAT_UT_CD
                          FROM PRI_RAT_UT
                         WHERE DELT_FLG         = 'N'
                           AND CTRT_USE_ONY_FLG = 'Y'
                           AND SUBSTR(RAT_UT_CD, LENGTH(RAT_UT_CD), 1) IN ('2','4','5','7') ) B 
                 WHERE A.RAT_UT_CD         = B.RAT_UT_CD
                   AND A.PROP_NO           = @[prop_no]
                   AND A.AMDT_SEQ          = @[amdt_seq]
                   AND A.SVC_SCP_CD        = @[svc_scp_cd]
                   AND A.SRC_INFO_CD       <> 'AD' ) T1 ) T2
 GROUP BY T2.PROP_NO , T2.AMDT_SEQ , T2.SVC_SCP_CD , T2.CMDT_HDR_SEQ, T2.ROUT_SEQ, T2.PREFIX, T2.PRC_CGO_TP_CD) RATE
     , ( 
 SELECT 
       A.PROP_NO 
     , A.AMDT_SEQ 
     , A.SVC_SCP_CD 
     , A.CMDT_HDR_SEQ
     , A.NOTE_CTNT AS CNOTE_DESC
  FROM PRI_RP_SCP_RT_CNOTE A
 WHERE A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND A.SRC_INFO_CD       <> 'AD' ) CNOTE
     , ( 
SELECT 
       A.PROP_NO 
     , A.AMDT_SEQ 
     , A.SVC_SCP_CD 
     , A.CMDT_HDR_SEQ
     , A.ROUT_SEQ
     , A.NOTE_CTNT AS RNOTE_DESC
  FROM PRI_RP_SCP_RT_CMDT_RNOTE A
 WHERE A.PROP_NO           = @[prop_no]
   AND A.AMDT_SEQ          = @[amdt_seq]
   AND A.SVC_SCP_CD        = @[svc_scp_cd]
   AND A.SRC_INFO_CD       <> 'AD' ) RNOTE
 
 
 WHERE CMDT.PROP_NO      = CUST.PROP_NO(+)
   AND CMDT.AMDT_SEQ     = CUST.AMDT_SEQ(+)
   AND CMDT.SVC_SCP_CD   = CUST.SVC_SCP_CD(+)
   AND CMDT.CMDT_HDR_SEQ = CUST.CMDT_HDR_SEQ(+)
   AND CMDT.PROP_NO      = ROUT.PROP_NO(+)   
   AND CMDT.AMDT_SEQ     = ROUT.AMDT_SEQ(+)   
   AND CMDT.SVC_SCP_CD   = ROUT.SVC_SCP_CD(+)   
   AND CMDT.CMDT_HDR_SEQ = ROUT.CMDT_HDR_SEQ(+)  
   AND ROUT.PROP_NO      = ORG.PROP_NO(+)   
   AND ROUT.AMDT_SEQ     = ORG.AMDT_SEQ(+)   
   AND ROUT.SVC_SCP_CD   = ORG.SVC_SCP_CD(+)   
   AND ROUT.CMDT_HDR_SEQ = ORG.CMDT_HDR_SEQ(+)     
   AND ROUT.ROUT_SEQ     = ORG.ROUT_SEQ(+)   
   AND ROUT.PROP_NO      = DES.PROP_NO(+)
   AND ROUT.AMDT_SEQ     = DES.AMDT_SEQ(+)   
   AND ROUT.SVC_SCP_CD   = DES.SVC_SCP_CD(+)   
   AND ROUT.CMDT_HDR_SEQ = DES.CMDT_HDR_SEQ(+)     
   AND ROUT.ROUT_SEQ     = DES.ROUT_SEQ(+) 
   AND ROUT.PROP_NO      = VORG.PROP_NO(+)
   AND ROUT.AMDT_SEQ     = VORG.AMDT_SEQ(+)   
   AND ROUT.SVC_SCP_CD   = VORG.SVC_SCP_CD(+)   
   AND ROUT.CMDT_HDR_SEQ = VORG.CMDT_HDR_SEQ(+)     
   AND ROUT.ROUT_SEQ     = VORG.ROUT_SEQ(+) 
   AND ROUT.PROP_NO      = VDES.PROP_NO(+)
   AND ROUT.AMDT_SEQ     = VDES.AMDT_SEQ(+)   
   AND ROUT.SVC_SCP_CD   = VDES.SVC_SCP_CD(+)   
   AND ROUT.CMDT_HDR_SEQ = VDES.CMDT_HDR_SEQ(+)     
   AND ROUT.ROUT_SEQ     = VDES.ROUT_SEQ(+) 
   AND ROUT.PROP_NO      = RATE.PROP_NO(+)
   AND ROUT.AMDT_SEQ     = RATE.AMDT_SEQ(+)   
   AND ROUT.SVC_SCP_CD   = RATE.SVC_SCP_CD(+)   
   AND ROUT.CMDT_HDR_SEQ = RATE.CMDT_HDR_SEQ(+)     
   AND ROUT.ROUT_SEQ     = RATE.ROUT_SEQ(+)    
   AND ROUT.PROP_NO      = CNOTE.PROP_NO(+)
   AND ROUT.AMDT_SEQ     = CNOTE.AMDT_SEQ(+)   
   AND ROUT.SVC_SCP_CD   = CNOTE.SVC_SCP_CD(+)   
   AND ROUT.CMDT_HDR_SEQ = CNOTE.CMDT_HDR_SEQ(+)      
   AND ROUT.PROP_NO      = RNOTE.PROP_NO(+)
   AND ROUT.AMDT_SEQ     = RNOTE.AMDT_SEQ(+)   
   AND ROUT.SVC_SCP_CD   = RNOTE.SVC_SCP_CD(+)   
   AND ROUT.CMDT_HDR_SEQ = RNOTE.CMDT_HDR_SEQ(+)     
   AND ROUT.ROUT_SEQ     = RNOTE.ROUT_SEQ(+)    
ORDER BY CMDT.CMDT_HDR_SEQ,CMDT.PRC_CMDT_DEF_NM,CUST.CUST_SEQ,CUST.CUST_LGL_ENG_NM, ROUT.ROUT_SEQ, ORG.ORG_ROUT_PNT_LOC_DEF_CD,
         VORG.ORG_ROUT_VIA_PORT_DEF_CD, VDES.DEST_ROUT_VIA_PORT_DEF_CD, DES.DEST_ROUT_PNT_LOC_DEF_CD			]]></sql>
			<params>
				<param name="prop_no" type="12" value="SEL090016" out="N"/>
				<param name="amdt_seq" type="2" value="1" out="N"/>
				<param name="svc_scp_cd" type="12" value="TPE" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
