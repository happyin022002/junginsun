<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCntrExportRfTempRSQL">
			<desc><![CDATA[searchCntrExportRfTemp]]></desc>
			<sql><![CDATA[
SELECT DECODE(BK.RC_FLG,'Y',DECODE(RF.FDO_TEMP,NULL,DECODE(RF.CDO_TEMP,NULL,'N','Y'),'Y'),'Y')
FROM BKG_BOOKING BK, BKG_RF_CGO RF
WHERE BK.BKG_NO = @[bkg_no]
AND BK.BKG_NO = RF.BKG_NO(+)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
