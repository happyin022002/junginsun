<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOXptImpLicRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
#if (${cnt_cd} == 'CA')
SELECT BKG_NO,
       IO_BND_CD,
       XPT_IMP_SEQ,
       CNT_CD,
       CAED_TP_CD,
       CAED_PFX_CTNT,
       CAED_NO1,
       CAED_NO2,
       CAED_NO3,
       G7_EDI_PFX_CTNT,
       G7_EDI_NO1,
       G7_EDI_NO2,
       MF_SMRY_RPT_PFX_CTNT,
       MF_SMRY_RPT_NO,
       B13A_XPT_PFX_CTNT,
       TO_CHAR(B13A_XPT_DT,'yyyymmddhh24mi') B13A_XPT_DT,
       B13A_XPT_NO1,
       B13A_XPT_NO2,
       CGO_CTRL_PFX_CTNT,
       CGO_CTRL_NO,
       NDR_REF_PFX_CTNT,
       NDR_REF_ID,
       NDR_REF_CTNT,
       CRE_USR_ID,
       UPD_USR_ID
#if (${ca_flg} == 'Y')
  FROM BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else    
  FROM BKG_XPT_IMP_LIC    
 WHERE BKG_NO = @[bkg_no]
#end
   AND IO_BND_CD = @[io_bnd_cd]
   AND CNT_CD = 'CA'
#elseif(${cnt_cd} == 'BR')   
SELECT BKG_NO,
       IO_BND_CD,
       XPT_IMP_SEQ,
	   CNT_CD,
	   SHPR_TAX_NO,
	   CNEE_TAX_NO,
       NTFY_TAX_NO,
       BRZ_DECL_NO,
       CRE_USR_ID,
	   UPD_USR_ID
#if (${ca_flg} == 'Y')
  FROM BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else    
  FROM BKG_XPT_IMP_LIC    
 WHERE BKG_NO = @[bkg_no]
#end
   AND IO_BND_CD = @[io_bnd_cd]
   AND CNT_CD = 'BR'
#elseif(${cnt_cd} == 'ID')     
SELECT BKG_NO,
       IO_BND_CD,
       XPT_IMP_SEQ,
	   CNT_CD,
	   ID_XPT_NO,
	   to_char(ID_XPT_NO_ISS_DT,'yyyyMMdd') ID_XPT_NO_ISS_DT,
       ID_OFC_ID AS ID_OFC_CD,
       ID_DECL_CD,
       CRE_USR_ID,
	   UPD_USR_ID
#if (${ca_flg} == 'Y')
  FROM BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else    
  FROM BKG_XPT_IMP_LIC    
 WHERE BKG_NO = @[bkg_no]
#end
   AND IO_BND_CD = @[io_bnd_cd]
   AND CNT_CD = 'ID'
#elseif(${cnt_cd} == 'IN')
SELECT BKG_NO,
       IO_BND_CD,
       XPT_IMP_SEQ,
	   CNT_CD,
	   IDA_IEC_NO,
       CRE_USR_ID,
	   UPD_USR_ID
#if (${ca_flg} == 'Y')
  FROM BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else    
  FROM BKG_XPT_IMP_LIC    
 WHERE BKG_NO = @[bkg_no]
#end
   AND IO_BND_CD = @[io_bnd_cd]
   AND CNT_CD = 'IN' 
#elseif(${cnt_cd} == 'MX' || ${cnt_cd} == 'CO' || ${cnt_cd} == 'EC' || ${cnt_cd} == 'PE')
SELECT BKG_NO,
       IO_BND_CD,
       XPT_IMP_SEQ,
	   CNT_CD,
       MX_SHPR_TAX_ID,
       MX_CNEE_TAX_ID,
       MX_NTFY_TAX_ID,
       CRE_USR_ID,
	   UPD_USR_ID
#if (${ca_flg} == 'Y')
  FROM BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else    
  FROM BKG_XPT_IMP_LIC    
 WHERE BKG_NO = @[bkg_no]
#end
   AND IO_BND_CD = @[io_bnd_cd]
   AND CNT_CD = @[cnt_cd]

#elseif(${cnt_cd} == 'KR')
#if (${popuptpcd} == 'S')
SELECT XPT_LIC_NO 
      ,PCK_QTY     
      ,PCK_TP_CD   
      ,CNTR_WGT AS MF_WGT    
      ,WGT_UT_CD    
      ,CGO_DIVD_FLG AS DIVD_FLG    
	  ,DIVD_SEQ
      ,STY_ID AS SAM_PCK_ID
      ,DIVD_PCK_QTY AS SAM_PCK_QTY
      ,DIVD_PCK_TP_CD AS SAM_PCK_TP_CD
	  ,'' AS UCR_NO
  FROM BKG_XTER_XPT_LIC_NO
 WHERE XTER_SNDR_ID  = @[xter_sndr_id]
   AND XTER_RQST_NO  = @[xter_rqst_no]
   AND XTER_RQST_SEQ = @[xter_rqst_seq]
#else
SELECT BKG_NO,
       IO_BND_CD,
       XPT_IMP_SEQ,
	   CNT_CD,
       XPT_LIC_NO,
       PCK_QTY,
       PCK_TP_CD,
       MF_WGT AS,
       WGT_UT_CD,
       DIVD_FLG,
	   DIVD_SEQ,
       SAM_PCK_ID,
       SAM_PCK_QTY,
       SAM_PCK_TP_CD,
       UCR_NO,
       TS_REF_NO,
	   CRE_USR_ID,
	   UPD_USR_ID,
	   CRE_DT,
	   TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',UPD_DT,'GMT'), 'YYYY-MM-DD HH24:MI') AS UPD_DT
#if (${ca_flg} == 'Y')
  FROM BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else    
  FROM BKG_XPT_IMP_LIC    
 WHERE BKG_NO = @[bkg_no]
#end
   AND IO_BND_CD = @[io_bnd_cd]
   AND CNT_CD = 'KR'
#end   
#elseif(${cnt_cd} == 'US')
#if (${popuptpcd} == 'S')
select aes_inlnd_trns_no    AES_INLND_TRNS_NO
        , aes_pta_no1       AES_PTA_NO1
        , aes_pta_no2       AES_PTA_NO2
        , aes_pta_dt        AES_PTA_DT
        , aes_ptu_no        AES_PTU_NO
        , aes_ptu_dt        AES_PTU_DT
        , aes_dwn_no       AES_DWN_NO
        , aes_dwn_dt       AES_DWN_DT
        , aes_expt_ctnt     AES_EXPT_CTNT
  from bkg_xter_aes
 where xter_sndr_id = @[xter_sndr_id]
   and xter_rqst_no = @[xter_rqst_no]
   and xter_rqst_seq= @[xter_rqst_seq]
#else
SELECT BKG_NO,
       IO_BND_CD,
       XPT_IMP_SEQ,
	   CNT_CD,
       AES_TP_CD,
       AES_INLND_TRNS_PFX_CTNT,
       TO_CHAR(AES_INLND_TRNS_NO) AES_INLND_TRNS_NO,
       AES_PTA_PFX_CTNT,
       TO_CHAR(AES_PTA_NO1) AES_PTA_NO1,
       TO_CHAR(AES_PTA_NO2) AES_PTA_NO2,
       TO_CHAR(AES_PTA_DT,'MM-DD-YYYY') AES_PTA_DT,
       AES_PTU_PFX_CTNT,
       TO_CHAR(AES_PTU_NO) AES_PTU_NO,
       TO_CHAR(AES_PTU_DT,'MM-DD-YYYY') AES_PTU_DT,
       AES_DWN_PFX_CTNT,
       TO_CHAR(AES_DWN_NO) AES_DWN_NO,
       TO_CHAR(AES_DWN_DT,'MM-DD-YYYY') AES_DWN_DT,
       AES_EXPT_ID,
       AES_EXPT_CTNT,
	   CRE_USR_ID,
	   UPD_USR_ID
#if (${ca_flg} == 'Y')
  FROM BKG_XPT_IMP_LIC_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else    
  FROM BKG_XPT_IMP_LIC    
 WHERE BKG_NO = @[bkg_no]
#end
   AND IO_BND_CD = @[io_bnd_cd]
   AND CNT_CD = 'US'
#end
#elseif(${cnt_cd} == 'AR')
		SELECT BKG_NO
		    , IO_BND_CD
		    , XPT_IMP_SEQ
		    , CNT_CD
		    , XPT_LIC_NO
	#if (${ca_flg} == 'Y')
		FROM BKG_XPT_IMP_LIC_HIS
		WHERE BKG_NO = @[bkg_no]
	    AND CORR_NO = 'TMP0000001'
	#else    
		FROM BKG_XPT_IMP_LIC
		WHERE BKG_NO = @[bkg_no]
	#end
		AND IO_BND_CD = @[io_bnd_cd]
		AND CNT_CD = 'AR'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
