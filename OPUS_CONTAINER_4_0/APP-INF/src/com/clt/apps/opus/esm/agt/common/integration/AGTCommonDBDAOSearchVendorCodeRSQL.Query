<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchVendorCodeRSQL">
			<desc><![CDATA[searchVendorCode]]></desc>
			<sql><![CDATA[
SELECT 
	VNDR_CNT_CD||TO_CHAR(VNDR_SEQ,'FM000000') AS VNDR_CD,
	VNDR_LGL_ENG_NM
FROM MDM_VENDOR
WHERE VNDR_SEQ = @[seq]
	AND NVL(DELT_FLG,'N') = 'N'			]]></sql>
			<params>
				<param name="seq" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
