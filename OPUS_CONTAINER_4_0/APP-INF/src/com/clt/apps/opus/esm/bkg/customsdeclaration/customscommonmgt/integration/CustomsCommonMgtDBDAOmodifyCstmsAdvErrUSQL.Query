<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOmodifyCstmsAdvErrUSQL">
			<desc><![CDATA[modifyCstmsAdvErr]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_ERR
SET ERR_CD_DESC = @[err_cd_desc],
	ERR_TP_CTNT = @[err_tp_ctnt]
WHERE CNT_CD = @[cnt_cd]
AND CSTMS_ERR_CD = @[cstms_err_cd]			]]></sql>
			<params>
				<param name="err_cd_desc" type="12" value="" out="N"/>
				<param name="err_tp_ctnt" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_err_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
