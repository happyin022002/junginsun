<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchBkgVvdRouteRSQL">
			<desc><![CDATA[Select VVD and route of BKG_Booking]]></desc>
			<sql><![CDATA[
SELECT
       SLAN_CD
	  ,VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD
	  ,POR_CD
      ,POL_CD
      ,POD_CD
      ,DEL_CD
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
