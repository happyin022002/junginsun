<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchCNTRCntInfoListRSQL">
			<desc><![CDATA[Download 된 컨테이너 목록조회]]></desc>
			<sql><![CDATA[
SELECT KC.BKG_NO        BKG_NO
     , KC.CNTR_NO       CNTR_NO
     , KC.FULL_MTY_CD   TP_CD
     , KC.CSTMS_BL_NO   BL_NO
FROM BKG_CSTMS_KR_CNTR KC
WHERE KC.BKG_NO = @[bkg_no]
AND KC.CSTMS_DECL_TP_CD =   @[tp_cd]
AND KC.DMST_PORT_CD     =   @[port_cd]
AND KC.CSTMS_BL_NO  	=   @[c_bl_no]
AND KC.TRNS_SEQ         =   (   SELECT MAX(TRNS_SEQ)
                                FROM BKG_CSTMS_KR_BL
                                WHERE BKG_NO = @[bkg_no]
                                AND CSTMS_DECL_TP_CD = @[tp_cd]
                                AND DMST_PORT_CD     = @[port_cd]
                                AND VSL_CD           = SUBSTR(@[vvd_cd], 1, 4)
                                AND SKD_VOY_NO       = SUBSTR(@[vvd_cd], 5, 4)
                                AND SKD_DIR_CD       = SUBSTR(@[vvd_cd], 9, 1)
								AND KC.CSTMS_BL_NO   = @[c_bl_no]
                                )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="tp_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="c_bl_no" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
