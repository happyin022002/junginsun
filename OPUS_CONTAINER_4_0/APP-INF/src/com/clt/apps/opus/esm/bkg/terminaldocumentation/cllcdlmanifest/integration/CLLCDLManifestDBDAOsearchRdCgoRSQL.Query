<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchRdCgoRSQL">
			<desc><![CDATA[searchRdCgo]]></desc>
			<sql><![CDATA[
SELECT 'X' RD_EXIST
FROM BKG_CONTAINER C
WHERE BKG_NO = @[bkg_no] 
AND CNTR_NO = @[cntr_no] 
AND NOT EXISTS (SELECT 'X' FROM BKG_RF_CGO R WHERE R.BKG_NO = C.BKG_NO AND R.CNTR_NO = C.CNTR_NO)
AND CNTR_TPSZ_CD LIKE 'R%'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
