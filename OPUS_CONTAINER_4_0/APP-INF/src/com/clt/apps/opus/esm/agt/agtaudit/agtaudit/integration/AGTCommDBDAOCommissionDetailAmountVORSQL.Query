<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommDBDAOCommissionDetailAmountVORSQL">
			<desc><![CDATA[ESM_AGT_0011 화면 Commission Detail Amount 조회]]></desc>
			<sql><![CDATA[
SELECT 
		DECODE(AC_TP_CD,'G','Common1',
                       'N','Common2',
                       'K','Brokerage',
                       'H','CHF',
                       'S','T/S',
                       'R','T/R',
                       'O','SOC',
                       'C','Cross',
                       'D','Doc') as AC_TP_CD,
       SUM(ACT_IF_COMM_AMT) AS ACT_IF_COMM_AMT,
       SUM(ACT_IF_LOCL_COMM_AMT) AS ACT_IF_LOCL_COMM_AMT
      ,DECODE(AC_TP_CD,'G',1,'N',2,'K',3,'H',4,'S',5,'R',6,'O',7,'C',8,'D',9) AS AC_TP_CD_NUM
  FROM AGT_AGN_COMM
 WHERE BKG_NO = @[bkg_no]
   AND AGN_CD = @[agn_cd]
   AND IO_BND_CD = @[io_bnd_cd]
   AND AC_TP_CD <> 'T'
   AND AC_SEQ = @[ac_seq]
 GROUP BY AC_TP_CD
 ORDER BY 4			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="Y"/>
				<param name="agn_cd" type="12" value="" out="Y"/>
				<param name="io_bnd_cd" type="12" value="" out="Y"/>
				<param name="ac_seq" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
