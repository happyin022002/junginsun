<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLaneBoundSwitchSubTradeListRSQL">
			<desc><![CDATA[Sub Trade List 조회 ]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SUB_TRD_CD AS TEXT,  
              SUB_TRD_CD AS CODE   
FROM SAQ_TGT_GRP_TRD               
WHERE TRD_CD = @[trd_cd]                
ORDER BY SUB_TRD_CD                    
			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
