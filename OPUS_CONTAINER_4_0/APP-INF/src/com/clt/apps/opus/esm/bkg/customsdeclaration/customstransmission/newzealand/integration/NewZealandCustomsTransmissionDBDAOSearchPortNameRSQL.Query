<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandCustomsTransmissionDBDAOSearchPortNameRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(SUBSTR(LOC_NM,1,35),'') PORT_NM
FROM MDM_LOCATION
WHERE LOC_CD= @[frm_port_cd]			]]></sql>
			<params>
				<param name="frm_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
