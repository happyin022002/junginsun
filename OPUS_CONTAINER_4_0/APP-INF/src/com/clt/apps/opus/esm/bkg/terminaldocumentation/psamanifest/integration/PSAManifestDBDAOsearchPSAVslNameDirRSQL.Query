<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPSAVslNameDirRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT UPPER(TRIM(PSA_VSL_NM)) AS PSA_VSL_NM,
       UPPER(TRIM(PSA_VOY_DIR_CD)) AS PSA_VOY_DIR_CD

  FROM BKG_CSTMS_PSA_VVD

 WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
