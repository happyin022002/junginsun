<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOModifyMultiShpCntrPrnUSQL">
			<desc><![CDATA[ContainerNo./Print Flag update]]></desc>
			<sql><![CDATA[
UPDATE BKG_CNTR_SHP
    SET CNTR_NO	= @[cntr_no]
    , PCK_QTY	= @[pck_qty]
    , PCK_TP_CD = @[pck_tp_cd]
    , CNTR_MF_WGT 	= @[cntr_mf_wgt]
    , WGT_UT_CD		= @[wgt_ut_cd]
	, MEAS_QTY		= @[meas_qty]
    , MEAS_UT_CD	= @[meas_ut_cd]
    , MK_DESC		= @[mk_desc]
    , CMDT_DESC		= @[cmdt_desc]
    , PRN_FLG 		= DECODE(@[prn_flg],'1','Y','N')
    , CNTR_SEAL_NO1 = @[cntr_seal_no1]
    , CNTR_SEAL_NO2 = @[cntr_seal_no2]
    , HAMO_TRF_CD	= @[hamo_trf_cd]
    , CMDT_HS_CD	= @[cmdt_hs_cd]
    , NCM_NO		= @[ncm_no]
    , PO_NO			= @[po_no]
    , UPD_USR_ID	= @[usr_id]
    , UPD_DT 		= sysdate
WHERE BKG_NO 	= @[bkg_no]
AND CNTR_MF_SEQ = @[cntr_mf_seq]
AND CNTR_SEQ 	= @[cntr_seq]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_mf_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="meas_ut_cd" type="12" value="" out="N"/>
				<param name="mk_desc" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="prn_flg" type="12" value="" out="N"/>
				<param name="cntr_seal_no1" type="12" value="" out="N"/>
				<param name="cntr_seal_no2" type="12" value="" out="N"/>
				<param name="hamo_trf_cd" type="12" value="" out="N"/>
				<param name="cmdt_hs_cd" type="12" value="" out="N"/>
				<param name="ncm_no" type="12" value="" out="N"/>
				<param name="po_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_mf_seq" type="12" value="" out="N"/>
				<param name="cntr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
