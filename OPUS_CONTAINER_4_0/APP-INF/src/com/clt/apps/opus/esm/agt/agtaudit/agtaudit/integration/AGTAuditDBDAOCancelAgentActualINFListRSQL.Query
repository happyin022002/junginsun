<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOCancelAgentActualINFListRSQL">
			<desc><![CDATA[CancelAgentActualINFList]]></desc>
			<sql><![CDATA[
SELECT
           A.BKG_NO,
           A.AGN_CD,
           A.IO_BND_CD,
           A.AC_TP_CD,
           A.AC_SEQ,
           A.ACT_PRE_COMM_AMT,
           A.ACT_COMM_AMT,
           A.ACT_IF_COMM_AMT,
           A.ACT_PRE_LOCL_COMM_AMT,
           A.ACT_LOCL_COMM_AMT,
           A.ACT_IF_LOCL_COMM_AMT,
		   B.CSR_NO	-- 2012.03.05 권상준 추가
      FROM AGT_AGN_COMM A,
           AP_PAY_INV B -- 2012.03.05 권상준 추가
     WHERE A.COMM_APRO_NO = @[comm_apro_no]
       AND A.COMM_APRO_NO = B.INV_NO(+) -- 2012.03.05 권상준 추가
       AND B.DELT_FLG(+) = 'N' -- 2012.03.05 권상준 추가			]]></sql>
			<params>
				<param name="comm_apro_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
