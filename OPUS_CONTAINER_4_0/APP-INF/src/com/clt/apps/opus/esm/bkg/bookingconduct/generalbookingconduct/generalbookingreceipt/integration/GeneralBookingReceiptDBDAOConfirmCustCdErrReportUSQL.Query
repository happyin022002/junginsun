<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOConfirmCustCdErrReportUSQL">
			<desc><![CDATA[0941 Inbound Customer Code Error Report를 Outbound, Inbound 및 Head Quarter에서 재점검 하는 것을 수용]]></desc>
			<sql><![CDATA[
UPDATE BKG_CUSTOMER
   SET OB_EV_CD = DECODE(@[ob_ev_cd], '1', 'C', NULL)
     , IB_EV_CD = DECODE(@[ib_ev_cd], '1', 'C', NULL)
     , HQ_EV_CD = DECODE(@[hq_ev_cd], '1', 'C', NULL)
 WHERE BKG_NO = @[bkg_no]
   AND BKG_CUST_TP_CD = @[bkg_cust_tp_cd]			]]></sql>
			<params>
				<param name="ob_ev_cd" type="12" value="" out="N"/>
				<param name="ib_ev_cd" type="12" value="" out="N"/>
				<param name="hq_ev_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
