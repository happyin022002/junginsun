<sqls>
	<querys>
		<query name="SCRateProposalDBDAOLoadExcelCheckActCustRSQL">
			<desc><![CDATA[Actual Customer check]]></desc>
			<sql><![CDATA[
--SELECT CUST_CNT_CD || LPAD(CUST_SEQ, 6, '0') AS CD
--      ,CUST_LGL_ENG_NM AS NM
--  FROM MDM_CUSTOMER
-- WHERE CUST_CNT_CD = SUBSTR(, 1, 2)
--   AND CUST_SEQ = SUBSTR(, 3)
--   AND DELT_FLG = 'N'
SELECT CUST_CNT_CD || LPAD(CUST_SEQ, 6, '0') AS CD
      ,CUST_LGL_ENG_NM AS NM
  FROM MDM_CUSTOMER
 WHERE CUST_CNT_CD = SUBSTR(@[cd], 1, 2)
   AND CUST_SEQ = SUBSTR(@[cd], 3)
   AND DELT_FLG = 'N'
   AND CNTR_DIV_FLG = 'Y'			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
