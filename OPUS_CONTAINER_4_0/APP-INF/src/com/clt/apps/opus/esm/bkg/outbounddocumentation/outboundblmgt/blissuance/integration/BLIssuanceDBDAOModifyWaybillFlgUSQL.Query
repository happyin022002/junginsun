<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOModifyWaybillFlgUSQL">
			<desc><![CDATA[SeaWaybill 출력시 flag 처리]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_ISS 
SET    UPD_DT             = SYSDATE
     , UPD_USR_ID         = @[upd_usr_id]
     , WBL_PRN_FLG       =  DECODE(@[wbl_prn_flg], 'Y','Y',WBL_PRN_FLG)
 WHERE BKG_NO             = @[bkg_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="wbl_prn_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
