<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchDGClassRSQL">
			<desc><![CDATA[DG Application 의 위험물 Class No 추출]]></desc>
			<sql><![CDATA[
SELECT IMDG_CLSS_CD
  FROM BKG_DG_CGO
 WHERE BKG_NO = @[bkg_no]
   AND DCGO_SEQ = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
