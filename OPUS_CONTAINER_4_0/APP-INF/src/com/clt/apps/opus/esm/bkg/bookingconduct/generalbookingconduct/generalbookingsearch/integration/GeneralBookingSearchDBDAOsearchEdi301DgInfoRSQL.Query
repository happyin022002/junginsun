<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchEdi301DgInfoRSQL">
			<desc><![CDATA[searchEdi301DgInfo]]></desc>
			<sql><![CDATA[
SELECT '{CNTR_DANGER'														      || CHR(10)
    || 'CNTN:'			|| DG.CNTR_NO                                             || CHR(10)
	|| 'UNBR:'			|| DG.IMDG_UN_NO                                          || CHR(10)
    || 'CLAS:'			|| DG.imdg_clss_cd									      || CHR(10)
	|| 'DESC:'			|| DG.PRP_SHP_NM								      	  || CHR(10)
	|| 'PHON:'			|| DG.EMER_CNTC_PHN_NO_CTNT								  || CHR(10)
    || 'EMG_PERSON:'    || DG.EMER_CNTC_PSON_NM 								  || CHR(10)
    || 'PAGE:'			                         								  || CHR(10)
	|| 'FPNT:'			|| DG.FLSH_PNT_CDO_TEMP									  || CHR(10)
	|| 'FPUN:'			|| 'C'												      || CHR(10)
	|| 'DG_REMARK:'		|| REPLACE(NVL(DG.DIFF_RMK, ' '),CHR(10), ' ')   || CHR(10)
	|| 'EMSNO:'			|| NVL(dg.ems_no, ' ')		         					  || CHR(10)
	|| 'PSACLS:'		|| NVL(dg.PSA_NO, ' ')								      || CHR(10)
	|| 'PKGGRP:'		|| NVL(dg.IMDG_PCK_GRP_CD, ' ')							  || CHR(10)
    || 'MFAG1:'			                                       					  || CHR(10)
    || 'MFAG2:'			            							                  || CHR(10)
	|| 'MAR_POLL:'		|| NVL(dg.MRN_POLUT_FLG,' ')							  || CHR(10)
	|| 'LABEL_CD:'		|| NVL((select LBL.IMDG_SUBS_RSK_LBL_CD IMO_SRL
                                  from SCG_IMDG_SUBS_RSK_LBL lbl 
                                 where DG.IMDG_UN_NO      = LBL.IMDG_UN_NO 
                                   and DG.IMDG_UN_NO_SEQ = LBL.IMDG_UN_NO_SEQ
                                   and rownum = 1), ' ') 						  || CHR(10)
	|| 'LABEL_DESC:'														      || CHR(10)
	|| 'D_PKG:'			|| NVL(dg.OUT_IMDG_PCK_QTY1, 0)							  || CHR(10)
	|| 'D_PKGUNIT:'		|| NVL(dg.OUT_IMDG_PCK_CD1, ' ')						  || CHR(10)
	|| 'D_INNER_PKG:'	|| NVL(DG.IN_IMDG_PCK_QTY1, 0)				  			  || CHR(10)
	|| 'D_INNER_PKGUNIT:' || NVL(dg.IN_IMDG_PCK_CD1, ' ')						  || CHR(10)
	|| 'NWGT:'			|| NVL(dg.NET_WGT, 0)								      || CHR(10)
	|| 'NWGT_UNIT:KGS'														      || CHR(10)
	|| 'GWGT:'			|| NVL(dg.GRS_WGT, 0)							          || CHR(10)
	|| 'GWGT_UNIT:'		|| NVL(dg.WGT_UT_CD, ' ')								  || CHR(10)
	|| 'MEA:'			|| NVL(dg.MEAS_QTY, 0)								      || CHR(10)
	|| 'MEA_UNIT:'		|| NVL(dg.MEAS_UT_CD, ' ')								  || CHR(10)
	|| 'HAZ_CONT:'		|| NVL(dg.HZD_DESC, ' ')							      || CHR(10)
	|| 'HAZ_CD_VERNO:'														      || CHR(10)
	|| 'STWG:'			|| NVL(dg.SPCL_STWG_RQST_DESC, ' ')						  || CHR(10)
    || 'LABEL:'			|| NVL((select LBL.IMDG_SUBS_RSK_LBL_CD IMO_SRL
                                  from SCG_IMDG_SUBS_RSK_LBL lbl 
                                 where DG.IMDG_UN_NO      = LBL.IMDG_UN_NO 
                                   and DG.IMDG_UN_NO_SEQ = LBL.IMDG_UN_NO_SEQ
                                   and rownum = 1), ' ') 						  || CHR(10)
	|| 'PROPER_SHIP_NAME:'|| NVL(dg.PRP_SHP_NM, ' ')							  || CHR(10)
	|| 'CONT_TPSZ:'		|| NVL(dg.cntr_tpsz_cd, ' ')							  || CHR(10)
	|| 'CARGO_SEQ:'		|| dg.DCGO_SEQ            								  || CHR(10)
	|| 'TERM_CARD_NO:'														      || CHR(10)
	|| 'MARK_LABEL_NO1:'		|| NVL(DG.IMDG_SUBS_RSK_LBL_CD1, ' ')			  || CHR(10)
	|| 'MARK_LABEL_NO2:'		|| NVL(DG.IMDG_SUBS_RSK_LBL_CD2, ' ')			  || CHR(10)
	|| 'MARK_LABEL_NO3:'		|| NVL(DG.IMDG_SUBS_RSK_LBL_CD3, ' ')			  || CHR(10)
	|| 'TECH_NAME:'				|| NVL(HZD_DESC, ' ')						  	  || CHR(10)
	|| 'MFAG_PAGE_NO:'														      || CHR(10)
	|| 'MFAG_TEXT:'															      || CHR(10)
	|| 'HARZARD_UP_PT:'														      || CHR(10)
	|| 'HARZARD_LOW_PT:'													      || CHR(10)
	|| 'PKG_INS_CD:'														      || CHR(10)
	|| 'LIMITED_QTY_IND:'		|| NVL(DG.IMDG_LMT_QTY_FLG, ' ')				  || CHR(10)
	|| 'REP_QTY_IND:'														      || CHR(10)
	|| 'NET_EXP_WGT_QTY:'		|| NVL(NET_EXPLO_WGT,0)							  || CHR(10)
	|| 'NET_EXP_WGT_UNIT:KGS'												      || CHR(10)
	|| 'CMD_SEQ:'		|| (SELECT CMF.CNTR_MF_SEQ FROM BKG_CNTR_MF_DESC CMF WHERE CMF.BKG_NO = DG.BKG_NO AND CMF.CNTR_NO = DG.CNTR_NO AND ROWNUM = 1) || CHR(10)
	|| '}CNTR_DANGER'				      										  || CHR(10) DG_INFO
  FROM BKG_DG_CGO DG, BKG_BOOKING bk
 WHERE DG.bkg_no		= @[bkg_no]
 AND DG.BKG_NO = BK.BKG_NO
 AND BK.DCGO_FLG = 'Y'
 Order by dg.dcgo_seq			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
