<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOAddDoCSQL">
			<desc><![CDATA[D/O 정보를 저장한다.( Korea_Cargo Release Order Creation )]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_DO (
	JP_DO_ID
,	VN_CGO_DE_CD
,	JP_DO_SND_STS_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	BKG_NO
,	RLSE_SEQ
,	DO_NO
,	DO_NO_SPLIT
,	CUST_PRN_FLG
,	SELF_TRNS_FLG
,	HBL_NO
,	RCVR_CO_NM
,	RCVR_PHN_NO
,	PIC_NM
,	RCVR_EML
,	RCVR_FAX_NO
,	RCVR_BIZ_NO
,	RCVR_CNEE_NM
,	IDA_DO_DMDT_PAY_TP_CD
,	IDA_DO_VTY_DT
,	DO_PRN_RMK
,	CGOR_RMK
) VALUES( 
	@[jp_do_id]
,	@[vn_cgo_de_cd]
,	@[jp_do_snd_sts_cd]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	@[bkg_no]
,	@[rlse_seq]
,	@[do_no]
,	@[do_no_split]
,	NVL(@[cust_prn_flg],'N')
,	NVL(@[self_trns_flg],'N')
,	@[hbl_no]
,	@[rcvr_co_nm]
,	@[rcvr_phn_no]
,	@[pic_nm]
,	@[rcvr_eml]
,	@[rcvr_fax_no]
,	@[rcvr_biz_no]
,	@[rcvr_cnee_nm]
,	@[ida_do_dmdt_pay_tp_cd]
,	TO_DATE(REPLACE(@[ida_do_vty_dt], '-',''),'YYYYMMDD')
,	@[do_prn_rmk]
,	@[cgor_rmk]
)			]]></sql>
			<params>
				<param name="jp_do_id" type="12" value="" out="N"/>
				<param name="vn_cgo_de_cd" type="12" value="" out="N"/>
				<param name="jp_do_snd_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rlse_seq" type="2" value="" out="N"/>
				<param name="do_no" type="12" value="" out="N"/>
				<param name="do_no_split" type="12" value="" out="N"/>
				<param name="cust_prn_flg" type="12" value="" out="N"/>
				<param name="self_trns_flg" type="12" value="" out="N"/>
				<param name="hbl_no" type="12" value="" out="N"/>
				<param name="rcvr_co_nm" type="12" value="" out="N"/>
				<param name="rcvr_phn_no" type="12" value="" out="N"/>
				<param name="pic_nm" type="12" value="" out="N"/>
				<param name="rcvr_eml" type="12" value="" out="N"/>
				<param name="rcvr_fax_no" type="12" value="" out="N"/>
				<param name="rcvr_biz_no" type="12" value="" out="N"/>
				<param name="rcvr_cnee_nm" type="12" value="" out="N"/>
				<param name="ida_do_dmdt_pay_tp_cd" type="12" value="" out="N"/>
				<param name="ida_do_vty_dt" type="12" value="" out="N"/>
				<param name="do_prn_rmk" type="12" value="" out="N"/>
				<param name="cgor_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
