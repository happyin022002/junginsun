<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandCustomsTransmissionDBDAOSearchDgGoodsInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(SUBSTR(IMDG_CLSS_CD, 1, 7), ' ') AS IMO_CLASS_NO ,
       '' AS IMO_PAGE_NO,
       '' AS HAZARD_CD ,
       ROUND(FLSH_PNT_CDO_TEMP, 3) AS FLASH_POINT ,
       '' AS FLASH_POINT_UNIT ,
       NVL(IMDG_PCK_GRP_CD, ' ') AS PACKING_GROUP ,
       NVL(SUBSTR(EMS_NO, 1, 6), ' ') AS EMS_NO ,
       '' AS MFAG ,
       '' AS TREM_CARD_NO

  FROM BKG_DG_CGO

 WHERE 1=1
   AND BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
