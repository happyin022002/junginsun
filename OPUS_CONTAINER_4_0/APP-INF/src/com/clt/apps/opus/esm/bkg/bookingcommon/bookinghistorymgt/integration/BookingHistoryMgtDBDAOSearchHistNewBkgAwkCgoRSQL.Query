<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchHistNewBkgAwkCgoRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchHistNewBkgAwkCgoRSQL]]></desc>
			<sql><![CDATA[
SELECT HIS_CATE_NM
     , PRE_CTNT
     , CRNT_CTNT
  FROM 
    (
        SELECT 'AWKWARD' HIS_CATE_NM
                , NULL PRE_CTNT
                , NOW.AWK_CGO_SEQ||
                  '/'||NOW.CNTR_NO||
                  '/'||TO_CHAR(NOW.OVR_FWRD_LEN,        '99999')||
                  ' CM/'||TO_CHAR(NOW.OVR_BKWD_LEN,     '99999')||
                  ' CM/'||TO_CHAR(NOW.OVR_HGT,          '99999')||
                  ' CM/'||TO_CHAR(NOW.OVR_LF_LEN,       '99999')||
                  ' CM/'||TO_CHAR(NOW.OVR_RT_LEN,       '99999')||
                  ' CM/'||TO_CHAR(NOW.OVR_VOID_SLT_QTY, '9999.0')||'FEU' CRNT_CTNT
#if (${ca_flg} == 'Y')
          FROM BKG_AWK_CGO_HIS NOW
         WHERE NOW.BKG_NO  = @[bkg_no] 
	       AND NOW.CORR_NO = 'TMP0000001' 
#else
          FROM BKG_AWK_CGO NOW
         WHERE NOW.BKG_NO = @[bkg_no] 
#end

#if ($field_list.size() > 0)
           AND (BKG_NO, AWK_CGO_SEQ) NOT IN (
           #foreach($field_id in ${field_list}) 
               #if($velocityCount < $field_list.size()) $field_id,  #else $field_id  #end
           #end
           )
#end
    )
 WHERE CRNT_CTNT IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
