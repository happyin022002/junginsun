<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContinentDBDAOMdmSubcontinentVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	SCONTI_CD
,	SCONTI_NM
FROM MDM_SUBCONTINENT
WHERE	CONTI_CD = @[conti_cd]
AND	DELT_FLG = 'N'
ORDER BY SCONTI_CD			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
