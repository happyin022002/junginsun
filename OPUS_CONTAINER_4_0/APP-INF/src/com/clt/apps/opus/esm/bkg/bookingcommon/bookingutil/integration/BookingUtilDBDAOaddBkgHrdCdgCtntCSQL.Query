<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOaddBkgHrdCdgCtntCSQL">
			<desc><![CDATA[i]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_HRD_CDG_CTNT
(
 HRD_CDG_ID
,HRD_CDG_ID_SEQ
,ATTR_CTNT1  
,ATTR_CTNT2
,ATTR_CTNT3
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
) VALUES (
 'KR_WHF_EXPT_CUST'
,(SELECT
NVL(MAX(HRD_CDG_ID_SEQ),0)+1
FROM BKG_HRD_CDG_CTNT
WHERE 1=1
AND HRD_CDG_ID = 'KR_WHF_EXPT_CUST')
,@[attr_ctnt1]
,@[attr_ctnt2]
,@[attr_ctnt3]
,@[cre_usr_id]
,SYSDATE
,@[upd_usr_id]
,SYSDATE
)
			]]></sql>
			<params>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="attr_ctnt3" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
