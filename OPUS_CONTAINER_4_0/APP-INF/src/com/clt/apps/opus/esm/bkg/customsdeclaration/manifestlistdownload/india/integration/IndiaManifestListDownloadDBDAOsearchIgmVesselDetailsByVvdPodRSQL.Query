<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaManifestListDownloadDBDAOsearchIgmVesselDetailsByVvdPodRSQL">
			<desc><![CDATA[searchIgmVesselDetailsByVvdPod]]></desc>
			<sql><![CDATA[
SELECT										
	@[vvd_cd] VVD_CD
	,VSL_CD										
	,SKD_VOY_NO									
	,SKD_DIR_CD									
	,POD_CD										
	,IDA_DECL_VSL_NO							
	,TO_CHAR(VSL_DECL_DT, 'YYYYMMDD') VSL_DECL_DT								
	,VSL_NM	
	,CALL_SGN_NO								
	,IDA_LINE_NO								
	,IDA_AGN_ID	AS CRR_AGN_CD								
	,CNT_CD 
	,OTR_DCHG_YD_ID PORT_CD						
	,TO_CHAR(ARR_DT, 'YYYYMMDD') ARR_DT			
	,TO_CHAR(ARR_DT2, 'YYYYMMDD') ARR_DT2		
	,IBD_NO	-- TP Bond No									
	,CRR_AGN_ID AS IDA_AGN_ID									
	,IDA_MRN_LINE_OPR_ID AS IDA_MRN_LINE_OPR_CD					
	,IDA_CFS_ID
	,BD_AREA_CD	--Bond Code								
	,IDA_VOY_NO									
	,NVL(IDA_YR_NO, '')  IDA_YR_NO
	,TRNS_OPR_ID								
	,CRE_USR_ID									
	,CRE_DT										
	,UPD_USR_ID									
	,UPD_DT										
FROM BKG_CSTMS_IDA_VSL						
WHERE 1=1										

AND VSL_CD 		= SUBSTR(@[vvd_cd], 1, 4)  
AND	SKD_VOY_NO 	= SUBSTR(@[vvd_cd], 5, 4)  
AND	SKD_DIR_CD 	= SUBSTR(@[vvd_cd], 9, 1)  
		
AND	POD_CD		= @[pod_cd]			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
