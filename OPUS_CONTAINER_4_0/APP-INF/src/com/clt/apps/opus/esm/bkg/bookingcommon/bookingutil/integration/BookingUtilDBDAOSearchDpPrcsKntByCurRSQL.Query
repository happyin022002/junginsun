<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchDpPrcsKntByCurRSQL">
			<desc><![CDATA[currency에 대한 소숫점 자릿수를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CURR_CD, DP_PRCS_KNT 
FROM MDM_CURRENCY
WHERE CURR_CD = @[curr_cd]			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
