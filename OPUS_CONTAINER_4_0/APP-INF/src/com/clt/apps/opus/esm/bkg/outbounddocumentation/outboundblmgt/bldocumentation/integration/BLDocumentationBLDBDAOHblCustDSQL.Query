<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOHblCustDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
DELETE FROM BKG_HBL_CUST_HIS
WHERE  BKG_NO = @[bkg_no]
AND    CORR_NO = 'TMP0000001'
AND    HBL_SEQ = @[hbl_seq]
#else 
DELETE FROM BKG_HBL_CUST
WHERE	BKG_NO = @[bkg_no]
AND	HBL_SEQ = @[hbl_seq]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="hbl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
