<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchEdiSendIdRSQL">
			<desc><![CDATA[EDI SEND ID를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(MCHN_TRD_PRNR_ID, 'DSEN_MAN') MCHN_TRD_PRNR_ID
  FROM BKG_EDI_GRP
 WHERE CUST_TRD_PRNR_ID = @[ofc_cd] --> ofc_cd
   AND ESVC_GRP_DELT_FLG = 'Y'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
