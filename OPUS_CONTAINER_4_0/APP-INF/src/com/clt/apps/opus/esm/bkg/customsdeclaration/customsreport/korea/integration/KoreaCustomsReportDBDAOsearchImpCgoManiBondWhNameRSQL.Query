<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchImpCgoManiBondWhNameRSQL">
			<desc><![CDATA[Import Cargo Manifest Print를 위한 배정신청 현황을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT WH.WH_NM||CHR(10)||'('||WH.CSTMS_CD||')' WH_NM
FROM BKG_CSTMS_KR_MF_SEQ_NO SEQ, BKG_WAREHOUSE WH
WHERE SEQ.CSTMS_CLR_WH_CD = WH.CSTMS_CD
AND SEQ.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
