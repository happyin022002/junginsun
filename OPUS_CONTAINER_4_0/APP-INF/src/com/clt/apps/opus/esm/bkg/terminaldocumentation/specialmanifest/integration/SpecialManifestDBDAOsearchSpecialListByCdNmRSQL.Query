<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchSpecialListByCdNmRSQL">
			<desc><![CDATA[special UN number정보를 조회한다.(ANRBS)]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_UN_NO
,	IMDG_UN_NO AS OLD_IMDG_UN_NO
,	ANR_SPCL_TP_ID
,	SPCL_ID_DESC
FROM BKG_CSTMS_EUR_DG_SPCL
WHERE 1=1

#if (${imdg_un_no} != '') 
AND IMDG_UN_NO = @[imdg_un_no]
#end

#if (${spcl_id_desc} != '') 
AND ANR_SPCL_TP_ID = @[spcl_id_desc]
#end

ORDER BY IMDG_UN_NO			]]></sql>
			<params>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="spcl_id_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
