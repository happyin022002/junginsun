<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesActivityManageDBDAOSearchCustSatisFactionRSQL">
			<desc><![CDATA[Sales Activity Report 화면의 3번째 탭 ( Cust.SatisFaction ) 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD
     , CUST_SEQ
     , SREP_CD
     , SLS_ACT_SEQ
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SES', CUST_SATSFC_GRD_CD, null)) SES
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SCR', CUST_SATSFC_GRD_CD, null)) SCR
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'EQS', CUST_SATSFC_GRD_CD, null)) EQS
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'CAH', CUST_SATSFC_GRD_CD, null)) CAH
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SEP', CUST_SATSFC_GRD_CD, null)) SEP
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'REL', CUST_SATSFC_GRD_CD, null)) REL
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'USF', CUST_SATSFC_GRD_CD, null)) USF
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'BOC', CUST_SATSFC_GRD_CD, null)) BOC
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'DOB', CUST_SATSFC_GRD_CD, null)) DOB
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'ATS', CUST_SATSFC_GRD_CD, null)) ATS
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'CLH', CUST_SATSFC_GRD_CD, null)) CLH
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'QUR', CUST_SATSFC_GRD_CD, null)) QUR
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'CUN', CUST_SATSFC_GRD_CD, null)) CUN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SRC', CUST_SATSFC_GRD_CD, null)) SRC
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SES', CUST_SATSFC_RSN)) SES_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SCR', CUST_SATSFC_RSN)) SCR_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'EQS', CUST_SATSFC_RSN)) EQS_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'CAH', CUST_SATSFC_RSN)) CAH_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SEP', CUST_SATSFC_RSN)) SEP_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'REL', CUST_SATSFC_RSN)) REL_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'USF', CUST_SATSFC_RSN)) USF_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'BOC', CUST_SATSFC_RSN)) BOC_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'DOB', CUST_SATSFC_RSN)) DOB_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'ATS', CUST_SATSFC_RSN)) ATS_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'CLH', CUST_SATSFC_RSN)) CLH_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'QUR', CUST_SATSFC_RSN)) QUR_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'CUN', CUST_SATSFC_RSN)) CUN_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'WSI', CUST_SATSFC_RSN)) WSI_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'CUR', CUST_SATSFC_RSN)) CUR_RSN
     , MAX(DECODE(CUST_SATSFC_ITM_CD, 'SRC', CUST_SATSFC_RSN)) SRC_RSN
FROM SAM_CUST_SATSFC
WHERE 1=1
AND SREP_CD 	= @[srep_cd]
AND CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ	= @[cust_seq]
AND SLS_ACT_SEQ	= @[sls_act_seq]
GROUP BY (CUST_CNT_CD
        , CUST_SEQ
        , SREP_CD
        , SLS_ACT_SEQ)			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="sls_act_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
