<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOchangeXterRqstBkgNoUSQL">
			<desc><![CDATA[EBookingReceiptDBDAOchangeXterRqstBkgNo]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_RQST_MST
SET BKG_NO = @[bkg_no]
WHERE ( XTER_SNDR_ID, XTER_RQST_NO, NVL(SNACCS_SPLIT_NO, ' '), SNACCS_MSG_TP_CD ) IN (
    SELECT XTER_SNDR_ID, XTER_RQST_NO, NVL(SNACCS_SPLIT_NO, ' '), SNACCS_MSG_TP_CD
    FROM BKG_XTER_RQST_MST
    WHERE XTER_SNDR_ID = @[sender_id]
    AND	XTER_RQST_NO = @[rqst_no]
    AND	XTER_RQST_SEQ = @[rqst_seq]
    UNION ALL
    SELECT XTER_SNDR_ID, XTER_RQST_NO, NVL(SNACCS_SPLIT_NO, ' '), DECODE(SNACCS_MSG_TP_CD,'SAT049', 'SAT050', 'SAT053','SAT054','')
    FROM BKG_XTER_RQST_MST
    WHERE XTER_SNDR_ID = @[sender_id]
    AND	XTER_RQST_NO = @[rqst_no]
    AND	XTER_RQST_SEQ = @[rqst_seq]
     )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
