<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOremoveBkgHblByBkgDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
delete from bkg_hbl 
where cntr_mf_no in (select cntr_mf_no  
         from bkg_cntr_mf_desc
         where bkg_no = @[bkg_no]
         and cntr_no = @[cntr_no])			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
