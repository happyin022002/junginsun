<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupCommodityProposalDBDAOPriSpScpGrpCmdtVODSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
DELETE FROM pri_sp_scp_grp_cmdt cmdt
WHERE 
    prop_no      = @[prop_no]
AND amdt_seq     = @[amdt_seq]
AND svc_scp_cd   = @[svc_scp_cd]
AND grp_cmdt_seq = @[grp_cmdt_seq]
AND NOT EXISTS  
(    SELECT 'OK'
      FROM pri_sp_scp_grp_cmdt_dtl
     WHERE prop_no 			= cmdt.prop_no
       AND amdt_seq 		= cmdt.amdt_seq
       AND svc_scp_cd 		= cmdt.svc_scp_cd
       AND grp_cmdt_seq 	= cmdt.grp_cmdt_seq
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="grp_cmdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
