<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaCustomsTransmissionDBDAOmodifyBlAckMsgForAgnUSQL">
			<desc><![CDATA[modifyBlAckMsg]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_CHN_SND_LOG_BL

   SET CHN_CSTMS_AGN_ACK_TP_CD = TRIM(@[chn_cstms_ack_tp_cd]),
       AGN_ACK_CTNT = TRIM(@[ack_ctnt]),
       AGN_ACK_UPD_DT = SYSDATE

 WHERE EDI_REF_ID = TRIM(@[edi_ref_id])
   AND BL_NO = SUBSTR(TRIM(@[bl_no]), 5)			]]></sql>
			<params>
				<param name="chn_cstms_ack_tp_cd" type="12" value="" out="N"/>
				<param name="ack_ctnt" type="12" value="" out="N"/>
				<param name="edi_ref_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
