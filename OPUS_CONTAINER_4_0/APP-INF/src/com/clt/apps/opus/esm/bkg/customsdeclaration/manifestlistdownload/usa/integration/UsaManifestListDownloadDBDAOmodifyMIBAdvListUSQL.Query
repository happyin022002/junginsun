			<desc><![CDATA[dwkim, ADV_BL.USA_LST_LOC_CD 갱신. 0408]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_BL
SET
	CNT_CD = CNT_CD
#if (${origin_bl_flag} == 'origin_bl') 

    #if (${locl_clr_ipi_mvmt_flg} == 'Y' ) 
		#if (${cstms_clr_tp_cd} == 'L') 
    		,USA_LST_LOC_CD = ''
		#else
			,USA_LST_LOC_CD = TRIM(@[usa_lst_loc_cd])
		#end 
	#else
		,USA_LST_LOC_CD = TRIM(@[usa_lst_loc_cd])
    #end
	#if (${cstms_clr_tp_cd} == 'L')
    ,CSTMS_LOC_CD = (SELECT POD_CD FROM BKG_BOOKING WHERE BL_NO = @[bl_no])
    #elseif(${cstms_clr_tp_cd} == 'I')
    ,CSTMS_LOC_CD = @[cstms_loc_cd]
    #end 
    ,HUB_LOC_CD = TRIM(@[hub_loc_cd])

#else
	#if (${cstms_clr_tp_cd_chg} == 'Y')
    	#if (${cstms_clr_tp_cd} == 'L')
    		,CSTMS_LOC_CD = (SELECT POD_CD FROM BKG_BOOKING WHERE BL_NO = @[bl_no])
			#if (${locl_clr_ipi_mvmt_flg} == 'Y')
				,USA_LST_LOC_CD = ''
			#end
    	#elseif (${cstms_clr_tp_cd} == 'I')
    	,CSTMS_LOC_CD = @[cstms_loc_cd]
	    #end 
	#end

#end
WHERE CNT_CD = 'US'
AND BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="usa_lst_loc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cstms_loc_cd" type="12" value="" out="N"/>
				<param name="hub_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
