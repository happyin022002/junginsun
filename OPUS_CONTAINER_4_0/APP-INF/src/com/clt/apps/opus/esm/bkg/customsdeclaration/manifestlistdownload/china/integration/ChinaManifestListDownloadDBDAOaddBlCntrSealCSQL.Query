<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOaddBlCntrSealCSQL">
			<desc><![CDATA[addBlCntrSeal]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_SEAL_NO (
         CNT_CD
        ,CSTMS_DIV_ID
        ,BL_NO
        ,CNTR_NO
        ,SEAL_NO_SEQ
        ,SEAL_NO
        ,SEAL_KND_CD
        ,SEAL_PTY_TP_CD
        ,SEAL_PTY_NM
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT )
VALUES (
         @[cnt_cd]
        ,'CTM'
        ,@[bl_no]
        ,@[cntr_no]
        ,NVL((SELECT /*+ index_desc(A XPKBKG_CSTMS_SEAL_NO) */
          		     SEAL_NO_SEQ
    	      FROM   BKG_CSTMS_SEAL_NO A
        	  WHERE  1=1
    		  AND	 CNT_CD 	  = @[cnt_cd]
              AND    CSTMS_DIV_ID = 'CTM'
              AND    BL_NO 		  = @[bl_no]
    	      AND    CNTR_NO 	  = @[cntr_no]
        	  AND    ROWNUM 	  = 1
         ),0)+1
        ,@[seal_no]
        ,TRIM(@[seal_knd_cd])
        ,TRIM(@[seal_pty_tp_cd])
        ,TRIM(@[seal_pty_nm])
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[upd_usr_id]
        ,SYSDATE )			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="seal_no" type="12" value="" out="N"/>
				<param name="seal_knd_cd" type="12" value="" out="N"/>
				<param name="seal_pty_tp_cd" type="12" value="" out="N"/>
				<param name="seal_pty_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
