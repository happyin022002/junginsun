<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOupdateVgmWgtUSQL">
			<desc><![CDATA[Update VGM Weight]]></desc>
			<sql><![CDATA[
UPDATE BKG_CONTAINER SET 

VGM_WGT = @[vgm_wgt],
VGM_WGT_UT_CD = @[vgm_wgt_ut_cd],
VGM_WGT_UPD_DT = TO_DATE(@[vgm_wgt_upd_dt],'YYYY-MM-DD HH24:MI'),
VGM_WGT_UPD_USR_ID = @[vgm_wgt_upd_usr_id]

WHERE	BKG_NO = @[bkg_no]
AND		CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="vgm_wgt" type="12" value="" out="N"/>
				<param name="vgm_wgt_ut_cd" type="12" value="" out="N"/>
				<param name="vgm_wgt_upd_dt" type="12" value="" out="N"/>
				<param name="vgm_wgt_upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
