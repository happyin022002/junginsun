<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOManageRefNoCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y') 
INSERT INTO BKG_REF_HIS (
#else
INSERT INTO BKG_REFERENCE (
#end
	BKG_NO,
#if (${ca_flg}== 'Y')
    CORR_NO,
#end
	REF_SEQ,
	BKG_REF_TP_CD,
	CUST_REF_NO_CTNT,
	CPY_DESC_FLG,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
) values( 
	@[bkg_no],
#if (${ca_flg}== 'Y')
   'TMP0000001',
#end
	(
#if (${ca_flg}== 'Y') 
		NVL((SELECT /*+ INDEX_DESC(BKG_REF_HIS XPKBKG_REF_HIS)  */
#else
		NVL((SELECT /*+ INDEX_DESC(BKG_REFERENCE XPKBKG_REFERENCE)  */
#end
                 REF_SEQ
#if (${ca_flg}== 'Y') 
			FROM BKG_REF_HIS
#else
			FROM BKG_REFERENCE
#end
    		WHERE BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        	AND   CORR_NO = 'TMP0000001'   
#end  
			AND   ROWNUM = 1
    ),0)+1),
	@[bkg_ref_tp_cd],
	@[cust_ref_no_ctnt],
	NVL(@[cpy_desc_flg],'N'),
	@[cre_usr_id],
	sysdate,
	@[upd_usr_id],
	sysdate
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_ref_tp_cd" type="12" value="" out="N"/>
				<param name="cust_ref_no_ctnt" type="12" value="" out="N"/>
				<param name="cpy_desc_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
