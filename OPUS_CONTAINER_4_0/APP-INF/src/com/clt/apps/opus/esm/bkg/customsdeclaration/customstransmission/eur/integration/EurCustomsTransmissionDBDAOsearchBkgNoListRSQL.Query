<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchBkgNoListRSQL">
			<desc><![CDATA[대상 bkg_no 리스트를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
    A.BKG_NO
FROM BKG_BOOKING A, BKG_VVD B
WHERE A.BKG_NO = B.BKG_NO
AND B.VSL_CD		= SUBSTR(@[vvd_cd], 1, 4)   
AND B.SKD_VOY_NO	= SUBSTR(@[vvd_cd], 5, 4)   
AND B.SKD_DIR_CD	= SUBSTR(@[vvd_cd], 9, 1)   

#if (${pol_cd} != '') 
AND B.POL_CD		= @[pol_cd]
#end

#if (${pod_cd} != '') 
AND B.POD_CD		= @[pod_cd]
#end

#if (${bl_no} != '') 
AND A.BL_NO		= @[bl_no]
#end

AND A.BKG_STS_CD NOT IN ('X', 'S')

#if (${exist_search_flag} == 'Y') 
AND ROWNUM = 1
#end			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
