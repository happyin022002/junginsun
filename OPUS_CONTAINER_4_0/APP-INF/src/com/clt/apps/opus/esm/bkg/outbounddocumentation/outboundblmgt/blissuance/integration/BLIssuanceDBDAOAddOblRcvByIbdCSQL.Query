<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOAddOblRcvByIbdCSQL">
			<desc><![CDATA[Original B/L 회수 여부와 상세 정보를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BL_ISS 
( BKG_NO
, OBL_RDEM_FLG
, OTR_DOC_CGOR_FLG
, BL_OTR_DOC_RCV_CD
, OTR_DOC_RCV_OFC_CD
, OTR_DOC_RCV_USR_ID
, OTR_DOC_RCV_DT
, IBD_DOC_RCV_FLG
, IBD_DOC_RCV_OFC_CD
, IBD_DOC_RCV_USR_ID
, IBD_DOC_RCV_DT
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT 
)
VALUES 
( @[bkg_no]
, @[obl_rdem_flg]
, @[otr_doc_cgor_flg]
, @[bl_otr_doc_rcv_cd]
, @[otr_doc_rcv_ofc_cd]     -- Login Office Code
, @[otr_doc_rcv_usr_id]     -- Login User ID
, GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(), SYSDATE, GLOBALDATE_PKG.GET_LOCCD_FNC(@[otr_doc_rcv_ofc_cd])) 
, @[ibd_doc_rcv_flg]
, @[ibd_doc_rcv_ofc_cd]    -- Login Office Code
, @[ibd_doc_rcv_usr_id]    -- Login User ID
, GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(), SYSDATE, GLOBALDATE_PKG.GET_LOCCD_FNC(@[ibd_doc_rcv_ofc_cd])) 
, @[cre_usr_id]            -- Login User ID
, SYSDATE
, @[upd_usr_id]            -- Login User ID
, SYSDATE 
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="obl_rdem_flg" type="12" value="" out="N"/>
				<param name="otr_doc_cgor_flg" type="12" value="" out="N"/>
				<param name="bl_otr_doc_rcv_cd" type="12" value="" out="N"/>
				<param name="otr_doc_rcv_ofc_cd" type="12" value="" out="N"/>
				<param name="otr_doc_rcv_usr_id" type="12" value="" out="N"/>
				<param name="ibd_doc_rcv_flg" type="12" value="" out="N"/>
				<param name="ibd_doc_rcv_ofc_cd" type="12" value="" out="N"/>
				<param name="ibd_doc_rcv_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
