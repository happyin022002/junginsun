<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCPrefixDBDAOCheckUsedPrefixRSQL">
			<desc><![CDATA[S/C Prefix & Scope Mapping Creation 에서 사용하는 Prefix 인지 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
FROM PRI_SC_PFX_MAPG
WHERE SC_PFX_CD = @[sc_pfx_cd]			]]></sql>
