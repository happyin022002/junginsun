<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchCustomsResultRSQL">
			<desc><![CDATA[UsaBlCustomsResultVO]]></desc>
			<sql><![CDATA[
SELECT 0 SEQ
      ,BL_NO
      ,DSPO_CD
      ,SCAC_CD
      ,IBD_REF_NO
      ,CNTR_QTY
      ,ENTR_TP_NO
      ,ENTR_NO
      ,RCV_LOC_CD
      ,VVD
      ,CSTMS_BAT_NO
      ,CSTMS_RMK
      ,CSTMS_CLR_CD
      ,RCV_MSG_TP_ID
      ,IO_BND_CD
      ,RCV_SEQ
      ,POL_CD
      ,POD_CD
      ,RCV_DATE
      ,RJCT_FLG
	  ,CSTMS_LOC_DIFF_FLG
  FROM (
        SELECT RS.BL_NO
              ,RS.CSTMS_SEQ
              ,RS.DSPO_CD
              ,RS.IBD_REF_NO
              ,RS.CNTR_QTY
              ,RS.ENTR_TP_NO
              ,RS.ENTR_NO
              ,RS.RCV_LOC_CD
              ,RS.VSL_CD||RS.SKD_VOY_NO||RS.SKD_DIR_CD AS VVD
              ,RS.CSTMS_BAT_NO
              ,TRIM(RS.CSTMS_RMK1) || ' ' || TRIM(RS.CSTMS_RMK2) || ' ' || TRIM(RS.CSTMS_RMK3) AS CSTMS_RMK
              ,RS.CSTMS_CLR_CD
              ,(CASE WHEN RS.DSPO_CD != 'RC' THEN NVL(RM.SCAC_CD, COM_ConstantMgr_PKG.COM_getScacCode_FNC())
                     ELSE RM.SCAC_CD
                 END ) SCAC_CD
              ,RM.RCV_MSG_TP_ID
              ,RM.IO_BND_CD
              ,RM.RCV_SEQ
              ,RM.POL_CD
              ,RM.POD_CD
              ,TO_CHAR(RS.ARR_DT, 'YYYYMMDDHH24MISS') AS RCV_DATE
              ,'N' AS RJCT_FLG
			  ,NVL(RS.CSTMS_LOC_DIFF_FLG , 'N') AS CSTMS_LOC_DIFF_FLG
          FROM BKG_CSTMS_ADV_RSLT  RS
              ,BKG_CSTMS_ADV_RCV_LOG RM
         WHERE RS.BL_NO  = @[bl_no]
           AND RS.CNT_CD = @[cnt_cd]
           AND RS.CNT_CD = RM.CNT_CD
           AND RS.BL_NO  = RM.BL_NO
           AND RS.ARR_DT = RM.RCV_DT
           AND RS.CSTMS_BAT_NO = RM.CSTMS_BAT_NO
           AND RM.IO_BND_CD = 'I'
        UNION ALL
        SELECT BL_NO
              ,0 AS CSTMS_SEQ
              ,DSPO_CD
              ,'' AS IBD_REF_NO
              ,NULL AS CNTR_QTY
              ,'' AS ENTR_TP_NO
              ,'' AS ENTR_NO
              ,RCV_LOC_CD
              ,VVD
              ,CSTMS_BAT_NO
              ,CSTMS_RMK
              ,'' AS CSTMS_CLR_CD
              ,SCAC_CD
              ,RCV_MSG_TP_ID
              ,IO_BND_CD
              ,RCV_SEQ
              ,'' AS POL_CD
              ,'' AS POD_CD
              ,RCV_DATE
              ,DECODE(CSTMS_RMK, NULL, 'N', 'Y') AS RJCT_FLG
              ,'N' AS CSTMS_LOC_DIFF_FLG
          FROM (
                SELECT BL_NO
                      ,DSPO_CD
                      ,RCV_LOC_CD
                      ,VVD
                      ,CSTMS_BAT_NO
                      ,MAX(CSTMS_RMK) AS CSTMS_RMK
                      ,SCAC_CD
                      ,RCV_MSG_TP_ID
                      ,IO_BND_CD
                      ,RCV_SEQ
                      ,RCV_DATE
                  FROM (
                SELECT RSPN.BL_NO
                      ,LOG.RCV_MSG_TP_ID AS DSPO_CD
                      ,LOG.POD_CD AS RCV_LOC_CD
                      ,LOG.VSL_CD || LOG.SKD_VOY_NO || LOG.SKD_DIR_CD AS VVD
                      ,LOG.CSTMS_BAT_NO
                      ,BKG_JOIN_FNC((CURSOR(SELECT SUBSTR(DTL.MSG_DESC, 40)
		                                      FROM BKG_CSTMS_ADV_RCV_LOG_DTL DTL
		                                     WHERE LOG.CNT_CD     = DTL.CNT_CD
		                                       AND LOG.IO_BND_CD  = DTL.IO_BND_CD
		                                       AND LOG.RCV_DT     = DTL.RCV_DT
		                                       AND LOG.RCV_SEQ    = DTL.RCV_SEQ
		                                       AND (DTL.MSG_DESC   LIKE 'W01 %'
                                                   OR DTL.MSG_DESC LIKE 'W01' || RSPN.BL_NO || '%'
                                                   OR DTL.MSG_DESC LIKE 'W01' || CNTR.CNTR_NO || '%')
                                             GROUP BY DTL.RCV_MSG_DTL_SEQ, DTL.MSG_DESC
                                             ORDER BY DTL.RCV_MSG_DTL_SEQ
		                                   )
		                              ), CHR(10)) AS CSTMS_RMK
                      ,LOG.SCAC_CD
                      ,LOG.RCV_MSG_TP_ID
                      ,LOG.IO_BND_CD
                      ,LOG.RCV_SEQ
                      ,TO_CHAR(LOG.RCV_DT, 'YYYYMMDDHH24MISS') AS RCV_DATE
                  FROM BKG_CSTMS_ADV_EDI_BL_RSPN RSPN
                      ,BKG_CSTMS_ADV_RCV_LOG     LOG
                      ,BKG_CSTMS_ADV_CNTR        CNTR
                 WHERE 1=1
                   AND RSPN.CNT_CD     = LOG.CNT_CD
                   AND RSPN.CRR_BAT_NO = LOG.CRR_BAT_NO
                   AND RSPN.CNT_CD     = @[cnt_cd]
                   AND RSPN.BL_NO      = @[bl_no]
                   AND RSPN.CNT_CD     = CNTR.CNT_CD
                   AND RSPN.BL_NO      = CNTR.BL_NO
                    )
              GROUP BY BL_NO
                      ,DSPO_CD
                      ,RCV_LOC_CD
                      ,VVD
                      ,CSTMS_BAT_NO
                      ,SCAC_CD
                      ,RCV_MSG_TP_ID
                      ,IO_BND_CD
                      ,RCV_SEQ
                      ,RCV_DATE
               )
     )RSLT
ORDER BY RSLT.RCV_DATE, RSLT.CSTMS_SEQ, RSLT.ENTR_TP_NO, RSLT.DSPO_CD			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
