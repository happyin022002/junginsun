<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOModifyBkgBlDocByCustUSQL">
			<desc><![CDATA[bkg_bl_doc에 ORG_CNT_NM column을 update한다]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y') 
update bkg_bl_doc_his
#else
update bkg_bl_doc
#end
   set org_cnt_nm = @[org_cnt_nm]
 where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="org_cnt_nm" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
