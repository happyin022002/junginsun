<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOsearchCBFBSRSQL">
			<desc><![CDATA[searchCBFBS
2010.11.04 김영철 [] Booking Creation / Update / Cancel시, Vessel Operation 모듈의 Final CBF 생성 여부를 체크해서 경고 메시지]]></desc>
			<sql><![CDATA[
SELECT T.CBF_IND_FLG,
       T.CBF_BKG_STS_CD,
       T.UPD_USR_ID,
       T.UPD_DT
FROM (
      SELECT DECODE(CBF_IND_FLG,'P','Pre','F','Final','null') CBF_IND_FLG, 
             DECODE(CBF_BKG_STS_CD,'','All','F','Firm','null') CBF_BKG_STS_CD,
             UPD_USR_ID, 
             TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',UPD_DT,'GMT'), 'YYYY-MM-DD HH24:MI') UPD_DT
        FROM OPF_CGO_BKG_FCAST
       WHERE VSL_CD                  = @[vsl_cd]
         AND SKD_VOY_NO              = @[skd_voy_no]
         AND SKD_DIR_CD              = @[skd_dir_cd]
         AND YD_CD||POL_CLPT_IND_SEQ = @[yd_cd]
       ORDER BY DECODE(BKG_SHPR_OWNR_FLG,'Y',1,2), UPD_DT DESC
) T
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
