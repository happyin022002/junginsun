<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDBDAOSearchLocCdNameRSQL">
			<desc><![CDATA[Location Name Select]]></desc>
			<sql><![CDATA[
select 
	loc_nm
from mdm_location
where	loc_cd = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="n"/>
			</params>
		</query>
	</querys>
</sqls>
