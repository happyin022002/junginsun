<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPsaBkgRlsOrdCntrInfoRSQL">
			<desc><![CDATA[PSA BKG Release Order와 CNTR에서 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT PBR.SUB_PSA_SER_NO SUB_PSA_SER_NO       
     , PBR.CNTR_QTY CNTR_QTY         
     , PBC.PSA_SER_NO PSA_SER_NO
     , PBC.DCGO_FLG DCGO_FLG
     , PBC.RC_FLG RC_FLG     
     , PBC.RD_CGO_FLG RD_CGO_FLG     
     , DECODE( PBC.OVR_HGT_FLG, 'Y', 'Y', DECODE( PBC.OVR_WDT_FLG, 'Y', 'Y', DECODE( PBC.OVR_LEN_FLG, 'Y', 'Y', 'N' ) ) ) AK_FLG
     , PBC.OVR_HGT_FLG OVR_HGT_FLG     
     , PBC.OVR_WDT_FLG OVR_WDT_FLG     
     , PBC.OVR_LEN_FLG OVR_LEN_FLG     
     , PBC.RC_TEMP RC_TEMP
FROM   BKG_CSTMS_PSA_RLSE_ORD  PBR ,
       BKG_CSTMS_PSA_CNTR      PBC
WHERE  PBC.BKG_NO         =   PBR.BKG_NO
AND    PBC.BKG_SEQ        =   PBR.BKG_SEQ
AND    PBC.PSA_SER_NO     =   PBR.PSA_SER_NO
AND    PBR.BKG_NO         =   @[bkg_no]
AND    PBR.BKG_SEQ        =   @[bkg_seq]
AND    PBR.CNTR_TPSZ_CD   =   @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
