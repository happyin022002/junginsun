<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchHQOfficeInfoRSQL">
			<desc><![CDATA[searchHQOfficeInfo]]></desc>
			<sql><![CDATA[
SELECT 
	DISTINCT AR_HD_QTR_OFC_CD 
FROM MDM_ORGANIZATION 
WHERE OFC_CD = @[ofcCd]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="ofcCd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
