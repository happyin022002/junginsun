<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOsearchBkgCntcPsonEmlRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
	CNTC_PSON_EML
FROM BKG_CNTC_PSON PSON, BKG_XTER_RQST_MST XTER
WHERE PSON.BKG_NO = @[bkg_no]
AND PSON.BKG_NO = XTER.BKG_NO
AND XTER.XTER_RQST_SEQ = 1
AND BKG_CNTC_PSON_TP_CD = 'BK'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
