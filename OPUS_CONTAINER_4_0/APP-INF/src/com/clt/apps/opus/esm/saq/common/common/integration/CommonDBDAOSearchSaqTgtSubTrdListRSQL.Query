<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqTgtSubTrdListRSQL">
			<desc><![CDATA[SAQ_MON_TGT_VVD의 Sub Trade List 조회 ]]></desc>
			<sql><![CDATA[
  SELECT DISTINCT SUB_TRD_CD AS TEXT,  
                  SUB_TRD_CD AS CODE   
    FROM SAQ_MON_TGT_VVD               
   WHERE 1 = 1                         
     AND BSE_YR           = @[bse_year]          
     AND BSE_QTR_CD       = @[bse_qtr_cd]          
     AND TRD_CD           = @[trd_cd]          
     AND DIR_CD           = @[dir_cd]          
ORDER BY SUB_TRD_CD                    			]]></sql>
			<params>
				<param name="bse_year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
