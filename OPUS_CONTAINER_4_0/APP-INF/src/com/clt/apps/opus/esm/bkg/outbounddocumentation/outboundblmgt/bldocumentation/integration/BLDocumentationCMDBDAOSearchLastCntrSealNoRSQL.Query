<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOSearchLastCntrSealNoRSQL">
			<desc><![CDATA[BLDocumentationCMDBDAOSearchLastCntrSealNo]]></desc>
			<sql><![CDATA[
SELECT 
/*+INDEX_DESC(CTM_MOVEMENT XUK1CTM_MOVEMENT */
    A.CNTR_SEAL_NO
 FROM CTM_MOVEMENT A
WHERE 1=1
AND CNTR_NO = @[cntr_no]
AND CNMV_CYC_NO = @[cnmv_cyc_no]
AND MVMT_STS_CD IN ('OP','OC')
AND CNTR_SEAL_NO <> @[cntr_seal_no]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_cyc_no" type="12" value="" out="N"/>
				<param name="cntr_seal_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
