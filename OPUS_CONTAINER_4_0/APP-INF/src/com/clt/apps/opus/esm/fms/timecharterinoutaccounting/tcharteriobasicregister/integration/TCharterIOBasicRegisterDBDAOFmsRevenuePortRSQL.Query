<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDBDAOFmsRevenuePortRSQL">
			<desc><![CDATA[Revenue Port Management Select]]></desc>
			<sql><![CDATA[
SELECT A.RLANE_CD
     , A.SKD_DIR_CD
     , A.REV_DIR_CD
     , A.SLAN_CD
     , A.FLET_IOC_CD
     , A.ST_PORT_CD
     , A.FNL_PORT_CD
     , A.CRE_USR_ID
     , A.CRE_DT
     , A.UPD_USR_ID
     , A.UPD_DT
     , (SELECT CASE
                 WHEN COUNT(RLANE_DIR_CD) > 0 THEN 'Y'
                 ELSE ''
               END AS USED_YN
          FROM AR_FINC_DIR_CONV
         WHERE 1=1
           AND DELT_FLG = 'N'
           AND RLANE_CD = A.RLANE_CD) AS USED_YN
  FROM FMS_BSE_PORT A
     , MDM_VSL_SVC_LANE_DIR M
 WHERE 1=1
   AND A.SLAN_CD = M.VSL_SLAN_CD
   AND A.SKD_DIR_CD = VSL_SLAN_DIR_CD
#if (${slan_cd} != '')                                                                               
   AND A.SLAN_CD = @[slan_cd]                                                                   
#end
#if (${rlane_cd} != '')                                                                               
   AND A.RLANE_CD = @[rlane_cd]
#end
 ORDER BY A.SLAN_CD
     , M.VSL_SLAN_DIR_SEQ
     , A.SKD_DIR_CD
     , A.REV_DIR_CD
     , A.RLANE_CD			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
