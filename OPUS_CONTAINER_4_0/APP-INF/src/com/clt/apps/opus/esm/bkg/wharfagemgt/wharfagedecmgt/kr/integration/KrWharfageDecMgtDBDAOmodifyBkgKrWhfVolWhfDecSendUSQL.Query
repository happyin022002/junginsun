<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOmodifyBkgKrWhfVolWhfDecSendUSQL">
			<desc><![CDATA[U]]></desc>
			<sql><![CDATA[
UPDATE BKG_KR_WHF_VOL
SET EDI_MSG_SND_ID = @[edi_msg_snd_id]
WHERE VSL_CD       = @[vsl_cd]
AND SKD_VOY_NO     = @[skd_voy_no]
AND SKD_DIR_CD     = @[skd_dir_cd]
AND PORT_CD        = @[port_cd]
AND SUBSTR(WHF_BND_CD,1,1) = SUBSTR(@[whf_bnd_cd],1,1)			]]></sql>
			<params>
				<param name="edi_msg_snd_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="whf_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
