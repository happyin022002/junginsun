<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDAOSearchOtherCodeListRSQL">
			<desc><![CDATA[OtherCodeList 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
PPT_NM,
PPT_SEQ,
PPT_CTNT,
PPT_NM org_ppt_nm,
PPT_SEQ org_ppt_seq
FROM FMS_PPT_SET

#if(${code_tp} != 'ALL')
WHERE PPT_NM = @[code_tp]
#end

ORDER BY PPT_NM , PPT_SEQ, PPT_CTNT			]]></sql>
			<params>
				<param name="code_tp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
