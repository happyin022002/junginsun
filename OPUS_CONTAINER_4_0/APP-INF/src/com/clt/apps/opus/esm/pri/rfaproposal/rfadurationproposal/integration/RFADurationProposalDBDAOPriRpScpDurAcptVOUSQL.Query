<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFADurationProposalDBDAOPriRpScpDurAcptVOUSQL">
			<desc><![CDATA[Main duration Accept시 Scope Duration도 같이 Accept한다.]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_DUR SET
    ACPT_USR_ID     = @[acpt_usr_id],
    ACPT_OFC_CD     = @[acpt_ofc_cd],
    ACPT_DT         = SYSDATE,
	PRC_PROG_STS_CD = 'A',
    UPD_USR_ID      = @[upd_usr_id],
    UPD_DT          = SYSDATE
WHERE
    PROP_NO         = @[prop_no]
AND AMDT_SEQ        = @[amdt_seq]
AND PRC_PROG_STS_CD = 'I'
AND TO_CHAR(CTRT_EXP_DT, 'yyyy-MM-dd') = (
			SELECT TO_CHAR(CTRT_EXP_DT, 'yyyy-MM-dd')
	   	  	FROM PRI_RP_DUR
			WHERE PROP_NO = @[prop_no]
			AND AMDT_SEQ = @[amdt_seq]
			)			]]></sql>
			<params>
				<param name="acpt_usr_id" type="12" value="" out="N"/>
				<param name="acpt_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
