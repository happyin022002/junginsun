<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselDBDAOMdmVslCntrVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
     , VSL_ENG_NM
FROM MDM_VSL_CNTR
WHERE DELT_FLG = 'N'
#if (${vsl_cd} != '')
AND   VSL_CD LIKE '%'||@[vsl_cd]||'%'
#end
#if (${vsl_eng_nm} != '')
AND   UPPER(VSL_ENG_NM) LIKE '%'||UPPER(@[vsl_eng_nm])||'%'
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vsl_eng_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
