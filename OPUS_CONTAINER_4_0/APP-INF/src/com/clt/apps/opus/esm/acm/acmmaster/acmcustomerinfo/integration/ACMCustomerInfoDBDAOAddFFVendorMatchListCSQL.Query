<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCustomerInfoDBDAOAddFFVendorMatchListCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_FF_VNDR_MTCH
       (FF_CNT_CD,
        FF_SEQ,
        VNDR_CNT_CD,
        VNDR_SEQ,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (SUBSTR(@[ff_cnt_seq], 1, 2),
        TO_NUMBER(SUBSTR(@[ff_cnt_seq], 3)),
        @[vndr_cnt_cd],
        @[vndr_seq],
        @[usr_id],
        SYSDATE,
        @[usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="ff_cnt_seq" type="12" value="" out="N"/>
				<param name="vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
