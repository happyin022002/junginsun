<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchNoneBLVVDInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT mani.MRN_NO||mani.MRN_CHK_NO                 MRN_NO
, mani.VSL_CD || mani.skd_voy_no || mani.skd_dir_cd VVD
, vsl.CLPT_IND_SEQ                                  VVD_SEQ
, v.VSL_ENG_NM                                      VSL_NM
, v.VSL_RGST_CNT_CD                                 VSL_CNT_CD       
, COM_ConstantMgr_PKG.COM_getScacCode_FNC()                                            SHP_CO_CD
, v.CALL_SGN_NO                                     VSL_CALL_SGN_CD
, DECODE(mani.PORT_CD,'KRPUS','030197004','KRKAN','062112001','KRINC','020104002','KRPTK','016105001','KRUSN','110109004','KRGIN','020112001','030197004') CSTMS_DCHG_CD
, DECODE(mani.IO_BND_CD, 'O', mani.PORT_CD) POL_CD /** POL **/ 
, DECODE(mani.IO_BND_CD, 'I', mani.PORT_CD) POD_CD /** POD **/ 
, NVL(TO_CHAR(vsl.vps_eta_dt,'YYYY-MM-DD'),' ') ETA_DT
, NVL(TO_CHAR(vsl.vps_etd_dt,'YYYY-MM-DD HH24MI'),' ') ETD_DT
, DECODE(mani.PORT_CD,'KRPUS',20,'KRINC',30,'KRKAN',622,'KRPTK',31,0) KT_PA /** kt_pa **/ 
-- Manifest Information
, '' F_DATE
, '' T_DATE
, '' MF_SND_RCVR_ID
, '' BD_AREA_CD
, '' CALL_KNT
, '' IO_TML_LOC_CD
-- Detail(s) Information
, DECODE(@[port_cd],'KRINC', '020', 'KRPUS', '030', 'KRKAN', '062','KRPTK','016') LOCL_CSTMS_CD /** Local Customs **/ 
, DECODE(@[port_cd],'KRINC', '10', 'KRPUS', DECODE(@[io_bnd_cd],'I', '27','10'), 'KRKAN', '10','KRPTK','10') LOCL_CSTMS_PRT_CD /** Local Customs **/ 
, '' MST_BL_KNT
, '' TTL_WGT
, '' TTL_WGT_UT_CD
, '' MTY_BL_KNT
, '' TTL_PCK_QTY
, '' TTL_PCK_UT_CD
, '' CNSL_BL_KNT
, '' TTL_LC_TEU_QTY
, '' TTL_LC_FEU_QTY
, '' TTL_LC_45FT_QTY
, '' WHF_NOTICE
, '' TTL_MEAS_QTY
, '' TTL_MEAS_UT_CD
, '' SMP_BL_KNT
, '' TTL_TS_TEU_QTY
, '' TTL_TS_FEU_QTY
, '' TTL_TS_45FT_QTY
, '' JO_CRR_KNT
, '' TTL_FULL_KNT
, '' TTL_MTY_TEU_QTY
, '' TTL_MTY_FEU_QTY
, '' TTL_MTY_45FT_QTY
, '' TTL_MTY_KNT
, '' TTL_TS_MTY_TEU_QTY
, '' TTL_TS_MTY_FEU_QTY
, '' TTL_TS_MTY_45FT_QTY
FROM BKG_CSTMS_KR_MF_REF_NO mani
, VSK_VSL_PORT_SKD vsl
, MDM_VSL_CNTR v
WHERE mani.VSL_CD = v.VSL_CD
AND mani.VSL_CD = vsl.VSL_CD
AND mani.skd_voy_no = vsl.skd_voy_no
AND mani.skd_dir_cd = vsl.skd_dir_cd
AND vsl.CLPT_IND_SEQ = '1'
AND mani.PORT_CD = vsl.VPS_PORT_CD
AND mani.PORT_CD like @[port_cd]
AND mani.IO_BND_CD = @[io_bnd_cd]
AND mani.VSL_CD     LIKE SUBSTR(@[vvd],1,4)||'%'
AND mani.SKD_VOY_NO LIKE SUBSTR(@[vvd],5,4)||'%'
AND mani.SKD_DIR_CD LIKE SUBSTR(@[vvd],9,1)||'%'			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
