<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDDAOBkgPtyXterUsrInfoRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	XUI.USR_ID,
	XUI.USR_N1ST_NM,
	XUI.USR_LST_NM,
	XUI.USR_STS_CD,
	XUI.CUST_CNT_CD,
	XUI.CUST_SEQ,
	XUI.RDY_FO_PRF_FLG,
	XUI.RDY_FO_PRN_FLG,
	XUI.SEA_WBL_EML_FLG
FROM BKG_XTER_USR_INFO XUI
     ,BKG_XTER_RQST_MST MST
WHERE XUI.USR_ID = MST.CUST_ID
AND MST.XTER_RQST_NO = @[rqst_no]
AND MST.XTER_RQST_SEQ = @[rqst_seq]
AND MST.XTER_SNDR_ID = @[sender_id]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
				<param name="sender_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
