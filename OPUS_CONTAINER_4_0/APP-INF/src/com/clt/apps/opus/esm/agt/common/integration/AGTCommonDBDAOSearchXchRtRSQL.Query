<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchXchRtRSQL">
			<desc><![CDATA[searchXchRt]]></desc>
			<sql><![CDATA[
SELECT 
	DECODE(A.CURR_CD,'USD',1,B.USD_LOCL_XCH_RT) AS XCHRT
FROM AGT_FINC_OFC_INFO A,
    GL_MON_XCH_RT B
WHERE A.AGN_CD = @[ofcCd]
AND NVL(A.DELT_FLG,'N') = 'N'
AND A.CURR_CD = B.CURR_CD
AND B.ACCT_XCH_RT_YRMON = SUBSTR(@[aplyDt],1,6)
AND B.ACCT_XCH_RT_LVL = '1'
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="ofcCd" type="12" value="" out="Y"/>
				<param name="aplyDt" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
