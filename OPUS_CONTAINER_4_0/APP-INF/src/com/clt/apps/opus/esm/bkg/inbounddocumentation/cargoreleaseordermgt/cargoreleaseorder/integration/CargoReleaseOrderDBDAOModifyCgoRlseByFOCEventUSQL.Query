<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOModifyCgoRlseByFOCEventUSQL">
			<desc><![CDATA[F.O.C Event 발생 시  U.S. Inbound CARGO RELEASE 정보 저장 Table (CT, CR 정보저장)]]></desc>
			<sql><![CDATA[
UPDATE BKG_CGO_RLSE 
   SET UPD_USR_ID      = @[upd_usr_id]
     , UPD_DT          = SYSDATE
#if (${foc_tp} == 'F') 
     , FRT_CLT_FLG     = @[frt_clt_flg]
     , FRT_CLT_LST_DT  = @[frt_clt_lst_dt]
     , OTS_IF_LST_SEQ  = @[ots_if_lst_seq]
#else if(${foc_tp} == 'O') 
     , OBL_RDEM_FLG    = @[obl_rdem_flg]
     , OBL_RDEM_LST_DT = @[obl_rdem_lst_dt]
#else if(${foc_tp} == 'C') 
     , CSTMS_CLR_CD    = @[cstms_clr_cd]
     , CSTMS_CLR_LST_D = @[cstms_clr_lst_dt]
#end
 WHERE BL_NO           = @[bl_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="frt_clt_flg" type="12" value="" out="N"/>
				<param name="frt_clt_lst_dt" type="12" value="" out="N"/>
				<param name="ots_if_lst_seq" type="12" value="" out="N"/>
				<param name="obl_rdem_flg" type="12" value="" out="N"/>
				<param name="obl_rdem_lst_dt" type="12" value="" out="N"/>
				<param name="cstms_clr_cd" type="12" value="" out="N"/>
				<param name="cstms_clr_lst_dt" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
