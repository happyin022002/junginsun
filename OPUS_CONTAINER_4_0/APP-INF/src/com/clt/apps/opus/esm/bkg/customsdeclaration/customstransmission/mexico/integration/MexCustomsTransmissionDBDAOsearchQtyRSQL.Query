<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MexCustomsTransmissionDBDAOsearchQtyRSQL">
			<desc><![CDATA[DWKIM, 0370, MxQtyVO]]></desc>
			<sql><![CDATA[
SELECT  NVL(CNTR_TPSZ_CD, ' ') HANTYPE
        ,NVL(OP_CNTR_QTY,0) COUNT
  FROM  BKG_QUANTITY
 WHERE  BKG_NO          =   @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
