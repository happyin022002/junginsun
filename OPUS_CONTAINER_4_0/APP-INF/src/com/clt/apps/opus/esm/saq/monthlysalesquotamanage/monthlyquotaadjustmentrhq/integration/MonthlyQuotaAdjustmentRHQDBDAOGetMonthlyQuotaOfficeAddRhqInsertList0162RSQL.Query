<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentRHQDBDAOGetMonthlyQuotaOfficeAddRhqInsertList0162RSQL">
			<desc><![CDATA[04 Step 사용 DB에 입력한 List를 문자열로 리턴.]]></desc>
			<sql><![CDATA[
SELECT SUB_TRD_CD||' / '||RLANE_CD||' / '||CTRT_RHQ_CD||' / '||
          DECODE(CTRT_AQ_CD, '', '', CTRT_AQ_CD||' / ')||CTRT_RGN_OFC_CD||' / '||
          TO_CHAR(TO_DATE(BSE_MON, 'MM'), 'Month', 'NLS_DATE_LANGUAGE=AMERICAN') AS MSG
     FROM (
            SELECT DISTINCT SUB_TRD_CD, RLANE_CD, CTRT_RHQ_CD, CTRT_AQ_CD, CTRT_RGN_OFC_CD, BSE_MON
              FROM SAQ_MON_QTA_RHQ
             WHERE OFC_ADD_FLG     = 'Y'
               AND MQTA_STEP_CD    = @[mqta_step_cd]
               AND BSE_YR          = @[bse_yr]
               AND BSE_QTR_CD      = @[bse_qtr_cd]
               AND TRD_CD          = @[trd_cd]
               AND DIR_CD          = @[dir_cd]
               AND MQTA_VER_NO     = @[mqta_ver_no]
               AND CTRT_RHQ_CD     = @[rhq_cd]
               AND SUB_TRD_CD      = @[sub_trd_cd]
               AND RLANE_CD        = @[rlane_cd]

#if (${aq_cd} != '') 
			   AND CTRT_AQ_CD      = @[aq_cd]
#else 
			   AND CTRT_AQ_CD      IS NULL
#end

               AND CTRT_RGN_OFC_CD = @[rgn_ofc_cd] )
 ORDER BY SUB_TRD_CD, DECODE(RLANE_CD, 'RBCCO', 'ZZ', SUBSTR(RLANE_CD, -2)), RLANE_CD, CTRT_RHQ_CD,
          DECODE(CTRT_AQ_CD, '', '', (DECODE(NVL(CTRT_AQ_CD, 99), '99', 99, 11)||CTRT_AQ_CD)), CTRT_RGN_OFC_CD, BSE_MON			]]></sql>
			<params>
				<param name="mqta_step_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="mqta_ver_no" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="aq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
