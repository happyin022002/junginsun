<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOCheckJapanStampRSQL">
			<desc><![CDATA[일본 지역의 issue booking check 쿼리]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','WEB') AS WEB_FLG
FROM
(
	/* form_rqst_via_cd 값이 WEB으로 넘어오면 찍혀요 */
    SELECT  'Y' 
    FROM BKG_BL_ISS ISS
    WHERE ISS.BKG_NO = @[bkg_no]
      AND ISS.OBL_ISS_FLG ='Y'
      AND EXISTS (SELECT 'Y' FROM MDM_ORGANIZATION 
                  WHERE OFC_CD = ISS.OBL_ISS_OFC_CD
                    AND LOC_CD LIKE 'JP%'
                    AND ROWNUM =1 )
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
