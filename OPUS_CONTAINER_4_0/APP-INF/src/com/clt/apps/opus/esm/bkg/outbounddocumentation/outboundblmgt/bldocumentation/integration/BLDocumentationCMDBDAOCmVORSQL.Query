<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCmVORSQL">
			<desc><![CDATA[   select]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
SELECT A1.BKG_NO
,	A1.CNTR_MF_SEQ
,	A1.CNTR_NO
,	A1.PCK_QTY
,	A1.PCK_TP_CD
,	A1.CNTR_MF_WGT
,	A1.WGT_UT_CD
,	A1.MEAS_QTY
,	A1.MEAS_UT_CD
,	A1.DCGO_FLG
,	A1.BB_CGO_FLG
,	A1.AWK_CGO_FLG
,	A1.RC_FLG
,	A1.RD_CGO_FLG
,	A1.HNGR_FLG
,	A1.CNTR_MF_MK_DESC
,	A1.CNTR_MF_GDS_DESC
,	A1.HBL_SEQ
,	A1.HAMO_TRF_CD
,	A1.CMDT_HS_CD
,	A1.NCM_NO
,	A1.PO_NO
,	A1.CNTR_MF_NO
,	A1.CSTMS_DECL_NO
,	A1.CRE_USR_ID
,	A1.UPD_USR_ID
,	A1.DCGO_SEQ
FROM  BKG_CNTR_MF_DESC_HIS A1
     ,BKG_CNTR_HIS A2
WHERE A1.BKG_NO = @[bkg_no]
AND   A1.CORR_NO = 'TMP0000001'
AND   A1.BKG_NO = A2.BKG_NO
AND   A1.CORR_NO = A2.CORR_NO
AND   A1.CNTR_NO = A2.CNTR_NO
ORDER BY A2.CNTR_DP_SEQ
     ,A1.CNTR_MF_SEQ
#else 
SELECT A1.BKG_NO
,	A1.CNTR_MF_SEQ
,	A1.CNTR_NO
,	A1.PCK_QTY
,	A1.PCK_TP_CD
,	A1.CNTR_MF_WGT
,	A1.WGT_UT_CD
,	A1.MEAS_QTY
,	A1.MEAS_UT_CD
,	A1.DCGO_FLG
,	A1.BB_CGO_FLG
,	A1.AWK_CGO_FLG
,	A1.RC_FLG
,	A1.RD_CGO_FLG
,	A1.HNGR_FLG
,	A1.CNTR_MF_MK_DESC
,	A1.CNTR_MF_GDS_DESC
,	A1.HBL_SEQ
,	A1.HAMO_TRF_CD
,	A1.CMDT_HS_CD
,	A1.NCM_NO
,	A1.PO_NO
,	A1.CNTR_MF_NO
,	A1.CSTMS_DECL_NO
,	A1.CRE_USR_ID
,	A1.UPD_USR_ID
,	A1.DCGO_SEQ
FROM BKG_CNTR_MF_DESC A1
    ,BKG_CONTAINER A2
WHERE	A1.BKG_NO = @[bkg_no]
AND     A1.BKG_NO = A2.BKG_NO
AND     A1.CNTR_NO = A2.CNTR_NO
ORDER BY A2.CNTR_DP_SEQ
     ,A1.CNTR_MF_SEQ
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
