<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateGuidelineDBDAOPriSgBlplExcelRSQL">
			<desc><![CDATA[Boiler Plate Excel Download]]></desc>
			<sql><![CDATA[
SELECT HD.BLPL_REF_YR
     /*, HD.BLPL_HDR_SEQ
     , HD.BLPL_NM
     , HD.EFF_DT
     , HD.EXP_DT
     , BP.BLPL_SEQ*/
     , BP.BLPL_TIT_NM
     /*, BP.DP_SEQ
     , CT.BLPL_CTNT_SEQ
     , CT.DP_SEQ AS CTNT_DP_SEQ*/
     , CT.BLPL_CTNT
FROM PRI_SG_BLPL_HDR HD
   , PRI_SG_BLPL BP
   , PRI_SG_BLPL_CTNT CT
WHERE HD.BLPL_REF_YR = @[blpl_ref_yr]
AND   BP.BLPL_HDR_SEQ = HD.BLPL_HDR_SEQ
AND   CT.BLPL_HDR_SEQ = BP.BLPL_HDR_SEQ
AND   CT.BLPL_SEQ = BP.BLPL_SEQ
ORDER BY BP.DP_SEQ, CT.DP_SEQ			]]></sql>
			<params>
				<param name="blpl_ref_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
