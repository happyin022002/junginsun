<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOCreateVvdCACSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOCreateVvdCACSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_VVD (
    BKG_NO 
#else
INSERT INTO BKG_VVD_HIS (
    BKG_NO 
    , CORR_NO 
#end
    , VSL_PRE_PST_CD 
    , VSL_SEQ 
    , SLAN_CD 
    , VSL_CD 
    , SKD_VOY_NO 
    , SKD_DIR_CD 
    , POL_CLPT_IND_SEQ 
    , OP_CD 
    , POL_CD 
    , POL_YD_CD 
    , POD_CD 
    , POD_YD_CD 
    , BKG_TRSP_MZD_CD 
    , CNTR_LODG_FLG 
    , REV_VVD_FLG 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
    , POD_CLPT_IND_SEQ 
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
        , VSL_PRE_PST_CD 
        , VSL_SEQ 
        , SLAN_CD 
        , VSL_CD 
        , SKD_VOY_NO 
        , SKD_DIR_CD 
        , POL_CLPT_IND_SEQ 
        , OP_CD 
        , POL_CD 
        , POL_YD_CD 
        , POD_CD 
        , POD_YD_CD 
        , BKG_TRSP_MZD_CD 
        , CNTR_LODG_FLG 
        , REV_VVD_FLG 
        , CRE_USR_ID 
        , CRE_DT 
        , UPD_USR_ID 
        , sysdate
        , POD_CLPT_IND_SEQ 
#if (${copy_type_cd} == 'BKG')
  FROM BKG_VVD_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_VVD
 WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
