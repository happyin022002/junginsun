<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MTCostDBDAOEqRepoCostVODSQL">
			<desc><![CDATA[@20140923.SJH : COA_MTY_REPO_UT_COST를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE 
  FROM COA_MTY_REPO_UT_COST
 WHERE COST_LOC_GRP_CD    	= @[cost_loc_grp_cd]   
   AND CNTR_TPSZ_CD       	= @[cntr_tpsz_cd]
   AND TRD_CD 			 	= @[trd_cd]		
   AND SCC_CD             	= @[scc_cd]
   AND EFF_FM_YRMON       	= @[eff_fm_yrmon]			]]></sql>
			<params>
				<param name="cost_loc_grp_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="eff_fm_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
