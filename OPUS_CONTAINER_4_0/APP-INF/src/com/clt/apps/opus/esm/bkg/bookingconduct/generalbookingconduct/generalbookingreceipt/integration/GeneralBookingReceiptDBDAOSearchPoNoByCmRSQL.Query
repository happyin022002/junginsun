<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchPoNoByCmRSQL">
			<desc><![CDATA[CM 별 Purchase Other Number와 그외 number 정보를 조회한다.]]></desc>
			<sql><![CDATA[
#if (${popuptpcd} == 'S')
SELECT 
	 ''          BKG_NO
	, ''          REF_SEQ
	, CNTR_NO     CNTR_NO
	, PO_NO       PO_NO
	, ''          ITM_NO
	, ''          ITM_DESC
	, PCK_QTY       PCK_QTY
	, PCK_TP_CD     PCK_TP_CD
	, CNTR_MF_WGT   CNTR_WGT
	, ''		WGT_UT_CD
	, MEAS_QTY      MEAS_QTY
	, ''		MEAS_UT_CD
FROM BKG_XTER_CNTR_MK_DESC
 where xter_sndr_id = @[xter_sndr_id]
   and xter_rqst_no = @[xter_rqst_no]
   and xter_rqst_seq= @[xter_rqst_seq]

#else


	#if (${ca_flg} == 'Y')
SELECT 
 BKG_NO,
 REF_SEQ,
 CNTR_NO,
 PO_NO,
 ITM_NO,
 ITM_DESC,
 PCK_QTY,
 PCK_TP_CD,
 CNTR_WGT,
 WGT_UT_CD,
 MEAS_QTY,
 MEAS_UT_CD
FROM BKG_REF_DTL_HIS
WHERE 
BKG_NO = @[bkg_no] 
AND CORR_NO = 'TMP0000001'
AND DE_NO IS NULL
AND PRT_NO IS NULL
	#else
SELECT 
 BKG_NO,
 REF_SEQ,
 CNTR_NO,
 PO_NO,
 ITM_NO,
 ITM_DESC,
 PCK_QTY,
 PCK_TP_CD,
 CNTR_WGT,
 WGT_UT_CD,
 MEAS_QTY,
 MEAS_UT_CD
FROM BKG_REF_DTL
WHERE 
BKG_NO = @[bkg_no] 
AND DE_NO IS NULL
AND PRT_NO IS NULL
	#end

#end			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
