<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOaddBkgHisMstCSQL">
			<desc><![CDATA[Booking History Master 를 기록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_HIS_MST (
	BKG_NO
,	HIS_SEQ
,	BKG_HIS_ISS_UI_ID
,	CRE_USR_ID
,	UPD_USR_ID
,   CORR_NO
,	CRE_DT
,	UPD_DT
, 	EVNT_DT
) VALUES( 
	@[bkg_no]
,	@[his_seq]
,	@[bkg_his_iss_ui_id]
,	NVL(@[cre_usr_id], 'SYSTEM')
,	NVL(@[upd_usr_id], 'SYSTEM')
,   @[corr_no] 
--Batch call AutoratingTaxCalculate Hard Coding 'TAXBATCH'
#if(${cre_usr_id} == 'TAXBATCH' || ${cre_usr_id} == 'EDI AUTO')
, 	GLOBALDATE_PKG.TIME_LOCAL_FNC('KRPUS')
, 	GLOBALDATE_PKG.TIME_LOCAL_FNC('KRPUS')
, 	GLOBALDATE_PKG.TIME_LOCAL_FNC('KRPUS')
#else
,	SYSDATE
,	SYSDATE
, 	GLOBALDATE_PKG.TIME_CONV_FNC(COM_CONSTANTMGR_PKG.COM_GETBASELOCATIONCODE_FNC(),sysdate,BKG_COM_USER_LOC_FNC(@[cre_usr_id]))
#end
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="his_seq" type="2" value="" out="N"/>
				<param name="bkg_his_iss_ui_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="corr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
