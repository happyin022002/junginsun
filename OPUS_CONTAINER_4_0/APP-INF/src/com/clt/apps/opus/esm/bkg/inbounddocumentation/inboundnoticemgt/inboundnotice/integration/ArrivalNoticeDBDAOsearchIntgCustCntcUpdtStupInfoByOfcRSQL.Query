<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOsearchIntgCustCntcUpdtStupInfoByOfcRSQL">
			<desc><![CDATA[Integrated Customer Data Update Setup]]></desc>
			<sql><![CDATA[
SELECT OFC_CD,
       AUTO_UPD_FLG,
       UPD_USR_ID,
       TO_CHAR(UPD_LOCL_DT,'YYYY-MM-DD HH24:MI') AS UPD_LOCL_DT
  FROM BKG_IB_CUST_CNTC_STUP
 WHERE OFC_CD       = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="NYCNA" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
