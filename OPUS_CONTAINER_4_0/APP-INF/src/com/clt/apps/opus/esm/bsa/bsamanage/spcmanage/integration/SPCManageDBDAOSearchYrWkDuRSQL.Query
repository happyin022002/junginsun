<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SPCManageDBDAOSearchYrWkDuRSQL">
			<desc><![CDATA[검색조건의 주차 기간을 조회]]></desc>
			<sql><![CDATA[
SELECT 
      MIN(COST_YR) COST_YR, 
      MIN(COST_WK) COST_WK, 
      COUNT(*) CNT 
 FROM 
      COA_WK_PRD 
 WHERE 
      COST_YR = @[cost_yr]
  AND COST_WK BETWEEN @[fm_wk] AND @[to_wk]			]]></sql>
			<params>
				<param name="cost_yr" type="12" value="" out="N"/>
				<param name="fm_wk" type="12" value="" out="N"/>
				<param name="to_wk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
