<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchCommonCodeListRSQL">
			<desc><![CDATA[Common Code 조회]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_CTNT    CODE,
         INTG_CD_VAL_DP_DESC TEXT
    FROM COM_INTG_CD_DTL
   WHERE INTG_CD_ID = @[code_cd]
ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="code_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
