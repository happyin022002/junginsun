<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharteIOContractDAOSearchContractListByPrepaymentHireNoRSQL">
			<desc><![CDATA[TCharteIOContractDAOSearchContractListByPrepaymentHireNoRSQL]]></desc>
			<sql><![CDATA[
SELECT PPAY_HIR_NO,
       (SELECT FLET_CTRT_TP_CD 
	      FROM FMS_CONTRACT 
		 WHERE FLET_CTRT_NO = @[flet_ctrt_no] 
		   AND ROWNUM = 1) FLET_CTRT_TP_CD
  FROM FMS_INVOICE FI
 WHERE FLET_CTRT_NO = @[flet_ctrt_no]
   AND FLET_ISS_TP_CD = 'PRE'
 ORDER BY PPAY_HIR_NO DESC			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
