<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchImpCgoManiDgSeqRSQL">
			<desc><![CDATA[BKG별 Danger Cargo의 Seq갯수를 구해서 searchimpCgoManiDgCd의 for문을 돌린다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(DCGO_SEQ) CNT
  FROM BKG_DG_CGO
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
