<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchCntrNoKorRSQL">
			<desc><![CDATA[SELECT CNTR_NO
        FROM   BKG_CSTMS_KR_CNTR
        WHERE  BKG_NO = SUBSTR(:a_bkg_no[i],1,11)
        AND    BKG_NO_SPLIT = SUBSTR(:a_bkg_no[i],12,2)
        AND    CSTMS_DECL_TP_CD = :a_tr_cd[i]
        AND    PORT_CD = :kt_port
        AND    TRNS_SEQ = :a_kt_seq[i]]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO
  FROM BKG_CSTMS_KR_CNTR
 WHERE BKG_NO = @[bkg_no]
   AND CSTMS_DECL_TP_CD = @[tr_cd]
   AND DMST_PORT_CD = @[kt_port]
   AND TRNS_SEQ = @[kt_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="tr_cd" type="12" value="" out="N"/>
				<param name="kt_port" type="12" value="" out="N"/>
				<param name="kt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
