<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOSearchAGTCSRInfoRSQL">
			<desc><![CDATA[SearchAGTCSRInfo]]></desc>
			<sql><![CDATA[
SELECT
           CSR_NO,
           SRC_CTNT
      FROM AP_INV_HDR
     WHERE CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
