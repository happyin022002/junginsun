<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchJapanScacCodeRSQL">
			<desc><![CDATA[flat file 형성을 위한 타선사의 Japan Scac code 조회 쿼리]]></desc>
			<sql><![CDATA[
SELECT M.CRR_CD, B.BKG_NO, B.SCAC_CD
FROM BKG_BOOKING B, BKG_VVD V, MDM_VSL_CNTR M
 WHERE B.BKG_NO=V.BKG_NO
AND B.POL_CD LIKE 'JP%'
AND B.BKG_CRE_DT > SYSDATE - 100
AND V.VSL_CD=M.VSL_CD
			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
