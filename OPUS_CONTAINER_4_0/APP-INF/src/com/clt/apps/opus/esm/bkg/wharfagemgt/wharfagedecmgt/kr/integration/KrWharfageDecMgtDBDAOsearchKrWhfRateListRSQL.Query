<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOsearchKrWhfRateListRSQL">
			<desc><![CDATA[s]]></desc>
			<sql><![CDATA[
SELECT
 BL_NO
,CHG_RT_SEQ
,CNTR_WGT
,MEAS_QTY
,TEU_QTY
,CNTR_RAT_UT_CD CNTR_PER_TP_CD
,OLD_CHG_AMT
,NEW_CHG_AMT
,DIFF_AMT
,WHF_DECL_NO
,WHF_DECL_DT
,KR_WHF_EXPT_CD
,WHF_DECL_IF_FLG
,AR_IF_FLG
,AR_IF_DT
,DECL_RMK
,TO_CHAR( CRE_DT, 'YYYY-MM-DD' )CRE_DT
,WGT_UT_CD
,MEAS_UT_CD
,VSL_CD
,SKD_VOY_NO
,SKD_DIR_CD
FROM BKG_KR_WHF_RT
WHERE 1=1
#if (${vvd} != '') 
AND VSL_CD      = SUBSTR( @[vvd],1,4 )
AND SKD_VOY_NO  = SUBSTR( @[vvd],5,4 )
AND SKD_DIR_CD  = SUBSTR( @[vvd],9,1 )
#end
#if (${bl_no} != '') 
AND BL_NO = @[bl_no]
#end

#if (${whf_bnd_cd} != 'AL') 
AND WHF_BND_CD = @[whf_bnd_cd]
#end

#if (${whf_decl_no} != '') 
AND WHF_DECL_NO = @[whf_decl_no]
#end			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="whf_bnd_cd" type="12" value="" out="N"/>
				<param name="whf_decl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
