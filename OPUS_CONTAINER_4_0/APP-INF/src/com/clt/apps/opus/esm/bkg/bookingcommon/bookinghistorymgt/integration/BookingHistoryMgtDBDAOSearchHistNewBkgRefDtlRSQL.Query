<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchHistNewBkgRefDtlRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchHistNewBkgRefDtlRSQL]]></desc>
			<sql><![CDATA[
SELECT HIS_CATE_NM
     , PRE_CTNT
     , CRNT_CTNT
  FROM 
    (
        SELECT 'P/O & OTHER NO' HIS_CATE_NM
                , '' PRE_CTNT
                , NOW.CNTR_NO||
                  '/'||NOW.PO_NO||
                  '/'||NOW.ITM_NO||
                  '/PACKAGE:'||NOW.PCK_QTY||' '||NOW.PCK_TP_CD||
                  '/WEIGHT:'||NOW.CNTR_WGT||' '||NOW.WGT_UT_CD||
                  '/MEASURE:'||NOW.MEAS_QTY||' '||NOW.MEAS_UT_CD CRNT_CTNT
#if (${ca_flg} == 'Y')
          FROM BKG_REF_DTL_HIS NOW
         WHERE NOW.BKG_NO  = @[bkg_no] 
	       AND NOW.CORR_NO = 'TMP0000001' 
#else
          FROM BKG_REF_DTL NOW
         WHERE NOW.BKG_NO = @[bkg_no] 
#end
      
#if ($filed_list.size() > 0)
           AND (BKG_NO, REF_SEQ) NOT IN (
           #foreach($filed_id in ${filed_list}) 
               #if($velocityCount < $filed_list.size()) $filed_id,  #else $filed_id  #end
           #end
           )
#end
    )
 WHERE CRNT_CTNT IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
