<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24CustomsTransmissionDBDAOAddAdvJpReceiveLogCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_JP_RCV_LOG

       (JP_MSG_TP_ID,
        RCV_DT,
        MSG_RCV_NO,
        RCV_SEQ,
        JP_SVC_ID,
        RCV_KEY_DAT_CTNT,
        JP_BAT_NO,
        VSL_CD,
        SKD_VOY_NO,
        SKD_DIR_CD,
        POL_CD,
        POD_CD,
        POR_CD,
        BKG_NO,
        EDI_RCV_MSG_CTNT,
        HBL_NO,
        EDI_REF_ID,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (@[jp_msg_tp_id],
        NVL(GLOBALDATE_PKG.TIME_LOCAL_FNC(@[pol_cd]), SYSDATE),
        SUBSTR(LTRIM(TO_CHAR(BKG_CSTM_EDI_SEQ.NEXTVAL, '000009'), ' '), 2)||TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS'),
        @[rcv_seq],
        @[jp_svc_id],
        @[rcv_key_dat_ctnt],
        @[jp_bat_no],
        @[vsl_cd],
        @[skd_voy_no],
        @[skd_dir_cd],
        @[pol_cd],
        @[pod_cd],
        @[por_cd],
        @[bkg_no],
        @[edi_rcv_msg_ctnt],
        @[hbl_no],
        @[edi_ref_id],
        @[usr_id],
        SYSDATE,
        @[usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="jp_msg_tp_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="rcv_seq" type="12" value="" out="N"/>
				<param name="jp_svc_id" type="12" value="" out="N"/>
				<param name="rcv_key_dat_ctnt" type="12" value="" out="N"/>
				<param name="jp_bat_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="edi_rcv_msg_ctnt" type="12" value="" out="N"/>
				<param name="hbl_no" type="12" value="" out="N"/>
				<param name="edi_ref_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
