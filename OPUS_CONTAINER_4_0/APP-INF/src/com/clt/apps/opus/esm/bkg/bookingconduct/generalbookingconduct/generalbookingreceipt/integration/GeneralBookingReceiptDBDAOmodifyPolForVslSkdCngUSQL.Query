<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOmodifyPolForVslSkdCngUSQL">
			<desc><![CDATA[VSK에서 VSK_VSL_PORT_SKD에 CLPT_IND_SEQ나 YD_CD가 변경되었을 때 BKG_VVD에 UPDATE하기 위해 사용함]]></desc>
			<sql><![CDATA[
UPDATE BKG_VVD
   SET POL_YD_CD        = @[new_yd_cd]
     , POL_CLPT_IND_SEQ = @[new_clpt_ind_seq]
     , upd_usr_id       = @[upd_usr_id]
	 , upd_dt           = SYSDATE
 WHERE VSL_CD     = substr(@[vvd], 1, 4)
   AND SKD_VOY_NO = substr(@[vvd], 5, 4)
   AND SKD_DIR_CD = substr(@[vvd], 9, 1)
   AND POL_CD     = @[port_cd]
   AND NVL(POL_CLPT_IND_SEQ, 1) = @[old_clpt_ind_seq]			]]></sql>
			<params>
				<param name="new_yd_cd" type="12" value="" out="N"/>
				<param name="new_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="old_clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
