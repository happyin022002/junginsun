<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiRefNoRSQL">
			<desc><![CDATA[searchDblEdiRefNo]]></desc>
			<sql><![CDATA[
SELECT CUST_REF_NO_CTNT
FROM   BKG_REFERENCE
WHERE  BKG_NO = @[bkg_no]
AND    BKG_REF_TP_CD = 'ESRF'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
