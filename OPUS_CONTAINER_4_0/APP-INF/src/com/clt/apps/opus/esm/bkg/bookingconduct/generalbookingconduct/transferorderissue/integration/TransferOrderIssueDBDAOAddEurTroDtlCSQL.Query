<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOAddEurTroDtlCSQL">
			<desc><![CDATA[TransferOrderIssueDBDAOAddEurTroDtlCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_EUR_TRO_DTL (
	BKG_NO
,	IO_BND_CD
,	TRO_SEQ
,	TRO_SUB_SEQ
,   DOR_ADDR_TP_CD
,   LOC_CD
,   ZN_CD
,   LOD_REF_NO
,   DOR_ZIP_ID
,   DOR_ADDR
,   ARR_DT
,   CNTC_PSON_NM
,   CNTC_PHN_NO
,   CNTC_EML
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[bkg_no]
,	@[io_bnd_cd]
,	@[tro_seq]
,	@[tro_sub_seq]
,   @[dor_addr_tp_cd]
,   @[dor_loc_cd]
#if (${zn_cd} != '') 
,   @[dor_loc_cd]||@[zn_cd]
#else 
,   @[zn_cd] 
#end 
,   @[lod_ref_no]
,   @[dor_pst_no]
,   SUBSTRB(RPAD(@[dor_addr_1], 50, ' '),1,50)||
    SUBSTRB(RPAD(@[dor_addr_2], 50, ' '),1,50)||
    SUBSTRB(RPAD(@[dor_addr_3], 50, ' '),1,50)||
    SUBSTRB(RPAD(@[dor_addr_4], 50, ' '),1,50)
#if (${arr_dt} != '') 
,   TO_DATE(@[arr_dt]||' '||@[arr_dt_hhmi], 'YYYY-MM-DD HH24:MI') 
#else 
,   @[arr_dt]
#end
,   @[cntc_pson_nm]
,   @[cntc_phn_no]
,   @[cntc_eml]
,	@[cre_usr_id]
,	sysdate 
,	@[upd_usr_id]
,	sysdate 
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
				<param name="tro_sub_seq" type="12" value="" out="N"/>
				<param name="dor_addr_tp_cd" type="12" value="" out="N"/>
				<param name="dor_loc_cd" type="12" value="" out="N"/>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="lod_ref_no" type="12" value="" out="N"/>
				<param name="dor_pst_no" type="12" value="" out="N"/>
				<param name="dor_addr_1" type="12" value="" out="N"/>
				<param name="dor_addr_2" type="12" value="" out="N"/>
				<param name="dor_addr_3" type="12" value="" out="N"/>
				<param name="dor_addr_4" type="12" value="" out="N"/>
				<param name="arr_dt" type="12" value="" out="N"/>
				<param name="arr_dt_hhmi" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_phn_no" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
