<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchInernettBlControlPartyListRSQL">
			<desc><![CDATA[select Internet B/L Control  List]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD,
       CUST_SEQ,
	   CUST_CNT_CD||LPAD(CUST_SEQ,6,'0') AS CUST_CD,
	   CUST_CNT_CD||LPAD(CUST_SEQ,6,'0') AS OLD_CUST_CD,
       CTRL_PTY_SEQ,
       (SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER WHERE CUST_CNT_CD = A.CUST_CNT_CD AND CUST_SEQ = A.CUST_SEQ) AS CUST_NM,
		UPD_USR_ID,
		UPD_DT
FROM BKG_INET_BL_CTRL_PTY A
WHERE 1 = 1
#if (${ctrl_pty_seq} != '') 
AND CTRL_PTY_SEQ = @[ctrl_pty_seq]
#end
#if (${cust_cnt_cd} != '') 
AND CUST_CNT_CD||LPAD(CUST_SEQ,6,'0') = @[cust_cnt_cd]
#end			]]></sql>
			<params>
				<param name="ctrl_pty_seq" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
