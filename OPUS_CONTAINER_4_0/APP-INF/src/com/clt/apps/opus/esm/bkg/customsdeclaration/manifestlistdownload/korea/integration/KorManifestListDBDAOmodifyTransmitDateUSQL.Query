<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOmodifyTransmitDateUSQL">
			<desc><![CDATA[Amendment 전송 일시 저장]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_KR_BL
   SET MF_SND_DT = SYSDATE
     , MF_SND_USR_ID = @[user_id]
 WHERE BKG_NO = @[bkg_no]
   AND CSTMS_DECL_TP_CD = @[cstms_decl_tp_cd]
   AND DMST_PORT_CD     = @[port_cd]
   AND TRNS_SEQ         = TO_NUMBER(SUBSTR(@[trns_seq],1,2))			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="trns_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
