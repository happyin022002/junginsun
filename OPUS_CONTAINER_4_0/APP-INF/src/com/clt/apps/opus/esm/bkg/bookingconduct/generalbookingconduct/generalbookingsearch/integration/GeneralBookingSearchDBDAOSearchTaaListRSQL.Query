<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchTaaListRSQL">
			<desc><![CDATA[Taa 정보를 조회한다.]]></desc>
			<sql><![CDATA[
select distinct cust.cust_cnt_cd
        , cust.cust_seq
        , cust.cust_lgl_eng_nm
        , (select hdr.taa_no from pri_taa_hdr hdr where hdr.taa_prop_no = main.taa_prop_no) taa_no
        , respb_sls_ofc_cd  sales_ofc
        , main.svc_scp_cd
		, (select svc_scp_nm from mdm_svc_scp scope where scope.svc_scp_cd = main.svc_scp_cd) svc_scp_nm
  from  (
#if (${bkg_no} != '') 
SELECT NVL((
            SELECT RT_APLY_DT 
              FROM BKG_RT_HIS R
             WHERE BKG_NO = @[bkg_no]
               AND CORR_NO = 'TMP0000001'
               AND RT_APLY_DT IS NOT NULL
           ),
       NVL((
            SELECT RT_APLY_DT
              FROM BKG_RATE R
             WHERE BKG_NO = @[bkg_no]
               AND RT_APLY_DT IS NOT NULL
           ),
       NVL((
            SELECT SKD.VPS_ETD_DT
              FROM BKG_VVD_HIS VVD, VSK_VSL_PORT_SKD SKD, BKG_BKG_HIS BK
             WHERE BK.BKG_NO          = @[bkg_no] 
               AND BK.CORR_NO         = 'TMP0000001'
               AND VVD.CORR_NO        = 'TMP0000001'
               AND BK.BKG_NO          = VVD.BKG_NO
               AND VVD.VSL_PRE_PST_CD IN ('S', 'T')
               AND VVD.POL_CD         = BK.POL_CD
               AND VVD.VSL_CD         = SKD.VSL_CD
               AND VVD.SKD_VOY_NO     = SKD.SKD_VOY_NO
               AND VVD.SKD_DIR_CD     = SKD.SKD_DIR_CD
               AND VVD.POL_CD         = SKD.VPS_PORT_CD
               AND VVD.POL_CLPT_IND_SEQ = SKD.CLPT_IND_SEQ
			   AND ROWNUM = 1
           ),
       NVL((
            SELECT SKD.VPS_ETD_DT
              FROM BKG_VVD VVD, VSK_VSL_PORT_SKD SKD, BKG_BOOKING BK
             WHERE BK.BKG_NO          = @[bkg_no] 
               AND BK.BKG_NO          = VVD.BKG_NO
               AND VVD.VSL_PRE_PST_CD IN ('S', 'T')
               AND VVD.POL_CD         = BK.POL_CD
               AND VVD.VSL_CD         = SKD.VSL_CD
               AND VVD.SKD_VOY_NO     = SKD.SKD_VOY_NO
               AND VVD.SKD_DIR_CD     = SKD.SKD_DIR_CD
               AND VVD.POL_CD         = SKD.VPS_PORT_CD
               AND VVD.POL_CLPT_IND_SEQ = SKD.CLPT_IND_SEQ
			   AND ROWNUM = 1
           ),SYSDATE)
       ))) AS APPL_DT
  FROM DUAL
#else
	SELECT SYSDATE AS APPL_DT FROM DUAL
#end
      ) appl
    , (select cust_cnt_cd, cust_seq, cust_lgl_eng_nm
         from mdm_customer
        where cust_cnt_cd     = nvl(@[s_cust_cnt_cd], 'xx')
#if (${s_cust_seq} == '' && ${s_cust_nm} == '') 
		  and cust_seq = 0 --seq, name이 둘다 없는 경우 조회에서 제외
#end  
#if (${s_cust_seq} != '')
		  and cust_seq 		  = @[s_cust_seq]
#end
#if (${s_cust_nm} != '') 
		  and cust_lgl_eng_nm like decode(@[include_flag], 'Y', '%', '')||@[s_cust_nm]||'%'
#end        
          and delt_flg        = 'N'
       union 
       select cust_cnt_cd, cust_seq, cust_lgl_eng_nm
         from mdm_customer
        where cust_cnt_cd     = nvl(@[c_cust_cnt_cd], 'xx')
#if (${c_cust_seq} == '' && ${c_cust_nm} == '') 
		  and cust_seq = 0 --seq, name이 둘다 없는 경우 조회에서 제외
#end  
#if (${c_cust_seq} != '')
		  and cust_seq 		  = @[c_cust_seq]
#end
#if (${c_cust_nm} != '') 
		  and cust_lgl_eng_nm like decode(@[include_flag], 'Y', '%', '')||@[c_cust_nm]||'%'
#end        
          and delt_flg        = 'N'
       union
       select cust_cnt_cd, cust_seq, cust_lgl_eng_nm
         from mdm_customer
        where cust_cnt_cd     = nvl(@[bkg_ctrl_pty_cust_cnt_cd], 'xx')
#if (${bkg_ctrl_pty_cust_seq} == '' && ${bkg_ctrl_pty_cust_nm} == '') 
		  and cust_seq = 0 --seq, name이 둘다 없는 경우 조회에서 제외
#end  
#if (${bkg_ctrl_pty_cust_seq} != '')
		  and cust_seq 		  = @[bkg_ctrl_pty_cust_seq]
#end
#if (${bkg_ctrl_pty_cust_nm} != '') 
          and cust_lgl_eng_nm like decode(@[include_flag], 'Y', '%', '')||@[bkg_ctrl_pty_cust_nm]||'%'
#end        
          and delt_flg        = 'N'
#if (${bkg_no} != '') 
	   union 
       select cust.cust_cnt_cd, cust.cust_seq, cust.cust_nm
         from bkg_customer cust, bkg_booking bk
        where bk.bkg_no          = @[bkg_no]   
          and bk.bkg_no          = cust.bkg_no
          and bk.CUST_TO_ORD_FLG = 'Y'
          and cust.bkg_cust_tp_Cd    = 'N'
#end
          ) cust
#if (${por_cd} != '' && ${del_cd} != '') 
    , (SELECT DISTINCT LOC_SCOPE.SVC_SCP_CD
		 FROM (SELECT ORG.SVC_SCP_CD SVC_SCP_CD, COUNT(1) CNT
        		 FROM MDM_SVC_SCP_LMT ORG
		            , MDM_SVC_SCP_LMT DEST
			    WHERE ORG.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[por_cd])
			      AND ORG.ORG_DEST_CD 		= 'O'
			      AND ORG.SVC_SCP_IND_FLG 	= 'Y'
			      AND ORG.DELT_FLG			= 'N'
			      AND DEST.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[del_cd])
			      AND DEST.ORG_DEST_CD 		= 'D'
			      AND DEST.SVC_SCP_IND_FLG 	= 'Y'
			      AND DEST.DELT_FLG 		= 'N'
        		  AND ORG.SVC_SCP_CD = DEST.SVC_SCP_CD
		        GROUP BY ORG.SVC_SCP_CD) LOC_SCOPE,
			  (SELECT ORG.SVC_SCP_CD SVC_SCP_CD, 1 CNT
			     FROM MDM_SVC_SCP_LANE LANE 
			        , MDM_SVC_SCP_LMT ORG
			        , MDM_SVC_SCP_LMT DEST
			        , VSK_VSL_SKD SKD
			    WHERE ORG.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[por_cd])
			      AND ORG.ORG_DEST_CD 		= 'O'
			      AND ORG.SVC_SCP_IND_FLG 	= 'Y'
			      AND ORG.DELT_FLG			= 'N'
			      AND DEST.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[del_cd])
			      AND DEST.ORG_DEST_CD 		= 'D'
			      AND DEST.SVC_SCP_IND_FLG 	= 'Y'
			      AND DEST.DELT_FLG 		= 'N'
			      AND ORG.SVC_SCP_CD   		= DEST.SVC_SCP_CD
			      AND LANE.VSL_SLAN_CD 		= SKD.VSL_SLAN_CD
			      AND SKD.VSL_CD    		= SUBSTR(@[bkg_vvd], 1, 4)
			      AND SKD.SKD_VOY_NO 		= SUBSTR(@[bkg_vvd], 5, 4)
			      AND SKD.SKD_DIR_CD 		= SUBSTR(@[bkg_vvd], 9, 1)
			    GROUP BY ORG.SVC_SCP_CD
			    UNION 
			    SELECT '   ' SVC_SCP_CD , 1 CNT FROM DUAL) VSL_SCOPE
		WHERE ((LOC_SCOPE.CNT = 1 AND LOC_SCOPE.CNT = VSL_SCOPE.CNT)
		       OR 
		       (LOC_SCOPE.CNT > 1 AND LOC_SCOPE.SVC_SCP_CD = VSL_SCOPE.SVC_SCP_CD)
		      )
         ) scope
#end
    , pri_taa_mn main
  where cust_cnt_cd         = ctrt_cust_cnt_cd 
    and cust_seq            = ctrt_cust_Seq
#if (${por_cd} != '' && ${del_cd} != '') 
    and main.svc_scp_cd    =  scope.SVC_SCP_CD
#end   
    and MAIN.eff_dt - 0.0001 < APPL.APPL_DT
    AND MAIN.exp_dt + 0.9999 > APPL.APPL_DT
    and main.cfm_flg = 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="s_cust_cnt_cd" type="12" value="KR" out="N"/>
				<param name="s_cust_seq" type="12" value="7" out="N"/>
				<param name="include_flag" type="12" value="N" out="N"/>
				<param name="s_cust_nm" type="12" value="" out="N"/>
				<param name="c_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="c_cust_seq" type="12" value="" out="N"/>
				<param name="c_cust_nm" type="12" value="" out="N"/>
				<param name="bkg_ctrl_pty_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="bkg_ctrl_pty_cust_seq" type="12" value="" out="N"/>
				<param name="bkg_ctrl_pty_cust_nm" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="KRPUS" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="bkg_vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
