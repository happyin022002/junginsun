<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOBkgRevDrNoteProgVORSQL">
			<desc><![CDATA[   PROG STATUS가  최신값을 조회]]></desc>
			<sql><![CDATA[
SELECT  RDN_RMK
FROM
	(
	SELECT  *
	FROM    BKG_REV_DR_NOTE_PROG
	WHERE   RDN_NO = @[rdn_no]
	AND     RDN_STS_CD IN ( 'IS', 'RV', 'CL', 'ST' )
	ORDER BY RVIS_SEQ DESC, PROG_SEQ DESC
	)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="rdn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
