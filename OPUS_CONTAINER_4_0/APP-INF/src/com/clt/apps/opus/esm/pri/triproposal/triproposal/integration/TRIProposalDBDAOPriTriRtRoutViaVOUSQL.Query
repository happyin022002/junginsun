<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriRtRoutViaVOUSQL">
			<desc><![CDATA[PRI_TRI_RT_ROUT_VIA UPDATE]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRI_RT_ROUT_VIA SET 
	ROUT_VIA_PORT_CD = @[rout_via_port_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	TRI_PROP_NO = @[tri_prop_no]
AND	ORG_DEST_TP_CD = @[org_dest_tp_cd]
AND	ROUT_VIA_SEQ = @[rout_via_seq]
			]]></sql>
			<params>
				<param name="rout_via_port_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="tri_prop_no" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="rout_via_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
