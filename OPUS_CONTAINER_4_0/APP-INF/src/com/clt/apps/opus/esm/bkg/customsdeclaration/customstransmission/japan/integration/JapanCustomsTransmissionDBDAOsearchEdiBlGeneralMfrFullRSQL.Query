<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOsearchEdiBlGeneralMfrFullRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT --06.Vessel Code (9)
       RPAD(NVL(SUBSTR(@[in_call_sgn_no], 1, 9), ' '), 9, ' ') AS DATA00,
       --07.Operating Carrier Voyage Number (10)
       RPAD(NVL(JP_TML_VSL_NO, SUBSTR(@[in_vvd_cd], 5, 5)), 10, ' ') AS DATA01,
       --08.Voyage Number (10)
       RPAD(NVL(JP_TML_VSL_NO, SUBSTR(@[in_vvd_cd], 5, 5)), 10, ' ') AS DATA02,
       --09.Consortium Voyage Number (10)
       RPAD(NVL(JP_TML_VSL_NO, ' '), 10, ' ') AS DATA03,
       --10.Carrier Code (4)
       COM_ConstantMgr_PKG.COM_getScacCode_FNC() AS DATA04,
       --------------------------------------------------------------------
       --14.Container Operator Code (5)
       RPAD(NVL(A.CY_OPR_ID, ' '), 5, ' ') AS DATA05,
       --15.B/L Number (35)
       RPAD(COM_ConstantMgr_PKG.COM_getScacCode_FNC()||@[bl_no], 35, ' ') AS DATA06,
       --16.Port of Loading Code (5)
       CASE
          WHEN SUBSTR(L1.LOC_CD, 1, 2) = 'JP' THEN
             NVL(L3.ATTR_CTNT2, L1.LOC_CD)
          ELSE
             NVL(L3.ATTR_CTNT2, DECODE(SUBSTR(L1.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L1.LOC_CD, 1, 2))||'ZZZ')
       END AS DATA07,
       --17.Final Destination Code (5)
       RPAD(' ', 5, ' ') AS DATA08,
       --18.Final Destination Name (20)
       RPAD(' ', 20, ' ') AS DATA09,
       --19.Place of Delivery Code (5)
       CASE
          WHEN SUBSTR(L2.LOC_CD, 1, 2) = 'JP' THEN
             NVL(L4.ATTR_CTNT2, L2.LOC_CD)
          ELSE
             NVL(L4.ATTR_CTNT2, DECODE(SUBSTR(L2.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L2.LOC_CD, 1, 2))||'ZZZ')
       END AS DATA10,
       --20.Place of Delivery Name (20)
       RPAD(SUBSTR(UPPER(L2.LOC_NM), 1, 20), 20, ' ') AS DATA11,
       --------------------------------------------------------------------
       --52.HS Code (The First 6-digit) (6)
       (SELECT NVL(SUBSTR(MAX(CMDT_HS_CD), 1, 6), '      ')
          FROM BKG_CNTR_MF_DESC
         WHERE BKG_NO = B.BKG_NO
           AND CNTR_MF_WGT IN (SELECT MAX(CMD.CNTR_MF_WGT)
                                 FROM BKG_CNTR_MF_DESC CMD
                                WHERE CMD.BKG_NO = B.BKG_NO
                                  AND SUBSTR(CMD.CMDT_HS_CD, 1, 2) NOT IN ('00', '98', '99'))
           AND SUBSTR(CMDT_HS_CD, 1, 2) NOT IN ('00', '98', '99')) AS DATA12,
       --54.Number of Packages (8)
       LPAD(NVL(A.PCK_QTY, 0), 8, ' ') AS DATA13,
       --55.Number of Packages Unit Code (3)
       RPAD(NVL(P.CSTMS_PCK_TP_CD, 'ZZ'), 3, ' ') AS DATA14,
       --56.Gross Weight (10)
       LPAD(TO_CHAR(NVL(A.GRS_WGT, 0), 'FM999990.000'), 10, ' ') AS DATA15,
       --57.Weight Unit Code (3)
       DECODE(NVL(A.WGT_UT_CD, 'KGS'), 'LBS', 'LBR', 'KGS', 'KGM', A.WGT_UT_CD ) AS DATA16,
       --58.Net Weight (10)
       RPAD(' ', 10, ' ') AS DATA17,
       --59.Weight Unit Code (3)
       RPAD(' ', 3, ' ') AS DATA18,
       --60.Measurement (10)
       LPAD(TO_CHAR(NVL(A.MEAS_QTY, 0), 'FM999990.000'), 10, ' ') AS DATA19,
       --61.Measurement Unit Code (3)
       DECODE(NVL(A.MEAS_UT_CD, 'CBM'), 'CMF', 'FTQ', 'MTQ') AS DATA20,
       --62.Country of Origin Code (2)
       RPAD(' ', 2, ' ') AS DATA21,
       --63.Dangerous Cargo Code (3)
       RPAD(' ', 3, ' ') AS DATA22,
       --64.Freight (18)
       RPAD(' ', 18, ' ') AS DATA23,
       --65.Freight Currency Code (3)
       RPAD(' ', 3, ' ') AS DATA24,
       --66.Value (18)
       RPAD(' ', 18, ' ') AS DATA25,
       --67.Value Currency Code (3)
       RPAD(' ', 3, ' ') AS DATA26,
       --------------------------------------------------------------------
       --68.Comprehensive In-Bond Transportation Approval Number (11)
       RPAD(NVL(A.APRO_NO, ' '), 11, ' ') AS DATA27,
       --69.Temporary Discharge Identifier (3)
       DECODE(NVL(A.LOCL_TS_IND_CD, 'L'), 'T', '28 ', '   ') AS DATA28,
       --70.Reason for Temporary Discharge Code (3)
       DECODE(NVL(A.LOCL_TS_IND_CD, ' '), 'T', NVL(A.JP_CSTMS_TRNS_CD, ' '), '   ') AS DATA29,
       --71.Duration of Temporary Discharge (2)
       DECODE(NVL(A.LOCL_TS_IND_CD, ' '), 'T', SUBSTR(TO_CHAR(NVL(A.LMT_NO, 0), '99'), 2), '  ') AS DATA30,
       --72.Estimated Start Date of Transportation (8)
       RPAD(' ', 8, ' ') AS DATA31,
       --73.Estimated Finish Date of Transportation (8)
       RPAD(' ', 8, ' ') AS DATA32,
       --74.Code of Transportation Mode of Separate Transit/In-Bond Transportation of Temporary Discharge Cargo (2)
       RPAD(NVL(DECODE(A.TRSP_MOD_CD, 'SH', '11', 'BA', '16', 'RA', '25', 'TR', '31', 'OT', '17', NULL), ' '), 2, ' ') AS DATA33,
       --75.Arrival Place Code (5)
       RPAD(NVL(A.CSTMS_CD, ' '), 5, ' ') AS DATA34,
       --76.Arrival Place Name (35)
       RPAD(NVL((SELECT NVL(WH_NM, ' ')
                   FROM BKG_CSTMS_JP_WH
                  WHERE CSTMS_CD = A.CSTMS_CD
                    AND ROWNUM = 1), ' '), 35, ' ') AS DATA35,
       --77.Code of Other Relevant Laws and Ordinances (2) x5
       RPAD(' ', 2, ' ') AS DATA36,
       RPAD(' ', 2, ' ') AS DATA37,
       RPAD(' ', 2, ' ') AS DATA38,
       RPAD(' ', 2, ' ') AS DATA39,
       RPAD(' ', 2, ' ') AS DATA40,
       --78.Remark (140)
       RPAD(' ', 140, ' ') AS DATA41

  FROM BKG_CSTMS_JP_BL A,
       BKG_BOOKING B,
       MDM_LOCATION L1,
       MDM_LOCATION L2,
       BKG_CSTMS_PCK_TP_CONV P,
       BKG_CSTMS_CD_CONV_CTNT L3,
       BKG_CSTMS_CD_CONV_CTNT L4

 WHERE A.BL_NO = @[bl_no]
   AND A.BL_SPLIT_NO = NVL(@[bl_split_no], '  ')
   AND A.BL_NO = B.BL_NO
   AND A.POL_CD = L1.LOC_CD(+)
   AND A.BKG_DEL_CD = L2.LOC_CD(+)
   AND L3.CNT_CD(+) = 'JP'
   AND L3.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.POL_CD = L3.ATTR_CTNT1(+)
   AND L4.CNT_CD(+) = 'JP'
   AND L4.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.BKG_DEL_CD = L4.ATTR_CTNT1(+)
   AND A.PCK_TP_CD = P.PCK_TP_CD(+)
   AND P.DELT_FLG(+) = 'N'
   AND P.CNT_CD(+) = 'JP'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="in_call_sgn_no" type="12" value="" out="N"/>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
