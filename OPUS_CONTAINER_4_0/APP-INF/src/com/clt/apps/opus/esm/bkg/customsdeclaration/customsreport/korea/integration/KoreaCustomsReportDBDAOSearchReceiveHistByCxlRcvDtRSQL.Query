<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOSearchReceiveHistByCxlRcvDtRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT MSG_LOG_TP_CD
      , TO_CHAR(RCV_DT,'YYYY-MM-DD HH24:MI:SS') AS RCV_DT
      , '' AS VVD
      , '' AS POL_CD
      , '' AS POD_CD
      , '' AS OFC_CD
      , '' AS BL_NO
      , DECODE(@[cboMsgTp], '5VJ', EDI_SND_MSG_CTNT, '') AS SMT_AMD_NO
      , DECODE(KR_CSTMS_ACPT_CD, 'C', '승인', 'E', '기각') AS MSG_STS
      , KR_CSTMS_RJCT_RSN1 AS MSG_TEXT
      , EDI_RCVR_NM
      , TML_CD
      , KR_VSL_CALL_SGN_CD
      , ARR_YR
      , CALL_KNT
      , CSTMS_REF_NM

FROM    BKG_CSTMS_KR_RCV_LOG

WHERE   1 = 1
AND     MSG_LOG_TP_CD = @[cboMsgTp]
AND     RCV_DT >= TO_DATE(@[from_dt]||'000000', 'YYYY-MM-DDHH24MISS')
AND     RCV_DT <= TO_DATE(@[to_dt]  ||'235959', 'YYYY-MM-DDHH24MISS')			]]></sql>
			<params>
				<param name="cboMsgTp" type="12" value="" out="N"/>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
