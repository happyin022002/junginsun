<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRISimulationDBDAOSearchPriSimRtListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT B.CNTR_TPSZ_CD AS CNTR_SZ_CD
      ,P.PCTL_NO
      ,P.SVC_SCP_CD
  FROM PRI_SIM_PARA P
      ,MDM_CNTR_TP_SZ       B
 WHERE P.PCTL_NO LIKE @[f_pctl_no]||'%'
   AND P.EQ_TP_CD = B.CNTR_TP_CD
   --AND B.CNTR_SZ_CD IN ('2','4','5', '7')
   AND B.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="f_pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
