<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOMultiOtherCommForRequestAgtCommBkgInfoUSQL">
			<desc><![CDATA[MultiOtherCommForRequestAgtCommBkgInfo]]></desc>
			<sql><![CDATA[
UPDATE AGT_COMM_BKG_INFO
       SET REV_VVD_CD      = @[vvd],
	       TRNK_VSL_CD     = SUBSTR(@[vvd],1,4),
	       TRNK_SKD_VOY_NO = SUBSTR(@[vvd],5,4),
	       TRNK_SKD_DIR_CD = SUBSTR(@[vvd],9,1),
	       TRNK_REV_DIR_CD = SUBSTR(@[vvd],10,1),
	       UPD_USR_ID      = @[upd_usr_id],
	       UPD_DT          = SYSDATE
     WHERE BKG_NO          = @[bkg_no]			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="bkg_no" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
