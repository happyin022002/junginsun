<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AverageRPBDBDAOSearchTradeRPBListRSQL">
			<desc><![CDATA[SearchTradeRPBList]]></desc>
			<sql><![CDATA[
SELECT RPB_YRMON
     , DIR_CD
     , TRD_CD
     , NET_20FT_AVG_REV
     , OFT_20FT_AVG_REV
     , MISC_20FT_AVG_REV
     , SCR_20FT_AVG_REV
     , NET_40FT_AVG_REV
     , OFT_40FT_AVG_REV
     , MISC_40FT_AVG_REV
     , SCR_40FT_AVG_REV 
FROM COA_MON_TRD_RPB
WHERE 1=1
  AND RPB_YRMON =  REPLACE(@[f_rpb_yrmon],'-','')
#if (${f_trd_cd} != '') 
  AND TRD_CD = @[f_trd_cd]
#end
#if (${f_dir_cd} != '') 
  AND DIR_CD = @[f_dir_cd]
#end
ORDER BY RPB_YRMON
     , DIR_CD
     , TRD_CD			]]></sql>
			<params>
				<param name="f_rpb_yrmon" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
