<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOModifyAGTCommProcStsCdUSQL">
			<desc><![CDATA[ModifyAGTCommProcStsCd]]></desc>
			<sql><![CDATA[
   UPDATE AGT_AGN_COMM A
      SET A.COMM_PROC_STS_CD  = 'IF',
          A.COMM_PROC_STS_RSN = 'Interface OK!',
          A.AC_IF_DT          = SYSDATE
    WHERE A.COMM_APRO_NO = @[inv_no]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
