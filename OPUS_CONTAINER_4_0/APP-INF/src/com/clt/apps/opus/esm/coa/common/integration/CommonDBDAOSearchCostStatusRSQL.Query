<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchCostStatusRSQL">
			<desc><![CDATA[SearchCostStatus]]></desc>
			<sql><![CDATA[
SELECT COST_WK,COST_CRE_STS_CD
  FROM COA_UT_COST_CRE_STS
 WHERE 1=1
   AND CM_UC_CD = DECODE(@[f_cost_type],'acm_oth','ACMC','nod_full','NFCC','nod_empty','NMCC','nod_incen','NICC','trans_full','TFCC','trans_empty','TMCC','trans_incen','TICC','OTHC')
   AND COST_YRMON = REPLACE(@[f_target_yrmon],'-','')			]]></sql>
			<params>
				<param name="f_cost_type" type="12" value="" out="N"/>
				<param name="f_target_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
