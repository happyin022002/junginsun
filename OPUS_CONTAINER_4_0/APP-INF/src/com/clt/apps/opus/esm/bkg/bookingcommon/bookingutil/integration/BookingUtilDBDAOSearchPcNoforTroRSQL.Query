<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchPcNoforTroRSQL">
			<desc><![CDATA[SearchPcNoforTro]]></desc>
			<sql><![CDATA[
#if (${cxl_flg} == 'EUR') 

SELECT 
	PCTL_NO
  FROM BKG_EUR_TRO
 WHERE BKG_NO       = @[bkg_no]
   AND IO_BND_CD    = 'O'
   AND TRO_SEQ      = @[tro_seq]

#else 

SELECT 
	TRO_SUB_SEQ PCTL_NO
  FROM BKG_TRO_DTL
 WHERE BKG_NO       = @[bkg_no]
   AND IO_BND_CD    = 'O'
   AND TRO_SEQ      = @[tro_seq]
   AND RTN_TRO_FLG  = @[rtn_tro_flg]
   AND CXL_FLG      = 'N'
   AND ROWNUM = 1

#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
				<param name="rtn_tro_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
