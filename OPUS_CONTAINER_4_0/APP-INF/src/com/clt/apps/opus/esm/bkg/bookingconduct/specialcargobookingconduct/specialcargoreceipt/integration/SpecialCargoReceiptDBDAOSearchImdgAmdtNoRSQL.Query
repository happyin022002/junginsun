<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOSearchImdgAmdtNoRSQL">
			<desc><![CDATA[Search IMDG_AMDT_NO]]></desc>
			<sql><![CDATA[
SELECT	LISTAGG(IMDG_AMDT_NO, ',') WITHIN GROUP (ORDER BY DECODE(IMDG_AMDT_STND_FLG,'Y','1','N','2','9')) AS IMDG_AMDT_NO
FROM	SCG_IMDG_AMDT_MST
WHERE	ACT_FLG = 'Y' 			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
