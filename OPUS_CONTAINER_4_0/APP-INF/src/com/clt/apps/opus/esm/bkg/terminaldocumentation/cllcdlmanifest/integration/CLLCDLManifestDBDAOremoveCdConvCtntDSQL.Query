<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOremoveCdConvCtntDSQL">
			<desc><![CDATA[removeCdConvCtnt]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CSTMS_CD_CONV_CTNT
WHERE	CNT_CD = @[cnt_cd]
AND	CSTMS_DIV_ID = @[cstms_div_id]
AND	CSTMS_DIV_ID_SEQ = @[cstms_div_id_seq]
			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_div_id" type="12" value="" out="N"/>
				<param name="cstms_div_id_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
