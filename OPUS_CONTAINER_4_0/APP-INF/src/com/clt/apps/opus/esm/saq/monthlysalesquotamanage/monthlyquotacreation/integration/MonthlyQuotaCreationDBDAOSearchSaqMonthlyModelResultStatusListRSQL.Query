<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOSearchSaqMonthlyModelResultStatusListRSQL">
			<desc><![CDATA[confirmMonthlyQuotaFinalVersion 메소드의 준비 단계로서 saq_mon_tgt_vvd 테이블의 tgt_vvd_sts_cd 정보를 확인하는 메소드]]></desc>
			<sql><![CDATA[
SELECT	TGT_VVD_STS_CD 	
FROM	SAQ_MON_TGT_VVD	
WHERE	BSE_YR	= @[year]
	AND	BSE_QTR_CD	= @[bseQtrCd]
	AND	ROWNUM = 1			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="bseQtrCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
