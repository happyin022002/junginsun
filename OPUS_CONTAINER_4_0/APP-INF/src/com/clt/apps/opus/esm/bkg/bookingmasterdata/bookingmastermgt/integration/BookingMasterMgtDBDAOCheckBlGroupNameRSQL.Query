<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOCheckBlGroupNameRSQL">
			<desc><![CDATA[check bl group name duplicate]]></desc>
			<sql><![CDATA[
SELECT COUNT(*)
FROM BKG_CTRL_BL_GRP
WHERE BL_GRP_NM = @[bl_grp_nm]
			]]></sql>
			<params>
				<param name="bl_grp_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
