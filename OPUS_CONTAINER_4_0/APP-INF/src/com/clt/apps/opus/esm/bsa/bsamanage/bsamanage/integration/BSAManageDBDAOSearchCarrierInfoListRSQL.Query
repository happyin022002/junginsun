<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOSearchCarrierInfoListRSQL">
			<desc><![CDATA[Carrier's Register Search]]></desc>
			<sql><![CDATA[
SELECT CRR_CD,CRR_NM
FROM MDM_CARRIER
WHERE NVL(DELT_FLG,'N') ='N'
AND LENGTH(CRR_CD) =3
AND CRR_CD NOT IN (SELECT DISTINCT CRR_CD 
                   FROM BSA_CRR_RGST)
ORDER BY CRR_CD 			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
