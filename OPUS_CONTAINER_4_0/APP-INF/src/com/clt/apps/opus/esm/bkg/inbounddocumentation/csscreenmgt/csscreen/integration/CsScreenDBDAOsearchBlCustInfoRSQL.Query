<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CsScreenDBDAOsearchBlCustInfoRSQL">
			<desc><![CDATA[searchBlCustInfo]]></desc>
			<sql><![CDATA[
SELECT BCST.BKG_CUST_TP_CD               AS BKG_CUST_TP_CD  
      ,BCST.CUST_CNT_CD || BCST.CUST_SEQ AS CUST_CD
      ,BCST.CUST_NM                      AS CUST_NM
      ,BKGM.POD_CD                       AS POD_CD
      ,BKGM.DEL_CD                       AS DEL_CD
      ,BCST.CUST_CNT_CD                  AS CUST_CNT_CD
      ,BCST.CUST_SEQ                     AS CUST_SEQ
      ,BCST.CUST_ADDR                    AS CUST_ADDR
      ,NTC.PKUP_YD_CD                    AS PKUP_YD_CD --Ceres 터미널 경우 CAHAL01일때 RD폼 변경위해
FROM BKG_BOOKING BKGM
    ,BKG_CUSTOMER BCST
    , (SELECT PKUP_YD_CD, BKG_NO
         FROM BKG_PKUP_NTC
        WHERE BKG_NO = @[bkg_no] 
          AND NTC_SEQ = (SELECT MAX(NTC_SEQ)
                           FROM BKG_PKUP_NTC
                          WHERE BKG_NO = @[bkg_no])) NTC 
WHERE BKGM.BKG_NO = @[bkg_no]  --'AARY1050014'
 AND  BKGM.BKG_NO = BCST.BKG_NO 
 AND BCST.BKG_CUST_TP_CD IN ('C', 'N', 'A')
 AND BKGM.BKG_NO = NTC.BKG_NO (+)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
