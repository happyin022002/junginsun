<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOupdateBkgXterRcvMsgUSQL">
			<desc><![CDATA[EBookingReceiptDBDAOupdateBkgXterRcvMsgUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_RCV_MSG 
   SET UPLD_FLG = @[upld_flg]
     , XTER_SNDR_ID = @[xter_sndr_id]
     , XTER_RQST_NO = @[xter_rqst_no]
     , XTER_RQST_SEQ = @[xter_rqst_seq]
     , UPD_DT = SYSDATE
 WHERE BKG_XTER_RCV_MSG_SEQ = @[bkg_xter_rcv_msg_seq]			]]></sql>
			<params>
				<param name="upld_flg" type="12" value="" out="N"/>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
				<param name="bkg_xter_rcv_msg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
