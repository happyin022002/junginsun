<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRatesDBDAOPriTrfInlndHistoryAmendVORSQL">
			<desc><![CDATA[Inland Rates Amend 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  
         TRF_PFX_CD
        ,TRF_NO
        ,TRF_INLND_SEQ
        ,AMDT_SEQ
        ,TRF_INLND_NM
        ,TO_CHAR(EFF_DT, 'YYYYMMDD') AS EFF_DT
        ,TO_CHAR(EXP_DT, 'YYYYMMDD') AS EXP_DT
        ,TO_CHAR(PUB_DT, 'YYYYMMDD') AS PUB_DT
        ,RQST_OFC_CD
        ,APRO_OFC_CD
        ,TRF_INLND_STS_CD
        ,CRE_USR_ID
        ,TO_CHAR(CRE_DT, 'YYYYMMDD') AS CRE_DT
        ,UPD_USR_ID
        ,TO_CHAR(UPD_DT, 'YYYYMMDD') AS UPD_DT
        ,ATCH_FILE_ID
     , (SELECT FILE_UPLD_NM 
		  FROM COM_UPLD_FILE 
		 WHERE FILE_SAV_ID = ATCH_FILE_ID 
		   AND DELT_FLG = 'N') ATCH_FILE_NM
        ,TRF_INLND_AMDT_TP_CD
  FROM  PRI_TRF_INLND
 WHERE  1 = 1
   AND  TRF_PFX_CD    = @[trf_pfx_cd]
   AND  TRF_NO        = @[trf_no]
   AND  TRF_INLND_SEQ = @[trf_inlnd_seq]
   AND  TRF_INLND_STS_CD = 'F'
#if (${access_dt} != '') 
   AND  EFF_DT     <= TO_DATE(@[access_dt], 'YYYY-MM-DD')
   AND  NVL(EXP_DT, TO_DATE('9999-12-31', 'YYYY-MM-DD')) >= TO_DATE(@[access_dt], 'YYYY-MM-DD')
#end
ORDER BY AMDT_SEQ DESC			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_inlnd_seq" type="12" value="" out="N"/>
				<param name="access_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
