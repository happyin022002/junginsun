<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchMaxOrdSubSeqRSQL">
			<desc><![CDATA[BKG_FULL_CGO_RLSE_ORD 테이블의 RLSE_ORD_SEQ의 MAX 값을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(RLSE_ORD_SEQ), 0) AS RLSE_ORD_SEQ
  FROM BKG_FULL_CGO_RLSE_ORD 
 WHERE BKG_NO = @[bkg_no] 
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
