<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMonthlyQuotaPerfIFDateListRSQL">
			<desc><![CDATA[월간 판매목표 조정 단계에서 Perfomance Data 의 IF 날짜를 조회 ]]></desc>
			<sql><![CDATA[
SELECT																
    TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('GMT', TO_DATE(SAQ_PPT_CTNT,'YYYYMMDDHH24MISS') , @[ofcCd] ),'yyyy/mm/dd hh24:mi') CODE,	
    TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('GMT', TO_DATE(SAQ_PPT_CTNT,'YYYYMMDDHH24MISS') , @[ofcCd] ),'yyyy/mm/dd hh24:mi') TEXT	
FROM																
    SAQ_COM_PPT    														
WHERE    															
    SAQ_PPT_CD = '00001'				]]></sql>
			<params>
				<param name="ofcCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
