<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MultiDimensionRPTDBDAOCheckPnlRptItemRSQL">
			<desc><![CDATA[P&L Report Item Insert 전 동일한 Key 를 가진 P&L Report Item 이 있는지 check]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM COA_PFIT_LSS_RPT_ITM
 WHERE STND_COST_CD = @[stnd_cost_cd]
	AND RPT_VW_CD = @[rpt_vw_cd]			]]></sql>
			<params>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
				<param name="rpt_vw_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
