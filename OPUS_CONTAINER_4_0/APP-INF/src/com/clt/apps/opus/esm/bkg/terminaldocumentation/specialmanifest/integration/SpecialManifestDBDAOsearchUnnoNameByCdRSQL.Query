<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchUnnoNameByCdRSQL">
			<desc><![CDATA[UN NO로 NAME을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT ANR_SPCL_TP_ID
FROM BKG_CSTMS_EUR_DG_SPCL
WHERE IMDG_UN_NO = @[frm_imdg_un_no]			]]></sql>
			<params>
				<param name="frm_imdg_un_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
