			<sql><![CDATA[
SELECT C.PCT_BSE_CD, C.CHG_CD, D.CNT
FROM PRI_SCG_PCT_BSE_CHG C,
(SELECT PCT_BSE_CD PCT_BSE_CD, COUNT(PCT_BSE_CD) CNT
FROM PRI_SCG_PCT_BSE_CHG
GROUP BY PCT_BSE_CD) D
WHERE C.PCT_BSE_CD = D.PCT_BSE_CD			]]></sql>
