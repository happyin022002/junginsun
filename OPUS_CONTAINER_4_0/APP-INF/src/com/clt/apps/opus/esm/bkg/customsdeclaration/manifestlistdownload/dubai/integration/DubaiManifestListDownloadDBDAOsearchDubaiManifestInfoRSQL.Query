<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DubaiManifestListDownloadDBDAOsearchDubaiManifestInfoRSQL">
			<desc><![CDATA[DubaiBlInfoVO, DubaiCstmsBlVO]]></desc>
			<sql><![CDATA[
SELECT B.BL_NO
      ,B.POD_CD
      ,B.VSL_CD
      ,B.SKD_VOY_NO
      ,B.SKD_DIR_CD
      ,B.DU_ROTN_NO
      ,B.DU_LINE_ID
      ,B.DU_VOY_AGN_ID
      ,B.ORG_PORT_CD
      ,B.POR_CD
      ,B.POL_CD
      ,B.DEL_CD
      ,B.DU_MF_NO
      ,B.DU_CGO_CD
      ,B.DU_CNTR_SVC_TP_CD
      ,B.DU_TRD_CD
      ,B.DU_TS_MOD_CD
      ,B.CNSL_CGO_FLG
      ,B.ORG_CNT_CD
      ,B.ORG_BL_NO
      ,B.ORG_VSL_CD
      ,B.ORG_SKD_VOY_NO
      ,B.ORG_SKD_DIR_CD
      ,B.VSL_NM
      ,B.MK_NO_CTNT
      ,B.DU_CMDT_CD
      ,B.CMDT_DESC
      ,B.PCK_QTY
      ,B.DU_PCK_DESC
      ,B.DU_PCK_TP_CD
      ,B.CNTR_NO
      ,B.CNTR_KNT
      ,B.BKG_TEU_QTY
      ,B.TARE_WGT
      ,B.CGO_WGT
      ,B.GRS_WGT
      ,B.MEAS_QTY
      ,B.DU_TTL_QTY
      ,B.DU_FRT_WGT
      ,B.PLT_QTY
      ,S.BKG_CUST_TP_CD  AS S_BKG_CUST_TP_CD
      ,S.CUST_CNT_CD     AS S_CUST_CNT_CD
      ,S.CUST_NM         AS S_CUST_NM
      ,S.CUST_ADDR       AS S_CUST_ADDR
      ,S.ORG_CUST_NM     AS S_ORG_CUST_NM
      ,S.ORG_CUST_ADDR   AS S_ORG_CUST_ADDR
      ,C.BKG_CUST_TP_CD  AS C_BKG_CUST_TP_CD
      ,C.DU_CUST_ID      AS C_DU_CUST_ID
      ,C.CUST_NM         AS C_CUST_NM
      ,C.CUST_ADDR       AS C_CUST_ADDR
      ,C.ORG_CUST_NM     AS C_ORG_CUST_NM
      ,C.ORG_CUST_ADDR   AS C_ORG_CUST_ADDR
      ,N.BKG_CUST_TP_CD  AS N_BKG_CUST_TP_CD
      ,N.CUST_NM         AS N_CUST_NM
      ,N.CUST_ADDR       AS N_CUST_ADDR
  FROM BKG_CSTMS_DU_BL B
      ,BKG_CSTMS_DU_CUST S
      ,BKG_CSTMS_DU_CUST C
      ,BKG_CSTMS_DU_CUST N
 WHERE 1 = 1
   AND B.BL_NO = S.BL_NO(+)
   AND B.POD_CD = S.POD_CD(+)
   AND S.BKG_CUST_TP_CD(+) = 'S'
   AND B.BL_NO = C.BL_NO(+)
   AND B.POD_CD = C.POD_CD(+)
   AND C.BKG_CUST_TP_CD(+) = 'C'
   AND B.BL_NO = N.BL_NO(+)
   AND B.POD_CD = N.POD_CD(+)
   AND N.BKG_CUST_TP_CD(+) = 'N'
   AND B.BL_NO = @[bl_no]
   AND B.POD_CD = @[pod_cd]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
