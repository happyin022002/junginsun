<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOcopyChgRateByBkgCSQL">
			<desc><![CDATA[copyChgRateByBkg]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CHG_RT(BKG_NO
        , RT_SEQ
        , DP_SEQ
        , FRT_TERM_CD
        , TRF_ITM_NO
        , CGO_CATE_CD
        , IMDG_CLSS_CD
        , CHG_CD
        , CURR_CD
        , RAT_UT_CD
        , BKG_QTY
        , RAT_AS_QTY
        , CHG_UT_AMT
        , CHG_AMT
        , RCV_TERM_CD
        , DE_TERM_CD
        , N3PTY_RCV_OFC_CD
        , N3PTY_CUST_CNT_CD
        , N3PTY_CUST_SEQ
        , FRT_INCL_XCLD_DIV_CD 
        , INV_STS_CD
        , PRN_HDN_FLG
        , AUTO_RAT_CD
        , APLY_XCH_RTO
        , AGMT_RAT_UT_CD
		, TAX_FLG
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT)
SELECT @[mst_bkg_no] BKG_NO
		,(SELECT /*+index_desc (bkg_chg_rt XPKBKG_CHG_RT)*/
					NVL(SUM(RT_SEQ),0)+CHG.RT_SEQ
					FROM BKG_CHG_RT
					WHERE RT_SEQ >= 0
					AND ROWNUM <= 1
					AND BKG_NO = @[mst_bkg_no]) RT_SEQ
        , DP_SEQ
        , FRT_TERM_CD
        , TRF_ITM_NO
        , CGO_CATE_CD
        , IMDG_CLSS_CD
        , CHG_CD
        , CURR_CD
        , RAT_UT_CD
        , BKG_QTY
        , RAT_AS_QTY
        , CHG_UT_AMT
        , CHG_AMT
        , RCV_TERM_CD
        , DE_TERM_CD
        , N3PTY_RCV_OFC_CD
        , N3PTY_CUST_CNT_CD
        , N3PTY_CUST_SEQ
        , FRT_INCL_XCLD_DIV_CD 
        , INV_STS_CD
        , PRN_HDN_FLG
        , AUTO_RAT_CD
        , APLY_XCH_RTO
        , AGMT_RAT_UT_CD
		, TAX_FLG
        , @[usr_id] CRE_USR_ID
        , SYSDATE
        , @[usr_id] UPD_USR_ID
        , SYSDATE
  FROM BKG_CHG_RT CHG
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="mst_bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
