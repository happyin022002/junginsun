<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchDuplicatedCntrRSQL">
			<desc><![CDATA[컨테이너 중복 여부를 저장한다.]]></desc>
			<sql><![CDATA[
SELECT 'Y'
  FROM BKG_CSTMS_EUR_DG
  WHERE 1=1 
  AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
  AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
  AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
#if (${div} != '')
	#if (${div} == 'cntr')
  AND CNTR_NO = @[cntr_no]
  #if (${cntr} != '')
 			AND	@[cntr_no] != (SELECT CNTR_NO
							   FROM BKG_CSTMS_EUR_DG
							   WHERE 1=1
  							   AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
							   AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
							   AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
							   AND CELL_PSN_NO = LPAD(@[cell_psn_no], 7,0 )
							   AND ROWNUM = 1)
		 #end
  AND CELL_PSN_NO IS NOT NULL
	#elseif (${div} == 'cell_psn')
  AND CELL_PSN_NO = LPAD(@[cell_psn_no], 7,0 )
		#if (${cntr} != '')
 			AND	@[cntr_no] != (SELECT CNTR_NO
							   FROM BKG_CSTMS_EUR_DG
							   WHERE 1=1
  							   AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
							   AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
							   AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
							   AND CELL_PSN_NO = LPAD(@[cell_psn_no], 7,0 )
							   AND ROWNUM = 1)
		 #end
	#end
#end
  AND BL_NO   <> @[bl_no]
  AND EUR_DG_DECL_TP_CD in ('L','T')
  AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cell_psn_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
