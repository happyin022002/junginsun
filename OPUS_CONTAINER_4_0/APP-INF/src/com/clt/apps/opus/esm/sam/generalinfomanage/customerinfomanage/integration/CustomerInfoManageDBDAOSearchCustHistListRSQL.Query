<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAOSearchCustHistListRSQL">
			<desc><![CDATA[SearchCustHistList]]></desc>
			<sql><![CDATA[
SELECT CUST_HIS_SEQ
     , DECODE(cng_itm_cd,0,'Customer Name',1,'Office Code',2,'Status',3,'Sales Rep Code',
              4,'Type',5,'Firm/Private',6,'Location',7,'Registration',
              8,'Key Account',9,'Group Customer',10,'Multi Trade Account',
              11,'Creator',12,'Office Name',13,'Sales Rep Name',14,'Phone No',
              15,'Fax No',16,'Email',17,'Address',18,'User Name',19,'User ID','N/A'
      ) AS CNG_ITM_CD
     , OLD_VAL_DESC
     , NEW_VAL_DESC
     , UPD_DT
FROM SAM_CUST_HIS
WHERE CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))
ORDER BY CUST_HIS_SEQ DESC			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
