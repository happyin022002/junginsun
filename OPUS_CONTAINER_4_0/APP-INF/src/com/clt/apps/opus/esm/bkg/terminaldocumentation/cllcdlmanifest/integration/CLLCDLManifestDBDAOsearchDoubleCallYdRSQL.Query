<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchDoubleCallYdRSQL">
			<desc><![CDATA[searchDoubleCallYd]]></desc>
			<sql><![CDATA[
SELECT	COUNT(*) YD_COUNT
FROM	
	(	
	SELECT	YD_CD 
	FROM	vsk_vsl_port_skd
	WHERE	vsl_cd		= @[vsl_cd]
	AND	skd_voy_no	= @[skd_voy_no]
	AND	skd_dir_cd	= @[skd_dir_cd]
	AND	vps_port_cd	= @[in_pol_cd]
	) A,
	(	
	SELECT	YD_CD 
	FROM	vsk_vsl_port_skd
	WHERE	vsl_cd		= @[vsl_cd2]
	AND	skd_voy_no	= @[skd_voy_no2]
	AND	skd_dir_cd	= @[skd_dir_cd2]
	AND	vps_port_cd	= @[in_pol_cd]
	) B
WHERE A.YD_CD = B.YD_CD			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="vsl_cd2" type="12" value="" out="N"/>
				<param name="skd_voy_no2" type="12" value="" out="N"/>
				<param name="skd_dir_cd2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
