<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsManifestListDownloadDBDAOmodifySelectBkgCstmsAnrBl2USQL">
			<desc><![CDATA[u]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ANR_BL_LOG A
SET VVD_SEQ = (SELECT VVD_SEQ
               FROM BKG_CSTMS_ANR_BL B
               WHERE A.BL_NO = B.BL_NO
               AND B.VSL_CD = substr(@[vvd],1,4)
			   AND B.SKD_VOY_NO = substr(@[vvd],5,4)
			   AND B.SKD_DIR_CD = substr(@[vvd],9,1)
               AND B.BKG_NO = @[bkg_no])
WHERE 1=1			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
