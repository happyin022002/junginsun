<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsCustomsTransmissionDBDAOmodifyCntrAnrMsgStsCdUSQL">
			<desc><![CDATA[U]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ANR_CNTR
SET
ANR_MSG_STS_CD = null
WHERE 1=1
AND BKG_NO = @[bkg_no]
AND VSL_CD     = SUBSTR(@[vvd],1,4)
AND SKD_VOY_NO = SUBSTR(@[vvd],5,4)
AND SKD_DIR_CD = SUBSTR(@[vvd],9,1)
#if (${cntr_chk} != '')
   AND CNTR_NO IN (
#foreach($cntrNo IN ${cntr_no})  
'$cntrNo',  
#end  
'')
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
