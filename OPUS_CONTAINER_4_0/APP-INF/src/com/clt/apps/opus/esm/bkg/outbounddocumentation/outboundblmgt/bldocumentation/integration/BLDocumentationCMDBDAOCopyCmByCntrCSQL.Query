<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCopyCmByCntrCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CNTR_MF_DESC
            (BKG_NO
,            CNTR_MF_SEQ
,            CNTR_NO
,            CMDT_HS_CD
,            PCK_QTY
,            PCK_TP_CD
,            CNTR_MF_WGT
,            WGT_UT_CD
,            MEAS_QTY
,            MEAS_UT_CD
,            DCGO_FLG
,            BB_CGO_FLG
,            AWK_CGO_FLG
,            RC_FLG
,            RD_CGO_FLG
,            HNGR_FLG
,            CNTR_MF_MK_DESC
,            CNTR_MF_GDS_DESC
,            HBL_SEQ
,            HAMO_TRF_CD
,            NCM_NO
,            PO_NO
,            CNTR_MF_NO
,            CSTMS_DECL_NO
,            CRE_USR_ID
,            CRE_DT
,            UPD_USR_ID
,            UPD_DT)
#if (${ca_flg} == 'Y') 
SELECT @[tgt_bkg_no] BKG_NO 
,      ((SELECT NVL(SUM(CNTR_MF_SEQ), 0)
         FROM   BKG_CNTR_MF_DESC_HIS
         WHERE  BKG_NO = @[tgt_bkg_no] AND CNTR_NO=@[cntr_no]) + ROW_NUMBER() OVER(PARTITION BY BKG_NO, CNTR_NO ORDER BY CNTR_MF_SEQ))
,      CNTR_NO
,      CMDT_HS_CD
,      PCK_QTY
,      PCK_TP_CD
,      CNTR_MF_WGT
,      WGT_UT_CD
,      MEAS_QTY
,      MEAS_UT_CD
,      DCGO_FLG
,      BB_CGO_FLG
,      AWK_CGO_FLG
,      RC_FLG
,      RD_CGO_FLG
,      HNGR_FLG
,      CNTR_MF_MK_DESC
,      CNTR_MF_GDS_DESC
,      HBL_SEQ
,      HAMO_TRF_CD
,      NCM_NO
,      PO_NO
,      '' CNTR_MF_NO
,      CSTMS_DECL_NO
,      @[cre_usr_id]
,      SYSDATE
,      @[cre_usr_id]
,      SYSDATE
FROM   BKG_CNTR_MF_DESC_HIS
WHERE  BKG_NO = @[src_bkg_no]
AND    CORR_NO = 'TMP0000001'
AND    CNTR_NO = @[cntr_no]
#else 
SELECT @[tgt_bkg_no] BKG_NO 
,      ((SELECT NVL(SUM(CNTR_MF_SEQ), 0)
         FROM   BKG_CNTR_MF_DESC
         WHERE  BKG_NO=@[tgt_bkg_no] AND CNTR_NO=@[cntr_no]) + ROW_NUMBER() OVER(PARTITION BY BKG_NO, CNTR_NO ORDER BY CNTR_MF_SEQ))
,      CNTR_NO
,      CMDT_HS_CD
,      PCK_QTY
,      PCK_TP_CD
,      CNTR_MF_WGT
,      WGT_UT_CD
,      MEAS_QTY
,      MEAS_UT_CD
,      DCGO_FLG
,      BB_CGO_FLG
,      AWK_CGO_FLG
,      RC_FLG
,      RD_CGO_FLG
,      HNGR_FLG
,      CNTR_MF_MK_DESC
,      CNTR_MF_GDS_DESC
,      HBL_SEQ
,      HAMO_TRF_CD
,      NCM_NO
,      PO_NO
,      '' CNTR_MF_NO
,      CSTMS_DECL_NO
,      @[cre_usr_id]
,      SYSDATE
,      @[cre_usr_id]
,      SYSDATE
FROM   BKG_CNTR_MF_DESC
WHERE  BKG_NO = @[src_bkg_no]
AND    CNTR_NO = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="tgt_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="src_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
