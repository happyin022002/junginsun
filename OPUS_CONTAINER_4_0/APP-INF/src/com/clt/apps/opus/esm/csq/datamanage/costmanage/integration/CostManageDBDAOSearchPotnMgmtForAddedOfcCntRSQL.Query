<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostManageDBDAOSearchPotnMgmtForAddedOfcCntRSQL">
			<desc><![CDATA[Freezing 되기 전이면서 Portion 데이터는 존재하는지 확인]]></desc>
			<sql><![CDATA[
SELECT NVL((SELECT COUNT(*) CNT
          FROM CSQ_QTA_STEP_VER
         WHERE BSE_TP_CD  = @[bse_tp_cd]
           AND BSE_YR     = @[bse_yr]
           AND BSE_QTR_CD = DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
           and QTA_STEP_CD in ('03', '04', '05')
           AND ROWNUM     = 1
        ), 0) AS CNF_CNT
       ,NVL((SELECT COUNT(*) CNT
          FROM CSQ_QTA_RLSE_VER
         WHERE BSE_TP_CD  = @[bse_tp_cd]
           AND BSE_YR     = @[bse_yr]
           AND BSE_QTR_CD = DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
           AND CSQ_VER_STS_CD = 'R'
           AND ROWNUM     = 1
        ), 0) AS FRZ_CNT
  FROM DUAL  			]]></sql>
			<params>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
