<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCPrefixDBDAOPriScPfxMapgVORSQL">
			<desc><![CDATA[Select]]></desc>
			<sql><![CDATA[
SELECT A.SVC_SCP_CD
      ,B.SVC_SCP_NM
      ,A.SCONTI_CD
      ,C.SCONTI_NM
      ,A.SC_PFX_CD
      ,D.SC_PFX_DESC
      ,A.CRE_USR_ID
      ,A.CRE_DT
      ,A.UPD_USR_ID
      ,A.UPD_DT
FROM PRI_SC_PFX_MAPG A
    ,MDM_SVC_SCP B
    ,MDM_SUBCONTINENT C
    ,PRI_SC_PFX D
WHERE A.SVC_SCP_CD = B.SVC_SCP_CD
AND   A.SCONTI_CD = C.SCONTI_CD
AND   A.SC_PFX_CD = D.SC_PFX_CD
ORDER BY A.SVC_SCP_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
