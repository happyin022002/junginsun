<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMonthlyQuotaStepVersionListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT MQTA_VER_NO||':'||GLINE_VER_NO AS CODE,                    
       SUBSTR(MQTA_VER_NO, 1, LENGTH(MQTA_VER_NO)-2)              
           ||'-'||SUBSTR(MQTA_VER_NO, LENGTH(MQTA_VER_NO)-1)      
           ||DECODE(SAQ_STS_CD, '00', '', '-'||SAQ_STS_CD) AS TEXT
FROM SAQ_MON_QTA_STEP_VER                                         
WHERE MQTA_STEP_CD = @[mQtaStepCd]                                            
AND   BSE_YR = @[year]                                                  
AND   BSE_QTR_CD = @[bse_qtr_cd]                                                 
AND   TRD_CD = @[trade]                                                  
AND   DIR_CD = @[dirCd]                                                  
AND   CRE_OFC_CD = @[ofcCd]                                              
AND   ('ALL' = @[searchFlag] OR SAQ_STS_CD IN ('FN', 'QN', 'QF'))             
ORDER BY MQTA_VER_NO DESC  				]]></sql>
			<params>
				<param name="mQtaStepCd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="dirCd" type="12" value="" out="N"/>
				<param name="ofcCd" type="12" value="" out="N"/>
				<param name="searchFlag" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
