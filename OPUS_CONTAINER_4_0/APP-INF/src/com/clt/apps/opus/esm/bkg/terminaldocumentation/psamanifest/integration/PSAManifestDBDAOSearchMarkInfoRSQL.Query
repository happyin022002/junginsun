<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOSearchMarkInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT    MD.MK_DESC MARK1
FROM      BKG_BL_MK_DESC MD
WHERE     1 = 1
AND       MD.BKG_NO = @[bkg_no]
AND       MD.MK_DESC IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
