<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchPortCutOffRSQL">
			<desc><![CDATA[SearchPortCutOff]]></desc>
			<sql><![CDATA[
SELECT
		BKG_NO
FROM	BKG_CLZ_TM
WHERE 	0=0
AND		BKG_NO = @[bkg_no]
AND		CLZ_TP_CD = 'T'
#if (${mnl_set_dt} == '')
AND (	MNL_SET_DT IS NULL
	OR	SYS_SET_DT = MNL_SET_DT
	)
#else
AND (	MNL_SET_DT = TO_DATE(@[mnl_set_dt],'YYYYMMDDHH24MISS')
	OR	SYS_SET_DT = TO_DATE(@[mnl_set_dt],'YYYYMMDDHH24MISS')
	)
#end 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="mnl_set_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
