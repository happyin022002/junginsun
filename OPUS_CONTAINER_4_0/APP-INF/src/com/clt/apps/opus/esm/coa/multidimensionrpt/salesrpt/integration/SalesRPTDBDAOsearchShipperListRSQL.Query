<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesRPTDBDAOsearchShipperListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT	CUST_CNT_CD
       ,CUST_SEQ
       ,MODI_CUST_CNT_CD
       ,MODI_CUST_SEQ
       ,CUST_LGL_ENG_NM
       ,CNTR_CUST_TP_CD
       ,OFC_CD 
FROM	MDM_CUSTOMER 
WHERE	DELT_FLG    <> 'Y' 
AND		CUST_CNT_CD = @[f_cust_cnt_cd] 
#if(${f_cust_seq} != '' )
AND		CUST_SEQ    = TO_NUMBER( @[f_cust_seq] )
#end
ORDER	BY CUST_CNT_CD, CUST_SEQ			]]></sql>
			<params>
				<param name="f_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="f_cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
