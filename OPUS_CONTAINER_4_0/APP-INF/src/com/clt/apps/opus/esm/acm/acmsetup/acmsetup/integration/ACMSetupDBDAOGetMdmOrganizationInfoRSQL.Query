<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSetupDBDAOGetMdmOrganizationInfoRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
  FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[agn_cd]
   AND NVL(DELT_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="agn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
