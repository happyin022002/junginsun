<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOCreateRefCACSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOCreateRefCACSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_REFERENCE (
    BKG_NO 
#else
INSERT INTO BKG_REF_HIS (
    BKG_NO 
    , CORR_NO 
#end
    , REF_SEQ 
    , BKG_REF_TP_CD 
    , CUST_REF_NO_CTNT 
    , CNTR_NO 
    , CNTR_MF_SEQ 
    , CPY_DESC_FLG 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
	, REF_NO
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
        , REF_SEQ 
        , BKG_REF_TP_CD 
        , CUST_REF_NO_CTNT 
        , CNTR_NO 
        , CNTR_MF_SEQ 
        , CPY_DESC_FLG 
        , CRE_USR_ID 
        , CRE_DT 
        , UPD_USR_ID 
        , sysdate
		, REF_NO
#if (${copy_type_cd} == 'BKG')
  FROM BKG_REF_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_REFERENCE
 WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
