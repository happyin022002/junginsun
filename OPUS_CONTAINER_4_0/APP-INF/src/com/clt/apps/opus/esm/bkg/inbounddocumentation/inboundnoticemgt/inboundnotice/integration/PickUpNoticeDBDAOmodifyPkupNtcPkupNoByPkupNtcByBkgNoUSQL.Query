<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOmodifyPkupNtcPkupNoByPkupNtcByBkgNoUSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
/* 삭제됨!!
UPDATE BKG_PKUP_NTC_PKUP_NO X
SET
       (PKUP_NO, PKUP_AVAL_DT, LST_FREE_DT, PKUP_YD_CD, RTN_YD_CD)
       =
       (SELECT PKUP_NO, PKUP_AVAL_DT, LST_FREE_DT, PKUP_YD_CD, RTN_YD_CD      
          FROM BKG_PKUP_NTC  A
              ,BKG_BOOKING   B
              ,MDM_LOCATION  C      
         WHERE A.BKG_NO  = @[bkg_no]
           AND A.NTC_SEQ = @[ntc_seq]
           AND B.BKG_NO  = A.BKG_NO
           AND B.DEL_CD  = C.LOC_CD
           AND X.BKG_NO  = A.BKG_NO
           AND X.CNTR_NO = A.CNTR_NO
           AND X.OFC_CD  = C.EQ_CTRL_OFC_CD
       )
       ,PKUP_UPD_USR_ID = @[usr_id]
       ,PKUP_UPD_DT     = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(OFC_CD)
       ,UPD_USR_ID      = @[usr_id]
       ,UPD_DT          = SYSDATE
WHERE  EXISTS
       (
        SELECT A.BKG_NO
          FROM BKG_PKUP_NTC  A
              ,BKG_BOOKING   B
              ,MDM_LOCATION  C      
         WHERE A.BKG_NO  = @[bkg_no]
           AND A.NTC_SEQ = @[ntc_seq]
           AND B.BKG_NO  = A.BKG_NO
           AND B.DEL_CD  = C.LOC_CD
           AND X.BKG_NO  = A.BKG_NO
           AND X.CNTR_NO = A.CNTR_NO
           AND X.OFC_CD  = C.EQ_CTRL_OFC_CD
       )
  AND DELT_FLG = 'N'
*/			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ntc_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
