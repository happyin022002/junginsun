<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGOHChargeGuidelineDBDAOPriSgGohChgVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX(A XPKPRI_SG_GOH_CHG ) */
      A.SVC_SCP_CD
     ,A.GLINE_SEQ
     ,A.GOH_CHG_SEQ
     ,A.ROUT_PNT_LOC_TP_CD
     ,A.ROUT_PNT_LOC_DEF_CD
     ,(DECODE (LENGTH(ROUT_PNT_LOC_DEF_CD),'2'
        ,(SELECT CNT_NM
          FROM MDM_COUNTRY 
          WHERE CNT_CD = A.ROUT_PNT_LOC_DEF_CD )
        ,(SELECT LOC_NM 
          FROM MDM_LOCATION 
          WHERE LOC_CD = A.ROUT_PNT_LOC_DEF_CD ))) AS LOC_DES
     ,A.RAT_UT_CD
     ,A.PRC_HNGR_BAR_TP_CD
     ,A.CURR_CD
     ,A.FRT_RT_AMT
     ,CASE A.PRC_HNGR_BAR_TP_CD
         WHEN 'S' THEN 1
         WHEN 'D' THEN 2
         WHEN 'T' THEN 3
      END BAR_ORD
     ,CASE A.RAT_UT_CD
         WHEN 'D2' THEN 1
         WHEN 'D4' THEN 2
         WHEN 'D5' THEN 3
         ELSE 4
      END PER_ORD
FROM PRI_SG_GOH_CHG A
WHERE SVC_SCP_CD  = @[svc_scp_cd]
AND   GLINE_SEQ   = @[gline_seq]
ORDER BY A.ROUT_PNT_LOC_TP_CD
        ,A.ROUT_PNT_LOC_DEF_CD
        ,BAR_ORD
        ,PER_ORD
        ,A.RAT_UT_CD
        ,A.CURR_CD			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
