<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOAddEdoMstCSQL">
			<desc><![CDATA[EDI 수신 정보를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_EDO_MST (
	EDO_RQST_NO
,	EDO_RQST_SEQ
,	EDO_TP_CD
,	EDO_FUNC_CD
,	BKG_NO
,	BL_NO
,	MF_SEQ_NO
,	EDO_VSL_NM
,	EDO_SKD_VOY_NO
,	EDO_SKD_DIR_CD
,	POD_CD
,	VSL_ARR_DT
,	EDO_RCT_DT
,	EDO_RCT_LOC_CD
,	EDO_ACK_CD
,	EDO_ACK_DT
,	EDO_ACK_USR_ID
,	DIFF_RMK
,   DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[edo_rqst_no]
,	@[edo_rqst_seq]
,	@[edo_tp_cd]
,	@[edo_func_cd]
,	(SELECT BKG_NO FROM BKG_BOOKING WHERE BL_NO = @[bl_no])
,	@[bl_no]
,	@[mf_seq_no]
,	@[edo_vsl_nm]
,	@[edo_skd_voy_no]
,	@[edo_skd_dir_cd]
,	@[pod_cd]
,	TO_DATE(@[vsl_arr_dt],'YYYYMMDD')
,	TO_DATE(@[edo_rct_dt],'YYYYMMDDHH24MISS')
,	@[edo_rct_loc_cd]
,	@[edo_ack_cd]
,	TO_DATE(@[edo_ack_dt],'YYYYMMDDHH24MISS')
,	@[edo_ack_usr_id]
,	@[diff_rmk]
,   DECODE( SUBSTR(@[edo_rqst_no],3,4),'COMU','Y','N' )
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="edo_rqst_no" type="12" value="" out="N"/>
				<param name="edo_rqst_seq" type="12" value="" out="N"/>
				<param name="edo_tp_cd" type="12" value="" out="N"/>
				<param name="edo_func_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="mf_seq_no" type="12" value="" out="N"/>
				<param name="edo_vsl_nm" type="12" value="" out="N"/>
				<param name="edo_skd_voy_no" type="12" value="" out="N"/>
				<param name="edo_skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="vsl_arr_dt" type="12" value="" out="N"/>
				<param name="edo_rct_dt" type="12" value="" out="N"/>
				<param name="edo_rct_loc_cd" type="12" value="" out="N"/>
				<param name="edo_ack_cd" type="12" value="" out="N"/>
				<param name="edo_ack_dt" type="12" value="" out="N"/>
				<param name="edo_ack_usr_id" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
