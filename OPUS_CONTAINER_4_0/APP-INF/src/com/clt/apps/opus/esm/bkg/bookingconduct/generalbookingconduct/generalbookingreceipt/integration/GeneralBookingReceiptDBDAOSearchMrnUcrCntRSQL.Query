<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchMrnUcrCntRSQL">
			<desc><![CDATA[MRN, UCR count]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(DECODE(BKG_REF_TP_CD,@[mrn_tp_cd], 1, 0)),0) AS MRN_CNT
    ,NVL(SUM(DECODE(BKG_REF_TP_CD,@[ucr_tp_cd], 1, 0)),0) AS UCR_CNT
FROM BKG_REFERENCE
WHERE BKG_NO = @[bkg_no]
AND BKG_REF_TP_CD IN ( @[mrn_tp_cd], @[ucr_tp_cd] )
AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="mrn_tp_cd" type="12" value="" out="N"/>
				<param name="ucr_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
