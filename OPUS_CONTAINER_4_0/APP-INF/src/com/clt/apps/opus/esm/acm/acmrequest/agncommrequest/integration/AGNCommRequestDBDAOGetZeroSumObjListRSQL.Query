<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAOGetZeroSumObjListRSQL">
			<desc><![CDATA[ACM_AGN_COMM에서 bkgNo로 데이터를 구한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_NO,
       AGN_CD,
       IO_BND_CD,
       AC_TP_CD,
       @[usr_id] AS USR_ID
  FROM ACM_AGN_COMM
 WHERE BKG_NO = @[bkg_no]
 GROUP BY BKG_NO, AGN_CD, IO_BND_CD, AC_TP_CD			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
