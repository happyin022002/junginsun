<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOModifyChgRateBkgRateRSQL">
			<desc><![CDATA[ModifyChgRateBkgRate]]></desc>
			<sql><![CDATA[
#if (${caflag} == 'Y') 

SELECT 
	COUNT(BKG_NO)
FROM BKG_RT_HIS
WHERE BKG_NO= @[bkg_no]
AND  CORR_NO ='TMP0000001'

#else 

SELECT 
	COUNT(BKG_NO)
FROM BKG_RATE
WHERE BKG_NO= @[bkg_no]

#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
