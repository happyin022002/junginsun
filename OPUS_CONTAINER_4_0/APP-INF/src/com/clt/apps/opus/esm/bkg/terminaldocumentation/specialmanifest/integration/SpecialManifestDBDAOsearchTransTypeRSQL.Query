<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchTransTypeRSQL">
			<desc><![CDATA[Decl 기본정보를 조회한다. ]]></desc>
			<sql><![CDATA[
SELECT 'Y'
FROM (
SELECT ACK_RCV_STS_CD
FROM BKG_CSTMS_EUR_DG_RCV
WHERE 1=1
AND EUR_EDI_MSG_TP_ID = 'IFD'
AND MSG_RCV_NO IN 
(
SELECT MSG_SND_NO
FROM BKG_CSTMS_EUR_DG_SND
WHERE 1=1
AND EUR_EDI_MSG_TP_ID = 'IFD'
AND   EUR_DG_DECL_TP_CD = @[d_type]
AND   VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
AND   SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
AND   SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
AND   PORT_CD     = @[port_cd]
) ) A
WHERE A.ACK_RCV_STS_CD = 'A'
AND ROWNUM =1			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
