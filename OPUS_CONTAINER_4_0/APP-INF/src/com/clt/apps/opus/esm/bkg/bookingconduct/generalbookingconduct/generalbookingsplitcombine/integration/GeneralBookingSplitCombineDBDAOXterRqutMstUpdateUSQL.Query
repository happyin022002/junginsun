<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSplitCombineDBDAOXterRqutMstUpdateUSQL">
			<desc><![CDATA[GeneralBookingSplitCombineDBDAOXterRqutMstUpdateUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_RQST_MST MST
SET MST.BKG_NO = @[taget_bkg_no]
, MST.BL_NO_CTNT = @[taget_bkg_no]
    , MST.CRE_USR_ID = 'SPLIT'
	, dpcs_split_no = 'L' 
WHERE MST.BKG_NO = @[bkg_no]
AND MST.DOC_TP_CD = 'S'
AND MST.XTER_RQST_VIA_CD = 'WEB'
AND MST.CRE_USR_ID = 'HOM1'
AND MST.SPLIT_STS_CD = 'S'
AND MST.BL_SPLIT_NO = @[bl_split_no]			]]></sql>
			<params>
				<param name="taget_bkg_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
