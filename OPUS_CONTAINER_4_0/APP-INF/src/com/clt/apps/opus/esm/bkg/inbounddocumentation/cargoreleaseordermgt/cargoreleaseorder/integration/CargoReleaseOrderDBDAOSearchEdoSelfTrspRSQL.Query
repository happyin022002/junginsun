<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchEdoSelfTrspRSQL">
			<desc><![CDATA[UI_BKG_0136 화면에서 사용하는 SQL문]]></desc>
			<sql><![CDATA[
SELECT A.EDO_RQST_NO,
       A.EDO_RQST_SEQ,
       DECODE(A.OWN_TRSP_CD,'A','A:자선운송','S','S:자가운송') AS OWN_TRSP_CD,
       A.EDO_ISS_DT,
       A.BD_DEP_AREA_NO,
       B.VNDR_NM          AS BD_DEP_AREA_VNDR_NM,
       A.BD_ARR_AREA_NO,
       A.BD_ARR_AREA_NM,
       C.VNDR_NM          AS BD_ARR_AREA_VNDR_NM,
       A.GDS_DESC1,
       A.GDS_DESC2,
       A.GDS_DESC3,
       A.GDS_DESC4,
       A.MISC_DESC,
       A.CRE_USR_ID,
       A.CRE_DT,
       A.UPD_USR_ID,
       A.UPD_DT
  FROM BKG_EDO_SELF_TRSP  A,
       BKG_EDO_VNDR       B,
       BKG_EDO_VNDR       C,
       BKG_EDO_MST        D
 WHERE A.EDO_RQST_NO      = @[rqstNo]
   AND A.EDO_RQST_SEQ     = (SELECT MAX(EDO_RQST_SEQ)
                               FROM BKG_EDO_MST
                              WHERE EDO_RQST_NO    = @[rqstNo]
                                AND EDO_TP_CD      = @[tpCd])
   AND A.BD_DEP_AREA_NO   = B.VNDR_NO (+)
   AND B.VNDR_CLSS_CD (+) = '2'
   AND A.BD_ARR_AREA_NO   = C.VNDR_NO (+)
   AND C.VNDR_CLSS_CD (+) = '2'
   AND A.EDO_RQST_NO      = D.EDO_RQST_NO
   AND A.EDO_RQST_SEQ     = D.EDO_RQST_SEQ
   AND D.EDO_TP_CD        = @[tpCd]			]]></sql>
			<params>
				<param name="rqstNo" type="12" value="" out="N"/>
				<param name="tpCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
