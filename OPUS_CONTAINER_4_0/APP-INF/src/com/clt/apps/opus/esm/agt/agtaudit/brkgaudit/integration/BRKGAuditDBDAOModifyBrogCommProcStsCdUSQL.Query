		<query name="BRKGAuditDBDAOModifyBrogCommProcStsCdUSQL">
			<desc><![CDATA[ModifyBrogCommProcStsCd]]></desc>
			<sql><![CDATA[
UPDATE AGT_BROG_COMM A
      SET A.COMM_PROC_STS_CD  = 'IF',
          A.COMM_PROC_RSLT_RSN = 'Interface OK!',
          A.BROG_IF_DT          = SYSDATE
    WHERE A.VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
