<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOupdatePartialBkgInfoForScheduleUSQL">
			<desc><![CDATA[updatePartialBkgInfoForSchedule]]></desc>
			<sql><![CDATA[
UPDATE BKG_TML_EDI_JP_BL SET
       CNTR_VOL_QTY1 = @[cntr_vol_qty1]
      ,CNTR_VOL_QTY2 = @[cntr_vol_qty2]
      ,CNTR_VOL_QTY3 = @[cntr_vol_qty3]
      ,CNTR_VOL_QTY4 = @[cntr_vol_qty4]
      ,CNTR_VOL_QTY5 = @[cntr_vol_qty5]
      ,SNACCS_TML_EDI_STS_CD = @[bkrbkc]
      ,SNACCS_TML_EDI_STS_CNG_FLG = @[snaccs_tml_edi_sts_cng_flg]
      ,EDI_SND_OFC_CD = @[edi_snd_ofc_cd]
      ,EDI_SND_USR_ID = @[upd_usr_id]
      ,UPD_USR_ID    = @[upd_usr_id]
      ,UPD_DT = SYSDATE
 WHERE BKG_NO = @[bkg_no]
   AND BKG_SKD_SEQ = 0			]]></sql>
			<params>
				<param name="cntr_vol_qty1" type="12" value="" out="N"/>
				<param name="cntr_vol_qty2" type="12" value="" out="N"/>
				<param name="cntr_vol_qty3" type="12" value="" out="N"/>
				<param name="cntr_vol_qty4" type="12" value="" out="N"/>
				<param name="cntr_vol_qty5" type="12" value="" out="N"/>
				<param name="bkrbkc" type="12" value="" out="N"/>
				<param name="snaccs_tml_edi_sts_cng_flg" type="12" value="" out="N"/>
				<param name="edi_snd_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
