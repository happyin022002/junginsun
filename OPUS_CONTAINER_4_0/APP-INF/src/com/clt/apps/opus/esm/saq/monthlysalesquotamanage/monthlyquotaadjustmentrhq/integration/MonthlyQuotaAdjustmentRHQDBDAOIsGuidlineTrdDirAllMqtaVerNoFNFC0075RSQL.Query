<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentRHQDBDAOIsGuidlineTrdDirAllMqtaVerNoFNFC0075RSQL">
			<desc><![CDATA[gline_ver_no 에 관련된 모든 STEP의 mqta_ver_no 가 마지막 단계이면 TRUE, 아니면 FALSE 리턴.]]></desc>
			<sql><![CDATA[
SELECT MIN(DECODE(MQTA_STEP_CD, '01',
                DECODE(COUNT(DISTINCT TRD_CD||DIR_CD||CRE_OFC_CD),
                        SUM(DECODE(SVR.SAQ_STS_CD, 'FN', 1, 0)), '0', '1'))) + -- step01,
       MIN(DECODE(MQTA_STEP_CD, '02',
                DECODE(COUNT(DISTINCT TRD_CD||DIR_CD||CRE_OFC_CD),
                        SUM(DECODE(SVR.SAQ_STS_CD, 'FC', 1, 0)), '0', '1'))) + -- step02,
       MIN(DECODE(MQTA_STEP_CD, '04',
                DECODE(COUNT(DISTINCT TRD_CD||DIR_CD||CRE_OFC_CD),
                        SUM(DECODE(SVR.SAQ_STS_CD, 'FN', 1, 0)), '0', '1'))) RET -- step04,
FROM SAQ_MON_QTA_STEP_VER SVR
WHERE SVR.BSE_YR = @[year]
AND   SVR.BSE_QTR_CD = @[bse_quarter]
AND   SVR.GLINE_VER_NO = @[glineVerNo]
AND   SVR.TRD_CD = @[trade]
AND   SVR.DIR_CD = @[bound]
AND   SVR.SAQ_STS_CD <> 'XX'
GROUP BY MQTA_STEP_CD			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_quarter" type="12" value="" out="N"/>
				<param name="glineVerNo" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
