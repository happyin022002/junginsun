<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOBkgCMCroChkListinVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.BKG_NO
      ,A.BL_NO
      ,A.USA_CSTMS_FILE_CD
      ,A.CND_CSTMS_FILE_CD
      ,B.OBL_ISS_OFC_CD
      ,B.OBL_ISS_USR_ID      
      ,DECODE(NVL(D.CUST_NM,'N'),'N','N','Y')            AS CUST_NM_S
      ,DECODE(NVL(D.CUST_ADDR,'N'),'N','N','Y')          AS CUST_ADDR_S
      ,DECODE(NVL(D.CUST_CTY_NM,'N'),'N','N','Y')        AS CUST_CTY_NM_S
      ,DECODE(NVL(D.CSTMS_DECL_CNT_CD,'N'),'N','N','Y')  AS CSTMS_DECL_CNT_CD_S
      ,DECODE(NVL(D.CUST_ZIP_ID,'N'),'N','N','Y')        AS CUST_ZIP_ID_S
      ,DECODE(NVL(E.CUST_NM,'N'),'N','N','Y')            AS CUST_NM_C
      ,DECODE(NVL(E.CUST_ADDR,'N'),'N','N','Y')          AS CUST_ADDR_C
      ,DECODE(NVL(E.CUST_CTY_NM,'N'),'N','N','Y')        AS CUST_CTY_NM_C
      ,DECODE(NVL(E.CSTMS_DECL_CNT_CD,'N'),'N','N','Y')  AS CSTMS_DECL_CNT_CD_C
	  ,DECODE(NVL(E.CUST_STE_CD,'N'),'N','N','Y')		 AS CUST_STE_CD_C
      ,DECODE(NVL(E.CUST_ZIP_ID,'N'),'N','N','Y')        AS CUST_ZIP_ID_C
      ,DECODE(NVL(F.CUST_NM,'N'),'N','N','Y')            AS CUST_NM_N
      ,DECODE(NVL(F.CUST_ADDR,'N'),'N','N','Y')          AS CUST_ADDR_N
      ,DECODE(NVL(F.CUST_CTY_NM,'N'),'N','N','Y')        AS CUST_CTY_NM_N
      ,DECODE(NVL(F.CSTMS_DECL_CNT_CD,'N'),'N','N','Y')  AS CSTMS_DECL_CNT_CD_N
	  ,DECODE(NVL(F.CUST_STE_CD,'N'),'N','N','Y')		 AS CUST_STE_CD_N
      ,DECODE(NVL(F.CUST_ZIP_ID,'N'),'N','N','Y')        AS CUST_ZIP_ID_N
      ,C.PCK_QTY                                         AS PCK_QTY_DA
      ,C.ACT_WGT            
      ,C.MEAS_QTY                                        AS MEAS_QTY_DA
      ,DECODE(C.PCK_QTY,G.PCK_QTY,'Y','N')               AS PCK_QTY_CHK
      ,DECODE(C.ACT_WGT,G.CNTR_MF_WGT,'Y','N')           AS ACT_WET_CHK
      ,DECODE(C.MEAS_QTY,G.MEAS_QTY,'Y','N')             AS MEAS_QTY_CHK
      ,G.PCK_QTY                                         AS PCK_QTY_CM
      ,G.CNTR_MF_WGT
      ,G.MEAS_QTY                                        AS MEAS_QTY_CM
      ,DECODE(NVL(G.CNTR_MF_MK_DESC,'N'),'N','N','Y')    AS CNTR_MF_MK_DESC
      ,DECODE(NVL(G.CNTR_MF_GDS_DESC,'N'),'N','N','Y')   AS CNTR_MF_GDS_DESC
      ,H.CNTR_NO
      ,H.PCK_QTY                                         AS PCK_QTY_CO
      ,H.CNTR_WGT
      ,H.MEAS_QTY                                        AS MEAS_QTY_CO
      ,DECODE(NVL(I.CNTR_SEAL_SEQ,''),'','N','Y')        AS CNTR_SEAL_SEQ
	  ,'' VSL_CD
	  ,'' SKD_VOY_NO
	  ,'' SKD_DIR_CD
	  ,'' POL_CD
	  ,'' POL_YD_CD
	  ,'' POD_CD
	  ,'' POD_YD_CD
	  ,'' CUST_CNT_CD
	  ,'' CUST_SEQ
	  ,'' CHK_ERR
	  ,'' BKG_OFC_CD
	  ,'' CRE_USR_ID
	  ,'' OB_SREP_CD
	  ,'' TAB_ITEM
	  ,'' B_BL_NO             
	  ,'' B_USA_CSTMS_FILE_NO 
	  ,'' B_HBL_NO            
	  ,'' B_CUST_NM_S         
	  ,'' B_CUST_ADDR_S       
	  ,'' B_CUST_NM_C         
	  ,'' B_CUST_ADDR_C       
	  ,'' B_CUST_NM_N         
	  ,'' B_CUST_ADDR_N       
	  ,'' B_PCK_QTY_DA        
	  ,'' B_HBL_WGT_DA        
	  ,'' B_MEAS_QTY_DA       
	  ,'' B_PCK_QTY_CHK       
	  ,'' B_HBL_WET_CHK       
	  ,'' B_MEAS_QTY_CHK      
	  ,'' B_PCK_QTY_CM        
	  ,'' B_CNTR_WGT_CM       
	  ,'' B_MEAS_QTY_CM       
	  ,'' B_CNTR_MF_MK_DESC   
	  ,'' B_CNTR_MF_GDS_DESC  
	  ,'' B_CNTR_MF_NO        
	  ,'' B_CNTR_NO           
	  ,'' B_CNTR_SEAL_NO      
	  ,'' B_PCK_QTY_CO        
	  ,'' B_CNTR_WGT_CO       
	  ,'' B_MEAS_QTY_CO 
  FROM BKG_BOOKING      A
      ,BKG_BL_ISS       B
      ,BKG_BL_DOC       C
      ,BKG_CUSTOMER     D
      ,BKG_CUSTOMER     E
      ,BKG_CUSTOMER     F
      ,BKG_CNTR_MF_DESC G
      ,BKG_CONTAINER    H
      ,BKG_CNTR_SEAL_NO I
 WHERE A.BKG_NO = B.BKG_NO
   AND A.BKG_NO = C.BKG_NO
   AND A.BKG_NO = D.BKG_NO
   AND A.BKG_NO = E.BKG_NO
   AND A.BKG_NO = F.BKG_NO
   AND A.BKG_NO = G.BKG_NO
   AND A.BKG_NO = H.BKG_NO
   AND A.BKG_NO = I.BKG_NO
   AND D.BKG_CUST_TP_CD = 'S'
   AND E.BKG_CUST_TP_CD = 'C'
   AND F.BKG_CUST_TP_CD = 'N'
#if (${vsl_cd} != '') 
   AND A.VSL_CD = @[vsl_cd]
   AND A.SKD_VOY_NO = @[skd_voy_no]
   AND A.SKD_DIR_CD = @[skd_dir_cd]
#end
#if (${pol_cd} != '') 
   AND A.POL_CD = @[pol_cd]
#end
#if (${pod_cd} != '') 
   AND A.POD_CD = @[pod_cd]
#end
#if (${cust_cnt_cd} != '') 

#end
#if (${usa_cstms_file_cd} != '') 
   AND A.USA_CSTMS_FILE_CD = @[usa_cstms_file_cd]
#end
#if (${cnd_cstms_file_cd} != '') 
   AND A.CND_CSTMS_FILE_CD = @[cnd_cstms_file_cd]
#end
#if (${bkg_ofc_cd} != '') 
   AND A.BKG_OFC_CD = @[bkg_ofc_cd]
#end
#if (${cre_usr_id} != '') 
   AND A.CRE_USR_ID = @[cre_usr_id]
#end
#if (${obl_iss_ofc_cd} != '') 
   AND B.OBL_ISS_OFC_CD = @[obl_iss_ofc_cd]
#end
#if (${obl_iss_usr_id} != '') 
   AND B.OBL_ISS_USR_ID = @[obl_iss_usr_id]
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="usa_cstms_file_cd" type="12" value="" out="N"/>
				<param name="cnd_cstms_file_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="obl_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="obl_iss_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
