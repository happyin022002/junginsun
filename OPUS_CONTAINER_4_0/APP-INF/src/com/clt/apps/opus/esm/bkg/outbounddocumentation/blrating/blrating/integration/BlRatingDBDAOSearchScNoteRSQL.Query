<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOSearchScNoteRSQL">
			<desc><![CDATA[S/C 계약의 Note 정보를 조회함]]></desc>
			<sql><![CDATA[
#if(${note_chg_tp_cd} =='O')

	/* OFT NOTE ROUTE */
	SELECT 
	'Route Note' TYPE,
	A.CMDT_HDR_SEQ||'.'||ROWNUM SEQ,
	'' TITLE,
	REPLACE(A.NOTE_CTNT,CHR(10),' ') CONTENT,
	TO_CHAR(K.EFF_DT,'YYYY-MM-DD') EFFECT_DT, /* N1ST_CMNC_AMDT_SEQ 로 따로 구함 */
	TO_CHAR(B.EXP_DT,'YYYY-MM-DD') EXPIRE_DT
	FROM PRI_SP_SCP_RT_CMDT_RNOTE A /* ROUTE NOTE 인데 CMDT 로 왜 시작하나(?) */
	                                /* ROUTE에 대한 NOTE 관리 */
	                                /* SPECIAL RATE 사용 */
	                                /* 3. NOTE_CHG_TP_CD 로 OFT,SURCHARGE 구분 */
	                                /* JAVA 에서 NOTE_CHG_TP_CD 에 따라 쿼리 분기하여 데이터 가져오록 작성 함 */ 
	
	
	,PRI_SP_SCP_MN B            /* S/C 계약을 위한 PROPOSAL의 MAIN TABLE */                             
	                            /* 1. PROP_NO, AMDT_SEQ, SVC_SCP_CD 조인 함 */
	                            /* 2. EFF_DT 는 AMDT_SEQ 와  PRI_SP_SCP_NOTE_CTNT 에서 N1ST_CMNC_AMDT_SEQ 로 조인해서 가져옴 */
	,PRI_SP_SCP_MN K   			/* EFFECT_DT 용 */ 
	WHERE 1=1
	AND A.PROP_NO = B.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = B.AMDT_SEQ
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.PROP_NO = K.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = K.AMDT_SEQ /* EFFECT_DT 용 */ 
	AND A.SVC_SCP_CD = K.SVC_SCP_CD
	AND A.NOTE_CHG_TP_CD ='O'
	AND A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.SVC_SCP_CD = @[svc_scp_cd]
	AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
	AND A.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
	#if(${rout_seq} !='')
		AND A.ROUT_SEQ = @[rout_seq]
	#end


	UNION ALL
	
	/* OFT CMDT NOTE */
	SELECT 
	'Commodity Note' TYPE,
	A.CMDT_HDR_SEQ||'.'||ROWNUM SEQ,
	'' TITLE,
	REPLACE(A.NOTE_CTNT,CHR(10),' ') CONTENT,
	TO_CHAR(K.EFF_DT,'YYYY-MM-DD') EFFECT_DT, /* N1ST_CMNC_AMDT_SEQ 로 따로 구함 */
	TO_CHAR(B.EXP_DT,'YYYY-MM-DD') EXPIRE_DT
	FROM PRI_SP_SCP_RT_CNOTE A      /* COMMODITY에 대한 NOTE 관리 */
	                                /* SPECIAL RATE 사용 */
	                                /* 3. NOTE_CHG_TP_CD 로 OFT,SURCHARGE 구분 */
	                                /* JAVA 에서 NOTE_CHG_TP_CD 에 따라 쿼리 분기하여 데이터 가져오록 작성 함 */ 
	
	,PRI_SP_SCP_MN B            /* S/C 계약을 위한 PROPOSAL의 MAIN TABLE */                             
	                            /* 1. PROP_NO, AMDT_SEQ, SVC_SCP_CD 조인 함 */
	                            /* 2. EFF_DT 는 AMDT_SEQ 와  PRI_SP_SCP_NOTE_CTNT 에서 N1ST_CMNC_AMDT_SEQ 로 조인해서 가져옴 */
	,PRI_SP_SCP_MN K   			/* EFFECT_DT 용 */ 
	WHERE 1=1
	AND A.PROP_NO = B.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = B.AMDT_SEQ
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.PROP_NO = K.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = K.AMDT_SEQ /* EFFECT_DT 용 */ 
	AND A.SVC_SCP_CD = K.SVC_SCP_CD
	AND A.NOTE_CHG_TP_CD ='O'
	AND A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.SVC_SCP_CD = @[svc_scp_cd]
	AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
	AND A.CMDT_HDR_SEQ = @[cmdt_hdr_seq]

	UNION ALL

	/* OFT NOTE */
	SELECT 
	'Special Note' TYPE,
	A.NOTE_SEQ||'.'||ROWNUM SEQ,
	C.NOTE_TIT_NM TITLE,
	REPLACE(A.NOTE_CTNT,CHR(10),' ') CONTENT,
	TO_CHAR(K.EFF_DT,'YYYY-MM-DD') EFFECT_DT, /* N1ST_CMNC_AMDT_SEQ 로 따로 구함 */
	TO_CHAR(B.EXP_DT,'YYYY-MM-DD') EXPIRE_DT
	FROM PRI_SP_SCP_NOTE C      /* 계약서에 출력하는 계약조항의 내용을 관리 */
	                            /* PRI_SP_SCP_NOTE 가 MASTER, PRI_SP_SCP_NOTE_CTNT 가 DETAIL 관계 */
	
	
	,PRI_SP_SCP_NOTE_CTNT A     /* STANDARD NOTE 와 SPECIAL NOTE 테이블 */
	                            /* 1. SPECIAL NOTE 만 사용 함 */
	                            /* 2. NOTE SEQ 와 ROWNUM 을 조합 하여 SEQ 생성 DP SEQ는 ORDER BY 용 */
	                            /* 3. NOTE_CHG_TP_CD 로 OFT,SURCHARGE 구분 */
	                            /* JAVA 에서 NOTE_CHG_TP_CD 에 따라 쿼리 분기하여 데이터 가져오록 작성 함 */ 
	                            
	,PRI_SP_SCP_MN B            /* S/C 계약을 위한 PROPOSAL의 MAIN TABLE */                             
	                            /* 1. PROP_NO, AMDT_SEQ, SVC_SCP_CD 조인 함 */
	                            /* 2. EFF_DT 는 AMDT_SEQ 와  PRI_SP_SCP_NOTE_CTNT 에서 N1ST_CMNC_AMDT_SEQ 로 조인해서 가져옴 */
	,PRI_SP_SCP_MN K   			/* EFFECT_DT 용 */ 
	WHERE 1=1
	AND C.PROP_NO = A.PROP_NO
	AND C.SVC_SCP_CD = A.SVC_SCP_CD
	AND C.AMDT_SEQ = A.AMDT_SEQ
	AND C.NOTE_TP_CD = A.NOTE_TP_CD
	AND C.NOTE_SEQ = A.NOTE_SEQ
	AND A.PROP_NO = B.PROP_NO
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.AMDT_SEQ = B.AMDT_SEQ
	AND A.PROP_NO = K.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = K.AMDT_SEQ /* EFFECT_DT 용 */ 
	AND A.SVC_SCP_CD = K.SVC_SCP_CD
	AND A.NOTE_TP_CD ='P' /* SPECIAL NOTE 사용 */
	AND A.NOTE_CHG_TP_CD ='O' /* OFT FREIGHT */
	AND A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.SVC_SCP_CD = @[svc_scp_cd]

#elseif (${note_chg_tp_cd} =='S')

	/* OFT NOTE ROUTE */
	SELECT 
	'Route Note' TYPE,
	A.CMDT_HDR_SEQ||'.'||ROWNUM SEQ,
	'' TITLE,
	REPLACE(A.NOTE_CTNT,CHR(10),' ') CONTENT,
	TO_CHAR(K.EFF_DT,'YYYY-MM-DD') EFFECT_DT, /* N1ST_CMNC_AMDT_SEQ 로 따로 구함 */
	TO_CHAR(B.EXP_DT,'YYYY-MM-DD') EXPIRE_DT
	FROM PRI_SP_SCP_RT_CMDT_RNOTE A /* ROUTE NOTE 인데 CMDT 로 왜 시작하나(?) */
	                                /* ROUTE에 대한 NOTE 관리 */
	                                /* SPECIAL RATE 사용 */
	                                /* 3. NOTE_CHG_TP_CD 로 OFT,SURCHARGE 구분 */
	                                /* JAVA 에서 NOTE_CHG_TP_CD 에 따라 쿼리 분기하여 데이터 가져오록 작성 함 */ 
	
	
	,PRI_SP_SCP_MN B            /* S/C 계약을 위한 PROPOSAL의 MAIN TABLE */                             
	                            /* 1. PROP_NO, AMDT_SEQ, SVC_SCP_CD 조인 함 */
	                            /* 2. EFF_DT 는 AMDT_SEQ 와  PRI_SP_SCP_NOTE_CTNT 에서 N1ST_CMNC_AMDT_SEQ 로 조인해서 가져옴 */
	,PRI_SP_SCP_MN K   			/* EFFECT_DT 용 */ 
	WHERE 1=1
	AND A.PROP_NO = B.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = B.AMDT_SEQ
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.PROP_NO = K.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = K.AMDT_SEQ /* EFFECT_DT 용 */ 
	AND A.SVC_SCP_CD = K.SVC_SCP_CD
	AND A.NOTE_CHG_TP_CD ='S'
	AND A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.SVC_SCP_CD = @[svc_scp_cd]
	AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
	AND A.CMDT_HDR_SEQ = @[cmdt_hdr_seq]

	UNION ALL
	
	/* OFT CMDT NOTE */
	SELECT 
	'Commodity Note' TYPE,
	A.CMDT_HDR_SEQ||'.'||ROWNUM SEQ,
	'' TITLE,
	REPLACE(A.NOTE_CTNT,CHR(10),' ') CONTENT,
	TO_CHAR(K.EFF_DT,'YYYY-MM-DD') EFFECT_DT, /* N1ST_CMNC_AMDT_SEQ 로 따로 구함 */
	TO_CHAR(B.EXP_DT,'YYYY-MM-DD') EXPIRE_DT
	FROM PRI_SP_SCP_RT_CNOTE A      /* COMMODITY에 대한 NOTE 관리 */
	                                /* SPECIAL RATE 사용 */
	                                /* 3. NOTE_CHG_TP_CD 로 OFT,SURCHARGE 구분 */
	                                /* JAVA 에서 NOTE_CHG_TP_CD 에 따라 쿼리 분기하여 데이터 가져오록 작성 함 */ 
	
	,PRI_SP_SCP_MN B            /* S/C 계약을 위한 PROPOSAL의 MAIN TABLE */                             
	                            /* 1. PROP_NO, AMDT_SEQ, SVC_SCP_CD 조인 함 */
	                            /* 2. EFF_DT 는 AMDT_SEQ 와  PRI_SP_SCP_NOTE_CTNT 에서 N1ST_CMNC_AMDT_SEQ 로 조인해서 가져옴 */
	,PRI_SP_SCP_MN K   			/* EFFECT_DT 용 */ 
	WHERE 1=1
	AND A.PROP_NO = B.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = B.AMDT_SEQ
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.PROP_NO = K.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = K.AMDT_SEQ /* EFFECT_DT 용 */ 
	AND A.SVC_SCP_CD = K.SVC_SCP_CD
	AND A.NOTE_CHG_TP_CD ='S'
	AND A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.SVC_SCP_CD = @[svc_scp_cd]
	AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
	AND A.CMDT_HDR_SEQ = @[cmdt_hdr_seq]

	UNION ALL

	/* OFT NOTE */
	SELECT 
	'Special Note' TYPE,
	A.NOTE_SEQ||'.'||ROWNUM SEQ,
	C.NOTE_TIT_NM TITLE,
	REPLACE(A.NOTE_CTNT,CHR(10),' ') CONTENT,
	TO_CHAR(K.EFF_DT,'YYYY-MM-DD') EFFECT_DT, /* N1ST_CMNC_AMDT_SEQ 로 따로 구함 */
	TO_CHAR(B.EXP_DT,'YYYY-MM-DD') EXPIRE_DT
	FROM PRI_SP_SCP_NOTE C      /* 계약서에 출력하는 계약조항의 내용을 관리 */
	                            /* PRI_SP_SCP_NOTE 가 MASTER, PRI_SP_SCP_NOTE_CTNT 가 DETAIL 관계 */
	
	
	,PRI_SP_SCP_NOTE_CTNT A     /* STANDARD NOTE 와 SPECIAL NOTE 테이블 */
	                            /* 1. SPECIAL NOTE 만 사용 함 */
	                            /* 2. NOTE SEQ 와 ROWNUM 을 조합 하여 SEQ 생성 DP SEQ는 ORDER BY 용 */
	                            /* 3. NOTE_CHG_TP_CD 로 OFT,SURCHARGE 구분 */
	                            /* JAVA 에서 NOTE_CHG_TP_CD 에 따라 쿼리 분기하여 데이터 가져오록 작성 함 */ 
	                            
	,PRI_SP_SCP_MN B            /* S/C 계약을 위한 PROPOSAL의 MAIN TABLE */                             
	                            /* 1. PROP_NO, AMDT_SEQ, SVC_SCP_CD 조인 함 */
	                            /* 2. EFF_DT 는 AMDT_SEQ 와  PRI_SP_SCP_NOTE_CTNT 에서 N1ST_CMNC_AMDT_SEQ 로 조인해서 가져옴 */
	,PRI_SP_SCP_MN K   			/* EFFECT_DT 용 */ 
	WHERE 1=1
	AND C.PROP_NO = A.PROP_NO
	AND C.SVC_SCP_CD = A.SVC_SCP_CD
	AND C.AMDT_SEQ = A.AMDT_SEQ
	AND C.NOTE_TP_CD = A.NOTE_TP_CD
	AND C.NOTE_SEQ = A.NOTE_SEQ
	AND A.PROP_NO = B.PROP_NO
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.AMDT_SEQ = B.AMDT_SEQ
	AND A.PROP_NO = K.PROP_NO
	AND A.N1ST_CMNC_AMDT_SEQ = K.AMDT_SEQ /* EFFECT_DT 용 */ 
	AND A.SVC_SCP_CD = K.SVC_SCP_CD	AND A.NOTE_TP_CD ='P' /* SPECIAL NOTE 사용 */
	AND A.NOTE_CHG_TP_CD ='S' /* SURCHARGE */
	AND A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.SVC_SCP_CD = @[svc_scp_cd]
#end			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
