<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchCodeConvUNLocRSQL">
			<desc><![CDATA[Un Code로 들어오는 Location은 OPUS Code로 Coversion]]></desc>
			<sql><![CDATA[
SELECT LOC_CD
FROM   MDM_LOCATION
WHERE  UN_LOC_CD = @[port_cd]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
