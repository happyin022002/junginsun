<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOmodifyHldNtcUsrUSQL">
			<desc><![CDATA[Hold Notice User 수정한다]]></desc>
			<sql><![CDATA[
UPDATE BKG_HLD_NTC_USR SET 
	CSTMS_DSPO_CD = @[cstms_dspo_cd]
,	NTC_MZD_CD = @[ntc_mzd_cd]
,	NTC_ENBL_FLG = @[ntc_enbl_flg]
,	NTC_EML = @[ntc_eml]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	HLD_SEQ = @[hld_seq]			]]></sql>
			<params>
				<param name="cstms_dspo_cd" type="12" value="" out="N"/>
				<param name="ntc_mzd_cd" type="12" value="" out="N"/>
				<param name="ntc_enbl_flg" type="12" value="" out="N"/>
				<param name="ntc_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="hld_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
