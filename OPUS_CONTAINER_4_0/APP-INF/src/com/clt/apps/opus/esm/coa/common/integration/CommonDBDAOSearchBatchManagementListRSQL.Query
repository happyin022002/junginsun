<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchBatchManagementListRSQL">
			<desc><![CDATA[Batch Management 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BAT_RCV_DT
   	       , CALL_FM_SRC_ID
   	       ,  BAT_DESC
   	       , COUNT(0) TOTAL
   	       , SUM(DECODE(BAT_FLG, 'Y', 1, 0)) COMPLETE
           --, SUM(DECODE(NVL(BAT_FLG,'N'), 'N', 1, 0)) MISSING		
           , NVL(COUNT(0),0) - NVL(SUM(DECODE(BAT_FLG, 'Y', 1, 0)),0) AS MISSING 	--20150522.ADD
   	  FROM COA_CALC_BAT
   	  GROUP BY BAT_RCV_DT, CALL_FM_SRC_ID, BAT_DESC
   	  ORDER BY BAT_RCV_DT DESC, CALL_FM_SRC_ID ASC, BAT_DESC ASC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
