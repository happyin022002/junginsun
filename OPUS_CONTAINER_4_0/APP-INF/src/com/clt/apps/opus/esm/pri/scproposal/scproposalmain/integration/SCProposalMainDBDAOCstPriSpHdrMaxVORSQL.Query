<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOCstPriSpHdrMaxVORSQL">
			<desc><![CDATA[S/C no Max 값을 구한다.

S/C No. 3월 한달간만 8자리로변경

yy -> 50  

SELECT MAP.SC_PFX_CD||TO_CHAR(SYSDATE,'YY')||(SELECT LPAD (MAX (SUBSTR (SC_NO, -3)) + 1, 3, '0') SC_NO
         FROM   PRI_SP_HDR
         WHERE  SC_NO LIKE MAP.SC_PFX_CD || TO_CHAR (SYSDATE, 'YY') || '%') SC_NO
FROM   PRI_SC_PFX_MAPG MAP
      ,MDM_ORGANIZATION ORG
      ,MDM_LOCATION LOC
WHERE  SVC_SCP_CD = @[svc_scp_cd]
AND    ORG.OFC_CD = @[ofc_cd]
AND    ORG.LOC_CD = LOC.LOC_CD
AND    LOC.SCONTI_CD = MAP.SCONTI_CD]]></desc>
			<sql><![CDATA[
SELECT MAP.SC_PFX_CD||TO_CHAR(SYSDATE,'YY')||(SELECT LPAD (MAX (SUBSTR (SC_NO, -3)) + 1, 4, '0') SC_NO
         FROM   PRI_SP_HDR
         WHERE  SC_NO LIKE MAP.SC_PFX_CD || TO_CHAR(SYSDATE,'YY') || '%') SC_NO
FROM   PRI_SC_PFX_MAPG MAP
      ,MDM_ORGANIZATION ORG
      ,MDM_LOCATION LOC
WHERE  SVC_SCP_CD = @[svc_scp_cd]
AND    ORG.OFC_CD = @[ofc_cd]
AND    ORG.LOC_CD = LOC.LOC_CD
AND    LOC.SCONTI_CD = MAP.SCONTI_CD			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
