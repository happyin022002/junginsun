<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MotFilingLocationPropertyDBDAOPriMotFileLocPptVOUSQL">
			<desc><![CDATA[PriMotFileLocPpt Update]]></desc>
			<sql><![CDATA[
UPDATE  PRI_MOT_FILE_LOC_PPT
SET     MOT_FILE_LOC_CD = @[mot_file_loc_cd]
    ,   MOT_FILE_LOC_NM = @[mot_file_loc_nm]
    ,   MOT_FILE_LANE_CD = @[mot_file_lane_cd]
    ,	DELT_FLG = DECODE(NVL(@[delt_flg],'0'),'0','N','1','Y')
    ,   UPD_USR_ID = @[upd_usr_id]
    ,   UPD_DT = SYSDATE
WHERE   ORG_DEST_TP_CD = @[org_dest_tp_cd]
AND     LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="mot_file_loc_cd" type="12" value="" out="N"/>
				<param name="mot_file_loc_nm" type="12" value="" out="N"/>
				<param name="mot_file_lane_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
