<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOaddIMFMODSndLogCSQL">
			<desc><![CDATA[IMFMOD전송기록을 남긴다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_KR_SND_LOG
	   (MSG_LOG_TP_ID,
		SND_DT,
		OFC_CD,
		FLT_FILE_REF_NO,
		TRSM_USR_ID,
		VSL_CD,
		SKD_VOY_NO,
		SKD_DIR_CD,
		POL_CD,
		BL_NO,
		SMT_AMD_NO,
		CORR_CD1,
		MF_RCVR_USR_ID,
		MF_SND_SEQ,
		CRE_USR_ID,
		CRE_DT,
		UPD_USR_ID,
		UPD_DT,
		EDI_SND_MSG_CTNT)

VALUES ('5LI',
		DECODE(@[snd_dt], NULL, SYSDATE, TO_DATE(@[snd_dt], 'YYYYMMDD HH24:MI:SS')),
		@[ofc_cd],
		@[flt_file_ref_no],
		@[user_id],
		SUBSTR(@[vvd], 1, 4),
		SUBSTR(@[vvd], 5, 4),
		SUBSTR(@[vvd], 9, 1),
		@[pol_cd],
		@[bl_no],
		@[sub_no],
		@[corr_cd],
		RPAD('KCS0004XML', 20),
		1,
		@[user_id],
		SYSDATE,
		@[user_id],
		SYSDATE,
		@[edi_snd_msg_ctnt])			]]></sql>
			<params>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="sub_no" type="12" value="" out="N"/>
				<param name="corr_cd" type="12" value="" out="N"/>
				<param name="edi_snd_msg_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
