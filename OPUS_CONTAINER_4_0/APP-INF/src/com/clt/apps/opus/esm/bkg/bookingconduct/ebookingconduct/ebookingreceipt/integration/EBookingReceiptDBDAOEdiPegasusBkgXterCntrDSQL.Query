<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOEdiPegasusBkgXterCntrDSQL">
			<desc><![CDATA[EBookingReceiptDBDAOEdiPegasusBkgXterCntrDSQL]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_XTER_CNTR BXC
WHERE BXC.XTER_SNDR_ID = 'PEGASUS'
AND BXC.XTER_RQST_NO = @[xter_rqst_no]
AND BXC.XTER_RQST_SEQ = @[xter_rqst_seq]
AND BXC.CNTR_NO = 'No Data'
AND NOT EXISTS (SELECT 'X' 
                FROM BKG_XTER_CNTR_SEAL_NO BXCS 
                WHERE BXCS.XTER_SNDR_ID = BXC.XTER_SNDR_ID
                AND BXCS.XTER_RQST_NO = BXC.XTER_RQST_NO
                AND BXCS.XTER_RQST_SEQ = BXC.XTER_RQST_SEQ)			]]></sql>
			<params>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
