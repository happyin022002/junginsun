<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCntrRtBkgQtyRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT bkg_no
,      cntr_tpsz_cd
,      op_cntr_qty
,      eq_subst_cntr_tpsz_cd
--,      eq_subst_cgo_qty
FROM   bkg_qty_dtl
WHERE  bkg_no = @[bkg_no]
AND    eq_subst_cntr_tpsz_cd IS NOT NULL
--AND    eq_subst_cgo_qty > 0
AND    NOT (CNTR_TPSZ_CD LIKE 'R%' AND RC_FLG = 'N')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
