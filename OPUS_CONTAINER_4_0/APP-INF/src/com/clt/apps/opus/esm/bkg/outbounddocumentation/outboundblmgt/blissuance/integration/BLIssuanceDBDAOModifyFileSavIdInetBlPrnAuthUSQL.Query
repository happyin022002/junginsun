<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOModifyFileSavIdInetBlPrnAuthUSQL">
			<desc><![CDATA[FILE_SAV_ID를 update 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_INET_BL_PRN_AUTH
    SET FILE_SAV_ID = @[file_sav_id]
    ,UPD_USR_ID = @[usr_id]
    ,UPD_DT = sysdate
WHERE BKG_NO = @[bkg_no]
AND INFO_SEQ = @[info_seq]			]]></sql>
			<params>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="info_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
