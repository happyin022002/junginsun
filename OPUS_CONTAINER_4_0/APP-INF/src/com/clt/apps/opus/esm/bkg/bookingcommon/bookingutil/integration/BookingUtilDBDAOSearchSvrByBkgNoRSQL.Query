<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchSvrByBkgNoRSQL">
			<desc><![CDATA[BKG No로 server 확인]]></desc>
			<sql><![CDATA[
SELECT 
	SYS_AREA_GRP_ID SVR_ID_CD
,	OFC_PFX_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_PFX_ROUT
WHERE	OFC_PFX_CD = @[ofc_pfx_cd]
AND  ROWNUM =1			]]></sql>
			<params>
				<param name="ofc_pfx_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
