<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOAddControllingPartyCSQL">
			<desc><![CDATA[add Controlling Party]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CTRL_PTY
(
    CTRL_PTY_SEQ,
    CTRL_PTY_NM,
    CTRL_PTY_DESC,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)VALUES
(
    @[ctrl_pty_seq],
    @[ctrl_pty_nm],
    @[ctrl_pty_desc],
    @[cre_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="ctrl_pty_seq" type="12" value="" out="N"/>
				<param name="ctrl_pty_nm" type="12" value="" out="N"/>
				<param name="ctrl_pty_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
