<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeMappingDBDAOAddNewLaneCSQL">
			<desc><![CDATA["NEW Lane & Office CMCB"의 마스터 테이블에 정보 입력]]></desc>
			<sql><![CDATA[
MERGE INTO CSQ_QTA_NEW_LANE T
USING (
         SELECT @[bse_tp_cd]  AS BSE_TP_CD
               ,@[bse_yr]     AS BSE_YR
               ,DECODE(@[bse_tp_cd], 'Y', '00', @[bse_qtr_cd]) AS BSE_QTR_CD
               ,@[trd_cd]     AS TRD_CD
               ,@[rlane_cd]   AS RLANE_CD
               ,@[dir_cd]     AS DIR_CD
               ,@[cre_usr_id] AS USR_ID
           FROM DUAL
          WHERE 1=1
            AND EXISTS (SELECT 1 
                          FROM CSQ_QTA_LANE_MGMT
                         WHERE CSQ_ACT_FLG = 'Y'
             			   AND BSE_TP_CD   = @[bse_tp_cd]
             			   AND BSE_YR      = @[bse_yr] 
             			   AND BSE_QTR_CD  = DECODE(@[bse_tp_cd], 'Y', '00', @[bse_qtr_cd])
                           AND IAS_SCTR_FLG = 'N'
                           AND TRD_CD       = @[trd_cd]
                           AND RLANE_CD     = @[rlane_cd]
                       )
      ) I
   ON (
                T.BSE_TP_CD  = I.BSE_TP_CD
            AND T.BSE_YR     = I.BSE_YR
            AND T.BSE_QTR_CD = I.BSE_QTR_CD
            AND T.TRD_CD     = I.TRD_CD
            AND T.RLANE_CD   = I.RLANE_CD
            AND T.DIR_CD     = I.DIR_CD
      )
WHEN NOT MATCHED THEN
    INSERT (
                BSE_TP_CD
               ,BSE_YR
               ,BSE_QTR_CD
               ,TRD_CD
               ,RLANE_CD
               ,DIR_CD
               ,CRE_USR_ID
               ,CRE_DT
               ,UPD_USR_ID
               ,UPD_DT
    ) VALUES (
                I.BSE_TP_CD
               ,I.BSE_YR
               ,I.BSE_QTR_CD
               ,I.TRD_CD
               ,I.RLANE_CD
               ,I.DIR_CD
               ,I.USR_ID
               ,SYSDATE
               ,I.USR_ID
               ,SYSDATE
    )			]]></sql>
			<params>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
