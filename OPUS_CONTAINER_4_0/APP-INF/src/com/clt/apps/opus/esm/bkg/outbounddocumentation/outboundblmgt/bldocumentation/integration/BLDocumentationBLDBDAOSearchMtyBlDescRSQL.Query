<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchMtyBlDescRSQL">
			<desc><![CDATA[BL Desc를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT cntr_tpsz_cd||' X '||TO_CHAR(count(cntr_no)) MTY_BL_DESC
FROM bkg_container
WHERE bkg_no = @[bkg_no]
GROUP BY cntr_tpsz_cd			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
