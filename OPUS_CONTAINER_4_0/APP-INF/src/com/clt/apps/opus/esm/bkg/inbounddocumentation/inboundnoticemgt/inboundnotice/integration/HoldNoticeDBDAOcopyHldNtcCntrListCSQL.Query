<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOcopyHldNtcCntrListCSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_HLD_N3RD_PTY_BIL_CNTR (
	BKG_NO
,	NTC_SEQ
,	CNTR_NO
,	N3PTY_BIL_NO
,	INCUR_CHG_AMT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT)
SELECT
 	@[bkg_no] AS BKG_NO
,	@[ntc_seq] AS NTC_SEQ
,	b.CNTR_NO
,	'' AS N3PTY_BIL_NO
,	'' AS INCUR_CHG_AMT
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM  BKG_BOOKING A
     ,BKG_CSTMS_ADV_CNTR B
WHERE  A.BKG_NO = @[bkg_no]
AND    A.BL_NO = B.BL_NO
AND    B.CNT_CD = @[cnt_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ntc_seq" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
