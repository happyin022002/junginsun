<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOcopySsCgoByBkgCSQL">
			<desc><![CDATA[copySsCgoByBkg]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_STWG_CGO(BKG_NO
        , STWG_SEQ
        , STWG_CD
        , STWG_RMK
#if(${copy_mode_cd} != 'C')
        , RQST_DT
        , RQST_USR_ID
        , SPCL_CGO_APRO_CD
#end
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT)
SELECT @[mst_bkg_no] BKG_NO
        , STWG_SEQ
        , STWG_CD
        , STWG_RMK
#if(${copy_mode_cd} != 'C')
        , RQST_DT
        , RQST_USR_ID
        , SPCL_CGO_APRO_CD
#end
        , @[usr_id] CRE_USR_ID
        , SYSDATE
        , @[usr_id] UPD_USR_ID
        , SYSDATE
  FROM BKG_STWG_CGO 
 WHERE BKG_NO = @[bkg_no] 			]]></sql>
			<params>
				<param name="mst_bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
