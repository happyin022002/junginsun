<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchCRNRSQL">
			<desc><![CDATA[crn number 조회]]></desc>
			<sql><![CDATA[
SELECT  VKG.VSL_CALL_REF_NO crn_number,VKG.VSL_CD,VKG.SKD_VOY_NO,VKG.SKD_DIR_CD,
 TO_CHAR(VVD_ETA_DT,'yyyy-mm-dd') VVD_ETA_DT,VVD_NM,
VKG.VSL_CD||VKG.SKD_VOY_no||VKG.SKD_DIR_CD vvd_number
FROM  BKG_CSTMS_RTM_VSL VKG
#if (${vsl_cd} == '' && ${frm_crn_number}!= '') 	
              WHERE   VKG.VSL_CALL_REF_NO       = @[frm_crn_number] 
#end
#if (${vsl_cd}!= '' && ${frm_crn_number} == '') 	
              WHERE VKG.VSL_CD        = @[vsl_cd] 
#end
#if (${vsl_cd}!= '' && ${frm_crn_number} != '') 
              WHERE VKG.VSL_CD                = @[vsl_cd] 
              AND   VKG.VSL_CALL_REF_NO       = @[frm_crn_number] 
#end
#if (${skd_voy_no}!= '') 
              AND VKG.SKD_VOY_no    = @[skd_voy_no]
#end
#if (${skd_dir_cd}!= '') 
               AND VKG.SKD_DIR_CD    = @[skd_dir_cd]
#end			]]></sql>
			<params>
				<param name="frm_crn_number" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
