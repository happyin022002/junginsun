<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBasicStandardNoteGuidelineDAOPriSgStndNoteHdrMaxSeqRSQL">
			<desc><![CDATA[노트 헤더 시퀀스 최대값 조회]]></desc>
			<sql><![CDATA[
select (NVL(Max(note_hdr_seq),0)+1) as note_hdr_seq
from pri_sg_stnd_note_hdr
where	svc_scp_cd = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
