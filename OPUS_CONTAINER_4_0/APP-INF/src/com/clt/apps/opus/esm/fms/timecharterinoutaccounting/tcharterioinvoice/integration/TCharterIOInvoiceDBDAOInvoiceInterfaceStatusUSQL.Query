<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDBDAOInvoiceInterfaceStatusUSQL">
			<desc><![CDATA[Invoice Interface Status 화면에서 취소시 수정]]></desc>
			<sql><![CDATA[
UPDATE FMS_OWNR_ACCT_SLP SET 
	CSR_SLP_FLG = 'N'
,	CSUL_SLP_TP_CD = NULL
,	CSUL_SLP_FUNC_CD = NULL
,	CSUL_SLP_OFC_CD = NULL
,	CSUL_SLP_ISS_DT = NULL
,	CSUL_SLP_SER_NO = NULL
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	CSUL_SLP_TP_CD = @[org_slp_tp_cd]
AND	CSUL_SLP_FUNC_CD = @[org_slp_func_cd]
AND	CSUL_SLP_OFC_CD = @[org_slp_ofc_cd]
AND	CSUL_SLP_ISS_DT = @[org_slp_iss_dt]
AND	CSUL_SLP_SER_NO = @[org_slp_ser_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="org_slp_tp_cd" type="12" value="" out="N"/>
				<param name="org_slp_func_cd" type="12" value="" out="N"/>
				<param name="org_slp_ofc_cd" type="12" value="" out="N"/>
				<param name="org_slp_iss_dt" type="12" value="" out="N"/>
				<param name="org_slp_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
