<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOsearchEdiCntrSealNoMfrFullRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
WITH SEAL_INFO AS (
    SELECT UPPER(CSN.CNTR_SEAL_NO) AS SEAL_NO,
           RANK() OVER (PARTITION BY CSN.BKG_NO, CSN.CNTR_NO ORDER BY CSN.CNTR_SEAL_SEQ ASC) AS RNK_SEQ
      FROM BKG_CNTR_SEAL_NO CSN,
           BKG_BOOKING BKG
     WHERE CSN.BKG_NO = BKG.BKG_NO
       AND BKG.BL_NO = @[bl_no]
       AND CSN.CNTR_NO = @[cntr_no])


SELECT RPAD(NVL((SELECT SEAL_NO
                   FROM SEAL_INFO
                  WHERE RNK_SEQ = 1), 'UNKNOWN'), 15, ' ') AS DATA00,
       RPAD(NVL((SELECT SEAL_NO
                   FROM SEAL_INFO
                  WHERE RNK_SEQ = 2), ' '), 15, ' ') AS DATA01,
       RPAD(NVL((SELECT SEAL_NO
                   FROM SEAL_INFO
                  WHERE RNK_SEQ = 3), ' '), 15, ' ') AS DATA02,
       RPAD(NVL((SELECT SEAL_NO
                   FROM SEAL_INFO
                  WHERE RNK_SEQ = 4), ' '), 15, ' ') AS DATA03,
       RPAD(NVL((SELECT SEAL_NO
                   FROM SEAL_INFO
                  WHERE RNK_SEQ = 5), ' '), 15, ' ') AS DATA04,
       RPAD(NVL((SELECT SEAL_NO
                   FROM SEAL_INFO
                  WHERE RNK_SEQ = 6), ' '), 15, ' ') AS DATA05
 FROM DUAL			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
