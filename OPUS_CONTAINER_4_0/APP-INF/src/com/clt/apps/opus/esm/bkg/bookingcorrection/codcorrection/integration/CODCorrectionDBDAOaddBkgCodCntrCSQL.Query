<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOaddBkgCodCntrCSQL">
			<desc><![CDATA[bkg_cod_cntr에 insert한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_COD_CNTR (
	COD_SLCT_FLG
,	CNTR_TPSZ_CD
,	CNTR_WGT
,	WGT_UT_CD
,	DCGO_FLG
,	RC_FLG
,	BB_CGO_FLG
,	AWK_CGO_FLG
,	SOC_FLG
,	BKG_NO
,	COD_RQST_SEQ
,	CNTR_NO
,	CNTR_STWG_NO
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[cod_slct_flg]
,	@[cntr_tpsz_cd]
,	TO_NUMBER(REPLACE(@[cntr_wgt], ',', ''))
,	@[wgt_ut_cd]
,	@[dcgo_flg]
,	@[rc_flg]
,	@[bb_cgo_flg]
,	@[awk_cgo_flg]
,	@[soc_flg]
,	@[bkg_no]
,	@[cod_rqst_seq]
,	@[cntr_no]
,	@[cntr_stwg_no]
,	@[usr_id]
,	sysdate
,	@[usr_id]
,	sysdate
)			]]></sql>
			<params>
				<param name="cod_slct_flg" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="dcgo_flg" type="12" value="" out="N"/>
				<param name="rc_flg" type="12" value="" out="N"/>
				<param name="bb_cgo_flg" type="12" value="" out="N"/>
				<param name="awk_cgo_flg" type="12" value="" out="N"/>
				<param name="soc_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_stwg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
