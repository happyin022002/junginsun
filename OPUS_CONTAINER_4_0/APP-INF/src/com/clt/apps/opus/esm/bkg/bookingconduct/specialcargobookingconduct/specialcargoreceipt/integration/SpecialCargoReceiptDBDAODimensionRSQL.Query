<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAODimensionRSQL">
			<desc><![CDATA[BkgAwkDimVO]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')

SELECT
DIM_WDT
,	DIM_HGT
,	BKG_NO
,	AWK_CGO_SEQ
,	DIM_SEQ
,	DIM_LEN
,	INDIV_PCK_WGT
FROM BKG_AWK_DIM_HIS
WHERE	BKG_NO = @[bkg_no]
AND 	AWK_CGO_SEQ = @[awk_cgo_seq]
and  CORR_NO = 'TMP0000001'

#else

SELECT 
	DIM_WDT
,	DIM_HGT
,	BKG_NO
,	AWK_CGO_SEQ
,	DIM_SEQ
,	DIM_LEN
,	INDIV_PCK_WGT
FROM BKG_AWK_DIM
WHERE	BKG_NO = @[bkg_no]
AND 	AWK_CGO_SEQ = @[awk_cgo_seq]

#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="awk_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
