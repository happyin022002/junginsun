<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentTradeRHQDBDAOCreateMonthlyAdjustmentTradeFinalInfoB0085CSQL">
			<desc><![CDATA[Final (STEP 03) 초기 정보 생성B]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_MON_QTA_TRD (
    MQTA_STEP_CD,
    BSE_YR,
    BSE_QTR_CD,
    TRD_CD,
    DIR_CD,
    MQTA_VER_NO,
    RLANE_CD,
    SPRT_GRP_CD,
    BSA_GRP_CD,
    CTRT_RHQ_CD,
    BSE_MON,
    SUB_TRD_CD,
    LOD_QTY,
    GRS_RPB_REV,
    CM_UC_AMT,
    OPFIT_UC_AMT,
    RA_CM_UC_AMT,
    RA_OPFIT_UC_AMT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
     )
SELECT
    '03' AS MQTA_STEP_CD,
    TRD.BSE_YR,
    TRD.BSE_QTR_CD,
    TRD.TRD_CD,
    TRD.DIR_CD,
    TRD.MQTA_VER_NO,
    TRD.RLANE_CD,
    TRD.SPRT_GRP_CD,
    TRD.BSA_GRP_CD,
    TRD.CTRT_RHQ_CD,
    TRD.BSE_MON,
    TRD.SUB_TRD_CD,
    0 AS LOD_QTY,
    0 AS GRS_RPB_REV,
    TRD.CM_UC_AMT,
    TRD.OPFIT_UC_AMT,
    TRD.RA_CM_UC_AMT,
    TRD.RA_OPFIT_UC_AMT,
    @[userId] AS cre_usr_id,
    SYSDATE AS CRE_DT,
    @[userId] AS upd_usr_id,
    SYSDATE AS UPD_DT
FROM SAQ_MON_QTA_STEP_VER SVR,
     SAQ_MON_QTA_TRD TRD
WHERE SVR.MQTA_STEP_CD = '01'
AND   SVR.BSE_YR = @[year]
AND   SVR.BSE_QTR_CD = @[bse_quarter]
AND   SVR.TRD_CD = @[trade]
AND   SVR.DIR_CD = @[bound]
AND   SVR.GLINE_VER_NO = @[glineVerNo]
AND   SVR.SAQ_STS_CD = 'DN'
AND   TRD.MQTA_STEP_CD = SVR.MQTA_STEP_CD
AND   TRD.BSE_YR = SVR.BSE_YR
AND   TRD.BSE_QTR_CD = SVR.BSE_QTR_CD
AND   TRD.TRD_CD = SVR.TRD_CD
AND   TRD.DIR_CD = SVR.DIR_CD
AND   TRD.MQTA_VER_NO = SVR.MQTA_VER_NO			]]></sql>
			<params>
				<param name="userId" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_quarter" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="glineVerNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
