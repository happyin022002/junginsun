<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSpcFileImptCustCSQL">
			<desc><![CDATA[File Import Save 를 수행한다.
2015.07.22. SKY[CLT-000042051-10] Virtual add call - VT_ADD_CALL_FLG IS  NULL  로직 추가
2016.02.18  SKY  COA_MON_VVD : DELT_FLG 추가
2016.02.23 박다은 COA_MON_VVD 에 RLANE_CD 조건 추가 및 POD_CD 생성 로직 보완
2016.03.15 SKY TURN_PORT_IND_CD IN ('Y','N') 로직 추가
2016.03.17 SKY PS3.POD_YD_CD 로직 수정 : DISTINCT 추가
2016.03.30 SKY CTRT_NO 조건 추가
2016.04.01 SKY PS3/PS2 로직 제거
2016.04.05 SKY C.TRD_CD = P.TRD_CD 로직 추가
2016.04.08 SKY PS.POD_YD 로직 수정
2016.06.27 SKY SLS_RGN_OFC_CD 조건에 추가]]></desc>
			<sql><![CDATA[
MERGE INTO SPC_DLY_FCAST_CUST A USING 
( 
WITH PARAMS AS  
       (SELECT @[trd_cd] AS TRD_CD 
            , @[sub_trd_cd] AS SUB_TRD_CD 
            , @[rlane_cd] AS RLANE_CD 
            , @[dir_cd] AS DIR_CD 
            , @[vsl_cd] AS VSL_CD 
            , @[skd_voy_no] AS SKD_VOY_NO 
            , @[skd_dir_cd] AS SKD_DIR_CD 
            , @[ioc_ts_cd] AS IOC_TS_CD 
            , @[cust_cnt_cd] AS CUST_CNT_CD 
            , @[cust_seq] AS CUST_SEQ 
            , @[pol_yd_cd] AS POL_CD 
            , @[pod_yd_cd] AS POD_CD 
            , @[fcast_ofc_cd] AS FCAST_OFC_CD
            , @[fcast_cust_tp_cd] AS FCAST_CUST_TP_CD 
            , TO_NUMBER(@[fcast_ttl_qty]) AS FCAST_TTL_QTY 
            , TO_NUMBER(@[fcast_40ft_hc_qty]) AS FCAST_40FT_HC_QTY 
            , TO_NUMBER(@[fcast_45ft_hc_qty]) AS FCAST_45FT_HC_QTY 
            , TO_NUMBER(@[fcast_53ft_qty]) AS FCAST_53FT_QTY 
            , TO_NUMBER(@[fcast_rf_qty]) AS FCAST_RF_QTY 
            , TO_NUMBER(@[fcast_ttl_wgt]) AS FCAST_TTL_WGT 
            , TO_NUMBER(@[cfm_ttl_qty]) AS CFM_TTL_QTY 
            , TO_NUMBER(@[cfm_40ft_hc_qty]) AS CFM_40FT_HC_QTY 
            , TO_NUMBER(@[cfm_45ft_hc_qty]) AS CFM_45FT_HC_QTY 
            , TO_NUMBER(@[cfm_53ft_qty]) AS CFM_53FT_QTY 
            , TO_NUMBER(@[cfm_rf_qty]) AS CFM_RF_QTY 
            , TO_NUMBER(@[cfm_ttl_wgt]) AS CFM_TTL_WGT 
            , TO_NUMBER(@[usd_bkg_ttl_qty]) AS USD_BKG_TTL_QTY 
            , TO_NUMBER(@[usd_bkg_20ft_qty]) AS USD_BKG_20FT_QTY 
            , TO_NUMBER(@[usd_bkg_40ft_qty]) AS USD_BKG_40FT_QTY 
            , TO_NUMBER(@[usd_bkg_40ft_hc_qty]) AS USD_BKG_40FT_HC_QTY
            , TO_NUMBER(@[usd_bkg_45ft_hc_qty]) AS USD_BKG_45FT_HC_QTY
            , TO_NUMBER(@[usd_bkg_53ft_qty]) AS USD_BKG_53FT_QTY 
            , TO_NUMBER(@[usd_bkg_rf_qty]) AS USD_BKG_RF_QTY 
            , TO_NUMBER(@[usd_bkg_ttl_wgt]) AS USD_BKG_TTL_WGT 
            , @[ctrl_lvl_cd] AS CTRL_LVL_CD
            , @[upd_usr_id] AS USR_ID 
            , TO_CHAR(SYSDATE, 'YYYYMMDD HH24:MI:SS') AS MODI_DT 
            --20160203.ADD, 20160325.MOD
            , @[ctrt_no] AS CTRT_NO
            , @[ctrt_cust_cnt_cd] AS CTRT_CUST_CNT_CD
            , @[ctrt_cust_seq]    AS CTRT_CUST_SEQ
         FROM DUAL 
       ) 
SELECT NVL(P.RLANE_CD, MV.RLANE_CD) AS RLANE_CD 
     , MV.DIR_CD 
     , MV.VSL_CD 
     , MV.SKD_VOY_NO 
     , MV.DIR_CD AS SKD_DIR_CD 
     , P.IOC_TS_CD 
     , NVL(@[srep_usr_id],SR.SREP_CD) AS SREP_USR_ID 
     , P.CUST_CNT_CD 
     , P.CUST_SEQ
     , P.POL_CD   AS POL_YD_CD
     , DECODE(LENGTH(P.POD_CD), 7, P.POD_CD, PS.POD_YD_CD) AS POD_YD_CD
--     , DECODE(LENGTH(P.POL_CD), 7, P.POL_CD, PS2.POL_YD_CD) AS POL_YD_CD 
--     , DECODE(LENGTH(P.POD_CD), 7, P.POD_CD, 5, PS3.POD_YD_CD, PS.POD_YD_CD) AS POD_YD_CD 
     , P.FCAST_OFC_CD 
     , P.FCAST_CUST_TP_CD
     , DECODE(P.IOC_TS_CD, 'O', 'O', 'I') AS IOC_CD 
     , RL.REP_TRD_CD 
     , 
       (SELECT RDRL.SUB_TRD_CD 
         FROM MDM_DTL_REV_LANE RDRL 
        WHERE RDRL.RLANE_CD = RL.RLANE_CD 
              AND RDRL.TRD_CD = RL.REP_TRD_CD 
              AND RDRL.VSL_SLAN_DIR_CD = P.DIR_CD 
              AND ROWNUM = 1 
       ) AS REP_SUB_TRD_CD
     , NVL(P.TRD_CD, MV.TRD_CD) AS TRD_CD
     , NVL(P.SUB_TRD_CD, MV.SUB_TRD_CD) AS SUB_TRD_CD
     , O.N2ND_PRNT_OFC_CD AS SLS_RHQ_CD 
     , O.N3RD_PRNT_OFC_CD AS SLS_AQ_CD 
     , O.N4TH_PRNT_OFC_CD AS SLS_RGN_OFC_CD
     , O.SPC_SLS_OFC_CD AS SLS_OFC_CD 
     , P.FCAST_TTL_QTY 
     , P.FCAST_40FT_HC_QTY
     , P.FCAST_45FT_HC_QTY
     , P.FCAST_53FT_QTY 
     , P.FCAST_RF_QTY 
     , P.FCAST_TTL_WGT 
     , P.FCAST_TTL_QTY AS CFM_TTL_QTY 
     , P.FCAST_40FT_HC_QTY AS CFM_40FT_HC_QTY
     , P.FCAST_45FT_HC_QTY AS CFM_45FT_HC_QTY
     , P.FCAST_53FT_QTY AS CFM_53FT_QTY 
     , P.FCAST_RF_QTY AS CFM_RF_QTY 
     , P.FCAST_TTL_WGT AS CFM_TTL_WGT 
     , P.USD_BKG_TTL_QTY 
     , P.USD_BKG_20FT_QTY 
     , P.USD_BKG_40FT_QTY 
     , P.USD_BKG_40FT_HC_QTY
     , P.USD_BKG_45FT_HC_QTY
     , P.USD_BKG_53FT_QTY 
     , P.USD_BKG_RF_QTY 
     , P.USD_BKG_TTL_WGT 
     , P.CTRL_LVL_CD 
     , CAST(SYS_EXTRACT_UTC(TO_TIMESTAMP(MODI_DT, 'YYYY/MM/DD HH24:MI:SS')) AS DATE) AS MODI_GDT
     , P.USR_ID AS CFM_USR_ID
     , SYSDATE AS CFM_DT 
     , P.USR_ID AS CRE_USR_ID
     , SYSDATE AS CRE_DT 
     , P.USR_ID AS UPD_USR_ID
     , SYSDATE AS UPD_DT 
     --20160203.ADD
     , (SELECT NVL(MAX(FCAST_SEQ),0)+1 FROM SPC_DLY_FCAST_CUST 
         WHERE RLANE_CD     = NVL(P.RLANE_CD, MV.RLANE_CD)   
           AND DIR_CD       = MV.DIR_CD    
           AND VSL_CD       = MV.VSL_CD      
           AND SKD_VOY_NO   = MV.SKD_VOY_NO
           AND SKD_DIR_CD   = MV.DIR_CD
           AND IOC_TS_CD    = P.IOC_TS_CD 
           AND SREP_USR_ID  = NVL(@[srep_usr_id],SR.SREP_CD) 
           AND CUST_CNT_CD  = P.CUST_CNT_CD
           AND CUST_SEQ     = P.CUST_SEQ   
           AND POL_YD_CD    = P.POL_CD
           AND POD_YD_CD    = DECODE(LENGTH(P.POD_CD), 7, P.POD_CD,PS.POD_YD_CD)
           AND FCAST_OFC_CD = P.FCAST_OFC_CD
           AND FCAST_CUST_TP_CD = P.FCAST_CUST_TP_CD ) FCAST_SEQ
     , P.CTRT_NO					--20160325.MOD
     , P.CTRT_CUST_CNT_CD
     , P.CTRT_CUST_SEQ
  FROM SPC_OFC_LVL O 
     , 
       (SELECT SREP_CD 
         FROM 
              (SELECT T1.SREP_CD 
                FROM MDM_SLS_REP T1 
                   , PARAMS T2 
               WHERE 1=1 
                     AND T1.OFC_CD = T2.FCAST_OFC_CD 
                     AND T1.DELT_FLG = 'N' 
            ORDER BY T1.SREP_CD 
              ) 
        WHERE ROWNUM = '1' 
       ) SR
     , MDM_REV_LANE RL
     , PARAMS P 
     , 
       (SELECT IOC_CD
            , VSL_CD
            , SKD_VOY_NO
            , DIR_CD 
            , RLANE_CD
            , TRD_CD 
         FROM COA_MON_VVD 
        WHERE 1=1 
              AND IOC_CD = 'O' 
              AND VSL_CD = @[vsl_cd] 
              AND SKD_VOY_NO = @[skd_voy_no] 
              AND DIR_CD = @[skd_dir_cd] 
              AND 'O' = DECODE(@[ioc_ts_cd], 'O', 'O', 'I') 
              AND RLANE_CD = @[rlane_cd] 
           UNION ALL 
       SELECT T3.IOC_CD
            , T3.VSL_CD
            , T3.SKD_VOY_NO
            , T3.DIR_CD 
            , T3.RLANE_CD
            , T4.TRD_CD 
         FROM 
              (SELECT IOC_CD
                   , VSL_CD
                   , SKD_VOY_NO
                   , DIR_CD 
                   , MAX(RLANE_CD) AS RLANE_CD 
                FROM COA_MON_VVD 
               WHERE 1=1 
                     AND IOC_CD = DECODE(@[ioc_ts_cd], 'O', 'O', 'I') 
                     AND VSL_CD = @[vsl_cd] 
                     AND SKD_VOY_NO = @[skd_voy_no] 
                     AND DIR_CD = @[skd_dir_cd] 
                     AND RLANE_CD = @[rlane_cd]
               GROUP BY IOC_CD
                   , VSL_CD
                   , SKD_VOY_NO
                   , DIR_CD 
              ) T3 
            , MDM_DTL_REV_LANE T4 
        WHERE 1=1 
              AND T3.RLANE_CD = T4.RLANE_CD 
              AND FM_CONTI_CD IN 
              (SELECT CONTI_CD 
                FROM MDM_LOCATION 
               WHERE 1=1 
                     AND LOC_CD = SUBSTR(@[pol_yd_cd], 1, 5) 
              ) 
              AND TO_CONTI_CD IN 
              (SELECT CONTI_CD 
                FROM MDM_LOCATION 
               WHERE 1=1 
                     AND LOC_CD = SUBSTR(@[pol_yd_cd], 1, 5) 
              ) 
              AND T4.DELT_FLG = 'N' 
              AND T3.DIR_CD = T4.VSL_SLAN_DIR_CD 
              AND T3.IOC_CD = T4.IOC_CD 
              AND 'I' = DECODE(@[ioc_ts_cd], 'O', 'O', 'I') 
       ) C
     , 
       (SELECT TT1.YD_CD AS POD_YD_CD 
         FROM 
              (SELECT T7.VPS_PORT_CD 
                   , T7.YD_CD 
                   , T7.VSL_CD 
                   , T7.SKD_VOY_NO 
                   , T7.SKD_DIR_CD 
                   , T7.CLPT_SEQ
                   , ML.CONTI_CD 
                FROM VSK_VSL_PORT_SKD T7
                   , MDM_LOCATION ML 
               WHERE 1=1 
                     AND T7.VSL_CD = @[vsl_cd] 
                     AND T7.SKD_VOY_NO = @[skd_voy_no] 
                     AND T7.SKD_DIR_CD = @[skd_dir_cd] 
                     --AND T7.CLPT_IND_SEQ = '1' 
                     AND T7.VT_ADD_CALL_FLG IS  NULL

                     AND NVL(T7.SKD_CNG_STS_CD, ' ') <> 'S' 
                     AND T7.VPS_PORT_CD NOT IN ('EGSCA', 'PAPCA')
                     AND SUBSTR(T7.YD_CD,1,5) =  ML.LOC_CD

                     AND T7.CLPT_SEQ > 
                     (SELECT MIN(CLPT_SEQ) 
                       FROM VSK_VSL_PORT_SKD 
                      WHERE 1=1 
                            AND VSL_CD = @[vsl_cd] 
                            AND SKD_VOY_NO = @[skd_voy_no] 
                            AND SKD_DIR_CD =@[skd_dir_cd] 
                            --AND CLPT_IND_SEQ = '1' 
                            AND YD_CD  = @[pol_yd_cd]
                            AND VT_ADD_CALL_FLG IS  NULL 
                            AND NVL(SKD_CNG_STS_CD, ' ') <> 'S' 
                            AND VPS_PORT_CD NOT IN ('EGSCA', 'PAPCA')
                            AND TURN_PORT_IND_CD IN ('Y','N')
                     ) 
              ) TT1
            , 
              (SELECT IOC_CD
                   , VSL_CD
                   , SKD_VOY_NO
                   , DIR_CD 
                   , MAX(RLANE_CD) AS RLANE_CD 
                FROM COA_MON_VVD 
               WHERE 1=1 
                     AND IOC_CD = DECODE(@[ioc_ts_cd], 'O', 'O', 'I') 
                     AND VSL_CD = @[vsl_cd] 
                     AND SKD_VOY_NO = @[skd_voy_no] 
                     AND DIR_CD = @[skd_dir_cd] 
					 AND RLANE_CD = @[rlane_cd]
               GROUP BY IOC_CD
                   , VSL_CD
                   , SKD_VOY_NO
                   , DIR_CD 
              ) TT2
            , MDM_DTL_REV_LANE TT3
--            , MDM_LOCATION TT4 
        WHERE 1=1 
              AND TT1.VSL_CD = TT2.VSL_CD 
              AND TT1.SKD_VOY_NO = TT2.SKD_VOY_NO 
              AND TT1.SKD_DIR_CD = TT2.DIR_CD 
              AND TT2.RLANE_CD = TT3.RLANE_CD 
              AND TT1.SKD_DIR_CD = TT3.VSL_SLAN_DIR_CD 
              AND TT3.IOC_CD = DECODE(@[ioc_ts_cd], 'O', 'O', 'I') 
              AND DECODE(TT3.FM_CONTI_CD, TT3.TO_CONTI_CD, 'I', 'O') = DECODE(@[ioc_ts_cd], 'O', 'O', 'I') 

              AND TT1.CONTI_CD IN   (SELECT TT5.TO_CONTI_CD 
									   FROM MDM_DTL_REV_LANE TT5
                                      WHERE 1=1
                                        AND TT5.FM_CONTI_CD IN (SELECT CONTI_CD FROM MDM_LOCATION WHERE LOC_CD = SUBSTR(@[pol_yd_cd], 1, 5))
                                        AND TT5.RLANE_CD = TT3.RLANE_CD
                                        AND TT5.VSL_SLAN_DIR_CD = TT3.VSL_SLAN_DIR_CD
                                        AND TT5.IOC_CD = TT3.IOC_CD)

--              AND TT4.LOC_CD = TT1.VPS_PORT_CD 
--              AND TT3.TO_CONTI_CD = TT4.CONTI_CD 
              AND ROWNUM = 1 
     ORDER BY CLPT_SEQ 
       ) PS
    
     , COA_MON_VVD MV 
 WHERE O.OFC_CD = P.FCAST_OFC_CD 
       AND RL.DELT_FLG = 'N' 
       AND RL.RLANE_CD = C.RLANE_CD 
       AND C.IOC_CD = MV.IOC_CD 
       AND C.VSL_CD = MV.VSL_CD 
       AND C.SKD_VOY_NO = MV.SKD_VOY_NO 
       AND C.DIR_CD = MV.DIR_CD 
       AND C.RLANE_CD = MV.RLANE_CD 
       AND C.TRD_CD = MV.TRD_CD 
       AND C.TRD_CD = P.TRD_CD --2016.04.05 ADD
       AND O.OFC_APLY_FM_YRWK <= SUBSTR(MV.SLS_YRMON, 1, 4) || MV.COST_WK 
       AND O.OFC_APLY_TO_YRWK >= SUBSTR(MV.SLS_YRMON, 1, 4) || MV.COST_WK 
) B 
ON ( A.RLANE_CD = B.RLANE_CD 
       AND A.DIR_CD = B.DIR_CD 
       AND A.VSL_CD = B.VSL_CD 
       AND A.SKD_VOY_NO = B.SKD_VOY_NO 
       AND A.SKD_DIR_CD = B.SKD_DIR_CD 
       AND A.IOC_TS_CD = B.IOC_TS_CD 
       AND A.SREP_USR_ID = B.SREP_USR_ID 
       AND A.CUST_CNT_CD = B.CUST_CNT_CD 
       AND A.CUST_SEQ = B.CUST_SEQ 
       AND A.POL_YD_CD = B.POL_YD_CD 
       AND A.POD_YD_CD = B.POD_YD_CD 
       AND A.FCAST_OFC_CD = B.FCAST_OFC_CD
       AND A.FCAST_CUST_TP_CD = B.FCAST_CUST_TP_CD 
       AND NVL(A.CTRT_CUST_CNT_CD,'00') = NVL(B.CTRT_CUST_CNT_CD,'00')		--20160203.ADD : PK FCAST_SEQ 의미 = CTRT_CUST_CNT_CD + CTRT_CUST_SEQ
       AND NVL(A.CTRT_CUST_SEQ,0) = NVL(B.CTRT_CUST_SEQ,0)
       AND NVL(A.CTRT_NO,'00') = NVL(B.CTRT_NO,'00')  --2016.03.30 ADD
       
) 
#if (${fcast_ttl_qty} != '' || ${fcast_40ft_hc_qty} != '' || ${fcast_45ft_hc_qty} != '' || ${fcast_53ft_qty} != '' || ${fcast_rf_qty} != '' || ${fcast_ttl_wgt} != '') 
WHEN MATCHED THEN  
    UPDATE 
         SET A.UPD_USR_ID = B.UPD_USR_ID
         , A.UPD_DT = SYSDATE 
         , A.CFM_DT = SYSDATE 
         , A.CFM_USR_ID = B.UPD_USR_ID
         , A.MODI_GDT = B.MODI_GDT         
         , A.SLS_RGN_OFC_CD = B.SLS_RGN_OFC_CD  --2016.06.27 SKY SLS_RGN_OFC_CD 추가
         #if (${fcast_ttl_qty} != '')  
		 , A.FCAST_TTL_QTY = B.FCAST_TTL_QTY
         , A.CFM_TTL_QTY = B.FCAST_TTL_QTY
         #end
		 #if (${fcast_40ft_hc_qty} != '')  
         , A.FCAST_40FT_HC_QTY = B.FCAST_40FT_HC_QTY
         , A.CFM_40FT_HC_QTY = B.FCAST_40FT_HC_QTY 
         #end
		 #if (${fcast_45ft_hc_qty} != '')  
         , A.FCAST_45FT_HC_QTY = B.FCAST_45FT_HC_QTY
         , A.CFM_45FT_HC_QTY = B.FCAST_45FT_HC_QTY 
         #end
		 #if (${fcast_53ft_qty} != '')  
         , A.FCAST_53FT_QTY = B.FCAST_53FT_QTY
         , A.CFM_53FT_QTY = B.FCAST_53FT_QTY
         #end
		 #if (${fcast_rf_qty} != '')  
         , A.FCAST_RF_QTY = B.FCAST_RF_QTY
         , A.CFM_RF_QTY = B.FCAST_RF_QTY 
         #end
		 #if (${fcast_ttl_wgt} != '')  
         , A.FCAST_TTL_WGT = B.FCAST_TTL_WGT
         , A.CFM_TTL_WGT = B.FCAST_TTL_WGT         
         #end

        
#end

WHEN NOT MATCHED THEN 
    INSERT 
           ( 
               RLANE_CD 
             , DIR_CD 
             , VSL_CD 
             , SKD_VOY_NO 
             , SKD_DIR_CD 
             , IOC_TS_CD 
             , SREP_USR_ID 
             , CUST_CNT_CD 
             , CUST_SEQ 
             , POL_YD_CD 
             , POD_YD_CD 
             , FCAST_OFC_CD 
             , FCAST_CUST_TP_CD 
             , IOC_CD 
             , REP_TRD_CD 
             , REP_SUB_TRD_CD 
             , TRD_CD 
             , SUB_TRD_CD 
             , SLS_RHQ_CD 
             , SLS_AQ_CD 
             , SLS_RGN_OFC_CD 
             , SLS_OFC_CD 
             , FCAST_TTL_QTY 
             , FCAST_40FT_HC_QTY 
             , FCAST_45FT_HC_QTY 
             , FCAST_53FT_QTY 
             , FCAST_RF_QTY 
             , FCAST_TTL_WGT 
             , CFM_TTL_QTY 
             , CFM_40FT_HC_QTY 
             , CFM_45FT_HC_QTY 
             , CFM_53FT_QTY 
             , CFM_RF_QTY 
             , CFM_TTL_WGT 
             , USD_BKG_TTL_QTY 
             , USD_BKG_20FT_QTY 
             , USD_BKG_40FT_QTY 
             , USD_BKG_40FT_HC_QTY
             , USD_BKG_45FT_HC_QTY
             , USD_BKG_53FT_QTY 
             , USD_BKG_RF_QTY 
             , USD_BKG_TTL_WGT 
             , CTRL_LVL_CD 
             , MODI_GDT 
             , CFM_USR_ID 
             , CFM_DT 
             , CRE_USR_ID 
             , CRE_DT 
             , UPD_USR_ID 
             , UPD_DT 
             --20160203.ADD, 20160325.MOD
             , FCAST_SEQ 
             , CTRT_NO
             , CTRT_CUST_CNT_CD
             , CTRT_CUST_SEQ
           ) 
           VALUES 
           ( 
               B.RLANE_CD 
             , B.DIR_CD 
             , B.VSL_CD 
             , B.SKD_VOY_NO 
             , B.SKD_DIR_CD 
             , B.IOC_TS_CD 
             , B.SREP_USR_ID 
             , B.CUST_CNT_CD 
             , B.CUST_SEQ 
             , B.POL_YD_CD 
             , B.POD_YD_CD 
             , B.FCAST_OFC_CD 
             , B.FCAST_CUST_TP_CD 
             , B.IOC_CD 
             , B.REP_TRD_CD 
             , B.REP_SUB_TRD_CD 
             , B.TRD_CD 
             , B.SUB_TRD_CD 
             , B.SLS_RHQ_CD 
             , B.SLS_AQ_CD 
             , B.SLS_RGN_OFC_CD 
             , B.SLS_OFC_CD 
             , B.FCAST_TTL_QTY 
             , B.FCAST_40FT_HC_QTY 
             , B.FCAST_45FT_HC_QTY 
             , B.FCAST_53FT_QTY 
             , B.FCAST_RF_QTY 
             , B.FCAST_TTL_WGT 
             , B.CFM_TTL_QTY 
             , B.CFM_40FT_HC_QTY 
             , B.CFM_45FT_HC_QTY 
             , B.CFM_53FT_QTY 
             , B.CFM_RF_QTY 
             , B.CFM_TTL_WGT 
             , B.USD_BKG_TTL_QTY 
             , B.USD_BKG_20FT_QTY 
             , B.USD_BKG_40FT_QTY 
             , B.USD_BKG_40FT_HC_QTY
             , B.USD_BKG_45FT_HC_QTY
             , B.USD_BKG_53FT_QTY 
             , B.USD_BKG_RF_QTY 
             , B.USD_BKG_TTL_WGT 
             , B.CTRL_LVL_CD 
             , B.MODI_GDT 
             , B.CFM_USR_ID 
             , B.CFM_DT 
             , B.CRE_USR_ID 
             , B.CRE_DT 
             , B.UPD_USR_ID 
             , B.UPD_DT 
             --20160203.ADD : FCAST_SEQ = CTRT_CUST_CNT_CD + CTRT_CUST_SEQ, 20160325.MOD : SC/RFA->CTRT
             , B.FCAST_SEQ
             , B.CTRT_NO
             , B.CTRT_CUST_CNT_CD
             , B.CTRT_CUST_SEQ
           )			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
