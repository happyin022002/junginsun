<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterCustForRejectEdiRSQL">
			<desc><![CDATA[searchXterCustForRejectEdi]]></desc>
			<sql><![CDATA[
select
    xter_cust_tp_cd					    ibcs_tp     	  
    , BKG_TOKEN_NL_FNC(NVL(cust_nm,   ''), 1,'')	ibcs_nm1    	  
    , BKG_TOKEN_NL_FNC(NVL(cust_nm,   ''), 2,'')	ibcs_nm2    	  
    , BKG_TOKEN_NL_FNC(NVL(cust_addr, ''), 1,'')	ibcs_addr1  	  
    , BKG_TOKEN_NL_FNC(NVL(cust_addr, ''), 2,'')	ibcs_addr2  	  
    , BKG_TOKEN_NL_FNC(NVL(cust_addr, ''), 3,'')	ibcs_addr3  	  
    , BKG_TOKEN_NL_FNC(NVL(cntc_nm,   ''), 1,'')	ibcs_c_nm1  	  
    , BKG_TOKEN_NL_FNC(NVL(cntc_nm,   ''), 2,'')	ibcs_c_nm2  	  
    , CNT_CD				            cnt_cd			
    , TO_CHAR(CUST_seq)			        cust_cd     	  
    , loc_ctnt			                ibcs_cust_loc    
    , st_nm				                ibcs_street      
    , loc_cd				            ibcs_loc_cd      
    , loc_nm				            ibcs_loc_nm      
    , pst_ctnt				            ibcs_zip_cd      
    , XTER_CUST_CNTC_TP_CD			    ibcs_c_tp        
    , cntc_phn_no_ctnt				    ibcs_c_tel       
    , cntc_fax_no				        ibcs_c_fax       
    , cntc_eml				            ibcs_c_email   
    , BKG_TOKEN_NL_FNC(NVL(CUST_ADDR,   ''), 4,'')	IBCS_ADDR4  	  
    , BKG_TOKEN_NL_FNC(NVL(CUST_ADDR,   ''), 5,'')	IBCS_ADDR5  	  
    , UN_LOC_CD
    , PRNR_CUST_CD
    , PRNR_REF_NO
    , STE_CD
  FROM bkg_xter_cust
 where xter_sndr_id  = @[sender_id]
   and xter_rqst_no  = @[rqst_no]
   and xter_rqst_seq = @[rqst_seq]			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
