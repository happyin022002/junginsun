<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchYardInfoRSQL">
			<desc><![CDATA[....]]></desc>
			<sql><![CDATA[
SELECT FULL_RLSE_EDI_CD
  FROM BKG_CGO_RLSE A,
       BKG_EDI_YD   B,
       BKG_BOOKING  C 
 WHERE A.BL_NO   = @[bl_no]
   AND A.BL_NO   = C.BL_NO
   AND B.PORT_CD = C.POD_CD
   AND C.SLAN_CD IN (B.SLAN_CD1,B.SLAN_CD2,B.SLAN_CD3,B.SLAN_CD4,B.SLAN_CD5,
                     B.SLAN_CD6,B.SLAN_CD7,B.SLAN_CD8,B.SLAN_CD9,B.SLAN_CD10)
   AND ROWNUM    = 1			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
