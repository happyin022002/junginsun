<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDAORemoveOtherCodeDSQL">
			<desc><![CDATA[OtherCode를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM FMS_PPT_SET 
WHERE PPT_NM = @[org_ppt_nm]
AND PPT_SEQ = @[org_ppt_seq]			]]></sql>
			<params>
				<param name="org_ppt_nm" type="12" value="" out="N"/>
				<param name="org_ppt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
