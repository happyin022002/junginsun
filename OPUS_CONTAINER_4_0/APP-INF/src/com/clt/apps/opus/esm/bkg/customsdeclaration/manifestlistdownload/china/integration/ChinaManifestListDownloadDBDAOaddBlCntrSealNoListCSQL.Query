<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOaddBlCntrSealNoListCSQL">
			<desc><![CDATA[addBlCntrSealNoList]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_SEAL_NO (
    CNT_CD,
    CSTMS_DIV_ID,
    BL_NO,
    CNTR_NO,
    SEAL_NO_SEQ,
    SEAL_NO,
	SEAL_PTY_TP_CD,
	SEAL_KND_CD,
    CRE_DT,
    CRE_USR_ID,
    UPD_DT,
    UPD_USR_ID )
VALUES (
    NVL(@[cnt_cd],'CN'),
    NVL(@[cstms_div_id],'CTM'),
    @[bl_no],
    @[cntr_no],
	NVL((SELECT /*+ index_desc(A XPKBKG_CSTMS_SEAL_NO)  */
      		     SEAL_NO_SEQ
	      FROM   BKG_CSTMS_SEAL_NO A
    	  WHERE  1=1
		  AND	 CNT_CD 	  = NVL(@[cnt_cd],'CN')
          AND    CSTMS_DIV_ID = NVL(@[cstms_div_id],'CTM')
          AND    BL_NO 		  = @[bl_no]
	      AND    CNTR_NO 	  = @[cntr_no]
    	  AND    ROWNUM 	  = 1
     ),0)+1,
    @[seal_no],
    @[seal_pty_tp_cd],
    @[seal_knd_cd],
    SYSDATE,
    @[cre_usr_id],
    SYSDATE,
    @[upd_usr_id]
)			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_div_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="seal_no" type="12" value="" out="N"/>
				<param name="seal_pty_tp_cd" type="12" value="" out="N"/>
				<param name="seal_knd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
