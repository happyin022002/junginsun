<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchCNTRinqInfoListRSQL">
			<desc><![CDATA[한국세관 CNTR테이블에 다운로드된 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT KC.BKG_NO 			KC_BKG_NO
     , KC.CSTMS_DECL_TP_CD 	KC_CSTMS_DECL_TP_CD
     , KC.TRNS_SEQ 			KC_TRNS_SEQ
     , KC.CNTR_NO 			KC_CNTR_NO
     , KC.FULL_MTY_CD 		KC_FULL_MTY_CD
     , KC.CNTR_TPSZ_CD 		KC_CNTR_TPSZ_CD
     , KC.CNTR_SEAL_NO1 	KC_CNTR_SEAL_NO1
     , KC.CNTR_SEAL_NO2 	KC_CNTR_SEAL_NO2
     , KC.PCK_QTY 			KC_PCK_QTY
     , KC.PCK_TP_CD 		KC_PCK_TP_CD
     , TRUNC(DECODE(NVL(KC.WGT_UT_CD,'   '),'LBS',ROUND(NVL(KC.CNTR_WGT,0)*0.4536,3),NVL(KC.CNTR_WGT,0)),2) KC_CNTR_WGT
     , DECODE(NVL(KC.WGT_UT_CD,'   '),'LBS','KGS',NVL(KC.WGT_UT_CD,'   ')) KC_WGT_UT_CD
     , DECODE(NVL(KC.MEAS_UT_CD,'   '),'CBF',ROUND(NVL(KC.MEAS_QTY,0)*0.0283,3),NVL(KC.MEAS_QTY,0)) KC_MEAS_QTY
     , DECODE(NVL(KC.MEAS_UT_CD,'   '),'CBF','CBM',NVL(KC.MEAS_UT_CD,'   ')) KC_MEAS_UT_CD
  FROM BKG_CSTMS_KR_CNTR KC
 WHERE KC.BKG_NO = @[bkg_no]
   AND KC.CSTMS_DECL_TP_CD =   @[cstms_decl_tp_cd]
   AND KC.DMST_PORT_CD     =   @[port_cd]
   AND KC.CSTMS_BL_NO	   =   @[bl_no]
   AND KC.TRNS_SEQ         =   (
                                 SELECT MAX(TRNS_SEQ)
                                   FROM BKG_CSTMS_KR_BL
                                  WHERE BKG_NO = @[bkg_no]
                                    AND CSTMS_DECL_TP_CD = @[cstms_decl_tp_cd]
                                    AND DMST_PORT_CD     = @[port_cd]
  									AND CSTMS_BL_NO		 = @[bl_no]
                                    AND VSL_CD           = substr(@[vvd], 1, 4)
                                    AND SKD_VOY_NO       = substr(@[vvd], 5, 4)
                                    AND SKD_DIR_CD       = substr(@[vvd], 9, 1)
                                )
ORDER BY KC_FULL_MTY_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
