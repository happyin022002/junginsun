<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsCustomsTransmissionDBDAOmodifyBlAnrMsgStsCdUSQL">
			<desc><![CDATA[U]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ANR_BL
SET
ANR_MSG_STS_CD = ''
WHERE 1=1
AND BL_NO = @[bl_no]
AND VSL_CD     = SUBSTR(@[vvd],1,4)
AND SKD_VOY_NO = SUBSTR(@[vvd],5,4)
AND SKD_DIR_CD = SUBSTR(@[vvd],9,1)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
