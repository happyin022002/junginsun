<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchCust301HpswaInfoRSQL">
			<desc><![CDATA[GeneralBookingSearchDBDAOsearchCust301HpswaInfoRSQL]]></desc>
			<sql><![CDATA[
SELECT '{HPSWA_INFO'               || CHR(10)
         || 'SHIP_ID:'  ||DE_NO             || CHR(10)
         || 'PART_NO:'  ||PRT_NO            || CHR(10)
         || '}HPSWA_INFO'                   AS HPSWA_INFO
FROM BKG_REF_DTL BRD
WHERE BRD.BKG_NO = @[bkg_no]
AND   (BRD.DE_NO IS NOT NULL OR BRD.PRT_NO IS NOT NULL )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
