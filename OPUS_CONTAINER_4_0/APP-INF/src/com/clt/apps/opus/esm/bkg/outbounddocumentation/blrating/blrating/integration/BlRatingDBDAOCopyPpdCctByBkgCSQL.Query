<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCopyPpdCctByBkgCSQL">
			<desc><![CDATA[booking copy시 ppd ofc와 cct ofc를 copy해준다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_RATE
        (BKG_NO
        , PPD_RCV_OFC_CD 
        , CLT_OFC_CD
        , FRT_TERM_CD
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT)
SELECT   @[target_bkg_no]
        , PPD_RCV_OFC_CD 
        , CLT_OFC_CD
        , FRT_TERM_CD
        , @[usr_id]
        , SYSDATE
        , @[usr_id]
        , SYSDATE
  FROM BKG_RATE
 WHERE BKG_NO = @[source_bkg_no]   			]]></sql>
			<params>
				<param name="target_bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="source_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
