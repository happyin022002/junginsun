<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOUpdateXterRqstCfmUSQL">
			<desc><![CDATA[bkg_xter_rqst_mst에 upload 되었음을 기혹한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_RQST_MST
   SET BKG_NO = @[bkg_no]
	 , UPLD_USR_ID = @[usr_id]
	 , UPLD_DT = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,(SELECT POL_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no]))
	 , UPLD_GDT = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,'GMT' )
	 , UPD_DT = SYSDATE
 	 , UPD_USR_ID = @[usr_id]
	 , HNDL_OFC_CD = nvl(HNDL_OFC_CD, @[ofc_cd])
	 , BKG_UPLD_STS_CD = 'F'
 WHERE XTER_SNDR_ID  = @[sender_id]
   AND XTER_RQST_NO  = @[rqst_no]
   AND XTER_RQST_SEQ = @[rqst_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
