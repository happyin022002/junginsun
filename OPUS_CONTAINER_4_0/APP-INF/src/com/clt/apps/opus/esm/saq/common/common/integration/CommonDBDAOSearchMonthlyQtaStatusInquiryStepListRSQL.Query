<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchMonthlyQtaStatusInquiryStepListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT					
    CODE, 				
    TEXT				
FROM					
    (					
    SELECT '0' AS CODE,		
	   ' ' AS TEXT		
    FROM DUAL       			
    UNION ALL				
    SELECT 				
	TO_CHAR(INTG_CD_VAL_DP_SEQ) AS CODE,	
	INTG_CD_VAL_DP_DESC AS TEXT			
    					
    FROM COM_INTG_CD_DTL		
    					
    WHERE 				
		INTG_CD_ID = 'CD01214'			
    AND INTG_CD_VAL_DP_SEQ NOT IN (6,7,8,10)				
    )					
    					
ORDER BY				
    LPAD(CODE, 2, '0') 				]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
