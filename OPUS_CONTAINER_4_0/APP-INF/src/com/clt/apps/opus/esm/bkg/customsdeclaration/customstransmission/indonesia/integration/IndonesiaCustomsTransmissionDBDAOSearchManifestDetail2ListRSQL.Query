<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndonesiaCustomsTransmissionDBDAOSearchManifestDetail2ListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(REPLACE(TRIM(A.CUST_NM), CHR(13)||CHR(10), CHR(10)), '-') AS SHPR_CUST_NM,
       NVL(REPLACE(TRIM(A.CUST_ADDR), CHR(13)||CHR(10), CHR(10)), '-') AS SHPR_CUST_ADDR,
       NVL(REPLACE(TRIM(B.CUST_NM), CHR(13)||CHR(10), CHR(10)), '-') AS CNEE_CUST_NM,
       NVL(REPLACE(TRIM(B.CUST_ADDR), CHR(13)||CHR(10), CHR(10)), '-') AS CNEE_CUST_ADDR,
       NVL(REPLACE(TRIM(C.CUST_NM), CHR(13)||CHR(10), CHR(10)), '-') AS NTFY_CUST_NM,
       NVL(REPLACE(TRIM(C.CUST_ADDR), CHR(13)||CHR(10), CHR(10)), '-') AS NTFY_CUST_ADDR,
       NVL(REPLACE(TRIM(D.MK_DESC), CHR(13)||CHR(10), CHR(10)), '-') AS MK_DESC,
       NVL(REPLACE(TRIM(E.PCK_CMDT_DESC||DECODE(E.PCK_CMDT_DESC, NULL, E.PCK_CMDT_DESC, CHR(10))|| E.CNTR_CMDT_DESC ||DECODE(E.CNTR_CMDT_DESC, NULL, E.CNTR_CMDT_DESC, CHR(10))|| D.CMDT_DESC), CHR(13)||CHR(10), CHR(10)), '-') AS CMDT_DESC

  FROM BKG_CUSTOMER A,
       BKG_CUSTOMER B,
       BKG_CUSTOMER C,
       BKG_BL_MK_DESC D,
       BKG_BL_DOC E
 
 WHERE A.BKG_NO(+) = @[bkg_no]
   AND A.BKG_CUST_TP_CD(+) = 'S'
   AND B.BKG_NO(+) = A.BKG_NO
   AND B.BKG_CUST_TP_CD(+) = 'C'
   AND C.BKG_NO(+) = A.BKG_NO
   AND C.BKG_CUST_TP_CD(+) = 'N'
   AND D.BKG_NO(+) = A.BKG_NO
   AND (D.MK_SEQ = 1 OR D.MK_SEQ IS NULL)
   AND E.BKG_NO(+) = A.BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
