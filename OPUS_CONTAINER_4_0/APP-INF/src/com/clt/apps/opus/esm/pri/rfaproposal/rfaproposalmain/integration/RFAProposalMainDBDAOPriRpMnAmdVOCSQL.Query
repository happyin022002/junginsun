<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpMnAmdVOCSQL">
			<desc><![CDATA[RFAProposalMainDBDAO PriRpMnAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_MN (
	PROP_NO
,	AMDT_SEQ
,	EFF_DT
,	EXP_DT
,	CTRT_CUST_CNT_CD
,	CTRT_CUST_SEQ
,	PRC_CTRT_CUST_TP_CD
,	PROP_SREP_CD
,	PROP_OFC_CD
,	PROP_APRO_OFC_CD
,	PROP_APRO_DT
,	PROP_STS_CD
,	RESPB_SREP_CD
,	RESPB_SLS_OFC_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,   TGT_MVC_QTY
,   CNTR_LOD_UT_CD
,   TRF_CTRT_FLG
,   CTRT_DUR_TP_CD
) 
SELECT
    PROP_NO
    ,@[amdt_seq]+1
    ,TO_DATE(@[eff_dt],'YYYY-MM-DD')
	,DECODE(@[exp_dt],null,EXP_DT,TO_DATE(@[exp_dt],'YYYY-MM-DD'))
	,CTRT_CUST_CNT_CD
	,CTRT_CUST_SEQ
	,PRC_CTRT_CUST_TP_CD
    ,PROP_SREP_CD
    ,PROP_OFC_CD
    ,''
    ,''
    ,'I'
    ,RESPB_SREP_CD
    ,RESPB_SLS_OFC_CD
    ,@[cre_usr_id]
    ,SYSDATE
    ,@[upd_usr_id]
    ,SYSDATE
,   TGT_MVC_QTY
,    CNTR_LOD_UT_CD
,   TRF_CTRT_FLG
,   CTRT_DUR_TP_CD
FROM PRI_RP_MN
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
