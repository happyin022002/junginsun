<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOsearchKrWhfTtlAmtRSQL">
			<desc><![CDATA[s]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(WHF_AMT),0) AS WHF_RT_AMT
  FROM BKG_KR_WHF_BL
 WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND DECODE(SUBSTR(@[whf_bnd_cd], 1, 1), 'O', WHF_POL_CD, WHF_POD_CD) = @[port_cd]
   AND WHF_BL_STS_CD <> 'D'
   AND WHF_BND_CD = DECODE(@[whf_bnd_cd], 'OO', 'OO', 'II', 'II', 'IN', 'II')			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="whf_bnd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
