<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyInbondArrFlagByBlUSQL">
			<desc><![CDATA[dwkim Arrval 전송 플래그 갱신 at IBD]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_IBD
	SET IBD_TRSP_ARR_SND_FLG = DECODE(@[trans_gubun], 'Arr', 'Y', IBD_TRSP_ARR_SND_FLG),
 		IBD_TRSP_XPT_SND_FLG = DECODE(@[trans_gubun], 'Exp', 'Y', IBD_TRSP_XPT_SND_FLG)
WHERE BL_NO = @[bl_no]
	AND CNT_CD = 'US'			]]></sql>
			<params>
				<param name="trans_gubun" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
