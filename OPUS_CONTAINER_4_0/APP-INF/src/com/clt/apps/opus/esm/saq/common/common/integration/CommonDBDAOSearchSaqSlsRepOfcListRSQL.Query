<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqSlsRepOfcListRSQL">
			<desc><![CDATA[분기 S.Rep : User의 RGN OFC CD 하위의 OFC CD조회]]></desc>
			<sql><![CDATA[
SELECT  OFC_CD  TEXT,
        OFC_CD  CODE
FROM    SAQ_ORGANIZATION_V
WHERE   1 = 1
AND     N4TH_PRNT_OFC_CD    = @[rgnOfcCd]
AND     DELT_FLG            = 'N'
ORDER BY
        LVL			]]></sql>
			<params>
				<param name="rgnOfcCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
