<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOPriSpMnAmdVOCSQL">
			<desc><![CDATA[SCProposalMainDBDAO PriSpMnAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_MN(
    PROP_NO,
    AMDT_SEQ,
    EFF_DT,
    EXP_DT,
    FILE_DT,
    PROP_SREP_CD,
    PROP_OFC_CD,
    PROP_APRO_OFC_CD,
    PROP_APRO_DT,
    PROP_STS_CD,
    RESPB_SREP_CD,
    RESPB_SLS_OFC_CD,
    REAL_CUST_CNT_CD,
    REAL_CUST_SEQ,
    REAL_CUST_TP_CD,
    REAL_CUST_SREP_CD,
    REAL_CUST_SLS_OFC_CD,
    RF_FLG,
    GAMT_FLG,
    BLPL_HDR_SEQ,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
    PRXY_FLG,
    CTRT_DUR_TP_CD
)
SELECT
    PROP_NO,
    @[amdt_seq]+1,
    TO_DATE(@[eff_dt],'YYYY-MM-DD'),
    EXP_DT,
    '',
    PROP_SREP_CD,
    PROP_OFC_CD,
    PROP_APRO_OFC_CD,
    '',
    'I',
    RESPB_SREP_CD,
    RESPB_SLS_OFC_CD,
    REAL_CUST_CNT_CD,
    REAL_CUST_SEQ,
    REAL_CUST_TP_CD,
    REAL_CUST_SREP_CD,
    REAL_CUST_SLS_OFC_CD,
    RF_FLG,
    GAMT_FLG,
    BLPL_HDR_SEQ,
    @[cre_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE,
    PRXY_FLG,
    CTRT_DUR_TP_CD
FROM 
    PRI_SP_MN 
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
