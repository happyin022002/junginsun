<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteConversionProposalDBDAOPriScNoteConvCopyVORSQL">
			<desc><![CDATA[COPY된 데이터를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT    '' NOTE_CONV_MAPG_ID
		, '0' NOTE_CONV_SEQ
		, NOTE_CONV_TP_CD
		, SVC_SCP_CD
		, PROP_NO
		, AMDT_SEQ
		, CASE WHEN CHG_RULE_TP_CD = 'C' THEN NOTE_CONV_CHG_CD
			   WHEN CHG_RULE_TP_CD = 'R' THEN NOTE_CONV_RULE_CD
			   END CHG_RULE_DEF_CD
		, CHG_RULE_TP_CD
		, NOTE_CONV_CHG_CD
		, NOTE_CONV_RULE_CD
		, TO_CHAR(EFF_DT,'YYYYMMDD') EFF_DT
		, TO_CHAR(EXP_DT,'YYYYMMDD') EXP_DT
		, RT_APPL_TP_CD
		, RT_OP_CD
		, CURR_CD
		, FRT_RT_AMT
		, PAY_TERM_CD
		, GEN_SPCL_RT_TP_CD
		, BKG_RAT_UT_CD
		, BKG_PRC_CGO_TP_CD
		, BKG_IMDG_CLSS_CD
		, BKG_CMDT_TP_CD
		, BKG_CMDT_DEF_CD
		, BKG_SCG_GRP_CMDT_CD
		, BKG_USA_SVC_MOD_CD
		, BKG_POR_TP_CD
		, DECODE(BKG_POR_TP_CD, 'T', SUBSTR(BKG_POR_DEF_CD, 3), BKG_POR_DEF_CD) BKG_POR_DEF_CD
		, BKG_POL_TP_CD
		, DECODE(BKG_POL_TP_CD, 'T', SUBSTR(BKG_POL_DEF_CD, 3), BKG_POL_DEF_CD) BKG_POL_DEF_CD
		, BKG_POD_TP_CD
		, DECODE(BKG_POD_TP_CD, 'T', SUBSTR(BKG_POD_DEF_CD, 3), BKG_POD_DEF_CD) BKG_POD_DEF_CD
		, BKG_DEL_TP_CD
		, DECODE(BKG_DEL_TP_CD, 'T', SUBSTR(BKG_DEL_DEF_CD, 3), BKG_DEL_DEF_CD) BKG_DEL_DEF_CD
		, BKG_ORG_TRSP_MOD_CD
		, BKG_DEST_TRSP_MOD_CD
		, BKG_RCV_TERM_CD
		, BKG_DE_TERM_CD
		, BKG_SLAN_CD
		, (BKG_VSL_CD||BKG_SKD_VOY_NO||BKG_SKD_DIR_CD) BKG_VVD_CD
		, BKG_VSL_CD
		, BKG_SKD_VOY_NO
		, BKG_SKD_DIR_CD
		, BKG_SOC_FLG
		, (BKG_ACT_CUST_CNT_CD||LPAD(BKG_ACT_CUST_SEQ,6,0)) BKG_ACT_CUST_DEF_CD
		, BKG_ACT_CUST_CNT_CD
		, BKG_ACT_CUST_SEQ
		, BKG_TS_PORT_TP_CD
		, BKG_TS_PORT_DEF_CD
		, BKG_DIR_CALL_FLG
		, BKG_MST_HBL_TP_CD
		, CONV_RAT_UT_CD
		, CONV_PRC_CGO_TP_CD
		, CONV_CMDT_TP_CD
		, CONV_CMDT_DEF_CD
		, CONV_ORG_LOC_TP_CD
		, DECODE(CONV_ORG_LOC_TP_CD, 'T', SUBSTR(CONV_ORG_LOC_DEF_CD, 3), CONV_ORG_LOC_DEF_CD) CONV_ORG_LOC_DEF_CD
		, CONV_ORG_VIA_LOC_TP_CD
		, DECODE(CONV_ORG_VIA_LOC_TP_CD, 'T', SUBSTR(CONV_ORG_VIA_LOC_DEF_CD, 3), CONV_ORG_VIA_LOC_DEF_CD) CONV_ORG_VIA_LOC_DEF_CD
		, CONV_DEST_VIA_LOC_TP_CD
		, DECODE(CONV_DEST_VIA_LOC_TP_CD, 'T', SUBSTR(CONV_DEST_VIA_LOC_DEF_CD, 3), CONV_DEST_VIA_LOC_DEF_CD) CONV_DEST_VIA_LOC_DEF_CD
		, CONV_DEST_LOC_TP_CD
		, DECODE(CONV_DEST_LOC_TP_CD, 'T', SUBSTR(CONV_DEST_LOC_DEF_CD, 3), CONV_DEST_LOC_DEF_CD) CONV_DEST_LOC_DEF_CD
		, CONV_PRC_RCV_TERM_CD
		, CONV_PRC_DE_TERM_CD
		, NOTE_HDR_SEQ
		, RULE_APPL_CHG_TP_CD

		, DECODE(BKG_POR_TP_CD, 'T', SUBSTR(BKG_POR_DEF_CD, 1, 2), '') BKG_POR_CNT_CD
		, DECODE(BKG_POL_TP_CD, 'T', SUBSTR(BKG_POL_DEF_CD, 1, 2), '') BKG_POL_CNT_CD
		, DECODE(BKG_POD_TP_CD, 'T', SUBSTR(BKG_POD_DEF_CD, 1, 2), '') BKG_POD_CNT_CD
		, DECODE(BKG_DEL_TP_CD, 'T', SUBSTR(BKG_DEL_DEF_CD, 1, 2), '') BKG_DEL_CNT_CD
		, DECODE(CONV_ORG_LOC_TP_CD, 'T', SUBSTR(CONV_ORG_LOC_DEF_CD, 1, 2), '') 			CONV_ORG_LOC_CNT_CD
		, DECODE(CONV_ORG_VIA_LOC_TP_CD, 'T', SUBSTR(CONV_ORG_VIA_LOC_DEF_CD, 1, 2), '') 	CONV_ORG_VIA_LOC_CNT_CD
		, DECODE(CONV_DEST_VIA_LOC_TP_CD, 'T', SUBSTR(CONV_DEST_VIA_LOC_DEF_CD, 1, 2), '') 	CONV_DEST_VIA_LOC_CNT_CD
		, DECODE(CONV_DEST_LOC_TP_CD, 'T', SUBSTR(CONV_DEST_LOC_DEF_CD, 1, 2), '') 			CONV_DEST_LOC_CNT_CD
		, BKG_IO_GA_CD
		, BKG_CNL_TZ_CD
		, BKG_ESVC_TP_CD
        , BKG_MIN_CGO_WGT
        , BKG_MAX_CGO_WGT
	 FROM PRI_SC_NOTE_CONV_CPY
    WHERE USR_ID = @[cre_usr_id]
	  AND SVC_SCP_CD = @[svc_scp_cd]
	ORDER BY CHG_RULE_DEF_CD			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
