<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentRHQDBDAOSearchAdjustmentRhqLoadZero0075ListRSQL">
			<desc><![CDATA[trd_cd, dir_cd, mqta_ver_no의 조회키별 Load 합이 0인 월을 체크하여 문자열로 리턴.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(TO_DATE(BSE_MON, 'MM'), 'MON', 'NLS_DATE_LANGUAGE=AMERICAN')
       ||' / '||RHQ.RLANE_CD||'-'||RHQ.SPRT_GRP_CD||'-'||RHQ.BSA_GRP_CD
       ||' / '||CTRT_RGN_OFC_CD  AS MSG,
       SUB_TRD_CD
FROM SAQ_MON_QTA_RHQ RHQ
WHERE  RHQ.MQTA_STEP_CD = @[mQtaStepCd]
AND    RHQ.BSE_YR = @[year]
AND    RHQ.BSE_QTR_CD = @[bse_quarter]
AND    RHQ.TRD_CD = @[trade]
AND    RHQ.DIR_CD = @[bound]
AND    RHQ.MQTA_VER_NO = @[mQtaVerNo]
AND    LOD_QTY = 0
ORDER BY BSE_MON, SUB_TRD_CD,DECODE(RHQ.RLANE_CD,'RBCCO','ZZ',SUBSTR(RHQ.RLANE_CD,-2)), RHQ.RLANE_CD, RHQ.SPRT_GRP_CD,
         RHQ.BSA_GRP_CD,  CTRT_RGN_OFC_CD			]]></sql>
			<params>
				<param name="mQtaStepCd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_quarter" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="mQtaVerNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
