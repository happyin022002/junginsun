<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateProposalDBDAOPriSpBlplHistoryCSQL">
			<desc><![CDATA[insert the history of PRI_SP_BLPL to PRI_SP_BLPL_MIG_HIS]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_BLPL_MIG_HIS (
	PROP_NO
,	AMDT_SEQ
,	BLPL_SEQ
,    BKUP_DT
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	BLPL_TIT_NM
,	DP_SEQ
,	N1ST_CMNC_AMDT_SEQ
,	ACPT_USR_ID
,	ACPT_OFC_CD
,	ACPT_DT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT
	PROP_NO
,	AMDT_SEQ
,	BLPL_SEQ
,    SYSDATE
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	BLPL_TIT_NM
,	DP_SEQ
,	N1ST_CMNC_AMDT_SEQ
,	ACPT_USR_ID
,	ACPT_OFC_CD
,	ACPT_DT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
  FROM PRI_SP_BLPL
 WHERE PROP_NO = @[prop_no]
   AND AMDT_SEQ = @[amdt_seq]
   AND BLPL_SEQ = @[blpl_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="blpl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
