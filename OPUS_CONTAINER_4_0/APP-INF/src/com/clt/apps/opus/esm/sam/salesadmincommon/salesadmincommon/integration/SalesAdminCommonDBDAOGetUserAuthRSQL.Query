<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesAdminCommonDBDAOGetUserAuthRSQL">
			<desc><![CDATA[User의 권한에 따른 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT USR_ROLE_CD
FROM COM_USR_ROLE_MTCH
WHERE USR_ID = @[usr_id]
AND USR_ROLE_CD = 'SAM01'			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
