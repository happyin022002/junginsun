<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentTradeDBDAOCreateSaqMonQtaOfcModMix0048CSQL">
			<desc><![CDATA[SAQ_MON_QTA_OFC_MOD_MIX 특정 조건의 데이터를 Version을 바꿔 Copy한다.(추가)]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_MON_QTA_OFC_MOD_MIX(
         MQTA_STEP_CD,
         MQTA_VER_NO,
         BSE_YR,
         BSE_QTR_CD,
         TRD_CD,
         DIR_CD,
         RLANE_CD,
         SPRT_GRP_CD,
         BSA_GRP_CD,
         CTRT_RHQ_CD,
         BSE_MON,
         SAQ_SVC_MOD_CD,
         MOD_MIX_RTO,
         SUB_TRD_CD,
         SVC_MOD_ADJ_AVAL_FLG,
         CRE_USR_ID,
         CRE_DT,
         UPD_USR_ID,
         UPD_DT
 )
SELECT
    	 @[newStepCd],
    	 @[newVersion],
         BSE_YR,
         BSE_QTR_CD,
         TRD_CD,
         DIR_CD,
         RLANE_CD,
         SPRT_GRP_CD,
         BSA_GRP_CD,
         CTRT_RHQ_CD,
         BSE_MON,
         SAQ_SVC_MOD_CD,
         MOD_MIX_RTO,
         SUB_TRD_CD,
         SVC_MOD_ADJ_AVAL_FLG,
         @[userId],
         SYSDATE,
         @[userId],
         SYSDATE
FROM   SAQ_MON_QTA_OFC_MOD_MIX
WHERE
    MQTA_STEP_CD = @[mQtaStepCd] AND
    BSE_YR = @[year] AND
    BSE_QTR_CD = @[bse_quarter] AND
    TRD_CD = @[trade] AND
    DIR_CD = @[bound] AND
    MQTA_VER_NO = @[mQtaVerNo]  AND
    CTRT_RHQ_CD = DECODE(@[ctrtOfcCd], NULL , CTRT_RHQ_CD, @[ctrtOfcCd])			]]></sql>
			<params>
				<param name="newStepCd" type="12" value="" out="N"/>
				<param name="newVersion" type="12" value="" out="N"/>
				<param name="userId" type="12" value="" out="N"/>
				<param name="mQtaStepCd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_quarter" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="mQtaVerNo" type="12" value="" out="N"/>
				<param name="ctrtOfcCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
