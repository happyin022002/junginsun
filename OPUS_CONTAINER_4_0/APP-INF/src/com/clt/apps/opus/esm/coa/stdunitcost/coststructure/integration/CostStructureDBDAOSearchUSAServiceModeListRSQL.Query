<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOSearchUSAServiceModeListRSQL">
			<desc><![CDATA[SearchUSAServiceModeList]]></desc>
			<sql><![CDATA[
SELECT /*+ index_asc(COA_USA_SVC_MOD XPKCOA_USA_SVC_MOD)*/ 
	ORG_RGN_CD,
	DEST_RGN_CD,
	SVC_MOD_CD
FROM COA_USA_SVC_MOD
WHERE 1 = 1
#if (${org_rgn_cd} != ''&& ${org_rgn_cd} !='All') 
	AND ORG_RGN_CD = @[org_rgn_cd]
#end
#if (${dest_rgn_cd} != ''&& ${dest_rgn_cd} !='All') 
	AND DEST_RGN_CD = @[dest_rgn_cd]
#end
#if (${svc_mod_cd} != ''&& ${svc_mod_cd} !='All') 
	AND SVC_MOD_CD = @[svc_mod_cd]
#end			]]></sql>
			<params>
				<param name="org_rgn_cd" type="12" value="" out="N"/>
				<param name="dest_rgn_cd" type="12" value="" out="N"/>
				<param name="svc_mod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
