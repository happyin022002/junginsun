<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAODeleteBlPrnAuthUSQL">
			<desc><![CDATA[BKG_INET_BL_PRN_AUTH delete flag 처리]]></desc>
			<sql><![CDATA[
UPDATE BKG_INET_BL_PRN_AUTH
    SET DELT_FLG = 'Y'
        , DELT_USR_ID 	= @[usr_id]
        , DELT_OFC_CD 	= @[ofc_cd]
        , DELT_DT 		= sysdate
        , UPD_USR_ID 	= @[usr_id]
        , UPD_DT 		= sysdate
WHERE BKG_NO = @[bkg_no]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
