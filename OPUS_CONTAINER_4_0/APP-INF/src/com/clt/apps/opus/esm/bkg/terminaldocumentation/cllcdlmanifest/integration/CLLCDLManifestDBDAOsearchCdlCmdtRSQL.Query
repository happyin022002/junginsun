<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCdlCmdtRSQL">
			<desc><![CDATA[searchCdlCmdt]]></desc>
			<sql><![CDATA[
SELECT	TRANSLATE(NVL(COM.CMDT_NM,' '),CHR(10),' ') CMDT_NM
FROM	BKG_BOOKING BKG, MDM_COMMODITY COM
WHERE	BKG.CMDT_CD	= COM.CMDT_CD
AND	BKG.BKG_NO	= @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
