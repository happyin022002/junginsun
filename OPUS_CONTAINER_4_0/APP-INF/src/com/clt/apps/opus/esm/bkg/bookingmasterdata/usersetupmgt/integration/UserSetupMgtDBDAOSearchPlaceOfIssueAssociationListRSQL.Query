<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOSearchPlaceOfIssueAssociationListRSQL">
			<desc><![CDATA[SearchPlaceOfIssueAssociationList]]></desc>
			<sql><![CDATA[
SELECT A.BL_ESIG_ASGN_SEQ
    ,(SELECT MC.SCONTI_CD FROM MDM_COUNTRY MC WHERE MC.CNT_CD = A.CNT_CD) AS REGION_NM
	, A.CNT_CD
    , A.BL_ISS_OFC_NM
    , A.BL_ISS_OFC_CD
	, B.ESIG_LST_NM || ',' || ESIG_N1ST_NM AS ISS_OFC_EMP_NM
    , A.AGN_CO_NM1
    , A.AGN_CO_NM2
    , A.AGN_CO_NM3
    , A.BL_ESIG_SEQ
    , A.BL_ESIG_RMK    
    , A.BL_ESIG_FLG
    , A.BL_CPY_ESIG_FLG
    , A.BL_KNT_FLG
FROM BKG_BL_ESIG_OFC_ASGN A
    ,BKG_BL_ESIG B
WHERE 1=1
  AND A.BL_ESIG_SEQ = B.BL_ESIG_SEQ(+)
#if (${cnt_cd} != '')
	AND A.CNT_CD = @[cnt_cd]
#end
#if (${bl_iss_ofc_nm} != '')
	AND UPPER(A.BL_ISS_OFC_NM) LIKE '%' || UPPER(@[bl_iss_ofc_nm]) || '%'
#end
#if (${bl_esig_asgn_seq} != '')
	AND A.BL_ESIG_ASGN_SEQ = @[bl_esig_asgn_seq]
#end
#if(${bl_iss_ofc_cd} != '')
	AND A.BL_ISS_OFC_CD = @[bl_iss_ofc_cd]
#end			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_iss_ofc_nm" type="12" value="" out="N"/>
				<param name="bl_esig_asgn_seq" type="12" value="" out="N"/>
				<param name="bl_iss_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
