<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCFlag4AMaxSeqRSQL">
			<desc><![CDATA[searchCFlag4AMaxSeq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(CSTMS_SEQ), -1) AS SEQ
  FROM BKG_CSTMS_ADV_RSLT 
 WHERE CNT_CD = 'US'
   AND BL_NO  = @[bl_no]
   AND DSPO_CD = '4A'
   AND NVL(RSND_FLG,'N') <> 'Y'
   AND BL_NVOCC_TP_CD = 'M'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
