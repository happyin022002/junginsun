<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOaddDgListCSQL">
			<desc><![CDATA[위험물 정보들을 신규생성한다.]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_CSTMS_EUR_DG
      (EUR_DG_DECL_TP_CD
      ,VSL_CD
      ,SKD_VOY_NO
      ,SKD_DIR_CD
      ,PORT_CD
      ,CNTR_NO
      ,CNTR_CGO_SEQ
      ,CNTR_TPSZ_CD
      ,BL_NO
      ,POL_CD
      ,POD_CD
      ,CELL_PSN_NO
      ,ANR_FWRD_ID
      ,SVC_RQST_NO
      ,ANR_CRR_TP_CD
      ,FDR_SVC_RQST_NO
      ,FDR_VVD_ID
      ,FDR_VSL_NM
      ,FDR_VSL_LLOYD_NO
      ,IMDG_PCK_GRP_CD
      ,IMDG_CLSS_CD
      ,IMDG_UN_NO
      ,IMDG_UN_NO_SEQ
      ,ANR_SPCL_TP_ID
      ,FLSH_PNT_CDO_TEMP
      ,NET_WGT
      ,GRS_WGT
      ,MFAG_NO
      ,PRP_SHP_NM
      ,HZD_DESC
      ,IMDG_LMT_QTY_FLG
      ,PCK_QTY
      ,PCK_TP_CD
      ,OUT_IMDG_PCK_QTY1
      ,OUT_IMDG_PCK_CD1
      ,EUR_OUTR_PCK_DESC
      ,IN_IMDG_PCK_QTY1
      ,IN_IMDG_PCK_CD1
      ,EUR_INR_PCK_DESC
      ,EMS_NO
      ,ANR_ROLE_DIV_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,EUR_PCK_DESC
      ,EUR_DCGO_MRN_POLUT_CD
      ,XTD_STAY_PRMT_NO
      ,DIFF_RMK
      ,CRR_DT
      ,NET_EXPLO_WGT
      ,CGO_OPR_CD
      ,APLY_NO
      ,CNTR_REF_NO
      ,DCGO_REF_NO
      ,EMER_CNTC_PHN_NO
      ,EMER_CNTC_PSON_NM
      ,IMDG_SUBS_RSK_LBL_CD1
      ,IMDG_SUBS_RSK_LBL_CD2
      ,IMDG_SUBS_RSK_LBL_CD3
      ,IMDG_SUBS_RSK_LBL_CD4
      ,HCDG_FLG
      ,EMER_RSPN_GID_NO
      ,SPCL_CNTR_SEQ
      ,SPCL_CGO_SEQ
     )
VALUES(@[d_type]
      ,SUBSTR(@[vvd_cd], 1, 4)
      ,SUBSTR(@[vvd_cd], 5, 4)
      ,SUBSTR(@[vvd_cd], 9, 1)
      ,@[port_cd]
      ,@[cntr_no]
      ,(SELECT NVL(MAX(CNTR_CGO_SEQ), 0) + 1 
          FROM BKG_CSTMS_EUR_DG
         WHERE EUR_DG_DECL_TP_CD = @[d_type]
           AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
           AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
           AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
           AND PORT_CD    = @[port_cd]
           AND BL_NO      = @[bl_no]
           AND CNTR_NO    = @[cntr_no]
           AND CGO_OPR_CD = @[cgo_opr_cd]
       )
      ,@[cntr_tpsz_cd]
      ,@[bl_no]
      ,@[pol_cd]
      ,@[pod_cd]
      ,NVL(LPAD(@[cell_psn_no], 7,0 ),(SELECT LPAD(OPF.VSL_BAY_NO || OPF.VSL_ROW_NO || OPF.VSL_TR_NO, 7,0 ) CELL_PSN_NO
                					   FROM OPF_BAY_PLN_LDIS OPF
                                       WHERE 1=1
                                       AND OPF.VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
                					   AND OPF.SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
									   AND OPF.SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
             					  	   AND OPF.POL_CD = @[pol_cd]
             					  	   AND OPF.POD_CD = @[pod_cd]
                					   AND OPF.CLPT_IND_SEQ = '1'
									   AND OPF.CNTR_REF_NO = @[cntr_no]
									   AND ROWNUM = 1))
      ,@[fwrd_id]
      ,@[svc_rqst_no]
      ,@[c_type]
      ,@[fdr_svc_rqst_no]
      ,@[fdr_vvd_id]
      ,@[fdr_vsl_nm]
      ,@[fdr_vsl_lloyd_no]
      ,@[imdg_pck_grp_cd]
      ,@[imdg_clss_cd]
      ,@[imdg_un_no]
      ,@[imdg_un_no_seq]
      ,@[dg_short_nm]
      ,@[flsh_pnt_cdo_temp]
      ,@[net_wgt]
      ,@[grs_wgt]
      ,@[mfag_no]
      ,@[prp_shp_nm]
      ,@[hzd_desc]
      ,@[imdg_lmt_qty_flg]
      ,NVL(@[out_imdg_pck_qty1], 0)
      ,@[pck_tp_cd]
      ,NVL(@[out_imdg_pck_qty1], 0)
      ,@[out_imdg_pck_cd1]
      ,NVL(@[eur_outr_pck_desc],NVL(@[out_imdg_pck_qty1], 0)||' '||(SELECT IMDG_PCK_DESC
                                           FROM SCG_IMDG_PCK_CD
                                           WHERE IMDG_PCK_CD = @[out_imdg_pck_cd1]))
      ,NVL(@[in_imdg_pck_qty1], 0)
      ,@[in_imdg_pck_cd1]
      ,@[eur_inr_pck_desc]
      ,@[ems_no]
      ,@[agent]
      ,@[cre_usr_id]   
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE
      ,NVL(@[eur_outr_pck_desc],NVL(@[out_imdg_pck_qty1], 0)||' '||(SELECT IMDG_PCK_DESC
                                           FROM SCG_IMDG_PCK_CD
                                           WHERE IMDG_PCK_CD = @[out_imdg_pck_cd1]))
      ,@[eur_dcgo_mrn_polut_cd]
      ,@[xtd_stay_prmt_no]
      ,@[diff_rmk]
      ,TO_DATE(@[crr_dt],'YYYY-MM-DD')
      ,@[net_explo_wgt]
      ,@[cgo_opr_cd]
      ,@[aply_no]
      ,@[cntr_ref_no]
      ,@[dcgo_ref_no]
      ,@[emer_cntc_phn_no]
      ,@[emer_cntc_pson_nm]
      ,@[imdg_subs_rsk_lbl_cd1]
      ,@[imdg_subs_rsk_lbl_cd2]
      ,@[imdg_subs_rsk_lbl_cd3]
      ,@[imdg_subs_rsk_lbl_cd4]
      ,NVL(@[hcdg_flg],'N')
      ,@[emer_rspn_gid_no]
      ,NVL(@[spcl_cntr_seq], (SELECT NVL(MAX(SPCL_CNTR_SEQ), 0) + 1 
          					  FROM BKG_CSTMS_EUR_DG
         					  WHERE EUR_DG_DECL_TP_CD = @[d_type]
					          AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
						      AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
						      AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
					          AND PORT_CD    = @[port_cd]
				              AND BL_NO      = @[bl_no]
					          AND CGO_OPR_CD = @[cgo_opr_cd]
                              AND CNTR_NO   != @[cntr_no]))
      ,NVL(@[spcl_cgo_seq],  (SELECT NVL(MAX(SPCL_CGO_SEQ), 0) + 1 
          					  FROM BKG_CSTMS_EUR_DG
         					  WHERE EUR_DG_DECL_TP_CD = @[d_type]
					          AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
						      AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
						      AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
					          AND PORT_CD    = @[port_cd]
				              AND BL_NO      = @[bl_no]
					          AND CGO_OPR_CD = @[cgo_opr_cd]
						      AND CNTR_NO    = @[cntr_no]))
      )			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cgo_opr_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="cell_psn_no" type="12" value="" out="N"/>
				<param name="fwrd_id" type="12" value="" out="N"/>
				<param name="svc_rqst_no" type="12" value="" out="N"/>
				<param name="c_type" type="12" value="" out="N"/>
				<param name="fdr_svc_rqst_no" type="12" value="" out="N"/>
				<param name="fdr_vvd_id" type="12" value="" out="N"/>
				<param name="fdr_vsl_nm" type="12" value="" out="N"/>
				<param name="fdr_vsl_lloyd_no" type="12" value="" out="N"/>
				<param name="imdg_pck_grp_cd" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
				<param name="dg_short_nm" type="12" value="" out="N"/>
				<param name="flsh_pnt_cdo_temp" type="6" value="" out="N"/>
				<param name="net_wgt" type="6" value="" out="N"/>
				<param name="grs_wgt" type="6" value="" out="N"/>
				<param name="mfag_no" type="12" value="" out="N"/>
				<param name="prp_shp_nm" type="12" value="" out="N"/>
				<param name="hzd_desc" type="12" value="" out="N"/>
				<param name="imdg_lmt_qty_flg" type="12" value="" out="N"/>
				<param name="out_imdg_pck_qty1" type="6" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="out_imdg_pck_cd1" type="12" value="" out="N"/>
				<param name="eur_outr_pck_desc" type="12" value="" out="N"/>
				<param name="in_imdg_pck_qty1" type="12" value="" out="N"/>
				<param name="in_imdg_pck_cd1" type="12" value="" out="N"/>
				<param name="eur_inr_pck_desc" type="12" value="" out="N"/>
				<param name="ems_no" type="12" value="" out="N"/>
				<param name="agent" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eur_dcgo_mrn_polut_cd" type="12" value="" out="N"/>
				<param name="xtd_stay_prmt_no" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="crr_dt" type="12" value="" out="N"/>
				<param name="net_explo_wgt" type="12" value="" out="N"/>
				<param name="aply_no" type="12" value="" out="N"/>
				<param name="cntr_ref_no" type="12" value="" out="N"/>
				<param name="dcgo_ref_no" type="12" value="" out="N"/>
				<param name="emer_cntc_phn_no" type="12" value="" out="N"/>
				<param name="emer_cntc_pson_nm" type="12" value="" out="N"/>
				<param name="imdg_subs_rsk_lbl_cd1" type="12" value="" out="N"/>
				<param name="imdg_subs_rsk_lbl_cd2" type="12" value="" out="N"/>
				<param name="imdg_subs_rsk_lbl_cd3" type="12" value="" out="N"/>
				<param name="imdg_subs_rsk_lbl_cd4" type="12" value="" out="N"/>
				<param name="hcdg_flg" type="12" value="" out="N"/>
				<param name="emer_rspn_gid_no" type="12" value="" out="N"/>
				<param name="spcl_cntr_seq" type="12" value="" out="N"/>
				<param name="spcl_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
