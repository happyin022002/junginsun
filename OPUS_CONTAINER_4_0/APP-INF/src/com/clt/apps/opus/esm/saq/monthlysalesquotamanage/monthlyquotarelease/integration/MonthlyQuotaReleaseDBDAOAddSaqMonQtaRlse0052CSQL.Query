<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaReleaseDBDAOAddSaqMonQtaRlse0052CSQL">
			<desc><![CDATA[addSAQ_MON_QTA_RLSE]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_MON_QTA_RLSE(
		    MQTA_RLSE_VER_NO, 
		    BSE_YR, 
		    BSE_QTR_CD, 
		    RLSE_GDT, 
		    QTA_RLSE_STS_CD, 
		    CRE_USR_ID,     
            CRE_DT,     
            UPD_USR_ID,
            UPD_DT
		) VALUES(
		    @[mqta_rlse_ver_no], 
		    @[bse_yr], 
		    @[bse_qtr_cd], 
		    CAST(SYS_EXTRACT_UTC(SYSTIMESTAMP) AS DATE), 
		    'R', 
		    @[cre_usr_id],     
            SYSDATE,    
            @[upd_usr_id],     
            SYSDATE
		)			]]></sql>
			<params>
				<param name="mqta_rlse_ver_no" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
