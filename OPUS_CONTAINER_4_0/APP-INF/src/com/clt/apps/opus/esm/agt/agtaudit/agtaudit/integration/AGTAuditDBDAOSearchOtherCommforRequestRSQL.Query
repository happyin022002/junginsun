<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOSearchOtherCommforRequestRSQL">
			<desc><![CDATA[SearchOtherCommforRequest]]></desc>
			<sql><![CDATA[
SELECT
           A.COMM_STND_COST_CD,
           A.OTR_COMM_ACCT_CTNT,
           A.VNDR_CNT_CD,
           A.AGN_CD AS OFC_CD,
           TO_CHAR(A.VNDR_SEQ, 'FM000000') AS VNDR_SEQ,
           NVL(D.VNDR_LGL_ENG_NM, D.VNDR_LOCL_LANG_NM) AS VNDR_LGL_ENG_NM,
           A.COMM_OCCR_INFO_CD,
           NVL(A.AP_CTR_CD, B.AP_CTR_CD) AS AP_CTR_CD,
           NVL(A.APLY_DT, TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,A.AR_OFC_CD) + TO_NUMBER(DECODE(D.GEN_PAY_TERM_CD, 'IN', '5', 'OUT', '60', 'O60', '0', 'O45', '0', D.GEN_PAY_TERM_CD)), 'YYYYMMDD')) AS APLY_DT,
           NVL(A.COMM_VSL_CD||A.COMM_SKD_VOY_NO||A.COMM_SKD_DIR_CD||A.COMM_REV_DIR_CD, 'CNTC'||SUBSTR(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,A.AR_OFC_CD), 3, 4)||'MM') AS VVD,
           A.CURR_CD,
           A.ACT_LOCL_COMM_AMT,
           NVL(A.MON_XCH_RT, C.USD_LOCL_XCH_RT) AS MON_XCH_RT,
           A.ACT_COMM_AMT,
           TO_CHAR(A.AC_APRO_DT, 'YYYYMMDD')   AS AC_APRO_DT,
           A.BKG_NO,
           A.AGN_CD,
           A.IO_BND_CD,
           A.AC_TP_CD,
           A.AC_SEQ,
           A.COMM_PROC_STS_CD,
           A.COMM_PROC_STS_RSN
      FROM AGT_AGN_COMM A,
           MDM_ORGANIZATION B,
           GL_MON_XCH_RT C,
           MDM_VENDOR D
     WHERE A.AC_TP_CD = 'T'
       AND A.COMM_PROC_STS_CD
        IN
         ( 'CS',
           'CE',
           'CA',
           'IC'
         )
       AND A.COMM_YRMON      = REPLACE(@[comm_yrmon], '-', '')
       AND A.AR_OFC_CD       = @[ar_ofc_cd]
       AND A.AR_OFC_CD       = B.OFC_CD
       AND A.COMM_YRMON      = C.ACCT_XCH_RT_YRMON
       AND A.CURR_CD         = C.CURR_CD
       AND C.ACCT_XCH_RT_LVL = '1'
       AND A.VNDR_SEQ        = D.VNDR_SEQ			]]></sql>
			<params>
				<param name="comm_yrmon" type="12" value="" out="Y"/>
				<param name="ar_ofc_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
