<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchMaxBKGHistSeqDtlRSQL">
			<desc><![CDATA[BKG History Detail ì˜ MAX SEQ]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(HIS_DTL_SEQ), 0) HIST_DTL_SEQ
  FROM BKG_HIS_DTL
 WHERE BKG_NO = @[bkg_no]
   AND HIS_SEQ = @[hist_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="hist_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
