<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOCheckSltCostRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(CASE WHEN BSA_SLT_PRC_FM_DT = @[bsa_slt_prc_fm_dt] AND BSA_SLT_PRC_TO_DT = @[bsa_slt_prc_to_dt]
                     AND DELT_FLG     = 'N' 
                    THEN 1 ELSE 0 END),0) AS CNT
      ,NVL(SUM(CASE WHEN @[bsa_slt_prc_fm_dt] BETWEEN BSA_SLT_PRC_FM_DT AND BSA_SLT_PRC_TO_DT
                     AND DELT_FLG     = 'N' 
                    THEN 1 
                    WHEN @[bsa_slt_prc_to_dt] BETWEEN BSA_SLT_PRC_FM_DT AND BSA_SLT_PRC_TO_DT
                     AND DELT_FLG     = 'N'                  
                    THEN 1 ELSE 0 END),0) AS CNT
   FROM BSA_OVR_USD_MST 
  WHERE TRD_CD     = @[trd_cd]
    AND RLANE_CD   = @[rlane_cd]
    AND DIR_CD     = @[dir_cd]
    AND FM_PORT_CD = @[fm_port_cd]
    AND TO_PORT_CD = @[to_port_cd]			]]></sql>
			<params>
				<param name="bsa_slt_prc_fm_dt" type="12" value="" out="N"/>
				<param name="bsa_slt_prc_to_dt" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="fm_port_cd" type="12" value="" out="N"/>
				<param name="to_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
