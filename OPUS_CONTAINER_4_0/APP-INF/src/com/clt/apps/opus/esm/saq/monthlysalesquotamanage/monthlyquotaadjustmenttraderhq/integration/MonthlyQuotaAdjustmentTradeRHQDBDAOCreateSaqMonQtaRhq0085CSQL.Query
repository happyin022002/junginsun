<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentTradeRHQDBDAOCreateSaqMonQtaRhq0085CSQL">
			<desc><![CDATA[saq_mon_qta_rhq테이블 data copy]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_MON_QTA_RHQ (
    MQTA_STEP_CD,
    BSE_YR,
    BSE_QTR_CD,
    TRD_CD,
    DIR_CD,
    MQTA_VER_NO,
    RLANE_CD,
    SPRT_GRP_CD,
    BSA_GRP_CD,
    CTRT_RGN_OFC_CD,
    BSE_MON,
    SUB_TRD_CD,
    CTRT_RHQ_CD,
    CTRT_AQ_CD,
    LOD_QTY,
    GRS_RPB_REV,
    CM_UC_AMT,
    OPFIT_UC_AMT,
    RA_CM_UC_AMT,
    RA_OPFIT_UC_AMT,
    OFC_ADD_FLG,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT )
SELECT
    @[mQtaStepCd] AS MQTA_STEP_CD,
    BSE_YR,
    BSE_QTR_CD,
    TRD_CD,
    DIR_CD,
    MQTA_VER_NO,
    RLANE_CD,
    SPRT_GRP_CD,
    BSA_GRP_CD,
    CTRT_RGN_OFC_CD,
    BSE_MON,
    SUB_TRD_CD,
    CTRT_RHQ_CD,
    CTRT_AQ_CD,
    LOD_QTY,
    GRS_RPB_REV,
    CM_UC_AMT,
    OPFIT_UC_AMT,
    RA_CM_UC_AMT,
    RA_OPFIT_UC_AMT,
    OFC_ADD_FLG,
    @[userId] AS CRE_USR_ID,
    CRE_DT,
    @[userId] AS UPD_USR_ID,
    UPD_DT
FROM  SAQ_MON_QTA_RHQ
WHERE  MQTA_STEP_CD = @[condStepCd]
AND    BSE_YR = @[year]
AND    BSE_QTR_CD = @[bse_quarter]
AND    TRD_CD = @[trade]
AND    DIR_CD = @[bound]
AND    MQTA_VER_NO = @[mQtaVerNo]			]]></sql>
			<params>
				<param name="mQtaStepCd" type="12" value="" out="N"/>
				<param name="userId" type="12" value="" out="N"/>
				<param name="condStepCd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_quarter" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="mQtaVerNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
