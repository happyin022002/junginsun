<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCNRUBlAtCanadaRSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신]]></desc>
			<sql><![CDATA[
SELECT 'X'
  FROM BKG_CSTMS_ADV_BL 
 WHERE CNT_CD = 'CA'
   AND BL_NO in( substr(@[in_hjbl], 5, 12) , substr(@[in_hjbl], 1, 12)  ) 
                 			]]></sql>
			<params>
				<param name="in_hjbl" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
