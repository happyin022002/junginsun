<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AustraliaManifestListDownloadDBDAOsearchAusSendHistoryByCntrNoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT MSG_SND_NO,
       TO_CHAR(SND_DT, 'YYYY-MM-DD HH24:MI') AS SND_DT,
       SND_USR_ID,
       MSG_FUNC_ID,
       PSA_DG_DECL_TP_CD,
       VSL_CD||SKD_VOY_NO||SKD_DIR_CD AS VVD_CD,
       PORT_CD

  FROM BKG_CSTMS_AUS_DG_SND

 WHERE 1 = 1
#if (${vvd_cd} != '')
   AND VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
#end
#if (${port_cd} != '')
   AND PORT_CD = @[port_cd]
#end
#if (${snd_dt_from} != '' && ${snd_dt_to} != '')
   AND SND_DT BETWEEN TO_DATE(REPLACE(@[snd_dt_from], '-', ''), 'YYYYMMDD') AND TO_DATE(REPLACE(@[snd_dt_to], '-', ''), 'YYYYMMDD') + 1
#end
 ORDER BY MSG_SND_NO DESC,
          SND_DT DESC,
          VVD_CD,
          PORT_CD			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="snd_dt_from" type="12" value="" out="N"/>
				<param name="snd_dt_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
