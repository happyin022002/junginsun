<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMonQtaRhqOfficeAddLaneListRSQL">
			<desc><![CDATA[Office Add 에 사용할 Lane List 조회 ]]></desc>
			<sql><![CDATA[
SELECT DISTINCT R.RLANE_CD AS TEXT, R.RLANE_CD AS CODE              
  FROM SAQ_MON_QTA_STEP_VER V,                                      
#if (${mqta_step_cd} == '08' ) 
	SAQ_MON_QTA_LOD_TGT R                                        
#else
	SAQ_MON_QTA_RHQ R 
#end  	                                           
WHERE V.MQTA_STEP_CD = @[mqta_step_cd]                                           
  AND V.MQTA_VER_NO  = @[mqta_ver_no]                                           
  AND V.BSE_YR       = @[bse_yr]                                           
  AND V.TRD_CD       = @[trd_cd]                                           
  AND V.DIR_CD       = @[dir_cd]                                           
  AND R.MQTA_STEP_CD = V.MQTA_STEP_CD                              
  AND R.BSE_YR       = V.BSE_YR                                    
  AND R.TRD_CD       = V.TRD_CD                                    
  AND R.DIR_CD       = V.DIR_CD                                    
  AND R.MQTA_VER_NO  = V.MQTA_VER_NO                               
  AND R.SUB_TRD_CD   = @[sub_trd_cd]                                           
ORDER BY DECODE(TEXT,'RBCCO','ZZ',SUBSTR(TEXT,-2)), TEXT  			]]></sql>
			<params>
				<param name="mqta_step_cd" type="12" value="" out="N"/>
				<param name="mqta_ver_no" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
