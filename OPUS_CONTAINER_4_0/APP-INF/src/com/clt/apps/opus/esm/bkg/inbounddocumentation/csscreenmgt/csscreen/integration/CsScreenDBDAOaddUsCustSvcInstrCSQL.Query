<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CsScreenDBDAOaddUsCustSvcInstrCSQL">
			<desc><![CDATA[addUsCustSvcInstr]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_USA_CUST_SVC_INSTR ( BKG_NO
                                    ,INSTR_RMK_SEQ
                                    ,INSTR_RMK
                                    ,UPD_LOCL_DT
                                    ,UPD_LOCL_GDT
                                    ,DELT_FLG
                                    ,CRE_USR_ID
                                    ,CRE_DT
                                    ,UPD_USR_ID
                                    ,UPD_DT
)                                
SELECT @[bkg_no]
      ,(SELECT NVL(MAX(INSTR_RMK_SEQ)+ 1, 1) FROM BKG_USA_CUST_SVC_INSTR WHERE BKG_NO = @[bkg_no]) AS INSTR_RMK_SEQ  
      ,@[instr_rmk]      
      ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[edi_evnt_ofc_cd])
      ,GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,sysdate,'GMT') 
      ,'N'
      ,@[cre_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE  
FROM  DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="instr_rmk" type="12" value="" out="N"/>
				<param name="edi_evnt_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
