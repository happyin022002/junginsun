<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchTroCfmRSQL">
			<desc><![CDATA[POR, POL, POD, DEL, R/D TERM이 변경되어 TRO가 UNCONFIRM된 경우]]></desc>
			<sql><![CDATA[
select nvl(max(tro), 0) cnt
  from (select 1 tro
          from bkg_tro 
         where bkg_no      = @[bkg_no]
           and io_bnd_cd   = 'O'
           and rtn_tro_flg = 'N'
           and cfm_flg     = 'Y'
           and rownum      = 1
        union
        select 1 tro
          from bkg_eur_tro
         where bkg_no      = @[bkg_no]
           and io_bnd_cd   = 'O'
           and cfm_flg     = 'Y'
           and rownum      = 1)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
