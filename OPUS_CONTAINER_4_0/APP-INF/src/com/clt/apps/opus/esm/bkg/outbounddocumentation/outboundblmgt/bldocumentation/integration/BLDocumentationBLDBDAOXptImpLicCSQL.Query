       DECODE(@[caed_tp_cd],'IT',decode(trim(@[cgo_ctrl_no]), '', '',trim(@[cgo_ctrl_no])), ''),
       DECODE(@[caed_tp_cd],'ND','P.O.R :',''),
       DECODE(@[caed_tp_cd],'ND',@[ndr_ref_id],''),
       @[ndr_ref_ctnt],
	   0,
	   'N',
       0,
       0,
       @[cre_usr_id],
       sysdate,
	   @[upd_usr_id],
	   sysdate
	   )
