       DECODE(@[caed_tp_cd],'EX',to_char(decode(trim(@[b13a_xpt_no1]), '', '',trim(@[b13a_xpt_no1]))), ''),
       DECODE(@[caed_tp_cd],'EX',to_char(decode(trim(@[b13a_xpt_no2]), '', '',trim(@[b13a_xpt_no2]))), ''),
