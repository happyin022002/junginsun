             IMDG_CLSS_CD DG_IMO_CLASS,
             IMDG_UN_NO DG_UNNO,
             to_char(FLSH_PNT_CDO_TEMP) DG_FLASH_PNT,
             PRP_SHP_NM DG_PKG_DESC,
             IMDG_PCK_GRP_CD PKGGRP
 FROM   BKG_DG_CGO
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
