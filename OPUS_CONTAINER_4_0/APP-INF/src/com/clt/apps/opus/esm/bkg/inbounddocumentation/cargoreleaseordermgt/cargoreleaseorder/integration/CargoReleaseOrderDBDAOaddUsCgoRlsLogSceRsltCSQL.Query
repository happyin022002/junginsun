<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOaddUsCgoRlsLogSceRsltCSQL">
			<desc><![CDATA[....]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CGO_RLSE_EDI_SND_LOG
           (
            BL_NO,
            HIS_SEQ,
            HIS_DTL_SEQ,
            CGOR_EDI_RCVR_TP_CD,
            EDI_RCV_ID         ,
            CGOR_EDI_SND_CD    ,
            CGOR_EDI_MSG_ID    ,
            CGOR_EDI_SCS_FLG   ,
            CGOR_EDI_SND_ID    ,
            CRE_USR_ID         ,
            CRE_DT             ,
            UPD_USR_ID         ,
            UPD_DT             
            )      
    (SELECT @[bl_no],
            @[his_seq],
            NVL(MAX(HIS_DTL_SEQ) + 1,1),
            'C',
            @[edi_rcv_id],  
            Null,
            @[sce_msg_id],  
            @[sce_scs_cd],
            @[edi_snd_id],
            @[usr_id],
            SYSDATE,
            @[usr_id],
            SYSDATE
       FROM BKG_CGO_RLSE_EDI_SND_LOG
      WHERE BL_NO   = @[bl_no]  
        AND HIS_SEQ = @[his_seq]) 			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="his_seq" type="12" value="" out="N"/>
				<param name="edi_rcv_id" type="12" value="" out="N"/>
				<param name="sce_msg_id" type="12" value="" out="N"/>
				<param name="sce_scs_cd" type="12" value="" out="N"/>
				<param name="edi_snd_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
