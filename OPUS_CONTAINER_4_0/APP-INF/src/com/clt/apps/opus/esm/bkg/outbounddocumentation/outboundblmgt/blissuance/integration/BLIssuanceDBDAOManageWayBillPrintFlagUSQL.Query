<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOManageWayBillPrintFlagUSQL">
			<desc><![CDATA[Issue & Release Cancel 화면에서 Waybill 을 Cancel 하는 경우 BKG_BL_ISS 에 WBL_PRN_FLG 를 N 으로 변경]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_ISS
    SET WBL_PRN_FLG = 'N'
    , UPD_USR_ID = @[upd_usr_id]
    , UPD_DT = SYSDATE
WHERE BKG_NO = (SELECT BKG_NO FROM BKG_BOOKING
                 WHERE BKG_NO = @[bkg_no]
                   AND BL_TP_CD ='W')			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
