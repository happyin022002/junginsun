<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqRhqCdByOfcCdListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT V.N2ND_PRNT_OFC_CD CODE, V.N2ND_PRNT_OFC_CD TEXT 
FROM  SAQ_ORGANIZATION_V V 
WHERE V.OFC_CD = @[ofcCd] 
			]]></sql>
			<params>
				<param name="ofcCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
