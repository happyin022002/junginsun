<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAORemoveUcrNoDSQL">
			<desc><![CDATA[DELETE UCR NO]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_REFERENCE
WHERE BKG_NO = @[bkg_no]
AND BKG_REF_TP_CD = @[ucr_tp_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ucr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
