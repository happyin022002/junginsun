<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOModifyDoRefUSQL">
			<desc><![CDATA[D/O Release Reference 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_DO_REF SET 
	CY_OP_CD = @[cy_op_cd]
,	CSTMS_REF_NM = NVL(@[cstms_ref_nm],' ')
,	CSTMS_REF_CTNT = NVL(@[cstms_ref_ctnt],' ')
,	CSTMS_ASGN_NM = NVL(@[cstms_asgn_nm],' ')
,	CSTMS_ASGN_CTNT = NVL(@[cstms_asgn_ctnt],' ')
,	INTER_RMK = @[inter_rmk]
,	DO_HLD_FLG = NVL(@[do_hld_flg],'N')
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,	INFO_CGO_FLG = @[info_cgo_flg]
,	IDA_IMP_GEN_MF_NO = @[ida_imp_gen_mf_no]
,	IDA_CGOR_ORD_YR = @[ida_cgor_ord_yr]
,	IDA_CSTMS_ASGN_LINE_NO = @[ida_cstms_asgn_line_no]
,	DO_SPLIT_FLG = NVL(@[do_split_flg],'N')
WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="cy_op_cd" type="12" value="" out="N"/>
				<param name="cstms_ref_nm" type="12" value="" out="N"/>
				<param name="cstms_ref_ctnt" type="12" value="" out="N"/>
				<param name="cstms_asgn_nm" type="12" value="" out="N"/>
				<param name="cstms_asgn_ctnt" type="12" value="" out="N"/>
				<param name="inter_rmk" type="12" value="" out="N"/>
				<param name="do_hld_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="info_cgo_flg" type="12" value="" out="N"/>
				<param name="ida_imp_gen_mf_no" type="12" value="" out="N"/>
				<param name="ida_cgor_ord_yr" type="12" value="" out="N"/>
				<param name="ida_cstms_asgn_line_no" type="12" value="" out="N"/>
				<param name="do_split_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
