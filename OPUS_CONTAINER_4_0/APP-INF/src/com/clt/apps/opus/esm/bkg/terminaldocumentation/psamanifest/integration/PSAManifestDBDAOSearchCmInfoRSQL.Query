<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOSearchCmInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DSC.CNTR_NO,
       DSC.CNTR_MF_SEQ,
       DSC.PCK_QTY ,
       NVL2(CNV.CSTMS_PCK_TP_CD, CNV.CSTMS_PCK_TP_CD, DSC.PCK_TP_CD) AS CSTMS_PCK_TP_CD,
       DSC.CNTR_MF_WGT,
       DSC.WGT_UT_CD,
       DSC.MEAS_QTY,
       DSC.MEAS_UT_CD,
       DSC.CMDT_HS_CD,
       NVL(SUBSTR(DSC.CMDT_HS_CD, 1, 2), '98') AS COMMODITY_CD,
       DSC.CNTR_MF_MK_DESC

  FROM BKG_CNTR_MF_DESC DSC,
       BKG_CSTMS_PCK_TP_CONV CNV

 WHERE DSC.BKG_NO = @[bkg_no]
   AND DSC.PCK_TP_CD = CNV.PCK_TP_CD(+)
   AND CNV.CNT_CD(+) = 'SG'

 ORDER BY DSC.CNTR_NO,
          DSC.CNTR_MF_SEQ
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
