<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24CustomsTransmissionDBDAOSearchVesselFIArrivalBlInfoRSQL">
			<desc><![CDATA[SearchVesselFIArrivalBlInfo]]></desc>
			<sql><![CDATA[
SELECT BL_NO
     , MVMT_REF_NO
     , BKG_POL_CD
     , BKG_POD_CD
     , DEL_CD
     , '' BL_ENTRY_LOC
  FROM BKG_CSTMS_EUR_BL
 WHERE (VSL_CD, SKD_VOY_NO, SKD_DIR_CD, BL_NO, CSTMS_PORT_CD) 
       IN ( SELECT EB.VSL_CD, EB.SKD_VOY_NO, EB.SKD_DIR_CD, EB.BL_NO, EB.CSTMS_PORT_CD
              FROM BKG_CSTMS_EUR_BL EB, BKG_VVD BV, BKG_BOOKING BK
             WHERE 1=1
               AND EB.BL_NO = BK.BL_NO
               AND BK.BKG_NO = BV.BKG_NO
               AND EB.VSL_CD = BV.VSL_CD
               AND EB.SKD_VOY_NO = BV.SKD_VOY_NO
               AND EB.SKD_DIR_CD = BV.SKD_DIR_CD
               AND EB.POD_CD = BV.POD_CD
               AND EB.VSL_CD   = @[vsl_cd]
               AND EB.SKD_VOY_NO = @[skd_voy_no]
               AND EB.SKD_DIR_CD = @[skd_dir_cd]
               AND EB.CSTMS_PORT_CD = @[cstms_port_cd]
               AND EB.BL_NO = @[bl_no]
               AND BK.BKG_STS_CD <> 'X'
             GROUP BY EB.VSL_CD, EB.SKD_VOY_NO, EB.SKD_DIR_CD, EB.BL_NO, EB.CSTMS_PORT_CD, EB.MSG_SND_NO )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
