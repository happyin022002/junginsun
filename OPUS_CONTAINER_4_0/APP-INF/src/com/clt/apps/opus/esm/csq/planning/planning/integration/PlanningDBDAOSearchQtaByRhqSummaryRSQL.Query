<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanningDBDAOSearchQtaByRhqSummaryRSQL">
			<desc><![CDATA[QTA Set up by RHQ_Office별 QTA Summary 조회]]></desc>
			<sql><![CDATA[
SELECT E1.BSE_YR
      ,E1.BSE_QTR_CD
      ,DECODE(E1.OB_DIV_CD,'N','N.OB','O/B') AS OB_DIV_CD
      ,DECODE(E1.OFC_VW_CD,'C','Contract','L','Loading') AS OFC_VW_CD
      ,E1.TRD_CD
      ,E1.CONV_DIR_CD
      ,E1.RHQ_CD
      ,E1.RGN_OFC_CD
      ,E1.OFC_QTY
      ,E1.OFC_WK_QTY
      ,ROUND(RATIO_TO_REPORT(E1.OFC_QTY) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100 AS OFC_QTY_RTO
      ,E1.OFC_REV
      ,E1.OFC_WK_REV
      ,ROUND(RATIO_TO_REPORT(E1.OFC_REV) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100 AS OFC_REV_RTO
      ,E2.PST_WK_QTY
      ,ROUND(RATIO_TO_REPORT(E2.PST_WK_QTY) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100 AS PST_QTY_RTO
      ,E2.PST_WK_REV
      ,ROUND(RATIO_TO_REPORT(E2.PST_WK_REV) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100 AS PST_REV_RTO
      ,E1.OFC_WK_QTY - E2.PST_WK_QTY AS DIFF_QTY
      ,ROUND(RATIO_TO_REPORT(E1.OFC_REV) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100 
      -ROUND(RATIO_TO_REPORT(E2.PST_WK_QTY) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100 AS DIFF_QTY_RTO
      ,E1.OFC_WK_REV - E2.PST_WK_REV AS DIFF_REV
      ,ROUND(RATIO_TO_REPORT(E1.OFC_REV) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100
      -ROUND(RATIO_TO_REPORT(E2.PST_WK_REV) OVER (PARTITION BY E1.OFC_VW_CD,E1.TRD_CD,E1.CONV_DIR_CD),4)*100 AS DIFF_REV_RTO
  FROM (
        SELECT V.BSE_TP_CD
              ,V.BSE_YR
              ,V.BSE_QTR_CD
              ,V.OB_DIV_CD
              ,V.OFC_VW_CD
              ,V.TRD_CD
              ,V.CONV_DIR_CD
              ,V.RHQ_CD
              ,RGN_OFC_CD
              ,SUM(OFC_QTY) AS OFC_QTY
              ,SUM(OFC_REV) AS OFC_REV
#if (${f_bse_tp_cd} == 'Y')
              ,SUM(OFC_QTY) / 53 AS OFC_WK_QTY
              ,SUM(OFC_REV) / 53 AS OFC_WK_REV
#else
              ,SUM(OFC_QTY) / 13 AS OFC_WK_QTY
              ,SUM(OFC_REV) / 13 AS OFC_WK_REV
#end
          FROM CSQ_DAT_RLT          R
              ,CSQ_SIMULATION_OFC_V V
         WHERE R.BSE_TP_CD   = @[f_bse_tp_cd]
           AND R.BSE_YR      = @[f_bse_yr]
           AND R.BSE_QTR_CD  = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
           AND R.OFC_VW_CD   = @[f_ofc_vw_cd]
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
           AND R.OB_DIV_CD   = @[f_ob_div_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
           AND R.TRD_CD      = @[f_trd_cd]
#end
#if (${f_click} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
           AND V.CONV_DIR_CD = @[f_dir_cd]
#end
           AND R.BSE_TP_CD   = V.BSE_TP_CD
           AND R.BSE_YR      = V.BSE_YR
           AND R.BSE_QTR_CD  = V.BSE_QTR_CD
           AND R.OFC_VW_CD   = V.OFC_VW_CD
           AND R.TRD_CD      = V.TRD_CD
           AND R.CONV_DIR_CD = V.CONV_DIR_CD
           AND R.RHQ_CD      = V.RHQ_CD
           AND R.RHQ_CD      = (SELECT CASE WHEN @[ofc_cd] IN (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255') THEN R.RHQ_CD
                                                                                                                                          ELSE @[ofc_cd]
                                        END RHQ_CD
                                  FROM DUAL)
      GROUP BY V.BSE_TP_CD
              ,V.BSE_YR
              ,V.BSE_QTR_CD
              ,V.OB_DIV_CD
              ,V.OFC_VW_CD
              ,V.TRD_CD
              ,V.CONV_DIR_CD
              ,V.RHQ_CD
              ,RGN_OFC_CD
        ) E1
      ,(
        SELECT A1.BSE_TP_CD
              ,A1.BSE_YR
              ,A1.BSE_QTR_CD
              ,A1.OFC_VW_CD
              ,A1.TRD_CD
              ,NVL(A2.CONV_DIR_CD,A1.DIR_CD) AS CONV_DIR_CD
              ,A1.RHQ_CD
              ,A1.RGN_OFC_CD
              ,SUM(A1.LOD_QTY) AS PST_QTY
              ,ROUND(SUM(A1.GRS_TTL_REV),0) AS PST_REV
              ,ROUND(SUM(DECODE(A1.VVD_KNT, 0, 0, A1.LOD_QTY/A1.VVD_KNT)),0) AS PST_WK_QTY
              ,ROUND(SUM(DECODE(A1.VVD_KNT, 0, 0, A1.GRS_TTL_REV/A1.VVD_KNT)),0) AS PST_WK_REV
              ,SUM(A1.VVD_KNT) AS VVD_KNT
          FROM CSQ_PERF_IF A1
              ,CSQ_DIR_CONV A2
         WHERE 1=1
           AND A1.BSE_TP_CD   = @[f_bse_tp_cd]
           AND A1.BSE_YR      = @[f_bse_yr]
#if (${f_bse_tp_cd} == 'Y')
           AND A1.BSE_QTR_CD  = '00'
#else
           AND A1.BSE_QTR_CD  = @[f_bse_qtr_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
           AND A1.TRD_CD      = @[f_trd_cd]
#end 
           AND A1.RHQ_CD      = (SELECT CASE WHEN @[ofc_cd] IN (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255') THEN A1.RHQ_CD
                                                                                                                                           ELSE @[ofc_cd]
                                         END TEAM_CD
                                   FROM DUAL)
           AND A1.QTA_TGT_CD  = 'D'
           AND A1.CSQ_LVL_CD  = '2'
           AND A1.BSE_TP_CD   = A2.BSE_TP_CD(+)
           AND A1.BSE_YR      = A2.BSE_YR (+)
           AND A1.BSE_QTR_CD  = A2.BSE_QTR_CD(+)
           AND A1.TRD_CD      = A2.TRD_CD(+)
           AND A1.RLANE_CD    = A2.RLANE_CD(+)
           AND A1.DIR_CD      = A2.DIR_CD(+)
         GROUP BY A1.BSE_TP_CD
              ,A1.BSE_YR
              ,A1.BSE_QTR_CD
              ,A1.OFC_VW_CD
              ,A1.TRD_CD
              ,A1.RHQ_CD
              ,RGN_OFC_CD
              ,NVL(A2.CONV_DIR_CD,A1.DIR_CD)
        ) E2
 WHERE 1=1
   AND E1.BSE_TP_CD    = E2.BSE_TP_CD(+)
   AND E1.BSE_YR       = E2.BSE_YR(+)
   AND E1.BSE_QTR_CD   = E2.BSE_QTR_CD(+)
   AND E1.OFC_VW_CD    = E2.OFC_VW_CD(+)
   AND E1.TRD_CD       = E2.TRD_CD(+)
   AND E1.CONV_DIR_CD  = E2.CONV_DIR_CD(+)
   AND E1.RHQ_CD       = E2.RHQ_CD(+)
   AND E1.RGN_OFC_CD   = E2.RGN_OFC_CD(+)
#if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
  AND E1.RGN_OFC_CD   = @[f_rgn_ofc_cd]
#end  
ORDER BY E1.OFC_VW_CD, E1.TRD_CD, E1.CONV_DIR_CD, E1.RHQ_CD, E1.RGN_OFC_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_ob_div_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="f_rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
