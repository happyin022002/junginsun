<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOModifyXterBkgNoUSQL">
			<desc><![CDATA[S]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_RQST_MST BXM
   SET BXM.BKG_NO = @[bkg_no]
 WHERE BXM.XTER_SNDR_ID = @[sender_id]
   AND BXM.XTER_RQST_NO = @[rqst_no]
   AND BXM.XTER_RQST_SEQ = @[rqst_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
