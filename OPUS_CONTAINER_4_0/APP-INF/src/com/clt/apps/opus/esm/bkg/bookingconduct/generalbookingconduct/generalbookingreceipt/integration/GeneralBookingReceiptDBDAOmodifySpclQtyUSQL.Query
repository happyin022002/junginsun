<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOmodifySpclQtyUSQL">
			<desc><![CDATA[modifySpclQty]]></desc>
			<sql><![CDATA[
UPDATE BKG_QUANTITY SET 

#if (${spcl_tp} == 'DG')
	DCGO_QTY = (select sum(cntr_vol_qty) from bkg_dg_cgo 
	WHERE	BKG_NO = @[bkg_no]
	AND	CNTR_TPSZ_CD = @[cntr_tpsz_cd])
#elseif (${spclTp} == 'AWK')
	AWK_CGO_QTY = (select sum(cntr_vol_qty) from bkg_awk_cgo 
	WHERE	BKG_NO = @[bkg_no]
	AND	CNTR_TPSZ_CD = @[cntr_tpsz_cd])
#elseif (${spclTp} == 'RF')
	RC_QTY = (select sum(cntr_vol_qty) from bkg_rf_cgo 
	WHERE	BKG_NO = @[bkg_no]
	AND	CNTR_TPSZ_CD = @[cntr_tpsz_cd])
#elseif (${spclTp} == 'BB')
	BB_CGO_QTY = (select sum(cntr_vol_qty) from bkg_bb_cgo 
	WHERE	BKG_NO = @[bkg_no]
	AND	CNTR_TPSZ_CD = @[cntr_tpsz_cd])
#end

WHERE	BKG_NO = @[bkg_no]
AND	CNTR_TPSZ_CD = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
