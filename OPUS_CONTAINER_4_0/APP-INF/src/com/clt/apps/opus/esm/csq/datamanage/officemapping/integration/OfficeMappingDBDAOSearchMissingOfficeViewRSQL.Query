<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeMappingDBDAOSearchMissingOfficeViewRSQL">
			<desc><![CDATA[CSQ_QTA_LANE_OFC 에 Loading, Contract 모두 생성되었는지 확인]]></desc>
			<sql><![CDATA[
SELECT NVL(DECODE(B1.OFC_VW_CD, 'L', 'Contract', 'C', 'Loading'), 'N') AS MISS_OFC_VW
FROM (        
      SELECT DECODE(A1.CNT,1,(SELECT DISTINCT OFC_VW_CD
                                FROM CSQ_QTA_LANE_OFC A1
                                WHERE 1=1
                                AND A1.BSE_TP_CD  = @[f_bse_tp_cd]
                                AND A1.BSE_YR     = @[f_bse_yr]
                                AND A1.BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
                                AND A1.TRD_CD     = @[f_trd_cd]
                                AND A1.SUB_TRD_CD = @[f_sub_trd_cd]
                                AND A1.DIR_CD     = @[f_dir_cd]
                                AND A1.RLANE_CD   = @[f_rlane_cd])) AS  OFC_VW_CD    
      FROM (     
           SELECT COUNT(DISTINCT OFC_VW_CD) CNT
            FROM CSQ_QTA_LANE_OFC A1
            WHERE 1=1
            AND A1.BSE_TP_CD  = @[f_bse_tp_cd]
            AND A1.BSE_YR     = @[f_bse_yr]
            AND A1.BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
            AND A1.TRD_CD     = @[f_trd_cd]
            AND A1.SUB_TRD_CD = @[f_sub_trd_cd]
            AND A1.DIR_CD     = @[f_dir_cd]
            AND A1.RLANE_CD   = @[f_rlane_cd]
            ) A1
        ) B1			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_sub_trd_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
