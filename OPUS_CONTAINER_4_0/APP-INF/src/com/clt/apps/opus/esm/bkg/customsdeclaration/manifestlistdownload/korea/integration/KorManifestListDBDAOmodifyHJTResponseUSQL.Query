<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOmodifyHJTResponseUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_KR_MF_REF_NO

   SET SND_DT = TO_DATE(@[snd_dt], 'YYYYMMDDHH24MI'),
       RSLT_ACK_DT  = TO_DATE(@[rslt_ack_dt], 'YYYYMMDDHH24MI'),
       TRSM_MSG_TP_ID  = @[trsm_msg_tp_id],
       ERR_MSG = @[err_msg]

 WHERE MRN_NO     = SUBSTR(@[mrn_no], 1, 10)
   AND MRN_CHK_NO = SUBSTR(@[mrn_no], 11, 1)
			]]></sql>
			<params>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="rslt_ack_dt" type="12" value="" out="N"/>
				<param name="trsm_msg_tp_id" type="12" value="" out="N"/>
				<param name="err_msg" type="12" value="" out="N"/>
				<param name="mrn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
