<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterCustAckRSQL">
			<desc><![CDATA[searchXterCustAck]]></desc>
			<sql><![CDATA[
SELECT	'{ACK_CUST_INFO' || CHR(10)
         || 'IB_TP:'           || cust.xter_cust_tp_cd                                                          || CHR(10)
         || 'IB_CUST_CD:'      || cust.cnt_cd||cust.cust_seq                                                    || CHR(10)
         || 'IB_UN_CUST_CD:'   || CUST.UN_LOC_CD                                                                || CHR(10)
         || 'IB_PCC_CUST_CD:'  || CUST.PRNR_CUST_CD                                                             || CHR(10)
		 || 'IB_PARTNER_REF_NO:'  || CUST.PRNR_REF_NO                                                            || CHR(10) 
         || 'IB_NM1:'          || REPLACE(REPLACE(BKG_TOKEN_NL_FNC(cust.cust_nm,   1, ''), '*', '-'), ':', '-')		  || CHR(10)
         || 'IB_NM2:'          || REPLACE(REPLACE(BKG_TOKEN_NL_FNC(cust.cust_nm,   2, ''), '*', '-'), ':', '-')		  || CHR(10)
         || 'IB_ADDR:'         || REPLACE(REPLACE(BKG_TOKEN_NL_FNC(cust.cust_addr, 1, ''), '*', '-'), ':', '-')	  	|| CHR(10)
         || 'IB_ADDR1:'        || REPLACE(REPLACE(BKG_TOKEN_NL_FNC(cust.cust_addr, 2, ''), '*', '-'), ':', '-')	  	|| CHR(10)
         || 'IB_ADDR2:'        || REPLACE(REPLACE(BKG_TOKEN_NL_FNC(cust.cust_addr, 3, ''), '*', '-'), ':', '-')	  	|| CHR(10)
         || 'IB_ADDR3:'        || REPLACE(REPLACE(BKG_TOKEN_NL_FNC(cust.cust_addr, 4, ''), '*', '-'), ':', '-')	  	|| CHR(10)   
         || 'IB_ADDR4:'        || REPLACE(REPLACE(BKG_TOKEN_NL_FNC(cust.cust_addr, 5, ''), '*', '-'), ':', '-')	  	|| CHR(10)
		 || 'IB_LOC_CD:'       || cust.LOC_CD                                                                   || CHR(10) 
         || 'IB_LOC_NM:'       || cust.loc_nm                                                                   || CHR(10)
		 || 'IB_STREET:'       || cust.ST_NM                                                                    || CHR(10)
         || 'IB_STATE:'        || cust.ste_cd                                                                   || CHR(10)
         || 'IB_ZIP_CD:'       || cust.PST_CTNT                                                                 || CHR(10)
         || 'IB_CNT_CD:'       || cust.cnt_cd                                                                   || CHR(10)
         || 'IB_C_TP:'         || cust.XTER_CUST_CNTC_TP_CD                                                     || CHR(10)
         || 'IB_C_NM:'         || cust.CNTC_NM                                                                  || CHR(10)
         || 'IB_C_DEPT:'       || ''                                                                            || CHR(10)
         || 'IB_C_EMAIL:'      || cust.CNTC_EML                                                                 || CHR(10)
         || 'IB_C_PHON:'       || cust.CNTC_PHN_NO_CTNT                                                         || CHR(10)
         || 'IB_C_FAX:'        || cust.CNTC_FAX_NO                                                              || CHR(10)
        || '}ACK_CUST_INFO'	AS CNTR_CUST_INFO
  FROM bkg_xter_Rqst_mst mst, bkg_xter_cust cust
 where mst.xter_sndr_id    = @[sender_id]
   and mst.xter_rqst_no    = @[rqst_no]
   and mst.xter_rqst_seq   = @[rqst_seq]
   and mst.xter_sndr_id    = cust.xter_sndr_id
   and mst.xter_rqst_no    = cust.xter_rqst_no
   and mst.xter_rqst_seq   = cust.xter_rqst_seq			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
