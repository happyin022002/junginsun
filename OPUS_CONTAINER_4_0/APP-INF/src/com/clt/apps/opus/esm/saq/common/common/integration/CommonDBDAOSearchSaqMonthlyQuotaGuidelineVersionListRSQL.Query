<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMonthlyQuotaGuidelineVersionListRSQL">
			<desc><![CDATA[SearchSaqTagetGroupTradeList 목록 조회]]></desc>
			<sql><![CDATA[
SELECT  GLINE_VER_NO AS CODE, 
        GLINE_VER_NO			 
		       ||DECODE(SAQ_STS_CD, '00', '', '-'||SAQ_STS_CD) AS TEXT  
FROM SAQ_MON_QTA_GLINE_VER 
WHERE BSE_YR = @[year] 
AND   BSE_QTR_CD = @[bse_qtr_cd] 
AND   SAQ_TGT_GRP_CD = @[targetGrp] 
AND   ('ALL' = @[searchFlag] OR SAQ_STS_CD IN ('FN', 'QN', 'QF')) 
ORDER BY GLINE_VER_NO DESC 			]]></sql>
			<params>
				<param name="year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="targetGrp" type="12" value="" out="N"/>
				<param name="searchFlag" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
