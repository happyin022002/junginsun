		, '' AS REMARK
		, NVL((SELECT OBL_ISS_FLG FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO),'N') AS ENABLE
        , BK.BKG_NO
        , BK.POR_CD
