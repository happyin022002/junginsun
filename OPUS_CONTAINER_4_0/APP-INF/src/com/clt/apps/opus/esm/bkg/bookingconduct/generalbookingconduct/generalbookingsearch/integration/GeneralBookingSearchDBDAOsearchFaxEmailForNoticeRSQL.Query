             FROM BKG_NTC_HIS
            WHERE BKG_NO = @[bkg_no]
              AND 'F'    = NTC_VIA_CD(+)
              AND DECODE((SELECT DECODE(USR_LOC.CONTI_CD, BK_LOC.CONTI_CD, 'O', 'I') IO_BND_CD
