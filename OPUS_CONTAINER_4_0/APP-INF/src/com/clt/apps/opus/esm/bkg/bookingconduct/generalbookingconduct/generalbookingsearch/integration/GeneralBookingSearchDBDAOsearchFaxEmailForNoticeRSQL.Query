                           WHERE EML_SND_NO = EML.SND_ID),1,2,2,2,3,3,4,4,1),
                  NVL2(EML.BKG_NTC_SND_RSLT_CD,
                  DECODE(EML.BKG_NTC_SND_RSLT_CD,3,3,4,4,1),1)) ) AS EML_SEND_RESULT
        , (SELECT EML_ERR_MSG
             FROM COM_EML_SND_INFO
            WHERE EML_SND_NO = EML.SND_ID) AS EML_FAIL_REASON
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_ALL_FLG ELSE EML.FRT_ALL_FLG END,'N') FRT_ALL_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_CLT_FLG ELSE EML.FRT_CLT_FLG END,'N') FRT_CLT_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_PPD_FLG ELSE EML.FRT_PPD_FLG END,'N') FRT_PPD_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_CHG_FLG ELSE EML.FRT_CHG_FLG END,'N') FRT_CHG_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_ARR_FLG ELSE EML.FRT_ARR_FLG END,'N') FRT_ARR_FLG
