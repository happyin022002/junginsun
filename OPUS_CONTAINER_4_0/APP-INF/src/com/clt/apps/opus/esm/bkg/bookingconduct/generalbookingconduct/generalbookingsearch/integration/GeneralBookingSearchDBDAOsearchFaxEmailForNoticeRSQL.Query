		<query name="GeneralBookingSearchDBDAOsearchFaxEmailForNoticeRSQL">
			<desc><![CDATA[searchFaxEmailForNotice]]></desc>
			<sql><![CDATA[
--BOOKING RECEIPT 
 SELECT /*+ INDEX_DESC(FAX XPKBKG_NTC_HIS) INDEX_DESC(EML XPKBKG_NTC_HIS) */1 ORD
		, (SELECT INTG_CD_VAL_DP_DESC 
          FROM COM_INTG_CD_DTL 
         WHERE INTG_CD_ID = 'CD01552'
           AND INTG_CD_VAL_CTNT = 'BK') NTC_KND_NM
        , 'BK' NTC_KND_CD
        --------------------------
        , NVL(FAX.NTC_FAX_NO, CNTC.CNTC_PSON_FAX_NO) FAX_NO
        , FAX.SND_USR_ID FAX_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = FAX.SND_USR_ID) FAX_SENDER_NM
		, NVL (FAX.SND_RQST_DT,
          NVL((SELECT XPT_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID),
           (SELECT UPD_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID)
          )) AS FAX_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT FAX_PROC_STS_CD
                          FROM COM_FAX_SND_INFO
                         WHERE FAX_SND_NO = FAX.SND_ID),
                  DECODE((SELECT FAX_PROC_STS_CD
                            FROM COM_FAX_SND_INFO
                           WHERE FAX_SND_NO = FAX.SND_ID),1,2,2,2,3,2,4,4,5,3,6,4,1),
                  NVL2(FAX.BKG_NTC_SND_RSLT_CD,
                  DECODE(FAX.BKG_NTC_SND_RSLT_CD,4,2,5,3,6,4,1),1))) AS FAX_SEND_RESULT
        , (SELECT NVL(XPT_ERR_MSG, XPT_ERR_DTL_MSG)
             FROM COM_FAX_SND_INFO
            WHERE FAX_SND_NO = FAX.SND_ID) AS FAX_FAIL_REASON
        --------------------------
        , NVL(EML.NTC_EML, CNTC.CNTC_PSON_EML) EML
		, EML.NTC_EML YD_EML
        , EML.SND_USR_ID EML_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = EML.SND_USR_ID) EML_SENDER_NM
        , NVL (EML.SND_RQST_DT,(SELECT EML_DT
                  FROM COM_EML_SND_INFO
                 WHERE EML_SND_NO = EML.SND_ID)) AS EML_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT EML_PROC_STS_CD
                          FROM COM_EML_SND_INFO
                         WHERE EML_SND_NO = EML.SND_ID),
                  DECODE((SELECT EML_PROC_STS_CD
                            FROM COM_EML_SND_INFO
                           WHERE EML_SND_NO = EML.SND_ID),1,2,2,2,3,3,4,4,1),
                  NVL2(EML.BKG_NTC_SND_RSLT_CD,
                  DECODE(EML.BKG_NTC_SND_RSLT_CD,3,3,4,4,1),1)) ) AS EML_SEND_RESULT
        , (SELECT EML_ERR_MSG
             FROM COM_EML_SND_INFO
            WHERE EML_SND_NO = EML.SND_ID) AS EML_FAIL_REASON
        , NULL FRT_ALL_FLG
        , NULL FRT_CLT_FLG
        , NULL FRT_PPD_FLG
        , NULL FRT_CHG_FLG
        , NULL FRT_ARR_FLG
        , NULL FRT_HDN_FLG
        , NULL DISPLAY_HIDDEN
        , (SELECT RCT_NTC_RMK
             FROM (SELECT RCT_NTC_RMK
                     FROM BKG_BOOKING A1, BKG_CUSTOMER A2, BKG_RPT_ITM_STUP A3
                    WHERE A1.BKG_NO = @[bkg_no]
                      AND A1.BKG_OFC_CD = A3.BKG_OFC_CD
                      AND A1.BKG_NO = A2.BKG_NO
                      AND A2.BKG_CUST_TP_CD IN('S', 'F')
                      AND A2.CUST_CNT_CD = A3.CUST_CNT_CD
                      AND A2.CUST_SEQ = A3.CUST_SEQ
                   UNION ALL
                   SELECT RCT_NTC_RMK
                     FROM BKG_BOOKING A1, BKG_RPT_ITM_STUP A2
                    WHERE A1.BKG_NO = @[bkg_no]
                      AND A1.BKG_OFC_CD = A2.BKG_OFC_CD
                      AND A2.CUST_CNT_CD IS NULL
                      AND A2.CUST_SEQ IS NULL)
            WHERE ROWNUM = 1) REMARK
		, 'Y' ENABLE
        , BK.BKG_NO
        , BK.POR_CD
        , NVL((
            SELECT BL_ESIG_FLG 
            FROM BKG_BL_ESIG_OFC_ASGN
            WHERE BL_ISS_OFC_CD = DECODE((SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO), NULL, BK.BKG_OFC_CD, 
                                         (SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO))
        ),'N') AS BL_ESIG_FLG
        , NVL((
            SELECT BL_CPY_ESIG_FLG
            FROM BKG_BL_ESIG_OFC_ASGN
            WHERE BL_ISS_OFC_CD = DECODE((SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO), NULL, BK.BKG_OFC_CD, 
                                         (SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO))
        ),'N') AS BL_CPY_ESIG_FLG

  FROM BKG_BOOKING BK
        , BKG_CNTC_PSON CNTC
        , BKG_NTC_HIS FAX
        , BKG_NTC_HIS EML
 WHERE BK.BKG_NO = @[bkg_no]
   AND BK.BKG_NO = FAX.BKG_NO (+)
   AND 'F'       = FAX.NTC_VIA_cD(+)
   AND 'BK'      = FAX.NTC_KND_CD(+)
   AND BK.BKG_NO = EML.BKG_NO (+)
   AND 'M'       = EML.NTC_VIA_cD(+)
   AND 'BK'      = EML.NTC_KND_CD(+)   
   AND BK.BKG_NO = CNTC.BKG_NO
   AND CNTC.BKG_CNTC_PSON_TP_CD = 'BK'
   AND ROWNUM    = 1
UNION   
--RELEASE ORDER
 SELECT /*+ INDEX_DESC(FAX XPKBKG_NTC_HIS) INDEX_DESC(EML XPKBKG_NTC_HIS) */2 ORD
		, (SELECT INTG_CD_VAL_DP_DESC 
          FROM COM_INTG_CD_DTL 
         WHERE INTG_CD_ID = 'CD01552'
           AND INTG_CD_VAL_CTNT = 'CN') NTC_KND_NM
        , 'CN' NTC_KND_CD
        --------------------------
        , NVL(FAX.NTC_FAX_NO, PKUP.FAX_NO) FAX_NO
        , FAX.SND_USR_ID FAX_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = FAX.SND_USR_ID) FAX_SENDER_NM
        , NVL (FAX.SND_RQST_DT,
          NVL((SELECT XPT_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID),
           (SELECT UPD_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID)
          )) AS FAX_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT FAX_PROC_STS_CD
                          FROM COM_FAX_SND_INFO
                         WHERE FAX_SND_NO = FAX.SND_ID),
                  DECODE((SELECT FAX_PROC_STS_CD
                            FROM COM_FAX_SND_INFO
                           WHERE FAX_SND_NO = FAX.SND_ID),1,2,2,2,3,2,4,4,5,3,6,4,1),
                  NVL2(FAX.BKG_NTC_SND_RSLT_CD,
                  DECODE(FAX.BKG_NTC_SND_RSLT_CD,4,2,5,3,6,4,1),1))) AS FAX_SEND_RESULT
        , (SELECT NVL(XPT_ERR_MSG, XPT_ERR_DTL_MSG)
             FROM COM_FAX_SND_INFO
            WHERE FAX_SND_NO = FAX.SND_ID) AS FAX_FAIL_REASON
        --------------------------
        , NVL(EML.NTC_EML, PKUP.YD_EML) EML
		, PKUP.YD_EML 
        , EML.SND_USR_ID EML_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = EML.SND_USR_ID) EML_SENDER_NM
        , NVL (EML.SND_RQST_DT,(SELECT EML_DT
                  FROM COM_EML_SND_INFO
                 WHERE EML_SND_NO = EML.SND_ID)) AS EML_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT EML_PROC_STS_CD
                          FROM COM_EML_SND_INFO
                         WHERE EML_SND_NO = EML.SND_ID),
                  DECODE((SELECT EML_PROC_STS_CD
                            FROM COM_EML_SND_INFO
                           WHERE EML_SND_NO = EML.SND_ID),1,2,2,2,3,3,4,4,1),
                  NVL2(EML.BKG_NTC_SND_RSLT_CD,
                  DECODE(EML.BKG_NTC_SND_RSLT_CD,3,3,4,4,1),1)) ) AS EML_SEND_RESULT
        , (SELECT EML_ERR_MSG
             FROM COM_EML_SND_INFO
            WHERE EML_SND_NO = EML.SND_ID) AS EML_FAIL_REASON
        , NULL FRT_ALL_FLG
        , NULL FRT_CLT_FLG
        , NULL FRT_PPD_FLG
        , NULL FRT_CHG_FLG
        , NULL FRT_ARR_FLG
        , NULL FRT_HDN_FLG
        , NULL DISPLAY_HIDDEN
		, '' REMARK
		, 'Y' ENABLE
        , BK.BKG_NO
        , BK.POR_CD
        , NVL((
            SELECT BL_ESIG_FLG 
            FROM BKG_BL_ESIG_OFC_ASGN
            WHERE BL_ISS_OFC_CD = DECODE((SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO), NULL, BK.BKG_OFC_CD, 
                                         (SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO))
        ),'N') AS BL_ESIG_FLG
        , NVL((
            SELECT BL_CPY_ESIG_FLG
            FROM BKG_BL_ESIG_OFC_ASGN
            WHERE BL_ISS_OFC_CD = DECODE((SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO), NULL, BK.BKG_OFC_CD, 
                                         (SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO))
        ),'N') AS BL_CPY_ESIG_FLG
  FROM BKG_BOOKING BK
        , BKG_NTC_HIS FAX
        , BKG_NTC_HIS EML
        , MDM_YARD PKUP
 WHERE BK.BKG_NO = @[bkg_no]
   AND BK.BKG_NO = FAX.BKG_NO (+)
   AND 'F'       = FAX.NTC_VIA_cD(+)
   AND 'CN'      = FAX.NTC_KND_CD(+)
   AND BK.BKG_NO = EML.BKG_NO (+)
   AND 'M'       = EML.NTC_VIA_cD(+)
   AND 'CN'      = EML.NTC_KND_CD(+)  
   AND BK.MTY_PKUP_YD_CD = PKUP.YD_CD(+)
   AND ROWNUM    = 1
UNION
--DRAFT B/L
SELECT /*+ INDEX_DESC(FAX XPKBKG_NTC_HIS) INDEX_DESC(EML XPKBKG_NTC_HIS) */3 ORD
		, (SELECT INTG_CD_VAL_DP_DESC 
          FROM COM_INTG_CD_DTL 
         WHERE INTG_CD_ID = 'CD01552'
           AND INTG_CD_VAL_CTNT = 'BL') NTC_KND_NM
        , 'BL' NTC_KND_CD
        --------------------------
        , NVL(FAX.NTC_FAX_NO, CNTC.CNTC_PSON_FAX_NO) FAX_NO
        , FAX.SND_USR_ID FAX_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = FAX.SND_USR_ID) FAX_SENDER_NM
        , NVL (FAX.SND_RQST_DT,
          NVL((SELECT XPT_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID),
           (SELECT UPD_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID)
          )) AS FAX_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT FAX_PROC_STS_CD
                          FROM COM_FAX_SND_INFO
                         WHERE FAX_SND_NO = FAX.SND_ID),
                  DECODE((SELECT FAX_PROC_STS_CD
                            FROM COM_FAX_SND_INFO
                           WHERE FAX_SND_NO = FAX.SND_ID),1,2,2,2,3,2,4,4,5,3,6,4,1),
                  NVL2(FAX.BKG_NTC_SND_RSLT_CD,
                  DECODE(FAX.BKG_NTC_SND_RSLT_CD,4,2,5,3,6,4,1),1))) AS FAX_SEND_RESULT
        , (SELECT NVL(XPT_ERR_MSG, XPT_ERR_DTL_MSG)
             FROM COM_FAX_SND_INFO
            WHERE FAX_SND_NO = FAX.SND_ID) AS FAX_FAIL_REASON
        --------------------------
        , NVL(EML.NTC_EML,    CNTC.CNTC_PSON_EML) EML
		, EML.NTC_EML YD_EML
        , EML.SND_USR_ID EML_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = EML.SND_USR_ID) EML_SENDER_NM
        , NVL (EML.SND_RQST_DT,(SELECT EML_DT
                  FROM COM_EML_SND_INFO
                 WHERE EML_SND_NO = EML.SND_ID)) AS EML_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT EML_PROC_STS_CD
                          FROM COM_EML_SND_INFO
                         WHERE EML_SND_NO = EML.SND_ID),
                  DECODE((SELECT EML_PROC_STS_CD
                            FROM COM_EML_SND_INFO
                           WHERE EML_SND_NO = EML.SND_ID),1,2,2,2,3,3,4,4,1),
                  NVL2(EML.BKG_NTC_SND_RSLT_CD,
                  DECODE(EML.BKG_NTC_SND_RSLT_CD,3,3,4,4,1),1)) ) AS EML_SEND_RESULT
        , (SELECT EML_ERR_MSG
             FROM COM_EML_SND_INFO
            WHERE EML_SND_NO = EML.SND_ID) AS EML_FAIL_REASON
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_ALL_FLG ELSE EML.FRT_ALL_FLG END,'N') FRT_ALL_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_CLT_FLG ELSE EML.FRT_CLT_FLG END,'N') FRT_CLT_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_PPD_FLG ELSE EML.FRT_PPD_FLG END,'N') FRT_PPD_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_CHG_FLG ELSE EML.FRT_CHG_FLG END,'N') FRT_CHG_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_ARR_FLG ELSE EML.FRT_ARR_FLG END,'N') FRT_ARR_FLG
        , NVL(CASE WHEN FAX.SND_RQST_DT > EML.SND_RQST_DT THEN FAX.FRT_HDN_FLG ELSE EML.FRT_HDN_FLG END,'N') FRT_HDN_FLG
        , 'Display Hidden' DISPLAY_HIDDEN
		, BKG_DRAFT_REMARK_FNC(BK.BKG_NO, '6', 'N','') REMARK
		, 'Y' ENABLE
        , BK.BKG_NO
        , BK.POR_CD
        , NVL((
            SELECT BL_ESIG_FLG 
            FROM BKG_BL_ESIG_OFC_ASGN
            WHERE BL_ISS_OFC_CD = DECODE((SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO), NULL, BK.BKG_OFC_CD, 
                                         (SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO))
        ),'N') AS BL_ESIG_FLG
        , NVL((
            SELECT BL_CPY_ESIG_FLG
            FROM BKG_BL_ESIG_OFC_ASGN
            WHERE BL_ISS_OFC_CD = DECODE((SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO), NULL, BK.BKG_OFC_CD, 
                                         (SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS WHERE BKG_NO = BK.BKG_NO))
        ),'N') AS BL_CPY_ESIG_FLG
  FROM BKG_BOOKING BK
        , BKG_CNTC_PSON CNTC
        , BKG_NTC_HIS FAX
        , BKG_NTC_HIS EML
 WHERE BK.BKG_NO = @[bkg_no]
   AND BK.BKG_NO = FAX.BKG_NO (+)
   AND 'F'       = FAX.NTC_VIA_cD(+)
   AND 'BL'      = FAX.NTC_KND_CD(+)
   AND BK.BKG_NO = EML.BKG_NO (+)
   AND 'M'       = EML.NTC_VIA_cD(+)
   AND 'BL'      = EML.NTC_KND_CD(+)   
   AND BK.BKG_NO = CNTC.BKG_NO
   AND CNTC.BKG_CNTC_PSON_TP_CD = 'SI'
   AND ROWNUM    = 1
UNION
--N/N Copy
SELECT /*+ INDEX_DESC(FAX XPKBKG_NTC_HIS) INDEX_DESC(EML XPKBKG_NTC_HIS) */4 ORD
		, (SELECT INTG_CD_VAL_DP_DESC 
          FROM COM_INTG_CD_DTL 
         WHERE INTG_CD_ID = 'CD01552'
           AND INTG_CD_VAL_CTNT = 'NN') NTC_KND_NM
        , 'NN' NTC_KND_CD
        --------------------------
        , NVL(FAX.NTC_FAX_NO, CNTC.CNTC_PSON_FAX_NO) FAX_NO
        , FAX.SND_USR_ID FAX_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = FAX.SND_USR_ID) FAX_SENDER_NM
        , NVL (FAX.SND_RQST_DT,
          NVL((SELECT XPT_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID),
           (SELECT UPD_DT FROM COM_FAX_SND_INFO WHERE FAX_SND_NO = FAX.SND_ID)
          )) AS FAX_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT FAX_PROC_STS_CD
                          FROM COM_FAX_SND_INFO
                         WHERE FAX_SND_NO = FAX.SND_ID),
                  DECODE((SELECT FAX_PROC_STS_CD
                            FROM COM_FAX_SND_INFO
                           WHERE FAX_SND_NO = FAX.SND_ID),1,2,2,2,3,2,4,4,5,3,6,4,1),
                  NVL2(FAX.BKG_NTC_SND_RSLT_CD,
                  DECODE(FAX.BKG_NTC_SND_RSLT_CD,4,2,5,3,6,4,1),1))) AS FAX_SEND_RESULT
        , (SELECT NVL(XPT_ERR_MSG, XPT_ERR_DTL_MSG)
             FROM COM_FAX_SND_INFO
            WHERE FAX_SND_NO = FAX.SND_ID) AS FAX_FAIL_REASON
        --------------------------
        , NVL(EML.NTC_EML,    CNTC.CNTC_PSON_EML) EML
		, EML.NTC_EML YD_EML
        , EML.SND_USR_ID EML_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = EML.SND_USR_ID) EML_SENDER_NM
        , NVL (EML.SND_RQST_DT,(SELECT EML_DT
                  FROM COM_EML_SND_INFO
                 WHERE EML_SND_NO = EML.SND_ID)) AS EML_SEND_DT
        , (SELECT INTG_CD_VAL_DP_DESC
             FROM COM_INTG_CD_DTL
            WHERE 'CD02396' = INTG_CD_ID
              AND INTG_CD_VAL_CTNT =
                  NVL2((SELECT EML_PROC_STS_CD
                          FROM COM_EML_SND_INFO
                         WHERE EML_SND_NO = EML.SND_ID),
