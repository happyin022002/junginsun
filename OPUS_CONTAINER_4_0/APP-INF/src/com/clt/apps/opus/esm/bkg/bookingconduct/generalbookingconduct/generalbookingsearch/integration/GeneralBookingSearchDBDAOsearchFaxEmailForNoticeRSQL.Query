				   WHERE USR_LOC.LOC_CD= USR.LOC_CD
					 AND USR.OFC_CD    = @[ofc_cd]
				     AND BK.POL_CD     = BK_LOC.LOC_CD
				     AND BK.BKG_NO     = @[bkg_no]), 'O', 'HO', 'HI') NTC_KND_CD
        --------------------------
        , FAX.NTC_FAX_NO FAX_NO
        , FAX.SND_USR_ID FAX_SENDER
        , (SELECT USR_NM FROM COM_USER USR WHERE USR.USR_ID = FAX.SND_USR_ID) FAX_SENDER_NM
        , NVL (FAX.SND_RQST_DT,
