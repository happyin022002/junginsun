<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOUpdateLaneAdjustUSQL">
			<desc><![CDATA[해당 버전의 Lane Adjust Data를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE SAQ_MON_FCAST_TRNS 
       SET RLANE_CD = @[rlane_cd] 
     , UPD_USR_ID = @[user_id] 
     , UPD_DT = SYSDATE 
 WHERE 1=1 
       AND MQTA_MDL_VER_NO = @[mqta_mdl_ver_no] 
       AND TRD_CD = @[trd_cd] 
       AND SUB_TRD_CD = @[sub_trd_cd] 
       AND RLANE_CD = @[org_rlane_cd]
       AND (FCAST_TRNS_STS_CD = '0' OR FCAST_TRNS_STS_CD IS NULL)			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="mqta_mdl_ver_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="org_rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
