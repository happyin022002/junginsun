<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAORsltReturnVORSQL">
			<desc><![CDATA[Returned 가 terms를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD, CNT
FROM(
    SELECT 'MAIN' SVC_SCP_CD
          ,1 CNT
    FROM   PRI_SP_MQC
    WHERE           PROP_NO = @[prop_no]
    AND             AMDT_SEQ = @[amdt_seq]
    AND             PRC_PROG_STS_CD = 'R'
    UNION ALL
    SELECT DISTINCT SVC_SCP_CD
                   ,1 CNT
    FROM            PRI_SP_SCP_MQC
    WHERE           PROP_NO = @[prop_no]
    AND             AMDT_SEQ = @[amdt_seq]
    AND             PRC_PROG_STS_CD = 'R'
    UNION ALL
    SELECT DISTINCT SVC_SCP_CD
                   ,1 CNT
    FROM            PRI_SP_SCP_TRSP_ADD_CHG
    WHERE           PROP_NO = @[prop_no]
    AND             AMDT_SEQ = @[amdt_seq]
    AND             PRC_PROG_STS_CD = 'R'
    UNION ALL
    SELECT DISTINCT SVC_SCP_CD
                   ,1 CNT
    FROM            PRI_SP_SCP_GOH_CHG
    WHERE           PROP_NO = @[prop_no]
    AND             AMDT_SEQ = @[amdt_seq]
    AND             PRC_PROG_STS_CD = 'R'
    UNION ALL
    SELECT DISTINCT SVC_SCP_CD
                   ,1 CNT
    FROM            PRI_SP_SCP_RT
    WHERE           PROP_NO = @[prop_no]
    AND             AMDT_SEQ = @[amdt_seq]
    AND             PRC_PROG_STS_CD = 'R'
    )			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
