<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOUpdateQtaEditIasSectorUSQL">
			<desc><![CDATA[[Qta Edit for IAS Sector]을 [저장] 합니다.]]></desc>
			<sql><![CDATA[
UPDATE CSQ_SCTR_CFM_QTA 
   SET LOD_QTY       = @[lod_qty] 
      ,GRS_RPB_REV   = @[grs_rpb_rev]
      ,CSQ_CNG_TP_CD = 'M'
      ,UPD_USR_ID    = @[upd_usr_id]
      ,UPD_DT        = SYSDATE 
 WHERE BSE_TP_CD                  = 'Q'
   AND BSE_YR                     = @[bse_yr]
   AND BSE_QTR_CD                 = @[bse_qtr_cd]
   AND OFC_VW_CD                  = @[ofc_vw_cd]
   AND SUBSTR(QTA_RLSE_VER_NO,-2) = '02'
   AND TRD_CD                     = @[trd_cd]
   AND SUB_TRD_CD                 = @[sub_trd_cd]
   AND RLANE_CD                   = @[rlane_cd]
   AND DIR_CD                     = @[dir_cd]
   AND RHQ_CD                     = @[rhq_cd]
   AND RGN_OFC_CD                 = @[rgn_ofc_cd]
   AND VSL_CD                     = @[vsl_cd]
   AND SKD_VOY_NO                 = @[skd_voy_no]
   AND SKD_DIR_CD                 = @[skd_dir_cd]
   AND POL_CD                     = @[pol_cd]
   AND POD_CD                     = @[pod_cd]
   AND CSQ_CNG_TP_CD              <> 'A'			]]></sql>
			<params>
				<param name="lod_qty" type="12" value="" out="N"/>
				<param name="grs_rpb_rev" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="ofc_vw_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
