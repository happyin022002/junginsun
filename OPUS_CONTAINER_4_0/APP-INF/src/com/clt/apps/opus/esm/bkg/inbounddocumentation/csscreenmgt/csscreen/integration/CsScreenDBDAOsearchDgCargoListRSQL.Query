<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CsScreenDBDAOsearchDgCargoListRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT LPAD(A.CNTR_CGO_SEQ, 3, '0') AS CNTR_CGO_SEQ
      ,A.IMDG_UN_NO                 AS IMDG_UN_NO
      ,A.IMDG_CLSS_CD               AS IMDG_CLSS_CD
      ,A.PRP_SHP_NM                 AS PRP_SHP_NM
      ,A.HZD_DESC                   AS HZD_DESC
      ,B.FILE_SAV_ID
      ,B.FILE_NM                    AS FILE_NM
      ,B.FILE_PATH_RMK              AS FILE_PATH_RMK
FROM BKG_DG_CGO A
	,BKG_IMG_STO B
WHERE A.BKG_NO = @[bkg_no]
AND A.CNTR_NO = @[cntr_no]
AND A.BKG_NO = B.BKG_NO(+)
AND A.DCGO_SEQ = B.DCGO_SEQ(+)
ORDER BY LPAD(A.CNTR_CGO_SEQ, 3, '0')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
