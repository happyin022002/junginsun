<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchDgCgoforNewBkgInfoRSQL">
			<desc><![CDATA[searchDgCgoforNewBkgInfo]]></desc>
			<sql><![CDATA[
SELECT DC.BKG_NO,
       @[bkg_skd_seq] AS BKG_SKD_SEQ,
       DC.DCGO_SEQ,
       DC.CNTR_TPSZ_CD,
       DC.CNTR_VOL_QTY,
       DC.CNTR_NO,
       DC.MRN_POLUT_FLG,
       DC.IMDG_LMT_QTY_FLG,
       DC.IMDG_CLSS_CD,
       DC.IMDG_UN_NO,
       DC.IMDG_PCK_GRP_CD

  FROM BKG_DG_CGO DC,
       BKG_VVD BV
 
 WHERE DC.BKG_NO LIKE SUBSTR(@[bkg_no], 1, 10)||'%'
   AND BV.VSL_Cd = @[vsl_cd]
   AND BV.SKD_VOY_NO = @[skd_voy_no]
   AND BV.SKD_DIR_CD = @[skd_dir_cd]
   AND BV.POL_CD = @[pol_cd]
   AND DC.BKG_NO = BV.BKG_NO			]]></sql>
			<params>
				<param name="bkg_skd_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
