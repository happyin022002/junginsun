<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAODocQueueBkgHistListVOUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE BKG_SR_CRNT_RQST SET 
	SR_URG_CD = @[sr_urg_cd]
,	SR_AMD_TP_CD = @[sr_amd_tp_cd]
,	IMG_PG_NO = @[img_pg_no]
,	DIFF_RMK = @[diff_rmk]
WHERE	SR_KND_CD = @[sr_knd_cd]
AND	SR_NO = @[sr_no]
AND	BKG_NO = @[bkg_no]
AND	SR_AMD_TP_CD = @[sr_amd_tp_cd]
AND	SR_AMD_SEQ = @[sr_amd_seq]			]]></sql>
			<params>
				<param name="sr_urg_cd" type="12" value="" out="N"/>
				<param name="sr_amd_tp_cd" type="12" value="" out="N"/>
				<param name="img_pg_no" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sr_amd_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
