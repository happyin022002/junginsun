<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOmodifyBkbcFrtUsCgoUSQL">
			<desc><![CDATA[...]]></desc>
			<sql><![CDATA[
UPDATE BKG_CGO_RLSE
   SET FRT_CLT_FLG    = NVL(@[frt_clt_flg],'N'),  /* 변수 치환 frtCltFlg */
       FRT_CLT_LST_DT = GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC,SYSDATE,'USNYC'),
       UPD_USR_ID     = @[evnt_usr_id], /* 변수 치환 evntUsrId */
       UPD_DT         = SYSDATE        
 WHERE BL_NO          = @[bl_no]  /* 변수 치환 blNo */			]]></sql>
			<params>
				<param name="frt_clt_flg" type="12" value="" out="N"/>
				<param name="evnt_usr_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
