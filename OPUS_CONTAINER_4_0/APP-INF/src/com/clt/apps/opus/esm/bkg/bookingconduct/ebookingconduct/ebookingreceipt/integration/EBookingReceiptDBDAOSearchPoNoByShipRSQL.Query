<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchPoNoByShipRSQL">
			<desc><![CDATA[CntrEtcInfo]]></desc>
			<sql><![CDATA[
SELECT 
 	BKG_NO,
 	REF_SEQ,
 	DE_NO,
 	PRT_NO,
 	CNTR_NO
FROM BKG_REF_DTL
WHERE BKG_NO = @[bkg_no] 
AND (DE_NO IS NOT NULL OR PRT_NO IS NOT NULL)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
