<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAOPriMotChgRtVOUSQL">
			<desc><![CDATA[update PriMotChgRt]]></desc>
			<sql><![CDATA[
UPDATE PRI_MOT_CHG_RT
   SET CUST_CNT_CD         = @[cust_cnt_cd]
     , CUST_SEQ            = @[cust_seq]
     , CUST_NM             = @[cust_nm]
     , SOC_FLG             = @[soc_flg]
     , PRC_CGO_TP_CD       = @[prc_cgo_tp_cd]
     , MOT_FILE_CNTR_TP_CD = @[mot_file_cntr_tp_cd]
     , MOT_FILE_LANE_CD    = @[mot_file_lane_cd]
     , PAY_TERM_CD         = @[pay_term_cd]
     , AGN_CD              = @[agn_cd]
     , EFF_DT              = TO_DATE(@[eff_dt], 'YYYYMMDD')
     , EXP_DT              = TO_DATE(NVL(@[exp_dt],'99991231'), 'YYYYMMDD')
     , CURR_CD             = @[curr_cd]
     , CNTR_20FT_RT_AMT    = @[cntr_20ft_rt_amt]
     , CNTR_40FT_RT_AMT    = @[cntr_40ft_rt_amt]
     , CNTR_45FT_RT_AMT    = @[cntr_45ft_rt_amt]
     , UPD_USR_ID          = @[upd_usr_id]
     , UPD_DT              = SYSDATE
     , VSL_SLAN_CD         = @[vsl_slan_cd]
 WHERE CHG_CD = @[chg_cd]
   AND POL_CD = @[pol_cd]
   AND CHG_RT_SEQ = @[chg_rt_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="soc_flg" type="12" value="" out="N"/>
				<param name="prc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="mot_file_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="mot_file_lane_cd" type="12" value="" out="N"/>
				<param name="pay_term_cd" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="cntr_20ft_rt_amt" type="12" value="" out="N"/>
				<param name="cntr_40ft_rt_amt" type="12" value="" out="N"/>
				<param name="cntr_45ft_rt_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="chg_rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
