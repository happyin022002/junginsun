<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOBeforeActualINFCSQL">
			<desc><![CDATA[BeforeActualINF]]></desc>
			<sql><![CDATA[
INSERT
      INTO AGT_AGN_COMM(
			SELECT 
				   BKG_NO,
				   AGN_CD,
				   IO_BND_CD,
				   AC_TP_CD,
				   @[ac_seq] + 1,
				   COMM_OCCR_INFO_CD,
				   COMM_YRMON,
				   AR_OFC_CD,
				   AP_OFC_CD,
				   AP_CTR_CD,
				   OFC_ENG_NM,
				   OTR_COMM_ACCT_CTNT,
				   COMM_STND_COST_CD,
				   COMM_SLAN_CD,
				   COMM_RLANE_CD,
				   COMM_VSL_CD,
				   COMM_SKD_VOY_NO,
				   COMM_SKD_DIR_CD,
				   COMM_REV_DIR_CD,
				   'IC'                       AS COMM_PROC_STS_CD,
				   'Interface Cancel!'        AS COMM_PROC_STS_RSN,
				   NULL                       AS COMM_APRO_NO,
				   NULL                       AS ASA_NO,
				   AGN_AGMT_NO,
				   VNDR_CNT_CD,
				   VNDR_SEQ,
				   SAIL_ARR_DT,
				   CHG_DDCT_AMT,
				   FDRG_DDCT_AMT,
				   HLG_DDCT_AMT,
				   AGN_AGMT_RT,
				   @[act_pre_comm_amt]       AS ACT_PRE_COMM_AMT,
				   @[act_comm_amt]           AS ACT_COMM_AMT,
				   @[act_if_comm_amt]        AS ACT_IF_COMM_AMT,
				   @[act_pre_locl_comm_amt]  AS ACT_PRE_LOCL_COMM_AMT,
				   @[act_locl_comm_amt]      AS ACT_LOCL_COMM_AMT,
				   @[act_if_locl_comm_amt]   AS ACT_IF_LOCL_COMM_AMT,
				   CURR_CD,
				   XCH_RT_APLY_LVL,
				   VVD_XCH_RT,
				   MON_XCH_RT,
				   DLY_XCH_RT,
				   OFC_CHR_LVL,
				   OFFST_AGN_FLG,
					   'N'                        AS ACCL_FLG,
				   NULL                       AS AC_APRO_USR_ID,
				   NULL                       AS AC_APRO_DT,
				   NULL                       AS AC_IF_USR_ID,
				   NULL                       AS AC_IF_DT,
				   APLY_DT,
				   NULL                       AS CSR_NO,
				   INV_TAX_RT,
				   CRE_USR_ID,
				   CRE_DT,
				   @[upd_usr_id]              AS UPD_USR_ID,
				   SYSDATE,
				   NULL                       AS AC_RQST_USR_ID,
				   NULL                       AS AC_RQST_USR_EML,
				   NULL                       AS AC_RQST_DT,
				   AC_APRO_USR_EML,
				   INV_NO,
				   AGN_APRO_RMK,
				   ESTM_USD_AMT,
				   GL_DT,
				   GRS_NET_DIV_CD
			  FROM AGT_AGN_COMM
			 WHERE BKG_NO          = @[bkg_no]
			   AND AGN_CD          = @[agn_cd]
			   AND IO_BND_CD       = @[io_bnd_cd]
			   AND AC_TP_CD        = @[ac_tp_cd]
			   AND AC_SEQ          = @[ac_seq]
	)			]]></sql>
			<params>
				<param name="ac_seq" type="12" value="" out="Y"/>
				<param name="act_pre_comm_amt" type="12" value="" out="Y"/>
				<param name="act_comm_amt" type="12" value="" out="Y"/>
				<param name="act_if_comm_amt" type="12" value="" out="Y"/>
				<param name="act_pre_locl_comm_amt" type="12" value="" out="Y"/>
				<param name="act_locl_comm_amt" type="12" value="" out="Y"/>
				<param name="act_if_locl_comm_amt" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="bkg_no" type="12" value="" out="Y"/>
				<param name="agn_cd" type="12" value="" out="Y"/>
				<param name="io_bnd_cd" type="12" value="" out="Y"/>
				<param name="ac_tp_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
