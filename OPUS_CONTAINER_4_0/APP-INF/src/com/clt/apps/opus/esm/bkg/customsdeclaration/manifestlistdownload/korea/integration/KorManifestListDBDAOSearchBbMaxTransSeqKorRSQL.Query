<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchBbMaxTransSeqKorRSQL">
			<desc><![CDATA[1]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(TRNS_SEQ),0) MAX_SEQ
  FROM BKG_CSTMS_KR_BL
 WHERE BKG_NO = SUBSTR(@[bkg_no], 1,2)||'M'||SUBSTR(@[bkg_no], 4)
   AND CSTMS_DECL_TP_CD = @[kcd_tp]
   AND DMST_PORT_CD = @[kt_port]
   AND CSTMS_BL_NO =  SUBSTR(@[bkg_no], 1,2)||'M'||SUBSTR(@[bkg_no], 4)
   AND MF_SND_DT IS NOT NULL   			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="kcd_tp" type="12" value="" out="N"/>
				<param name="kt_port" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
