<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOaddDgListHisCSQL">
			<desc><![CDATA[위험물 정보들을 신규생성한다.]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_CSTMS_EUR_DG_HIS
      (EUR_DG_DECL_TP_CD
      ,VSL_CD
      ,SKD_VOY_NO
      ,SKD_DIR_CD
      ,PORT_CD
      ,CNTR_NO
      ,CNTR_CGO_SEQ
      ,HIS_SEQ
      ,CNTR_TPSZ_CD
      ,BL_NO
      ,POL_CD
      ,POD_CD
      ,CELL_PSN_NO
      ,ANR_FWRD_ID
      ,SVC_RQST_NO
      ,ANR_CRR_TP_CD
      ,FDR_SVC_RQST_NO
      ,FDR_VVD_ID
      ,FDR_VSL_NM
      ,FDR_VSL_LLOYD_NO
      ,IMDG_PCK_GRP_CD
      ,IMDG_CLSS_CD
      ,IMDG_UN_NO
      ,IMDG_UN_NO_SEQ
      ,ANR_SPCL_TP_ID
      ,FLSH_PNT_CDO_TEMP
      ,NET_WGT
      ,GRS_WGT
      ,MFAG_NO
      ,PRP_SHP_NM
      ,HZD_DESC
      ,BRTH_YD_CD
      ,BRTH_YD_NM
      ,IMDG_LMT_QTY_FLG
      ,PCK_QTY
      ,PCK_TP_CD
      ,OUT_IMDG_PCK_QTY1
      ,OUT_IMDG_PCK_CD1
      ,EUR_OUTR_PCK_DESC
      ,IN_IMDG_PCK_QTY1
      ,IN_IMDG_PCK_CD1
      ,EUR_INR_PCK_DESC
      ,EMS_NO
      ,ANR_ROLE_DIV_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,EUR_PCK_DESC
      ,EUR_DCGO_MRN_POLUT_CD
      ,XTD_STAY_PRMT_NO
      ,DIFF_RMK
      ,CRR_DT
      ,NET_EXPLO_WGT
      ,CGO_OPR_CD
      ,APLY_NO
      ,CNTR_REF_NO
	  ,EMER_CNTC_PHN_NO
	  ,EMER_CNTC_PSON_NM
      ,IMDG_SUBS_RSK_LBL_CD1
      ,IMDG_SUBS_RSK_LBL_CD2
      ,IMDG_SUBS_RSK_LBL_CD3
      ,IMDG_SUBS_RSK_LBL_CD4
      ,EMER_RSPN_GID_NO
      ,DCGO_REF_NO
      ,SPCL_CNTR_SEQ
      ,SPCL_CGO_SEQ
      )
SELECT EUR_DG_DECL_TP_CD
      ,VSL_CD
      ,SKD_VOY_NO
      ,SKD_DIR_CD
      ,PORT_CD
      ,CNTR_NO
      ,CNTR_CGO_SEQ
      ,(SELECT NVL(MAX(HIS_SEQ),0) + 1
          FROM BKG_CSTMS_EUR_DG_HIS
         WHERE 0=0
           AND VSL_CD = SUBSTR(@[vvd_cd],1,4)
           AND SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
           AND SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
           AND CNTR_NO = @[cntr_no]
           AND BL_NO = @[bl_no]
       )
      ,CNTR_TPSZ_CD
      ,BL_NO
      ,POL_CD
      ,POD_CD
      ,CELL_PSN_NO
      ,ANR_FWRD_ID
      ,SVC_RQST_NO
      ,ANR_CRR_TP_CD
      ,FDR_SVC_RQST_NO
      ,FDR_VVD_ID
      ,FDR_VSL_NM
      ,FDR_VSL_LLOYD_NO
      ,IMDG_PCK_GRP_CD
      ,IMDG_CLSS_CD
      ,IMDG_UN_NO
      ,IMDG_UN_NO_SEQ
      ,ANR_SPCL_TP_ID
      ,FLSH_PNT_CDO_TEMP
      ,NET_WGT
      ,GRS_WGT
      ,MFAG_NO
      ,PRP_SHP_NM
      ,HZD_DESC
      ,BRTH_YD_CD
      ,BRTH_YD_NM
      ,IMDG_LMT_QTY_FLG
      ,PCK_QTY
      ,PCK_TP_CD
      ,OUT_IMDG_PCK_QTY1
      ,OUT_IMDG_PCK_CD1
      ,EUR_OUTR_PCK_DESC
      ,IN_IMDG_PCK_QTY1
      ,IN_IMDG_PCK_CD1
      ,EUR_INR_PCK_DESC
      ,EMS_NO
      ,ANR_ROLE_DIV_CD
      ,@[upd_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,TO_DATE(@[upd_dt],'YYYY/MM/DD HH24:MI:SS')
      ,EUR_PCK_DESC
      ,EUR_DCGO_MRN_POLUT_CD
      ,XTD_STAY_PRMT_NO
      ,DIFF_RMK
      ,CRR_DT
      ,NET_EXPLO_WGT
      ,CGO_OPR_CD
      ,APLY_NO
      ,CNTR_REF_NO
      ,EMER_CNTC_PHN_NO
	  ,EMER_CNTC_PSON_NM
      ,IMDG_SUBS_RSK_LBL_CD1
      ,IMDG_SUBS_RSK_LBL_CD2
      ,IMDG_SUBS_RSK_LBL_CD3
      ,IMDG_SUBS_RSK_LBL_CD4
      ,EMER_RSPN_GID_NO
      ,DCGO_REF_NO
      ,SPCL_CNTR_SEQ
      ,SPCL_CGO_SEQ
  FROM BKG_CSTMS_EUR_DG
 WHERE EUR_DG_DECL_TP_CD = @[d_type]
   AND VSL_CD = SUBSTR(@[vvd_cd],1,4)
   AND SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
   AND SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
   AND PORT_CD = @[port_cd]
   AND CNTR_NO = @[cntr_no]
   AND BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
