<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OrganizationDBDAOMdmOrzVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT OFC_TP_CD
     , OFC_CD
     , OFC_ENG_NM
     , LOC_CD
     , PRNT_OFC_CD
  FROM MDM_ORGANIZATION
 WHERE OFC_CD LIKE @[ofc_cd] || '%'
   AND DELT_FLG = 'N'
ORDER BY OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
