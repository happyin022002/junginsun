<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOSearchSitProCntrSealNoRSQL">
			<desc><![CDATA[SITPRO 전송 시 Seal No. 를 조회]]></desc>
			<sql><![CDATA[
SELECT ROWNUM SEAL_SEQ
     , CNTR_SEAL_NO		AS SEAL_NBR
     , PRN_FLG
FROM BKG_CNTR_SEAL_NO
WHERE 1=1
AND CNTR_SEAL_SEQ > 0
AND BKG_NO = @[bkg_no]
AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
