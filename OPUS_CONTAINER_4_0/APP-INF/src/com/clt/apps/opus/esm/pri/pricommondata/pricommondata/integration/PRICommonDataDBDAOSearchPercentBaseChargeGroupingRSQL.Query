<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDataDBDAOSearchPercentBaseChargeGroupingRSQL">
			<desc><![CDATA[retrieve CHG_CD about PCT_BSE_CD]]></desc>
			<sql><![CDATA[
SELECT A.PCT_BSE_CD
     , A.CHG_CD
	 , B.CHG_NM
FROM   PRI_SCG_PCT_BSE_CHG A,
		MDM_CHARGE B
WHERE A.CHG_CD = B.CHG_CD
AND	NVL( B.DELT_FLG, 'N' ) != 'Y'
AND A.PCT_BSE_CD = @[pct_bse_cd]			]]></sql>
			<params>
				<param name="pct_bse_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
