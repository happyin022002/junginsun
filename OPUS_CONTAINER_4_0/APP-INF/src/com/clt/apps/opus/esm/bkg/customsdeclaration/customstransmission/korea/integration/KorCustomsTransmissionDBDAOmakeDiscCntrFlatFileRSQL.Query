<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOmakeDiscCntrFlatFileRSQL">
			<desc><![CDATA[CNTR Flat Buffer Creation]]></desc>
			<sql><![CDATA[
SELECT  SUBSTR(MAX(TO_CHAR(KC.TRNS_SEQ, '00000')||KC.CNTR_NO),7)
FROM    BKG_CSTMS_KR_CNTR KC
WHERE   KC.BKG_NO           =   @[bkg_no]
AND     KC.CSTMS_DECL_TP_CD =   @[cstms_decl_tp_cd]
AND     KC.DMST_PORT_CD     =   @[port_cd]
AND     KC.CSTMS_BL_NO 		=   @[c_bl_no]
GROUP BY KC.CNTR_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="c_bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
