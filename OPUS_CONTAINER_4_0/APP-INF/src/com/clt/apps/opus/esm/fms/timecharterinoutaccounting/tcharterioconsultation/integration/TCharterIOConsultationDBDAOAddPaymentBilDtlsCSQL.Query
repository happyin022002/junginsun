<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOAddPaymentBilDtlsCSQL">
			<desc><![CDATA[TCharterIOConsultationDBDAOAddPaymentBilDtlsCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO FMS_BIL_DTL(
	BIL_INV_YRMON,
	OFC_CD,
	BIL_SER_NO,
	BIL_DTL_SER_NO,
	ITM_NM,
	SPL_AMT,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
) VALUES (
	@[tax_inv_yrmon],
	@[ofc_cd],
	(SELECT LPAD(TO_NUMBER(NVL(MAX(BIL_SER_NO),0)),5,'0')
       FROM FMS_BILL
      WHERE BIL_INV_YRMON = @[tax_inv_yrmon]
        AND OFC_CD = @[ofc_cd]),
	TO_NUMBER(@[tax_dtl_ser_no]),
	@[itm_nm],
	@[spl_amt],
	@[cre_usr_id],
	SYSDATE,
	@[upd_usr_id],
	SYSDATE
)			]]></sql>
			<params>
				<param name="tax_inv_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="tax_dtl_ser_no" type="12" value="" out="N"/>
				<param name="itm_nm" type="12" value="" out="N"/>
				<param name="spl_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
