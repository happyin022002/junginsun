<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchSpecialCgoRSQL">
			<desc><![CDATA[searchSpecialCgo]]></desc>
			<sql><![CDATA[
#if (${in_spcl_cgo_type} == 'D') 
SELECT	
	'CLS:'||NVL(IMDG_CLSS_CD,' ')||' UN:'||NVL(IMDG_UN_NO,' ')||DECODE(NVL(TRIM(HCDG_FLG), 'N'), 'Y', ' HCDG', '') SPCL_CGO_DESC
FROM	BKG_DG_CGO
WHERE	BKG_NO       = @[in_bkg_no]
AND	CNTR_NO      = @[in_cntr_no]
AND	ROWNUM       = 1
#elseif (${in_spcl_cgo_type} == 'R') 
SELECT	TO_CHAR(NVL(CDO_TEMP,0),9990.9)||'C'  SPCL_CGO_DESC
FROM	BKG_RF_CGO
WHERE	BKG_NO       = @[in_bkg_no]
AND	CNTR_NO      = @[in_cntr_no]
AND	ROWNUM       = 1
#elseif (${in_spcl_cgo_type} == 'A') 
SELECT	'F:'||TO_CHAR(NVL(OVR_FWRD_LEN,0))||'/'||
		'B:'||TO_CHAR(NVL(OVR_BKWD_LEN,0))||'/'||
		'H:'||TO_CHAR(NVL(OVR_HGT,0))||'/'||
		'P:'||TO_CHAR(NVL(OVR_LF_LEN,0))||'/'||
		'S:'||TO_CHAR(NVL(OVR_RT_LEN,0))  SPCL_CGO_DESC
FROM	BKG_AWK_CGO
WHERE	BKG_NO       = @[in_bkg_no]
AND	CNTR_NO      = @[in_cntr_no]
AND	ROWNUM = 1
#end			]]></sql>
			<params>
				<param name="in_bkg_no" type="12" value="" out="N"/>
				<param name="in_cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
