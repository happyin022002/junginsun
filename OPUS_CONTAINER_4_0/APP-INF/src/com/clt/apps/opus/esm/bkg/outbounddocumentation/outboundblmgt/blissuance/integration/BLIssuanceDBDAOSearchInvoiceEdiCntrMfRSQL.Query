<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchInvoiceEdiCntrMfRSQL">
			<desc><![CDATA[BLIssuanceDBDAOSearchInvoiceEdiCntrMf]]></desc>
			<sql><![CDATA[
SELECT '{CM_INFO' || CHR(10) 
       || 'CMD_CD:' || 'TEST' ||CHR(10) 
       || 'CMD_DESC:' || 'TEST123' ||CHR(10) 
       || '{PKG_INFO' || CHR(10) 
       || 'CM_PKG_LVL:' ||'1'|| CHR(10) 
       || 'CM_PKG_QTY:' || PCK_QTY || CHR(10) 
       || 'CM_PKG_UNIT:' || PCK_TP_CD || CHR(10)
       || 'CM_PKG_UNIT_DESC:' || (SELECT PCK_NM FROM MDM_PCK_TP WHERE PCK_CD = BC.PCK_TP_CD)|| CHR(10) 
       || '}PKG_INFO' || CHR(10) 
       || '{MEA_INFO' || CHR(10) 
       || 'CM_MEA_TP_CD:' || 'GVOL'|| CHR(10)
       || 'CM_MEA_UNIT:' ||MEAS_UT_CD ||CHR(10) 
       || 'CM_MEA_QTY:' || MEAS_QTY||CHR(10)    
       || '}MEA_INFO' || CHR(10)
       || '}CM_INFO' || CHR(10)
  FROM BKG_CNTR_MF_DESC BC
 WHERE BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
