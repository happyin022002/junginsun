<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOSearchTroExistRSQL">
			<desc><![CDATA[TransferOrderIssueDBDAOSearchTroExistRSQL]]></desc>
			<sql><![CDATA[
SELECT 'GENERAL' STR_RETURN
  FROM BKG_TRO TRO
     , BKG_BL_DOC BL
 WHERE TRO.BKG_NO      = BL.BKG_NO
   AND TRO.BKG_NO      = @[bkg_no]
   AND TRO.IO_BND_CD   = @[io_bnd_cd]
   AND TRO.RTN_TRO_FLG = 'N'
   AND ROWNUM          = 1
UNION
SELECT 'EUROPE' STR_RETURN
  FROM BKG_EUR_TRO TRO
     , BKG_BL_DOC BL
 WHERE TRO.BKG_NO      = BL.BKG_NO
   AND TRO.BKG_NO      = @[bkg_no]
   AND TRO.IO_BND_CD   = @[io_bnd_cd]
   AND ROWNUM          = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
