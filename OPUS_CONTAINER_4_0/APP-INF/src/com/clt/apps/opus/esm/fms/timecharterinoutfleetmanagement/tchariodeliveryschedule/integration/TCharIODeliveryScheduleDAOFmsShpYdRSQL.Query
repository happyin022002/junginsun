<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharIODeliveryScheduleDAOFmsShpYdRSQL">
			<desc><![CDATA[Ship Yard Select]]></desc>
			<sql><![CDATA[
SELECT FS.YD_SEQ,
       FS.SHP_YD_NM,
       NVL((SELECT 'Y' FROM DUAL
             WHERE NOT EXISTS (SELECT 1 
                                 FROM FMS_NEW_BLD_SKD FN 
                                WHERE FN.YD_SEQ = FS.YD_SEQ)
           ),'N') DELT_YN
  FROM FMS_SHP_YD FS
 ORDER BY FS.SHP_YD_NM			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
