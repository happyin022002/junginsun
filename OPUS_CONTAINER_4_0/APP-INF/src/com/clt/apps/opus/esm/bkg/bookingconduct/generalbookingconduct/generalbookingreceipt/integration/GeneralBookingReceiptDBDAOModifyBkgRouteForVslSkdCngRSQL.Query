<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModifyBkgRouteForVslSkdCngRSQL">
			<desc><![CDATA[PORT]]></desc>
			<sql><![CDATA[
SELECT 
	BKG_NO
FROM BKG_VVD
WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
AND ((POL_CD = @[port_cd] AND NVL(POL_CLPT_IND_SEQ, 1) = @[old_clpt_ind_seq]) OR (POD_CD = @[port_cd] AND NVL(POD_CLPT_IND_SEQ, 1) = @[old_clpt_ind_seq]))			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="old_clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
