<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOsearchOBUNLocCdRSQL">
			<desc><![CDATA[Outbound이기 때문에 POL에 대한 UNLOCODE 검색]]></desc>
			<sql><![CDATA[
SELECT NVL(UN_LOC_CD, LOC_CD) UNLOC_CODE
  FROM MDM_LOCATION
 WHERE LOC_CD = @[pol_cd]			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
