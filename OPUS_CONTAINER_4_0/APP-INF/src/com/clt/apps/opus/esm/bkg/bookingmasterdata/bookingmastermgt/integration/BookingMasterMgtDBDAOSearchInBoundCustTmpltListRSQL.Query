<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchInBoundCustTmpltListRSQL">
			<desc><![CDATA[ 0192 B/L Customer Information Template 조회			]]></desc>
			<sql><![CDATA[
SELECT     UPPER(CUST_CNT_CD) AS CUST_CNT_CD
 ,  UPPER(CUST_SEQ) AS CUST_SEQ
 ,  UPPER(TMPLT_SEQ) AS TMPLT_SEQ
 ,  UPPER(CUST_NM) AS CUST_NM
 ,  UPPER(CUST_ADDR) AS CUST_ADDR
 ,  UPPER(CUST_PHN_NO) AS CUST_PHN_NO
 ,  UPPER(ATND_NM) AS PIC
 ,  UPPER(CRE_USR_ID) AS CRE_USR_ID
 ,  UPPER(CUST_FAX_NO) AS CUST_FAX_NO
 ,  UPPER(CUST_CTY_NM) AS CUST_CTY_NM
 ,  UPPER(CUST_STE_CD) AS CUST_STE_CD
 ,  UPPER(CUST_ZIP_CD) AS CUST_ZIP_CD
 ,  UPPER(CUST_EML) AS CUST_EML
 ,  UPPER(TMPLT_RMK) AS TMPLT_RMK
 ,  UPPER(CRE_USR_ID) AS CRE_USR_ID
 ,  UPPER(UPD_USR_ID) AS UPD_USR_ID
,  CSTMS_DECL_CNT_CD AS CSTMS_DECL_CNT_CD
, UPPER(EUR_CSTMS_ST_NM) AS EUR_CSTMS_ST_NM
, UPPER(EORI_NO) AS EORI_NO
FROM   BKG_CUST_TMPLT
WHERE CUST_CNT_CD = @[cust_cnt_cd]
AND   CUST_SEQ    = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
