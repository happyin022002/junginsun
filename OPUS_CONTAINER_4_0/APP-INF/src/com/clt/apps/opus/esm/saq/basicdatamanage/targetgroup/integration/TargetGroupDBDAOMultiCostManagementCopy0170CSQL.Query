<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TargetGroupDBDAOMultiCostManagementCopy0170CSQL">
			<desc><![CDATA[TargetGroupDBDAOMultiCostManagementCopyCSQL : Management Copy (Insert)]]></desc>
			<sql><![CDATA[
INSERT INTO SAQ_COST_APPL_BSE (  
            BSE_YR                
          , BSE_QTR_CD            
          , COST_DIV_CD           
          , COST_DIV_DESC         
          , APPL_YR               
          , APPL_MON              
          , CRE_USR_ID            
          , CRE_DT                
          , UPD_USR_ID            
          , UPD_DT )              
   SELECT   @[bse_yr]                     
          , @[bse_qtr_cd]                     
          , COST_DIV_CD           
          , COST_DIV_DESC         
          , APPL_YR               
          , APPL_MON              
          , @[cre_usr_id]                     
          , SYSDATE               
          , @[cre_usr_id]                     
          , SYSDATE               
     FROM SAQ_COST_APPL_BSE       
    WHERE BSE_YR     = @[copyBse_yr]          
      AND BSE_QTR_CD = @[copyBse_qtr_cd]			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="copyBse_yr" type="12" value="" out="N"/>
				<param name="copyBse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
