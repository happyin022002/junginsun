<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOSearchBSATableHeaderListRSQL">
			<desc><![CDATA[SearchBSATableHeaderList]]></desc>
			<sql><![CDATA[
SELECT  
      A.BSA_OP_JB_CD
     ,(SELECT 
            NVL(BSA_OP_JB_DESC,'')
       FROM   
            BSA_OP_JB
       WHERE  
            BSA_OP_CD = @[bsa_op_cd]
        AND BSA_OP_JB_CD = A.BSA_OP_JB_CD
        ) AS BSA_OP_JB_NM
        ,A.CRR_CD
 FROM    
      BSA_CRR_RGST A
 WHERE   
      A.BSA_OP_CD = @[bsa_op_cd]
  AND ((A.BSA_OP_JB_CD = '001' AND A.CRR_CD <> COM_ConstantMgr_PKG.COM_getCompanyCode_FNC)
        OR A.BSA_OP_JB_CD  IN ('002','003','004','005'))
  AND A.APLY_FLG = 'Y'
 GROUP BY
         A.BSA_OP_JB_CD, 
         A.CRR_CD
 ORDER BY
         A.BSA_OP_JB_CD, 
         A.CRR_CD			]]></sql>
			<params>
				<param name="bsa_op_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
