<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOsearchPrnrCodCntrRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
select CNTR_NO
     , (select cntr_tpsz_cd from mst_container mst where mst.cntr_no = cntr.cntr_no) cntr_tpsz_cd
     , to_char(cntr_wgt, '999,999,990.000') cntr_wgt
     , DCGO_FLG
     , BB_CGO_FLG
     , AWK_CGO_FLG
     , RC_FLG
	 , soc_flg
     , nvl(CNTR_STWG_NO, '') cntr_stwg_no
  from bkg_cod_cntr cntr
 where bkg_no       = @[bkg_no]
   and cod_rqst_seq = @[cod_rqst_seq]
   and COD_SLCT_FLG = 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="KORX2025395" out="N"/>
				<param name="cod_rqst_seq" type="2" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
