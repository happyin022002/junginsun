<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOMndVORSQL">
			<desc><![CDATA[   Select]]></desc>
			<sql><![CDATA[
SELECT 
	col_nm
,	pre_ctnt
,	crnt_ctnt
,	ca_flg
,	cre_usr_id
,	cre_dt
,	upd_usr_id
