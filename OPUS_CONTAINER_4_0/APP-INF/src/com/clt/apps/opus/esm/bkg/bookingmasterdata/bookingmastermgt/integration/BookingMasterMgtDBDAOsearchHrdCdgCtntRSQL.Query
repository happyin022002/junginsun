<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOsearchHrdCdgCtntRSQL">
			<desc><![CDATA[searchHrdCdgCtnt에서 하나의 ID에 대한 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
HRD_CDG_ID,
HRD_CDG_ID_SEQ,
ATTR_CTNT1,
ATTR_CTNT2,
ATTR_CTNT3,
ATTR_CTNT4,
ATTR_CTNT5,
ATTR_CTNT6,
ATTR_CTNT7,
ATTR_CTNT8,
ATTR_CTNT9,
ATTR_CTNT10
FROM BKG_HRD_CDG_CTNT
WHERE HRD_CDG_ID = @[hrd_cdg_id]
ORDER BY HRD_CDG_ID_SEQ			]]></sql>
			<params>
				<param name="hrd_cdg_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
