<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAORfBkgInfoVORSQL">
			<desc><![CDATA[RfBkgInfoVO]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')

select a.BKG_NO
,   a.BL_NO
,	a.BKG_STS_CD
,   a.VSL_CD
,	a.RCV_TERM_CD
,	a.DE_TERM_CD
,	a.POL_CD
,	a.POL_NOD_CD
,	a.POD_CD
,	a.POD_NOD_CD
,   a.POR_CD
,   a.DEL_CD
,   a.CMDT_CD
,   a.CMDT_NM
,   b.CORR_N1ST_DT
,	b.CORR_NO
,   b.BDR_FLG
,   c.BKG_NO
,   c.PCK_QTY
,   c.PCK_TP_CD
,   c.GRS_WGT
,   c.WGT_UT_CD
,	d.VSL_PRE_PST_CD
from
(SELECT
a.BKG_NO
,   a.BL_NO
,	a.BKG_STS_CD
,   CONCAT(CONCAT(a.VSL_CD, a.SKD_VOY_NO),a.SKD_DIR_CD) VSL_CD
,	a.RCV_TERM_CD
,	a.DE_TERM_CD
,	a.POL_CD
,	SUBSTR(a.POL_NOD_CD, 6,2) POL_NOD_CD
,	a.POD_CD
,	SUBSTR(a.POD_NOD_CD, 6,2) POD_NOD_CD
,   a.POR_CD
,   a.DEL_CD
,   a.CMDT_CD
,   b.CMDT_NM
,   a.CORR_NO
FROM BKG_BKG_HIS a, MDM_COMMODITY b
where a.bkg_no = @[bkg_no]
and   a.CORR_NO = 'TMP0000001'
and   a.CMDT_CD = b.CMDT_CD
) a,
(select
BKG_NO
,   CORR_N1ST_DT
,	CORR_NO
,   BDR_FLG
from BKG_BL_DOC_HIS
where bkg_no = @[bkg_no]
and   CORR_NO = 'TMP0000001'
) b,
(select
BKG_NO
,   sum(PCK_QTY) PCK_QTY
,   PCK_TP_CD
,   sum(GRS_WGT) GRS_WGT
,   WGT_UT_CD
,   CORR_NO
from bkg_rf_cgo_his
where bkg_no = @[bkg_no]
and   CORR_NO = 'TMP0000001'
group by
BKG_NO
,   PCK_TP_CD
,   WGT_UT_CD
,   CORR_NO
) c,
(
SELECT BV.VSL_PRE_PST_CD, BV.BKG_NO, BK.CORR_NO
FROM BKG_BKG_HIS BK
,BKG_VVD BV
,MDM_VSL_SVC_LANE SL
WHERE BK.BKG_NO = @[bkg_no]
and   BK.CORR_NO = 'TMP0000001'
AND BK.BKG_NO = BV.BKG_NO
AND BV.SLAN_CD = SL.VSL_SLAN_CD
AND SL.SPCL_CGO_RQST_TGT_LANE_FLG = 'Y'
AND SL.VSL_SVC_TP_CD <> 'O'
AND BV.VSL_PRE_PST_CD = 'U'
) d
where a.BKG_NO = @[bkg_no]
and a.CORR_NO = 'TMP0000001'
and a.BKG_NO = b.BKG_NO
and a.BKG_NO = d.BKG_NO(+)
and b.BKG_NO = c.BKG_NO(+)
and a.CORR_NO = b.CORR_NO
and b.CORR_NO = c.CORR_NO(+)
and a.CORR_NO = d.CORR_NO(+)

#else

select a.BKG_NO
,   a.BL_NO
,	a.BKG_STS_CD
,   a.VSL_CD
,	a.RCV_TERM_CD
,	a.DE_TERM_CD
,	a.POL_CD
,	a.POL_NOD_CD
,	a.POD_CD
,	a.POD_NOD_CD
,   a.POR_CD
,   a.DEL_CD
,   a.CMDT_CD
,   a.CMDT_NM
,   b.CORR_N1ST_DT
,	b.CORR_NO
,   b.BDR_FLG
,   c.BKG_NO
,   c.PCK_QTY
,   c.PCK_TP_CD
,   c.GRS_WGT
,   c.WGT_UT_CD
,	d.VSL_PRE_PST_CD
from
(SELECT
a.BKG_NO
,   a.BL_NO
,	a.BKG_STS_CD
,   CONCAT(CONCAT(a.VSL_CD, a.SKD_VOY_NO),a.SKD_DIR_CD) VSL_CD
,	a.RCV_TERM_CD
,	a.DE_TERM_CD
,	a.POL_CD
,	SUBSTR(a.POL_NOD_CD, 6,2) POL_NOD_CD
,	a.POD_CD
,	SUBSTR(a.POD_NOD_CD, 6,2) POD_NOD_CD
,   a.POR_CD
,   a.DEL_CD
,   a.CMDT_CD
,   b.CMDT_NM
FROM BKG_BOOKING a, MDM_COMMODITY b
where a.bkg_no = @[bkg_no]
and   a.CMDT_CD = b.CMDT_CD
) a,
(select
BKG_NO
,   CORR_N1ST_DT
,	CORR_NO
,   BDR_FLG
from BKG_BL_DOC
where bkg_no = @[bkg_no]
) b,
(select
BKG_NO
,   sum(PCK_QTY) PCK_QTY
,   PCK_TP_CD
,   sum(GRS_WGT) GRS_WGT
,   WGT_UT_CD
from bkg_rf_cgo
where bkg_no = @[bkg_no]
group by
BKG_NO
,   PCK_TP_CD
,   WGT_UT_CD
) c,
(
SELECT BV.VSL_PRE_PST_CD, BV.BKG_NO
  FROM BKG_BOOKING BK
      ,BKG_VVD BV
      ,MDM_VSL_SVC_LANE SL
 WHERE BK.BKG_NO = @[bkg_no]
   AND BK.BKG_NO = BV.BKG_NO
   AND BV.SLAN_CD = SL.VSL_SLAN_CD
   AND SL.SPCL_CGO_RQST_TGT_LANE_FLG = 'Y'
   AND SL.VSL_SVC_TP_CD <> 'O'
   AND BV.VSL_PRE_PST_CD = 'U' 
) d
where a.BKG_NO = @[bkg_no]
and a.BKG_NO = b.BKG_NO
and a.BKG_NO = d.BKG_NO(+)
and b.BKG_NO = c.BKG_NO(+)

#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
