<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOcheckPckTpCdRSQL">
			<desc><![CDATA[checkPckTpCd]]></desc>
			<sql><![CDATA[
SELECT COUNT(0)
FROM MDM_PCK_TP
WHERE delt_flg = 'N'
AND pck_cd = @[pck_tp_cd]			]]></sql>
			<params>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
