<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialReportDBDAOReportTemplateListVO2RSQL">
			<desc><![CDATA[check user]]></desc>
			<sql><![CDATA[
SELECT 
	USR_ID
FROM BKG_RPT_SET
WHERE	USR_ID = @[usr_id]
AND	BKG_RPT_KND_CD = @[bkg_rpt_knd_cd]
AND	RPT_ID = @[rpt_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_rpt_knd_cd" type="12" value="" out="N"/>
				<param name="rpt_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
