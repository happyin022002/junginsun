<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndCustomsTransmissionDBDAOsearchBkgCstmsAdvBlRSQL">
			<desc><![CDATA[searchBkgCstmsAdvBl]]></desc>
			<sql><![CDATA[
SELECT  BL_NO
       ,MF_STS_CD
       ,MF_SND_DT
  FROM  BKG_CSTMS_ADV_BL
 WHERE  CNT_CD = 'CA'
   AND  BL_NO  = @[bl_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
