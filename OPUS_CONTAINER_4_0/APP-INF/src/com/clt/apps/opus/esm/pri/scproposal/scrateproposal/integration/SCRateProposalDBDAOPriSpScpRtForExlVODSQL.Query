<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAOPriSpScpRtForExlVODSQL">
			<desc><![CDATA[delete PRI_SP_SCP_RT for sc excel upload]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_SP_SCP_RT
WHERE	PROP_NO = @[prop_no]
AND	AMDT_SEQ = @[amdt_seq]
AND	SVC_SCP_CD = @[svc_scp_cd]
#if (${CASCADE_LEVEL} == '0') 

#elseif (${CASCADE_LEVEL} == '1') 
AND	GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
AND	CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#elseif (${CASCADE_LEVEL} == '2') 
AND	GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
AND	CMDT_HDR_SEQ = @[cmdt_hdr_seq]
AND	ROUT_SEQ = @[rout_seq]
#else 
AND	GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
AND	CMDT_HDR_SEQ = @[cmdt_hdr_seq]
AND	ROUT_SEQ = @[rout_seq]
AND	RT_SEQ = @[rt_seq]
#end
#if (${IS_DEL_AMEND} == 'Y') 
AND SRC_INFO_CD IN ('NW', 'GC', 'GM', 'PC', 'PM')
AND PRC_PROG_STS_CD = 'I'
#end
AND PRC_CGO_TP_CD = @[prc_cgo_tp_cd]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="rt_seq" type="12" value="" out="N"/>
				<param name="prc_cgo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
