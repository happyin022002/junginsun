<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateProposalDBDAOPriSpBlplCtntCopyVOCSQL">
			<desc><![CDATA[pri_sp_blpl_ctnt를 guideline에서 copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_BLPL_CTNT (
	PROP_NO
,	AMDT_SEQ
,	BLPL_SEQ
,	BLPL_CTNT_SEQ
,   DP_SEQ
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	BLPL_CTNT
,	N1ST_CMNC_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT 
	@[prop_no]
,	@[amdt_seq]
,	DENSE_RANK() OVER (ORDER BY BLPL_SEQ) BLPL_SEQ
,	DENSE_RANK() OVER (PARTITION BY BLPL_SEQ ORDER BY BLPL_CTNT_SEQ) BLPL_CTNT_SEQ
,	DENSE_RANK() OVER (PARTITION BY BLPL_SEQ ORDER BY DP_SEQ) DP_SEQ
,	'I'
,	'GC'
,	BLPL_CTNT
,	@[amdt_seq]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM PRI_SG_BLPL_CTNT
WHERE BLPL_HDR_SEQ = @[blpl_hdr_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="blpl_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
