<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOmodifyDgEdiCNTRInfoUSQL">
			<desc><![CDATA[EDI 전송후 컨테이너 정보 UPDATE]]></desc>
			<sql><![CDATA[
UPDATE  BKG_CSTMS_KR_DG_CGO 
SET     SND_DT      =   SYSDATE,
        SND_USR_ID  =   @[user_id]
WHERE   (BKG_NO, CSTMS_DECL_TP_CD, CNTR_NO, CNTR_SEQ)
        IN  (   SELECT BKG_NO, CSTMS_DECL_TP_CD, CNTR_NO, MAX(CNTR_SEQ)
                FROM    BKG_CSTMS_KR_DG_CGO
                WHERE   VSL_CD      =   SUBSTR(@[vvd], 1, 4)
                AND     SKD_VOY_NO  =   SUBSTR(@[vvd], 5, 4)
                AND     SKD_DIR_CD  =   SUBSTR(@[vvd], 9, 1)
                AND     ((@[io_bnd_cd] = 'I' AND CSTMS_DECL_TP_CD IN ('I', 'T')) OR
                         (@[io_bnd_cd] = 'O' AND CSTMS_DECL_TP_CD IN ('E', 'R')))
                GROUP BY BKG_NO, CSTMS_DECL_TP_CD, CNTR_NO
            )			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
