<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManilaManifestListDownloadDBDAOsearchCntrInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT  REG_NUMBER3,
        BL_NO2,
        CONTAINER_NO,
        TYPE_SIZE,
        CNTR_SEAL_NO1,
        CNTR_SEAL_NO2,
        CNTR_SEAL_NO3,
        DELIVERY_TYPE,
        ROWNUM AS SEQ

   FROM (
         SELECT @[reg_no] AS REG_NUMBER3,
                COM_CONSTANTMGR_PKG.COM_GETSCACCODE_FNC()||BKG.BL_NO AS BL_NO2,
                NVL(BC.CNTR_NO, ' ') AS CONTAINER_NO,
                NVL(BC.CNTR_TPSZ_CD, ' ') AS TYPE_SIZE,
                (SELECT SUBSTR(NVL(CNTR_SEAL_NO, ' '), 1, 10)
                   FROM BKG_CNTR_SEAL_NO
                  WHERE BKG_NO = BC.BKG_NO
                    AND CNTR_NO = BC.CNTR_NO
                    AND CNTR_SEAL_SEQ = 1 ) AS CNTR_SEAL_NO1,
                (SELECT SUBSTR(NVL(CNTR_SEAL_NO, ' '), 1, 10)
                   FROM BKG_CNTR_SEAL_NO
                  WHERE BKG_NO = BC.BKG_NO
                    AND CNTR_NO = BC.CNTR_NO
                    AND CNTR_SEAL_SEQ = 2 ) AS CNTR_SEAL_NO2,
                (SELECT SUBSTR(NVL(CNTR_SEAL_NO, ' '), 1, 10)
                   FROM BKG_CNTR_SEAL_NO
                  WHERE BKG_NO = BC.BKG_NO
                    AND CNTR_NO = BC.CNTR_NO
                    AND CNTR_SEAL_SEQ = 3 ) AS CNTR_SEAL_NO3,
                NVL(BKG.RCV_TERM_CD||BKG.DE_TERM_CD, 0) AS DELIVERY_TYPE

           FROM BKG_BOOKING BKG,
                BKG_CONTAINER BC,
                BKG_VVD BV
 
          WHERE BKG.BKG_NO = BC.BKG_NO
            AND BKG.BKG_NO = BV.BKG_NO
            AND BV.VSL_CD = @[vsl_cd]
            AND BV.SKD_VOY_NO = @[skd_voy_no]
            AND BV.SKD_DIR_CD = @[skd_dir_cd]
#if (${pol_cd}!= '')
            AND BV.POL_CD LIKE @[pol_cd]
#end
#if (${pod_cd}!= '')
            AND BV.POD_CD LIKE @[pod_cd]
#end
            AND BKG.BKG_STS_CD IN ('F', 'W')
            AND BKG.BL_NO > ' '
        )			]]></sql>
			<params>
				<param name="reg_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
