    @[chn_mf_snd_ind_cd],
    @[bl_no],
    @[cntr_no],
    @[cntr_tpsz_cd],
	@[full_mty_cd],
	NVL(@[cntr_wgt], 0),
	@[wgt_ut_cd],
	NVL(@[cntr_meas_qty], 0),
