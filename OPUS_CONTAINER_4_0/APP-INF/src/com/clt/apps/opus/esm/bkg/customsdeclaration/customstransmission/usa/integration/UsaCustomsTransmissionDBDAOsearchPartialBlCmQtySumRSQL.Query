<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchPartialBlCmQtySumRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
SUM(C.PCK_QTY) BL_CM_QTY
  FROM BKG_CSTMS_ADV_BL A
      ,BKG_CSTMS_ADV_BL B
      ,BKG_CSTMS_ADV_CNTR C
      ,BKG_CSTMS_ADV_CNTR_MF CM
 WHERE 1=1 
   AND C.CNT_CD     = 'US'
   AND C.CNTR_NO    LIKE @[in_cntr]||'%'
   AND B.BL_NO = @[bl_no]
   AND A.CNT_CD = B.CNT_CD
   AND A.VSL_CD     = B.VSL_CD
   AND A.SKD_VOY_NO = B.SKD_VOY_NO
   AND A.SKD_DIR_CD = B.SKD_DIR_CD
   AND C.CNT_CD     = A.CNT_CD
   AND C.BL_NO      = A.BL_NO
   AND A.MF_NO IS NULL
   AND C.CNT_CD = CM.CNT_CD
   AND C.BL_NO = CM.BL_NO
   AND C.CNTR_NO = CM.CNTR_NO			]]></sql>
			<params>
				<param name="in_cntr" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
