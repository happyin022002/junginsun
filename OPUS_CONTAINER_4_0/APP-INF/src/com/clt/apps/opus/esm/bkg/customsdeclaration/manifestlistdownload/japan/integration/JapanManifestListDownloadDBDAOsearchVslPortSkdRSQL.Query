<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchVslPortSkdRSQL">
			<desc><![CDATA[searchVslPortSkd
2011.04.06 김영철 [CHM-201109426-01] Sea-NACCS MFR 송신에러 ( WGT 정수자리수가 7자리 이상인지 체크함. )]]></desc>
			<sql><![CDATA[
SELECT
	TO_CHAR(COUNT(B.BKG_NO)) BKG_COUNT,
	NVL(S1.POL_CD,' ') POL_CD,     
	MAX(NVL(S1.POD_CD,' ')) POD_CD,
	MAX(NVL(TO_CHAR(S3.VPS_ETD_DT,'YYYY-MM-DD HH24:MI'),' ')) VPS_ETD_DT,
	MAX(NVL(TO_CHAR(S4.VPS_ETA_DT,'YYYY-MM-DD HH24:MI'),' ')) VPS_ETA_DT,
	MAX(DECODE(NVL(S2.TRNK_AUTO_BDR_FLG,'N'),'N',DECODE(NVL(S2.TRNK_MNL_BDR_FLG,'N'),'N','N','Y','Y'),'Y','Y')) BDR_FLG,
	MAX(DECODE(NVL(TO_CHAR(S2.TRNK_AUTO_BDR_DT,'YYYY-MM-DD HH24:MI'),' '),' ',DECODE(NVL(TO_CHAR(S2.TRNK_MNL_BDR_DT,'YYYY-MM-DD'),' '),' ',' ',TO_CHAR(S2.TRNK_MNL_BDR_DT,'YYYY-MM-DD HH24:MI')),TO_CHAR(S2.TRNK_AUTO_BDR_DT,'YYYY-MM-DD HH24:MI'))) BDR_TIME,
    MAX(S4.IB_CSSM_VOY_NO) CON_VVD
FROM 
	BKG_VVD S1, 
	BKG_BOOKING B, 
	BKG_VVD_BDR_LOG S2,
	VSK_VSL_PORT_SKD S3, 
	VSK_VSL_PORT_SKD S4
WHERE 	S1.VSL_CD         = @[in_vsl_cd]
AND S1.SKD_VOY_NO  = @[in_skd_voy_no]
AND S1.SKD_DIR_CD     = @[in_skd_dir_cd]
AND S1.POD_CD        = @[in_pod_cd]
#if (${in_pol_cd}!= '') 	
AND S1.POL_CD     = @[in_pol_cd]
#end
AND S1.BKG_NO         = B.BKG_NO
AND (B.BKG_STS_CD        = 'F'
OR   B.BKG_STS_CD        = 'W'  )
AND B.BL_NO           > ' '
AND S1.VSL_CD         = S2.VSL_CD(+)
AND S1.SKD_VOY_NO  = S2.SKD_VOY_NO(+)
AND S1.SKD_DIR_CD     = S2.SKD_DIR_CD(+)
AND S1.POL_CD        = S2.POL_CD(+)
AND S1.POD_CD        = S2.POD_CD(+)
AND S1.VSL_CD         = S3.VSL_CD
AND S1.SKD_VOY_NO  = S3.SKD_VOY_NO
AND S1.SKD_DIR_CD     = S3.SKD_DIR_CD
AND S1.POL_CD        = S3.VPS_PORT_CD
AND S3.CLPT_IND_SEQ   =  
(SELECT MIN(P.CLPT_IND_SEQ) 
                                 FROM VSK_VSL_PORT_SKD P
                                WHERE 1=1
                                  AND P.VSL_CD = S3.VSL_CD
                                  AND P.SKD_VOY_NO = S3.SKD_VOY_NO
                                  AND P.SKD_DIR_CD = S3.SKD_DIR_CD
                                  AND P.VPS_PORT_CD = S3.VPS_PORT_CD
                                  AND NVL(P.SKD_CNG_STS_CD, 'X') <> 'S')    /* Skip */

AND S1.VSL_CD         = S4.VSL_CD
AND S1.SKD_VOY_NO  = S4.SKD_VOY_NO
AND S1.SKD_DIR_CD     = S4.SKD_DIR_CD
AND S1.POD_CD        = S4.VPS_PORT_CD
AND S4.CLPT_IND_SEQ  = @[in_call_ind]
GROUP BY S1.POL_CD			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pod_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_call_ind" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
