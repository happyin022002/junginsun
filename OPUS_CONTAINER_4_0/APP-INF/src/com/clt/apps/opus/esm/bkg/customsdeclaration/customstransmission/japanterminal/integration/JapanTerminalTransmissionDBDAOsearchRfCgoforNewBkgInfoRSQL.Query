<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchRfCgoforNewBkgInfoRSQL">
			<desc><![CDATA[searchRfCgoforNewBkgInfo]]></desc>
			<sql><![CDATA[
SELECT RC.BKG_NO,
       @[bkg_skd_seq] AS BKG_SKD_SEQ,
       RC.RC_SEQ,
       RC.CNTR_TPSZ_CD,
       RC.CNTR_VOL_QTY,
       RC.CNTR_NO,
       RC.CLNG_TP_CD,
       RC.FDO_TEMP,
       RC.CDO_TEMP,
       RC.CNTR_VENT_TP_CD,
       RC.VENT_RTO,
       RC.CBM_PER_HR_QTY,
       RC.HUMID_NO,
       (SELECT COM.INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL COM
         WHERE COM.INTG_CD_ID = 'CD03004'
           AND COM.INTG_CD_VAL_CTNT=RC.VENT_RTO) AS SNACCS_TML_EDI_VENT_RTO_CD

  FROM BKG_RF_CGO RC,
       BKG_VVD BV

 WHERE RC.BKG_NO LIKE SUBSTR(@[bkg_no], 1, 10)||'%'
   AND BV.VSL_Cd = @[vsl_cd]
   AND BV.SKD_VOY_NO = @[skd_voy_no]
   AND BV.SKD_DIR_CD = @[skd_dir_cd]
   AND BV.POL_CD = @[pol_cd]
   AND RC.BKG_NO = BV.BKG_NO			]]></sql>
			<params>
				<param name="bkg_skd_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
