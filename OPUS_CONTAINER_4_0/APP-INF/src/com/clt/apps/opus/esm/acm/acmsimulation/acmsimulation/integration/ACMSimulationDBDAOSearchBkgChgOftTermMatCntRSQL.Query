<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAOSearchBkgChgOftTermMatCntRSQL">
			<desc><![CDATA[SearchBkgChgOftTermMatCnt]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(1),0,'N','Y') 
AS BKG_CHG_RT_COUNT
FROM BKG_CHG_RT 
WHERE BKG_NO = @[bkg_no] 
AND FRT_INCL_XCLD_DIV_CD = 'N' 
AND CHG_CD = 'OFT' 
AND frt_term_cd = @[oft_pay_term_cd]
AND RTRIM(N3PTY_RCV_OFC_CD) IS NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="oft_pay_term_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
