<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOAkDimByCntrCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_AWK_DIM
            (BKG_NO
,            AWK_CGO_SEQ
,            DIM_SEQ
,            DIM_LEN
,            DIM_WDT
,            DIM_HGT
,			 INDIV_PCK_WGT
,            CRE_USR_ID
,            CRE_DT
,            UPD_USR_ID
,            UPD_DT
            )
	SELECT @[tgt_bkg_no]
,          (SELECT NVL(MAX(AWK_CGO_SEQ), 0)
              FROM BKG_AWK_DIM
             WHERE BKG_NO = @[tgt_bkg_no]) + 1
,          DIM_SEQ
,          DIM_LEN
,          DIM_WDT
,          DIM_HGT
,		   INDIV_PCK_WGT
,          @[cre_usr_id]
,          SYSDATE
,          @[cre_usr_id]
,          SYSDATE
    FROM   BKG_AWK_DIM
    WHERE  BKG_NO = @[src_bkg_no]
    AND    AWK_CGO_SEQ IN (SELECT AWK_CGO_SEQ FROM   BKG_AWK_CGO
                           WHERE  BKG_NO = @[src_bkg_no]
                           AND    CNTR_NO = @[cntr_no])			]]></sql>
			<params>
				<param name="tgt_bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="src_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
