<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchComboVVDInfoRSQL">
			<desc><![CDATA[VVD 관련 정보를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT 
    MDM.VSL_ENG_NM || ' ' ||VSK.OB_CSSM_VOY_NO AS name
    ,VSL_PRE_PST_CD AS val
  FROM BKG_VVD VVD
      ,MDM_VSL_CNTR MDM
	  ,VSK_VSL_PORT_SKD VSK
 WHERE VVD.VSL_CD = MDM.VSL_CD
   AND VVD.BKG_NO = @[bkg_no]
#if (${vsl_pre_pst_cd} == 'S')
   AND VSL_PRE_PST_CD = 'S'
#end
   AND VVD.VSL_CD = VSK.VSL_CD
   AND VVD.SKD_VOY_NO = VSK.SKD_VOY_NO
   AND VVD.SKD_DIR_CD = VSK.SKD_DIR_CD
   AND VVD.POL_CD = VSK.VPS_PORT_CD
   AND VVD.POL_CLPT_IND_SEQ = VSK.CLPT_IND_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
