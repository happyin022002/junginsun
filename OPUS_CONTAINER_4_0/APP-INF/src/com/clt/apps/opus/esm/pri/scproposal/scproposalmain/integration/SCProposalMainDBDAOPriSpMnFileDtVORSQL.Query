<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOPriSpMnFileDtVORSQL">
			<desc><![CDATA[이전 filing data를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT MAX(TO_CHAR(FILE_DT, 'YYYYMMDD')) LAST_FILE_DT
      ,TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[prop_ofc_cd]),  'YYYYMMDD') FILE_DT
      ,TO_CHAR(SYSDATE, 'YYYYMMDD') SYS_DT
      ,MAX(TO_CHAR(EFF_DT, 'YYYYMMDD')) PRE_EFF_DT
      ,'' PROP_NO
      ,'' AMDT_SEQ 
	  ,'' PROP_OFC_CD
FROM PRI_SP_MN
WHERE PROP_NO = @[prop_no]
AND AMDT_SEQ = @[amdt_seq] -1			]]></sql>
			<params>
				<param name="prop_ofc_cd" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
