<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharIODeliveryScheduleDAOCheckShpYdUseRSQL">
			<desc><![CDATA[Ship Yard Registration Check]]></desc>
			<sql><![CDATA[
select 
	shp_nm
from fms_new_bld_skd
where yd_seq in (
#foreach($key IN ${yd_seq})
#if($velocityCount < $yd_seq.size())
		'$key',
#else
	'$key'
#end
	#end
)			]]></sql>
			<params>
				<param name="yd_seq" type="2" value="" out="n"/>
			</params>
		</query>
	</querys>
</sqls>
