<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchHeaderRSQL">
			<desc><![CDATA[헤더 문자열을 가져온다]]></desc>
			<sql><![CDATA[
SELECT
    RPAD('BKG',20, ' ') ||
    RPAD(NVL(TRIM(@[receiver_id]),' '),20,' ')||
    RPAD(NVL(TRIM('IFTMCS'),' '),10,' ')||
    RPAD('BKC' || TO_CHAR(SYSDATE, 'YYMMDD') || LTRIM(TO_CHAR(BKG_CSTM_EDI_SEQ.NEXTVAL,'000009')),15,' ') HEADER
FROM DUAL			]]></sql>
			<params>
				<param name="receiver_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
