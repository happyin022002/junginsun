<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchBccEmailAddrRSQL">
			<desc><![CDATA[BKG EMAIL 종류별 BCC EMAIL 가져오기]]></desc>
			<sql><![CDATA[
SELECT ATTR_CTNT2 AS EMAIL
FROM BKG_HRD_CDG_CTNT
WHERE HRD_CDG_ID = 'BKG_BCC_EML'
AND ATTR_CTNT1 = @[bkg_eml_knd_cd]
AND ATTR_CTNT3 = 'Y'			]]></sql>
			<params>
				<param name="bkg_eml_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
