<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCMQCProposalDBDAOPriSpScpMqcUnitVOUSQL">
			<desc><![CDATA[main 에서 unit을 변경하였을 경우 Scope의 Unit 또한 자동으로 변경]]></desc>
			<sql><![CDATA[
UPDATE PRI_SP_SCP_MQC SET 
	   CNTR_LOD_UT_CD = @[cntr_lod_ut_cd]
	  ,UPD_USR_ID 	  = @[upd_usr_id]
	  ,UPD_DT		  = sysdate
WHERE  PROP_NO        = @[prop_no]
AND	   AMDT_SEQ       = @[amdt_seq]
AND    CNTR_LOD_UT_CD <> @[cntr_lod_ut_cd]			]]></sql>
			<params>
				<param name="cntr_lod_ut_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
