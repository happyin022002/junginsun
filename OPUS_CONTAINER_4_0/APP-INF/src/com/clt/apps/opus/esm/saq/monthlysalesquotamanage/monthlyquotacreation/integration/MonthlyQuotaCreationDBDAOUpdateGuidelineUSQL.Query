<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOUpdateGuidelineUSQL">
			<desc><![CDATA[당 분기 기초 정보를 수정한다.]]></desc>
			<sql><![CDATA[
MERGE INTO SAQ_MON_INIT_GLINE 
USING DUAL 
ON (MQTA_MDL_VER_NO = @[mqta_mdl_ver_no] 
      AND TRD_CD = @[trd_cd] 
      AND SUB_TRD_CD = @[sub_trd_cd] 
      AND DIR_CD = @[dir_cd] 
      AND BSE_MON = @[bse_mon] 
      AND BSE_YR = @[bse_yr] ) 
WHEN MATCHED THEN
    UPDATE 
           SET LDF_RTO = @[ldf_rto]
         , LOD_QTY = @[lod_qty]
         , GLINE_CM_AMT = @[gline_cm_amt] * 1000 
         , UPD_USR_ID = @[user_id]  
         , UPD_DT = SYSDATE			]]></sql>
			<params>
				<param name="mqta_mdl_ver_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="bse_mon" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="ldf_rto" type="12" value="" out="N"/>
				<param name="lod_qty" type="12" value="" out="N"/>
				<param name="gline_cm_amt" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
