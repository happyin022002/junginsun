<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqCfmTgtAqListRSQL">
			<desc><![CDATA[SAQ_MON_CFM_TGT_VVD의 Aq List 조회]]></desc>
			<sql><![CDATA[
  SELECT DISTINCT AQ_CD AS TEXT,  
                  AQ_CD AS CODE   
    FROM SAQ_MON_CFM_TGT_VVD      
   WHERE MQTA_RLSE_VER_NO = @[rlse_ver_no]     
     AND BSE_YR           = @[bse_year]     
     AND BSE_QTR_CD       = @[bse_qtr_cd]     
     AND TRD_CD           = @[trd_cd]     
     AND DIR_CD           = @[dir_cd]     
     AND SUB_TRD_CD       = @[sub_trd_cd]     
     AND RLANE_CD         = @[rlane_cd]     
ORDER BY AQ_CD   			]]></sql>
			<params>
				<param name="rlse_ver_no" type="12" value="" out="N"/>
				<param name="bse_year" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
