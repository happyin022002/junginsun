<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOPriScgRtVOPagingRSQL">
			<desc><![CDATA[SurchargeDBDAOPriScgRtVORSQL의 페이징]]></desc>
			<sql><![CDATA[
SELECT 
       SVC_SCP_CD
     , CHG_CD
     , SCG_SEQ
     , EFF_DT
     , EXP_DT
     , SUB_TRD_CD
     , VSL_SLAN_CD
     , POR_TP_CD
     , POR_DEF_CD
     , POL_TP_CD
     , POL_DEF_CD
     , POD_TP_CD
     , POD_DEF_CD
     , DEL_TP_CD
     , DEL_DEF_CD
     , TS_PORT_CD
     , TML_CD
     , ORG_TRSP_MOD_CD
     , DEST_TRSP_MOD_CD
     , USA_SVC_MOD_CD
     , PRC_RCV_TERM_CD
     , PRC_DE_TERM_CD
     , PRC_HNGR_BAR_TP_CD
     , DIR_CALL_FLG
     , MIN_CGO_WGT
     , MAX_CGO_WGT
     , CMDT_CD
     , SCG_GRP_CMDT_CD
     , PRC_CGO_TP_CD
     , SCG_IMDG_CLSS_CD
     , RAT_UT_CD
     , CURR_CD
     , SCG_AMT
     , PAY_TERM_CD
     , WDR_FLG
     , SOC_FLG
     , IO_GA_CD
     , DELT_FLG
     , SCG_RMK
     , CNL_TZ_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , LVL1
     , LVL2
     , LVL3
     , LVL4
     , BKG_ESVC_TP_CD
     , NO
     
  FROM (
SELECT 
       A.SVC_SCP_CD
     , A.CHG_CD
     , A.SCG_SEQ
     , A.EFF_DT
     , A.EXP_DT
     , A.SUB_TRD_CD
     , A.VSL_SLAN_CD
     , A.POR_TP_CD
     , A.POR_DEF_CD
     , A.POL_TP_CD
     , A.POL_DEF_CD
     , A.POD_TP_CD
     , A.POD_DEF_CD
     , A.DEL_TP_CD
     , A.DEL_DEF_CD
     , A.TS_PORT_CD
     , A.TML_CD
     , A.ORG_TRSP_MOD_CD
     , A.DEST_TRSP_MOD_CD
     , A.USA_SVC_MOD_CD
     , A.PRC_RCV_TERM_CD
     , A.PRC_DE_TERM_CD
     , A.PRC_HNGR_BAR_TP_CD
     , A.DIR_CALL_FLG
     , A.MIN_CGO_WGT
     , A.MAX_CGO_WGT
     , A.CMDT_CD
     , A.SCG_GRP_CMDT_CD
     , A.PRC_CGO_TP_CD
     , A.SCG_IMDG_CLSS_CD
     , A.RAT_UT_CD
     , A.CURR_CD
     , A.SCG_AMT
     , A.PAY_TERM_CD
     , A.WDR_FLG
     , A.SOC_FLG
     , A.IO_GA_CD
     , A.DELT_FLG
     , A.SCG_RMK
     , A.CNL_TZ_CD
     , A.CRE_USR_ID
     , A.CRE_DT
     , A.UPD_USR_ID
     , A.UPD_DT
     , A.LVL1
     , A.LVL2
     , A.LVL3
     , A.LVL4
     , A.BKG_ESVC_TP_CD
     , ROW_NUMBER() OVER (ORDER BY LVL1, LVL2, LVL3, LVL4, SUB_TRD_CD, VSL_SLAN_CD, TS_PORT_CD, DIR_CALL_FLG, TML_CD, ORG_TRSP_MOD_CD, DEST_TRSP_MOD_CD, USA_SVC_MOD_CD, PRC_RCV_TERM_CD
     , PRC_DE_TERM_CD, PRC_HNGR_BAR_TP_CD, MIN_CGO_WGT, MAX_CGO_WGT, CMDT_CD, SCG_GRP_CMDT_CD, SOC_FLG, IO_GA_CD, RAT_UT_CD, PRC_CGO_TP_CD, SCG_IMDG_CLSS_CD, CURR_CD
     , SCG_AMT, PAY_TERM_CD) NO
  FROM (

SELECT A.SVC_SCP_CD
     , A.CHG_CD
     , A.SCG_SEQ
     , TO_CHAR(A.EFF_DT, 'YYYY-MM-DD') AS EFF_DT
     , DECODE(TO_CHAR(A.EXP_DT, 'YYYYMMDD'), '99991231', NULL, TO_CHAR(A.EXP_DT, 'YYYY-MM-DD')) AS EXP_DT
     , A.SUB_TRD_CD
     , A.VSL_SLAN_CD
     , A.POR_TP_CD
     , A.POR_DEF_CD
     , A.POL_TP_CD
     , A.POL_DEF_CD
     , A.POD_TP_CD
     , A.POD_DEF_CD
     , A.DEL_TP_CD
     , A.DEL_DEF_CD
     , A.TS_PORT_CD
     , A.TML_CD
     , A.ORG_TRSP_MOD_CD
     , A.DEST_TRSP_MOD_CD
     , A.USA_SVC_MOD_CD
     , A.PRC_RCV_TERM_CD
     , A.PRC_DE_TERM_CD
     , A.PRC_HNGR_BAR_TP_CD
     , A.DIR_CALL_FLG
     , A.MIN_CGO_WGT
     , A.MAX_CGO_WGT
     , A.CMDT_CD
     , A.SCG_GRP_CMDT_CD
     , A.PRC_CGO_TP_CD
     , A.SCG_IMDG_CLSS_CD
     , A.RAT_UT_CD
     , A.CURR_CD
     , A.SCG_AMT
     , A.PAY_TERM_CD
     , DECODE(A.WDR_FLG, 'N', '0', '1') AS WDR_FLG
     , A.SOC_FLG
     , A.IO_GA_CD
     , A.DELT_FLG
     , A.SCG_RMK
     , A.CNL_TZ_CD
     , A.CRE_USR_ID
     , A.CRE_DT
     , A.UPD_USR_ID
     , TO_CHAR(A.UPD_DT, 'YYYY-MM-DD') AS UPD_DT
     , CASE WHEN A.POR_TP_CD IS NULL THEN 1
            WHEN A.POR_TP_CD = 'C'   THEN 2
            WHEN A.POR_TP_CD = 'G'   THEN 3
            WHEN A.POR_TP_CD = 'R'   THEN 4
            WHEN A.POR_TP_CD = 'L'   THEN 5
       END AS LVL1
     , CASE WHEN A.POL_TP_CD IS NULL THEN 1
            WHEN A.POL_TP_CD = 'C'   THEN 2
            WHEN A.POL_TP_CD = 'G'   THEN 3
            WHEN A.POL_TP_CD = 'R'   THEN 4
            WHEN A.POL_TP_CD = 'L'   THEN 5
       END AS LVL2
     , CASE WHEN A.POD_TP_CD IS NULL THEN 1
            WHEN A.POD_TP_CD = 'C'   THEN 2
            WHEN A.POD_TP_CD = 'G'   THEN 3
            WHEN A.POD_TP_CD = 'R'   THEN 4
            WHEN A.POD_TP_CD = 'L'   THEN 5
       END AS LVL3
     , CASE WHEN A.DEL_TP_CD IS NULL THEN 1
            WHEN A.DEL_TP_CD = 'C'   THEN 2
            WHEN A.DEL_TP_CD = 'G'   THEN 3
            WHEN A.DEL_TP_CD = 'R'   THEN 4
            WHEN A.DEL_TP_CD = 'L'   THEN 5
       END AS LVL4 
     , A.BKG_ESVC_TP_CD
  FROM PRI_SCG_RT A
#if (${por_def_cd} != '')
     , (SELECT POR_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS POR_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS POR_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS POR_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT 'NULL' AS POR_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT SCG_GRP_LOC_CD
          FROM PRI_SCG_GRP_LOC
         WHERE SCG_GRP_LOC_CD = @[por_def_cd]
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS POR_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT POR_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS POR_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS POR_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS POR_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                 WHERE A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.CHG_CD = @[chg_cd]
                   AND A.DTL_LOC_DEF_CD = B.POR_DEF_CD
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) B
#end
#if (${pol_def_cd} != '')
     , (SELECT POL_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS POL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS POL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS POL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT 'NULL' AS POL_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT SCG_GRP_LOC_CD
          FROM PRI_SCG_GRP_LOC
         WHERE SCG_GRP_LOC_CD = @[pol_def_cd]
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS POL_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT POL_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS POL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS POL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS POL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                 WHERE A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.CHG_CD = @[chg_cd]
                   AND A.DTL_LOC_DEF_CD = B.POL_DEF_CD
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) C
#end
#if (${pod_def_cd} != '')
     , (SELECT POD_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS POD_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS POD_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS POD_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT 'NULL' AS POD_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT SCG_GRP_LOC_CD
          FROM PRI_SCG_GRP_LOC
         WHERE SCG_GRP_LOC_CD = @[pod_def_cd]
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS POD_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT POD_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS POD_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS POD_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS POD_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                 WHERE A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.CHG_CD = @[chg_cd]
                   AND A.DTL_LOC_DEF_CD = B.POD_DEF_CD
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) D
#end
#if (${del_def_cd} != '')
     , (SELECT DEL_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS DEL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS DEL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS DEL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT 'NULL' AS DEL_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT SCG_GRP_LOC_CD
          FROM PRI_SCG_GRP_LOC
         WHERE SCG_GRP_LOC_CD = @[del_def_cd]
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS DEL_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT DEL_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS DEL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS DEL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS DEL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                 WHERE A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.CHG_CD = @[chg_cd]
                   AND A.DTL_LOC_DEF_CD = B.DEL_DEF_CD
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) E
#end
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND CHG_CD = @[chg_cd]
#if (${scg_seq} != '') 
   AND SCG_SEQ = @[scg_seq]
#end
#if (${por_def_cd} != '') 
   AND NVL(A.POR_DEF_CD, 'NULL') = B.POR_DEF_CD
#end
#if (${pol_def_cd} != '') 
   AND NVL(A.POL_DEF_CD, 'NULL') = C.POL_DEF_CD
#end
#if (${pod_def_cd} != '') 
   AND NVL(A.POD_DEF_CD, 'NULL') = D.POD_DEF_CD
#end
#if (${del_def_cd} != '')
   AND NVL(A.DEL_DEF_CD, 'NULL') = E.DEL_DEF_CD
#end
#if (${eff_dt} != '')
AND TO_DATE(@[eff_dt], 'YYYY-MM-DD') BETWEEN EFF_DT AND EXP_DT
#end
#if (${scg_imdg_clss_cd} != '')
   AND NVL(SCG_IMDG_CLSS_CD, 'NULL') IN (@[scg_imdg_clss_cd], 'NULL')
#end
#if (${prc_cgo_tp_cd} != '')
   AND	NVL(PRC_CGO_TP_CD, 'NULL') IN (
					#foreach(${key} in ${prc_cgo_tp_cd})
						#if($velocityCount < $prc_cgo_tp_cd.size())
							'$key', 
						#else  
							'$key'
						#end 
					#end 
 					)
#end
#if (${bkg_esvc_tp_cd} != '')
   AND NVL(BKG_ESVC_TP_CD, 'NULL') IN (@[bkg_esvc_tp_cd], 'NULL')
#end
   AND DELT_FLG = 'N'
) A
   
ORDER BY LVL1, LVL2, LVL3, LVL4, SUB_TRD_CD, VSL_SLAN_CD, TS_PORT_CD, DIR_CALL_FLG, TML_CD, ORG_TRSP_MOD_CD, DEST_TRSP_MOD_CD, USA_SVC_MOD_CD, PRC_RCV_TERM_CD
     , PRC_DE_TERM_CD, PRC_HNGR_BAR_TP_CD, MIN_CGO_WGT, MAX_CGO_WGT, CMDT_CD, SCG_GRP_CMDT_CD, SOC_FLG, IO_GA_CD, RAT_UT_CD, PRC_CGO_TP_CD, SCG_IMDG_CLSS_CD, CURR_CD
     , SCG_AMT, PAY_TERM_CD

)
WHERE NO BETWEEN @[startpage] AND @[endpage]			]]></sql>
			<params>
				<param name="por_def_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="pol_def_cd" type="12" value="" out="N"/>
				<param name="pod_def_cd" type="12" value="" out="N"/>
				<param name="del_def_cd" type="12" value="" out="N"/>
				<param name="scg_seq" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="scg_imdg_clss_cd" type="12" value="" out="N"/>
				<param name="bkg_esvc_tp_cd" type="12" value="" out="N"/>
				<param name="startpage" type="12" value="" out="N"/>
				<param name="endpage" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
