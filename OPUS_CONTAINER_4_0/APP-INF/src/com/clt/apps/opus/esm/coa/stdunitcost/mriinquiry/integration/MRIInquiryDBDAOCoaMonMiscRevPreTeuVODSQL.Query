<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MRIInquiryDBDAOCoaMonMiscRevPreTeuVODSQL">
			<desc><![CDATA[MRI Inquiry (PA/RA) 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE
  FROM COA_MON_MISC_REV_PRE_TEU
 WHERE REV_YRMON      = @[rev_yrmon]
   AND TRD_CD         = @[trd_cd]
   AND DIR_CD         = @[dir_cd]
   AND RLANE_CD       = @[rlane_cd]			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
