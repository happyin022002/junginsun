<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOModifyMltShpFlgUSQL">
			<desc><![CDATA[modifyMltShpFlg]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')
UPDATE BKG_BL_DOC_HIS
    SET MLT_SHP_FLG = @[mlt_shp_flg]
WHERE BKG_NO = @[bkg_no]
AND CORR_NO = 'TMP0000001'
#else
UPDATE BKG_BL_DOC
    SET MLT_SHP_FLG = @[mlt_shp_flg]
WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="mlt_shp_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
