<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOModifyPlaceOfIssueAssociationUSQL">
			<desc><![CDATA[update PlaceOfIssueAssociation]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_ESIG_OFC_ASGN
SET BL_ISS_OFC_CD = @[bl_iss_ofc_cd]
	, BL_ISS_OFC_NM = @[bl_iss_ofc_nm]
	, CNT_CD = @[cnt_cd]
	, AGN_CO_NM1 = @[agn_co_nm1]
	, AGN_CO_NM2 = @[agn_co_nm2]
	, AGN_CO_NM3 = @[agn_co_nm3]
	, BL_ESIG_SEQ = @[bl_esig_seq]
	, BL_ESIG_RMK = @[bl_esig_rmk]
	, UPD_USR_ID = @[upd_usr_id]
	, UPD_DT = SYSDATE
	, BL_ESIG_FLG = NVL(@[bl_esig_flg],'N')
	, BL_CPY_ESIG_FLG = NVL(@[bl_cpy_esig_flg],'N')
	, BL_KNT_FLG = NVL(@[bl_knt_flg],'N')
WHERE BL_ESIG_ASGN_SEQ = @[bl_esig_asgn_seq]			]]></sql>
			<params>
				<param name="bl_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_iss_ofc_nm" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="agn_co_nm1" type="12" value="" out="N"/>
				<param name="agn_co_nm2" type="12" value="" out="N"/>
				<param name="agn_co_nm3" type="12" value="" out="N"/>
				<param name="bl_esig_seq" type="12" value="" out="N"/>
				<param name="bl_esig_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_esig_flg" type="12" value="" out="N"/>
				<param name="bl_cpy_esig_flg" type="12" value="" out="N"/>
				<param name="bl_knt_flg" type="12" value="" out="N"/>
				<param name="bl_esig_asgn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
