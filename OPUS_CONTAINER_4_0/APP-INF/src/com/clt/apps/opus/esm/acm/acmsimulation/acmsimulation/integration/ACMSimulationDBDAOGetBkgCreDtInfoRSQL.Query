<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAOGetBkgCreDtInfoRSQL">
			<desc><![CDATA[GetBkgCreDtInfo]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(CRE_DT,'YYYYMMDD') AS BKG_CRE_DT , BL_NO_TP, BKG_STS_CD, SVC_SCP_CD
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
