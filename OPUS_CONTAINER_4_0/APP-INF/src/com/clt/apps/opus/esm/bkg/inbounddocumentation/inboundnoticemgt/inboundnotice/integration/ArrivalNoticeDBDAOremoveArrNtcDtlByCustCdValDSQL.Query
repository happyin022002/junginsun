<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOremoveArrNtcDtlByCustCdValDSQL">
			<desc><![CDATA[Customer Code Validation정보를 제거할 경우 Arrival Notice Detail이 삭제된다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_ARR_NTC_DTL
WHERE	BKG_NO = @[bkg_no]
AND	BKG_CUST_TP_CD = @[bkg_cust_tp_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
