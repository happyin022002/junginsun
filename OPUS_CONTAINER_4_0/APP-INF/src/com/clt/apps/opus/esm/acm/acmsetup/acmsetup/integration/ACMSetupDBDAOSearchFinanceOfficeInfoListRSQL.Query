<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSetupDBDAOSearchFinanceOfficeInfoListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.AGN_INFO_SEQ,
       A.RHQ_CD,
       A.OFC_CD,
       A.AGN_CD,
       A.OFC_GRP_ID,
       A.DP_GRP_NM,
       A.OFC_CHR_CD,
       A.AGN_FM_DT_CD,
       A.AGN_FM_DT,
       A.AGN_TO_DT_CD,
       A.AGN_TO_DT,
       A.VNDR_SEQ,
       A.XCH_RT_DIV_LVL,
       A.CURR_CD,
       A.AR_OFC_CD
  FROM ACM_OFC_INFO A
#if (${rhq_cd} != '')
 WHERE A.RHQ_CD = NVL(@[rhq_cd], A.RHQ_CD)
#end
ORDER BY A.OFC_GRP_ID,A.AGN_FM_DT,A.AGN_CD			]]></sql>
			<params>
				<param name="rhq_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
