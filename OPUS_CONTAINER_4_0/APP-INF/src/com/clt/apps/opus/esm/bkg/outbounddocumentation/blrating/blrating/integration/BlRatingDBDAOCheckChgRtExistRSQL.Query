<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCheckChgRtExistRSQL">
			<desc><![CDATA[checking the charge is existed or not]]></desc>
			<sql><![CDATA[
SELECT NVL(DECODE(COUNT(*), 0, 'N', 'Y'), 'N') CHG_EXIST
#if (${ca_flag} == 'Y') 
FROM BKG_CHG_RT_HIS
#else 
FROM BKG_CHG_RT
#end
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
