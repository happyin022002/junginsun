		<query name="CommonDBDAOSearchCoaInferfaceYNListRSQL">
			<desc><![CDATA[Base Data Preparation COA Interface 버튼 활성화 여부 조회]]></desc>
			<sql><![CDATA[
SELECT DECODE(MAX(FCAST_TRNS_STS_CD), 'N', 'N', 'Y') AS TEXT 
     , 'COA_STS_CD' AS CODE 
  FROM SAQ_MON_FCAST_TRNS 
 WHERE MQTA_MDL_VER_NO = TO_CHAR(SYSDATE, 'YY') || TO_CHAR(ADD_MONTHS(SYSDATE,3), 'Q') || 'Q01'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
