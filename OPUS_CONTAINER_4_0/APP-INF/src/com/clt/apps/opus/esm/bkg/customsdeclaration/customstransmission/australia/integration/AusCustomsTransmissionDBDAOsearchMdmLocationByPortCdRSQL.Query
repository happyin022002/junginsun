<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchMdmLocationByPortCdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(LOC_NM, 1, 35) AS LOC_NM

  FROM MDM_LOCATION

 WHERE LOC_CD = @[port_cd]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
