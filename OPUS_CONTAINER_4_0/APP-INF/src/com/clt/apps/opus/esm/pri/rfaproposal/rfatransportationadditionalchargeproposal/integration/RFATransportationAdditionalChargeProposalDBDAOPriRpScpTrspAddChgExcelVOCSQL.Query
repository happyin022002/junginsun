<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFATransportationAdditionalChargeProposalDBDAOPriRpScpTrspAddChgExcelVOCSQL">
			<desc><![CDATA[엑셀업로드]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_TRSP_ADD_CHG (
	  PROP_NO
	, AMDT_SEQ
	, SVC_SCP_CD
	, ADD_CHG_TP_CD
	, ORG_DEST_TP_CD
	, ADD_CHG_SEQ
	, BSE_PORT_TP_CD
	, BSE_PORT_DEF_CD
	, ROUT_PNT_LOC_TP_CD
	, ROUT_PNT_LOC_DEF_CD
	, RAT_UT_CD
	, PRC_CGO_TP_CD
	, PRC_TRSP_MOD_CD
	, RCV_DE_TERM_CD
	, MIN_CGO_WGT
	, MAX_CGO_WGT
	, CUST_CNT_CD
	, CUST_SEQ
	, CURR_CD
	, PROP_FRT_RT_AMT
	, ADD_CHG_NOTE_CTNT
	, NOTE_DP_SEQ
	, PRC_PROG_STS_CD
	, SRC_INFO_CD
	, N1ST_CMNC_AMDT_SEQ
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
)
SELECT
	  @[prop_no]
	, @[amdt_seq]
	, @[svc_scp_cd]
	, DECODE ( @[rcv_de_term_cd], 'D', 'I', @[add_chg_tp_cd] )
	, @[org_dest_tp_cd]
	, (SELECT NVL(MAX(ADD_CHG_SEQ), 0)+1
         FROM PRI_RP_SCP_TRSP_ADD_CHG A
        WHERE SVC_SCP_CD	= @[svc_scp_cd]
          AND PROP_NO		= @[prop_no]
          AND AMDT_SEQ		= @[amdt_seq]
          AND ORG_DEST_TP_CD	= @[org_dest_tp_cd])
	, @[bse_port_tp_cd]
	, @[bse_port_def_cd]
	, @[rout_pnt_loc_tp_cd]
	, @[rout_pnt_loc_def_cd]
	, @[rat_ut_cd]
	, @[prc_cgo_tp_cd]
	, @[prc_trsp_mod_cd]
	, @[rcv_de_term_cd]
	, @[min_cgo_wgt]
	, @[max_cgo_wgt]
	, @[cust_cnt_cd]
	, @[cust_seq]
	, @[curr_cd]
	, @[prop_frt_rt_amt]
	, @[add_chg_note_ctnt]
	, @[note_dp_seq]
	, 'I'
	, 'NW'
	, @[amdt_seq] 
	, @[cre_usr_id]
	, sysdate
	, @[upd_usr_id]
	, sysdate
 FROM DUAL			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="add_chg_tp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="bse_port_tp_cd" type="12" value="" out="N"/>
				<param name="bse_port_def_cd" type="12" value="" out="N"/>
				<param name="rout_pnt_loc_tp_cd" type="12" value="" out="N"/>
				<param name="rout_pnt_loc_def_cd" type="12" value="" out="N"/>
				<param name="rat_ut_cd" type="12" value="" out="N"/>
				<param name="prc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="prc_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="min_cgo_wgt" type="12" value="" out="N"/>
				<param name="max_cgo_wgt" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="prop_frt_rt_amt" type="12" value="" out="N"/>
				<param name="add_chg_note_ctnt" type="12" value="" out="N"/>
				<param name="note_dp_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
