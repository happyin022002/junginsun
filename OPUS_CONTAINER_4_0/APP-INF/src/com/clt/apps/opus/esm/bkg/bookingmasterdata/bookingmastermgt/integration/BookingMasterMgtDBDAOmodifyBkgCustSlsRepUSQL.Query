<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOmodifyBkgCustSlsRepUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE BKG_CUST_SLS_REP SET 
	DELT_FLG = @[delt_flg]
,	DP_SEQ = @[dp_seq]
,	SREP_CUST_CLSS_CD = @[srep_cust_clss_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	SREP_CD = @[srep_cd]
AND	CUST_CNT_CD = @[cust_cnt_cd]
AND	CUST_SEQ = @[cust_seq]
			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="dp_seq" type="2" value="" out="N"/>
				<param name="srep_cust_clss_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
