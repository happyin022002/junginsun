<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentTradeDBDAOSearchSaqMonQtaStepTrdLane0048ListRSQL">
			<desc><![CDATA[첫번째 rlane_cd를 조회]]></desc>
			<sql><![CDATA[
SELECT MIN(R.RLANE_CD) RLANE_CD
FROM  SAQ_MON_QTA_STEP_VER V,
      SAQ_MON_QTA_TRD R
WHERE V.MQTA_STEP_CD = @[mQtaStepCd]
AND   V.BSE_YR = @[year]
AND   V.TRD_CD = @[trade]
AND   V.DIR_CD = @[bound]
AND   V.MQTA_VER_NO = @[mQtaVerNo]
AND   R.MQTA_STEP_CD = V.MQTA_STEP_CD
AND   R.BSE_YR = V.BSE_YR
AND   R.TRD_CD = V.TRD_CD
AND   R.DIR_CD = V.DIR_CD
AND   R.MQTA_VER_NO = V.MQTA_VER_NO
AND   R.BSE_QTR_CD = @[bse_qtr_cd]
AND   R.BSE_MON = substr(@[targetMonth],5,7)			]]></sql>
			<params>
				<param name="mQtaStepCd" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="mQtaVerNo" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="targetMonth" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
