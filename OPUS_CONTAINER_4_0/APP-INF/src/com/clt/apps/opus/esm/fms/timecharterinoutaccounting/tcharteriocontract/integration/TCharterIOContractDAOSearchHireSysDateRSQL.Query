<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOContractDAOSearchHireSysDateRSQL">
			<desc><![CDATA[TCharterIOContractDAOSearchHireSysDateRSQL]]></desc>
			<sql><![CDATA[
SELECT 
		TO_CHAR(EFF_DT,'YYYY-MM-DD') EFF_DT,
		TO_CHAR(EFF_DT,'HH24:MI') EFF_DT_TIME,
		TO_CHAR(EXP_DT,'YYYY-MM-DD') EXP_DT,
		TO_CHAR(EXP_DT,'HH24:MI') EXP_DT_TIME,
		HIR_CURR_N1ST_CD,
		TO_CHAR(HIR_RT_N1ST_AMT,'FM999,999,999,999,000.00') HIR_RT_N1ST_AMT,
		HIR_CURR_N2ND_CD,
        TO_CHAR(HIR_RT_N2ND_AMT,'FM999,999,999,999,000.00') HIR_RT_N2ND_AMT
  FROM  FMS_HIRE
 WHERE  FLET_CTRT_NO = @[flet_ctrt_no]
   AND  TO_CHAR(EFF_DT,'YYYYMMDDHH24MI')||TO_CHAR(EXP_DT,'YYYYMMDDHH24MI') = ( 
                           SELECT MAX(TO_CHAR(EFF_DT,'YYYYMMDDHH24MI')||TO_CHAR(EXP_DT,'YYYYMMDDHH24MI'))
                             FROM FMS_HIRE
 						    WHERE FLET_CTRT_NO = @[flet_ctrt_no]
                              AND EFF_DT <= SYSDATE
                              AND EXP_DT >= SYSDATE )			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
