<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOmodifyCodPctlNoUSQL">
			<desc><![CDATA[해당 cod rqst건에 대한 pctl no를 update한다.]]></desc>
			<sql><![CDATA[
update bkg_cod
   set pctl_no = @[pctl_no]
	 , upd_usr_id = @[usr_id]
	 , upd_dt = sysdate
 where bkg_no = @[bkg_no]
   and cod_rqst_seq = @[cod_rqst_seq]
   AND COD_STS_CD NOT IN ('F', 'N')			]]></sql>
			<params>
				<param name="pctl_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
