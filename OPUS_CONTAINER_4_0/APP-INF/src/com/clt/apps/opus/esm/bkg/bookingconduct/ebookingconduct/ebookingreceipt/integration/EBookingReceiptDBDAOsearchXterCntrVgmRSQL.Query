<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterCntrVgmRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT
	XTER_SNDR_ID,
  	XTER_RQST_NO,
  	XTER_RQST_SEQ,
  	CNTR_NO,
  	CNTR_SEQ,
  	CNTR_TPSZ_CD,
  	PCK_QTY,
  	PCK_TP_CD,
  	CNTR_WGT,
  	WGT_UT_CD,
  	MEAS_QTY,
  	MEAS_UT_CD,
  	SOC_FLG,
  	PRT_FLG,
  	CST_FLG,
  	SHP_REF_NO,
  	PO_NO,
  	NET_WGT_QTY,
  	NET_WGT_UT_CD,
  	OB_HLG_TP_CD,
  	IB_HLG_TP_CD,
  	EDW_UPD_DT,
  	VGM_DOC_ID_NO,
  	VGM_WGT,
  	VGM_WGT_UT_CD,
  	VGM_DOC_TP_CD,
  	VGM_DT_TP_CD,
  	VGM_HNDL_DT,
  	VGM_CUST_CNTC_TP_CD,
  	VGM_CUST_CNTC_NM,
  	VGM_CUST_FAX_NO,
  	VGM_CUST_EML,
  	VGM_CUST_PHN_NO,
  	VGM_CUST_ADDR
  FROM BKG_XTER_CNTR
WHERE 1 = 1
AND XTER_SNDR_ID  = @[sender_id]
AND XTER_RQST_NO  = @[rqst_no]
AND XTER_RQST_SEQ = @[rqst_seq]			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
