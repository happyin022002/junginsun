<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRatesDBDAOPriTrfInlndHistoryVORSQL">
			<desc><![CDATA[Inland 리스트를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT  
        TRF_PFX_CD
       ,TRF_NO
       ,TRF_INLND_SEQ
       ,MAX(TRF_INLND_NM) AS TRF_INLND_NM
  FROM  PRI_TRF_INLND
 WHERE  1 = 1
   AND  TRF_PFX_CD = @[trf_pfx_cd]
   AND  TRF_NO     = @[trf_no]
   AND  TRF_INLND_STS_CD = 'F'
#if (${access_dt} != '') 
   AND  EFF_DT     <= TO_DATE(@[access_dt], 'YYYY-MM-DD')
   AND  NVL(EXP_DT, TO_DATE('9999-12-31', 'YYYY-MM-DD')) >= TO_DATE(@[access_dt], 'YYYY-MM-DD')
#end
GROUP BY TRF_PFX_CD, TRF_NO, TRF_INLND_SEQ
ORDER BY TRF_INLND_NM			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="access_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
