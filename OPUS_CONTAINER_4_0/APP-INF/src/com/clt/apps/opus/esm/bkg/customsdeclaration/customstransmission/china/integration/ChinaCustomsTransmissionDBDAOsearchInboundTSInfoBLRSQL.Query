<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaCustomsTransmissionDBDAOsearchInboundTSInfoBLRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT BL.BKG_NO,
       BL.POR_CD,
       BL.POL_CD,
       BL.DEL_CD,
       (SELECT COM.CMDT_NM
          FROM BKG_BOOKING BKG,
               MDM_COMMODITY COM
         WHERE BKG.BKG_NO = BL.BKG_NO
           AND COM.CMDT_CD = BKG.CMDT_CD
           AND ROWNUM = 1) AS CMDT_NM,
       BL.PCK_QTY,
       BL.ACT_WGT,
       BL.MEAS_QTY,
       DSC.MK_DESC,
       DSC.CMDT_DESC

  FROM BKG_BL_DOC BL,
       BKG_BL_MK_DESC DSC

 WHERE BL.BKG_NO = @[bkg_no]
   AND BL.BKG_NO = DSC.BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
