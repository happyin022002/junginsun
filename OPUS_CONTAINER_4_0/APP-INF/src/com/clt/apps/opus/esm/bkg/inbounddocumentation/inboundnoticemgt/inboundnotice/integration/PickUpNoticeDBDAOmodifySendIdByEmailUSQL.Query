<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOmodifySendIdByEmailUSQL">
			<desc><![CDATA[Email 전송 후 전송 ID ( Send ID ) 값을 저장한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_PKUP_NTC_DTL SET 
	EML_SND_DT = NULL
,	EML_SND_GDT = NULL
,	EML_SND_USR_ID = @[eml_snd_usr_id]
,	EML_NTC_SND_ID = @[eml_ntc_snd_id]
,	EML_NTC_SND_RSLT_CD = NULL
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	BKG_NO = @[bkg_no]
AND	NTC_SEQ = @[ntc_seq]
AND	CUST_CNTC_TP_CD = @[cust_cntc_tp_cd]			]]></sql>
			<params>
				<param name="eml_snd_usr_id" type="12" value="" out="N"/>
				<param name="eml_ntc_snd_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ntc_seq" type="2" value="" out="N"/>
				<param name="cust_cntc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
