<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoRiderDBDAOmanageBlRiderDSQL">
			<desc><![CDATA[BKG_IMG_STO 삭제 (BkgImgStoVO)]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_IMG_STO 
WHERE 
BKG_NO = @[bkg_no]  
#if (${gubun} == 'SpclRider')
AND FILE_SAV_ID = @[file_sav_id]  
#else
AND	IMG_SEQ = @[img_seq]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="img_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
