<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOaddBkgXterCntrSealNoCSQL">
			<desc><![CDATA[addBkgXterCntrSealNo]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_XTER_CNTR_SEAL_NO
(
XTER_SNDR_ID,
XTER_RQST_NO,
XTER_RQST_SEQ,
CNTR_NO,
CNTR_SEQ,
CNTR_SEAL_SEQ,
XTER_CNTR_SEAL_NO,
SEAL_PTY_NM,
SEAL_KND_CD,
PRN_FLG,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID,
UPD_DT
)
SELECT
@[xter_sndr_id],
@[xter_rqst_no],
@[xter_rqst_seq],
@[cntr_no],
(
SELECT NVL(MAX(CNTR_SEQ), 1)
  FROM BKG_XTER_CNTR
 WHERE XTER_SNDR_ID  = @[xter_sndr_id]
   AND XTER_RQST_NO  = @[xter_rqst_no]
   AND XTER_RQST_SEQ = @[xter_rqst_seq]
   AND CNTR_NO       = @[cntr_no]
),
ROWNUM + (
SELECT NVL(MAX(CNTR_SEAL_SEQ),0)
  FROM BKG_XTER_CNTR_SEAL_NO
 WHERE XTER_SNDR_ID  = @[xter_sndr_id]
   AND XTER_RQST_NO  = @[xter_rqst_no]
   AND XTER_RQST_SEQ = @[xter_rqst_seq]
   AND CNTR_NO       = @[cntr_no]
),
@[xter_cntr_seal_no],
@[seal_pty_nm],
@[seal_knd_cd],
NVL(@[prn_flg], 'N'),
'SYSTEM',
SYSDATE,
'SYSTEM',
SYSDATE
FROM DUAL			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="xter_cntr_seal_no" type="12" value="" out="N"/>
				<param name="seal_pty_nm" type="12" value="" out="N"/>
				<param name="seal_knd_cd" type="12" value="" out="N"/>
				<param name="prn_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
