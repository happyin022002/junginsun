<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOSearchLOfcVerifyRSQL">
			<desc><![CDATA[해당 버전의 Loading Office중 유효하지 않은 Office를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT T1.MQTA_MDL_VER_NO 
     , T1.SLS_RHQ_CD AS RHQ_CD 
     , T1.SLS_OFC_CD AS OFC_CD 
     , T1.SLS_RHQ_CD     
     , T1.SLS_OFC_CD
     , 'SLS' AS SAV_FLG
  FROM SAQ_MON_FCAST_TRNS T1 
     , SAQ_ORGANIZATION_V T2 
 WHERE 1=1 
       AND T1.SLS_OFC_CD = T2.OFC_CD (+) 
       AND 'N' = T2.DELT_FLG (+) 
       AND T2.OFC_CD IS NULL 
       AND T1.MQTA_MDL_VER_NO = @[mqta_mdl_ver_no] 
       AND T1.TRD_CD = @[trd_cd]  
       AND (FCAST_TRNS_STS_CD = '0' OR FCAST_TRNS_STS_CD IS NULL)
       #if(${bound} != '') 
       AND T1.DIR_CD = @[bound] 
       #end  
 GROUP BY T1.MQTA_MDL_VER_NO 
     , T1.SLS_RHQ_CD 
     , T1.SLS_OFC_CD			]]></sql>
			<params>
				<param name="mqta_mdl_ver_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
