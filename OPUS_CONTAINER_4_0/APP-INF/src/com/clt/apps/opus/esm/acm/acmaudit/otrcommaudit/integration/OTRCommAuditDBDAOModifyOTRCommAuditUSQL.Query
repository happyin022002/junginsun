<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OTRCommAuditDBDAOModifyOTRCommAuditUSQL">
			<desc><![CDATA[ModifyOTRCommAudit]]></desc>
			<sql><![CDATA[
UPDATE ACM_AGN_OTR_COMM
   SET AC_STS_CD   = 'AS',
       AUD_NO      = @[aud_no],
       AUD_USR_ID  = @[usr_id],
       AUD_GDT     = GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,'GMT'),
       AUD_DT      = SYSDATE,
       UPD_USR_ID  = @[usr_id],
       UPD_DT      = SYSDATE
 WHERE RQST_DT     IS NOT NULL
   AND AUD_DT      IS NULL
   AND APRO_DT     IS NULL
   AND AC_STS_CD
    IN
     (
       'RS','RM'
     )
AND OTR_COMM_NO||AGN_CD||IO_BND_CD||AC_SEQ IN (${bkg_no})			]]></sql>
			<params>
				<param name="aud_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
