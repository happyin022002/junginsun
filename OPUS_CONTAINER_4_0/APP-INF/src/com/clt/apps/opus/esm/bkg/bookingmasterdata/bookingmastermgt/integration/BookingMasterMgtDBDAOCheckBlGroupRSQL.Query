<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOCheckBlGroupRSQL">
			<desc><![CDATA[check BL Group]]></desc>
			<sql><![CDATA[
SELECT COUNT(*)
FROM BKG_CTRL_PTY_BL_GRP
WHERE CTRL_PTY_SEQ = @[ctrl_pty_seq]
AND BL_GRP_SEQ = @[bl_grp_seq]			]]></sql>
			<params>
				<param name="ctrl_pty_seq" type="12" value="" out="N"/>
				<param name="bl_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
