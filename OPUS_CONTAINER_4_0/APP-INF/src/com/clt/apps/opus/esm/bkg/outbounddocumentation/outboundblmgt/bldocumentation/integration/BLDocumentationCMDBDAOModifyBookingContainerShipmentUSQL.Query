<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOModifyBookingContainerShipmentUSQL">
			<desc><![CDATA[ModifyBookingContainerShipment]]></desc>
			<sql><![CDATA[
UPDATE BKG_CNTR_SHP
    SET PRN_FLG	= 'Y'
    ,UPD_USR_ID	= @[usr_id]
    ,UPD_DT		= sysdate
WHERE BKG_NO	= @[bkg_no]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
