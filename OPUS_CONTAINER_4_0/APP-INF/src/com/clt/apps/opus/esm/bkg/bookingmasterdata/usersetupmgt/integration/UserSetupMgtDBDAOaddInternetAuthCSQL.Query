<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOaddInternetAuthCSQL">
			<desc><![CDATA[UserSetupMgtDBDAOaddInternetAuthCSQL]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_INET_AUTH TA
    USING (SELECT @[usr_id] USR_ID FROM DUAL) TB
    ON(TA.USR_ID = TB.USR_ID)
 WHEN MATCHED THEN 
    UPDATE SET TA.DELT_FLG = 'N', UPD_USR_ID = @[upd_usr_id],UPD_DT = SYSDATE
 WHEN NOT MATCHED THEN 
    INSERT (USR_ID    , RGST_USR_ID   , RGST_DT, DELT_FLG, CRE_USR_ID, CRE_DT , UPD_USR_ID, UPD_DT, INET_AUTH_FLG, INET_FTP_AUTH_FLG)
    VALUES (@[usr_id] , @[upd_usr_id] , SYSDATE, 'N'     , @[upd_usr_id] , SYSDATE, @[upd_usr_id] , SYSDATE
            , DECODE(@[inet_auth_flg],'1','Y','N'), DECODE(@[inet_ftp_auth_flg],'1','Y','N'))			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="inet_auth_flg" type="12" value="" out="N"/>
				<param name="inet_ftp_auth_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
