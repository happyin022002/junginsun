<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOSearchSpcScPortDownDetailListRSQL">
			<desc><![CDATA[SearchSpcScPortDownDetailList SELECT]]></desc>
			<sql><![CDATA[
SELECT 
       B.BSA_SEQ
      ,A.BSA_FM_DT
      ,B.TRD_CD
      ,B.RLANE_CD
      ,B.DIR_CD
      ,B.VSL_SEQ
      ,A.VSL_CD
      ,B.BSA_OP_CD
      ,B.BSA_OP_JB_CD
      ,B.CRR_CD
      ,B.PORT_SEQ
      ,B.PORT_CD
      ,B.PORT_BSA_CAPA 
FROM 
      BSA_SLT_CHTR_BZC A, 
      BSA_SLT_CHTR_PORT_DWN B 
WHERE 
      A.BSA_SEQ       = B.BSA_SEQ 
  AND A.RLANE_CD      = B.RLANE_CD 
  AND A.DIR_CD        = B.DIR_CD 
  AND A.TRD_CD        = B.TRD_CD 
  AND A.VSL_SEQ       = B.VSL_SEQ 
  AND B.BSA_SEQ       = @[bsa_seq] 
  AND B.TRD_CD        = @[trd_cd] 
  AND B.RLANE_CD      = @[rlane_cd] 
  AND B.DIR_CD        = @[dir_cd] 
  AND B.VSL_SEQ       = @[vsl_seq] 
  AND B.BSA_OP_CD     = @[bsa_op_cd] 
  AND B.BSA_OP_JB_CD  = @[bsa_op_jb_cd] 
  AND B.CRR_CD        = @[crr_cd] 
ORDER BY 
      A.BSA_FM_DT, 
      B.TRD_CD, 
      B.RLANE_CD, 
      B.DIR_CD, 
      B.VSL_SEQ, 
      B.PORT_SEQ			]]></sql>
			<params>
				<param name="bsa_seq" type="12" value="" out="Y"/>
				<param name="trd_cd" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
				<param name="vsl_seq" type="12" value="" out="Y"/>
				<param name="bsa_op_cd" type="12" value="" out="Y"/>
				<param name="bsa_op_jb_cd" type="12" value="" out="Y"/>
				<param name="crr_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
