<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOmodifyBkgKrWhfPortRtUSQL">
			<desc><![CDATA[U]]></desc>
			<sql><![CDATA[
UPDATE BKG_KR_WHF_PORT_RT
SET
 CNTR_BLK_DIV_CD = @[cntr_blk_div_cd]
,PORT_CD         = @[port_cd] 
,IO_BND_CD       = @[io_bnd_cd] 
,DC_RTO_NO       = @[dc_rto_no]
,TEU_PRC         = @[teu_prc] 
,FEU_PRC         = @[feu_prc]
,HC_PRC          = @[hc_prc]
,TEU_AMT_RT      = @[teu_amt_rt]
,FEU_AMT_RT      = @[feu_amt_rt]
,HC_AMT_RT       = @[hc_amt_rt] 
,UPD_USR_ID      = @[upd_usr_id]
,UPD_DT          = SYSDATE
WHERE 1=1
AND CNTR_BLK_DIV_CD = @[cntr_blk_div_cd]
AND PORT_CD         = @[port_cd]
AND IO_BND_CD       = @[io_bnd_cd]  
AND DC_RTO_NO       = @[dc_rto_no]			]]></sql>
			<params>
				<param name="cntr_blk_div_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dc_rto_no" type="12" value="" out="N"/>
				<param name="teu_prc" type="12" value="" out="N"/>
				<param name="feu_prc" type="12" value="" out="N"/>
				<param name="hc_prc" type="12" value="" out="N"/>
				<param name="teu_amt_rt" type="12" value="" out="N"/>
				<param name="feu_amt_rt" type="12" value="" out="N"/>
				<param name="hc_amt_rt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
