<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchCustInfoRSQL">
			<desc><![CDATA[Custinfo  조회]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(DECODE(BKG_CUST_TP_CD,'S',DECODE(NVL(LENGTH(TRIM(REPLACE(NVL(CUST_NM,' '),CHR(10),' '))),0),0,'N','Y'),'N')), 'N') SHPR_NAME
     , NVL(MAX(DECODE(BKG_CUST_TP_CD,'S',DECODE(NVL(LENGTH(TRIM(REPLACE(NVL(CUST_ADDR,' '),CHR(10),' '))),0),0,'N','Y'),'N')),'N') SHPR_ADDR
     , NVL(MAX(DECODE(BKG_CUST_TP_CD,'C',DECODE(NVL(LENGTH(TRIM(REPLACE(NVL(CUST_NM,' '),CHR(10),' '))),0),0,'N','Y'),'N')), 'N') CNEE_NAME
     , NVL(MAX(DECODE(BKG_CUST_TP_CD,'C',DECODE(NVL(LENGTH(TRIM(REPLACE(NVL(CUST_ADDR,' '),CHR(10),' '))),0),0,'N','Y'),'N')),'N') CNEE_ADDR
     , NVL(MAX(DECODE(BKG_CUST_TP_CD,'N',DECODE(NVL(LENGTH(TRIM(REPLACE(NVL(CUST_NM,' '),CHR(10),' '))),0),0,'N','Y'),'N')), 'N') NTFY_NAME
     , NVL(MAX(DECODE(BKG_CUST_TP_CD,'N',DECODE(NVL(LENGTH(TRIM(REPLACE(NVL(CUST_ADDR,' '),CHR(10),' '))),0),0,'N','Y'),'N')),'N') NTFY_ADDR
     , NVL(SUBSTR(MAX(DECODE(@[in_bound],'O',DECODE(BKG_CUST_TP_CD,'S',NVL(CUST_NM,' '),' '),
           DECODE(BKG_CUST_TP_CD,'C',NVL(CUST_NM,' '),' '))),1,35),' ') USRS14
  FROM BKG_CUSTOMER
 WHERE BKG_NO = @[bkg_no]
   AND BKG_CUST_TP_CD IN ('S','C','N')			]]></sql>
			<params>
				<param name="in_bound" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
