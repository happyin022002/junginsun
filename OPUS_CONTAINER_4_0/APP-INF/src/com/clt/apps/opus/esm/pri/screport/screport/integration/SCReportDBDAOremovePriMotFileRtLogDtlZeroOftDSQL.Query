<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAOremovePriMotFileRtLogDtlZeroOftDSQL">
			<desc><![CDATA[일별 데이터 중 OFT 가 Null 이거나 Zero 인 경우에 대해 생성된 Surcharge Detail 삭제
MOT Tariff Matching 을 위한 준비 과정
PRI_MOT_FILE_RT_LOG_DTL]]></desc>
			<sql><![CDATA[
DELETE  PRI_MOT_FILE_RT_LOG_DTL A
    WHERE   A.BAT_EXE_DT = TO_DATE (@[exec_dt], 'YYYY-MM-DD' )
    AND     A.MOT_FILE_RT_LOG_SEQ IN ( SELECT  B.MOT_FILE_RT_LOG_SEQ
                                       FROM    PRI_MOT_FILE_RT_LOG B
                                       WHERE   B.BAT_EXE_DT = TO_DATE (@[exec_dt], 'YYYY-MM-DD' )
                                       AND   ( B.MOT_FILE_RT_AMT IS NULL OR B.MOT_FILE_RT_AMT = 0 ) )			]]></sql>
			<params>
				<param name="exec_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
