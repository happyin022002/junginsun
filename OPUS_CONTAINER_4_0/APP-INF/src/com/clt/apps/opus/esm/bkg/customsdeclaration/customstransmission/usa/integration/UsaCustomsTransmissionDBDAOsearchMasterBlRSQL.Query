<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchMasterBlRSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신 outVO : None, 연관VO: UsaResultCntrVO]]></desc>
			<sql><![CDATA[
SELECT BL_NO in_bl
  FROM BKG_CSTMS_ADV_BL
 WHERE CNT_CD = 'US'
   AND
	(
		BL_NO = SUBSTR(@[in_hjbl], 1, 12)
		OR
		BL_NO = SUBSTR(@[in_hjbl], 5, 12)
	)
UNION
SELECT BL_NO
  FROM BKG_BOOKING I
      ,BKG_USA_CSTMS_FILE_NO N
 WHERE I.BKG_NO = N.BKG_NO
   AND I.BKG_STS_CD <> 'X'
   AND N.USA_CSTMS_FILE_NO = @[in_nvobl]			]]></sql>
			<params>
				<param name="in_hjbl" type="12" value="" out="N"/>
				<param name="in_nvobl" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
