<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOPndlmSvcCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
MERGE INTO COA_PNDLM_SVC B1 
USING (
       SELECT '1' FROM DUAL 
	  ) B2	
ON (     B1.SLAN_CD     = @[slan_cd]   
     AND B1.DIR_CD      = @[dir_cd]
     AND B1.EFF_FM_DT 	= @[eff_fm_dt]	) 
WHEN MATCHED THEN
      UPDATE 
         SET VVD_CD	    = @[vvd_cd]
            ,UPD_USR_ID = @[upd_usr_id]
            ,UPD_DT     = SYSDATE
WHEN NOT MATCHED THEN 
      INSERT (SLAN_CD
             ,DIR_CD
             ,EFF_FM_DT
             ,EFF_TO_DT
             ,VVD_CD
             ,CRE_USR_ID
             ,CRE_DT
             ,UPD_USR_ID
             ,UPD_DT
             ) 
      VALUES (@[slan_cd]
             ,@[dir_cd]
             ,@[eff_fm_dt]
             ,@[eff_to_dt]
             ,@[vvd_cd]
             ,@[cre_usr_id]
             ,SYSDATE
             ,@[upd_usr_id]
             ,SYSDATE)			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
