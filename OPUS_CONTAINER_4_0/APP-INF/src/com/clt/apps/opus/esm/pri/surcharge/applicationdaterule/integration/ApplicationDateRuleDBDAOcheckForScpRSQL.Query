<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApplicationDateRuleDBDAOcheckForScpRSQL">
			<desc><![CDATA[해당 로케이션이 scope내에 region에 속하는지 확인하는 벨리데이션]]></desc>
			<sql><![CDATA[
SELECT COUNT(*)
  FROM MDM_SVC_SCP_LMT LMT
      ,MDM_LOCATION LOC
 WHERE LOC.LOC_CD = @[chk_location]
   AND LMT.RGN_CD = LOC.RGN_CD
   AND LMT.SVC_SCP_CD = @[chk_scp_cd]
   AND LMT.ORG_DEST_CD = DECODE(@[chk_flg],'1','O','D')			]]></sql>
			<params>
				<param name="chk_location" type="12" value="" out="N"/>
				<param name="chk_scp_cd" type="12" value="" out="N"/>
				<param name="chk_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
