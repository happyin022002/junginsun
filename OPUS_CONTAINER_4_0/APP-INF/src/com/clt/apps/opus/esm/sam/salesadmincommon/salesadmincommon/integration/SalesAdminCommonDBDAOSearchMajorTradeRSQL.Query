<sqls>
	<querys>
		<query name="SalesAdminCommonDBDAOSearchMajorTradeRSQL">
			<desc><![CDATA[Major Trade 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT TRD_CD CD
     , TRD_NM CD_DESC
FROM MDM_TRADE
WHERE DELT_FLG = 'N'
  AND FM_CONTI_CD IS NOT NULL
  AND TO_CONTI_CD IS NOT NULL
ORDER BY TRD_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
