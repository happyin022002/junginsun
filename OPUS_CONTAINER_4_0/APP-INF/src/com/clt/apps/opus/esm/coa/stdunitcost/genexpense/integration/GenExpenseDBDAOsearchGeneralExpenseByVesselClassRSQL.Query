<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GenExpenseDBDAOsearchGeneralExpenseByVesselClassRSQL">
			<desc><![CDATA[Own Vessel Daily hire 조회]]></desc>
			<sql><![CDATA[
SELECT COST_YRMON			AS COST_YRMON4		--SJH.20141229.MOD : NAME
      , VSL_CLSS_CAPA		AS VSL_CLSS_CAPA4
      , ROUND(DHIR_AMT,2) 	AS DHIR_AMT4
   FROM COA_OWN_VSL_DLY_HIR
  WHERE COST_YRMON   = @[cost_yrmon]
    AND VSL_CD       = 'XXXX'
    AND STND_COST_CD = '75000000'
  ORDER BY VSL_CLSS_CAPA			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
