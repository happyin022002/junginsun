<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAOSearchKeyManaListRSQL">
			<desc><![CDATA[SearchKeyManaList]]></desc>
			<sql><![CDATA[
SELECT CUST_KMAN_SEQ
     , KMAN_N1ST_NM
     , KMAN_LST_NM
     , KMAN_GND_CD
     , JB_TIT_RMK
     , INTL_PHN_NO
     , KMAN_OFC_PHN_NO
     , KMAN_OFC_FAX_NO
     , KMAN_EML
FROM SAM_CUST_KMAN_INFO
WHERE CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND   CUST_SEQ = SUBSTR(@[cust_cd],3,6)
ORDER BY CUST_KMAN_SEQ			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
