<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDBDAOCheckArFincDirConvUsedFlagRSQL">
			<desc><![CDATA[AR_FINC_DIR_CONV 존재여부]]></desc>
			<sql><![CDATA[
SELECT CASE
         WHEN COUNT(RLANE_DIR_CD) > 0 THEN 'Y'
         ELSE ''
       END AS USED_YN
  FROM AR_FINC_DIR_CONV
 WHERE 1=1
   AND DELT_FLG = 'N'
#if (${rlane_cd} != '')
   AND RLANE_CD = @[rlane_cd]
#end
#if (${slan_cd} != '')
   AND SLAN_CD = @[slan_cd]
#end
#if (${slan_dir_cd} != '')
   AND SLAN_DIR_CD = @[slan_dir_cd]
#end
#if (${rlane_dir_cd} != '')
   AND RLANE_DIR_CD = @[rlane_dir_cd]
#end			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="slan_dir_cd" type="12" value="" out="N"/>
				<param name="rlane_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
