<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOaddContractDeleteHistoryCSQL">
			<desc><![CDATA[계약 삭제 history 관리]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_CTRT_DELT_HIS
(
 PROP_NO,
 AMDT_SEQ,
 PRC_CTRT_DELT_DT,
 PRC_CTRT_TP_CD,
 PRC_CTRT_NO,
 PRC_CTRT_DELT_USR_ID,
 PRC_CTRT_CRE_USR_ID,
 PRC_CTRT_CRE_DT,
 PRC_CTRT_DELT_RMK,
 CRE_USR_ID,
 CRE_DT,
 UPD_USR_ID,
 UPD_DT
)
(
SELECT PROP_NO,
       AMDT_SEQ,
       SYSDATE,
       'S',
       '',
       @[prc_ctrt_delt_usr_id],
       CRE_USR_ID,
       CRE_DT,
       NVL(@[prc_ctrt_delt_rmk], ''),
       @[usr_id],
       SYSDATE,
       @[usr_id],
       SYSDATE
FROM PRI_SP_MN
WHERE PROP_NO = @[prop_no] AND AMDT_SEQ = @[amdt_seq]
)			]]></sql>
			<params>
				<param name="prc_ctrt_delt_usr_id" type="12" value="" out="N"/>
				<param name="prc_ctrt_delt_rmk" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
