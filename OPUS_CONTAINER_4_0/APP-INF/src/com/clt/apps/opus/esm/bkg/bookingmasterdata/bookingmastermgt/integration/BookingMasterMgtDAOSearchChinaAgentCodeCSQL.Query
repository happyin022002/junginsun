<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDAOSearchChinaAgentCodeCSQL">
			<desc><![CDATA[중국 Booking Agent 정보 등록 화면]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CHN_AGN (
	CHN_AGN_CD
,	AGN_NM
,	DIFF_RMK
,	VNDR_CNT_CD
,	VNDR_SEQ
,	CUST_CNT_CD
,	CUST_SEQ
,	AUTO_DP_CHK_FLG
,	AGN_EML
,	OFC_CD
,	FINC_OFC_CD
,	AR_OFC_CD
,	DIR_PAY_OFC_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[chn_agn_cd]
,	@[agn_nm]
,	@[diff_rmk]
,	@[vndr_cnt_cd]
,	@[vndr_seq]
,	@[cust_cnt_cd]
,	@[cust_seq]
,	DECODE(@[auto_dp_chk_flg],'1','Y','0','N',@[auto_dp_chk_flg])
,	@[agn_eml]
,	@[ofc_cd]
,	@[finc_ofc_cd]
,	@[ar_ofc_cd]
,	@[dir_pay_ofc_cd]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="chn_agn_cd" type="12" value="" out="N"/>
				<param name="agn_nm" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="auto_dp_chk_flg" type="12" value="" out="N"/>
				<param name="agn_eml" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="finc_ofc_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="dir_pay_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
