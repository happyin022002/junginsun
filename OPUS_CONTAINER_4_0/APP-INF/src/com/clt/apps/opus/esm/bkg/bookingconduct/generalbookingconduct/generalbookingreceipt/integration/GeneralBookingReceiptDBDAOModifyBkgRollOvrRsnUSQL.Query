<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModifyBkgRollOvrRsnUSQL">
			<desc><![CDATA[update BKG_ROLL_OVR]]></desc>
			<sql><![CDATA[
UPDATE BKG_ROLL_OVR SET  
    ROLL_OVR_RSN_CD = @[roll_ovr_rsn_cd]
,   DIFF_RMK = @[diff_rmk] 
,	EVNT_USR_ID = @[upd_usr_id]
,   EVNT_DT = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,BKG_COM_USER_LOC_FNC(@[upd_usr_id]))
,   UPD_USR_ID = @[upd_usr_id]
,   UPD_DT = SYSDATE
WHERE	BKG_NO = @[bkg_no]
AND     ROLL_OVR_SEQ = @[roll_ovr_seq]
--AND	TO_CHAR(EVNT_DT, 'yyyy-mm-dd hh24:mi') = [evnt_dt]
AND NEW_VSL_CD IS NOT NULL			]]></sql>
			<params>
				<param name="roll_ovr_rsn_cd" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="roll_ovr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
