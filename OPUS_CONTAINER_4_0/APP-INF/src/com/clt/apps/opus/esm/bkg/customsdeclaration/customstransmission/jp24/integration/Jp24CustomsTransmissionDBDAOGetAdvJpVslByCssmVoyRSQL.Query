<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24CustomsTransmissionDBDAOGetAdvJpVslByCssmVoyRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT SKD_VOY_NO,
       SKD_DIR_CD 

  FROM BKG_CSTMS_ADV_JP_VSL_SKD

 WHERE IB_CSSM_VOY_NO = @[ib_cssm_voy_no]
#if (${call_sgn_no} != '')
   AND CALL_SGN_NO = @[call_sgn_no]
#end
   AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="ib_cssm_voy_no" type="12" value="" out="N"/>
				<param name="call_sgn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
