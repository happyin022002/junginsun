<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusManageDBDAOCreateTransferCsqDatRltCSQL">
			<desc><![CDATA[Qta Transfer 시 연간 정보로 1Q CSQ_DAT_RLT 생성]]></desc>
			<sql><![CDATA[
INSERT INTO CSQ_DAT_RLT (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,TRD_CD
      ,CONV_DIR_CD
      ,RHQ_CD
      ,TEAM_CD
      ,OB_DIV_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT 'Q'    AS BSE_TP_CD
      ,BSE_YR AS BSE_YR
      ,'1Q'   AS BSE_QTR_CD
      ,OFC_VW_CD
      ,TRD_CD
      ,CONV_DIR_CD
      ,RHQ_CD
      ,TEAM_CD
      ,OB_DIV_CD
      ,@[f_usr_id] AS CRE_USR_ID
      ,SYSDATE     AS CRE_DT
      ,@[f_usr_id] AS UPD_USR_ID
      ,SYSDATE     AS UPD_DT
  FROM CSQ_DAT_RLT
 WHERE BSE_TP_CD  = 'Y'
   AND BSE_YR     = @[f_bse_yr]
   AND BSE_QTR_CD = '00'			]]></sql>
			<params>
				<param name="f_usr_id" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
