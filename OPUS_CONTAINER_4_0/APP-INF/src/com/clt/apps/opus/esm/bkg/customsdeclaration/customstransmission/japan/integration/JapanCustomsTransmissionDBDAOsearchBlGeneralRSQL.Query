<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOsearchBlGeneralRSQL">
			<desc><![CDATA[searchBlGeneral]]></desc>
			<sql><![CDATA[
SELECT RPAD(NVL(SUBSTR(@[in_call_sgn_no], 1, 9), ' '), 9, ' ') AS IN_CALL_SGN_NO,
       RPAD(NVL(JP_TML_VSL_NO, SUBSTR(@[in_vvd_cd], 5, 5)), 10, ' ') AS IN_VVD_CD,
       RPAD(NVL(JP_TML_VSL_NO, ' '), 10, ' ') AS JP_TML_VSL_NO,
       RPAD(' ', 1, ' ') AS DATA1, --7 POD Split No (1)
       RPAD(NVL(A.CY_OPR_ID, ' '), 5, ' ') AS CY_OPR_CD,
       RPAD(COM_ConstantMgr_PKG.COM_getScacCode_FNC()||@[bl_no], 35, ' ') AS DATA2,
       CASE
          WHEN SUBSTR(L1.LOC_CD, 1, 2) = 'JP' THEN
             NVL(L3.ATTR_CTNT2, L1.LOC_CD)
          ELSE
             DECODE(L3.ATTR_CTNT2, NULL, DECODE(SUBSTR(L1.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L1.LOC_CD, 1, 2))||'ZZZ', L3.ATTR_CTNT2)
       END AS UN_LOC_ID1,
       RPAD(' ', 5, ' ') AS DATA3, --12 Final. DEST Code  (5) C
       RPAD(' ', 20, ' ') AS DATA4, --13 Final. DEST Name  (20) C
       CASE
          WHEN SUBSTR(L2.LOC_CD, 1, 2) = 'JP' THEN
             NVL(L4.ATTR_CTNT2, L2.LOC_CD)
          ELSE
             DECODE(L4.ATTR_CTNT2, NULL, DECODE(SUBSTR(L2.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L2.LOC_CD, 1, 2))||'ZZZ', L4.ATTR_CTNT2)
       END AS UN_LOC_ID2,
       RPAD(SUBSTR(UPPER(L2.LOC_NM), 1, 20), 20, ' ') AS LOC_NM,
       --RPAD(' ', 4, ' ') AS DATA5, --47 대표 품목번호  (4)   C
       (SELECT NVL(SUBSTR(MAX(CMDT_HS_CD), 1, 4), '    ')
          FROM BKG_CNTR_MF_DESC
         WHERE BKG_NO = B.BKG_NO
           AND CNTR_MF_WGT IN (SELECT MAX(CMD.CNTR_MF_WGT)
                                 FROM BKG_CNTR_MF_DESC CMD
                                WHERE CMD.BKG_NO = B.BKG_NO
                                  AND SUBSTR(CMD.CMDT_HS_CD, 1, 2) NOT IN ('00', '98', '99'))
           AND SUBSTR(CMDT_HS_CD, 1, 2) NOT IN ('00', '98', '99')) AS DATA5, --47 대표 품목번호  (4)   C
       LPAD(NVL(A.PCK_QTY, 0), 8, ' ') AS PCK_QTY,
       RPAD(DECODE(P.CSTMS_PCK_TP_CD, NULL, 'ZZ', P.CSTMS_PCK_TP_CD), 3, ' ') AS PCK_CSTMS_CD,
       --LPAD(DECODE(NVL(A.GRS_WGT, 0), 0, 0, SUBSTR(TO_CHAR(NVL(A.GRS_WGT, 0), '0999999.999'), 2)), 10, ' ') AS GRS_WGT,
       LPAD(TO_CHAR(NVL(A.GRS_WGT, 0), 'FM999990.000'), 10, ' ') AS GRS_WGT,
       DECODE(NVL(A.WGT_UT_CD, 'KGS'), 'LBS', 'LBR', 'KGS', 'KGM', A.WGT_UT_CD ) AS WGT_UT_CD,
       RPAD(' ', 10, ' ') AS DATA6, --53 인터넷중량  (10)   C
       RPAD(' ', 3, ' ') AS DATA7, --54 중량단위 코드  (3)   C-M
       --LPAD(DECODE(NVL(A.MEAS_QTY, 0), 0, 0, SUBSTR(TO_CHAR(NVL(A.MEAS_QTY, 0), '0999999.999'), 2)), 10, ' ') AS MEAS_QTY,
       LPAD(TO_CHAR(NVL(A.MEAS_QTY, 0), 'FM999990.000'), 10, ' ') AS MEAS_QTY,
       DECODE(NVL(A.MEAS_UT_CD, 'CBM'), 'CMF', 'FTQ', 'MTQ') AS MEAS_UT_CD,
       RPAD(' ', 2, ' ') AS DATA8, --57 원산국 코드  (2)   C
       RPAD(' ', 3, ' ') AS DATA9, --58 위험화물등 코드  (3)   C
       RPAD(' ', 18, ' ') AS DATA10, --59 해상운임(후레토】)  (18)   C
       RPAD(' ', 3, ' ') AS DATA11, --60 해상운임통화종별코드  (3)   M
       RPAD(' ', 18, ' ') AS DATA12, --61 가격  (18)   C
       RPAD(' ', 3, ' ') AS DATA13, --62 가격통화종별코드  (3)   C-M
       RPAD(NVL(A.APRO_NO, ' '), 11, ' ') AS DATA14, --63 포괄 보세 운송 승인 번호 (11) C
       DECODE(NVL(A.LOCL_TS_IND_CD, 'L'), 'T', '28 ', '   ') AS LOCL_TS_FLG1,
       DECODE(NVL(A.LOCL_TS_IND_CD, ' '), 'T', NVL(A.JP_CSTMS_TRNS_CD, ' '), '   ') AS LOCL_TS_FLG2,
       DECODE(NVL(A.LOCL_TS_IND_CD, ' '), 'T', SUBSTR(TO_CHAR(NVL(A.LMT_NO, 0), '99'), 2), '  ') AS LOCL_TS_FLG3,
       RPAD(' ', 8, ' ') AS DATA15, --67 운송 기간시작 예정일  (8)   C
       RPAD(' ', 8, ' ') AS DATA16, --68 운송 기간종료 예정일  (8)   C
       --RPAD(NVL(A.TRSP_MOD_CD, ' '), 2, ' ') DATA17, --69 개별운송 또는 가양육 화물보세 운송의 운송 도구 코드 (2) C
       RPAD(NVL(DECODE(A.TRSP_MOD_CD, 'SH', '11', 'BA', '16', 'RA', '25', 'TR', '31', 'OT', '17', NULL), ' '), 2, ' ') AS DATA17, --69 개별운송 또는 가양육 화물보세 운송의 운송 도구 코드 (2) C
       RPAD(NVL(A.CSTMS_CD, ' '), 5, ' ') AS DATA18, --70 도착지코드 (5) C
       RPAD(NVL((SELECT NVL(WH_NM, ' ')
                   FROM BKG_CSTMS_JP_WH
                  WHERE CSTMS_CD = A.CSTMS_CD
                    AND ROWNUM = 1), ' '), 35, ' ') AS DATA19, --71 도착지명 (35) C
       RPAD(' ', 2, ' ') AS DATA20, --72 타법령 코드  (2)   5[반복]   C
       RPAD(' ', 2, ' ') AS DATA21, --72 타법령 코드  (2)   5[반복]   C
       RPAD(' ', 2, ' ') AS DATA22, --72 타법령 코드  (2)   5[반복]   C
       RPAD(' ', 2, ' ') AS DATA23, --72 타법령 코드  (2)   5[반복]   C
       RPAD(' ', 2, ' ') AS DATA24, --72 타법령 코드  (2)   5[반복]   C
       RPAD(' ', 140, ' ') AS DATA25 --73 기사  (140)   C

  FROM BKG_CSTMS_JP_BL A,
       BKG_BOOKING B,
       MDM_LOCATION L1,
       MDM_LOCATION L2,
       BKG_CSTMS_PCK_TP_CONV P,
       BKG_CSTMS_CD_CONV_CTNT L3,
       BKG_CSTMS_CD_CONV_CTNT L4

 WHERE A.BL_NO = @[bl_no]
   AND A.BL_SPLIT_NO = NVL(@[bl_split_no], '  ')
   AND A.BL_NO = B.BL_NO
   AND A.POL_CD = L1.LOC_CD(+)
   AND A.BKG_DEL_CD = L2.LOC_CD(+)
   AND L3.CNT_CD(+) = 'JP'
   AND L3.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.POL_CD = L3.ATTR_CTNT1(+)
   AND L4.CNT_CD(+) = 'JP'
   AND L4.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND A.BKG_DEL_CD = L4.ATTR_CTNT1(+)
   AND A.PCK_TP_CD = P.PCK_TP_CD(+)
   AND P.DELT_FLG(+) = 'N'
   AND P.CNT_CD(+) = 'JP'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="in_call_sgn_no" type="12" value="" out="N"/>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
