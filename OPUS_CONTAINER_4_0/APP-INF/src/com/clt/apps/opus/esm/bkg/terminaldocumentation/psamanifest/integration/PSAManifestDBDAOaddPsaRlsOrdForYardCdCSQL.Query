<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOaddPsaRlsOrdForYardCdCSQL">
			<desc><![CDATA[Yard Assign By CNTR화면에서 Row add시 insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_PSA_RLSE_ORD
    (       BKG_NO          ,
            BKG_SEQ         ,
            PSA_SER_NO      ,
            SUB_PSA_SER_NO  ,
            PSA_IF_CD       ,
            YD_CD           ,
            CNTR_TPSZ_CD    ,
            CNTR_QTY        ,
            CRE_DT          ,
            CRE_USR_ID      ,
            UPD_DT          ,
            UPD_USR_ID   )
    VALUES
    (       @[bkg_no]       ,
            @[bkg_seq] + 1  ,
            1               ,
            1               ,
            'N'             ,
            @[yd_cd]        ,
            @[cntr_tpsz_cd] ,
            @[cntr_qty]     ,
            SYSDATE         ,
            @[usr_id]       ,
            SYSDATE         ,
            @[usr_id]       )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_qty" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
