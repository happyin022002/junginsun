<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOUpdateGuidelineConfirmUSQL">
			<desc><![CDATA[해당 버전의 Guideline 을 Confirm 한다]]></desc>
			<sql><![CDATA[
UPDATE SAQ_MON_INIT_GLINE 
       SET GLINE_STS_FLG = 'C' 
     , UPD_USR_ID = @[user_id]  
     , UPD_DT = SYSDATE
 WHERE 1=1 
       AND MQTA_MDL_VER_NO = @[mqta_mdl_ver_no]        
       #if(${trade} != '' && ${trade} != 'ALL') 
	   AND TRD_CD = @[trade]
       #end 
       #if(${subtrade} != '' && ${subtrade} != 'ALL') 
       AND SUB_TRD_CD = @[subtrade]
       #end
       #if(${bound} != '' && ${bound} != 'ALL') 
	   AND DIR_CD = @[bound]
       #end			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="mqta_mdl_ver_no" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="subtrade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
