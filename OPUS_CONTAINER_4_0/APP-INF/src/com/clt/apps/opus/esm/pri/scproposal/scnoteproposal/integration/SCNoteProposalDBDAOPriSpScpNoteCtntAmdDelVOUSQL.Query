<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteProposalDBDAOPriSpScpNoteCtntAmdDelVOUSQL">
			<desc><![CDATA[Standard Note - Amend 시 Delete 버튼]]></desc>
			<sql><![CDATA[
UPDATE PRI_SP_SCP_NOTE_CTNT
   SET SRC_INFO_CD 			= @[src_info_cd]
     , PRC_PROG_STS_CD 		= @[prc_prog_sts_cd]
     , N1ST_CMNC_AMDT_SEQ 	= @[amdt_seq]
     , UPD_DT 				= SYSDATE
     , UPD_USR_ID 			= @[upd_usr_id]
 WHERE PROP_NO 			= @[prop_no]
   AND AMDT_SEQ 		= @[amdt_seq]
   AND SVC_SCP_CD 		= @[svc_scp_cd]
   AND NOTE_TP_CD 		= @[note_tp_cd]			]]></sql>
			<params>
				<param name="src_info_cd" type="12" value="" out="N"/>
				<param name="prc_prog_sts_cd" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="note_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
