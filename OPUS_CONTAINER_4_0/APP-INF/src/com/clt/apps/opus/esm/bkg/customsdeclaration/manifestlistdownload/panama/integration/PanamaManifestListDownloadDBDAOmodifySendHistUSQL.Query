<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PanamaManifestListDownloadDBDAOmodifySendHistUSQL">
			<desc><![CDATA[modifySendHist]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_PNM_VVD SET
	EDI_SND_DT = TO_DATE(@[edi_snd_dt],'YYYY-MM-DD HH24:MI:SS'),
	EDI_SND_SEQ = NVL(EDI_SND_SEQ,0)+1,
	EDI_SND_USR_ID = @[upd_usr_id],
	UPD_USR_ID = @[upd_usr_id],
	UPD_DT = SYSDATE
WHERE VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="edi_snd_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
