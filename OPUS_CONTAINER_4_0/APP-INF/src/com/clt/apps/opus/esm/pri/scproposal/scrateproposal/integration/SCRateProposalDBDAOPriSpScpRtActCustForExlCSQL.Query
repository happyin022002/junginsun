<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAOPriSpScpRtActCustForExlCSQL">
			<desc><![CDATA[create PRI_SP_SCP_RT_ACT_CUST]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_RT_ACT_CUST (
	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	GEN_SPCL_RT_TP_CD
,	CMDT_HDR_SEQ
,	ACT_CUST_SEQ
,	CUST_CNT_CD
,	CUST_SEQ
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	N1ST_CMNC_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT
	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	GEN_SPCL_RT_TP_CD
,	CMDT_HDR_SEQ
,   NVL(A.ACT_CUST_SEQ, (B.MAX_ACT_CUST_SEQ + 1)) AS ACT_CUST_SEQ
,	CUST_CNT_CD
,	CUST_SEQ
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	N1ST_CMNC_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
  FROM (
SELECT 
	@[prop_no] AS PROP_NO
,	@[amdt_seq] AS AMDT_SEQ
,	@[svc_scp_cd] AS SVC_SCP_CD
,	@[gen_spcl_rt_tp_cd] AS GEN_SPCL_RT_TP_CD
,	@[cmdt_hdr_seq] AS CMDT_HDR_SEQ
,   @[act_cust_seq] AS ACT_CUST_SEQ
,	@[cust_cnt_cd] AS CUST_CNT_CD
,	@[cust_seq] AS CUST_SEQ
,	@[prc_prog_sts_cd] AS PRC_PROG_STS_CD
,	@[src_info_cd] AS SRC_INFO_CD
,   @[n1st_cmnc_amdt_seq] AS N1ST_CMNC_AMDT_SEQ
,	@[cre_usr_id] AS CRE_USR_ID
,	DECODE(NVL(@[cre_dt],'1'),'1',SYSDATE,TO_DATE(REPLACE(@[cre_dt],'.0',''),'YYYY-MM-DD HH24:MI:SS')) AS CRE_DT
,	@[upd_usr_id] AS UPD_USR_ID
,	DECODE(NVL(@[upd_dt],'1'),'1',SYSDATE,TO_DATE(REPLACE(@[upd_dt],'.0',''),'YYYY-MM-DD HH24:MI:SS')) AS UPD_DT
FROM DUAL) A,
(
SELECT NVL((SELECT /*+ INDEX_DESC(A PRI_SP_SCP_RT_ACT_CUST) */
                  ACT_CUST_SEQ
             FROM PRI_SP_SCP_RT_ACT_CUST A
            WHERE PROP_NO = @[prop_no]
              AND AMDT_SEQ = @[amdt_seq]
              AND SVC_SCP_CD = @[svc_scp_cd]
              AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
              AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
              AND ROWNUM = 1)
          ,0) AS MAX_ACT_CUST_SEQ
  FROM DUAL) B			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="act_cust_seq" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="prc_prog_sts_cd" type="12" value="" out="N"/>
				<param name="src_info_cd" type="12" value="" out="N"/>
				<param name="n1st_cmnc_amdt_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
