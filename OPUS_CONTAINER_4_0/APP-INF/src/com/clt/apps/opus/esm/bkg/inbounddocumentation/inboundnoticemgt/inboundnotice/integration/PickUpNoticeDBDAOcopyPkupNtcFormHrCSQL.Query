<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOcopyPkupNtcFormHrCSQL">
			<desc><![CDATA[Form Type별 Event발생시 Notice 발송 시간에 대한 Setting 정보를  Copy한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_PKUP_NTC_HR (
	PKUP_NTC_SEQ
,	PKUP_NTC_FOM_CD
,	NTC_SEQ
,	NTC_BSE_HRS
,	NTC_COND_CD
,	MVMT_STS_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT)
SELECT (SELECT PKUP_NTC_SEQ
        FROM   BKG_PKUP_NTC_STUP T
        WHERE  PKUP_NTC_SND_TP_CD = 'A'
        AND    OFC_CD             = @[ofc_cd]
        AND    DEL_CD             = DECODE(nvl(@[del_cd],'ALL'),'ALL','*',@[del_cd])) AS PKUP_NTC_SEQ
,	T2.PKUP_NTC_FOM_CD
,	T2.NTC_SEQ
,	T2.NTC_BSE_HRS
,	T2.NTC_COND_CD
,	T2.MVMT_STS_CD
,	@[usr_id] AS CRE_USR_ID
,	SYSDATE       AS CRE_DT
,	@[usr_id] AS UPD_USR_ID
,	SYSDATE       AS UPD_DT
FROM   BKG_PKUP_NTC_STUP T1
      ,BKG_PKUP_NTC_HR   T2
WHERE  T1.PKUP_NTC_SND_TP_CD = 'A'
AND    T1.OFC_CD             = @[fm_ofc_cd]
AND    T1.DEL_CD             = DECODE(nvl(@[fm_del_cd],'ALL'),'ALL','*',@[fm_del_cd])
AND    T2.PKUP_NTC_SEQ       = T1.PKUP_NTC_SEQ			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="fm_ofc_cd" type="12" value="" out="N"/>
				<param name="fm_del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
