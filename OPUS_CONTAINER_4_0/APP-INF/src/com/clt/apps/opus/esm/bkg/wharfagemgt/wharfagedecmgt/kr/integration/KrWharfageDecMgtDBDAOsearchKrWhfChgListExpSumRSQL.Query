<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOsearchKrWhfChgListExpSumRSQL">
			<desc><![CDATA[S]]></desc>
			<sql><![CDATA[
SELECT '20''' AS GUBUN,
       SUM(CASE WHEN A.CNTR_SZ_CD = '2' AND A.BKG_RT_WHF_EXPT_CD IS NULL THEN A.RAT_AS_QTY_SUM
                ELSE 0
           END) AS BUGUADAESANG,
       SUM(DECODE(B.ATTR_CTNT2, 'HYOSUNG', A.F_TEU_QTY, 0)) AS HYOSUNG,
       SUM(DECODE(B.ATTR_CTNT2, 'DAEWOO', A.F_TEU_QTY, 0)) AS DAEWOOMot,
       SUM(DECODE(B.ATTR_CTNT2, 'DONGBU', A.F_TEU_QTY, 0)) AS DONGBUJEKANG,
       SUM(DECODE(B.ATTR_CTNT2, 'HYUNDAIH', A.F_TEU_QTY, 0)) AS HYUNDAEHI,
       SUM(DECODE(B.ATTR_CTNT2, 'DONGGUK', A.F_TEU_QTY, 0)) AS DONGGUKJEKANG,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'X', A.F_TEU_QTY, 0)) AS TS,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'I', A.F_TEU_QTY, 0)) AS IPI,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'J', A.F_TEU_QTY, 0)) AS Military,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'K', A.F_TEU_QTY, 0)) AS JODALCHEONG,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'D', A.F_TEU_QTY, 0)) AS Mty,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'B', A.F_TEU_QTY, 0)) AS Bulk,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', A.F_TEU_QTY, 0)) AS InclOFT,
       SUM(CASE WHEN A.BKG_RT_WHF_EXPT_CD IN ('S', 'X', 'I', 'J', 'K', 'D', 'B', 'N') THEN A.F_TEU_QTY
                ELSE 0
           END) AS EXEMPTTOTAL,
       SUM(A.AMOUNT) AS RatingAmount,
       SUM(A.AMOUNT) + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_TEU_QTY * UNIT_20,0 ), 0))
                     + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_FEU_QTY * UNIT_40,0 ), 0))
                     + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_HCB_QTY * UNIT_45,0 ), 0)) AS SINGOYEJEONGGUMAEK
  FROM ( SELECT A.BL_NO, A.CUST_NM, A.XPT_REF_NO,
       B.RAT_UT_CD,
       B.RAT_AS_QTY_SUM,
       DECODE(B.CURR_CD, ' ', B.CHG_AMT_SUM, B.CHG_AMT_SUM * C.LOCL_CNY_XCH_RT) AS AMOUNT,
       D.TEU_QTY, D.FEU_QTY, D.HCB_QTY,
       A.BL_CVRD_TP_CD,
       A.BKG_RT_WHF_EXPT_CD,
       F.ATTR_CTNT4,
       A.WHF_SHPR_RGST_NO,
       A.BL_TP_CD,
       B.CNTR_SZ_CD,
       DECODE(D.TEU_QTY, 0, 0, E.TEU_QTY) AS F_TEU_QTY,
       DECODE(D.FEU_QTY, 0, 0, E.FEU_QTY) AS F_FEU_QTY,
       DECODE(D.HCB_QTY, 0, 0, E.HCB_QTY) AS F_HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, ( SELECT A.BKG_NO, B.RAT_UT_CD, B.CURR_CD, TO_CHAR(B.CRE_DT, 'YYYYMM') AS CRE_DT,
       DECODE(C.RAT_UT_GRP_CD, 'E', C.CNTR_SZ_CD, C.RAT_UT_GRP_CD) AS CNTR_SZ_CD,
       SUM(B.CHG_AMT) AS CHG_AMT_SUM, SUM(B.RAT_AS_QTY) AS RAT_AS_QTY_SUM
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_CHG_RT B, PRI_RAT_UT C
 WHERE B.BKG_NO = A.BKG_NO
   AND B.CHG_CD = 'WHF'
   AND C.RAT_UT_CD(+) = B.RAT_UT_CD
 GROUP BY A.BKG_NO, B.RAT_UT_CD, B.CURR_CD, TO_CHAR(B.CRE_DT, 'YYYYMM'),
       DECODE(C.RAT_UT_GRP_CD, 'E', C.CNTR_SZ_CD, C.RAT_UT_GRP_CD) ) B, GL_MON_XCH_RT C, ( SELECT A.BKG_NO,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) =  '2' THEN OP_CNTR_QTY ELSE 0 END), 0) AS TEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('3', '4', '5') THEN OP_CNTR_QTY ELSE 0 END), 0) AS FEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('2', '3', '4', '5') THEN 0 ELSE OP_CNTR_QTY END), 0) AS HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_QUANTITY B
 WHERE B.BKG_NO = A.BKG_NO
   AND B.CNTR_TPSZ_CD NOT LIKE 'Q%'
 GROUP BY A.BKG_NO ) D, ( SELECT A.BKG_NO,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) =  '2' THEN CNTR_VOL_QTY ELSE 0 END), 0) AS TEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('3', '4', '5') THEN CNTR_VOL_QTY ELSE 0 END), 0) AS FEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('2', '3', '4', '5') THEN 0 ELSE CNTR_VOL_QTY END), 0) AS HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_CONTAINER B
 WHERE B.BKG_NO = A.BKG_NO
   AND (NVL(A.BKG_RT_WHF_EXPT_CD, 'N') <> 'B' OR
        (A.BKG_RT_WHF_EXPT_CD = 'B' AND B.RCV_TERM_CD IN ('I', 'T')))
 GROUP BY A.BKG_NO ) E,
       BKG_HRD_CDG_CTNT F
 WHERE B.BKG_NO(+) = A.BKG_NO
   AND C.ACCT_XCH_RT_YRMON(+) = B.CRE_DT
   AND C.ACCT_XCH_RT_LVL(+) = '1'
   AND C.CURR_CD(+) = B.CURR_CD
   AND D.BKG_NO(+) = A.BKG_NO
   AND E.BKG_NO(+) = A.BKG_NO
   AND F.HRD_CDG_ID(+) = 'KR_WHF_EXEMPT_CD'
   AND F.ATTR_CTNT1(+) = A.BKG_RT_WHF_EXPT_CD
#if (${bkg_sts_cd} == 'N')
   AND A.BKG_RT_WHF_EXPT_CD = 'N'
#end
#if (${bkg_sts_cd} == 'Y')
   AND A.BKG_RT_WHF_EXPT_CD IS NOT NULL
#end
#if (${bkg_sts_cd} == 'X')
   AND A.BKG_RT_WHF_EXPT_CD IS NULL
#end ) A, BKG_HRD_CDG_CTNT B, ( SELECT DC_RTO_NO, TEU_PRC * (1 - TEU_AMT_RT) AS UNIT_20,
       FEU_PRC * (1 - FEU_AMT_RT) AS UNIT_40,
       HC_PRC * (1 - HC_AMT_RT)  AS UNIT_45
  FROM BKG_KR_WHF_PORT_RT
 WHERE CNTR_BLK_DIV_CD = 'C'
   AND PORT_CD = DECODE( @[bkg_cust_tp_cd], 'S', @[pol_cd], -- OUTBOUND 이면 :polCd
                                                 @[pod_cd])   -- INBOUND 이면 :podCd
   AND IO_BND_CD = DECODE(@[bkg_cust_tp_cd], 'S', 'O',  -- OUTBOUND 이면 'O'
                                                  'I')  -- INBOUND 이면 'I'
   AND DC_RTO_NO = NVL((SELECT	WHF_VOL_DC_CD FROM BKG_KR_WHF_VOL
                        WHERE	VSL_CD = SUBSTR(@[vvd],1,4)
			AND	SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
			AND	SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
			AND	PORT_CD	   = DECODE( @[bkg_cust_tp_cd], 'S', @[pol_cd], @[pod_cd])
			AND	WHF_BND_CD = DECODE(@[bkg_cust_tp_cd], 'S', 'OO', 'II')), 0)
   AND ROWNUM = 1 ) C
 WHERE B.HRD_CDG_ID(+) = 'KR_WHF_EXEMPT_CUST'
   AND B.ATTR_CTNT1(+) = A.WHF_SHPR_RGST_NO
UNION ALL
SELECT '40''' AS GUBUN,
       SUM(CASE WHEN A.CNTR_SZ_CD = '4' AND A.BKG_RT_WHF_EXPT_CD IS NULL THEN A.RAT_AS_QTY_SUM
                ELSE 0
           END) AS BUGUADAESANG,
       SUM(DECODE(B.ATTR_CTNT2, 'HYOSUNG', A.F_FEU_QTY, 0)) AS HYOSUNG,
       SUM(DECODE(B.ATTR_CTNT2, 'DAEWOO', A.F_FEU_QTY, 0)) AS DAEWOOMot,
       SUM(DECODE(B.ATTR_CTNT2, 'DONGBU', A.F_FEU_QTY, 0)) AS DONGBUJEKANG,
       SUM(DECODE(B.ATTR_CTNT2, 'HYUNDAIH', A.F_FEU_QTY, 0)) AS HYUNDAEHI,
       SUM(DECODE(B.ATTR_CTNT2, 'DONGGUK', A.F_FEU_QTY, 0)) AS DONGGUKJEKANG,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'X', A.F_FEU_QTY, 0)) AS TS,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'I', A.F_FEU_QTY, 0)) AS IPI,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'J', A.F_FEU_QTY, 0)) AS Military,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'K', A.F_FEU_QTY, 0)) AS JODALCHEONG,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'D', A.F_FEU_QTY, 0)) AS Mty,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'B', A.F_FEU_QTY, 0)) AS Bulk,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', A.F_FEU_QTY, 0)) AS InclOFT,
       SUM(CASE WHEN A.BKG_RT_WHF_EXPT_CD IN ('S', 'X', 'I', 'J', 'K', 'D', 'B', 'N') THEN A.F_FEU_QTY
                ELSE 0
           END) AS EXEMPTTOTAL,
       SUM(A.AMOUNT) AS RatingAmount,
       SUM(A.AMOUNT) + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_TEU_QTY * UNIT_20,0 ), 0))
                     + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_FEU_QTY * UNIT_40,0 ), 0))
                     + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_HCB_QTY * UNIT_45,0 ), 0)) AS SINGOYEJEONGGUMAEK
  FROM ( SELECT A.BL_NO, A.CUST_NM, A.XPT_REF_NO,
       B.RAT_UT_CD,
       B.RAT_AS_QTY_SUM,
       DECODE(B.CURR_CD, ' ', B.CHG_AMT_SUM, B.CHG_AMT_SUM * C.LOCL_CNY_XCH_RT) AS AMOUNT,
       D.TEU_QTY, D.FEU_QTY, D.HCB_QTY,
       A.BL_CVRD_TP_CD,
       A.BKG_RT_WHF_EXPT_CD,
       F.ATTR_CTNT4,
       A.WHF_SHPR_RGST_NO,
       A.BL_TP_CD,
       B.CNTR_SZ_CD,
       DECODE(D.TEU_QTY, 0, 0, E.TEU_QTY) AS F_TEU_QTY,
       DECODE(D.FEU_QTY, 0, 0, E.FEU_QTY) AS F_FEU_QTY,
       DECODE(D.HCB_QTY, 0, 0, E.HCB_QTY) AS F_HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, ( SELECT A.BKG_NO, B.RAT_UT_CD, B.CURR_CD, TO_CHAR(B.CRE_DT, 'YYYYMM') AS CRE_DT,
       DECODE(C.RAT_UT_GRP_CD, 'E', C.CNTR_SZ_CD, C.RAT_UT_GRP_CD) AS CNTR_SZ_CD,
       SUM(B.CHG_AMT) AS CHG_AMT_SUM, SUM(B.RAT_AS_QTY) AS RAT_AS_QTY_SUM
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_CHG_RT B, PRI_RAT_UT C
 WHERE B.BKG_NO = A.BKG_NO
   AND B.CHG_CD = 'WHF'
   AND C.RAT_UT_CD(+) = B.RAT_UT_CD
 GROUP BY A.BKG_NO, B.RAT_UT_CD, B.CURR_CD, TO_CHAR(B.CRE_DT, 'YYYYMM'),
       DECODE(C.RAT_UT_GRP_CD, 'E', C.CNTR_SZ_CD, C.RAT_UT_GRP_CD) ) B, GL_MON_XCH_RT C, ( SELECT A.BKG_NO,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) =  '2' THEN OP_CNTR_QTY ELSE 0 END), 0) AS TEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('3', '4', '5') THEN OP_CNTR_QTY ELSE 0 END), 0) AS FEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('2', '3', '4', '5') THEN 0 ELSE OP_CNTR_QTY END), 0) AS HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_QUANTITY B
 WHERE B.BKG_NO = A.BKG_NO
   AND B.CNTR_TPSZ_CD NOT LIKE 'Q%'
 GROUP BY A.BKG_NO ) D, ( SELECT A.BKG_NO,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) =  '2' THEN CNTR_VOL_QTY ELSE 0 END), 0) AS TEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('3', '4', '5') THEN CNTR_VOL_QTY ELSE 0 END), 0) AS FEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('2', '3', '4', '5') THEN 0 ELSE CNTR_VOL_QTY END), 0) AS HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_CONTAINER B
 WHERE B.BKG_NO = A.BKG_NO
   AND (NVL(A.BKG_RT_WHF_EXPT_CD, 'N') <> 'B' OR
        (A.BKG_RT_WHF_EXPT_CD = 'B' AND B.RCV_TERM_CD IN ('I', 'T')))
 GROUP BY A.BKG_NO ) E,
       BKG_HRD_CDG_CTNT F
 WHERE B.BKG_NO(+) = A.BKG_NO
   AND C.ACCT_XCH_RT_YRMON(+) = B.CRE_DT
   AND C.ACCT_XCH_RT_LVL(+) = '1'
   AND C.CURR_CD(+) = B.CURR_CD
   AND D.BKG_NO(+) = A.BKG_NO
   AND E.BKG_NO(+) = A.BKG_NO
   AND F.HRD_CDG_ID(+) = 'KR_WHF_EXEMPT_CD'
   AND F.ATTR_CTNT1(+) = A.BKG_RT_WHF_EXPT_CD
#if (${bkg_sts_cd} == 'N')
   AND A.BKG_RT_WHF_EXPT_CD = 'N'
#end
#if (${bkg_sts_cd} == 'Y')
   AND A.BKG_RT_WHF_EXPT_CD IS NOT NULL
#end
#if (${bkg_sts_cd} == 'X')
   AND A.BKG_RT_WHF_EXPT_CD IS NULL
#end ) A, BKG_HRD_CDG_CTNT B, ( SELECT DC_RTO_NO, TEU_PRC * (1 - TEU_AMT_RT) AS UNIT_20,
       FEU_PRC * (1 - FEU_AMT_RT) AS UNIT_40,
       HC_PRC * (1 - HC_AMT_RT)  AS UNIT_45
  FROM BKG_KR_WHF_PORT_RT
 WHERE CNTR_BLK_DIV_CD = 'C'
   AND PORT_CD = DECODE( @[bkg_cust_tp_cd], 'S', @[pol_cd], -- OUTBOUND 이면 :polCd
                                                 @[pod_cd])   -- INBOUND 이면 :podCd
   AND IO_BND_CD = DECODE(@[bkg_cust_tp_cd], 'S', 'O',  -- OUTBOUND 이면 'O'
                                                  'I')  -- INBOUND 이면 'I'
    AND DC_RTO_NO = NVL((SELECT	WHF_VOL_DC_CD FROM BKG_KR_WHF_VOL
                        WHERE	VSL_CD = SUBSTR(@[vvd],1,4)
			AND	SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
			AND	SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
			AND	PORT_CD	   = DECODE( @[bkg_cust_tp_cd], 'S', @[pol_cd], @[pod_cd])
			AND	WHF_BND_CD = DECODE(@[bkg_cust_tp_cd], 'S', 'OO', 'II')), 0)
   AND ROWNUM = 1 ) C
 WHERE B.HRD_CDG_ID(+) = 'KR_WHF_EXEMPT_CUST'
   AND B.ATTR_CTNT1(+) = A.WHF_SHPR_RGST_NO
UNION ALL
SELECT '45''' AS GUBUN,
       SUM(CASE WHEN A.CNTR_SZ_CD = '7' AND A.BKG_RT_WHF_EXPT_CD IS NULL THEN A.RAT_AS_QTY_SUM
                ELSE 0
           END) AS BUGUADAESANG,
       SUM(DECODE(B.ATTR_CTNT2, 'HYOSUNG', A.F_HCB_QTY, 0)) AS HYOSUNG,
       SUM(DECODE(B.ATTR_CTNT2, 'DAEWOO', A.F_HCB_QTY, 0)) AS DAEWOOMot,
       SUM(DECODE(B.ATTR_CTNT2, 'DONGBU', A.F_HCB_QTY, 0)) AS DONGBUJEKANG,
       SUM(DECODE(B.ATTR_CTNT2, 'HYUNDAIH', A.F_HCB_QTY, 0)) AS HYUNDAEHI,
       SUM(DECODE(B.ATTR_CTNT2, 'DONGGUK', A.F_HCB_QTY, 0)) AS DONGGUKJEKANG,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'X', A.F_HCB_QTY, 0)) AS TS,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'I', A.F_HCB_QTY, 0)) AS IPI,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'J', A.F_HCB_QTY, 0)) AS Military,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'K', A.F_HCB_QTY, 0)) AS JODALCHEONG,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'D', A.F_HCB_QTY, 0)) AS Mty,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'B', A.F_HCB_QTY, 0)) AS Bulk,
       SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', A.F_HCB_QTY, 0)) AS InclOFT,
       SUM(CASE WHEN A.BKG_RT_WHF_EXPT_CD IN ('S', 'X', 'I', 'J', 'K', 'D', 'B', 'N') THEN A.F_HCB_QTY
                ELSE 0
           END) AS EXEMPTTOTAL,
       SUM(A.AMOUNT) AS RatingAmount,
       SUM(A.AMOUNT) + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_TEU_QTY * UNIT_20,0 ), 0))
                     + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_FEU_QTY * UNIT_40,0 ), 0))
                     + SUM(DECODE(A.BKG_RT_WHF_EXPT_CD, 'N', DECODE( NVL(A.AMOUNT,0), 0, A.F_HCB_QTY * UNIT_45,0 ), 0)) AS SINGOYEJEONGGUMAEK
  FROM ( SELECT A.BL_NO, A.CUST_NM, A.XPT_REF_NO,
       B.RAT_UT_CD,
       B.RAT_AS_QTY_SUM,
       DECODE(B.CURR_CD, ' ', B.CHG_AMT_SUM, B.CHG_AMT_SUM * C.LOCL_CNY_XCH_RT) AS AMOUNT,
       D.TEU_QTY, D.FEU_QTY, D.HCB_QTY,
       A.BL_CVRD_TP_CD,
       A.BKG_RT_WHF_EXPT_CD,
       F.ATTR_CTNT4,
       A.WHF_SHPR_RGST_NO,
       A.BL_TP_CD,
       B.CNTR_SZ_CD,
       DECODE(D.TEU_QTY, 0, 0, E.TEU_QTY) AS F_TEU_QTY,
       DECODE(D.FEU_QTY, 0, 0, E.FEU_QTY) AS F_FEU_QTY,
       DECODE(D.HCB_QTY, 0, 0, E.HCB_QTY) AS F_HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, ( SELECT A.BKG_NO, B.RAT_UT_CD, B.CURR_CD, TO_CHAR(B.CRE_DT, 'YYYYMM') AS CRE_DT,
       DECODE(C.RAT_UT_GRP_CD, 'E', C.CNTR_SZ_CD, C.RAT_UT_GRP_CD) AS CNTR_SZ_CD,
       SUM(B.CHG_AMT) AS CHG_AMT_SUM, SUM(B.RAT_AS_QTY) AS RAT_AS_QTY_SUM
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_CHG_RT B, PRI_RAT_UT C
 WHERE B.BKG_NO = A.BKG_NO
   AND B.CHG_CD = 'WHF'
   AND C.RAT_UT_CD(+) = B.RAT_UT_CD
 GROUP BY A.BKG_NO, B.RAT_UT_CD, B.CURR_CD, TO_CHAR(B.CRE_DT, 'YYYYMM'),
       DECODE(C.RAT_UT_GRP_CD, 'E', C.CNTR_SZ_CD, C.RAT_UT_GRP_CD) ) B, GL_MON_XCH_RT C, ( SELECT A.BKG_NO,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) =  '2' THEN OP_CNTR_QTY ELSE 0 END), 0) AS TEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('3', '4', '5') THEN OP_CNTR_QTY ELSE 0 END), 0) AS FEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('2', '3', '4', '5') THEN 0 ELSE OP_CNTR_QTY END), 0) AS HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_QUANTITY B
 WHERE B.BKG_NO = A.BKG_NO
   AND B.CNTR_TPSZ_CD NOT LIKE 'Q%'
 GROUP BY A.BKG_NO ) D, ( SELECT A.BKG_NO,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) =  '2' THEN CNTR_VOL_QTY ELSE 0 END), 0) AS TEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('3', '4', '5') THEN CNTR_VOL_QTY ELSE 0 END), 0) AS FEU_QTY,
       NVL(SUM(CASE WHEN SUBSTR(B.CNTR_TPSZ_CD, 2, 1) IN ('2', '3', '4', '5') THEN 0 ELSE CNTR_VOL_QTY END), 0) AS HCB_QTY
  FROM ( SELECT B.BKG_NO, B.BL_NO, B.BL_TP_CD, E.CUST_NM AS CUST_NM, F.CUST_NM AS XPT_REF_NO, D.BKG_RT_WHF_EXPT_CD, D.WHF_SHPR_RGST_NO, C.BL_CVRD_TP_CD
  FROM BKG_VVD A, BKG_BOOKING B, BKG_BL_DOC C, BKG_RATE D, BKG_CUSTOMER E, BKG_CUSTOMER F
WHERE A.VSL_CD = SUBSTR( @[vvd],1,4 )
   AND A.SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
   AND A.SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
#if (${pol_cd} != '') 
   AND A.POL_CD LIKE @[pol_cd] || '%' -- POL 조건
#end	
#if (${pod_cd} != '') 
   AND A.POD_CD LIKE @[pod_cd] || '%' -- POD 조건
	AND B.POD_CD = A.POD_CD -- POD 조건
#end
   
   AND B.BKG_NO = A.BKG_NO
   AND B.POL_CD = A.POL_CD
   AND B.BKG_STS_CD NOT IN ('X', 'A')
   AND C.BKG_NO(+) = B.BKG_NO
   AND D.BKG_NO(+) = B.BKG_NO
   AND E.BKG_NO(+) = B.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = @[bkg_cust_tp_cd]
   AND F.BKG_NO(+) = B.BKG_NO
   AND F.BKG_CUST_TP_CD(+) = 'E' ) A, BKG_CONTAINER B
 WHERE B.BKG_NO = A.BKG_NO
   AND (NVL(A.BKG_RT_WHF_EXPT_CD, 'N') <> 'B' OR
        (A.BKG_RT_WHF_EXPT_CD = 'B' AND B.RCV_TERM_CD IN ('I', 'T')))
 GROUP BY A.BKG_NO ) E,
       BKG_HRD_CDG_CTNT F
 WHERE B.BKG_NO(+) = A.BKG_NO
   AND C.ACCT_XCH_RT_YRMON(+) = B.CRE_DT
   AND C.ACCT_XCH_RT_LVL(+) = '1'
   AND C.CURR_CD(+) = B.CURR_CD
   AND D.BKG_NO(+) = A.BKG_NO
   AND E.BKG_NO(+) = A.BKG_NO
   AND F.HRD_CDG_ID(+) = 'KR_WHF_EXEMPT_CD'
   AND F.ATTR_CTNT1(+) = A.BKG_RT_WHF_EXPT_CD
#if (${bkg_sts_cd} == 'N')
   AND A.BKG_RT_WHF_EXPT_CD = 'N'
#end
#if (${bkg_sts_cd} == 'Y')
   AND A.BKG_RT_WHF_EXPT_CD IS NOT NULL
#end
#if (${bkg_sts_cd} == 'X')
   AND A.BKG_RT_WHF_EXPT_CD IS NULL
#end ) A, BKG_HRD_CDG_CTNT B, ( SELECT DC_RTO_NO, TEU_PRC * (1 - TEU_AMT_RT) AS UNIT_20,
       FEU_PRC * (1 - FEU_AMT_RT) AS UNIT_40,
       HC_PRC * (1 - HC_AMT_RT)  AS UNIT_45
  FROM BKG_KR_WHF_PORT_RT
 WHERE CNTR_BLK_DIV_CD = 'C'
   AND PORT_CD = DECODE( @[bkg_cust_tp_cd], 'S', @[pol_cd], -- OUTBOUND 이면 :polCd
                                                 @[pod_cd])   -- INBOUND 이면 :podCd
   AND IO_BND_CD = DECODE(@[bkg_cust_tp_cd], 'S', 'O',  -- OUTBOUND 이면 'O'
                                                  'I')  -- INBOUND 이면 'I'
    AND DC_RTO_NO = NVL((SELECT	WHF_VOL_DC_CD FROM BKG_KR_WHF_VOL
                        WHERE	VSL_CD = SUBSTR(@[vvd],1,4)
			AND	SKD_VOY_NO = SUBSTR( @[vvd],5,4 )
			AND	SKD_DIR_CD = SUBSTR( @[vvd],9,1 )
			AND	PORT_CD	   = DECODE( @[bkg_cust_tp_cd], 'S', @[pol_cd], @[pod_cd])
			AND	WHF_BND_CD = DECODE(@[bkg_cust_tp_cd], 'S', 'OO', 'II')), 0)
   AND ROWNUM = 1 ) C
 WHERE B.HRD_CDG_ID(+) = 'KR_WHF_EXEMPT_CUST'
   AND B.ATTR_CTNT1(+) = A.WHF_SHPR_RGST_NO			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
