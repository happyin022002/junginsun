<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyReleaseOrderDBDAOsearchBkgLodFctrDlLogDtlRSQL">
			<desc><![CDATA[EmptyReleaseOrderDBDAOsearchBkgLodFctrDlLogDtlRSQL]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(LDF_DL_DT, 'YYYY-MM-DD HH24:MI:SS') AS LDF_DL_DT
     , BKG_OFC_CD
     , BKG_NO
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
 FROM BKG_LOD_FCTR_DL_LOG_DTL
WHERE 1 = 1
 #if (${ldf_dl_dt} != '')
  AND LDF_DL_DT = TO_DATE(@[ldf_dl_dt], 'YYYY-MM-DD HH24:MI:SS')
 #end
 #if (${bkg_ofc_cd} != '')
  AND BKG_OFC_CD = @[bkg_ofc_cd]
 #end			]]></sql>
			<params>
				<param name="ldf_dl_dt" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
