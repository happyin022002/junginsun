<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCustIsf5InfoRSQL">
			<desc><![CDATA[searchCustIsf5Info]]></desc>
			<sql><![CDATA[
SELECT A.BL_NO
      ,RPAD('N00' ||'BKP' || RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_NM),1, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS BKP_N00
      ,RPAD('N02' || RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_ADDR),1, ''), 'X')),'.'),35,' ')||
                     RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_ADDR),2, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS BKP_N02
      ,RPAD('N03' || RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(A.CUST_ADDR),3, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS BKP_N03
      ,RPAD('N00' ||'ST ' || RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_NM),1, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS ST_N00
      ,RPAD('N02' || RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_ADDR),1, ''), 'X')),'.'),35,' ')||
                     RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_ADDR),2, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS ST_N02
      ,RPAD('N03' || RPAD(NVL(RTRIM(BKG_SPCLCHAR_CONV_FNC(BKG_TOKEN_NL_FNC(RTRIM(B.CUST_ADDR),3, ''), 'X')),'.'),35,' '),80,' ') || CHR(10) AS ST_N03
  FROM BKG_CSTMS_ADV_CUST A
      ,BKG_CSTMS_ADV_CUST B
      ,BKG_CSTMS_ADV_BL   BB
      ,BKG_BOOKING        BK
 WHERE 1=1
   AND A.CNT_CD         = B.CNT_CD
   AND A.BL_NO          = B.BL_NO
   AND B.CNT_CD         = BB.CNT_CD
   AND B.BL_NO          = BB.BL_NO
   AND BB.BKG_NO        = BK.BKG_NO
   AND A.CNT_CD         = 'US'
   AND A.BKG_CUST_TP_CD = 'S'
   AND B.BKG_CUST_TP_CD = DECODE(BK.CUST_TO_ORD_FLG, 'Y', 'N', 'C')
   AND A.BL_NO          = 'BAS500001100'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
