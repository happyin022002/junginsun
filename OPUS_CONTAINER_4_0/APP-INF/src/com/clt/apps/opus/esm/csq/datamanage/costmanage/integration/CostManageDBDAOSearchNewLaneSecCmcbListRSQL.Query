<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostManageDBDAOSearchNewLaneSecCmcbListRSQL">
			<desc><![CDATA[New Lane Sector CMCB List 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT A1.BSE_TP_CD
       ,A1.BSE_YR
       ,A1.BSE_QTR_CD
       ,A1.RLANE_CD
       ,A1.DIR_CD
       ,A1.TRD_CD
       ,A1.SUB_TRD_CD
       ,A1.SRC_RLANE_CD
       ,DECODE(NVL(A2.RLANE_CD, 'N'), 'N', 'N', 'Y') CRE_FLG
FROM CSQ_SCTR_NEW_LANE A1, CSQ_SCTR_NEW_PAIR_COST A2
WHERE 1=1
AND A1.BSE_TP_CD = @[f_bse_tp_cd]
AND A1.BSE_YR = @[f_bse_yr]
AND A1.BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
AND A1.BSE_TP_CD = A2.BSE_TP_CD(+)
AND A1.BSE_YR = A2.BSE_YR(+)
AND A1.BSE_QTR_CD = A2.BSE_QTR_CD(+)
AND A1.RLANE_CD = A2.RLANE_CD(+)
AND A1.DIR_CD = A2.DIR_CD(+)
AND A1.SUB_TRD_CD = A2.SUB_TRD_CD(+)
ORDER BY A1.BSE_TP_CD , A1.BSE_YR, A1.BSE_QTR_CD, A1.SUB_TRD_CD, A1.RLANE_CD, A1.DIR_CD, A1.SRC_RLANE_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
