<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOBlIssueDtHistoryUSQL">
			<desc><![CDATA[BlIssueDtHistory]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_ISS_HIS
	SET OBL_ISS_DT = TO_DATE(NVL(@[obl_iss_dt_sd],@[obl_iss_dt]), 'YYYY-MM-DD')
       ,OBL_ISS_OFC_CD = DECODE(NVL(@[obl_iss_dt_sd], @[obl_iss_dt]), 'N', NULL, @[obl_iss_ofc_cd])
	   ,OBL_ISS_USR_ID = DECODE(NVL(@[obl_iss_dt_sd], @[obl_iss_dt]), 'N', NULL, @[obl_iss_usr_id])
	   ,OBL_ISS_FLG = NVL(@[obl_iss_flg], 'N')
	   ,UPD_USR_ID = nvl(@[obl_iss_usr_id],@[upd_usr_id])
	   ,UPD_DT = SYSDATE
	   ,ORG_PPD_RCV_CD = NVL(DECODE(NVL(@[credit_chk], 'N'), 'Y', 'C', ''), ORG_PPD_RCV_CD)
	   ,ORG_PPD_RCV_UPD_OFC_CD = NVL(DECODE(NVL(@[credit_chk], 'N'), 'N', NULL, @[obl_iss_ofc_cd]), ORG_PPD_RCV_UPD_OFC_CD)
	   ,ORG_PPD_RCV_UPD_USR_ID = NVL(DECODE(NVL(@[credit_chk], 'N'), 'N', NULL, @[obl_iss_usr_id]), ORG_PPD_RCV_UPD_USR_ID)
	   ,ORG_PPD_RCV_UPD_DT = NVL(DECODE(NVL(@[credit_chk], 'N'), 'N', NULL, sysdate), ORG_PPD_RCV_UPD_DT)
	   ,BL_ISS_TP_CD = NVL(@[bl_iss_tp_cd],'B')
WHERE BKG_NO = @[bkg_no]
AND CORR_NO = 'TMP0000001'
			]]></sql>
			<params>
				<param name="obl_iss_dt_sd" type="12" value="" out="N"/>
				<param name="obl_iss_dt" type="12" value="" out="N"/>
				<param name="obl_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="obl_iss_usr_id" type="12" value="" out="N"/>
				<param name="obl_iss_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="credit_chk" type="12" value="" out="N"/>
				<param name="bl_iss_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
