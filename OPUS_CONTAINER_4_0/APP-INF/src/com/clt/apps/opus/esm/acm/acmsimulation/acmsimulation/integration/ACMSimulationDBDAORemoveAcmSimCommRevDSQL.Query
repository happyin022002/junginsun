<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAORemoveAcmSimCommRevDSQL">
			<desc><![CDATA[RemoveAcmSimCommRev]]></desc>
			<sql><![CDATA[
DELETE ACM_SIM_COMM_REV
WHERE 1=1 
AND BKG_NO = @[bkg_no]
AND SIM_NO = @[sim_no]
AND (BKG_NO,AGN_CD,IO_BND_CD,AC_TP_CD,AC_SEQ) NOT IN 
    (
        SELECT BKG_NO,AGN_CD,IO_BND_CD,AC_TP_CD,AC_SEQ
        FROM   ACM_AGN_COMM
        WHERE  BKG_NO = @[bkg_no]
		AND	   SIM_NO = @[sim_no]
    )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
