<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOSelectCompareBkgRevUmchItmDtlRSQL">
			<desc><![CDATA[selectCompareBkgRevUmchItmDtl]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM(

SELECT * 
FROM (
        ( 
          SELECT UMCH_TP_CD, UMCH_BKG_CTRT_TP_CD, UMCH_ITM_SEQ, CHG_CD, RAT_UT_CD, CURR_CD
          FROM   BKG_REV_UMCH_ITM_DTL 
          WHERE  BKG_NO = @[bkg_no] 
          AND    UMCH_BKG_SEQ = TO_NUMBER(@[max_umch_bkg_seq])
        )
         MINUS
        ( 
          SELECT UMCH_TP_CD, UMCH_BKG_CTRT_TP_CD, UMCH_ITM_SEQ, CHG_CD, RAT_UT_CD, CURR_CD
          FROM   BKG_REV_UMCH_ITM_DTL 
          WHERE  BKG_NO = @[bkg_no] 
          AND    UMCH_BKG_SEQ = TO_NUMBER(@[max_umch_bkg_seq]) - 1
        )
     )

UNION ALL

SELECT * 
FROM (
        ( 
          SELECT UMCH_TP_CD, UMCH_BKG_CTRT_TP_CD, UMCH_ITM_SEQ, CHG_CD, RAT_UT_CD, CURR_CD
          FROM   BKG_REV_UMCH_ITM_DTL 
          WHERE  BKG_NO = @[bkg_no] 
          AND    UMCH_BKG_SEQ = TO_NUMBER(@[max_umch_bkg_seq]) - 1
        )
         MINUS
        ( 
          SELECT UMCH_TP_CD, UMCH_BKG_CTRT_TP_CD, UMCH_ITM_SEQ, CHG_CD, RAT_UT_CD, CURR_CD
          FROM   BKG_REV_UMCH_ITM_DTL 
          WHERE  BKG_NO = @[bkg_no] 
          AND    UMCH_BKG_SEQ = TO_NUMBER(@[max_umch_bkg_seq])
        )
     )

)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="max_umch_bkg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
