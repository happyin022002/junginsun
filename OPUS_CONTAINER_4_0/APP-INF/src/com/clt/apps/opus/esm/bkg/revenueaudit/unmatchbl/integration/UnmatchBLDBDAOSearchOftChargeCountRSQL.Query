<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOSearchOftChargeCountRSQL">
			<desc><![CDATA[UnmatchBLDBDAOSearchOftChargeCountRSQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(DISTINCT CHG_CD) CNT
FROM BKG_CHG_RT
WHERE BKG_NO = @[bkg_no]
AND CHG_CD IN ('OFT', 'OAR', 'OIH', 'DAR', 'DIH')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
