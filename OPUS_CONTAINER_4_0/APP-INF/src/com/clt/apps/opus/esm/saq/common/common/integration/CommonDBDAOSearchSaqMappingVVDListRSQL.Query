<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSaqMappingVVDListRSQL">
			<desc><![CDATA[SAQ_MON_CFM_TGT_VVD의 Lane List 조회 ]]></desc>
			<sql><![CDATA[
SELECT DISTINCT RLANE_CD AS TEXT,  
               RLANE_CD AS CODE   
 FROM SAQ_MON_CFM_TGT_VVD         
WHERE MQTA_RLSE_VER_NO = @[rlse_ver_no]        
  AND BSE_YR           = @[year]        
  AND BSE_QTR_CD       = @[qtr_cd]        
  AND TRD_CD           = @[trd_cd]        
  AND DIR_CD           = @[dir_cd]        
#if (${sub_trd_cd} != '' ) 
	AND SUB_TRD_CD       = @[sub_trd_cd]        
#end

#if (${lane_cd} != '' ) 
	AND RLANE_CD       = @[lane_cd]        
#end
ORDER BY RLANE_CD			]]></sql>
			<params>
				<param name="rlse_ver_no" type="12" value="" out="N"/>
				<param name="year" type="12" value="" out="N"/>
				<param name="qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
