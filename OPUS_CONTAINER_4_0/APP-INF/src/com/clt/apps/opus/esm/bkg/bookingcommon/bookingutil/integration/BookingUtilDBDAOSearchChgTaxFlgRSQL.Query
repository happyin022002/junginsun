<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchChgTaxFlgRSQL">
			<desc><![CDATA[MDM_CHARGE 의 tax_flg 조회]]></desc>
			<sql><![CDATA[
SELECT CHG_CD, CHG_NM, TAX_FLG, TAX_CNT_CD
FROM MDM_CHARGE
WHERE CHG_CD = @[chg_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
