<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOaddJpcusBlCSQL">
			<desc><![CDATA[addJpcusBl]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_JP_BL

       (CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT,
        BL_NO,
        BL_SPLIT_NO,
        VSL_CD,
        SKD_VOY_NO,
        SKD_DIR_CD,
        POL_CD,
        POD_CD,
        BKG_POR_CD,
        BKG_POL_CD,
        BKG_DEL_CD,
        PST_RLY_POD_CD,
        PST_VSL_CD,
        PST_SKD_VOY_NO,
        PST_SKD_DIR_CD,
        PCK_QTY,
        PCK_TP_CD,
        GRS_WGT,
        WGT_UT_CD,
        MEAS_QTY,
        MEAS_UT_CD,
        RCV_TERM_CD,
        DE_TERM_CD,
        DCGO_FLG,
        BDR_FLG,
        BDR_DT,
        CA_DT,
        CA_NO,
        JP_BL_STS_CD,
        IF_DT,
        LOCL_TS_IND_CD,
        JP_CSTMS_TRNS_CD,
        FULL_MTY_CD,
        SPLIT_FLG,
        LMT_NO,
        PRE_RLY_PORT_CD,
        PRE_VSL_CD,
        PRE_SKD_VOY_NO,
        PRE_SKD_DIR_CD
        )

VALUES (@[cre_usr_id],
        SYSDATE,
        @[cre_usr_id],
        SYSDATE,
        @[bl_no],
        DECODE(@[bl_split_no], '  ', '  ', LPAD(TO_CHAR(NVL((@[bl_split_no]), 0)), 2, 0)),
        @[vsl_cd],
        @[skd_voy_no],
        @[skd_dir_cd],
        @[pol_cd],
        @[pod_cd],
        @[bkg_por_cd],
        @[bkg_pol_cd],
        @[bkg_del_cd],
        @[pst_rly_pod_cd],
        @[pst_vsl_cd],
        @[pst_skd_voy_no],
        @[pst_skd_dir_cd],
        REPLACE(NVL(@[pck_qty], 0), ',', ''),
        @[pck_tp_cd],
        REPLACE(NVL(@[grs_wgt], 0), ',', ''),
        @[wgt_ut_cd],
        REPLACE(NVL(@[meas_qty], 0), ',', ''),
        @[meas_ut_cd],
        @[rcv_term_cd],
        @[de_term_cd],
        NVL(@[dcgo_flg], 'N'),
        NVL(@[bdr_flg], 'N'),
        TO_DATE(@[bdr_dt], 'YYYYMMDDHH24MISS'),
        TO_DATE(@[ca_dt], 'YYYYMMDDHH24MISS'),
        @[ca_no],
        'A',
        SYSDATE,
        @[locl_ts_ind_cd],
        DECODE(@[locl_ts_ind_cd], 'T', 'TRS', NULL),
        NVL(@[full_mty_cd], 'F'),
        NVL(@[split_flg], 'N'),
        DECODE(@[locl_ts_ind_cd], 'T', '30'),
        @[pre_rly_port_cd],
        @[pre_vsl_cd],
        @[pre_skd_voy_no],
        @[pre_skd_dir_cd]
        )			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bkg_por_cd" type="12" value="" out="N"/>
				<param name="bkg_pol_cd" type="12" value="" out="N"/>
				<param name="bkg_del_cd" type="12" value="" out="N"/>
				<param name="pst_rly_pod_cd" type="12" value="" out="N"/>
				<param name="pst_vsl_cd" type="12" value="" out="N"/>
				<param name="pst_skd_voy_no" type="12" value="" out="N"/>
				<param name="pst_skd_dir_cd" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="grs_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="meas_ut_cd" type="12" value="" out="N"/>
				<param name="rcv_term_cd" type="12" value="" out="N"/>
				<param name="de_term_cd" type="12" value="" out="N"/>
				<param name="dcgo_flg" type="12" value="" out="N"/>
				<param name="bdr_flg" type="12" value="" out="N"/>
				<param name="bdr_dt" type="12" value="" out="N"/>
				<param name="ca_dt" type="12" value="" out="N"/>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="locl_ts_ind_cd" type="12" value="" out="N"/>
				<param name="full_mty_cd" type="12" value="" out="N"/>
				<param name="split_flg" type="12" value="" out="N"/>
				<param name="pre_rly_port_cd" type="12" value="" out="N"/>
				<param name="pre_vsl_cd" type="12" value="" out="N"/>
				<param name="pre_skd_voy_no" type="12" value="" out="N"/>
				<param name="pre_skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
