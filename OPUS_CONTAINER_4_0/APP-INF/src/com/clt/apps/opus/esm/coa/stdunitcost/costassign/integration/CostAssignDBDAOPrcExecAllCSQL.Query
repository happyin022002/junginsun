<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostAssignDBDAOPrcExecAllCSQL">
			<desc><![CDATA[비용계산용 PRC, PKG 실행]]></desc>
			<sql><![CDATA[
#if (${methodname} == 'createCoaCostPkgMainPrdAvg') 
/*
  PRD 호출시 비용계산(PKG)
  01. int createCoaCostPkgMainPrdAvg(String str)
  bkg_no, start_pctl_no, end_pctl_no, cost_yrmon
*/
CALL COA_COST_PARA_ASSIGN_PKG.MAIN_PRD_AVG('',@[start_pctl_no],@[end_pctl_no],@[cost_yrmon])

#elseif (${methodname} == 'createCoaCostPkgMainCopAvg') 
/*
  COP(배치) 호출시 비용계산(PKG)
  02. int createCoaCostPkgMainCopAvg(String str)
  bkg_no, start_pctl_no, end_pctl_no, cost_yrmon
*/
CALL COA_COST_PARA_ASSIGN_PKG.MAIN_COP_AVG(@[bkg_no],@[start_pctl_no],@[end_pctl_no],@[cost_yrmon])

#elseif (${methodname} == 'createCoaCostPkgMainPrdMst')
/*
06. int createCoaCostPkgMainPrdMst(String str)
start_pctl_no, end_pctl_no, cost_yrmon
*/
CALL COA_COST_PARA_ASSIGN_PKG.main_prd_prod_ctl_mst(@[start_pctl_no],@[end_pctl_no],@[cost_yrmon])

#elseif (${methodname} == 'createCoaCostPkgMainComTtl')
/*
07. int createCoaCostPkgMainComTtl(String str)
bkg_no, start_pctl_no, end_pctl_no, cost_yrmon
*/
CALL COA_COST_PARA_ASSIGN_PKG.MAIN_COM_TTL_PARA(@[bkg_no],@[start_pctl_no],@[end_pctl_no],@[cost_yrmon])

#elseif (${methodname} == 'createLog')
/*
08. String createLog(String errMsg)
errMsg
*/
CALL ENIS_LOG_PRC (SYSDATE, 'PRD_CALL', @[err_msg], 'ERROR')

#elseif (${methodname} == 'createCoaBkgExpnDtl')
/*
18. int createCoaBkgExpnDtl(String str) 
B, NULL, NULL, NULL, bkg_no
*/
CALL COA_BKG_EXPN_DTL_PRC( 'B','','','',@[bkg_no])

#elseif (${methodname} == 'createCoaCostPrcAssignPrd')
/* 
  PRD에서 Call한 경우 기본정보 생성
  09. int createCoaCostPrcAssignPrd(String str) 
  OracleTypes.NUMBER, start_pctl_no, end_pctl_no, user_id, cost_yrmon
*/
CALL COA_COST_ASSIGN_PRC(@[out_para_number],@[start_pctl_no],@[end_pctl_no],@[usr_id], @[cost_yrmon])

#elseif (${methodname} == 'createCoaCostPrcAssignCop')
/* 
  COP에서 Call한 경우(배치작업)
  10. int createCoaCostPrcAssignCop(String str) 
  OracleTypes.NUMBER, bkg_no, user_id
*/
CALL COA_COST_ASSIGN_COP_PRC(@[out_para_number],@[bkg_no],@[usr_id])

#elseif (${methodname} == 'createTrsAgmtApplyToCoa')
/* 
  TRS 계약 비용 계산
  13. int createTrsAgmtApplyToCoa(String str) 
  bkg_no, start_pctl_no, end_pctl_no, OracleTypes.VARCHAR
  
  2010.06.30 박은주  TRS 계약 비용요율 계산 Procedure명 변경
                    TRS_AGMT_APPLY_TO_COA_PRC -> TRS_AGMT_APLY_TO_COA_PRC
  2010.07.09 박은주  프로시져에 debug flag 추가로 소스변경
*/
CALL TRS_AGMT_APLY_TO_COA_PRC(@[bkg_no],@[start_pctl_no],@[end_pctl_no],'N',@[out_para_number])

#elseif (${methodname} == 'createTesCoaRate')
/* 
  TES 계약 비용 계산
  14. int createTesCoaRate(String str) 
  bkg_no, start_pctl_no, end_pctl_no, OracleTypes.NUMBER
*/
CALL TES_COA_RATE_PRC(@[bkg_no],@[start_pctl_no],@[end_pctl_no],@[out_para_number])

#elseif (${methodname} == 'createAcmAplyOtrCommToCoa')
/* 
  Agent commission other 계약 비용 계산
  15. int createAcmAplyOtrCommToCoa(String str, String str) 
  pctl_no, usr_id
*/
CALL ACM_APLY_OTR_COMM_TO_COA_PRC(@[pctl_no], @[usr_id])

#elseif (${methodname} == 'createAcmAplyCommToCoa')
/* 
  Agent commission 계약 비용 계산
  15. int createAcmAplyCommToCoa(String str, String str) 
  bkg_no, usr_id
*/
CALL ACM_APLY_COMM_TO_COA_PRC(@[bkg_no], @[usr_id])

#elseif (${methodname} == 'createCoaBkgInfoInst')
/* 
  BKG 정보 생성
  15. int createCoaBkgInfoInst(String str) 
  bkg_no, user_id, OracleTypes.VARCHAR
*/
CALL COA_BKG_INFO_INST_PRC(@[bkg_no], @[usr_id], @[out_para_varchar])

#elseif (${methodname} == 'createCoaBkgCostSmry')
/* 
  SANDARD COST CODE Level의 BKG 비용 생성
  16. int createCoaBkgCostSmry(String str) 
  bkg_no, user_id, del_para, OracleTypes.VARCHAR
*/
CALL  COA_BKG_COST_SMRY_PRC(@[bkg_no], @[usr_id], @[del_para],@[out_para_varchar])

#elseif (${methodname} == 'createCoaBkgRevDtl')
/*
  BKG 수입  생성
  20. int createCoaBkgRevDtl(String str)
  bkg_no, OracleTypes.VARCHAR
*/
CALL COA_BKG_REV_DTL_PRC(@[bkg_no],@[out_para_varchar])
#end			]]></sql>
			<params>
				<param name="start_pctl_no" type="12" value="" out="N"/>
				<param name="end_pctl_no" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="err_msg" type="12" value="" out="N"/>
				<param name="out_para_number" type="12" value="" out="Y"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="pctl_no" type="12" value="" out="N"/>
				<param name="out_para_varchar" type="12" value="" out="Y"/>
				<param name="del_para" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
