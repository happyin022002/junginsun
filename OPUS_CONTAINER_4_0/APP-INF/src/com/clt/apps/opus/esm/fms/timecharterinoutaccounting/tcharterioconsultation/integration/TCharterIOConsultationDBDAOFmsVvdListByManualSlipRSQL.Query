<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOFmsVvdListByManualSlipRSQL">
			<desc><![CDATA[Manual Slip â€“ Brokerage / Window Select]]></desc>
			<sql><![CDATA[
WITH V_VVD AS(
        SELECT DISTINCT VVD_CD
             , VST_DT
             , VED_DT
             , FLET_CTRT_NO
             , FLET_CTRT_TP_CD
             , COM_VVD_FLG
             , CASE WHEN COM_VVD_FLG = 'Y' AND FLET_CTRT_TP_CD = 'TO' THEN FLET_CTRT_NO
                    WHEN COM_VVD_FLG = 'N' AND FLET_CTRT_TP_CD = 'TI' THEN FLET_CTRT_NO
                    ELSE NULL
               END AS EXIST_FLET_CTRT_NO
          FROM (
                SELECT FV.VSL_CD || FV.SKD_VOY_NO || FV.SKD_DIR_CD || FV.REV_DIR_CD VVD_CD
                     , FV.VST_DT
                     , FV.VED_DT
                     , FC.FLET_CTRT_NO
                     , FC.FLET_CTRT_TP_CD
                     , NVL((SELECT 'Y'
                              FROM MDM_REV_LANE M
                             WHERE M.REP_TRD_CD = 'COM'
                               AND M.RLANE_CD = FV.RLANE_CD),'N') AS COM_VVD_FLG
                  FROM FMS_CONTRACT FC
                     , FMS_VVD FV
                 WHERE 1=1
                   AND FC.FLET_CTRT_NO = @[flet_ctrt_no]
                   AND FC.VSL_CD = FV.VSL_CD
                 UNION ALL
                SELECT FV.VSL_CD || FV.SKD_VOY_NO || FV.SKD_DIR_CD || FV.REV_DIR_CD VVD_CD
                     , FV.VST_DT
                     , FV.VED_DT
                     , FI.FLET_CTRT_NO
                     , SUBSTR(FI.FLET_CTRT_NO, 5,2) AS FLET_CTRT_TP_CD
                     , NVL((SELECT 'Y'
                              FROM MDM_REV_LANE M
                             WHERE M.REP_TRD_CD = 'COM'
                               AND M.RLANE_CD = FV.RLANE_CD),'N') AS COM_VVD_FLG
                  FROM FMS_ID_VSL FI
                     , FMS_VVD FV
                 WHERE 1=1
                   AND FI.FLET_CTRT_NO = @[flet_ctrt_no]
                   AND FI.USE_FLG = 'Y'
                   AND FI.VSL_CD = FV.VSL_CD )
         WHERE 1=1
           AND (CASE WHEN COM_VVD_FLG = 'Y' AND FLET_CTRT_TP_CD = 'TO' THEN FLET_CTRT_NO
                     WHEN COM_VVD_FLG = 'N' AND FLET_CTRT_TP_CD = 'TI' THEN FLET_CTRT_NO
                     ELSE NULL
                END) IS NOT NULL
         ORDER BY VVD_CD, VST_DT 
        )
--SELECT * FROM V_VVD;        
SELECT DISTINCT VVD_CD
  FROM (
        SELECT V.VVD_CD 
          FROM V_VVD V
         WHERE 1=1
         AND ( REPLACE(@[eff_dt],'-','') BETWEEN V.VST_DT AND V.VED_DT
            OR REPLACE(@[exp_dt],'-','') BETWEEN V.VST_DT AND V.VED_DT )
       )
 WHERE VVD_CD IS NOT NULL 			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
