<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFATransportationAdditionalChargeProposalDBDAOChkArbiLocationRSQL">
			<desc><![CDATA[check the location of Arbi's Point and Base port]]></desc>
			<sql><![CDATA[
WITH TMP AS
(
SELECT 
       ATTR_CTNT1 AS SEQ
     , ATTR_CTNT2 AS ROUT_PNT_LOC_DEF_CD
     , DECODE(LENGTH(ATTR_CTNT2),4,'G',5,'L') AS ROUT_PNT_LOC_TP_CD
     , ATTR_CTNT4 AS PRC_TRSP_MOD_CD
     , ATTR_CTNT5 AS RCV_DE_TERM_CD
     , ATTR_CTNT6 AS MIN_CGO_WGT
     , ATTR_CTNT7 AS MAX_CGO_WGT
     , ATTR_CTNT8 AS BSE_PORT_DEF_CD
     , DECODE(LENGTH(ATTR_CTNT8),4,'G',5,'L') AS BSE_PORT_TP_CD
     , ATTR_CTNT10 AS CUST_DEF_CD
     , ATTR_CTNT11 AS RAT_UT_CD
     , ATTR_CTNT12 AS PRC_CGO_TP_CD
     , ATTR_CTNT13 AS CURR_CD
     , ATTR_CTNT14 AS PROP_FRT_RT_AMT
     , ATTR_CTNT15 AS NOTE_DP_SEQ
     , ATTR_CTNT16 AS ADD_CHG_NOTE_CTNT
     , ATTR_CTNT17 AS ORG_DEST_TP_CD
     , ATTR_CTNT18 AS PROP_NO
     , ATTR_CTNT19 AS AMDT_SEQ
     , ATTR_CTNT20 AS SVC_SCP_CD
  FROM PRI_VAL_TMP

)
SELECT
       TO_NUMBER(A.SEQ) AS ADD_CHG_SEQ
     , A.UKEY
     , SUM(A.ERR_ROW_CNT) OVER (PARTITION BY A.GB) AS TOT_ERR_CNT
     , SUM(A.ERR_ROW_CNT) OVER (PARTITION BY A.SEQ) AS ROW_ERR_CNT
     , A.DUP_CNT
     , A.CHK_LEN_ROUT_PNT_LOC_DEF_CD
     , A.CHK_LEN_BSE_PORT_DEF_CD
     , A.CHK_RCV_DE_TERM_CD
     , A.CHK_RAT_UT_CD
     , A.CHK_CURR_CD
     , A.CHK_MIN_CGO_WGT
     , A.CHK_MAX_CGO_WGT
     , A.CHK_PROP_FRT_RT_AMT
     , A.CHK_TERM_PNT_PORT
     , A.CHK_ROUT_PNT_LOC_DEF_CD
     , A.CHK_BSE_PORT_DEF_CD
     , A.ROUT_PNT_LOC_DEF_CD
     , A.ROUT_PNT_LOC_TP_CD
     , A.PRC_TRSP_MOD_CD
     , A.RCV_DE_TERM_CD
     , A.MIN_CGO_WGT
     , A.MAX_CGO_WGT
     , A.BSE_PORT_DEF_CD
     , A.BSE_PORT_TP_CD
     , A.CUST_DEF_CD
     , A.RAT_UT_CD
     , A.PRC_CGO_TP_CD
     , A.CURR_CD
     , A.PROP_FRT_RT_AMT
     , A.NOTE_DP_SEQ
     , A.ORG_DEST_TP_CD
     , A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD
  FROM (
SELECT 
       A.SEQ
     , A.GB
     , A.UKEY
     , A.ERR_ROW_CNT
     , A.DUP_CNT
     , A.CHK_LEN_ROUT_PNT_LOC_DEF_CD
     , A.CHK_LEN_BSE_PORT_DEF_CD
     , A.CHK_RCV_DE_TERM_CD
     , A.CHK_RAT_UT_CD
     , A.CHK_CURR_CD
     , A.CHK_MIN_CGO_WGT
     , A.CHK_MAX_CGO_WGT
     , A.CHK_PROP_FRT_RT_AMT
     , A.CHK_TERM_PNT_PORT
     , A.CHK_ROUT_PNT_LOC_DEF_CD
     , A.CHK_BSE_PORT_DEF_CD
     , A.ROUT_PNT_LOC_DEF_CD
     , A.ROUT_PNT_LOC_TP_CD
     , A.PRC_TRSP_MOD_CD
     , A.RCV_DE_TERM_CD
     , A.MIN_CGO_WGT
     , A.MAX_CGO_WGT
     , A.BSE_PORT_DEF_CD
     , A.BSE_PORT_TP_CD
     , A.CUST_DEF_CD
     , A.RAT_UT_CD
     , A.PRC_CGO_TP_CD
     , A.CURR_CD
     , A.PROP_FRT_RT_AMT
     , A.NOTE_DP_SEQ
     , A.ORG_DEST_TP_CD
     , A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD


  FROM (
SELECT
        CASE WHEN (A.CHK_LEN_ROUT_PNT_LOC_DEF_CD = 'F' OR A.CHK_LEN_BSE_PORT_DEF_CD = 'F'     OR A.CHK_RCV_DE_TERM_CD = 'F' OR 
                  A.CHK_RAT_UT_CD = 'F' OR A.CHK_CURR_CD = 'F' OR A.CHK_MIN_CGO_WGT = 'F' OR 
                  A.CHK_MAX_CGO_WGT = 'F' OR A.CHK_PROP_FRT_RT_AMT = 'F' OR A.CHK_TERM_PNT_PORT = 'F' OR 
                  A.DUP_CNT > 1 OR A.CHK_ROUT_PNT_LOC_DEF_CD = 'F' OR A.CHK_BSE_PORT_DEF_CD = 'F' ) THEN 1 ELSE 0 END ERR_ROW_CNT
      , A.*
  FROM (
SELECT
       CASE WHEN LENGTH(NVL(A.ROUT_PNT_LOC_DEF_CD,'')) = 5 THEN 'T' ELSE 'F' END CHK_LEN_ROUT_PNT_LOC_DEF_CD
     , CASE WHEN ( (NVL(A.BSE_PORT_DEF_CD,'1') <> '1') AND ( LENGTH(NVL(A.BSE_PORT_DEF_CD,'')) = 5 OR LENGTH(NVL(A.BSE_PORT_DEF_CD,'')) = 4 ) ) THEN 'T' ELSE 'F' END CHK_LEN_BSE_PORT_DEF_CD
     , CASE WHEN NVL(A.RCV_DE_TERM_CD,'1') <> '1' THEN 'T' ELSE 'F' END CHK_RCV_DE_TERM_CD
     , CASE WHEN NVL(A.RAT_UT_CD,'1') <> '1' THEN 'T' ELSE 'F' END CHK_RAT_UT_CD
     , CASE WHEN NVL(A.CURR_CD,'1') <> '1' THEN 'T' ELSE 'F' END CHK_CURR_CD
     , CASE WHEN A.MIN_CGO_WGT IS NOT NULL AND A.MIN_CGO_WGT > 999.999 THEN 'F' ELSE 'T' END CHK_MIN_CGO_WGT
     , CASE WHEN A.MAX_CGO_WGT IS NOT NULL AND A.MAX_CGO_WGT > 999.999 THEN 'F' ELSE 'T' END CHK_MAX_CGO_WGT
     , CASE WHEN A.PROP_FRT_RT_AMT IS NOT NULL AND LENGTH(A.PROP_FRT_RT_AMT) > 10 THEN 'F' ELSE 'T' END CHK_PROP_FRT_RT_AMT
     , CASE WHEN A.RCV_DE_TERM_CD <> 'D' AND (A.ROUT_PNT_LOC_DEF_CD = A.BSE_PORT_DEF_CD) THEN 'F' ELSE 'T' END CHK_TERM_PNT_PORT
     , A.UKEY
     , COUNT(A.UKEY) OVER(PARTITION BY A.UKEY) AS DUP_CNT
     , A.CHK_ROUT_PNT_LOC_DEF_CD
     , A.CHK_BSE_PORT_DEF_CD
     , A.GB
     , A.SEQ
     , A.ROUT_PNT_LOC_DEF_CD
     , A.ROUT_PNT_LOC_TP_CD
     , A.PRC_TRSP_MOD_CD
     , A.RCV_DE_TERM_CD
     , A.MIN_CGO_WGT
     , A.MAX_CGO_WGT
     , A.BSE_PORT_DEF_CD
     , A.BSE_PORT_TP_CD
     , A.CUST_DEF_CD
     , A.RAT_UT_CD
     , A.PRC_CGO_TP_CD
     , A.CURR_CD
     , A.PROP_FRT_RT_AMT
     , A.NOTE_DP_SEQ
     , A.ORG_DEST_TP_CD
     , A.PROP_NO
     , A.AMDT_SEQ
     , A.SVC_SCP_CD

  FROM (
SELECT 
       '1' AS GB
     , CASE WHEN B.LOC_CD IS NOT NULL THEN 'T' ELSE 'F' END CHK_ROUT_PNT_LOC_DEF_CD
     , CASE WHEN C.LOC_CD IS NOT NULL THEN 'T' ELSE 'F' END CHK_BSE_PORT_DEF_CD
     , A.ROUT_PNT_LOC_DEF_CD||'|'||NVL(A.PRC_TRSP_MOD_CD,'X')||'|'||A.RCV_DE_TERM_CD||'|'||A.BSE_PORT_DEF_CD||'|'||A.RAT_UT_CD||'|'||NVL(A.PRC_CGO_TP_CD,'X')||'|'||TO_CHAR(A.MIN_CGO_WGT)||'|'||TO_CHAR(A.MAX_CGO_WGT)||'|'||NVL(A.CUST_DEF_CD,'X')||'|'||TO_CHAR(A.AMDT_SEQ)||'|'||NVL(A.ADD_CHG_NOTE_CTNT,'X') AS UKEY
     , A.SEQ
     , A.ROUT_PNT_LOC_DEF_CD
     , A.ROUT_PNT_LOC_TP_CD
     , A.PRC_TRSP_MOD_CD
     , A.RCV_DE_TERM_CD
     , TO_NUMBER(A.MIN_CGO_WGT) AS MIN_CGO_WGT
     , TO_NUMBER(A.MAX_CGO_WGT) AS MAX_CGO_WGT
     , A.BSE_PORT_DEF_CD
     , A.BSE_PORT_TP_CD
     , A.CUST_DEF_CD
     , A.RAT_UT_CD
     , A.PRC_CGO_TP_CD
     , A.CURR_CD
     , TO_NUMBER(A.PROP_FRT_RT_AMT) AS PROP_FRT_RT_AMT
     , TO_NUMBER(A.NOTE_DP_SEQ) AS NOTE_DP_SEQ
     , A.ADD_CHG_NOTE_CTNT
     , A.ORG_DEST_TP_CD
     , A.PROP_NO
     , TO_NUMBER(A.AMDT_SEQ) AS AMDT_SEQ
     , A.SVC_SCP_CD


  FROM TMP A
     , (SELECT 
               LOC_CD
          FROM MDM_LOCATION
         WHERE DELT_FLG ='N') B
     , (SELECT
               LOC_CD
          FROM (SELECT 
                       LOC_CD
                  FROM MDM_LOCATION
                 WHERE DELT_FLG ='N'
                 UNION
                SELECT 
                       PRC_GRP_LOC_CD AS LOC_CD
                  FROM PRI_RP_SCP_GRP_LOC A
                     , (SELECT 
                               DISTINCT PROP_NO, AMDT_SEQ, GRP_LOC_SEQ, SVC_SCP_CD
                          FROM PRI_RP_SCP_GRP_LOC_DTL
                         WHERE SRC_INFO_CD != 'AD' ) B
                 WHERE A.PROP_NO		= B.PROP_NO
                   AND A.AMDT_SEQ		= B.AMDT_SEQ
                   AND A.SVC_SCP_CD		= B.SVC_SCP_CD
                   AND A.GRP_LOC_SEQ	= B.GRP_LOC_SEQ
                   AND A.PROP_NO 		= @[prop_no]
                   AND A.AMDT_SEQ 		= @[amdt_seq]
                   AND A.SVC_SCP_CD 	= @[svc_scp_cd]) ) C
 WHERE A.ROUT_PNT_LOC_DEF_CD    = B.LOC_CD(+)
   AND A.BSE_PORT_DEF_CD        = C.LOC_CD(+)
 
 UNION ALL
 SELECT
       '2' AS GB
     , NULL AS CHK_ROUT_PNT_LOC_DEF_CD
     , NULL AS CHK_BSE_PORT_DEF_CD
     , ROUT_PNT_LOC_DEF_CD||'|'||NVL(PRC_TRSP_MOD_CD,'X')||'|'||RCV_DE_TERM_CD||'|'||BSE_PORT_DEF_CD||'|'||RAT_UT_CD||'|'||NVL(PRC_CGO_TP_CD,'X')||'|'||TO_CHAR(MIN_CGO_WGT)||'|'||TO_CHAR(MAX_CGO_WGT)||'|'||NVL((CUST_CNT_CD||TO_CHAR(CUST_SEQ)),'X')||'|'||TO_CHAR(AMDT_SEQ)||'|'||NVL(ADD_CHG_NOTE_CTNT,'X') AS UKEY
     , NULL AS SEQ
     , ROUT_PNT_LOC_DEF_CD
     , ROUT_PNT_LOC_TP_CD
     , PRC_TRSP_MOD_CD
     , RCV_DE_TERM_CD
     , MIN_CGO_WGT
     , MAX_CGO_WGT
     , BSE_PORT_DEF_CD
     , BSE_PORT_TP_CD
     , CUST_CNT_CD||TO_CHAR(CUST_SEQ) AS CUST_DEF_CD
     , RAT_UT_CD
     , PRC_CGO_TP_CD
     , CURR_CD
     , PROP_FRT_RT_AMT
     , NOTE_DP_SEQ
     , ADD_CHG_NOTE_CTNT
     , ORG_DEST_TP_CD
     , PROP_NO
     , AMDT_SEQ
     , SVC_SCP_CD
  FROM PRI_RP_SCP_TRSP_ADD_CHG WHERE PROP_NO = @[prop_no] AND AMDT_SEQ = @[amdt_seq] AND SVC_SCP_CD = @[svc_scp_cd] 
     ) A ) A 
WHERE A.GB = '1' ) A ) A
ORDER BY TO_NUMBER(A.SEQ)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
