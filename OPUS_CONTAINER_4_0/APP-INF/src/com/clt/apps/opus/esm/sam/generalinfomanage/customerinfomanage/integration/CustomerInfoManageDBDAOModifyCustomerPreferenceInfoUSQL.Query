<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAOModifyCustomerPreferenceInfoUSQL">
			<desc><![CDATA[ModifyCustomerPreferenceInfo]]></desc>
			<sql><![CDATA[
UPDATE SAM_CUST_PRF_INFO
SET CUST_PRF_SEQ = @[cust_prf_seq]
  , PRF_CATE_CD = @[prf_cate_cd]
  , PRF_MOD_CD = @[prf_mod_cd]
  , PRF_FM_LOC_CD = @[prf_fm_loc_cd]
  , PRF_TO_LOC_CD = @[prf_to_loc_cd]
  , PRF_VNDR_SEQ = @[prf_vndr_seq]
  , UPD_USR_ID = @[user_id]
  , UPD_DT = SYSDATE
WHERE CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,8))
AND CUST_PRF_SEQ = @[cust_prf_seq]			]]></sql>
			<params>
				<param name="cust_prf_seq" type="12" value="" out="N"/>
				<param name="prf_cate_cd" type="12" value="" out="N"/>
				<param name="prf_mod_cd" type="12" value="" out="N"/>
				<param name="prf_fm_loc_cd" type="12" value="" out="N"/>
				<param name="prf_to_loc_cd" type="12" value="" out="N"/>
				<param name="prf_vndr_seq" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
