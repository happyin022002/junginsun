<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSearchOfficeLvlCheckRSQL">
			<desc><![CDATA[Office 레벨이 4 or 5 인지 확인한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*), 0, 'N', 'Y') AS OFC_LVL_YN 
  FROM SPC_OFC_LVL  
 WHERE N4TH_PRNT_OFC_CD = @[fcast_ofc_cd] 
       OR N5TH_PRNT_OFC_CD = @[fcast_ofc_cd] 			]]></sql>
			<params>
				<param name="fcast_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
