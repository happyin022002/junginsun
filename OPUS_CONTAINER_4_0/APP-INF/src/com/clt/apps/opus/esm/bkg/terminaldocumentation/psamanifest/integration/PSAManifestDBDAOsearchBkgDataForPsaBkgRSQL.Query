<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchBkgDataForPsaBkgRSQL">
			<desc><![CDATA[MAKE PSA BKG WITH OPUS BKG SELECT OPUS DATA FOR PSA_BKG]]></desc>
			<sql><![CDATA[
SELECT BKG_STS_CD
     , POL_CD     
     , POD_CD     
     , DECODE( BKG_CGO_TP_CD, 'P', 'E', 'R', 'E', 'F' )  BKG_CGO_TP_CD
     , NVL( SPCL_HIDE_FLG, '0' ) SPCL_HIDE_FLG
     , DEL_CD
     , SLAN_CD
     , TRANSLATE(SUBSTR(XTER_RMK, 1, 140),CHR(10),' ') INTER_RMK
     , NVL(DCGO_FLG, '0') DCGO_FLG
     , NVL(PRCT_FLG, '0') PRCT_FLG
  FROM BKG_BOOKING
 WHERE BKG_NO      = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
