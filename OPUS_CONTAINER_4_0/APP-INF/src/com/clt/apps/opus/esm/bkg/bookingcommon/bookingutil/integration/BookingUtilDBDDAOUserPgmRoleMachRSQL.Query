<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDDAOUserPgmRoleMachRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	COUNT(B.USR_ID) CNT
FROM COM_PGM_ROLE A
     ,COM_USR_ROLE_MTCH B
WHERE 1 = 1
AND A.USR_ROLE_CD = B.USR_ROLE_CD
#if (${pgm_id} != '') 
AND A.PGM_NO = @[pgm_id]
#end
AND B.USR_ID = @[usr_id]
AND A.USR_ROLE_CD IN (SELECT ATTR_CTNT1 FROM BKG_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'WORK_WITH_BKG_ROLE')			]]></sql>
			<params>
				<param name="pgm_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
