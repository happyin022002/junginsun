<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOSearchCarrierRegisterHeaderListRSQL">
			<desc><![CDATA[SearchCarrierRegisterHeaderList SELECT]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
	   A.CRR_CD,
	   B.CRR_NM 
 FROM 
	  BSA_CRR_RGST A,
      MDM_CARRIER  B
 WHERE 
	  A.CRR_CD= B.CRR_CD
 ORDER BY
      CRR_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
