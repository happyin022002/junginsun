<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOMdmCarrierVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
select 
	crr_cd,
	crr_nm
from mdm_carrier
where	1=1
#if (${crr_cd} != '')     
and crr_cd = @[crr_cd]
#end
order by crr_cd			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
