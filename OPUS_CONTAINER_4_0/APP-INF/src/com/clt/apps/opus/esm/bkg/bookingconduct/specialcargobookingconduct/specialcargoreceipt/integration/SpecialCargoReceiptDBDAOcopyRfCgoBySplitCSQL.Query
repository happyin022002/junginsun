<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOcopyRfCgoBySplitCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
insert into bkg_rf_cgo(BKG_NO 
,RC_SEQ 
,CNTR_TPSZ_CD 
,CNTR_NO 
,PCK_TP_CD 
,PCK_QTY 
,NET_WGT 
,GRS_WGT 
,WGT_UT_CD 
,CMDT_CD 
,CMDT_DESC 
,FDO_TEMP 
,CDO_TEMP 
,CNTR_VENT_TP_CD 
,VENT_RTO 
,HUMID_NO 
,DIFF_RMK 
,RF_DCGO_SEQ 
,PWR_SPL_CBL_FLG 
,VLTG_NO 
,CTRL_ATMS_FLG 
,MODI_ATMS_FLG 
,HUMID_CTRL_FLG 
,CNTR_DRN_CD 
,CLNG_TP_CD 
,RQST_DT 
,RQST_USR_ID 
,SPCL_CGO_APRO_CD 
,CRE_USR_ID 
,CRE_DT 
,UPD_USR_ID 
,UPD_DT
,cntr_vol_qty
,SNS_CGO_KND_CD
,OXGN_RTO
,CRBN_DXD_RTO
,CBM_PER_HR_QTY
)
select @[targetBkg] BKG_NO 
,(
	SELECT NVL(MAX(RC_SEQ), 0)
	FROM   BKG_RF_CGO
	WHERE  BKG_NO = @[targetBkg]) + ROW_NUMBER() OVER (PARTITION BY BKG_NO ORDER BY RC_SEQ 
) AS RC_SEQ 
,CNTR_TPSZ_CD 
,CNTR_NO 
,PCK_TP_CD 
,PCK_QTY 
,NET_WGT 
,GRS_WGT 
,WGT_UT_CD 
,CMDT_CD 
,CMDT_DESC 
,FDO_TEMP 
,CDO_TEMP 
,CNTR_VENT_TP_CD 
,VENT_RTO 
,HUMID_NO 
,DIFF_RMK 
,RF_DCGO_SEQ 
,PWR_SPL_CBL_FLG 
,VLTG_NO 
,CTRL_ATMS_FLG 
,MODI_ATMS_FLG 
,HUMID_CTRL_FLG 
,CNTR_DRN_CD 
,CLNG_TP_CD 
,RQST_DT 
,RQST_USR_ID 
,SPCL_CGO_APRO_CD 
,@[usr_id] CRE_USR_ID 
,sysdate CRE_DT 
,@[usr_id] UPD_USR_ID 
,sysdate UPD_DT
,nvl((select cntr.CNTR_VOL_QTY 
        from bkg_container cntr 
       where cntr.bkg_no  = @[targetBkg]
         and cntr.cntr_no = spcl.cntr_no), 1)
, SNS_CGO_KND_CD
, OXGN_RTO
, CRBN_DXD_RTO
, CBM_PER_HR_QTY
from bkg_rf_cgo spcl
where bkg_no =  @[bkg_no]

#if(${rc_seq} != 'all')
	and rc_seq = @[rc_seq]
#else
	and cntr_no = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="targetBkg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rc_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
