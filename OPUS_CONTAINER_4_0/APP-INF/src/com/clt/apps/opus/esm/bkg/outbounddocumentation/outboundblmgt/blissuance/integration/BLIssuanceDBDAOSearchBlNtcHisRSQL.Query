<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchBlNtcHisRSQL">
			<desc><![CDATA[BLIssuanceDBDAOSearchBlNtcHis]]></desc>
			<sql><![CDATA[
SELECT  
	BKG_NO
	,NTC_VIA_CD
	,NTC_KND_CD
FROM	BKG_NTC_HIS
WHERE 0=0
AND BKG_NO = @[bkg_no]
AND NTC_VIA_CD = @[ntc_via_cd]
AND NTC_KND_CD = @[ntc_knd_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ntc_via_cd" type="12" value="" out="N"/>
				<param name="ntc_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
