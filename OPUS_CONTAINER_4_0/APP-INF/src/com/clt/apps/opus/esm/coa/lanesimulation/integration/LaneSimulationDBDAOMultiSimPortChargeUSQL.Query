<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOMultiSimPortChargeUSQL">
			<desc><![CDATA[Port Change 수정]]></desc>
			<sql><![CDATA[
UPDATE COA_SIM_PORT_CHG 
SET PORT_TRF_AMT     = @[port_trf_amt]
   ,CNL_FEE_AMT      = @[cnl_fee_amt]
   ,UPD_USR_ID       = @[upd_usr_id]
   ,UPD_DT           = SYSDATE
WHERE SIM_DT         = @[sim_dt] 
  AND SIM_NO         = @[sim_no] 
  AND VSL_CLSS_CAPA  = @[vsl_clss_capa] 
  AND TML_CD         = @[tml_cd]			]]></sql>
			<params>
				<param name="port_trf_amt" type="12" value="" out="Y"/>
				<param name="cnl_fee_amt" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="sim_dt" type="12" value="" out="Y"/>
				<param name="sim_no" type="12" value="" out="Y"/>
				<param name="vsl_clss_capa" type="12" value="" out="N"/>
				<param name="tml_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
