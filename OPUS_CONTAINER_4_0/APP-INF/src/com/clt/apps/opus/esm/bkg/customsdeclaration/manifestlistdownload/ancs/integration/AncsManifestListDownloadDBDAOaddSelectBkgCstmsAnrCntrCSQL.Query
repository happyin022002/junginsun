<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsManifestListDownloadDBDAOaddSelectBkgCstmsAnrCntrCSQL">
			<desc><![CDATA[INSERT]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ANR_CNTR (
VSL_CD, SKD_VOY_NO, SKD_DIR_CD, BKG_NO, CNTR_NO,
CNTR_TPSZ_CD, ISO_CNTR_TPSZ_CD, PCK_QTY, PCK_TP_CD, CNTR_WGT, WGT_UT_CD,
ORG_RCV_TERM_CD, DEST_DE_TERM_CD, DECL_FLG,
CRE_OFC_CD, UPD_OFC_CD, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT, ANR_MSG_STS_CD)

SELECT A.VSL_CD, A.SKD_VOY_NO, A.SKD_DIR_CD, A.BKG_NO, A.CNTR_NO
,A.CNTR_TPSZ_CD, A.CNTR_TPSZ_ISO_CD, A.PCK_QTY, A.PCK_TP_CD, A.CNTR_WGT, A.WGT_UT_CD
,A.ORG_RCV_TERM_CD ,A.DEST_DE_TERM_CD, A.DECL_FLG
,A.CRE_OFC_CD ,A.UPD_OFC_CD, A.CRE_USR_ID, A.CRE_DT, A.UPD_USR_ID, A.UPD_DT
,DECODE(B.EDI_SND_STS_CD||B.EDI_RCV_STS_CD, 'OA', 'A' ,'CA','N','TA','A','') AS ANR_MSG_STS_CD -- 세관에서 Accepeted를 받고나서 다시 다운로드 받으면 ANR_MSG_STS_CD값이 초기화되는 현재 로직 수정
                                                                                               -- Original이나 Correction 전송에 대한 Accepet은 A로 Cancel 전송에 대한 Accepet은 N으로 표시
FROM (
SELECT B.VSL_CD, B.SKD_VOY_NO, B.SKD_DIR_CD, C.BKG_NO, C.CNTR_NO, 
C.CNTR_TPSZ_CD, D.CNTR_TPSZ_ISO_CD, NVL(C.PCK_QTY,0) AS PCK_QTY, C.PCK_TP_CD, NVL(C.CNTR_WGT,0) AS CNTR_WGT, C.WGT_UT_CD,
DECODE(C.RCV_TERM_CD, 'S', 'LCL', 'FCL') AS ORG_RCV_TERM_CD, DECODE(C.DE_TERM_CD, 'S', 'LCL', 'FCL') AS DEST_DE_TERM_CD, 'Y' AS DECL_FLG
, @[cre_ofc_cd] AS CRE_OFC_CD
, @[upd_ofc_cd] AS UPD_OFC_CD
, @[cre_usr_id] AS CRE_USR_ID
, SYSDATE AS CRE_DT
, @[upd_usr_id] AS UPD_USR_ID
, SYSDATE AS UPD_DT
, (SELECT MAX(REF_SEQ)
   FROM BKG_CSTMS_ANR_CNTR_LOG
   WHERE CNTR_NO = C.CNTR_NO) MAX_REF_SEQ ,(SELECT ANR_DECL_NO FROM BKG_CSTMS_ANR_CNTR_LOG WHERE C.CNTR_NO = CNTR_NO  AND ROWNUM =1 ) ANR_DECL_NO 
   ,(SELECT MSG_TP_CD FROM BKG_CSTMS_ANR_CNTR_LOG WHERE B.BKG_NO = BKG_NO  AND ROWNUM =1 ) AS MSG_TP_CD
FROM BKG_VVD A, BKG_CSTMS_ANR_BL B, BKG_CONTAINER C, MDM_CNTR_TP_SZ D
WHERE A.VSL_CD = substr(@[vvd],1,4)
AND A.SKD_VOY_NO = substr(@[vvd],5,4)
AND A.SKD_DIR_CD = substr(@[vvd],9,1)
AND A.POL_CD = @[pol]
AND A.POD_CD = @[pod]


#if (${pol_clpt_ind_seq} != '') 
AND A.POL_CLPT_IND_SEQ = @[pol_clpt_ind_seq]
#end
#if (${pod_clpt_ind_seq} != '') 
AND A.POD_CLPT_IND_SEQ = @[pod_clpt_ind_seq]
#end
AND B.VSL_CD = A.VSL_CD
AND B.SKD_VOY_NO = A.SKD_VOY_NO
AND B.SKD_DIR_CD = A.SKD_DIR_CD
AND B.BKG_NO = A.BKG_NO
AND C.BKG_NO = B.BKG_NO
AND D.CNTR_TPSZ_CD(+) = C.CNTR_TPSZ_CD
#if (${chk_down} == 'Y' || ${chk_down} == 'C') 
AND NOT EXISTS (SELECT '*' FROM BKG_CSTMS_ANR_CNTR E WHERE E.VSL_CD = B.VSL_CD AND E.SKD_VOY_NO = B.SKD_VOY_NO AND E.SKD_DIR_CD = B.SKD_DIR_CD
                   AND E.BKG_NO = C.BKG_NO
                   AND E.CNTR_NO = C.CNTR_NO)
#else 
AND NOT EXISTS (SELECT '*' FROM BKG_CSTMS_ANR_CNTR E WHERE E.VSL_CD = B.VSL_CD AND E.SKD_VOY_NO = B.SKD_VOY_NO AND E.SKD_DIR_CD = B.SKD_DIR_CD
                   AND E.BKG_NO = C.BKG_NO)
#end
) A, BKG_CSTMS_ANR_EDI_HIS B

WHERE A.MAX_REF_SEQ = B.REF_SEQ(+)
AND  A.MSG_TP_CD = B.MSG_TP_CD(+)
AND A.ANR_DECL_NO = B.ANR_DECL_NO(+)			]]></sql>
			<params>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="pod_clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
