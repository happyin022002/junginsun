<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDBDAOSearchServiceLaneDirectionRSQL">
			<desc><![CDATA[Service Lane Direction Search.]]></desc>
			<sql><![CDATA[
SELECT D.VSL_SLAN_DIR_CD AS SLAN_DIR_CD
  FROM MDM_VSL_SVC_LANE L
     , MDM_VSL_SVC_LANE_DIR D
 WHERE 1=1
   AND L.DELT_FLG = 'N'
   AND L.VSL_SLAN_CD = D.VSL_SLAN_CD
   AND L.VSL_SLAN_CD = @[slan_cd]
   AND D.VSL_SLAN_DIR_CD = @[slan_dir_cd]			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="slan_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
