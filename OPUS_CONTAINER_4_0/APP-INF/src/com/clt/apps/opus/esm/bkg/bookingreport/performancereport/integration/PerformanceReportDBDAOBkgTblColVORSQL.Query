<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOBkgTblColVORSQL">
			<desc><![CDATA[report datail search]]></desc>
			<sql><![CDATA[
SELECT 
	A.TBL_NM
,	A.COL_NM
,	A.STS_RPT_FLG
,	A.CA_RPT_FLG
,	A.VIP_RPT_FLG
,	A.HIS_CATE_NM
,	A.TBL_COL_NM
,	A.DP_NM
,	A.SQL_CTNT
,	A.HIS_FLG
,	A.CA_EXPT_FLG
,	A.CA_REV_FLG
,	A.CA_COST_FLG
,	A.INV_IF_FLG
,	A.COP_IF_FLG
,	A.COA_IF_FLG
,	A.UPD_GRP_CD
,	A.UPD_SUB_GRP_CD
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,	A.UPD_DT
,	B.RPT_ID
,	B.BKG_RPT_KND_CD
,	B.ORD_SEQ
FROM BKG_TBL_COL  		A
    ,BKG_RPT_DFLT_DTL 	B
WHERE A.TBL_NM = B.TBL_NM
AND A.COL_NM = B.COL_NM
AND B.RPT_ID = @[rpt_id]
AND B.BKG_RPT_KND_CD = @[bkg_rpt_knd_cd]			]]></sql>
			<params>
				<param name="rpt_id" type="12" value="" out="N"/>
				<param name="bkg_rpt_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
