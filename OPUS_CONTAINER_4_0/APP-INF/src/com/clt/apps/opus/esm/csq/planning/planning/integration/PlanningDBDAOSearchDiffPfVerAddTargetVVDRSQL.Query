<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanningDBDAOSearchDiffPfVerAddTargetVVDRSQL">
			<desc><![CDATA[Add-Creation시 Target VVD Fix를 하는데 PF Skd Group MGMT에서 생성된 PF SKD VER과 다른것이 있는지 조회한다.]]></desc>
			<sql><![CDATA[
SELECT A1.RLANE_CD||'-'||A1.PF_SKD_TP_CD AS DIFF_PF_VER
FROM 
    (    
       SELECT DISTINCT
              A1.TRD_CD
             ,A1.RLANE_CD
             ,A1.SUB_TRD_CD
             ,A3.PF_SKD_TP_CD
      FROM COA_MON_VVD A1
          ,BSA_VVD_MST A2
          ,VSK_VSL_SKD A3
     WHERE 1=1
       AND A1.RLANE_CD              = @[f_rlane_cd]
       AND SUBSTR(A1.SLS_YRMON,0,4) = @[f_bse_yr]
       AND A1.COST_WK      BETWEEN @[f_fm_wk] AND @[f_to_wk]

       AND A1.TRD_CD       = A2.TRD_CD(+)
       AND A1.RLANE_CD     = A2.RLANE_CD(+)
       AND A1.VSL_CD       = A2.VSL_CD(+)
       AND A1.SKD_VOY_NO   = A2.SKD_VOY_NO(+)
       AND A1.DIR_CD       = A2.SKD_DIR_CD(+)

       AND A1.VSL_CD       = A3.VSL_CD
       AND A1.SKD_VOY_NO   = A3.SKD_VOY_NO
       AND A1.DIR_CD       = A3.SKD_DIR_CD
       AND A1.SLAN_CD      = A3.VSL_SLAN_CD

       AND A1.DELT_FLG     = 'N'
       AND A1.TRD_CD       = @[f_trd_cd]
       ) A1
WHERE 1=1
AND NOT EXISTS (SELECT 'X' 
                FROM (
                       SELECT DISTINCT SUB_TRD_CD, RLANE_CD, PF_SVC_TP_CD
                        FROM CSQ_SCTR_PF_GRP
                        WHERE 1=1
                        AND BSE_TP_CD  = @[f_bse_tp_cd]
                        AND BSE_YR     = @[f_bse_yr]
                        AND BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
                        AND RLANE_CD   = @[f_rlane_cd]
                        ) A2
                WHERE A2.SUB_TRD_CD||A2.RLANE_CD||A2.PF_SVC_TP_CD =  A1.SUB_TRD_CD||A1.RLANE_CD||A1.PF_SKD_TP_CD)
ORDER BY RLANE_CD , PF_SKD_TP_CD			]]></sql>
			<params>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
