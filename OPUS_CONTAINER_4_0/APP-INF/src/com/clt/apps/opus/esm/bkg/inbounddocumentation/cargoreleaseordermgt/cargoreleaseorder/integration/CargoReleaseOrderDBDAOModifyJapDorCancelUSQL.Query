<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOModifyJapDorCancelUSQL">
			<desc><![CDATA[EDI로 전송을 요청한 상태 코드를 CANCEL(X) 상태로 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_JP_DO_IF 
   SET JP_DO_GRP_NO='-1'
      ,JP_DO_SND_STS_CD ='X'
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT     = SYSDATE
WHERE BKG_NO      =@[bkg_no]
  AND RLSE_SEQ    =@[rlse_seq]
  AND DO_IF_SEQ = ( SELECT MAX(DO_IF_SEQ)
                      FROM BKG_JP_DO_IF
                     WHERE BKG_NO=@[bkg_no]
                       AND RLSE_SEQ=@[rlse_seq]
                       AND JP_DO_SND_STS_CD ='T'
                  )			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rlse_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
