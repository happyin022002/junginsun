<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOsearchMrnInfoByMRNRSQL">
			<desc><![CDATA[MRN Check]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
  FROM BKG_CSTMS_KR_MF_REF_NO
 WHERE MRN_NO = @[mrn_no]
   AND IO_BND_CD <> @[io_bnd_cd]			]]></sql>
			<params>
				<param name="mrn_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
