<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOMultiNonOpExpnUSQL">
			<desc><![CDATA[영엉외비용 수정]]></desc>
			<sql><![CDATA[
UPDATE COA_SIM_NON_OP_EXPN SET 
	VSL_CD = @[vsl_cd]
,	UC_AMT = @[uc_amt]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	COST_YRMON = @[cost_yrmon]
AND	VSL_CD = @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="uc_amt" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
