<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOsearchPodDelForCodSplitRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
select substr(new_pod_yd_cd, 1, 5) pod_cd
        , new_pod_yd_cd pod_yd_cd
        , substr(new_del_yd_cd, 1, 5) del_cd
        , new_del_yd_cd del_yd_cd
  from bkg_cod
 where bkg_no = @[bkg_no]
   and cod_rqst_seq = @[codRqstSeq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="codRqstSeq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
