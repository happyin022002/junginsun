<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchDiscLocRSQL">
			<desc><![CDATA[searchDiscLoc]]></desc>
			<sql><![CDATA[
SELECT OTR_DCHG_CD
  FROM BKG_DCHG_LOC
 WHERE PORT_CD    = @[port_cd]
   AND SLAN_CD    = @[slan_cd]
   AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
