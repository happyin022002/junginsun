<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAORsltAllRtListVORSQL">
			<desc><![CDATA[Accept All List]]></desc>
			<sql><![CDATA[
WITH ACPT_KEY_ROUT AS (
  SELECT DISTINCT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ
    FROM (SELECT A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, B.ROUT_SEQ
            FROM PRI_SP_SCP_RT_CMDT A, PRI_SP_SCP_RT_CMDT_ROUT B
           WHERE A.PROP_NO = @[prop_no]
             AND A.AMDT_SEQ = @[amdt_seq]
             AND A.SVC_SCP_CD = @[svc_scp_cd]
             AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ
             AND A.PROP_NO = B.PROP_NO
             AND A.AMDT_SEQ = B.AMDT_SEQ
             AND A.SVC_SCP_CD = B.SVC_SCP_CD
             AND A.GEN_SPCL_RT_TP_CD = B.GEN_SPCL_RT_TP_CD
             AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
          UNION ALL
          SELECT A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, B.ROUT_SEQ
            FROM PRI_SP_SCP_RT_ACT_CUST A, PRI_SP_SCP_RT_CMDT_ROUT B
           WHERE A.PROP_NO = @[prop_no]
             AND A.AMDT_SEQ = @[amdt_seq]
             AND A.SVC_SCP_CD = @[svc_scp_cd]
             AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ
             AND A.PROP_NO = B.PROP_NO
             AND A.AMDT_SEQ = B.AMDT_SEQ
             AND A.SVC_SCP_CD = B.SVC_SCP_CD
             AND A.GEN_SPCL_RT_TP_CD = B.GEN_SPCL_RT_TP_CD
             AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
          UNION ALL
          SELECT A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, B.ROUT_SEQ
            FROM PRI_SP_SCP_RT_CNOTE A, PRI_SP_SCP_RT_CMDT_ROUT B
           WHERE A.PROP_NO = @[prop_no]
             AND A.AMDT_SEQ = @[amdt_seq]
             AND A.SVC_SCP_CD = @[svc_scp_cd]
             AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ
             AND A.PROP_NO = B.PROP_NO
             AND A.AMDT_SEQ = B.AMDT_SEQ
             AND A.SVC_SCP_CD = B.SVC_SCP_CD
             AND A.GEN_SPCL_RT_TP_CD = B.GEN_SPCL_RT_TP_CD
             AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
          UNION ALL
          SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ
            FROM PRI_SP_SCP_RT_ROUT_PNT
           WHERE PROP_NO = @[prop_no]
             AND AMDT_SEQ = @[amdt_seq]
             AND SVC_SCP_CD = @[svc_scp_cd]
             AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND AMDT_SEQ = N1ST_CMNC_AMDT_SEQ
          UNION ALL
          SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ
            FROM PRI_SP_SCP_RT_ROUT_VIA
           WHERE PROP_NO = @[prop_no]
             AND AMDT_SEQ = @[amdt_seq]
             AND SVC_SCP_CD = @[svc_scp_cd]
             AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND AMDT_SEQ = N1ST_CMNC_AMDT_SEQ
          UNION ALL
          SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ
            FROM PRI_SP_SCP_RT_ROUT_DIR
           WHERE PROP_NO = @[prop_no]
             AND AMDT_SEQ = @[amdt_seq]
             AND SVC_SCP_CD = @[svc_scp_cd]
             AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND AMDT_SEQ = N1ST_CMNC_AMDT_SEQ
          UNION ALL
          SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ
            FROM PRI_SP_SCP_RT_CMDT_RNOTE
           WHERE PROP_NO = @[prop_no]
             AND AMDT_SEQ = @[amdt_seq]
             AND SVC_SCP_CD = @[svc_scp_cd]
             AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND AMDT_SEQ = N1ST_CMNC_AMDT_SEQ
          UNION ALL
          SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ
            FROM PRI_SP_SCP_RT
           WHERE PROP_NO = @[prop_no]
             AND AMDT_SEQ = @[amdt_seq]
             AND SVC_SCP_CD = @[svc_scp_cd]
             AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
             AND AMDT_SEQ = N1ST_CMNC_AMDT_SEQ)
),
ACPT_KEY_HDR AS (
  SELECT DISTINCT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ FROM ACPT_KEY_ROUT
)
SELECT /*+ ORDERED */
       A.PROP_NO
      ,A.AMDT_SEQ
      ,A.SVC_SCP_CD
      ,A.GEN_SPCL_RT_TP_CD
      ,A.CMDT_HDR_SEQ
      ,A.BLET_DP_SEQ
      ,REPLACE(B.PRC_CMDT_DEF_NM, '|||', ' / ' || CHR(13)) AS PRC_CMDT_DEF_NM
      ,NVL(REPLACE(C.CUST_LGL_ENG_NM, '|||', ' / ' || CHR(13)), ' ') AS CUST_LGL_ENG_NM
      ,NVL(REPLACE(D.CNOTE_CTNT, '|||', ' '), ' ') AS CNOTE_CTNT
      ,E.ROUT_SEQ
      ,REPLACE(F.ROUT_PNT_LOC_DEF_NM, '|||', CHR(13)) AS ORG_ROUT_PNT_LOC_DEF_NM
      ,REPLACE(G.ROUT_VIA_PORT_DEF_CD, '|||', CHR(13)) AS ORG_ROUT_VIA_PORT_DEF_CD
      ,REPLACE(H.ROUT_VIA_PORT_DEF_CD, '|||', CHR(13)) AS DEST_ROUT_VIA_PORT_DEF_CD
      ,REPLACE(I.ROUT_PNT_LOC_DEF_NM, '|||', CHR(13)) AS DEST_ROUT_PNT_LOC_DEF_NM
      ,L.DIR_CALL_FLG || L.SRC_INFO_CD AS DIR_CALL_FLG
      ,REPLACE(J.RNOTE_CTNT, '|||', ' ') AS RNOTE_CTNT
      ,REPLACE(K.RT_CTNT, '|||', CHR(13)) AS RT_CTNT
      ,REPLACE(K.GRI_CTNT, '|||', CHR(13)) AS GRI_CTNT
  FROM PRI_SP_SCP_RT_CMDT_HDR A
      ,ACPT_KEY_HDR AK
      ,(
        SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,PRC_CMDT_DEF_NM_A||DECODE(PRC_CMDT_DEF_NM_B,'','','|||')||PRC_CMDT_DEF_NM_B PRC_CMDT_DEF_NM        
        FROM (            
                SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,GEN_SPCL_RT_TP_CD
                      ,CMDT_HDR_SEQ
                      ,MAX(CASE WHEN PART_SEQ = 'A' THEN PRC_CMDT_DEF_NM ELSE NULL END) AS PRC_CMDT_DEF_NM_A
                      ,MAX(CASE WHEN PART_SEQ = 'B' THEN PRC_CMDT_DEF_NM ELSE NULL END) AS PRC_CMDT_DEF_NM_B
                FROM (      
                      SELECT PROP_NO
                              ,AMDT_SEQ
                              ,SVC_SCP_CD
                              ,GEN_SPCL_RT_TP_CD
                              ,CMDT_HDR_SEQ
                              ,PART_SEQ
                              ,SUBSTR(SYS_CONNECT_BY_PATH(PRC_CMDT_DEF_NM || SRC_INFO_CD, '|||'), 4) AS PRC_CMDT_DEF_NM
                          FROM (
                                SELECT 
                                        PROP_NO
                                       ,AMDT_SEQ
                                       ,SVC_SCP_CD
                                       ,GEN_SPCL_RT_TP_CD
                                       ,CMDT_HDR_SEQ
                                       ,PRC_CMDT_DEF_NM
                                       ,PRC_PROG_STS_CD
                                       ,SRC_INFO_CD
                                       ,CASE WHEN RN <= 100 THEN 'A'
                                            WHEN RN <= 200 THEN 'B'
                                       ELSE 'C'
                                       END AS PART_SEQ
                                       ,CASE WHEN RN <= 100 THEN RN
                                            WHEN RN <= 200 THEN RN - 100
                                       ELSE RN - 200
                                       END AS RN,
                                       CNT
                                FROM            
                                        (SELECT /*+ LEADING(K) */
                                               A.PROP_NO
                                              ,A.AMDT_SEQ
                                              ,A.SVC_SCP_CD
                                              ,A.GEN_SPCL_RT_TP_CD
                                              ,A.CMDT_HDR_SEQ
                                              ,DECODE(PRC_CMDT_TP_CD
                                                     ,'G'
                                                     ,(SELECT PRC_GRP_CMDT_DESC
                                                        FROM PRI_SP_SCP_GRP_CMDT
                                                       WHERE PROP_NO = A.PROP_NO
                                                         AND AMDT_SEQ = A.AMDT_SEQ
                                                         AND SVC_SCP_CD = A.SVC_SCP_CD
                                                         AND PRC_GRP_CMDT_CD = A.PRC_CMDT_DEF_CD
                                                         AND ROWNUM = 1)
                                                     ,'C'
                                                     ,(SELECT CMDT_NM
                                                        FROM MDM_COMMODITY
                                                       WHERE CMDT_CD = A.PRC_CMDT_DEF_CD
                                                         AND ROWNUM = 1)) AS PRC_CMDT_DEF_NM
                                              ,PRC_PROG_STS_CD
                                              ,CASE
                                                 WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                                                  (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                                                     FROM COM_INTG_CD_DTL
                                                    WHERE INTG_CD_ID = 'CD02064'
                                                      AND INTG_CD_VAL_CTNT = SRC_INFO_CD
                                                      AND ROWNUM = 1)
                                                 ELSE
                                                  ''
                                               END AS SRC_INFO_CD
                                              ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ ORDER BY DECODE(A.PRC_CMDT_TP_CD, 'G', '1', 'C', '2'), PRC_CMDT_DEF_CD) AS RN
                                              ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ) AS CNT
                                          FROM PRI_SP_SCP_RT_CMDT A, ACPT_KEY_HDR K
                                         WHERE A.PROP_NO = @[prop_no]
                                           AND A.AMDT_SEQ = @[amdt_seq]
                                           AND A.SVC_SCP_CD = @[svc_scp_cd]
                                           AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                                           AND A.PROP_NO = K.PROP_NO
                                           AND A.AMDT_SEQ = K.AMDT_SEQ
                                           AND A.SVC_SCP_CD = K.SVC_SCP_CD
                                           AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                                           AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ))
                         WHERE LEVEL = CNT
                        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ AND PRIOR PART_SEQ = PART_SEQ
                               AND PRIOR RN = RN - 1 
                       )
                       GROUP BY 
                               PROP_NO
                              ,AMDT_SEQ
                              ,SVC_SCP_CD
                              ,GEN_SPCL_RT_TP_CD
                              ,CMDT_HDR_SEQ                   
       )) B
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,SUBSTR(SYS_CONNECT_BY_PATH(CUST_LGL_ENG_NM || SRC_INFO_CD, '|||'), 4) AS CUST_LGL_ENG_NM
          FROM (SELECT /*+ LEADING(K) */
                       A.PROP_NO
                      ,A.AMDT_SEQ
                      ,A.SVC_SCP_CD
                      ,A.GEN_SPCL_RT_TP_CD
                      ,A.CMDT_HDR_SEQ
                      ,(SELECT CUST_LGL_ENG_NM
                          FROM MDM_CUSTOMER
                         WHERE CUST_CNT_CD = A.CUST_CNT_CD
                           AND CUST_SEQ = A.CUST_SEQ
                           AND ROWNUM = 1) AS CUST_LGL_ENG_NM
                      ,PRC_PROG_STS_CD
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = SRC_INFO_CD
                              AND ROWNUM = 1)
                         ELSE
                          ''
                       END AS SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ ORDER BY A.ACT_CUST_SEQ) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ) AS CNT
                  FROM PRI_SP_SCP_RT_ACT_CUST A, ACPT_KEY_HDR K
                 WHERE A.PROP_NO = @[prop_no]
                   AND A.AMDT_SEQ = @[amdt_seq]
                   AND A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND A.PROP_NO = K.PROP_NO
                   AND A.AMDT_SEQ = K.AMDT_SEQ
                   AND A.SVC_SCP_CD = K.SVC_SCP_CD
                   AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                   AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ)
         WHERE LEVEL = CNT
        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
               AND PRIOR RN = RN - 1) C
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,SUBSTR(SYS_CONNECT_BY_PATH(NOTE_CTNT || SRC_INFO_CD, '|||'), 4) AS CNOTE_CTNT
          FROM (SELECT /*+ LEADING(K) */
                       A.PROP_NO
                      ,A.AMDT_SEQ
                      ,A.SVC_SCP_CD
                      ,A.GEN_SPCL_RT_TP_CD
                      ,A.CMDT_HDR_SEQ
                      ,A.NOTE_CTNT
                      ,A.PRC_PROG_STS_CD
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                              AND ROWNUM = 1)
                         ELSE
                          ''
                       END AS SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ ORDER BY A.CMDT_NOTE_SEQ) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ) AS CNT
                  FROM PRI_SP_SCP_RT_CNOTE A, ACPT_KEY_HDR K
                 WHERE A.PROP_NO = @[prop_no]
                   AND A.AMDT_SEQ = @[amdt_seq]
                   AND A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND A.PROP_NO = K.PROP_NO
                   AND A.AMDT_SEQ = K.AMDT_SEQ
                   AND A.SVC_SCP_CD = K.SVC_SCP_CD
                   AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                   AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ)
         WHERE LEVEL = CNT
        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
               AND PRIOR RN = RN - 1) D
      ,(SELECT /*+ LEADING(K) */
               ROWNUM, A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ
          FROM PRI_SP_SCP_RT_CMDT_ROUT A, ACPT_KEY_ROUT K
         WHERE A.PROP_NO = @[prop_no]
           AND A.AMDT_SEQ = @[amdt_seq]
           AND A.SVC_SCP_CD = @[svc_scp_cd]
           AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
           AND A.PROP_NO = K.PROP_NO
           AND A.AMDT_SEQ = K.AMDT_SEQ
           AND A.SVC_SCP_CD = K.SVC_SCP_CD
           AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
           AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ
           AND A.ROUT_SEQ = K.ROUT_SEQ) E
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_NM || NVL2(RCV_DE_TERM_CD, '(' || RCV_DE_TERM_CD || ')', '') ||
                                          SRC_INFO_CD
                                         ,'|||')
                     ,4) AS ROUT_PNT_LOC_DEF_NM
          FROM (SELECT /*+ LEADING(K) */
                       A.PROP_NO
                      ,A.AMDT_SEQ
                      ,A.SVC_SCP_CD
                      ,A.GEN_SPCL_RT_TP_CD
                      ,A.CMDT_HDR_SEQ
                      ,A.ROUT_SEQ
                      ,A.ORG_DEST_TP_CD
                      ,A.ROUT_PNT_LOC_DEF_CD
                      ,DECODE(A.ROUT_PNT_LOC_TP_CD
                             ,'G'
                             ,(SELECT PRC_GRP_LOC_DESC
                                FROM PRI_SP_SCP_GRP_LOC
                               WHERE PROP_NO = A.PROP_NO
                                 AND AMDT_SEQ = A.AMDT_SEQ
                                 AND SVC_SCP_CD = A.SVC_SCP_CD
                                 AND PRC_GRP_LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                 AND ROWNUM = 1)
                             ,'L'
                             ,(SELECT LOC_NM
                                FROM MDM_LOCATION
                               WHERE LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                 AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM
                      ,(SELECT INTG_CD_VAL_DP_DESC
                          FROM COM_INTG_CD_DTL
                         WHERE INTG_CD_ID = 'CD02138'
                           AND INTG_CD_VAL_CTNT = A.RCV_DE_TERM_CD
                           AND ROWNUM = 1) AS RCV_DE_TERM_CD
                      ,A.PRC_PROG_STS_CD
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                              AND ROWNUM = 1)
                         ELSE
                          ''
                       END AS SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD ORDER BY DECODE(A.ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), A.ROUT_PNT_LOC_DEF_CD) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD) AS CNT
                  FROM PRI_SP_SCP_RT_ROUT_PNT A, ACPT_KEY_ROUT K
                 WHERE A.PROP_NO = @[prop_no]
                   AND A.AMDT_SEQ = @[amdt_seq]
                   AND A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND A.ORG_DEST_TP_CD = 'O'
                   AND A.PROP_NO = K.PROP_NO
                   AND A.AMDT_SEQ = K.AMDT_SEQ
                   AND A.SVC_SCP_CD = K.SVC_SCP_CD
                   AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                   AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ
                   AND A.ROUT_SEQ = K.ROUT_SEQ)
         WHERE LEVEL = CNT
        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
               AND PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1) F
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD || SRC_INFO_CD, '|||'), 4) AS ROUT_VIA_PORT_DEF_CD
          FROM (SELECT /*+ LEADING(K) */
                       A.PROP_NO
                      ,A.AMDT_SEQ
                      ,A.SVC_SCP_CD
                      ,A.GEN_SPCL_RT_TP_CD
                      ,A.CMDT_HDR_SEQ
                      ,A.ROUT_SEQ
                      ,A.ORG_DEST_TP_CD
                      ,A.ROUT_VIA_PORT_DEF_CD
                      ,A.PRC_PROG_STS_CD
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                              AND ROWNUM = 1)
                         ELSE
                          ''
                       END AS SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD ORDER BY DECODE(A.ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), A.ROUT_VIA_PORT_DEF_CD) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD) AS CNT
                  FROM PRI_SP_SCP_RT_ROUT_VIA A, ACPT_KEY_ROUT K
                 WHERE A.PROP_NO = @[prop_no]
                   AND A.AMDT_SEQ = @[amdt_seq]
                   AND A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND A.ORG_DEST_TP_CD = 'O'
                   AND A.PROP_NO = K.PROP_NO
                   AND A.AMDT_SEQ = K.AMDT_SEQ
                   AND A.SVC_SCP_CD = K.SVC_SCP_CD
                   AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                   AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ
                   AND A.ROUT_SEQ = K.ROUT_SEQ)
         WHERE LEVEL = CNT
        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
               AND PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1) G
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD || SRC_INFO_CD, '|||'), 4) AS ROUT_VIA_PORT_DEF_CD
          FROM (SELECT /*+ LEADING(K) */
                       A.PROP_NO
                      ,A.AMDT_SEQ
                      ,A.SVC_SCP_CD
                      ,A.GEN_SPCL_RT_TP_CD
                      ,A.CMDT_HDR_SEQ
                      ,A.ROUT_SEQ
                      ,A.ORG_DEST_TP_CD
                      ,A.ROUT_VIA_PORT_DEF_CD
                      ,A.PRC_PROG_STS_CD
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                              AND ROWNUM = 1)
                         ELSE
                          ''
                       END AS SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD ORDER BY DECODE(A.ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), A.ROUT_VIA_PORT_DEF_CD) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD) AS CNT
                  FROM PRI_SP_SCP_RT_ROUT_VIA A, ACPT_KEY_ROUT K
