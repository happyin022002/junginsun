                      ,PRC_PROG_STS_CD
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                              AND ROWNUM = 1)
                         ELSE
                          ''
                       END AS SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD ORDER BY DECODE(A.ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), A.ROUT_PNT_LOC_DEF_CD) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ, A.ORG_DEST_TP_CD) AS CNT
                  FROM PRI_SP_SCP_RT_ROUT_PNT A, ACPT_KEY_ROUT K
                 WHERE A.PROP_NO = @[prop_no]
                   AND A.AMDT_SEQ = @[amdt_seq]
                   AND A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND A.ORG_DEST_TP_CD = 'D'
                   AND A.PROP_NO = K.PROP_NO
                   AND A.AMDT_SEQ = K.AMDT_SEQ
                   AND A.SVC_SCP_CD = K.SVC_SCP_CD
                   AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                   AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ
                   AND A.ROUT_SEQ = K.ROUT_SEQ)
         WHERE LEVEL = CNT
        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
               AND PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1) I
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(SYS_CONNECT_BY_PATH(NOTE_CTNT || SRC_INFO_CD, '|||'), 4) AS RNOTE_CTNT
          FROM (SELECT /*+ LEADING(K) */
                       A.PROP_NO
                      ,A.AMDT_SEQ
                      ,A.SVC_SCP_CD
                      ,A.GEN_SPCL_RT_TP_CD
                      ,A.CMDT_HDR_SEQ
                      ,A.ROUT_SEQ
                      ,A.NOTE_CTNT
                      ,A.PRC_PROG_STS_CD
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                              AND ROWNUM = 1)
