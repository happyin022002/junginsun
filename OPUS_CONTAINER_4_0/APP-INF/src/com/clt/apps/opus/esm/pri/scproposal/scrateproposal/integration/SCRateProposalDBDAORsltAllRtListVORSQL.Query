                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ ORDER BY A.ROUT_NOTE_SEQ) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ) AS CNT
                  FROM PRI_SP_SCP_RT_CMDT_RNOTE A, ACPT_KEY_ROUT K
