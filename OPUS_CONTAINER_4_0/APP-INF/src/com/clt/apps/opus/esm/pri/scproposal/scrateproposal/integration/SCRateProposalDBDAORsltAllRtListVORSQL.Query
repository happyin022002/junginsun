                   AND A.SVC_SCP_CD = K.SVC_SCP_CD
                   AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                   AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ
                   AND A.ROUT_SEQ = K.ROUT_SEQ)
         WHERE LEVEL = CNT
        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
               AND PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1) J
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(SYS_CONNECT_BY_PATH(RAT_UT_CD || ' ' || PRC_CGO_TP_CD || ' ' || CURR_CD || ' ' ||
                                          TO_CHAR(PROP_FRT_RT_AMT, '9,999,990.00') || '' || NVL(SRC_INFO_CD,'              ')
                                         ,'|||')
                     ,4) AS RT_CTNT
              ,SUBSTR(SYS_CONNECT_BY_PATH(DECODE(GRI_APPL_TP_CD, 'N', ' ', TO_CHAR(GRI_APPL_AMT, '9,999,999.99')), '|||')
                     ,4) AS GRI_CTNT
          FROM (SELECT /*+ LEADING(K) */
                       A.PROP_NO
                      ,A.AMDT_SEQ
                      ,A.SVC_SCP_CD
                      ,A.GEN_SPCL_RT_TP_CD
                      ,A.CMDT_HDR_SEQ
                      ,A.ROUT_SEQ
                      ,A.RAT_UT_CD
                      ,A.PRC_CGO_TP_CD
                      ,A.CURR_CD
                      ,A.PROP_FRT_RT_AMT
                      ,A.PRC_PROG_STS_CD
                      ,GRI_APPL_TP_CD
                      ,GRI_APPL_AMT
                      ,CASE
                         WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                          (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                             FROM COM_INTG_CD_DTL
                            WHERE INTG_CD_ID = 'CD02064'
                              AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                              AND ROWNUM = 1)
                         ELSE
                          ''
                       END AS SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ ORDER BY A.RT_SEQ) AS RN
                      ,COUNT(*) OVER(PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ, A.ROUT_SEQ) AS CNT
                  FROM PRI_SP_SCP_RT A, ACPT_KEY_ROUT K
                 WHERE A.PROP_NO = @[prop_no]
                   AND A.AMDT_SEQ = @[amdt_seq]
                   AND A.SVC_SCP_CD = @[svc_scp_cd]
                   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND A.PROP_NO = K.PROP_NO
                   AND A.AMDT_SEQ = K.AMDT_SEQ
                   AND A.SVC_SCP_CD = K.SVC_SCP_CD
                   AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
                   AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ
                   AND A.ROUT_SEQ = K.ROUT_SEQ)
         WHERE LEVEL = CNT
        CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
               AND PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1) K
      ,(SELECT /*+ LEADING(K) */
               A.PROP_NO
              ,A.AMDT_SEQ
              ,A.SVC_SCP_CD
              ,A.GEN_SPCL_RT_TP_CD
              ,A.CMDT_HDR_SEQ
              ,A.ROUT_SEQ
              ,A.DIR_CALL_FLG
              ,A.PRC_PROG_STS_CD
              ,CASE
                 WHEN A.AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ AND A.AMDT_SEQ > 0 THEN
                  (SELECT '(' || INTG_CD_VAL_DP_DESC || ')'
                     FROM COM_INTG_CD_DTL
                    WHERE INTG_CD_ID = 'CD02064'
                      AND INTG_CD_VAL_CTNT = A.SRC_INFO_CD
                      AND ROWNUM = 1)
                 ELSE
                  ''
               END AS SRC_INFO_CD
          FROM PRI_SP_SCP_RT_ROUT_DIR A, ACPT_KEY_ROUT K
         WHERE A.PROP_NO = @[prop_no]
           AND A.AMDT_SEQ = @[amdt_seq]
           AND A.SVC_SCP_CD = @[svc_scp_cd]
           AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
           AND A.PROP_NO = K.PROP_NO
           AND A.AMDT_SEQ = K.AMDT_SEQ
           AND A.SVC_SCP_CD = K.SVC_SCP_CD
           AND A.GEN_SPCL_RT_TP_CD = K.GEN_SPCL_RT_TP_CD
           AND A.CMDT_HDR_SEQ = K.CMDT_HDR_SEQ
           AND A.ROUT_SEQ = K.ROUT_SEQ) L
 WHERE A.PROP_NO = B.PROP_NO(+)
   AND A.AMDT_SEQ = B.AMDT_SEQ(+)
