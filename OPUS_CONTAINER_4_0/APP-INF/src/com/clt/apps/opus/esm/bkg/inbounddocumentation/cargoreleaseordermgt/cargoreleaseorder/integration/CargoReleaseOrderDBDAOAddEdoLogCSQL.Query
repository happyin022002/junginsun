<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOAddEdoLogCSQL">
			<desc><![CDATA[Edo Log 입력]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_EDO_LOG (
 BKG_NO
, EDO_SEQ
, MF_REF_NO
, MF_SEQ_NO
, EDO_RSLT_CD
, EDO_RCV_DT
, ACK_MSG
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
) 
SELECT BKG_NO
     , (SELECT NVL(MAX(EDO_SEQ)+1, 1) FROM BKG_EDO_LOG WHERE BKG_NO = MSN.BKG_NO )
     , @[mf_ref_no]
     , @[mf_seq_no]
     , @[edo_rslt_cd]
     , TO_DATE(@[edo_rcv_dt],'YYYYMMDDHH24MISS')
     , @[ack_msg]
     , @[cre_usr_id]
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
FROM BKG_CSTMS_KR_MF_SEQ_NO MSN
WHERE MF_REF_NO = SUBSTR(@[mf_ref_no],1,10)
  AND MRN_CHK_NO = SUBSTR(@[mf_ref_no],11,1)
  AND MF_SEQ_NO  = @[mf_seq_no]			]]></sql>
			<params>
				<param name="mf_ref_no" type="12" value="" out="N"/>
				<param name="mf_seq_no" type="12" value="" out="N"/>
				<param name="edo_rslt_cd" type="12" value="" out="N"/>
				<param name="edo_rcv_dt" type="12" value="" out="N"/>
				<param name="ack_msg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
