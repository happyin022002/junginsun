<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPMasterDBDAOCheckOmPortMngRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(CASE WHEN @[eff_fm_dt] BETWEEN EFF_FM_DT AND EFF_TO_DT THEN 1 
                    WHEN @[eff_to_dt] BETWEEN EFF_FM_DT AND EFF_TO_DT THEN 1 
                    ELSE 0 END),0) AS CNT
   FROM COA_MON_VVD_PORT_EXPT
  WHERE LOC_CD     = @[loc_cd]			--20150612.MOD	
    AND RLANE_CD   = NVL(@[rlane_cd],'XXXXX')
    AND DIR_CD     = NVL(@[dir_cd],'X')			]]></sql>
			<params>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
