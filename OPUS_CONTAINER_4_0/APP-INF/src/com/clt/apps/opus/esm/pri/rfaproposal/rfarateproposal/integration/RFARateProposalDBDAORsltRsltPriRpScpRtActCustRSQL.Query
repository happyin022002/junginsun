<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltRsltPriRpScpRtActCustRSQL">
			<desc><![CDATA[SEARCH PRI_RP_SCP_RT_ACT_CUST]]></desc>
			<sql><![CDATA[
SELECT
       PROP_NO
     , AMDT_SEQ
     , SVC_SCP_CD
     , CMDT_HDR_SEQ
     , ACT_CUST_SEQ
     , CUST_CNT_CD
     , CUST_SEQ
     , PRC_PROG_STS_CD
     , SRC_INFO_CD
     , N1ST_CMNC_AMDT_SEQ
     , ACPT_USR_ID
     , ACPT_OFC_CD
     , ACPT_DT
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
  FROM PRI_RP_SCP_RT_ACT_CUST
 WHERE PROP_NO             = @[prop_no]
   AND AMDT_SEQ            IN (@[amdt_seq], TO_NUMBER(@[amdt_seq])-1)
   AND SVC_SCP_CD          = @[svc_scp_cd]
   AND CMDT_HDR_SEQ        = @[cmdt_hdr_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
