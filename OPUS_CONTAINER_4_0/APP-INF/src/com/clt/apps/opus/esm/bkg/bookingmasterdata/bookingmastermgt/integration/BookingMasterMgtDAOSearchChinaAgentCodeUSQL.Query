<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDAOSearchChinaAgentCodeUSQL">
			<desc><![CDATA[중국 Booking Agent 정보 등록 화면]]></desc>
			<sql><![CDATA[
UPDATE BKG_CHN_AGN SET 
	CHN_AGN_CD = @[chn_agn_cd]
,	AGN_NM = @[agn_nm]
,	DIFF_RMK = @[diff_rmk]
,	VNDR_CNT_CD = @[vndr_cnt_cd]
,	VNDR_SEQ = @[vndr_seq]
,	CUST_CNT_CD = @[cust_cnt_cd]
,	CUST_SEQ = @[cust_seq]
,	AUTO_DP_CHK_FLG = DECODE(@[auto_dp_chk_flg],'1','Y','0','N',@[auto_dp_chk_flg])
,	AGN_EML = @[agn_eml]
,	OFC_CD = @[ofc_cd]
,	FINC_OFC_CD = @[finc_ofc_cd]
,	AR_OFC_CD = @[ar_ofc_cd]
,	DIR_PAY_OFC_CD = @[dir_pay_ofc_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,   BKG_BLCK_FLG = @[bkg_blck_flg]
WHERE	CHN_AGN_CD = @[chn_agn_cd]			]]></sql>
			<params>
				<param name="chn_agn_cd" type="12" value="" out="N"/>
				<param name="agn_nm" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="auto_dp_chk_flg" type="12" value="" out="N"/>
				<param name="agn_eml" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="finc_ofc_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="dir_pay_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_blck_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
