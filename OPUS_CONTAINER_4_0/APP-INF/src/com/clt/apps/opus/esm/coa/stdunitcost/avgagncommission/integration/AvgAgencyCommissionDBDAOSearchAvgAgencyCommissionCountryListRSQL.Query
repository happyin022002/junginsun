<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AvgAgencyCommissionDBDAOSearchAvgAgencyCommissionCountryListRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT A.COMM_YRMON
     , A.CNT_CD
     , A.IO_BND_CD
     , A.AC_TP_CD
     , (SELECT M.AC_CD_RMK
          FROM ACM_COMM_TP_CD_MAPG M
         WHERE 1=1
           AND M.COMM_TP_CD = 'C'
           AND M.AC_TP_CD   <> 'T'
           AND M.AC_TP_CD   = A.AC_TP_CD 
           AND ROWNUM       = 1
       ) AS AC_TP_NM
     , A.CNTR_TPSZ_CD
     , A.COMM_STND_COST_CD
     , (SELECT M.COA_COST_SRC_CD_NM
          FROM COA_COST_SRC_ACCT M
         WHERE M.SGRP_COST_CD       = 'CVAC'
           AND M.STND_COST_CD       <> '51401014'
           AND M.COA_COST_SRC_CD    = A.COMM_STND_COST_CD 
           AND ROWNUM       = 1
       ) AS COMM_STND_COST_NM
     , A.TRD_CD
     , A.SUB_TRD_CD
     , A.BKG_QTY
     , A.COMM_TTL_AMT
     , A.COMM_UT_AMT
  FROM ACM_COMM_CNT_UT_COST A
 WHERE 1=1
   AND A.COMM_YRMON = REPLACE(@[f_cost_yrmon],'-','')			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
