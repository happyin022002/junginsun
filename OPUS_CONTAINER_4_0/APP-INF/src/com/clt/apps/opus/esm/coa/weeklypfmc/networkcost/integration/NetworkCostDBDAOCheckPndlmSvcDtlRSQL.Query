<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOCheckPndlmSvcDtlRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(CASE WHEN @[eff_fm_dt] BETWEEN EFF_FM_DT AND EFF_TO_DT
                    THEN 1 ELSE 0 END),0) AS CNT
   FROM COA_PNDLM_SVC 
  WHERE 1=1
    AND SLAN_CD   = @[slan_cd]
    AND DIR_CD    = @[dir_cd]			]]></sql>
			<params>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
