<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyResultCdByBlUSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신, IBD테이블에 수신 결과 셋업.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_IBD
   SET CSTMS_CLR_RSLT_CD = 'R'

 WHERE CNT_CD = 'US'
   AND BL_NO = DECODE(@[ibd_rst_update_flg], 'HJBL', @[in_hjbl], @[bl_no])			]]></sql>
			<params>
				<param name="ibd_rst_update_flg" type="12" value="" out="N"/>
				<param name="in_hjbl" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
