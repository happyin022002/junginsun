<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchDgInfoAtLocalRSQL">
			<desc><![CDATA[구주위험물 테이블들에서 데이타를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT DENSE_RANK() OVER (ORDER BY POL_CD, POD_CD, BL_NO, CGO_OPR_CD) AS SEQ
      ,CNTR_CNT            -- TOTAL CONTAINER COUNT
      ,CGO_OPR_CD
      ,BL_NO MERGE_BL_NO 
      ,BL_NO              -- BL_NO
      ,BKG_NO
      ,POL_CD             -- POL_CD
      ,POD_CD             -- POD_CD
      ,CNTR_NO            -- Container No
      ,CNTR_CGO_SEQ       -- Container Cargo Seq
      #if(${auto_update}=='Y')
      ,CASE WHEN CELL_PSN_NO = NVL(CELL_PSN_NO_OPF, CELL_PSN_NO) 
       THEN CELL_PSN_NO ELSE CELL_PSN_NO_OPF
       END CELL_PSN_NO -- 저장 후 CELL_POSITION이 UPDATE되면 BAY_PLAN에 있는 CELL_POSITION을 가지고 온다.
      ,CASE WHEN NVL(CELL_PSN_NO,1) = NVL(CELL_PSN_NO_OPF, NVL(CELL_PSN_NO,1)) 
       THEN 1 ELSE 2 -- CELL POSITION UPDATE를 나타내기 위한 FLAG
       END CELL_CHK
      #else
      ,CELL_PSN_NO
      ,1 as CELL_CHK
      #end
      ,IMDG_CLSS_CD       -- Class
      ,IMDG_UN_NO         -- UN No.
      ,IMDG_UN_NO_SEQ     -- UN No SEQ.
      ,IMDG_COMP_GRP_CD
      ,ANR_SPCL_TP_ID AS DG_SHORT_NM     -- S.D/G
      ,FLSH_PNT_CDO_TEMP                  -- Flash Point
      ,ANR_ROLE_DIV_CD AS AGENT          -- Agent
      ,ANR_CRR_TP_CD AS C_TYPE             -- Carriage Type
      ,FDR_SVC_RQST_NO    -- SSR(Feeder)
      ,IMDG_PCK_GRP_CD    -- Package Group
      ,OUT_IMDG_PCK_QTY1  -- Outer Package QTY
      ,OUT_IMDG_PCK_CD1   -- Outer Package Code
      ,EUR_OUTR_PCK_DESC  -- Outer Package Desc
      ,IN_IMDG_PCK_QTY1
      ,IN_IMDG_PCK_CD1 
      ,EUR_INR_PCK_DESC
      ,EMS_NO             -- EMS
      ,NET_WGT            -- Net Weight
      ,GRS_WGT            -- Gross Weight
      ,EUR_PCK_DESC AS PACKAGES                     -- Packages
      ,PRP_SHP_NM                                 -- Substance
      ,HZD_DESC                                   -- Hazardous Contents
      ,ANR_FWRD_ID AS FWRD_ID                     -- Forwarder Code
      ,FWRD_NM                                      -- Forwarder Name
      ,EUR_DCGO_MRN_POLUT_CD                       -- Marine Pollutant
      ,IMDG_LMT_QTY_FLG                            -- Limited quantity
      ,FDR_VSL_NM           -- Feeder Name
      ,FDR_VSL_LLOYD_NO     -- Feeder Lloyd No
      ,FDR_VVD_ID           -- Feeder VVD
      ,TO_CHAR(CRR_DT, 'YYYYMMDD')    CRR_DT       -- on carriage date
      ,MSG_FUNC_ID SEND_TYPE
      ,SCR_FILE_NO SCR_FILE_NO
      ,MSG_SND_NO MSG_SND_NO
      ,FIRST_MSG_SND_NO
      ,COUNT(DISTINCT BL_NO) OVER(PARTITION BY FIRST_MSG_SND_NO ) AS GROUP_CNT
      ,NET_EXPLO_WGT
      ,CNTR_TPSZ_CD
      ,CNTR_NO AS CNTR_NO_OLD
      ,(SELECT DECODE(@[d_type], 'L', STUP.LOD_CD, 'T', STUP.TZ_CD, 'D', STUP.DCHG_CD, '') STUP_FLG
          FROM BKG_CSTMS_EUR_DG_SND_STUP STUP, MDM_VSL_CNTR CNTR
         WHERE 1=1
           AND STUP.CRR_CD = CNTR.CRR_CD
     AND CNTR.VSL_CD = SUBSTR(@[vvd_cd],1,4)
           AND STUP.PORT_CD     = @[port_cd]
           AND ROWNUM = 1
       ) AS STUP_FLG
      ,CNTR_REF_NO
      ,DCGO_REF_NO
      ,EMER_CNTC_PHN_NO
      ,EMER_CNTC_PSON_NM
      ,IMDG_SUBS_RSK_LBL_CD1
      ,IMDG_SUBS_RSK_LBL_CD2
      ,IMDG_SUBS_RSK_LBL_CD3
      ,IMDG_SUBS_RSK_LBL_CD4
      ,MFAG_NO
      ,DIFF_RMK
      ,HCDG_FLG
      ,EMER_RSPN_GID_NO
      ,XTD_STAY_PRMT_NO
      ,SPCL_CNTR_SEQ
      ,SPCL_CGO_SEQ
  FROM (SELECT A.VSL_CD
              ,COUNT(DISTINCT A.CNTR_NO) OVER() CNTR_CNT
              ,A.BL_NO              -- BL_NO
              ,C.BKG_NO
              ,A.POL_CD             -- POL_CD
              ,A.POD_CD             -- POD_CD
              ,A.CNTR_NO            -- Container No
              ,A.CNTR_CGO_SEQ          -- Container Cargo Seq
              ,A.EUR_OUTR_PCK_DESC
              ,A.IN_IMDG_PCK_QTY1
              ,A.IN_IMDG_PCK_CD1
              ,A.EUR_INR_PCK_DESC
#if (${bay_pln_id} != '')
              ,NVL(BAY.CELL_PSN_NO, A.CELL_PSN_NO) AS CELL_PSN_NO
#else
              ,A.CELL_PSN_NO
#end
              ,A.IMDG_CLSS_CD       -- Class
              ,A.IMDG_UN_NO         -- UN No.
              ,A.IMDG_UN_NO_SEQ     -- UN No SEQ.
              ,SIUN.IMDG_COMP_GRP_CD
              ,A.ANR_SPCL_TP_ID     -- S.D/G
              ,A.FLSH_PNT_CDO_TEMP  -- Flash Point
              ,A.ANR_ROLE_DIV_CD    -- Agent
              ,A.ANR_CRR_TP_CD      -- Carriage Type
              ,A.FDR_SVC_RQST_NO    -- SSR(Feeder)
              ,A.IMDG_PCK_GRP_CD    -- Package Group
              ,A.OUT_IMDG_PCK_QTY1  -- Outer Package QTY
              ,A.OUT_IMDG_PCK_CD1   -- Outer Package Code
              ,A.EMS_NO             -- EMS
              ,A.NET_WGT            -- Net Weight
              ,A.GRS_WGT            -- Gross Weight
              ,A.EUR_PCK_DESC       -- Packages
              ,A.PRP_SHP_NM         -- Substance
              ,A.HZD_DESC           -- Hazardous Contents
              ,A.ANR_FWRD_ID
              ,B.FWRD_NM            -- Forwarder Name
              ,A.EUR_DCGO_MRN_POLUT_CD
              ,A.IMDG_LMT_QTY_FLG
              ,A.FDR_VSL_NM           -- Feeder Name
              ,A.FDR_VSL_LLOYD_NO     -- Feeder Lloyd No
              ,A.FDR_VVD_ID           -- Feeder VVD
              ,A.CRR_DT               -- on carriage date
              ,DECODE(LOG.MSG_FUNC_ID, 'O', DECODE(LOG.ACK_RCV_STS_CD, 'R', '', LOG.MSG_FUNC_ID), LOG.MSG_FUNC_ID) MSG_FUNC_ID
              ,LOG.MSG_SND_NO
              ,F_LOG.SCR_FILE_NO
              ,F_LOG.MSG_SND_NO   FIRST_MSG_SND_NO
              ,A.NET_EXPLO_WGT
              ,A.CGO_OPR_CD
              ,A.CNTR_TPSZ_CD
              ,A.CNTR_REF_NO
              ,A.DCGO_REF_NO
              ,A.EMER_CNTC_PHN_NO
              ,A.EMER_CNTC_PSON_NM
              ,A.IMDG_SUBS_RSK_LBL_CD1
              ,A.IMDG_SUBS_RSK_LBL_CD2
              ,A.IMDG_SUBS_RSK_LBL_CD3
              ,A.IMDG_SUBS_RSK_LBL_CD4
              ,A.MFAG_NO
              ,A.DIFF_RMK
              ,A.HCDG_FLG
              ,A.EMER_RSPN_GID_NO
              ,A.XTD_STAY_PRMT_NO
              ,A.SPCL_CNTR_SEQ
              ,A.SPCL_CGO_SEQ
              ,(SELECT LPAD(X.VSL_BAY_NO || X.VSL_ROW_NO || X.VSL_TR_NO, 7,0 )
                FROM OPF_BAY_PLN_LDIS X, VSK_VSL_PORT_SKD PS
                WHERE PS.VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
                AND PS.SKD_VOY_NO=SUBSTR(@[vvd_cd], 5, 4)
                AND PS.SKD_DIR_CD =SUBSTR(@[vvd_cd], 9, 1)
                AND X.VSL_CD = PS.VSL_CD
                AND CASE WHEN PS.TURN_PORT_IND_CD IN ('Y','N') THEN PS.SKD_VOY_NO ELSE PS.TURN_SKD_VOY_NO END = X.SKD_VOY_NO
                AND CASE WHEN PS.TURN_PORT_IND_CD IN ('Y','N') THEN PS.SKD_DIR_CD ELSE PS.TURN_SKD_DIR_CD END = X.SKD_DIR_CD
                AND X.VPS_PORT_CD = PS.VPS_PORT_CD
                AND CASE WHEN PS.TURN_PORT_IND_CD IN ('Y','N') THEN PS.CLPT_IND_SEQ ELSE PS.TURN_CLPT_IND_SEQ END = X.CLPT_IND_SEQ
                AND X.CNTR_REF_NO =A.CNTR_NO
                AND X.LODG_DCHG_IND_CD = 'C') AS CELL_PSN_NO_OPF

			  
          FROM BKG_CSTMS_EUR_DG A
              ,BKG_CSTMS_EUR_DG_FWRD B
              ,SCG_IMDG_UN_NO SIUN
              ,BKG_BOOKING C
#if (${bay_pln_id} != '')
              ,(SELECT A.BAY_PLN_ID
                      ,A.VSL_CD
                      ,A.ETA_DT
                      ,A.ETD_DT
                      ,B.EUR_DG_CNTR_ID CNTR_NO
                      ,B.CELL_PSN_NO
                      ,B.POL_CD
                      ,B.POD_CD
                  FROM BKG_CSTMS_EUR_DG_BAY_PLN A
                      ,BKG_CSTMS_EUR_DG_BAY_DTL B
                 WHERE A.BAY_PLN_ID = B.BAY_PLN_ID
                   AND A.VSL_CD            = SUBSTR(@[vvd_cd], 1, 4)
                   AND A.VSL_VOY_DIR_NO    = SUBSTR(@[vvd_cd], 5, 5)
                   AND B.EUR_DG_CNTR_ID > ' '
                   AND B.BAY_PLN_ID  = @[bay_pln_id]
               ) BAY
#end
#if (${ui_type} == 'ESM_BKG_0965')
              ,(SELECT A.EUR_DG_DECL_TP_CD
                      ,A.VSL_CD
                      ,A.SKD_VOY_NO
                      ,A.SKD_DIR_CD
                      ,A.PORT_CD
                      ,B.BL_NO
                      ,B.CNTR_NO
                      ,B.CNTR_CGO_SEQ
                      ,A.MSG_FUNC_ID
                      ,B.MSG_SND_NO
                      ,B.EDI_RSPN_SEQ
                      ,C.ACK_RCV_STS_CD
                  FROM BKG_CSTMS_EUR_DG_SND A
                      ,BKG_CSTMS_EUR_DG_EDI_RSPN B
                      ,BKG_CSTMS_EUR_DG_RCV C
                 WHERE A.EUR_DG_DECL_TP_CD = @[d_type]
                   AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                   AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                   AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                   AND A.PORT_CD     = @[port_cd]
                   AND A.EUR_EDI_MSG_TP_ID ='IFD'
                   AND A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                   AND A.MSG_SND_NO = B.MSG_SND_NO
                   AND A.EUR_EDI_MSG_TP_ID= C.EUR_EDI_MSG_TP_ID(+)
                   AND A.MSG_SND_NO = C.MSG_RCV_NO(+)
                   AND (B.BL_NO, B.MSG_SND_NO) IN (SELECT B.BL_NO, MAX(B.MSG_SND_NO)
                                                     FROM BKG_CSTMS_EUR_DG_SND A
                                                         ,BKG_CSTMS_EUR_DG_EDI_RSPN B
                                                    WHERE A.EUR_DG_DECL_TP_CD = @[d_type]
                                                      AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                                                      AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                                                      AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                                                      AND A.PORT_CD     = @[port_cd]
                                                      AND A.EUR_EDI_MSG_TP_ID ='IFD'
                                                      AND A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                                                      AND A.MSG_SND_NO = B.MSG_SND_NO
                                                    GROUP BY A.EUR_EDI_MSG_TP_ID, B.BL_NO
                                                  )
               ) LOG
              ,(SELECT A.EUR_DG_DECL_TP_CD
                      ,A.VSL_CD
                      ,A.SKD_VOY_NO
                      ,A.SKD_DIR_CD
                      ,A.PORT_CD
                      ,B.BL_NO
                      ,B.CNTR_NO
                      ,B.CNTR_CGO_SEQ
                      ,A.MSG_FUNC_ID
                      ,B.MSG_SND_NO
                      ,B.EDI_RSPN_SEQ
                      ,(SELECT /*+ INDEX_DESC(BKG_CSTMS_EUR_DG_RCV XPKBKG_CSTMS_EUR_DG_RCV)*/ SCR_FILE_NO
                          FROM BKG_CSTMS_EUR_DG_RCV
                         WHERE EUR_EDI_MSG_TP_ID = 'IFD'
                           AND MSG_RCV_NO = A.MSG_SND_NO
                           AND ROWNUM = 1
                       ) SCR_FILE_NO
                  FROM BKG_CSTMS_EUR_DG_SND A
                      ,BKG_CSTMS_EUR_DG_EDI_RSPN B
                 WHERE A.EUR_DG_DECL_TP_CD = @[d_type]
                   AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                   AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                   AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                   AND A.PORT_CD     = @[port_cd]
                   AND A.EUR_EDI_MSG_TP_ID ='IFD'
                   AND A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                   AND A.MSG_SND_NO = B.MSG_SND_NO
                   AND A.MSG_FUNC_ID = 'O'
                   AND (B.BL_NO, B.MSG_SND_NO) IN (SELECT B.BL_NO, MAX(B.MSG_SND_NO)
                                                     FROM BKG_CSTMS_EUR_DG_SND A
                                                         ,BKG_CSTMS_EUR_DG_EDI_RSPN B
                                                    WHERE A.EUR_DG_DECL_TP_CD = @[d_type]
                                                      AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                                                      AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                                                      AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                                                      AND A.PORT_CD     = @[port_cd]
                                                      AND A.EUR_EDI_MSG_TP_ID ='IFD'
                                                      AND A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                                                      AND A.MSG_SND_NO = B.MSG_SND_NO
                                                      AND A.MSG_FUNC_ID = 'O'
                                                    GROUP BY A.EUR_EDI_MSG_TP_ID, B.BL_NO
                                                  )
               ) F_LOG
#else
            -- 0966
              ,(SELECT A.EUR_DG_DECL_TP_CD
                      ,A.VSL_CD
                      ,A.SKD_VOY_NO
                      ,A.SKD_DIR_CD
                      ,A.PORT_CD
                      ,A.MSG_FUNC_ID
                      ,A.MSG_SND_NO
                      ,B.ACK_RCV_STS_CD
                  FROM BKG_CSTMS_EUR_DG_SND A
                      ,BKG_CSTMS_EUR_DG_RCV B
                 WHERE A.EUR_EDI_MSG_TP_ID= B.EUR_EDI_MSG_TP_ID(+)
                   AND A.MSG_SND_NO = B.MSG_RCV_NO(+)
                   AND A.EUR_DG_DECL_TP_CD = @[d_type]
                   AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                   AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                   AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                   AND A.PORT_CD     = @[port_cd]
                   AND A.EUR_EDI_MSG_TP_ID ='IFD'
                   AND B.RCV_LOG_SEQ = (SELECT MAX(R.RCV_LOG_SEQ)
                                          FROM BKG_CSTMS_EUR_DG_RCV R
                                         WHERE R.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                                           AND R.MSG_RCV_NO        = B.MSG_RCV_NO
                                       )
                   AND A.MSG_SND_NO = (SELECT MAX(MSG_SND_NO)
                                       FROM BKG_CSTMS_EUR_DG_SND A
                                       WHERE 1=1
                                       AND A.EUR_DG_DECL_TP_CD = @[d_type]
                 				       AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                                       AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                                       AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                                       AND A.PORT_CD     = @[port_cd]
                                       AND A.EUR_EDI_MSG_TP_ID ='IFD')
               ) LOG
             , (SELECT A.EUR_DG_DECL_TP_CD
                      ,A.VSL_CD
                      ,A.SKD_VOY_NO
                      ,A.SKD_DIR_CD
                      ,A.PORT_CD
                      ,A.MSG_FUNC_ID
                      ,A.MSG_SND_NO
                      ,B.SCR_FILE_NO
                  FROM BKG_CSTMS_EUR_DG_SND A
                      ,BKG_CSTMS_EUR_DG_RCV B
                 WHERE A.EUR_EDI_MSG_TP_ID= B.EUR_EDI_MSG_TP_ID(+)
                   AND A.MSG_SND_NO = B.MSG_RCV_NO(+)
                   AND A.EUR_DG_DECL_TP_CD = @[d_type]
                   AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                   AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                   AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                   AND A.PORT_CD     = @[port_cd]
                   AND A.EUR_EDI_MSG_TP_ID ='IFD'
                   AND B.RCV_LOG_SEQ = (SELECT MAX(R.RCV_LOG_SEQ)
                                          FROM BKG_CSTMS_EUR_DG_RCV R
                                         WHERE R.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                                           AND R.MSG_RCV_NO        = B.MSG_RCV_NO
                                        )
 				   AND A.MSG_SND_NO = (SELECT MAX(MSG_SND_NO) 
                                       FROM BKG_CSTMS_EUR_DG_SND A
                                       WHERE 1=1
                                       AND A.EUR_DG_DECL_TP_CD = @[d_type]
                 				       AND A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                                       AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                                       AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                                       AND A.PORT_CD     = @[port_cd]
                                       AND A.EUR_EDI_MSG_TP_ID ='IFD')
               ) F_LOG
#end
         WHERE A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
           AND A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
           AND A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
           AND A.PORT_CD     = @[port_cd]
           AND A.EUR_DG_DECL_TP_CD = @[d_type]
#if (${cgo_opr} != '')
           AND A.CGO_OPR_CD = @[cgo_opr]
#end
           AND A.BL_NO         = C.BL_NO(+)
           AND A.ANR_FWRD_ID = B.ANR_FWRD_ID(+)
#if (${bay_pln_id} != '')
           AND A.VSL_CD      = BAY.VSL_CD(+)
           AND A.POL_CD      = BAY.POL_CD(+)
           AND A.POD_CD      = BAY.POD_CD(+)
           AND A.CNTR_NO     = BAY.CNTR_NO(+)
#end
           AND A.EUR_DG_DECL_TP_CD = LOG.EUR_DG_DECL_TP_CD(+)
           AND A.VSL_CD      = LOG.VSL_CD(+)
           AND A.SKD_VOY_NO  = LOG.SKD_VOY_NO(+)
           AND A.SKD_DIR_CD  = LOG.SKD_DIR_CD(+)
           AND A.PORT_CD     = LOG.PORT_CD(+)
           AND A.EUR_DG_DECL_TP_CD = F_LOG.EUR_DG_DECL_TP_CD(+)
           AND A.VSL_CD      = F_LOG.VSL_CD(+)
           AND A.SKD_VOY_NO  = F_LOG.SKD_VOY_NO(+)
           AND A.SKD_DIR_CD  = F_LOG.SKD_DIR_CD(+)
           AND A.PORT_CD     = F_LOG.PORT_CD(+)
           AND A.IMDG_UN_NO    =     SIUN.IMDG_UN_NO(+)
           AND A.IMDG_UN_NO_SEQ = SIUN.IMDG_UN_NO_SEQ(+)
   		   #if(${cargo_oper_cd} != '')
	       AND A.CGO_OPR_CD = @[cargo_oper_cd]
	       #end
  		   #if(${pol_code} != '')
	       AND A.POL_CD = @[pol_code]
	       #end
	       #if(${pod_code} != '')
	       AND A.POD_CD = @[pod_code]
	       #end
	       #if(${bl_number} != '')
	       AND A.BL_NO = @[bl_number]
	       #end
#if (${ui_type} == 'ESM_BKG_0965')
           AND A.BL_NO       = LOG.BL_NO(+)
           AND A.CNTR_NO     = LOG.CNTR_NO(+)
           AND A.CNTR_CGO_SEQ = LOG.CNTR_CGO_SEQ(+)
           AND A.BL_NO       = F_LOG.BL_NO(+)
           AND A.CNTR_NO     = F_LOG.CNTR_NO(+)
           AND A.CNTR_CGO_SEQ = F_LOG.CNTR_CGO_SEQ(+)
#end
        ) SUB1
WHERE 1=1
ORDER BY   SEQ, POL_CD,POD_CD,BL_NO,DCGO_REF_NO,CGO_OPR_CD			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="bay_pln_id" type="12" value="" out="N"/>
				<param name="cgo_opr" type="12" value="" out="N"/>
				<param name="cargo_oper_cd" type="12" value="" out="N"/>
				<param name="pol_code" type="12" value="" out="N"/>
				<param name="pod_code" type="12" value="" out="N"/>
				<param name="bl_number" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
