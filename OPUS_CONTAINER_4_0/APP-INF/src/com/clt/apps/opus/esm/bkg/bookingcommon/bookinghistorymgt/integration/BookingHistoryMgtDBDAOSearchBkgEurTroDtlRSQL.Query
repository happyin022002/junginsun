			<sql><![CDATA[
SELECT BKG_NO
, IO_BND_CD
, TRO_SEQ
, TRO_SUB_SEQ
, DOR_ADDR_TP_CD
, LOC_CD
, ZN_CD
, LOD_REF_NO
, DOR_ZIP_ID
, DOR_ADDR
, TO_CHAR(ARR_DT, 'YYYY-MM-DD HH24:MI:SS') ARR_DT
, CNTC_PSON_NM
, CNTC_PHN_NO
, CNTC_EML
, CRE_USR_ID
, TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, UPD_USR_ID
, TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
  FROM BKG_EUR_TRO_DTL
 WHERE BKG_NO = @[bkg_no]			]]></sql>
