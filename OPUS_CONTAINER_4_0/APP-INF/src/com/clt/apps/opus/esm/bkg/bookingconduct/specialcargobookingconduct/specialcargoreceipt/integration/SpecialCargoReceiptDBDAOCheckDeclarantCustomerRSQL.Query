<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOCheckDeclarantCustomerRSQL">
			<desc><![CDATA[container 별 DG Declarant  존재여부 체크]]></desc>
			<sql><![CDATA[
SELECT DECODE(count(*),0,'N','Y') AS CHECK_YN
FROM BKG_DG_DECL
WHERE BKG_NO = @[bkg_no]
AND DG_CNTR_SEQ = @[dg_cntr_seq]
AND BKG_CUST_TP_CD = @[bkg_cust_tp_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dg_cntr_seq" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
