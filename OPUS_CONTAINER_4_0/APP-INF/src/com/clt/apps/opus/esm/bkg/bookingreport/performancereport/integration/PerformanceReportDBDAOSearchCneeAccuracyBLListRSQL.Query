<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchCneeAccuracyBLListRSQL">
			<desc><![CDATA[Doc Performance Report
Documentation 실적 산출 기능
Cnee Accuracy B/L 조회]]></desc>
			<sql><![CDATA[
SELECT 
    A.BKG_NO,
    A.BL_NO,
    A.CLASS_TYPE,
    A.BKG_DT,
    A.BKG_STF,
    A.BKG_OFC_CD,
    A.OB_SLS_OFC_CD,
	A.POR_CD,
    A.POL_CD,
    A.POD_CD,
    A.SLAN_CD,
    A.BKG_STS,
    A.VVD_CD,
    A.SKD_VOY_NO,
    A.VSL_CD,
    A.SKD_DIR_CD,
	A.POD_ETA_DT DOCP_DT,
    A.BKG_DOC_DT PORT_CLZ_DT,
	'' DPCS_SMRY_RMK,
    A.VAL_DT BATCH_DT
FROM 
(
SELECT BK.BKG_NO
      ,BK.BL_NO
      ,'10' CLASS_TYPE
      ,BK.BKG_CRE_DT BKG_DT
      ,BK.DOC_USR_ID BKG_STF
      ,BK.BKG_OFC_CD
      ,BK.OB_SLS_OFC_CD
	  ,BK.POR_CD
      ,BK.POL_CD
      ,BK.POD_CD
      ,BK.SLAN_CD
      ,	CASE WHEN BC.MTCH_FLG = 'Y' AND BC.VAL_CD IS NULL THEN 'Auto'
			 ELSE (SELECT CD.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL CD WHERE CD.INTG_CD_ID = 'CD01655' AND CD.INTG_CD_VAL_CTNT = BC.VAL_CD) 
		END BKG_STS			
      ,BK.VSL_CD || BK.SKD_VOY_NO || BK.SKD_DIR_CD VVD_CD
      ,BK.SKD_VOY_NO
      ,BK.VSL_CD
      ,BK.SKD_DIR_CD
   	  ,TO_CHAR(SKD.VPS_ETA_DT,'YYYY-MM-DD') POD_ETA_DT
      ,'10' DOC_KND_STS_CD
      ,TO_CHAR(BL.BDR_DT,'YYYYMMDD') BKG_DOC_DT
      ,TO_CHAR(BC.VAL_DT,'YYYY-MM-DD HH24:MI:SS') VAL_DT
FROM   BKG_BOOKING BK
     , BKG_VVD VVD
     , VSK_VSL_PORT_SKD SKD
     , BKG_CUSTOMER BC
	 , BKG_BL_DOC BL
 WHERE BK.BKG_NO = VVD.BKG_NO
   AND BK.POD_CD = VVD.POD_CD
   AND BK.BKG_NO = BL.BKG_NO
   AND VVD.VSL_PRE_PST_CD IN ('T','U')
   AND (VVD.VSL_PRE_PST_CD, VVD.VSL_SEQ) = (SELECT /*+ index_desc (v2 xpkbkg_vvd) */ 
                                                    V2.VSL_PRE_PST_CD, V2.VSL_SEQ
                                              FROM BKG_VVD V2
                                             WHERE V2.BKG_NO = BK.BKG_NO
                                               AND V2.VSL_PRE_PST_CD IN ('T','U')
                                               AND ROWNUM = 1)
   AND VVD.VSL_CD     = SKD.VSL_CD
   AND VVD.SKD_VOY_NO = SKD.SKD_VOY_NO
   AND VVD.SKD_DIR_CD = SKD.SKD_DIR_CD
   AND VVD.POD_CD     = SKD.VPS_PORT_CD
   AND VVD.POD_CLPT_IND_SEQ = SKD.CLPT_IND_SEQ
   AND BK.BKG_NO      = BC.BKG_NO
   AND BK.BKG_STS_CD  <> 'X'
   AND BK.BKG_CGO_TP_CD = 'F'
   AND BC.BKG_CUST_TP_CD = DECODE(BK.CUST_TO_ORD_FLG,'Y','N','C')
   AND BC.VAL_DT IS NOT NULL
#if (${fr_dt} != '')                                             
   AND SKD.VPS_ETA_DT >= TO_DATE(@[fr_dt],'YYYY-MM-DD')
#end
#if (${to_dt} != '')
   AND SKD.VPS_ETA_DT <= TO_DATE(@[to_dt],'YYYY-MM-DD') +0.99999
#end
#if (${vvd_cd} != '')
	AND    BK.VSL_CD     = SUBSTR(@[vvd_cd],1,4)
	AND    BK.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
	AND    BK.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
#end
#if (${bkg_no} != '') 
	AND    BK.BKG_NO = @[bkg_no]
#end
#if (${bl_no} != '') 
	AND    BK.BL_NO = @[bl_no]
#end
#if (${ob_sls_ofc_cd} != '') 
	AND	   BK.OB_SLS_OFC_CD = @[ob_sls_ofc_cd]
#end
#if (${pol_cd} != '') 
	AND	   BK.POL_CD = @[pol_cd]
#end					
#if (${slan_cd} != '') 
	AND	   BK.SLAN_CD = @[slan_cd]
#end					
#if (${sel_bkg_ofc_cd} != '') 
	and    BK.BKG_OFC_CD = @[sel_bkg_ofc_cd]
#end

) A			]]></sql>
			<params>
				<param name="fr_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="ob_sls_ofc_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="sel_bkg_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
