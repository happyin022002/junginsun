<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOCreateCoaSimRqst1CSQL">
			<desc><![CDATA[VSK의 요청으로 COA_SIM_INFO에 데이터를 입력한다.[복제]]]></desc>
			<sql><![CDATA[
#if (${param} == 'COA_SIM_INFO') 
INSERT INFO COA_SIM_INFO
(SIM_DT, SIM_NO, SLAN_CD, SIM_STS_CD, EXPC_OT_PERF_RTO, CO_CD, SVC_DUR_DYS, BRTH_ITVAL_DYS, SIM_DEPT_CD, EXTD_LANE_FLG, SIM_RMK, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
SELECT @[sim_dt]
      ,@[sim_no]
      ,SLAN_CD
      ,SIM_DEPT_CD
      ,SIM_RMK
      ,EXTD_LANE_FLG
      ,@[usr_id]
      ,SYSDATE
      ,@[usr_id]
      ,SYSDATE
  FROM COA_SIM_INFO
WHERE SIM_DT = @[f_sim_dt]
  AND SIM_NO = @[f_sim_no]
#elseif (${param} == 'COA_SIM_SVC_LANE') 
INSERT INTO COA_SIM_SVC_LANE
            (
               SIM_DT
              ,SIM_NO
              ,SECT_NO
              ,FREQ_NO
              ,TRD_CD
              ,SUB_TRD_CD
              ,RLANE_CD
              ,IOC_CD
              ,SKD_DIR_CD
              ,LOD_TTL_QTY
              ,BSA_CAPA
              ,GRS_RPB_REV
              ,GRS_TTL_REV
              ,LDF_RTO
              ,CRE_USR_ID
              ,CRE_DT
              ,UPD_USR_ID
              ,UPD_DT
            )
SELECT @[sim_dt]
      ,@[sim_no]
      ,SECT_NO
      ,FREQ_NO
      ,TRD_CD
      ,SUB_TRD_CD
      ,RLANE_CD
      ,IOC_CD
      ,SKD_DIR_CD
      ,LOD_TTL_QTY
      ,BSA_CAPA
      ,0
      ,0
      ,LDF_RTO
      ,@[usr_id]
      ,SYSDATE
      ,@[usr_id]
      ,SYSDATE
  FROM COA_SIM_SVC_LANE
WHERE SIM_DT = @[f_sim_dt]
  AND SIM_NO = @[f_sim_no]
#elseif (${param} == 'COA_SIM_VSL_SET_INFO') 
INSERT INTO COA_SIM_VSL_SET_INFO 
(SIM_DT, SIM_NO, SECT_NO, VSL_CD, SIM_DIV_CD, VSL_CLSS_CAPA, VOP_CD, VSL_CAPA, BSA_CAPA, FNL_HJS_BSA_CAPA, LDF_RTO, OTR_CRR_BSA_CAPA1, OTR_CRR_BSA_CAPA2, OTR_CRR_BSA_CAPA3, OTR_CRR_BSA_CAPA4, OTR_CRR_BSA_CAPA5, HJS_BFR_BSA_CAPA, SUB_LSE_CAPA1, SUB_LSE_CAPA2, SUB_LSE_CAPA3, SUB_LSE_CAPA4, SUB_LSE_CAPA5, SUB_CHTR_CAPA1, SUB_CHTR_CAPA2, SUB_CHTR_CAPA3, SUB_CHTR_CAPA4, SUB_CHTR_CAPA5, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
SELECT @[sim_dt]
      ,@[sim_no]
      ,SECT_NO
      ,VSL_CD
      ,SIM_DIV_CD
      ,VSL_CLSS_CAPA
      ,VOP_CD
      ,VSL_CAPA
      ,BSA_CAPA
      ,FNL_HJS_BSA_CAPA
      ,LDF_RTO
      ,OTR_CRR_BSA_CAPA1
      ,OTR_CRR_BSA_CAPA2
      ,OTR_CRR_BSA_CAPA3
      ,OTR_CRR_BSA_CAPA4
      ,OTR_CRR_BSA_CAPA5
      ,HJS_BFR_BSA_CAPA
      ,SUB_LSE_CAPA1
      ,SUB_LSE_CAPA2
      ,SUB_LSE_CAPA3
      ,SUB_LSE_CAPA4
      ,SUB_LSE_CAPA5
      ,SUB_CHTR_CAPA1
      ,SUB_CHTR_CAPA2
      ,SUB_CHTR_CAPA3
      ,SUB_CHTR_CAPA4
      ,SUB_CHTR_CAPA5
      ,@[usr_id]
      ,SYSDATE
      ,@[usr_id]
      ,SYSDATE
  FROM COA_SIM_VSL_SET_INFO
 WHERE SIM_DT = @[f_sim_dt]
   AND SIM_NO = @[f_sim_no]
#end			]]></sql>
			<params>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="f_sim_dt" type="12" value="" out="N"/>
				<param name="f_sim_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
