<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchBlckStwgCdRSQL">
			<desc><![CDATA[Block Stowage Code를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(BLCK_STWG_CD, NULL) BLK_STWG_CD
FROM BKG_BOOKING 
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
