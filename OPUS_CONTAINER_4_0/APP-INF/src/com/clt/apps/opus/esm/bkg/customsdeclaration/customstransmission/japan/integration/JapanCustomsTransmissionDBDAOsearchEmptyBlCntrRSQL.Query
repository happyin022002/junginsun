<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOsearchEmptyBlCntrRSQL">
			<desc><![CDATA[searchEmptyBlCntr]]></desc>
			<sql><![CDATA[
SELECT 
	RPAD(CNTR_NO,12,' ') CNTR_NO, 				
	RPAD(NVL(CNTR_SEAL_NO,' '),15,' ') CNTR_SEAL_NO, 
	RPAD(' ',15,' ') DATA1, --75 Seal No.  						
	RPAD(' ',15,' ') DATA2, --75 Seal No.  						
	RPAD(' ',15,' ') DATA3,	--75 Seal No.  					
	RPAD(' ',15,' ') DATA4,	--75 Seal No.  					
	RPAD(' ',15,' ') DATA5,	--75 Seal No.  					
	'4  ' DATA6, --76 Empty/Full 컨테이너 표시  (3)   M 										
	DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'2','22','3','25','4','42','5','45','6','92','7','95','8','92','9','95','95') DATA7, --77 콘테나사이즈코도  (2)   M
	DECODE(SUBSTR(CNTR_TPSZ_CD,1,1),'D','GP','R','RT','O','UT','S','UT','F','PF','A','PF','P','PF','T','TN','GP') DATA8, --78 콘테나타이푸코도  (2)   M
	'  ' DATA9, --79 DEL Term  (2)   C 									
	'2  ' DATA10, --80 컨테이너 소유 형태 코드  (3)   M										
	'   ' DATA11, --81 밴닝 형태 코드  (3)   C										
	'1' DATA12, --82 컨테이너 조약적용 식별  (1)   M 										
	' ' DATA13 	--83 도매 컨테이너 자동추출 대상외 식별  (1)   C		
FROM 
	BKG_CSTMS_JP_BL_CNTR
WHERE	BL_NO = @[bl_no]
AND	BL_SPLIT_NO = nvl(@[bl_split_no],'  ')
AND JP_CSTMS_CNTR_STS_CD = 'A'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
