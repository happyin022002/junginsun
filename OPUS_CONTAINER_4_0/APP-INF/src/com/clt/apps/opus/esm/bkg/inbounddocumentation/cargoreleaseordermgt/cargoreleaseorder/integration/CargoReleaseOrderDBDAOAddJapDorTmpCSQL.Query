<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOAddJapDorTmpCSQL">
			<desc><![CDATA[전송될 대상으로 JPCUS_SLOG Table에 Insert 한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_JP_DO_IF A
( BKG_NO
, RLSE_SEQ
, DO_IF_SEQ
, EVNT_DT
, JP_DO_SND_STS_CD
, JP_DO_GRP_NO
, EVNT_USR_ID
, EVNT_OFC_CD
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
)
SELECT @[bkg_no]           AS BKG_NO
     , @[rlse_seq]         AS RLSE_SEQ
     , NVL((SELECT /*+INDEX_DESC(T XPKBKG_JP_DO_IF) */ DO_IF_SEQ
              FROM BKG_JP_DO_IF T
             WHERE T.BKG_NO   = @[bkg_no]
               AND T.RLSE_SEQ = @[rlse_seq]
               AND ROWNUM =1),0)+1 AS DO_IF_SEQ
     , SYSDATE             AS EVNT_DT
     , 'R'                 AS JP_DO_SND_STS_CD
     , '-1'                AS JP_DO_GRP_NO
     , @[evnt_usr_id]      AS EVNT_USR_ID
     , @[evnt_ofc_cd]      AS EVNT_OFC_CD
     , @[cre_usr_id]       AS CRE_USR_ID
     , SYSDATE             AS CRE_DT
     , @[upd_usr_id]       AS UPD_USR_ID
     , SYSDATE             AS UPD_DT
  FROM DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rlse_seq" type="12" value="" out="N"/>
				<param name="evnt_usr_id" type="12" value="" out="N"/>
				<param name="evnt_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
