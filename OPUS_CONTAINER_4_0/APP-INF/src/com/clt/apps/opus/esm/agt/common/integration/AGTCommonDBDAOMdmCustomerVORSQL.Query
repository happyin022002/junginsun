<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOMdmCustomerVORSQL">
			<desc><![CDATA[mdm_customer 조회]]></desc>
			<sql><![CDATA[
SELECT 
	CUST_CNT_CD||TO_CHAR(CUST_SEQ,'FM000000') AS CODE, 
	CUST_LGL_ENG_NM AS NAME 
FROM MDM_CUSTOMER 
WHERE CUST_CNT_CD = @[cust_cnt_cd] 
AND CUST_SEQ = @[cust_seq] 
AND NVL(DELT_FLG,'N') = 'N'			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="Y"/>
				<param name="cust_seq" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
