<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SalesActivityManageDBDAOModifyCallFlgActDtUSQL">
			<desc><![CDATA[Sales Activity Report 값이 입력 되는 경우 Sales Activity Management 화면의 Call Report 값과 Actual Date 값 동기화]]></desc>
			<sql><![CDATA[
UPDATE SAM_SLS_ACT
SET CALL_RPT_FLG = 'Y'
  , SLS_ACT_ACT_DT = @[sls_act_act_dt]
WHERE SREP_CD = @[srep_cd]
AND CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]
AND SLS_ACT_SEQ = @[sls_act_seq]			]]></sql>
			<params>
				<param name="sls_act_act_dt" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="sls_act_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
