#end
#if (${cnl_tz_flg} == 'N')
	 , CNL_TZ_CD = ''
#end
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND CHG_CD = @[chg_cd]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
