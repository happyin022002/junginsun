<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPsaBkgCntrMaxSerialNoPlsRSQL">
			<desc><![CDATA[PSA BKG CNTR 테이블의 Serial No에 +1을 한 값을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL( MAX( PSA_SER_NO ), 0 ) + 1 PSA_SER_NO
FROM   BKG_CSTMS_PSA_CNTR
WHERE  BKG_NO          =   @[bkg_no]
AND    BKG_SEQ         =   @[bkg_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
