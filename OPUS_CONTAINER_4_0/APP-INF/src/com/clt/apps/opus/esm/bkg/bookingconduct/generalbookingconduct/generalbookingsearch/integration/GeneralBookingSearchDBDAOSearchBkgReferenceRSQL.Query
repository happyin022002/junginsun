<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchBkgReferenceRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT
    SUBSTR(V1.BKG_REF_TP_CD, 2, 5) BKG_REF_TP_CD
   ,V1.BKG_REF_TP_NM
   ,V2.REF_SEQ
   ,V2.CUST_REF_NO_CTNT
   ,V2.CPY_DESC_FLG
   ,@[bkg_no] BKG_NO
FROM
    (        
        SELECT  '1FINV' BKG_REF_TP_CD
               ,'Invoice Ref. No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '2EBRF' BKG_REF_TP_CD
               ,'BKG Ref. No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '3EBSH' BKG_REF_TP_CD
               ,'BKG SH Ref. No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '4EBFF' BKG_REF_TP_CD
               ,'BKG FF Ref. No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '5ESRF' BKG_REF_TP_CD
               ,'S/I Ref. No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '6ESSH' BKG_REF_TP_CD
               ,'S/I SH Ref. No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '7ESFF' BKG_REF_TP_CD
               ,'S/I FF Ref. No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '8RGBK' BKG_REF_TP_CD
               ,'Regional BKG No.' BKG_REF_TP_NM
        FROM    DUAL
        UNION ALL
        SELECT  '9XMRN' BKG_REF_TP_CD
               ,'Export MRN No.' BKG_REF_TP_NM
        FROM    DUAL
    ) V1,
(    
SELECT  BKG_REF_TP_CD
        , REF_SEQ
        , CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS
#else
		FROM    BKG_REFERENCE
#end
        WHERE   BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     BKG_REF_TP_CD = 'FINV'
UNION ALL
select BKG_REF_TP_CD
        , REF_SEQ
        , CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS
#else
		FROM    BKG_REFERENCE
#end
        WHERE   BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     BKG_REF_TP_CD = 'EBRF'
UNION ALL
select BKG_REF_TP_CD
        , MAX(REF_SEQ) REF_SEQ
        , MAX(NVL(CUST_REF_NO_CTNT, SHPR_REF_NO)) CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , REF.BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS REF, BKG_XTER_RQST_MST MST
#else
		FROM    BKG_REFERENCE REF, BKG_XTER_RQST_MST MST
#end
        WHERE   REF.BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     REF.BKG_NO = MST.BKG_NO(+)
AND     'B'        = MST.DOC_TP_CD(+)
AND     REF.BKG_REF_TP_CD = 'EBSH'
GROUP BY BKG_REF_TP_CD
        , CPY_DESC_FLG
        , REF.BKG_NO
UNION ALL
select BKG_REF_TP_CD
        , MAX(REF_SEQ) REF_SEQ
        , MAX(NVL(CUST_REF_NO_CTNT, FWRD_REF_NO)) CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , REF.BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS REF, BKG_XTER_RQST_MST MST
#else
		FROM    BKG_REFERENCE REF, BKG_XTER_RQST_MST MST
#end
        WHERE   REF.BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     REF.BKG_NO = MST.BKG_NO(+)
AND     'B'        = MST.DOC_TP_CD(+)
AND     REF.BKG_REF_TP_CD = 'EBFF'
GROUP BY BKG_REF_TP_CD
        , CPY_DESC_FLG
        , REF.BKG_NO
UNION ALL
select BKG_REF_TP_CD
        , REF_SEQ
        , CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS
#else
		FROM    BKG_REFERENCE
#end
        WHERE   BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     BKG_REF_TP_CD = 'ESRF'
UNION ALL
select BKG_REF_TP_CD
        , MAX(REF_SEQ) REF_SEQ
        , MAX(NVL(CUST_REF_NO_CTNT, SHPR_REF_NO)) CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , REF.BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS REF, BKG_XTER_RQST_MST MST
#else
		FROM    BKG_REFERENCE REF, BKG_XTER_RQST_MST MST
#end
        WHERE   REF.BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     REF.BKG_NO = MST.BKG_NO(+)
AND     'S'        = MST.DOC_TP_CD(+)
AND     REF.BKG_REF_TP_CD = 'ESSH'
GROUP BY BKG_REF_TP_CD
        , CPY_DESC_FLG
        , REF.BKG_NO
UNION ALL
select BKG_REF_TP_CD
        , MAX(REF_SEQ) REF_SEQ
        , MAX(NVL(CUST_REF_NO_CTNT, FWRD_REF_NO)) CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , REF.BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS REF, BKG_XTER_RQST_MST MST
#else
		FROM    BKG_REFERENCE REF, BKG_XTER_RQST_MST MST
#end
        WHERE   REF.BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     REF.BKG_NO = MST.BKG_NO(+)
AND     'S'        = MST.DOC_TP_CD(+)
AND     REF.BKG_REF_TP_CD = 'ESFF'
GROUP BY BKG_REF_TP_CD
        , CPY_DESC_FLG
        , REF.BKG_NO
UNION
SELECT  BKG_REF_TP_CD
        , REF_SEQ
        , CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS
#else
		FROM    BKG_REFERENCE
#end
        WHERE   BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     BKG_REF_TP_CD = 'RGBK'
UNION
SELECT  BKG_REF_TP_CD
        , REF_SEQ
        , CUST_REF_NO_CTNT
        , CPY_DESC_FLG
        , BKG_NO
#if (${ca_flg}== 'Y') 
        FROM    BKG_REF_HIS
#else
		FROM    BKG_REFERENCE
#end
        WHERE   BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y') 
        AND     CORR_NO = 'TMP0000001'   
#end  
AND     BKG_REF_TP_CD = 'XMRN'
    ) V2
WHERE SUBSTR(V1.BKG_REF_TP_CD, 2, 5) = V2.BKG_REF_TP_CD(+)	
ORDER BY V1.BKG_REF_TP_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
