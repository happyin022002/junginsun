<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAOMaxChgRtSeqForPriMotChgRtRSQL">
			<desc><![CDATA[retrive max charge rate seq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(CHG_RT_SEQ),0) AS CHG_RT_SEQ
  FROM PRI_MOT_CHG_RT
 WHERE CHG_CD = @[chg_cd]
   AND POL_CD = @[pol_cd]			]]></sql>
			<params>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
