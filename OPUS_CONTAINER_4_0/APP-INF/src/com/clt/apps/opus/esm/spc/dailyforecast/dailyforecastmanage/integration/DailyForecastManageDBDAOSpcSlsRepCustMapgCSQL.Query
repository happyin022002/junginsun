<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSpcSlsRepCustMapgCSQL">
			<desc><![CDATA[SpcSlsRepCustMapg]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_SLS_REP_CUST_MAPG ( 
    SREP_CD         ,
    TRD_CD          ,
    SUB_TRD_CD      ,
    RLANE_CD        ,
    DIR_CD          ,
    IOC_TS_CD       ,
    CUST_CNT_CD     ,
    CUST_SEQ        ,
    FCAST_CUST_TP_CD,
    REP_TRD_CD      ,
    REP_SUB_TRD_CD  ,
    SLS_RHQ_CD      ,
    SLS_AQ_CD       ,
    SLS_RGN_OFC_CD  ,
    SLS_OFC_CD      ,
    FCAST_OFC_CD    ,
    FCAST_SEQ       ,
    CTRT_CUST_CNT_CD,
    CTRT_CUST_SEQ	,
    CRE_USR_ID      ,
    CRE_DT          ,
    UPD_USR_ID      ,
    UPD_DT
)
WITH PARAMS AS (
    SELECT @[srep_cd]          AS SREP_CD         ,
           @[trd_cd]           AS TRD_CD          ,
           @[sub_trd_cd]       AS SUB_TRD_CD      ,
           @[rlane_cd]         AS RLANE_CD        ,
           @[dir_cd]           AS DIR_CD          ,
           @[ioc_ts_cd]        AS IOC_TS_CD       ,
           @[cust_cnt_cd]      AS CUST_CNT_CD     ,
           @[cust_seq]         AS CUST_SEQ        ,
           @[fcast_cust_tp_cd] AS FCAST_CUST_TP_CD,
		   @[ctrt_cust_cnt_cd] AS CTRT_CUST_CNT_CD,
		   @[ctrt_cust_seq]	   AS CTRT_CUST_SEQ	  ,
           @[cre_usr_id]       AS USR_ID
      FROM DUAL
)
  SELECT P.SREP_CD         ,
         P.TRD_CD          ,
         P.SUB_TRD_CD      ,
         P.RLANE_CD        ,
         P.DIR_CD          ,
         P.IOC_TS_CD       ,
         P.CUST_CNT_CD     ,
         P.CUST_SEQ        ,
         P.FCAST_CUST_TP_CD,
         SAQ_GET_REP_TRD_FNC(P.RLANE_CD)     AS REP_TRD_CD    ,
         SAQ_GET_REP_SUB_TRD_FNC(P.RLANE_CD) AS REP_SUB_TRD_CD,
         O.N2ND_PRNT_OFC_CD AS SLS_RHQ_CD    ,
         O.N3RD_PRNT_OFC_CD AS SLS_AQ_CD     ,
         O.N4TH_PRNT_OFC_CD AS SLS_RGN_OFC_CD,
         NVL(O.N5TH_PRNT_OFC_CD, O.N4TH_PRNT_OFC_CD) AS SLS_OFC_CD,
         SR.OFC_CD AS FCAST_OFC_CD,
        (SELECT NVL(MAX(FCAST_SEQ)+1, 1)
           FROM SPC_SLS_REP_CUST_MAPG
          WHERE 1=1
            AND SREP_CD = P.SREP_CD
            AND TRD_CD = P.TRD_CD
            AND SUB_TRD_CD = P.SUB_TRD_CD
            AND RLANE_CD  = P.RLANE_CD
            AND DIR_CD  = P.DIR_CD
            AND IOC_TS_CD  = P.IOC_TS_CD
            AND CUST_CNT_CD = P.CUST_CNT_CD
            AND CUST_SEQ = P.CUST_SEQ
            AND FCAST_CUST_TP_CD = P.FCAST_CUST_TP_CD) AS FCAST_SEQ,	 
         P.CTRT_CUST_CNT_CD AS CTRT_CUST_CNT_CD,
         P.CTRT_CUST_SEQ AS CTRT_CUST_SEQ,		
         P.USR_ID  AS CRE_USR_ID  ,
         SYSDATE   AS CRE_DT      ,
         P.USR_ID  AS UPD_USR_ID  ,
         SYSDATE   AS UPD_DT
    FROM SPC_OFC_LVL O ,
         MDM_SLS_REP SR,
         PARAMS      P ,
         COA_WK_PRD  W
   WHERE O.OFC_CD   = SR.OFC_CD
     AND SR.SREP_CD = P.SREP_CD
     AND W.COST_YR || W.COST_WK BETWEEN O.OFC_APLY_FM_YRWK AND O.OFC_APLY_TO_YRWK
     AND TO_CHAR(SYSDATE, 'YYYYMMDD') BETWEEN W.SLS_FM_DT AND W.SLS_TO_DT			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="ioc_ts_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="fcast_cust_tp_cd" type="12" value="" out="N"/>
				<param name="ctrt_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="ctrt_cust_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
