<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPSADGDetailInfoRSQL">
			<desc><![CDATA[searchUnmatchBKGList에서 special Dg Cargo 가 있을 경우(=> spe_dg_ind = '1') 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(IMDG_CLSS_CD,NULL,'','IMO'||IMDG_CLSS_CD) DG_DETAIL
FROM  BKG_DG_CGO
WHERE BKG_NO  = @[bkg_no]
AND   CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
