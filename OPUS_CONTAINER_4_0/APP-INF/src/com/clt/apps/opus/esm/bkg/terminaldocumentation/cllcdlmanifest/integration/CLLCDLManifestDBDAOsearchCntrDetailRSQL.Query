<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCntrDetailRSQL">
			<desc><![CDATA[searchCntrDetail]]></desc>
			<sql><![CDATA[
SELECT  
	'{CNTR_INFO'||CHR(10)||
	'CNTR_QUAL:'||'CN'||CHR(10)||
	'CNTR_NUM:'||CLL.CNTR_NO||CHR(10)||
	'CNTR_TPSZ:'||VSL.ATTR_CTNT2||CHR(10)||
	'CNTR_STS:'||DECODE(CLL.KR_CLL_TS_CD, 'TS', '14', 'TT', '13', '')||CHR(10)||
	'CNTR_FM:'||DECODE(CLL.MTY_BKG_CD, 'F', 5, 4)||CHR(10)||
	'BKG_NUM:'||CLL.BKG_NO||CHR(10)||
	'{LOC_INFO'||CHR(10)||
	'LOC_QUAL:'||'11'||CHR(10)||
	'LOC_CD:'|| 	DECODE(POD.ATTR_CTNT2,NULL,DECODE(CLL.BLCK_STWG_CD,NULL,CLL.POD_CD,DECODE(LENGTH(CLL.BLCK_STWG_CD),2,SUBSTR(CLL.POD_CD,1,3)||CLL.BLCK_STWG_CD,1,SUBSTR(CLL.POD_CD,1,4)||CLL.BLCK_STWG_CD)),
					DECODE(CLL.BLCK_STWG_CD,NULL,CLL.POD_CD,DECODE(LENGTH(CLL.BLCK_STWG_CD),2,SUBSTR(POD.ATTR_CTNT2,1,3)||CLL.BLCK_STWG_CD,1,SUBSTR(POD.ATTR_CTNT2,1,4)||CLL.BLCK_STWG_CD))
					)||CHR(10)||
	'LOC_NM:'||(SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = (SELECT LOC_CD FROM MDM_LOCATION WHERE NVL(UN_LOC_CD,LOC_CD) = CLL.POD_CD AND ROWNUM=1))||CHR(10)||
	'BLK_STWG:'||CLL.BLCK_STWG_CD||CHR(10)||
	'HOT_DEL:'||DECODE(CLL.HOT_FLG,'Y','1','N','0',CLL.HOT_FLG)||CHR(10)||
	'}LOC_INFO'||CHR(10)||
	'{WGT_INFO'||CHR(10)||
	'WGT_QUAL:'||'WT'||CHR(10)||
	'WGT_UNIT:'||DECODE(CLL.WGT_UT_CD, 'K', 'KGM', CLL.WGT_UT_CD)||CHR(10)||
	'WGT:'||CLL.BL_WGT||CHR(10)||
	'}WGT_INFO'||CHR(10)||
	'{DIM_INFO'||CHR(10)||
	'DIM_QUAL:'||'10'||CHR(10)||
	'DIM_UNIT:'||'CMT'||CHR(10)||
	'DIM_LENGTH:'||NVL(CLL.OVR_LEN_QTY, '')||CHR(10)||
	'DIM_WIDTH:'||NVL(CLL.OVR_WGT_QTY, '')||CHR(10)||
	'DIM_HEIGHT:'||NVL(CLL.OVR_HGT_QTY, '')||CHR(10)||
	'}DIM_INFO'||CHR(10)||
	'{TEMP_INFO'||CHR(10)||
	'TEMP_QUAL:'||DECODE(CLL.CDO_TEMP,NULL,'','2')||CHR(10)||
	'TEMP:'||ROUND(NVL(CLL.CDO_TEMP, ''))||CHR(10)||
	'TEMP_UNIT:'||DECODE(CLL.CDO_TEMP,NULL,'','CEL')||CHR(10)||
	'}TEMP_INFO'||CHR(10)||
	'{SEAL_INFO'||CHR(10)||
	'SEAL_NUM:'||REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE((RTRIM(LTRIM(CLL.SEAL_NO))),CHR(42),NULL),CHR(13),NULL),CHR(126),NULL),CHR(34),NULL),CHR(124),NULL),CHR(33),NULL),CHR(47),NULL),CHR(40),NULL),CHR(41),NULL),CHR(38),NULL),CHR(94),NULL),CHR(37),NULL),CHR(36),NULL),CHR(35),NULL),CHR(64),NULL),CHR(43),NULL),CHR(125),NULL),CHR(92),NULL),CHR(123),NULL),CHR(46),NULL),CHR(96),NULL),CHR(63),NULL),CHR(61),NULL),CHR(95),NULL),CHR(45),NULL),CHR(91),NULL),CHR(93),NULL),CHR(32),NULL)||CHR(10)||
	'SEAL_CD:'||'CA'||CHR(10)||
	'}SEAL_INFO'||CHR(10)||
	'{REMARK'||CHR(10)||
	'REMARK_QUAL:'||
    	DECODE(CLL.CLL_RMK1||CLL.CLL_RMK2||CLL.KR_TML_PRCT_ID
    	,NULL,'','AAN')||CHR(10)||
	'REMARK:'||
    	DECODE(CLL.CLL_RMK1||CLL.CLL_RMK2||CLL.STWG_CD||CLL.KR_TML_PRCT_ID
    	,NULL,'',
        CLL.CLL_RMK1||' '||CLL.CLL_RMK2||' '
           ||DECODE(CLL.KR_TML_PRCT_ID,'P','P', NULL))||CHR(10)||
	'}REMARK'||CHR(10)||
	'{DG_INFO'||CHR(10)||
	'IMDG_QUAL:'||DECODE(CLL.TML_IMDG_ID,NULL,'','IMD')||CHR(10)||
	'IMDG_CD:'||DECODE(CLL.TML_IMDG_ID,NULL,'',CLL.IMDG_CLSS_ID)||CHR(10)||
	'CLASS:'||DECODE(CLL.TML_IMDG_ID,NULL,'',CLL.TML_IMDG_ID)||CHR(10)||
	'}DG_INFO'||CHR(10)||
	'}CNTR_INFO'||CHR(10) CNTR_DETAIL
FROM 
	BKG_CSTMS_TML_KR_CLL CLL,
	BKG_CSTMS_CD_CONV_CTNT VSL,
	BKG_CSTMS_CD_CONV_CTNT POD
WHERE CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no],2,3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd],3,3)
AND CLL.CNTR_NO IN ( ${cntr_no_list} )
AND CLL.CNTR_TPSZ_CD = VSL.ATTR_CTNT1(+)
AND CLL.POD_CD = POD.ATTR_CTNT1(+)
AND VSL.CNT_CD(+) = 'KR'
AND VSL.CSTMS_DIV_ID(+) = 'CLL_CNTR_ISO_CD' 
AND POD.CNT_CD(+) = 'KR'
AND POD.CSTMS_DIV_ID(+) = 'KR_CLL_EDI_POD_CD' 
AND VSL.DELT_FLG(+) = 'N'
#if (${in_pol_yd_cd} != '' ) 
AND CLL.POL_YD_CD = @[in_pol_yd_cd]
#end
#if (${in_cll_type} == 'LOCAL')
AND CLL.KR_CLL_TS_CD IS NULL
AND CLL.MTY_BKG_CD = 'F'
#elseif (${in_cll_type} == 'TS')
AND CLL.KR_CLL_TS_CD IS NOT NULL
AND CLL.MTY_BKG_CD = 'F'
#elseif (${in_cll_type} == 'EMPTY')
AND CLL.MTY_BKG_CD = 'M'
#end			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
