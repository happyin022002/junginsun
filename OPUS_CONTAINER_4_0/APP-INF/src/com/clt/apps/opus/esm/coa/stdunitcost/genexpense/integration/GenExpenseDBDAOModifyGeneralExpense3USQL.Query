<sqls>
	<querys>
		<query name="GenExpenseDBDAOModifyGeneralExpense3USQL">
			<desc><![CDATA[@ SJH.20141229.ADD : 신규]]></desc>
			<sql><![CDATA[
UPDATE COA_OWN_VSL_DLY_HIR
   SET DHIR_AMT			    = @[dhir_amt3]
      ,UPD_USR_ID         	= @[upd_usr_id]
      ,UPD_DT             	= SYSDATE
WHERE COST_YRMON     = @[cost_yrmon3]  
  AND VSL_CD         = @[vsl_cd3]  
  AND VSL_CLSS_CAPA  = @[vsl_clss_capa3]
  AND STND_COST_CD   = '75000000'			]]></sql>
			<params>
				<param name="dhir_amt3" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon3" type="12" value="" out="N"/>
				<param name="vsl_cd3" type="12" value="" out="N"/>
				<param name="vsl_clss_capa3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
