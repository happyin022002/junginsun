<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SPCLCmpnAgreementDBDAOAddSPCLCmpnAgreementCSQL">
			<desc><![CDATA[AddSPCLCmpnAgreement

2016.04.21 박다은 [CSR:#11434] CSA - ACM - Special Compensation CSR to be created in local currency]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_SPCL_AGMT
       (SPCL_OFC_CD,
        CUST_CNT_CD,
        CUST_SEQ,
        SPCL_AGMT_SEQ,
        CUST_KND_CD,
        SC_NO,
        RFA_NO,
        SHPR_CNT_CD,
        SHPR_SEQ,
        FM_EFF_DT,
        TO_EFF_DT,
        CMDT_TP_CD,
        CMDT_CD,
        POR_GRP_TP_CD,
        POR_ROUT_CD,
        POL_GRP_TP_CD,
        POL_ROUT_CD,
        POD_GRP_TP_CD,
        POD_ROUT_CD,
        SPCL_DIV_CD,
        SPCL_BKG_RT,
        SPCL_BX_AMT,
        SPCL_TEU_AMT,
        SPCL_FEU_AMT,
        SPCL_RF_AMT,
        SPCL_RF_TEU_AMT,
        SPCL_RF_FEU_AMT,
        SPCL_CHG_CTNT,
        CURR_CD,
        PAY_XCH_RT,
        DELT_FLG,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (@[spcl_ofc_cd],
        SUBSTR(@[cust_cnt_seq], 0, 2),
        SUBSTR(@[cust_cnt_seq], 3),
        (SELECT NVL(MAX(SPCL_AGMT_SEQ) + 1, 1)
           FROM ACM_SPCL_AGMT
          WHERE SPCL_OFC_CD = @[spcl_ofc_cd]
            AND CUST_CNT_CD = SUBSTR(@[cust_cnt_seq], 0, 2)
            AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cnt_seq], 3))),
        @[cust_knd_cd],
        @[sc_no],
        @[rfa_no],
        DECODE(@[shpr_cnt_seq], '*', '*', SUBSTR(@[shpr_cnt_seq], 0, 2)),
        DECODE(@[shpr_cnt_seq], '*', 0, TO_NUMBER(SUBSTR(@[shpr_cnt_seq], 3))),
        @[fm_eff_dt],
        @[to_eff_dt],
        @[cmdt_tp_cd],
        @[cmdt_cd],
        @[por_grp_tp_cd],
        @[por_rout_cd],
        @[pol_grp_tp_cd],
        @[pol_rout_cd],
        @[pod_grp_tp_cd],
        @[pod_rout_cd],
        @[spcl_div_cd],
        @[spcl_bkg_rt],
        @[spcl_bx_amt],
        @[spcl_teu_amt],
        @[spcl_feu_amt],
        @[spcl_rf_amt],
        @[spcl_rf_teu_amt],
        @[spcl_rf_feu_amt],
        @[spcl_chg_ctnt],
        @[curr_cd],
        @[pay_xch_rt],
        'N',
        @[usr_id],
        SYSDATE,
        @[usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="spcl_ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_seq" type="12" value="" out="N"/>
				<param name="cust_knd_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="shpr_cnt_seq" type="12" value="" out="N"/>
				<param name="fm_eff_dt" type="12" value="" out="N"/>
				<param name="to_eff_dt" type="12" value="" out="N"/>
				<param name="cmdt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="por_grp_tp_cd" type="12" value="" out="N"/>
				<param name="por_rout_cd" type="12" value="" out="N"/>
				<param name="pol_grp_tp_cd" type="12" value="" out="N"/>
				<param name="pol_rout_cd" type="12" value="" out="N"/>
				<param name="pod_grp_tp_cd" type="12" value="" out="N"/>
				<param name="pod_rout_cd" type="12" value="" out="N"/>
				<param name="spcl_div_cd" type="12" value="" out="N"/>
				<param name="spcl_bkg_rt" type="12" value="" out="N"/>
				<param name="spcl_bx_amt" type="12" value="" out="N"/>
				<param name="spcl_teu_amt" type="12" value="" out="N"/>
				<param name="spcl_feu_amt" type="12" value="" out="N"/>
				<param name="spcl_rf_amt" type="12" value="" out="N"/>
				<param name="spcl_rf_teu_amt" type="12" value="" out="N"/>
				<param name="spcl_rf_feu_amt" type="12" value="" out="N"/>
				<param name="spcl_chg_ctnt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="pay_xch_rt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
