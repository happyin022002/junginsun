<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOModifyOffInvoiceUSQL">
			<desc><![CDATA[TCharterIOInvoiceDAOModifyOffInvoiceUSQL]]></desc>
			<sql><![CDATA[
update fms_invoice 
   set flet_offh_rsn_cd = @[flet_offh_rsn_cd],
       upd_usr_id = @[upd_usr_id],
       upd_dt = sysdate
 where flet_ctrt_no = @[flet_ctrt_no] 
   and flet_iss_tp_cd = 'OFF'
   and inv_seq = @[inv_seq]			]]></sql>
			<params>
				<param name="flet_offh_rsn_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
