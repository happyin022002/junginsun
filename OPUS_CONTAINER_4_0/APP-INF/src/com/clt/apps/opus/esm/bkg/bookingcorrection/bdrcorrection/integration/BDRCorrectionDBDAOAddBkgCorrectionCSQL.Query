<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOAddBkgCorrectionCSQL">
			<desc><![CDATA[BDRCorrectionDBDAOAddBkgCorrectionCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CORRECTION (
            BKG_NO
          , CORR_NO
          , CA_RSN_CD
          , CORR_USR_ID
          , BKG_CORR_RMK
          , CORR_DT  
		  , CORR_GDT
          , RAT_FLG
          , EXPN_FLG
          , NEW_BKG_CRE_FLG
          , RCRE_MODI_FLG
          , BKG_CRE_MODI_FLG
          , CORR_CXL_FLG
          , OB_TRO_MODI_FLG
          , IB_TRO_MODI_FLG
          , BKG_SPLIT_MODI_FLG
          , COD_MODI_FLG
          , CXL_MODI_FLG
          , CA_RLY_PORT_MODI_FLG
          , DG_MODI_FLG
          , AWK_MODI_FLG
          , RF_MODI_FLG
          , BB_MODI_FLG
          , RD_MODI_FLG
          , HNGR_MODI_FLG
          , SOC_MODI_FLG
          , EQ_SUB_MODI_FLG
          , CUST_MODI_FLG
          , BL_MK_DESC_MODI_FLG
          , CNSL_MODI_FLG
          , BKG_CNTR_MODI_FLG
          , CNTR_MF_MODI_FLG
          , RT_MODI_FLG
          , BL_OBRD_CORR_FLG
          , RT_CORR_FLG
          , CHG_TERM_CORR_FLG
          , RCVDE_TERM_CORR_FLG
          , ROUT_CORR_FLG
          , CUST_CORR_FLG
          , QTY_CORR_FLG
          , MEAS_QTY_CORR_FLG
          , CMDT_CORR_FLG
          , TRNK_VSL_CORR_FLG
          , PRPST_VSL_CORR_FLG
          , CA_OTR_RSN_CORR_FLG
          , BND_CORR_FLG
          , BROG_GEN_CORR_FLG
          , BIS_SYS_IF_FLG
		  , BKG_CMB_MODI_FLG
          , CORR_OFC_CD  
          , RDN_NO 
          , RVIS_SEQ
          , RDN_ACPT_FLG
          , CRE_USR_ID
          , CRE_DT
          , UPD_USR_ID
          , UPD_DT
   ) VALUES (
     @[bkg_no]
   , @[corr_no]
   , @[ca_rsn_cd]
   , @[corr_usr_id]
   , @[bkg_corr_rmk]
#if (${corr_no} == '0000000001') 
   , GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),TO_DATE('1977/05/16','YYYY/MM/DD'),BKG_COM_USER_LOC_FNC(@[cre_usr_id])) 
#else 
   , GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),sysdate,BKG_COM_USER_LOC_FNC(@[cre_usr_id]))
#end 
#if (${corr_no} == '0000000001') 
   , TO_DATE('1977/05/16','YYYY/MM/DD') 
#else 
   , GLOBALDATE_PKG.TIME_CONV_FNC(COM_ConstantMgr_PKG.COM_getBaseLocationCode_FNC(),sysdate,'GMT')
#end 
   , nvl(@[rat_flg],				'N')--RAT_FLG          
   , nvl(@[expn_flg],               'N')--EXPN_FLG
   , nvl(@[new_bkg_cre_flg],        'N')--NEW_BKG_CRE_FLG
   , nvl(@[rcre_modi_flg],          'N')--RCRE_MODI_FLG
   , nvl(@[bkg_cre_modi_flg],       'N')--BKG_CRE_MODI_FLG
   , nvl(@[corr_cxl_flg],           'N')--CORR_CXL_FLG
   , nvl(@[ob_tro_modi_flg],        'N')--OB_TRO_MODI_FLG
   , nvl(@[ib_tro_modi_flg],        'N')--IB_TRO_MODI_FLG
   , nvl(@[bkg_split_modi_flg],     'N')--BKG_SPLIT_MODI_FLG
   , nvl(@[cod_modi_flg],           'N')--cod_modi_flg
   , nvl(@[cxl_modi_flg],           'N')--CXL_MODI_FLG
   , nvl(@[ca_rly_port_modi_flg],   'N')--CA_RLY_PORT_MODI_FLG
   , nvl(@[dg_modi_flg],            'N')--DG_MODI_FLG
   , nvl(@[awk_modi_flg],           'N')--AWK_MODI_FLG
   , nvl(@[rf_modi_flg],            'N')--RF_MODI_FLG
   , nvl(@[bb_modi_flg],            'N')--BB_MODI_FLG
   , nvl(@[rd_modi_flg],            'N')--RD_MODI_FLG
   , nvl(@[hngr_modi_flg],          'N')--HNGR_MODI_FLG
   , nvl(@[soc_modi_flg],           'N')--SOC_MODI_FLG
   , nvl(@[eq_sub_modi_flg],        'N')--EQ_SUB_MODI_FLG
   , nvl(@[cust_modi_flg],          'N')--CUST_MODI_FLG
   , nvl(@[bl_mk_desc_modi_flg],    'N')--BL_MK_DESC_MODI_FLG
   , nvl(@[cnsl_modi_flg],          'N')--CNSL_MODI_FLG
   , nvl(@[bkg_cntr_modi_flg],      'N')--BKG_CNTR_MODI_FLG
   , nvl(@[cntr_mf_modi_flg],       'N')--CNTR_MF_MODI_FLG
   , nvl(@[rt_modi_flg],            'N')--RT_MODI_FLG
   , nvl(@[bl_obrd_corr_flg],       'N')--BL_OBRD_CORR_FLG
   , nvl(@[rt_corr_flg],            'N')--RT_CORR_FLG
   , nvl(@[chg_term_corr_flg],      'N')--CHG_TERM_CORR_FLG
   , nvl(@[rcvde_term_corr_flg],    'N')--RCVDE_TERM_CORR_FLG
   , nvl(@[rout_corr_flg],          'N')--ROUT_CORR_FLG
   , nvl(@[cust_corr_flg],          'N')--CUST_CORR_FLG
   , nvl(@[qty_corr_flg],           'N')--QTY_CORR_FLG
   , nvl(@[meas_qty_corr_flg],      'N')--MEAS_QTY_CORR_FLG
   , nvl(@[cmdt_corr_flg],          'N')--CMDT_CORR_FLG
   , nvl(@[trnk_vsl_corr_flg],      'N')--TRNK_VSL_CORR_FLG
   , nvl(@[prpst_vsl_corr_flg],     'N')--PRPST_VSL_CORR_FLG
   , nvl(@[ca_otr_rsn_corr_flg],    'N')--CA_OTR_RSN_CORR_FLG
   , nvl(@[bnd_corr_flg],           'N')--BND_CORR_FLG
   , nvl(@[brog_gen_Corr_flg],      'N')--BROG_GEN_CORR_FLG
   , nvl(@[bis_sys_if_flg],         'N')--BIS_SYS_IF_FLG
   , nvl(@[bkg_cmb_modi_flg],		'N')--BKG_CMB_MODI_FLG
   , @[corr_ofc_cd]
   , @[rdn_no]
   , @[rvis_seq]
   , NVL(@[rdn_acpt_flg], 'N')
   , @[cre_usr_id]
   , sysdate
   , @[upd_usr_id]
   , sysdate
   )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="corr_no" type="12" value="" out="N"/>
				<param name="ca_rsn_cd" type="12" value="" out="N"/>
				<param name="corr_usr_id" type="12" value="" out="N"/>
				<param name="bkg_corr_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="rat_flg" type="12" value="" out="N"/>
				<param name="expn_flg" type="12" value="" out="N"/>
				<param name="new_bkg_cre_flg" type="12" value="" out="N"/>
				<param name="rcre_modi_flg" type="12" value="" out="N"/>
				<param name="bkg_cre_modi_flg" type="12" value="" out="N"/>
				<param name="corr_cxl_flg" type="12" value="" out="N"/>
				<param name="ob_tro_modi_flg" type="12" value="" out="N"/>
				<param name="ib_tro_modi_flg" type="12" value="" out="N"/>
				<param name="bkg_split_modi_flg" type="12" value="" out="N"/>
				<param name="cod_modi_flg" type="12" value="" out="N"/>
				<param name="cxl_modi_flg" type="12" value="" out="N"/>
				<param name="ca_rly_port_modi_flg" type="12" value="" out="N"/>
				<param name="dg_modi_flg" type="12" value="" out="N"/>
				<param name="awk_modi_flg" type="12" value="" out="N"/>
				<param name="rf_modi_flg" type="12" value="" out="N"/>
				<param name="bb_modi_flg" type="12" value="" out="N"/>
				<param name="rd_modi_flg" type="12" value="" out="N"/>
				<param name="hngr_modi_flg" type="12" value="" out="N"/>
				<param name="soc_modi_flg" type="12" value="" out="N"/>
				<param name="eq_sub_modi_flg" type="12" value="" out="N"/>
				<param name="cust_modi_flg" type="12" value="" out="N"/>
				<param name="bl_mk_desc_modi_flg" type="12" value="" out="N"/>
				<param name="cnsl_modi_flg" type="12" value="" out="N"/>
				<param name="bkg_cntr_modi_flg" type="12" value="" out="N"/>
				<param name="cntr_mf_modi_flg" type="12" value="" out="N"/>
				<param name="rt_modi_flg" type="12" value="" out="N"/>
				<param name="bl_obrd_corr_flg" type="12" value="" out="N"/>
				<param name="rt_corr_flg" type="12" value="" out="N"/>
				<param name="chg_term_corr_flg" type="12" value="" out="N"/>
				<param name="rcvde_term_corr_flg" type="12" value="" out="N"/>
				<param name="rout_corr_flg" type="12" value="" out="N"/>
				<param name="cust_corr_flg" type="12" value="" out="N"/>
				<param name="qty_corr_flg" type="12" value="" out="N"/>
				<param name="meas_qty_corr_flg" type="12" value="" out="N"/>
				<param name="cmdt_corr_flg" type="12" value="" out="N"/>
				<param name="trnk_vsl_corr_flg" type="12" value="" out="N"/>
				<param name="prpst_vsl_corr_flg" type="12" value="" out="N"/>
				<param name="ca_otr_rsn_corr_flg" type="12" value="" out="N"/>
				<param name="bnd_corr_flg" type="12" value="" out="N"/>
				<param name="brog_gen_Corr_flg" type="12" value="" out="N"/>
				<param name="bis_sys_if_flg" type="12" value="" out="N"/>
				<param name="bkg_cmb_modi_flg" type="12" value="" out="N"/>
				<param name="corr_ofc_cd" type="12" value="" out="N"/>
				<param name="rdn_no" type="12" value="" out="N"/>
				<param name="rvis_seq" type="12" value="" out="N"/>
				<param name="rdn_acpt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
