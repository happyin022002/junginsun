<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRatesDBDAOComCodeDescVORSQL">
			<desc><![CDATA[공통코드 존재유무 체크]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_CTNT CD
  	 , INTG_CD_VAL_DP_DESC NM
  FROM COM_INTG_CD_DTL
 WHERE INTG_CD_ID = @[etc3]
   AND INTG_CD_VAL_CTNT = @[cd]			]]></sql>
			<params>
				<param name="etc3" type="12" value="" out="N"/>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
