<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchRdApplCdFtpRSQL">
			<desc><![CDATA[FTP , MRD 에 대한 RD_APPL_CD 조회]]></desc>
			<sql><![CDATA[
SELECT  RD_APPL_CD
		, RD_SUB_SYS_CD
FROM COM_RPT_DSGN_SND_APPL
WHERE RD_TMPLT_NM 	= @[mrd_nm]
AND FAX_EML_DIV_CD 	= 'F'			]]></sql>
			<params>
				<param name="mrd_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
