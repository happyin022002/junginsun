<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchVaildHamTrfCdRSQL">
			<desc><![CDATA[BookingMasterMgtDBDAOSearchVaildHamTrfCdRSQL]]></desc>
			<sql><![CDATA[
--1. exp_dt 가 널이 2개 이상이면 안된다.
SELECT NVL(MAX(FLG),'N')
FROM (
SELECT 'Y' FLG FROM BKG_HAMO_TRF
WHERE 1=1
AND HAMO_TRF_CD =  @[hamo_trf_cd]
AND HAMO_TP_CD = @[hamo_tp_cd]
AND DELT_FLG = 'N'
AND '' IS NULL AND  EXP_DT IS NULL
HAVING COUNT(1) > 1 
)			]]></sql>
			<params>
				<param name="hamo_trf_cd" type="12" value="" out="N"/>
				<param name="hamo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
