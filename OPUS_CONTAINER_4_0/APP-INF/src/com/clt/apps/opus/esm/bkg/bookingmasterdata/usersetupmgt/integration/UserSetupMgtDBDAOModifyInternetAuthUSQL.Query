<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOModifyInternetAuthUSQL">
			<desc><![CDATA[UserSetupMgtDBDAOModifyInternetAuthUSQL]]></desc>
			<sql><![CDATA[
UPDATE  BKG_INET_AUTH
SET DELT_FLG = 'N'
	, INET_AUTH_FLG=DECODE(@[inet_auth_flg],'1','Y','N')
	, INET_FTP_AUTH_FLG=DECODE(@[inet_ftp_auth_flg],'1','Y','N')
	, UPD_USR_ID=@[upd_usr_id]
	, UPD_DT = SYSDATE
WHERE USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="inet_auth_flg" type="12" value="" out="N"/>
				<param name="inet_ftp_auth_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
