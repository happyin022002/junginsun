<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltCustBySaleRepRSQL">
			<desc><![CDATA[customer ë³„ sale rep]]></desc>
			<sql><![CDATA[
SELECT A.SREP_CD CD
      ,A.SREP_NM NM
FROM   MDM_SLS_REP A
      ,BKG_CUST_SLS_REP B
WHERE A.SREP_CD = B.SREP_CD
AND B.CUST_CNT_CD = @[etc2]
AND B.CUST_SEQ = @[etc3]
AND B.DELT_FLG = 'N'
AND A.OFC_CD = @[etc1]
AND A.DELT_FLG  = 'N'
ORDER BY A.SREP_CD			]]></sql>
			<params>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="etc3" type="12" value="" out="N"/>
				<param name="etc1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
