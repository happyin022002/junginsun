                    DECODE(C.CNTR_WGT,0,ROUND(NVL(D.ACT_WGT,0) * DECODE(SUBSTR(C.CNTR_TPSZ_CD,2,1),'2',1,2) / (SELECT SUM(DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'2',1,2)) FROM BKG_CONTAINER BC WHERE BC.BKG_NO = C.BKG_NO),2),C.CNTR_WGT)
                     + DECODE(NVL(DECODE(NVL(SPEC.TARE_WGT, 0), 0, DECODE(NVL(MDM.CNTR_TPSZ_TARE_WGT, 0), 0, DECODE(O.CNTR_TPSZ_CD, 'T2', 3600, 'T4', 5200, 0), MDM.CNTR_TPSZ_TARE_WGT), SPEC.TARE_WGT  ),0),0,(SELECT CNTR_TPSZ_TARE_WGT FROM MDM_CNTR_TP_SZ WHERE CNTR_TPSZ_CD = O.CNTR_TPSZ_CD), NVL(DECODE(NVL(SPEC.TARE_WGT, 0), 0, DECODE(NVL(MDM.CNTR_TPSZ_TARE_WGT, 0), 0, DECODE(O.CNTR_TPSZ_CD, 'T2', 3600, 'T4', 5200, 0), MDM.CNTR_TPSZ_TARE_WGT), SPEC.TARE_WGT  ),0)),2)
                   ||CHR(10)||
        'CNTR_WGT_UNIT:'||'KGS'||CHR(10)||

        'CNTR_TOTAL_WGT:'|| (
