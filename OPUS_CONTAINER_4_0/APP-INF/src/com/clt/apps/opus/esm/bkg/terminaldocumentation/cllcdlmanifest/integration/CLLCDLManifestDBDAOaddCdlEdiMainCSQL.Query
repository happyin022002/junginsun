                            AND XTER_VGM.VGM_CRE_GDT = (SELECT MAX(VGM_CRE_GDT)
                                                          FROM BKG_XTER_VGM
                                                         WHERE BKG_NO = XTER_VGM.BKG_NO
                                                           AND CNTR_NO = XTER_VGM.CNTR_NO
                                                           AND VGM_SEQ = XTER_VGM.VGM_SEQ
                                                           AND ACT_TP_CD = 'I')

