		<query name="TCharterIOContractDAOFmsHireDSQL">
			<desc><![CDATA[TCharterIOContractDAOFmsHireDSQL]]></desc>
			<sql><![CDATA[
delete from fms_hire
 where flet_ctrt_no = @[flet_ctrt_no]
   and eff_dt = TO_DATE(@[ori_eff_dt],'YYYYMMDDHH24MI')
   and exp_dt = TO_DATE(@[ori_exp_dt],'YYYYMMDDHH24MI')			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="ori_eff_dt" type="12" value="" out="N"/>
				<param name="ori_exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
