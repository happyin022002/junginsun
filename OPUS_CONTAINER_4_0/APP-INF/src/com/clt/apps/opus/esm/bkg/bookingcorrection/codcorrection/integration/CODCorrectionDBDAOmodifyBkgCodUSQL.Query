<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOmodifyBkgCodUSQL">
			<desc><![CDATA[bkg_cod에 Update한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_COD SET 
	COD_STS_CD 		= @[cod_sts_cd] 
,	COD_RQST_RSN_CD = @[cod_rqst_rsn_cd]
,	RGN_CD 			= @[rgn_cd]
,	OLD_VSL_CD 		= @[old_vsl_cd]
,	OLD_SKD_VOY_NO 	= @[old_skd_voy_no]
,	OLD_SKD_DIR_CD 	= @[old_skd_dir_cd]
,	OLD_POR_YD_CD 	= @[old_por_yd_cd]
,	OLD_POL_YD_CD 	= @[old_pol_yd_cd]
,	OLD_POD_YD_CD 	= @[old_pod_yd_cd]
,	OLD_DEL_YD_CD 	= @[old_del_yd_cd]
,   OLD_DE_TERM_CD 	= @[old_de_term_cd]
,	NEW_VSL_CD 		= @[new_vsl_cd]
,	NEW_SKD_VOY_NO 	= @[new_skd_voy_no]
,	NEW_SKD_DIR_CD 	= @[new_skd_dir_cd]
,	NEW_POR_YD_CD 	= @[new_por_yd_cd]
,	NEW_POL_YD_CD 	= @[new_pol_yd_cd]
,	NEW_POD_YD_CD 	= @[new_pod_yd_cd]
,	NEW_DEL_YD_CD 	= @[new_del_yd_cd]
,   NEW_DE_TERM_CD	= @[new_de_term_cd]
,	COD_RHND_PORT_CD 	= @[cod_rhnd_port_cd]
,	COD_RHND_PORT_YD_CD = @[cod_rhnd_port_yd_cd]
,	COD_CNG_COST_RMK	= @[cod_cng_cost_rmk]
,	COD_CHG_TTL_COST_AMT= @[cod_chg_ttl_cost_amt]
,	COD_TRF_COST_AMT 	= @[cod_trf_cost_amt]
,	COD_FNL_COST_AMT 	= @[cod_fnl_cost_amt]
,	COD_RQST_OFC_CD 	= @[cod_rqst_ofc_cd]
,	COD_AUTH_FLG 		= @[cod_auth_flg]
,	COD_RJCT_CD 		= @[cod_rjct_cd]
,	COD_RJCT_RSN_RMK 	= @[cod_rjct_rsn_rmk]
,	PCTL_NO			= @[pctl_no]
,	DIFF_RMK 		= @[diff_rmk]
,	UPD_USR_ID 		= @[usr_id]
,	UPD_DT 			= sysdate
,	COD_MNL_FLG		= @[cod_mnl_flg]
 WHERE	BKG_NO 			= @[bkg_no]
   AND	COD_RQST_SEQ 	= @[cod_rqst_seq]			]]></sql>
			<params>
				<param name="cod_sts_cd" type="12" value="" out="N"/>
				<param name="cod_rqst_rsn_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="old_vsl_cd" type="12" value="" out="N"/>
				<param name="old_skd_voy_no" type="12" value="" out="N"/>
				<param name="old_skd_dir_cd" type="12" value="" out="N"/>
				<param name="old_por_yd_cd" type="12" value="" out="N"/>
				<param name="old_pol_yd_cd" type="12" value="" out="N"/>
				<param name="old_pod_yd_cd" type="12" value="" out="N"/>
				<param name="old_del_yd_cd" type="12" value="" out="N"/>
				<param name="old_de_term_cd" type="12" value="" out="N"/>
				<param name="new_vsl_cd" type="12" value="" out="N"/>
				<param name="new_skd_voy_no" type="12" value="" out="N"/>
				<param name="new_skd_dir_cd" type="12" value="" out="N"/>
				<param name="new_por_yd_cd" type="12" value="" out="N"/>
				<param name="new_pol_yd_cd" type="12" value="" out="N"/>
				<param name="new_pod_yd_cd" type="12" value="" out="N"/>
				<param name="new_del_yd_cd" type="12" value="" out="N"/>
				<param name="new_de_term_cd" type="12" value="" out="N"/>
				<param name="cod_rhnd_port_cd" type="12" value="" out="N"/>
				<param name="cod_rhnd_port_yd_cd" type="12" value="" out="N"/>
				<param name="cod_cng_cost_rmk" type="12" value="" out="N"/>
				<param name="cod_chg_ttl_cost_amt" type="6" value="" out="N"/>
				<param name="cod_trf_cost_amt" type="6" value="" out="N"/>
				<param name="cod_fnl_cost_amt" type="6" value="" out="N"/>
				<param name="cod_rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="cod_auth_flg" type="12" value="" out="N"/>
				<param name="cod_rjct_cd" type="12" value="" out="N"/>
				<param name="cod_rjct_rsn_rmk" type="12" value="" out="N"/>
				<param name="pctl_no" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cod_mnl_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
