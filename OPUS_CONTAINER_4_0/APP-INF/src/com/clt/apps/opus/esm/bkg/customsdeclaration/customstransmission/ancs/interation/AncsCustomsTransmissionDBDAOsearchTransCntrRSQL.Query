<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsCustomsTransmissionDBDAOsearchTransCntrRSQL">
			<desc><![CDATA[SELECT]]></desc>
			<sql><![CDATA[
SELECT CNTR.CNTR_NO      AS CNTR_NO
      ,CNTR.CNTR_TPSZ_CD AS CNTR_TS
      ,DECODE(BL.BKG_CGO_TP_CD, 'F', 'F', 'E') CNTR_FM
      ,CNTR_WGT          AS CNTR_WGT
      ,CNTR.ORG_RCV_TERM_CD||CNTR.DEST_DE_TERM_CD AS RD_TERM
  FROM BKG_CSTMS_ANR_CNTR CNTR
      ,BKG_CSTMS_ANR_BL   BL
 WHERE BL.VSL_CD     = CNTR.VSL_CD
   AND BL.SKD_VOY_NO = CNTR.SKD_VOY_NO
   AND BL.SKD_DIR_CD = CNTR.SKD_DIR_CD
   AND BL.BKG_NO     = CNTR.BKG_NO
   AND BL.BL_NO      = @[bl_no]
   AND BL.VSL_CD     = SUBSTR(@[vvd],1,4)
   AND BL.SKD_VOY_NO = SUBSTR(@[vvd],5,4)
   AND BL.SKD_DIR_CD = SUBSTR(@[vvd],9,1)
#if (${cntr_chk} != '')
   AND CNTR.CNTR_NO IN (
#foreach($cntrNo IN ${cntr_no})  
'$cntrNo',  
#end  
'')
#end			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
