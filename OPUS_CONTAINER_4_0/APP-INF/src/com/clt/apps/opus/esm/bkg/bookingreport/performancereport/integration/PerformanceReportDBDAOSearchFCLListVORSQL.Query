<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchFCLListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
#if (${vvd_chk} =='T')
	SELECT
		ROW_NUMBER() OVER (ORDER BY A.BKG_NO) SEQ,
		/*DENSE_RANK() OVER( ORDER BY A.BKG_NO) SEQ,*/
--		ROW_NUMBER() OVER (PARTITION BY A.BKG_NO  ORDER BY A.BKG_NO ),
	    A.BKG_NO BKG_NO,
	    A.BL_NO BL_NO,
	    (SELECT B.VSL_CD||B.SKD_VOY_NO||B.SKD_DIR_CD
	     FROM BKG_VVD B
	     WHERE 1=1
	     AND A.BKG_NO = B.BKG_NO
	     AND B.VSL_SEQ ='1'
	     AND B.VSL_PRE_PST_CD ='S' ) AS VVD_CD,
	    A.PRE_RLY_PORT_CD PRE_PORT,
	    A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD T_VVD_CD,
	    A.SKD_DIR_CD||A.REV_DIR_CD REV_DIR_CD ,
	    A.POR_CD,
	    A.POL_CD,
	    A.POD_CD,
	    A.DEL_CD,
	    A.RCV_TERM_CD ,
	    A.DE_TERM_CD ,
		E.CHG_CD CHG_TP,
--	    TRIM(TO_CHAR(DECODE(E.RAT_AS_QTY,1,1,DECODE(GREATEST(E.RAT_AS_QTY,1),1,0,E.RAT_AS_QTY)),'99999999.99')) RATED_AS,
        TRIM(TO_CHAR(E.RAT_AS_QTY,'99999990.999')) RATED_AS,
	    TRIM(TO_CHAR(E.CHG_UT_AMT,'999999999990.99')) RATE,
	    E.RAT_UT_CD PER,
	    E.CURR_CD CUR,
	    E.RAT_UT_CD,
	    TRIM(TO_CHAR(E.CHG_AMT,'999999999990.99')) AMOUNT, 
	    E.N3PTY_RCV_OFC_CD PAY_OFC,
		E.FRT_TERM_CD PC,
	    A.SC_NO,
	    A.RFA_NO,
	    E.TRF_ITM_NO TARIFF_NO,
	    (SELECT SUBSTR(REPLACE(B1.CUST_NM,(chr(10)), ' '),1,150) 
	     FROM BKG_CUSTOMER B1
	     WHERE 1=1
	     AND A.BKG_NO = B1.BKG_NO
	     AND B1.BKG_CUST_TP_CD ='S' )AS SHPR_NM,
	     (SELECT SUBSTR(REPLACE(B2.CUST_NM,(chr(10)), ' '),1,150)
	     FROM BKG_CUSTOMER B2
	     WHERE 1=1 
	     AND A.BKG_NO = B2.BKG_NO
	     AND B2.BKG_CUST_TP_CD ='C') AS CNEE_NM,
	    (SELECT SUBSTR(REPLACE(G.CMDT_DESC, (chr(10)), ' '),1,150) 
	     FROM BKG_BL_MK_DESC G
	     WHERE 1=1
	     AND A.BKG_NO = G.BKG_NO
	     AND G.MK_SEQ ='1'   ) AS CUST_DESC,
		--K.CMDT_CD CMDT,
		(SELECT K.CMDT_CD FROM MDM_COMMODITY K WHERE K.CMDT_CD = A.CMDT_CD) AS CMDT,
		(SELECT SUBSTR(REPLACE(K.CMDT_NM, (chr(10)), ' '),1,150) FROM MDM_COMMODITY K WHERE K.CMDT_CD = A.CMDT_CD) AS CMDT_DESC,
		--SUBSTR(REPLACE(K.CMDT_NM, (chr(10)), ' '),1,150) CMDT_DESC,
	    H.REP_CMDT_CD,
	    SUBSTR(REPLACE(H.REP_CMDT_NM, (chr(10)), ' '),1,150) REP_CMDT,
		(SELECT TO_CHAR(K.RT_APLY_DT,'YYYY-MM-DD') 
		 FROM BKG_RATE K 
		 WHERE K.BKG_NO = A.BKG_NO
		 )RT_APLY_DT,
		'S' SPL_FLG /* SPLIT FLG */
	FROM BKG_BOOKING A, BKG_CHG_RT E, MDM_REP_CMDT  H --MDM_COMMODITY K,   
	WHERE 1=1
	#if(${vvd_cd} !='')
		AND A.VSL_CD = SUBSTR(@[vvd_cd],1,4)
		AND A.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
		AND A.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
	#end
	  AND A.BKG_STS_CD <>'X'
	  AND A.BKG_NO = E.BKG_NO
	  AND E.FRT_INCL_XCLD_DIV_CD ='N'
	  AND A.REP_CMDT_CD = H.REP_CMDT_CD(+)
	#if(${por_cd} !='')
		AND A.POR_CD = @[por_cd]
	#end
	#if(${pol_cd} !='')
		AND A.POL_CD = @[pol_cd]
	#end 
	#if(${pod_cd} !='')
		AND A.POD_CD = @[pod_cd]
	#end
	#if(${del_cd} !='')
		AND A.DEL_CD = @[del_cd]
	#end
	#if(${sls_ofc} !='')
		AND A.OB_SLS_OFC_CD = @[sls_ofc]
	#end
	#if(${bkg_ofc} !='')
		AND A.BKG_OFC_CD = @[bkg_ofc]
	#end
	#if(${trd_cd} !='')
		AND A.SKD_DIR_CD||A.REV_DIR_CD =@[trd_cd]
	#end
	#if(${sc_no} !='')
		AND A.SC_NO =@[sc_no]
	#end
ORDER BY A.BKG_NO, E.DP_SEQ
/* actual */
#elseif (${vvd_chk} =='A')
	SELECT
  		ROW_NUMBER() OVER (ORDER BY A.BKG_NO) SEQ,
		/*DENSE_RANK() OVER( ORDER BY A.BKG_NO) SEQ,*/
--		ROW_NUMBER() OVER (PARTITION BY A.BKG_NO  ORDER BY A.BKG_NO ),
		A.BKG_NO,
	    A.BL_NO,
	    B.VSL_CD||B.SKD_VOY_NO||B.SKD_DIR_CD VVD_CD,
	    A.PRE_RLY_PORT_CD PRE_PORT,
	    A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD T_VVD_CD,
	    A.SKD_DIR_CD||A.REV_DIR_CD REV_DIR_CD ,
	    A.POR_CD,
	    A.POL_CD,
	    A.POD_CD,
	    A.DEL_CD,
	    A.RCV_TERM_CD,
	    A.DE_TERM_CD,
	    E.CHG_CD CHG_TP,
--	    TRIM(TO_CHAR(DECODE(E.RAT_AS_QTY,1,1,DECODE(GREATEST(E.RAT_AS_QTY,1),1,0,E.RAT_AS_QTY)),'99999999.99')) RATED_AS,
        TRIM(TO_CHAR(E.RAT_AS_QTY,'99999990.999')) RATED_AS,
	    TRIM(TO_CHAR(E.CHG_UT_AMT,'999999999990.99')) RATE,
	    E.RAT_UT_CD PER,
	    E.CURR_CD CUR,
	    E.RAT_UT_CD,
	    TRIM(TO_CHAR(E.CHG_AMT,'999999999990.99')) AMOUNT, 
	    E.N3PTY_RCV_OFC_CD PAY_OFC,
		E.FRT_TERM_CD PC,
	    A.SC_NO,
	    A.RFA_NO,
	    E.TRF_ITM_NO TARIFF_NO,
	    SUBSTR(REPLACE(B1.CUST_NM,(chr(10)), ' '),1,150) SHPR_NM,
	    SUBSTR(REPLACE(B2.CUST_NM,(chr(10)), ' '),1,150) CNEE_NM,
	    SUBSTR(REPLACE(G.CMDT_DESC, (chr(10)), ' '),1,150) CUST_DESC,
	    K.CMDT_CD CMDT,
	    SUBSTR(REPLACE(K.CMDT_NM, (chr(10)), ' '),1,150) CMDT_DESC,
	    H.REP_CMDT_CD,
	    SUBSTR(REPLACE(H.REP_CMDT_NM, (chr(10)), ' '),1,150) REP_CMDT,
		(SELECT TO_CHAR(K.RT_APLY_DT,'YYYY-MM-DD') 
		 FROM BKG_RATE K 
		 WHERE K.BKG_NO = A.BKG_NO
		 )RT_APLY_DT,
		'S' SPL_FLG /* SPLIT FLG */
	FROM BKG_VVD B, BKG_BOOKING A, BKG_CHG_RT E, BKG_CUSTOMER B1,
	     BKG_CUSTOMER B2, BKG_BL_MK_DESC G, MDM_COMMODITY K,   MDM_REP_CMDT  H, BKG_VVD I
	WHERE 1=1
	  AND A.BKG_NO = E.BKG_NO
	  AND E.FRT_INCL_XCLD_DIV_CD ='N'
	  AND A.BKG_NO = B1.BKG_NO
	  AND A.BKG_NO = B2.BKG_NO
	  AND B1.BKG_CUST_TP_CD ='S'
	  AND B2.BKG_CUST_TP_CD ='C'
	  AND A.BKG_NO = G.BKG_NO(+)
	  AND G.MK_SEQ(+) ='1'  
	  AND A.BKG_STS_CD <>'X'
	  AND A.CMDT_CD = K.CMDT_CD(+)
	  AND A.REP_CMDT_CD = H.REP_CMDT_CD(+)
	  AND A.BKG_NO = B.BKG_NO
	  AND I.bkg_no(+)=a.bkg_no
	  AND I.VSL_PRE_PST_CD(+) ='S'
	  AND I.VSL_SEQ(+) ='1'
	#if(${vvd_cd} !='')
		AND B.VSL_CD = SUBSTR(@[vvd_cd],1,4)
		AND B.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
		AND B.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
	#end
	#if(${por_cd} !='')
		AND A.POR_CD = @[por_cd]
	#end
	#if(${pol_cd} !='')
		AND A.POL_CD = @[pol_cd]
	#end 
	#if(${pod_cd} !='')
		AND A.POD_CD = @[pod_cd]
	#end
	#if(${del_cd} !='')
		AND A.DEL_CD = @[del_cd]
	#end
	#if(${sls_ofc} !='')
		AND A.OB_SLS_OFC_CD = @[sls_ofc]
	#end
	#if(${bkg_ofc} !='')
		AND A.BKG_OFC_CD = @[bkg_ofc]
	#end
	#if(${trd_cd} !='')
		AND A.SKD_DIR_CD||A.REV_DIR_CD =@[trd_cd]
	#end
	#if(${sc_no} !='')
		AND A.SC_NO =@[sc_no]
	#end
ORDER BY A.BKG_NO, E.DP_SEQ
#end			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="sls_ofc" type="12" value="" out="N"/>
				<param name="bkg_ofc" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
