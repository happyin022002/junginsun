<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchVslSeqListRSQL">
			<desc><![CDATA[VSL_SEQ를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VSL_SEQ FROM BKG_VVD
WHERE BKG_NO   = @[bkg_no]
AND VSL_CD     = SUBSTR(@[vvd_cd], 1, 4)
AND SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
AND SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
