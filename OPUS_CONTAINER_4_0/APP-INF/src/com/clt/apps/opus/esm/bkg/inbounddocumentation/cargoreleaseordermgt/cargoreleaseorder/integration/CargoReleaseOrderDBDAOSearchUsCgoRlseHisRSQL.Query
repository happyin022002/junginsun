<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchUsCgoRlseHisRSQL">
			<desc><![CDATA[UI_BKG_0923 Inbound Cargo Release for POD Office_Popup History 리스트 조회]]></desc>
			<sql><![CDATA[
SELECT RHIS.BL_NO               AS BL_NO
      ,TO_CHAR(RHIS.EVNT_DT, 'YYYY-MM-DD HH24:MI', 'NLS_DATE_LANGUAGE=ENGLISH')  AS EVNT_DT
      ,RHIS.FRT_CLT_FLG         AS FRT_CLT_FLG
      ,RHIS.OBL_RDEM_FLG        AS OBL_RDEM_FLG
      ,RHIS.CSTMS_CLR_CD        AS CSTMS_CLR_CD
      ,RHIS.MRN_TML_EDI_SND_CD  AS MRN_TML_EDI_SND_CD
      ,TMCD.INTG_CD_VAL_DP_DESC AS CGOR_TEAM_CD_DESC
      ,RHIS.CGOR_EVNT_NM        AS CGOR_EVNT_NM 
      , EVNT_OFC_CD || '/' || EVNT_USR_ID         AS USR_NM
FROM BKG_CGO_RLSE_HIS RHIS
   , (SELECT INTG_CD_VAL_CTNT
           , INTG_CD_VAL_DP_DESC
        FROM COM_INTG_CD_DTL
       WHERE INTG_CD_ID = 'CD02373' ) TMCD  
WHERE RHIS.BL_NO = @[bl_no]
  AND TMCD.intg_cd_val_ctnt(+) = RHIS.CGOR_TEAM_CD
  AND RHIS.CGOR_EVNT_NM <> 'EDI_CGO_REL_01'  
ORDER BY RHIS.EVNT_DT DESC, RHIS.HIS_SEQ DESC			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
