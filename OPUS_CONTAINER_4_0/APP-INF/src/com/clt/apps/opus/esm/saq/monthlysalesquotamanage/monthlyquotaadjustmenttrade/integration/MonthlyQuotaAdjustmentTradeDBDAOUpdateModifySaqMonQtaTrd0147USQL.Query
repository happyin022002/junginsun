<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentTradeDBDAOUpdateModifySaqMonQtaTrd0147USQL">
			<desc><![CDATA[SAQ_MON_QTA_TRD의 지정된 ibflag 값에 따라 DB에 반영한다.]]></desc>
			<sql><![CDATA[
UPDATE SAQ_MON_QTA_TRD SET
    LOD_QTY = @[lod_qty] * ${unit_flag},
    GRS_RPB_REV = @[grs_rpb_rev] / ${unit_flag}
WHERE
    MQTA_STEP_CD = @[mqta_step_cd] AND
    BSE_YR = @[bse_yr] AND
    BSE_QTR_CD = @[bse_qtr_cd] AND
    TRD_CD = @[trd_cd] AND
    DIR_CD = @[dir_cd] AND
    MQTA_VER_NO = @[mqta_ver_no] AND
    BSE_MON = @[bse_mon] AND
    SPRT_GRP_CD = @[sprt_grp_cd] AND
    BSA_GRP_CD = @[bsa_grp_cd] AND
    CTRT_RHQ_CD = @[ctrt_rhq_cd] AND
    RLANE_CD = @[rlane_cd]			]]></sql>
			<params>
				<param name="lod_qty" type="12" value="" out="N"/>
				<param name="grs_rpb_rev" type="12" value="" out="N"/>
				<param name="mqta_step_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="mqta_ver_no" type="12" value="" out="N"/>
				<param name="bse_mon" type="12" value="" out="N"/>
				<param name="sprt_grp_cd" type="12" value="" out="N"/>
				<param name="bsa_grp_cd" type="12" value="" out="N"/>
				<param name="ctrt_rhq_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
