<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyCNRUInfoAtCanadaUSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_CNTR
   SET RAIL_CRR_REF_NO = @[in_snp] || @[in_nvobl]
      ,USA_IB_TRSP_NO = DECODE(@[icr_code], '69', TRIM(@[icr_et_no]), USA_IB_TRSP_NO)
 WHERE CNT_CD = 'CA'
   AND BL_NO in( substr(@[in_hjbl], 5, 12) , substr(@[in_hjbl], 1, 12)  )
   AND CNTR_NO LIKE TRIM(@[in_cntr]) || '%'
   AND @[in_snp] IN ( SELECT ATTR_CTNT1 FROM 
						BKG_CSTMS_CD_CONV_CTNT
						WHERE CNT_CD='US'
						AND CSTMS_DIV_ID='US_CA_RAIL_CMPY_CD'
						)			]]></sql>
			<params>
				<param name="in_snp" type="12" value="" out="N"/>
				<param name="in_nvobl" type="12" value="" out="N"/>
				<param name="icr_code" type="12" value="" out="N"/>
				<param name="icr_et_no" type="12" value="" out="N"/>
				<param name="in_hjbl" type="12" value="" out="N"/>
				<param name="in_cntr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
