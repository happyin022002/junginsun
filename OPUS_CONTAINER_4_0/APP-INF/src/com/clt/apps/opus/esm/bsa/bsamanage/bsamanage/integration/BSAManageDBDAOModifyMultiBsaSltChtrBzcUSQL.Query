 SET    VVD_CD               = @[vvd_cd]
       ,BSA_FM_DT            = @[bsa_fm_dt]
       ,BSA_TO_DT            = @[bsa_to_dt]
       ,VSL_CD               = @[vsl_cd]
       ,CO_FNL_BSA_CAPA      = @[co_fnl_bsa_capa]
       ,CO_BSA_BFR_SUB_CAPA  = @[co_bsa_bfr_sub_capa]
       ,VSL_BSA_CHK_FLG      = @[vsl_bsa_chk_flg]
       ,SCHT_DESC            = @[scht_desc]
       ,UPD_USR_ID           = @[upd_usr_id]
       ,UPD_DT               = SYSDATE
 WHERE  BSA_SEQ              = @[bsa_seq]
 AND    TRD_CD               = @[trd_cd]
 AND    RLANE_CD             = @[rlane_cd]
 AND    DIR_CD               = @[dir_cd]
 AND    VSL_SEQ              = @[vsl_seq]			]]></sql>
