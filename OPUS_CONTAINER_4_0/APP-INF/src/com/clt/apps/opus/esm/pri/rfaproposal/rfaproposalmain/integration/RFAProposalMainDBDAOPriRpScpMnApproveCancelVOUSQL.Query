			<sql><![CDATA[
UPDATE PRI_RP_SCP_MN A
   SET EXP_DT =
          (SELECT CTRT_EXP_DT
           FROM   PRI_RP_SCP_DUR
           WHERE  PROP_NO = A.PROP_NO
           AND    AMDT_SEQ = A.AMDT_SEQ
           AND    SVC_SCP_CD = A.SVC_SCP_CD)
 WHERE PROP_NO = @[prop_no]
AND    AMDT_SEQ = @[amdt_seq]			]]></sql>
