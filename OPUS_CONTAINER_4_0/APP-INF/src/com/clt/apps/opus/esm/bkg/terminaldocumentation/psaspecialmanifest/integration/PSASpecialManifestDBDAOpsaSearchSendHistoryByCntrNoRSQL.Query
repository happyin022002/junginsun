<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSASpecialManifestDBDAOpsaSearchSendHistoryByCntrNoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT A.MSG_SND_NO,
       TO_CHAR(A.SND_DT, 'YYYY-MM-DD HH24:MI') AS SND_DT,
       A.SND_USR_ID,
       A.MSG_FUNC_ID,
       A.PSA_DG_DECL_TP_CD,
       A.VSL_CD||SKD_VOY_NO||SKD_DIR_CD AS VVD_CD,
       A.PORT_CD

  FROM BKG_CSTMS_PSA_DG_SND A

 WHERE 1 = 1
#if (${vvd_cd} != '')
   AND A.VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
   AND A.SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
   AND A.SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
#end
#if (${port_cd} != '')
   AND A.PORT_CD = @[port_cd]
#end
#if (${snd_dt_from} != '')
   AND A.SND_DT BETWEEN TO_DATE(REPLACE(@[snd_dt_from], '-', ''), 'YYYYMMDD') AND TO_DATE(REPLACE(@[snd_dt_to], '-', ''), 'YYYYMMDD') + 1
#end
 ORDER BY MSG_SND_NO DESC,
       SND_DT DESC,
       VVD_CD,
       PORT_CD
			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="snd_dt_from" type="12" value="" out="N"/>
				<param name="snd_dt_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
