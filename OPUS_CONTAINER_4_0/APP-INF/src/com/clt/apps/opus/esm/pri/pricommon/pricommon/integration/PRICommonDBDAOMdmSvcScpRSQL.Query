<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOMdmSvcScpRSQL">
			<desc><![CDATA[MDM_SVC_SCP 테의블의 SVC_SCP_CD와 SVC_SCP_NM을 가져오는 query (ESM_PRI_5001)]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD AS CD
     , SVC_SCP_NM AS NM
  FROM MDM_SVC_SCP
 WHERE DELT_FLG = 'N'
 ORDER BY SVC_SCP_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
