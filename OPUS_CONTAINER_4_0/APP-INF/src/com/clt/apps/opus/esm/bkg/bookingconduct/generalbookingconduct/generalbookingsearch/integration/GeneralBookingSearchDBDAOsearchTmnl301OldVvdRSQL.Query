<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchTmnl301OldVvdRSQL">
			<desc><![CDATA[searchTmnl301OldVvd]]></desc>
			<sql><![CDATA[
SELECT OLD_VVD
FROM
(
SELECT	'VVDOLDVSL:'			|| NVL(@[vsl_cd], '')	|| CHR(10)
	||	'VVDOLDLOYD:'		|| NVL(LLOYD_NO, '')			|| CHR(10)
	||	'VVDOLDVSLNAME:'	|| NVL(VSL_ENG_NM, '')			|| CHR(10)
	||	'VVDOLDVSL_CALL_SIGN:'	|| NVL(CALL_SGN_NO, '')			|| CHR(10)
	||	'VVDOLDVOY:'		|| NVL(@[skd_voy_no], '')	|| CHR(10)
	||	'VVDOLDDIR:'		|| NVL(@[skd_dir_cd], '')	|| CHR(10)
    ||	'VVDOLDCONSORT_VOY:'|| VSK_COMMON_PKG.GET_CSSM_VOY_NO_FNC(@[vsl_cd],@[skd_voy_no],@[skd_dir_cd],@[pol_cd],'O')	|| CHR(10) 
	||	'}BL_VVD'							|| CHR(10) OLD_VVD
FROM	MDM_VSL_CNTR
WHERE	VSL_CD = @[vsl_cd]

UNION ALL

SELECT	'VVDOLDVSL:'			|| CHR(10)
	||	'VVDOLDLOYD:'			|| CHR(10)
	||	'VVDOLDVSLNAME:'		|| CHR(10)
	||	'VVDOLDVSL_CALL_SIGN:'	|| CHR(10)
	||	'VVDOLDVOY:'			|| CHR(10)
	||	'VVDOLDDIR:'			|| CHR(10)
    ||	'VVDOLDCONSORT_VOY:'	|| CHR(10)
	||	'}BL_VVD'				|| CHR(10) OLD_VVD
FROM	DUAL
)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
