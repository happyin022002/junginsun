<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApplicationDateRuleDBDAOaddLocationInfoCSQL">
			<desc><![CDATA[Route Locaiton conversion에 데이터를 삽입한다.]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_ROUT_LOC_CONV
(
SVC_SCP_CD,
ORG_LOC_CD,
CONV_LOC_CD,
POR_APPL_FLG,
POL_APPL_FLG,
POD_APPL_FLG,
DEL_APPL_FLG,
PRE_RLY_PORT_APPL_FLG,
PST_RLY_PORT_APPL_FLG,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID, 
UPD_DT
)
VALUES
(
@[svc_scp_cd],
@[org_loc_cd],
@[conv_loc_cd],
DECODE(@[por_appl_flg], '1','Y','0','N'),
DECODE(@[pol_appl_flg], '1','Y','0','N'),
DECODE(@[pod_appl_flg], '1','Y','0','N'),
DECODE(@[del_appl_flg], '1','Y','0','N'),
DECODE(@[pre_rly_port_appl_flg], '1','Y','0','N'),
DECODE(@[pst_rly_port_appl_flg], '1','Y','0','N'),
@[cre_usr_id],
SYSDATE,
@[upd_usr_id],
SYSDATE
)			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_loc_cd" type="12" value="" out="N"/>
				<param name="conv_loc_cd" type="12" value="" out="N"/>
				<param name="por_appl_flg" type="12" value="" out="N"/>
				<param name="pol_appl_flg" type="12" value="" out="N"/>
				<param name="pod_appl_flg" type="12" value="" out="N"/>
				<param name="del_appl_flg" type="12" value="" out="N"/>
				<param name="pre_rly_port_appl_flg" type="12" value="" out="N"/>
				<param name="pst_rly_port_appl_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
