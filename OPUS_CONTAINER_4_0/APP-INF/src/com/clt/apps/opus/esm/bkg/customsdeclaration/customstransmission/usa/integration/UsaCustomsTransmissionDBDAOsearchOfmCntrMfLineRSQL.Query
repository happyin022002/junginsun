<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchOfmCntrMfLineRSQL">
			<desc><![CDATA[dwkim Ofm생성을 위한 CNTR_MF 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(CMDT_GDS_SEQ,'00') cmd_seq, NVL(HAMO_CMDT_CD,' ') icm_ht_cd,
		GRS_WGT*20 icm_wgt_val,
		GRS_WGT icm_wgt_qty,
		NVL(WGT_UT_CD,'KG') icm_wgt_tp,
		PCK_QTY icm_pkg_qty,
		DECODE(NVL(CGO_DESC,' '),' ',BKG_SPCLCHAR_CONV_FNC(NVL(@[cmdt_desc],' '),'M'),BKG_SPCLCHAR_CONV_FNC(NVL(CGO_DESC,' '),'M')) icm_desc
	FROM BKG_CSTMS_ADV_CNTR_MF
	WHERE CNT_CD = 'US'
	  AND BL_NO = @[bl_no]
	  AND CNTR_NO    = TRIM(@[cntr_no])			]]></sql>
			<params>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
