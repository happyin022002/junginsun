<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCContractPartyProposalDBDAOCstPriSpCtrtPtyProviderCSQL">
			<desc><![CDATA[provider contract party insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_CTRT_PTY (
     PROP_NO
,    AMDT_SEQ
,    PRC_CTRT_PTY_TP_CD
,    CTRT_PTY_NM
,    CTRT_PTY_ADDR
,    CTRT_PTY_SGN_NM
,    CTRT_PTY_SGN_TIT_NM
,    PRC_PROG_STS_CD
,    SRC_INFO_CD
,	 N1ST_CMNC_AMDT_SEQ
,    CRE_USR_ID
,    CRE_DT
,    UPD_USR_ID
,    UPD_DT
)
SELECT
     @[prop_no]
,    @[amdt_seq]
,    'P'
,    COM_ConstantMgr_PKG.COM_getCompanyName_FNC -- Provider ëª…
,    NVL(OFC_ADDR,'Please Input')
,    DECODE(PTY.CTRT_PTY_SGN_NM, NULL,'Please Input',PTY.CTRT_PTY_SGN_NM)
,    DECODE(PTY.CTRT_PTY_SGN_TIT_NM, NULL,'Please Input',CTRT_PTY_SGN_TIT_NM)
,    'I'
,    'NW'
,	 @[amdt_seq]
,    @[cre_usr_id]
,    sysdate
,    @[upd_usr_id]
,    sysdate
FROM MDM_ORGANIZATION ORG
,(
SELECT PTY.CTRT_PTY_SGN_NM
      ,PTY.CTRT_PTY_SGN_TIT_NM
      ,PROP_OFC_CD
FROM   PRI_SP_MN MN
      ,PRI_SP_CTRT_PTY PTY
WHERE  MN.PROP_NO = PTY.PROP_NO
AND    MN.AMDT_SEQ = PTY.AMDT_SEQ
AND    MN.PROP_STS_CD = 'F'
AND    MN.PROP_OFC_CD = @[ofc_cd]
AND    PTY.PRC_CTRT_PTY_TP_CD = 'P'
AND    PTY.UPD_DT =
          (SELECT MAX (PTY.UPD_DT)
           FROM   PRI_SP_MN MN
                 ,PRI_SP_CTRT_PTY PTY
           WHERE  MN.PROP_NO = PTY.PROP_NO
           AND    MN.AMDT_SEQ = PTY.AMDT_SEQ  
		   AND    MN.PROP_OFC_CD = @[ofc_cd]
		   AND    MN.PROP_STS_CD = 'F'
           AND    PTY.PRC_CTRT_PTY_TP_CD = 'P') 
AND   ROWNUM = 1
)PTY
WHERE OFC_CD = @[ofc_cd] 
AND PROP_OFC_CD (+) = OFC_CD			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
