<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOBkgTroActCustVOCSQL">
			<desc><![CDATA[insert Sql]]></desc>
			<sql><![CDATA[
insert into bkg_tro_act_cust (
	tro_act_cust_knd_cd,
	tro_vndr_seq,
	ofc_cd,
	tro_act_rep_seq,
	cnt_cd,
	cust_seq,
	vndr_seq,
	loc_cd,
	zn_cd,
	act_shpr_nm,
	act_shpr_addr,
	act_shpr_addr1,
	act_shpr_addr2,
	act_shpr_addr3,
	cntc_pson_nm,
	cntc_phn_no,
	cntc_fax_no,
	cntc_mphn_no,
	cntc_eml,
	dor_zip_id,
	diff_rmk,
	cre_usr_id,
	cre_dt,
	upd_usr_id,
	upd_dt
) values( 
	@[tro_act_cust_knd_cd],
    nvl((select /*+ index_desc(bkg_tro_act_cust XPKBKG_TRO_ACT_CUST)  */   
               tro_vndr_seq   
          from bkg_tro_act_cust   
         where tro_act_cust_knd_cd = @[tro_act_cust_knd_cd]      
           and rownum = 1 ),0)+1,
	@[ofc_cd],
    @[tro_act_rep_seq], 
	@[cnt_cd],
	@[cust_seq],
	@[vndr_seq],
	@[loc_cd],
	@[zn_cd],
	@[act_shpr_nm],
	@[act_shpr_addr],
	@[act_shpr_addr1],
	@[act_shpr_addr2],
	@[act_shpr_addr3],
	@[cntc_pson_nm],
	@[cntc_phn_no],
	@[cntc_fax_no],
	@[cntc_mphn_no],
	@[cntc_eml],
	@[dor_zip_id],
	@[diff_rmk],
	@[cre_usr_id],
	sysdate,
	@[cre_usr_id],
	sysdate
)			]]></sql>
			<params>
				<param name="tro_act_cust_knd_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="tro_act_rep_seq" type="2" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="act_shpr_nm" type="12" value="" out="N"/>
				<param name="act_shpr_addr" type="12" value="" out="N"/>
				<param name="act_shpr_addr1" type="12" value="" out="N"/>
				<param name="act_shpr_addr2" type="12" value="" out="N"/>
				<param name="act_shpr_addr3" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_phn_no" type="12" value="" out="N"/>
				<param name="cntc_fax_no" type="12" value="" out="N"/>
				<param name="cntc_mphn_no" type="12" value="" out="N"/>
