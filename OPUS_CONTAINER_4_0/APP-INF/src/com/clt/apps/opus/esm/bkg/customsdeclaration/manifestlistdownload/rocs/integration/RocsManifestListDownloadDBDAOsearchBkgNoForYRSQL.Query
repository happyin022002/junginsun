<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchBkgNoForYRSQL">
			<desc><![CDATA[bkg_no select]]></desc>
			<sql><![CDATA[
SELECT BKG.BKG_NO
FROM   BKG_BOOKING BKG, BKG_BL_DOC DOC
WHERE  BKG.BL_NO = substr(@[bl_no],0,12)
AND    BKG.BKG_NO = DOC.BKG_NO
AND    BKG.BKG_STS_CD <> 'X'
AND    BKG.BKG_CGO_TP_CD IN ('F','P','R','B')
AND    DOC.BDR_FLG = 'Y'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
