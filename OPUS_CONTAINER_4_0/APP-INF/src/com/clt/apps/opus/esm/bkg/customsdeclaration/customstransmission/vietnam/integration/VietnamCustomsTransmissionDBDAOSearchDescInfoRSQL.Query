<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VietnamCustomsTransmissionDBDAOSearchDescInfoRSQL">
			<desc><![CDATA[SearchDescInfo]]></desc>
			<sql><![CDATA[
SELECT BKG_SPCLCHAR_CONV_CLOB_FNC(SUBSTR(B.PCK_CMDT_DESC||B.CNTR_CMDT_DESC||M.CMDT_DESC, 1, 6920), 'Y') DESCRIPTION
-- BKG_SPCLCHAR_CONV_FNC(dbms_lob.substr(m.cmdt_desc, 3999), 'Y') DESCRIPTION
 --BKG_SPCLCHAR_CONV_FNC(SUBSTR(M.CMDT_DESC, 1, 70),'Y') DESCRIPTION
FROM BKG_BL_MK_DESC M, BKG_BL_DOC B
WHERE 1=1
AND   M.BKG_NO = B.BKG_NO
AND   B.BKG_NO = @[bkg_no]        			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
