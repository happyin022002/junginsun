<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOupdateBkgCgoListUSQL">
			<desc><![CDATA[BKG_DG_CGO 테이블의 해당 CGO값을 제외처리한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_DG_CGO SET
		CSTMS_EXPT_FLG = 'Y'
       ,N1ST_PRE_VVD_CD = @[vvd_cd] -- VVD Change가 일어나는 경우에 대비하기 위해서 삭제할 당시의 VVD를 입력한다.
WHERE 1=1	
AND	BKG_NO = @[bl_no]
AND CNTR_NO = @[cntr_no]
AND DG_CNTR_SEQ = @[spcl_cntr_seq]
AND CNTR_CGO_SEQ = @[spcl_cgo_seq]			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="spcl_cntr_seq" type="12" value="" out="N"/>
				<param name="spcl_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
