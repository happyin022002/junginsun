<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInbondTransmissionDBDAOsearchInbondArrivalManifestListRSQL">
			<desc><![CDATA[dwkim, 0533조회용]]></desc>
			<sql><![CDATA[
#if (${bl_cntr_gubun} != 'BL')
SELECT A.CNTR_NO
      ,A.BL_NO
      ,A.VVD
      ,A.DEL_CD
      ,A.CSTMS_LOC_CD
      ,A.IBD_TRSP_NO
      ,A.IBD_TP_CD
      ,NVL(A.ARR_DT, MAX(A.ARR_DT) OVER(PARTITION BY A.VVD,A.CNTR_NO ORDER BY A.VVD,A.CNTR_NO)) AS ARR_DT
      ,A.TRSP_ISS_DT
      ,NVL(A.ARR_TIME, MAX(A.ARR_TIME) OVER(PARTITION BY A.VVD,A.CNTR_NO ORDER BY A.VVD,A.CNTR_NO)) AS ARR_TIME
      ,A.TRSP_ISS_TIME
      ,A.USA_LST_LOC_CD
      ,A.MJR_IBD_AUTH_DT
      ,A.MJR_IBD_AUTH_TIME
      ,A.XPT_DT,A.XPT_TIME
      ,A.XPT_ACPT_DT
      ,A.XPT_ACPT_TIME
      ,A.CNMV_STS_CD
      ,A.CSTMS_POD_CD
      ,A.POD_CD
      ,A.HUB_LOC_CD
      ,A.FRT_CLT_FLG
      ,A.OBL_RDEM_FLG
      ,A.CSTMS_CLR_CD
      ,MAX(A.ARR_FLG) OVER(PARTITION BY A.VVD,A.CNTR_NO ORDER BY A.VVD,A.CNTR_NO) AS ARR_FLG
      ,MAX(A.XPT_FLG) OVER(PARTITION BY A.VVD,A.CNTR_NO ORDER BY A.VVD,A.CNTR_NO) AS XPT_FLG
      ,A.TRSP_AUTH_DT
      ,A.TRSP_AUTH_TIME
      ,CASE WHEN LENGTH(TRIM(SUBSTR(A.PKUP, 1, 20))) > 0
                 AND  LENGTH(TRIM(SUBSTR(A.PKUP, 21, 20))) > 0
                 AND  LENGTH(TRIM(SUBSTR(A.PKUP, 41))) > 0
            THEN TRIM(SUBSTR(A.PKUP, 1, 20))
            ELSE A.PKUP_NO
        END AS PKUP_NO
      ,CASE WHEN LENGTH(TRIM(SUBSTR(A.PKUP, 1, 20))) > 0
                 AND  LENGTH(TRIM(SUBSTR(A.PKUP, 21, 20))) > 0
                 AND  LENGTH(TRIM(SUBSTR(A.PKUP, 41))) > 0
            THEN TRIM(SUBSTR(A.PKUP, 21, 20))
            ELSE A.YD_CD
        END AS YD_CD
      ,CASE WHEN LENGTH(TRIM(SUBSTR(A.PKUP, 1, 20))) > 0
                 AND  LENGTH(TRIM(SUBSTR(A.PKUP, 21, 20))) > 0
                 AND  LENGTH(TRIM(SUBSTR(A.PKUP, 41))) > 0
            THEN TRIM(SUBSTR(A.PKUP, 41))
            ELSE A.AVAL_DT
        END AS AVAL_DT
      ,'C' AS BL_CNTR_FLAG
      ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', SYSDATE, 'USNYC'), 'YYYY-MM-DD HH24:MI') AS USDATE
      ,A.ARR_DT||A.ARR_TIME AS ARR_DT_BEFORE
      ,A.XPT_DT||A.XPT_TIME AS XPT_DT_BEFORE
      ,(SELECT CNTR_NO
          FROM BKG_CONTAINER
         WHERE BKG_NO = a.bkg_no
           AND CNTR_NO = A.CNTR_NO
           AND CNTR_VOL_QTY != 1
       ) AS PARTIAL_CNTR_NO
      ,'' AS LCL_FLG
      ,'' AS INBOND_LOCAL
      ,'' AS LOCAL_BL_CNT    -- 0408에서 사용됨
      ,'' AS INBOND_BL_CNT   -- 0408에서 사용됨
      ,'' AS TOTAL_BL_CNT    -- 0408에서 사용됨
      ,'' AS POD_SCC         -- 0408에서 사용됨
      ,'' AS DEL_SCC         -- 0408에서 사용됨
  FROM (SELECT
#if (${arr_gubun} == '1'||${arr_gubun} == 'on')
        /*+ INDEX(I XPKBKG_CSTMS_ADV_BL) */
#end
               C.CNTR_NO
              ,I.BKG_NO
              ,I.BL_NO
              ,I.VSL_CD||I.SKD_VOY_NO||I.SKD_DIR_CD AS VVD
              ,I.DEL_CD
              ,I.CSTMS_LOC_CD
              ,IBD.IBD_TRSP_NO
              ,IBD.IBD_TRSP_TP_CD AS IBD_TP_CD
              ,TO_CHAR(C.ARR_DT,'yyyy-mm-dd') ARR_DT
              ,TO_CHAR(C.CGO_ARR_ACPT_DT, 'YYYY-MM-DD') AS TRSP_AUTH_DT
              ,TO_CHAR(IBD.IBD_TRSP_ISS_DT,'yyyy-mm-dd') AS TRSP_ISS_DT
              ,TO_CHAR(C.ARR_DT,'HH24:MI') AS ARR_TIME
              ,TO_CHAR(C.CGO_ARR_ACPT_DT, 'HH24:MI') AS TRSP_AUTH_TIME
              ,TO_CHAR(IBD_TRSP_ISS_DT,'HH24:MI') AS TRSP_ISS_TIME
              ,I.USA_LST_LOC_CD
              ,TO_CHAR(IBD.IBD_TRSP_ACPT_DT,'yyyy-mm-dd') AS MJR_IBD_AUTH_DT
              ,TO_CHAR(IBD.IBD_TRSP_ACPT_DT,'HH24:MI') AS MJR_IBD_AUTH_TIME
              ,TO_CHAR(C.XPT_DT,'yyyy-mm-dd') AS XPT_DT
              ,TO_CHAR(C.XPT_DT,'HH24:MI') AS XPT_TIME
              ,TO_CHAR(C.XPT_ACPT_DT,'yyyy-mm-dd') AS XPT_ACPT_DT
              ,TO_CHAR(C.XPT_ACPT_DT,'HH24:MI') AS XPT_ACPT_TIME
              ,C.PKUP_NO
              ,TO_CHAR(C.AVAL_DT,'yyyy-mm-dd HH24:MI') AS AVAL_DT
              ,C.YD_CD
              ,(SELECT /*+ INDEX_DESC(CTM_MOVEMENT XUK1CTM_MOVEMENT) */  MVMT_STS_CD||' ('||INP_YD_CD||') '||TO_CHAR(CNMV_EVNT_DT, 'YYYY-MM-DD HH24:MI')
                  FROM CTM_MOVEMENT
                 WHERE CNTR_NO = C.CNTR_NO
                   AND ROWNUM = 1
               ) AS CNMV_STS_CD
              ,I.CSTMS_POD_CD
              ,I.POD_CD
              ,I.HUB_LOC_CD
              ,R.FRT_CLT_FLG
              ,R.OBL_RDEM_FLG
              ,R.CSTMS_CLR_CD
              ,DECODE(C.ARR_FLG, 'Y','Y', ' ') AS ARR_FLG
              ,DECODE(C.XPT_FLG, 'Y','Y', ' ') AS XPT_FLG
              ,SUBSTR(MAX(NVL(TO_CHAR(P.PKUP_AVAL_DT,'yyyymmddHH24MISS'),'++++++++++++++')||RPAD(P.PKUP_NO, 20, ' ')||RPAD(P.PKUP_YD_CD, 20, ' ')||TO_CHAR(P.PKUP_AVAL_DT,'yyyy-mm-dd HH24:MI')), 15) PKUP
          FROM BKG_CSTMS_ADV_IBD IBD
              ,BKG_CSTMS_ADV_BL I
              ,BKG_CSTMS_ADV_CNTR C
              ,BKG_CGO_RLSE R
              ,BKG_CSTMS_ADV_RSLT R2
              ,BKG_PKUP_NTC_PKUP_NO P
         WHERE I.CNT_CD = 'US'
           AND I.CNT_CD = IBD.CNT_CD
           AND I.BL_NO = IBD.BL_NO
           AND I.CNT_CD = C.CNT_CD
           AND I.BL_NO = C.BL_NO
           AND I.BL_NO = R.BL_NO(+)
           AND I.CNT_CD = R2.CNT_CD(+)
           AND I.BL_NO = R2.BL_NO(+)
           AND I.MF_NO IS NULL
           AND R2.DSPO_CD(+) = '1J'
           AND R2.ENTR_NO(+) LIKE BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)||'%'
           AND R2.RCV_LOC_CD(+) LIKE 'US%'
           AND R2.CSTMS_SEQ(+) > 0
#if (${vvd} != '')
           AND I.VSL_CD = SUBSTR(@[vvd],1,4)
           AND I.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
           AND I.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
#end
#if (${pod} != '')
           AND I.CSTMS_POD_CD = @[pod]
#end
#if (${bl_no} != '')
           AND I.BL_NO = @[bl_no]
#end
#if (${cntr_no} != '')
           AND C.CNTR_NO = @[cntr_no]
#end
#if (${arr_gubun} == '1'||${arr_gubun} == 'on')
    #if (${eq_ofc} != '' )
           AND NVL(I.DEL_CD, ' ') IN (SELECT LOC_CD FROM MDM_LOCATION WHERE NVL(EQ_CTRL_OFC_CD,' ') LIKE @[eq_ofc]||'%')
           AND NVL(C.YD_CD,' ') in (SELECT NVL(MTY_PKUP_YD_CD,' ') FROM MDM_LOCATION WHERE NVL(EQ_CTRL_OFC_CD,' ') like @[eq_ofc]||'%')
    #end
    #if (${fromd} != '')
           AND C.ARR_DT >= TO_DATE(REPLACE(REPLACE(@[fromd], '-', ''), '/', '')||' '||REPLACE(@[fromt], ':', '') ,'YYYYMMDD HH24MI')
    #end
    #if (${tod} != '')
           AND C.ARR_DT < TO_DATE(REPLACE(REPLACE(@[tod], '-', ''), '/', '')||' '||REPLACE(@[tot], ':', '') ,'YYYYMMDD HH24MI')+1
    #end
#end
#if (${ibd_tp_cd} == 'All')
           AND 1=1
#elseif (${ibd_tp_cd} == '612')
           AND IBD.IBD_TRSP_TP_CD IN ('61','62')
#elseif (${ibd_tp_cd} == '623')
           AND IBD.IBD_TRSP_TP_CD IN ('62','63')
#else
           AND IBD.IBD_TRSP_TP_CD = @[ibd_tp_cd]
#end
#if (${arr_dt} != '')
           AND NVL(C.ARR_FLG, 'N') = @[arr_dt]
#end
#if (${xpt_dt} != '')
           AND NVL(C.XPT_FLG, 'N') = @[xpt_dt]
#end
#if (${edi_error} == 'NA')
           AND C.CGO_ARR_ACPT_DT IS NULL AND @[edi_error] = @[edi_error]
#elseif (${edi_error} == 'NE')
           AND C.XPT_ACPT_DT IS NULL
#end
           AND CASE WHEN NVL(@[hub], 'X') = 'X' THEN 1
                    WHEN NVL(@[hub], ' ') = 'USCHI' THEN
                      INSTR('USCHI,USCLE,USCMH,USCVG,USDET,USDWH,USEVA,USFAR,USGRB,USGRR,USIND,USMKE,USMRV,USMSP,USOMA,USPIA,USRXG,USSDF,USSGF,USSTE,USSTL,USTOL,USWRZ,USDAY,USMKC,USDSM,USKCK', I.HUB_LOC_CD)
                    ELSE INSTR(I.HUB_LOC_CD, @[hub])
                END  > 0
           AND I.MF_STS_CD = 'A'
           AND IBD.IBD_TRSP_NO LIKE BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)||'%'
           AND @[bl_cntr_gubun] = @[bl_cntr_gubun]
           AND C.BL_NO = P.BL_NO(+)
           AND C.CNTR_NO = P.CNTR_NO(+)
      GROUP BY C.CNTR_NO
              ,I.BKG_NO
              ,I.BL_NO
              ,I.VSL_CD
              ,I.SKD_VOY_NO
              ,I.SKD_DIR_CD
              ,I.CSTMS_POD_CD
              ,I.POD_CD
              ,I.HUB_LOC_CD
              ,I.DEL_CD
              ,I.CSTMS_LOC_CD
              ,IBD.IBD_TRSP_NO
              ,IBD.IBD_TRSP_TP_CD
              ,C.ARR_DT
              ,IBD.IBD_TRSP_ISS_DT
              ,C.CGO_ARR_ACPT_DT
              ,I.USA_LST_LOC_CD
              ,IBD.IBD_TRSP_ACPT_DT
              ,C.XPT_DT
              ,C.XPT_ACPT_DT
              ,C.PKUP_NO
              ,C.AVAL_DT
              ,C.YD_CD
              ,C.CNMV_STS_CD
              ,R.FRT_CLT_FLG
              ,R.OBL_RDEM_FLG
              ,R.CSTMS_CLR_CD
              ,C.ARR_FLG
              ,C.XPT_FLG
         ) A
#else
SELECT A.CNTR_NO
      ,A.BL_NO
      ,A.VVD
      ,A.DEL_CD
      ,A.CSTMS_LOC_CD
      ,A.IBD_TRSP_NO
      ,A.IBD_TP_CD
      ,A.ARR_DT
      ,A.TRSP_ISS_DT
      ,A.ARR_TIME
      ,A.TRSP_ISS_TIME
      ,A.USA_LST_LOC_CD
      ,A.MJR_IBD_AUTH_DT
      ,A.MJR_IBD_AUTH_TIME
      ,A.XPT_DT
      ,A.XPT_TIME
      ,A.XPT_ACPT_DT
      ,A.XPT_ACPT_TIME
      ,A.CNMV_STS_CD
      ,A.CSTMS_POD_CD
      ,A.POD_CD
      ,A.HUB_LOC_CD
      ,A.FRT_CLT_FLG
      ,A.OBL_RDEM_FLG
      ,A.CSTMS_CLR_CD
      ,A.ARR_FLG
      ,A.XPT_FLG
      ,A.TRSP_AUTH_DT
      ,A.TRSP_AUTH_TIME
      ,A.PKUP_NO
      ,A.YD_CD
      ,A.AVAL_DT
      ,'B' BL_CNTR_FLAG
      ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', SYSDATE,'USNYC'),'YYYY-MM-DD HH24:MI') AS USDATE
      ,A.ARR_DT||A.ARR_TIME ARR_DT_BEFORE
      ,A.XPT_DT||A.XPT_TIME XPT_DT_BEFORE
      ,'' PARTIAL_CNTR_NO
      ,'' LCL_FLG
      ,'' INBOND_LOCAL
      ,'' LOCAL_BL_CNT   -- 0408에서 사용됨
      ,'' TOTAL_BL_CNT   -- 0408에서 사용됨
  FROM (SELECT
#if (${arr_gubun} == '1'||${arr_gubun} == 'on')
        /*+ INDEX(I XPKBKG_CSTMS_ADV_BL) */
#end
               ' ' AS CNTR_NO
              ,I.BKG_NO
              ,I.BL_NO BL_NO
              ,I.VSL_CD||I.SKD_VOY_NO||I.SKD_DIR_CD AS VVD
              ,I.DEL_CD
              ,I.CSTMS_LOC_CD
              ,IBD.IBD_TRSP_NO
              ,IBD.IBD_TRSP_TP_CD IBD_TP_CD
              ,TO_CHAR(IBD.IBD_TRSP_ARR_DT,'yyyy-mm-dd') AS ARR_DT
              ,TO_CHAR(IBD.IBD_TRSP_ARR_ACPT_DT, 'YYYY-MM-DD') AS TRSP_AUTH_DT
              ,TO_CHAR(IBD.IBD_TRSP_ISS_DT,'yyyy-mm-dd') AS TRSP_ISS_DT
              ,TO_CHAR(IBD.IBD_TRSP_ARR_DT,'HH24:MI') AS ARR_TIME
              ,TO_CHAR(IBD.IBD_TRSP_ARR_ACPT_DT, 'HH24:MI') AS TRSP_AUTH_TIME
              ,TO_CHAR(IBD.IBD_TRSP_ISS_DT,'HH24:MI') AS TRSP_ISS_TIME
              ,I.USA_LST_LOC_CD
              ,TO_CHAR(IBD.IBD_TRSP_ACPT_DT,'yyyy-mm-dd') AS MJR_IBD_AUTH_DT
              ,TO_CHAR(IBD.IBD_TRSP_ACPT_DT,'HH24:MI') AS MJR_IBD_AUTH_TIME
              ,TO_CHAR(IBD.IBD_TRSP_XPT_DT,'yyyy-mm-dd') AS XPT_DT
              ,TO_CHAR(IBD.IBD_TRSP_XPT_DT,'HH24:MI') AS XPT_TIME
              ,TO_CHAR(IBD.IBD_TRSP_XPT_ACPT_DT,'yyyy-mm-dd') AS XPT_ACPT_DT
              ,TO_CHAR(IBD.IBD_TRSP_XPT_ACPT_DT,'HH24:MI') AS XPT_ACPT_TIME
              ,' ' AS PKUP_NO
              ,' ' AS AVAL_DT
              ,' ' AS YD_CD
              ,' ' AS CNMV_STS_CD
              ,I.CSTMS_POD_CD
              ,I.POD_CD
              ,I.HUB_LOC_CD
              ,R.FRT_CLT_FLG
              ,R.OBL_RDEM_FLG
              ,R.CSTMS_CLR_CD
              ,DECODE(IBD.IBD_TRSP_ARR_SND_FLG, 'Y','Y', ' ') AS ARR_FLG
              ,DECODE(IBD.IBD_TRSP_XPT_SND_FLG, 'Y','Y', ' ') AS XPT_FLG
          FROM BKG_CSTMS_ADV_IBD IBD
              ,BKG_CSTMS_ADV_BL I
              ,BKG_CGO_RLSE R
              ,BKG_CSTMS_ADV_RSLT R2
#if (${cntr_no} != '')
              ,BKG_CSTMS_ADV_CNTR C
#end
         WHERE I.CNT_CD = 'US'
           AND I.CNT_CD = IBD.CNT_CD
           AND I.BL_NO = IBD.BL_NO
           AND I.BL_NO = R.BL_NO(+)
           AND I.CNT_CD = R2.CNT_CD(+)
           AND I.BL_NO = R2.BL_NO(+)
           AND I.MF_NO IS NULL
           AND R2.DSPO_CD(+) = '1J'
           AND R2.ENTR_NO(+) LIKE BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)||'%'
           AND R2.RCV_LOC_CD(+) LIKE 'US%'
           AND R2.CSTMS_SEQ(+) > 0
#if (${vvd} != '')
           AND I.VSL_CD = SUBSTR(@[vvd],1,4)
           AND I.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
           AND I.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
#end
#if (${pod} != '')
           AND I.CSTMS_POD_CD = @[pod]
#end
#if (${bl_no} != '')
           AND I.BL_NO = @[bl_no]
#end
#if (${cntr_no} != '')
           AND I.BL_NO = C.BL_NO
           AND C.CNTR_NO = @[cntr_no]
#end
#if (${arr_gubun} == '1'||${arr_gubun} == 'on')
    #if (${eq_ofc} != '' )
           AND NVL(I.DEL_CD, ' ') IN (SELECT LOC_CD FROM MDM_LOCATION WHERE NVL(EQ_CTRL_OFC_CD,' ') LIKE @[eq_ofc]||'%')
    #end
    #if (${fromd} != '')
           AND IBD.IBD_TRSP_ARR_DT >= TO_DATE(REPLACE(REPLACE(@[fromd], '-', ''), '/', '')||' '||REPLACE(@[fromt], ':', '') ,'YYYYMMDD HH24MI')
    #end
    #if (${tod} != '')
           AND IBD.IBD_TRSP_ARR_DT < TO_DATE(REPLACE(REPLACE(@[tod], '-', ''), '/', '')||' '||REPLACE(@[tot], ':', '') ,'YYYYMMDD HH24MI')+1
    #end
#end
#if (${ibd_tp_cd} == 'All')
           AND 1=1
#elseif (${ibd_tp_cd} == '612')
           AND IBD.IBD_TRSP_TP_CD IN ('61','62')
#elseif (${ibd_tp_cd} == '623')
           AND IBD.IBD_TRSP_TP_CD IN ('62','63')
#else
           AND IBD.IBD_TRSP_TP_CD = @[ibd_tp_cd]
#end
#if (${arr_dt} != '')
           AND NVL(IBD.IBD_TRSP_ARR_SND_FLG, 'N') = @[arr_dt]
#end
#if (${xpt_dt} != '')
           AND NVL(IBD.IBD_TRSP_XPT_SND_FLG, 'N') = @[xpt_dt]
#end
#if (${edi_error} == 'NA')
           AND IBD.IBD_TRSP_ARR_ACPT_DT IS NULL AND @[edi_error] = @[edi_error]
#elseif (${edi_error} == 'NE')
           AND IBD.IBD_TRSP_XPT_ACPT_DT IS NULL
#end
           AND CASE WHEN NVL(@[hub], 'X') = 'X' THEN 1
                    WHEN NVL(@[hub], ' ') = 'USCHI' THEN
                        INSTR('USCHI,USCLE,USCMH,USCVG,USDET,USDWH,USEVA,USFAR,USGRB,USGRR,USIND,USMKE,USMRV,USMSP,USOMA,USPIA,USRXG,USSDF,USSGF,USSTE,USSTL,USTOL,USWRZ,USDAY,USMKC,USDSM,USKCK', I.HUB_LOC_CD)
                    ELSE INSTR(I.HUB_LOC_CD, @[hub])
                 END  > 0
           AND I.MF_STS_CD = 'A'
           AND IBD.IBD_TRSP_NO LIKE BKG_GET_BKG_CTMS_CD_FNC('US','AMS_ASGN_CO_CD',1,1)||'%'
      GROUP BY I.BKG_NO
              ,I.BL_NO
              ,I.VSL_CD
              ,I.SKD_VOY_NO
              ,I.SKD_DIR_CD
              ,I.CSTMS_POD_CD
              ,I.POD_CD
              ,I.HUB_LOC_CD
              ,I.DEL_CD
              ,I.CSTMS_LOC_CD
              ,IBD.IBD_TRSP_NO
              ,IBD.IBD_TRSP_TP_CD
              ,IBD.IBD_TRSP_ARR_DT
              ,IBD.IBD_TRSP_ISS_DT
              ,IBD.IBD_TRSP_ARR_ACPT_DT
              ,I.USA_LST_LOC_CD
              ,IBD.IBD_TRSP_ACPT_DT
              ,IBD.IBD_TRSP_XPT_DT
              ,IBD.IBD_TRSP_XPT_ACPT_DT
              ,R.FRT_CLT_FLG
              ,R.OBL_RDEM_FLG
              ,R.CSTMS_CLR_CD
              ,IBD.IBD_TRSP_ARR_SND_FLG
              ,IBD.IBD_TRSP_XPT_SND_FLG
      ) A
#end
			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="eq_ofc" type="12" value="" out="N"/>
				<param name="fromd" type="12" value="" out="N"/>
				<param name="fromt" type="12" value="" out="N"/>
				<param name="tod" type="12" value="" out="N"/>
				<param name="tot" type="12" value="" out="N"/>
				<param name="ibd_tp_cd" type="12" value="" out="N"/>
				<param name="arr_dt" type="12" value="" out="N"/>
				<param name="xpt_dt" type="12" value="" out="N"/>
				<param name="edi_error" type="12" value="" out="N"/>
				<param name="hub" type="12" value="" out="N"/>
				<param name="bl_cntr_gubun" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
