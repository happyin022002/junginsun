<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOReceiptXterVermasCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO ${table_name} (
CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
#if ($table_date_columns.size() > 0)
     #foreach($tableDateColumns IN ${table_date_columns})
        ,${tableDateColumns}
     #end
#end
#if ($table_columns.size() > 0)
     #foreach($tableColumn IN ${table_columns})
		,${tableColumn}
     #end
#end
#if (${table_name} == 'BKG_XTER_VGM_REF_NO' || ${table_name} == 'BKG_XTER_VGM_CUST') 
	, XTER_REF_SEQ
#end
#if (${table_name} == 'BKG_XTER_VGM_RQST') 
	, EDI_PRNR_YD_NM
	, EDI_PRNR_LOC_NM
	, VSL_NM
#end
#if (${table_name} == 'BKG_XTER_VGM_CUST') 
	, VGM_CUST_ADDR
	, VGM_CUST_NM
	, VGM_CUST_CNTC_NM
	, VGM_CUST_CTY_NM
	, VGM_CUST_STE_NM
	, VGM_CUST_PST_ADDR
#end
) VALUES (
@[usr_id]
,SYSDATE
,@[usr_id]
,SYSDATE
#if ($table_date_values.size() > 0)
     #foreach($tableDateValue IN ${table_date_values})
        ,to_date('${tableDateValue}','yyyyMMddhh24miss')
     #end
#end
#if ($table_values.size() > 0)
     #foreach($tableValue IN ${table_values})
         , REPLACE('${tableValue}','$^',CHR(39))
     #end
#end
#if (${table_name} == 'BKG_XTER_VGM_REF_NO' || ${table_name} == 'BKG_XTER_VGM_CUST') 
	, (SELECT COUNT(*)+1 AS XTER_REF_SEQ 
		FROM ${table_name} 
		WHERE XTER_SNDR_ID = @[XTER_SNDR_ID] 
		AND XTER_VGM_DOC_ID = @[XTER_VGM_DOC_ID] 
		AND XTER_VGM_RQST_SEQ = @[XTER_VGM_RQST_SEQ] 
		AND CNTR_NO = @[CNTR_NO])
#end
#if (${table_name} == 'BKG_XTER_VGM_RQST') 
	, @[EDI_PRNR_YD_NM]
	, @[EDI_PRNR_LOC_NM]
	, @[VSL_NM]
#end
#if (${table_name} == 'BKG_XTER_VGM_CUST') 
	, @[VGM_CUST_ADDR]
	, @[VGM_CUST_NM]
	, @[VGM_CUST_CNTC_NM]
	, @[VGM_CUST_CTY_NM]
	, @[VGM_CUST_STE_NM]
	, @[VGM_CUST_PST_ADDR]
#end
)			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="XTER_SNDR_ID" type="12" value="" out="N"/>
				<param name="XTER_VGM_DOC_ID" type="12" value="" out="N"/>
				<param name="XTER_VGM_RQST_SEQ" type="12" value="" out="N"/>
				<param name="CNTR_NO" type="12" value="" out="N"/>
				<param name="EDI_PRNR_YD_NM" type="12" value="" out="N"/>
				<param name="EDI_PRNR_LOC_NM" type="12" value="" out="N"/>
				<param name="VSL_NM" type="12" value="" out="N"/>
				<param name="VGM_CUST_ADDR" type="12" value="" out="N"/>
				<param name="VGM_CUST_NM" type="12" value="" out="N"/>
				<param name="VGM_CUST_CNTC_NM" type="12" value="" out="N"/>
				<param name="VGM_CUST_CTY_NM" type="12" value="" out="N"/>
				<param name="VGM_CUST_STE_NM" type="12" value="" out="N"/>
				<param name="VGM_CUST_PST_ADDR" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
