<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOSearchBunkerTariff2MonthCountRSQL">
			<desc><![CDATA[SearchBunkerTariff2MonthCount SELECT]]></desc>
			<sql><![CDATA[
SELECT COUNT(DISTINCT A1.COST_YRMON) CNT
  FROM COA_MON_VVD A1
     , COA_BNK_TRF A2
 WHERE 1 = 1
   AND A1.SLS_YRMON     = A2.COST_YRMON
   AND A1.SLAN_CD       = A2.SLAN_CD
   AND A1.RLANE_CD      = A2.RLANE_CD
   AND A1.DIR_CD        = A2.SLAN_DIR_CD
   AND A1.SLS_YRMON     LIKE @[sls_yrmon] ||'%'
   AND A1.COST_WK       = @[cost_wk]
   AND A1.DELT_FLG      <> 'Y'
   AND A2.SLAN_CD       = NVL(@[slan_cd], A2.SLAN_CD)
   AND A2.RLANE_CD      = NVL(@[rlane_cd], A2.RLANE_CD)
   AND A2.VSL_CLSS_CAPA = NVL(@[vsl_clss_capa], A2.VSL_CLSS_CAPA)
			]]></sql>
			<params>
				<param name="sls_yrmon" type="12" value="" out="Y"/>
				<param name="cost_wk" type="12" value="" out="Y"/>
				<param name="slan_cd" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="vsl_clss_capa" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
