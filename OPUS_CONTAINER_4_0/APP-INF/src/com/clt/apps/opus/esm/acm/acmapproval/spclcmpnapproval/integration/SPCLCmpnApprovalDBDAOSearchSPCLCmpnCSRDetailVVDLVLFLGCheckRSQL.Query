<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SPCLCmpnApprovalDBDAOSearchSPCLCmpnCSRDetailVVDLVLFLGCheckRSQL">
			<desc><![CDATA[SearchSPCLCmpnCSRDetailVVDLVLFLGCheck]]></desc>
			<sql><![CDATA[
/* GET VVD_LVL_FLG */
SELECT NVL(DECODE(@[vvd_lvl],'1',VVD_LVL_FLG1, 
                    '2',VVD_LVL_FLG2,  
                    '3',VVD_LVL_FLG3, 
                    '4',VVD_LVL_FLG4, 
                    '5',VVD_LVL_FLG5,VVD_LVL_FLG6),'N') AS VVD_LVL_FLG
  FROM MDM_ACCOUNT 
 WHERE ACCT_CD = @[acct_cd]			]]></sql>
			<params>
				<param name="vvd_lvl" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
