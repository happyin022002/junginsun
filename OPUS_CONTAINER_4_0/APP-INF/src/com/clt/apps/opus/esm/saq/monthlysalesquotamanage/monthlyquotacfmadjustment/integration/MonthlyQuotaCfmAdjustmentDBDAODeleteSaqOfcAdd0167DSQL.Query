<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCfmAdjustmentDBDAODeleteSaqOfcAdd0167DSQL">
			<desc><![CDATA[Quota Editing - Office Add Poup 관련 Data 처리]]></desc>
			<sql><![CDATA[
DELETE 
  FROM SAQ_MON_CFM_QTA_OFC_ADD 
 WHERE 1=1 
       AND SKD_VOY_NO = @[skd_voy_no] 
       AND VSL_CD = @[vsl_cd] 
       AND DIR_CD = @[dir_cd] 
       AND RLANE_CD = @[new_rlane_cd] 
       AND TRD_CD = @[trd_cd] 
       AND QTA_TGT_CD = @[qta_tgt_cd] 
       AND BSE_QTR_CD = @[bse_qtr_cd] 
       AND BSE_YR = @[bse_yr] 
       AND CUST_GRP_ID = '00000000000000000000' 
       AND RGN_OFC_CD = @[rgn_ofc_cd] 
       AND SKD_DIR_CD = @[skd_dir_cd] 
       AND MQTA_RLSE_VER_NO = @[mqta_rlse_ver_no]			]]></sql>
			<params>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="new_rlane_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="qta_tgt_cd" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="mqta_rlse_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
