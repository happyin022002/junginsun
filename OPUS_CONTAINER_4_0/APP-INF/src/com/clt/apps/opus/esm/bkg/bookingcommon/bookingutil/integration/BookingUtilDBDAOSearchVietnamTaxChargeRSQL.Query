<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchVietnamTaxChargeRSQL">
			<desc><![CDATA[Vietnam Tax charge check]]></desc>
			<sql><![CDATA[
SELECT
    CASE WHEN (SELECT COUNT(TAX_CNT_CD) FROM MDM_CHARGE WHERE TAX_CNT_CD='VN' AND DELT_FLG ='N' AND CHG_CD = @[chg_cd]) > 0 THEN 'Y' 
    ELSE 'N' 
    END VN_FLG
FROM DUAL			]]></sql>
			<params>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
