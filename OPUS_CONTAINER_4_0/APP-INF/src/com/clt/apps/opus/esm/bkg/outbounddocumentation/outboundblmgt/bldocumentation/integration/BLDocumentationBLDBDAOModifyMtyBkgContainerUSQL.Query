<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOModifyMtyBkgContainerUSQL">
			<desc><![CDATA[Empty Container 수정]]></desc>
			<sql><![CDATA[
UPDATE 	BKG_CONTAINER SET
		MCNTR_BDL_NO = @[mcntr_bdl_no]
       ,BDL_BTM_FLG = @[bdl_btm_flg]
	   ,UPD_USR_ID = @[upd_usr_id]
       ,UPD_DT = SYSDATE
WHERE 	BKG_NO = @[bkg_no]
AND	  	CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="mcntr_bdl_no" type="12" value="" out="N"/>
				<param name="bdl_btm_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
