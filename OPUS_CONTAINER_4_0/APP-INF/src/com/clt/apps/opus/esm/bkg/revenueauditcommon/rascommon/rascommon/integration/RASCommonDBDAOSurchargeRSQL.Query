<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RASCommonDBDAOSurchargeRSQL">
			<desc><![CDATA[Surcharge]]></desc>
			<sql><![CDATA[
SELECT CHG_CD AS CD,
	   CHG_CD||'\t'||CHG_NM AS NM
FROM MDM_CHARGE
WHERE DELT_FLG = 'N'
AND   CHG_CD <> 'OFT'
ORDER BY CHG_CD ASC
			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
