<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDAOremoveHrdCdgCtntDSQL">
			<desc><![CDATA[HrdCdgCtnt 테이블에 있는 정보를 delete한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_HRD_CDG_CTNT
WHERE HRD_CDG_ID = @[hrd_cdg_id]
AND HRD_CDG_ID_SEQ = @[hrd_cdg_id_seq]			]]></sql>
			<params>
				<param name="hrd_cdg_id" type="12" value="" out="N"/>
				<param name="hrd_cdg_id_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
