<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateGuidelineDBDAOPriRgRtRoutViaVODSQL">
			<desc><![CDATA[Route Via 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_RG_RT_ROUT_VIA
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND GLINE_SEQ = @[gline_seq]
#if (${CASCADE_LEVEL} == "1") 
   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#elseif (${CASCADE_LEVEL} == "2") 
   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
   AND ROUT_SEQ = @[rout_seq]
#else 
   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
   AND ROUT_SEQ = @[rout_seq]
   AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND ROUT_VIA_SEQ = @[rout_via_seq]
#end
			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="2" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="rout_seq" type="2" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="rout_via_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
