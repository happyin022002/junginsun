<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOSearchEurTroChangeAfterConfirmRSQL">
			<desc><![CDATA[Search change after confirm]]></desc>
			<sql><![CDATA[
SELECT MST.BKG_NO
	  ,MST.TRO_SEQ
	  ,MST.CNTR_PKUP_YD_CD
	  ,MST.CNTR_RTN_YD_CD
	  ,TO_CHAR(MST.CNTR_RTN_DT,'YYYYMMDDHH24MISS') CNTR_RTN_DT
	  ,MST.HLG_TP_CD
	  ,MST.CNTR_NO
	  ,MST.BKG_TRSP_MZD_CD
	  ,DTL.ZN_CD
FROM BKG_EUR_TRO MST
LEFT OUTER JOIN BKG_EUR_TRO_DTL DTL
ON MST.BKG_NO = DTL.BKG_NO
AND MST.IO_BND_CD = DTL.IO_BND_CD
AND MST.TRO_SEQ = DTL.TRO_SEQ
AND DTL.MLT_STOP_SEQ = 1
WHERE 0=0
AND MST.BKG_NO = @[bkg_no]
AND MST.IO_BND_CD = @[io_bnd_cd]
#if (${tro_seq} != '') 
AND MST.TRO_SEQ = @[tro_seq]
#end
AND NVL(MST.CFM_FLG,'N') = 'Y'
AND NVL(MST.CXL_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
