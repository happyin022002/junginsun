<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NewZealandCustomsTransmissionDBDAOModifyCstmsNzlSendLogCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_NZL_SND_LOG
   SET EDI_RCV_MSG_CTNT = @[edi_rcv_msg_ctnt] ,
       CSTMS_RQST_FLG = @[cstms_rqst_flg] ,
#if (${bl_no}!= '')
       MSG_RGST_ID = 'B/L' ,
       RCV_KEY_DAT_CTNT = @[rcv_key_dat_ctnt] ,
#end
       LOG_FLG = 'Y',
       MRN_NO = @[mrn_no],
       UPD_DT = SYSDATE
 WHERE EDI_REF_ID = @[edi_ref_id]
#if (${bl_no}!= '')
   AND BL_NO = @[bl_no]
#end			]]></sql>
			<params>
				<param name="edi_rcv_msg_ctnt" type="12" value="" out="N"/>
				<param name="cstms_rqst_flg" type="12" value="" out="N"/>
				<param name="rcv_key_dat_ctnt" type="12" value="" out="N"/>
				<param name="mrn_no" type="12" value="" out="N"/>
				<param name="edi_ref_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
