<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerInfoManageDBDAOcheckPrmrySalesRepRSQL">
			<desc><![CDATA[Customer의 Primary Sales Rep을 확인]]></desc>
			<sql><![CDATA[
SELECT SREP_CD
FROM SAM_CUST_SLS_REP_INFO
 WHERE DELT_FLG = 'N'
  AND CUST_CNT_CD = SUBSTR(@[cust_cd],1, 2)
  AND CUST_SEQ = TO_NUMBER(substr(@[cust_cd], 3))
  AND SREP_PRMRY_FLG = 'Y'			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
