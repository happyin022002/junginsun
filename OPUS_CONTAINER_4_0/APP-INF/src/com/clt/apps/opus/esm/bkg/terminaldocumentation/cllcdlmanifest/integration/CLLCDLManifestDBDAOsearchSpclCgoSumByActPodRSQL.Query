		                                                                                   'S', DECODE(CLL.CLL_RMK1, NULL, 0, 'IN/GU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0), 0),
		                'TS', DECODE(DG.IMDG_UN_NO, NULL, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 1, 1), 'O', DECODE(CLL.CLL_RMK1, NULL, 0, 'IN/GU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)),
		                                                                                   'S', DECODE(CLL.CLL_RMK1, NULL, 0, 'IN/GU', 0, DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0)), 0), 0), 0) TS_OT_20,

		DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '5', 0, '7', 0, DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0)),
		               'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '5', 0, '7', 0, DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0)), 0) TS_PCOD_40,
		DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '5', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0),
		               'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '5', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0), 0) TS_PCOD_40H,
		DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0),
		            'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0), 0) TS_PCOD_45,
		DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0),
		    	         'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', DECODE(KR_TML_PRCT_ID, 'P', COUNT(CLL.BKG_NO), 0), 0), 0) TS_PCOD_20
	FROM		BKG_CSTMS_TML_KR_CLL CLL,
		(
		SELECT	DISTINCT IMDG_UN_NO
		FROM 		BKG_DG_CGO
		) DG
	WHERE		CLL.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no],2,3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd],3,3)
	AND 	CLL.BKG_POD_CD IS NOT NULL
	AND  NVL(CLL.POL_YD_CD,' ') like @[in_pol_yd_cd]||'%'
	AND 	DG.IMDG_UN_NO(+) = CLL.CLL_RMK1
	#if (${in_cll_type} == 'LOCAL')
	AND CLL.KR_CLL_TS_CD IS NULL
	#elseif (${in_cll_type} == 'TS')
	AND CLL.KR_CLL_TS_CD IS NOT NULL
	#end
	GROUP BY 	BKG_POD_CD, POD_CD, KR_CLL_TS_CD, MTY_BKG_CD, CNTR_TPSZ_CD, DG.IMDG_UN_NO, CLL_RMK1, KR_TML_PRCT_ID
)
GROUP BY BKG_POD_CD			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
