<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCContractClauseGuidelineDBDAOPriSgCtrtCluzDtlVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	svc_scp_cd
,	gline_seq
,	ctrt_cluz_seq
,	ctrt_cluz_dtl_seq
,	chg_cd
,	ctrt_cluz_ctnt
,	cre_usr_id
,	cre_dt
,	upd_usr_id
,	upd_dt
FROM pri_sg_ctrt_cluz_dtl
WHERE	svc_scp_cd = @[svc_scp_cd]
AND	gline_seq = @[gline_seq]
AND	ctrt_cluz_seq = @[ctrt_cluz_seq]
ORDER BY CHG_CD ASC, ctrt_cluz_dtl_seq ASC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="2" value="" out="N"/>
				<param name="ctrt_cluz_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
