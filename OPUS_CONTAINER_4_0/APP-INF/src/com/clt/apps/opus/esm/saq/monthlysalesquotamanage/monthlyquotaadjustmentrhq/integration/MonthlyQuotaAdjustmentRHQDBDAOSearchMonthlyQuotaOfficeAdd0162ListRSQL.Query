<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaAdjustmentRHQDBDAOSearchMonthlyQuotaOfficeAdd0162ListRSQL">
			<desc><![CDATA[누락 점소추가 List 조회]]></desc>
			<sql><![CDATA[
SELECT SUB_TRD_CD, RLANE_CD, AQ_CD, RGN_OFC_CD
     FROM SAQ_MON_QTA_OFC_ADD
    WHERE MQTA_STEP_CD = @[mqta_step_cd]
      AND BSE_YR       = @[bse_yr]
      AND BSE_QTR_CD   = @[bse_qtr_cd]
      AND TRD_CD       = @[trd_cd]
      AND DIR_CD       = @[dir_cd]
      AND RHQ_CD       = @[rhq_cd]
ORDER BY SUB_TRD_CD, RLANE_CD, DECODE(NVL(AQ_CD, 99), '99', 99, 11)||AQ_CD, RGN_OFC_CD			]]></sql>
			<params>
				<param name="mqta_step_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
