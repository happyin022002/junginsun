<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOremoveCstmsEmlNtfcDSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
DELETE FROM
       BKG_CSTMS_EML_NTFC

 WHERE CNT_CD = @[cnt_cd]
   AND EDI_MSG = @[org_edi_msg]
   AND EDI_MSG_TP_ID = @[org_edi_msg_tp_id]
   AND POL_CD = NVL(@[org_pol_cd], ' ')
   AND POD_CD = NVL(@[org_pod_cd], ' ')
			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="org_edi_msg" type="12" value="" out="N"/>
				<param name="org_edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="org_pol_cd" type="12" value="" out="N"/>
				<param name="org_pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
