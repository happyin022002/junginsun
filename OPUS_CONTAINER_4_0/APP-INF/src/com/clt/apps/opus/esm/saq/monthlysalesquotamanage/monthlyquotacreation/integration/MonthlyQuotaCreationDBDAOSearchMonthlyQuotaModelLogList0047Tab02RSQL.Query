<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MonthlyQuotaCreationDBDAOSearchMonthlyQuotaModelLogList0047Tab02RSQL">
			<desc><![CDATA[Engine Status 조회]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(SYSDATE, 'YYYY.MM.DD HH24:MI:SS') SYS_DATE, 
		TO_CHAR(A.EXEC_DT, 'YYYY/MM/DD HH24:MI:SS') EXEC_DT, 
		A.MOD_NAME MOD_NAME, 
		A.LOG_DESC LOG_DESC
FROM   ENIS_LOG A 
WHERE  1=1       
AND    EXEC_DT  > SYSDATE - @[intervalTime]
AND    MOD_NAME IN ( 
                    -- 분기 진행상태 조회 
                     UPPER('SAQ_MON_PROCESS_CRE'            )                
                    -- 평균단가 생성  
                    ,UPPER('MAIN_SAQ_AVG_COST_CRE'          )          -- 수행시간 : 20분 
                    ,UPPER('SAQ_AVG_COST_IF_CRE'            )                    
                    ,UPPER('SAQ_AVG_COST_CRE'               )          -- 수행시간 : 1분 
                    ,UPPER('SAQ_AVG_COST_OFC_CRE'           )          -- 수행시간 : 1 ~ 2분 
                    -- RBC FORECAST I/F 
                    --,UPPER('SAQ_RBC_FCAST_IF_CRE'           )          -- 수행시간 : 10초 
                    -- SALES FORECAST I/F 
                    ,UPPER('SAQ_MON_FCAST_TRNS_NEW_CRE'     )          -- 수행시간 : 6분 
                    ,UPPER('SAQ_BKG_INFO_CRE_PRC'           )
                    -- LOGICAL CHECK 
                    ,UPPER('SAQ_MON_FCAST_LOGIC_CHK'        ) 
                    -- RBC FORECAST 실적비용 생성 
                    ,UPPER('MAIN_SAQ_MON_RBC_PA_NEW_CRE'    )          -- 수행시간 : 7분 
                    -- MAS 단가 I/F (ETL) CHECK 
                    ,UPPER('SAQ_MON_SMU_UC_CHK'             )          -- 수행시간 : 1분 
                    ,UPPER('SAQ_MON_EMU_CRE_PRC'            )
                    ,UPPER('SAQ_MON_SMU_CRE_PRC'            )
                    -- SALES FORECAST 실적비용 생성 
                    ,UPPER('MAIN_SAQ_MON_PA_NEW_CRE'        )          -- 수행시간 : 6분 
                    -- FORECAST EMU 생성 
                    ,UPPER('MAIN_SAQ_MON_RA_EMU_COST_CRE'   )          -- 수행시간 : 4분 
                    -- FORECAST SMU 생성 
                    ,UPPER('SAQ_MON_RA_SMU_COST_CRE'        )          -- 수행시간 : 14분 
                    -- T.VVD CHECK 
                    ,UPPER('SAQ_MON_TGT_VVD_CHK'            )          -- 수행시간 : 1분 
                    -- FORECAST를 VVD 배부 및 FORECAST SUMMARY 생성 
                    ,UPPER('SAQ_MON_INIT_MDL_SMRY_CRE'      )          -- 수행시간 : 2분           
                    ) 
ORDER BY A.EXEC_DT DESC			]]></sql>
			<params>
				<param name="intervalTime" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
