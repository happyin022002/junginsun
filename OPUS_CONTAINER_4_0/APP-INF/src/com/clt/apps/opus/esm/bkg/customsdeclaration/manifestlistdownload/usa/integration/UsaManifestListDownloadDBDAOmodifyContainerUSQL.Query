<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyContainerUSQL">
			<desc><![CDATA[modifyContainer]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_CSTMS_ADV_CNTR CT
USING ( 
    SELECT @[cnt_cd] AS CNT_CD, 
		   @[bl_no] AS BL_NO, 
		   @[cntr_no] AS CNTR_NO 
	FROM DUAL 
) TM
ON ( CT.CNT_CD = TM.CNT_CD AND 
	 CT.BL_NO = TM.BL_NO AND 
	 CT.CNTR_NO = TM.CNTR_NO 
)
WHEN MATCHED THEN
        UPDATE 	
        SET		UPD_USR_ID = @[upd_usr_id]
        ,		UPD_DT = SYSDATE
		,		CNTR_TPSZ_CD = @[cntr_tpsz_cd]
		,		FULL_MTY_CD = @[full_mty_cd]
		,		IBD_CNTR_STS_CD = 'A'
WHEN NOT MATCHED THEN
        INSERT ( CNT_CD
                ,BL_NO
				,CNTR_NO
			    ,CNTR_TPSZ_CD
				,IBD_CNTR_STS_CD
				,FULL_MTY_CD
                ,CRE_USR_ID
                ,CRE_DT
                ,UPD_USR_ID
                ,UPD_DT )
        VALUES ( @[cnt_cd]
                ,@[bl_no]
				,@[cntr_no]
				,@[cntr_tpsz_cd]
				,NVL(@[ibd_cntr_sts_cd],'A')
                ,@[full_mty_cd]
                ,@[cre_usr_id]
                ,SYSDATE
                ,@[upd_usr_id]
                ,SYSDATE )			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="full_mty_cd" type="12" value="" out="N"/>
				<param name="ibd_cntr_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
