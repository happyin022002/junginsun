<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOModifyPeriodClosingInfoUSQL">
			<desc><![CDATA[Modify Period Closing Info]]></desc>
			<sql><![CDATA[
UPDATE SCO_PERIOD
   SET PRD_APPL_CD = @[prd_appl_cd]
     , EFF_YRMON = @[eff_yrmon]
     , PRD_STS_CD = @[prd_sts_cd]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT = SYSDATE
 WHERE PRD_APPL_CD = @[prd_appl_cd]
   AND EFF_YRMON = @[eff_yrmon]			]]></sql>
			<params>
				<param name="prd_appl_cd" type="12" value="" out="N"/>
				<param name="eff_yrmon" type="12" value="" out="N"/>
				<param name="prd_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
