<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAORemoveInvoicePayScheduleByInvSeqDSQL">
			<desc><![CDATA[SAP_PAY_SKD를 INV_SEQ 조건으로 삭제]]></desc>
			<sql><![CDATA[
DELETE SAP_PAY_SKD
WHERE INV_SEQ = TO_NUMBER(@[inv_seq])			]]></sql>
			<params>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
