<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingIFDBDAOsearchPreviousSakuraArIFToSeqRSQL">
			<desc><![CDATA[searchPreviousSakuraArIFToSeq]]></desc>
			<sql><![CDATA[
SELECT MAX(IF_SEQ_NO) AS IF_SEQ_NO
  FROM SAR_AR_IF
 WHERE IF_DOC_TP_CD ='H1'
   AND REF_DOC_NO = SUBSTR(@[check_if_no], 1, LENGTH(@[check_if_no])-1)			]]></sql>
			<params>
				<param name="check_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
