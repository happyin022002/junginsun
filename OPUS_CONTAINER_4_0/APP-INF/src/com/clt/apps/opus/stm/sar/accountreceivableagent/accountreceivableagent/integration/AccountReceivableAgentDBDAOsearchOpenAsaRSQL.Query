<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOsearchOpenAsaRSQL">
			<desc><![CDATA[searchOpenAsa]]></desc>
			<sql><![CDATA[
WITH PARAM AS (
    SELECT ASA_NO_CTNT1
          ,ASA_NO_CTNT2
          ,ASA_NO_CTNT3
    FROM SAR_ASA_MST
    WHERE ASA_NO = @[asa_no]
)
SELECT ASA_NO
FROM SAR_ASA_MST A
 ,PARAM
WHERE A.ASA_NO_CTNT1 = PARAM.ASA_NO_CTNT1
AND A.ASA_STS_CD = 'O'
AND TO_NUMBER(A.ASA_NO_CTNT2 || A.ASA_NO_CTNT3) >= TO_NUMBER(PARAM.ASA_NO_CTNT2 || PARAM.ASA_NO_CTNT3)
ORDER BY TO_NUMBER(A.ASA_NO_CTNT2 || A.ASA_NO_CTNT3) ASC			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
