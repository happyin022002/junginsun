<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOModifyScoBatHisUSQL">
			<desc><![CDATA[ModifyScoBatHis]]></desc>
			<sql><![CDATA[
UPDATE SCO_BAT_HIS 
SET
	  BAT_RSLT_CD = @[bat_rslt_cd] 
#if (${bat_rslt_cd} == 'B' || ${bat_rslt_cd} == 'S') 
	 ,BAT_END_DT = SYSDATE
#end
#if (${bat_rslt_desc} != '') 
	 ,BAT_RSLT_DESC = SUBSTR(@[bat_rslt_desc],0,1000)
#end
	 ,UPD_USR_ID = @[upd_usr_id]
	 ,UPD_DT = SYSDATE
WHERE BAT_SEQ = @[bat_seq]			]]></sql>
			<params>
				<param name="bat_rslt_cd" type="12" value="" out="N"/>
				<param name="bat_rslt_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bat_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
