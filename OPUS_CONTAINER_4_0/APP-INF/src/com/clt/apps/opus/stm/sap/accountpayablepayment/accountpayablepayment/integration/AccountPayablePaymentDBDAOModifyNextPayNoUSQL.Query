<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOModifyNextPayNoUSQL">
			<desc><![CDATA[AccountPayablePaymentDBDAOModifyNextPayNoUSQL]]></desc>
			<sql><![CDATA[
UPDATE SCO_LU_DTL
SET  ATTR_CTNT1 = @[pay_no]
    ,UPD_USR_ID = @[usr_id]
    ,UPD_DT = SYSDATE    
WHERE LU_TP_CD ='PAYMENT METHOD'
AND LU_CD = @[pay_mzd_lu_cd]			]]></sql>
			<params>
				<param name="pay_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="pay_mzd_lu_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
