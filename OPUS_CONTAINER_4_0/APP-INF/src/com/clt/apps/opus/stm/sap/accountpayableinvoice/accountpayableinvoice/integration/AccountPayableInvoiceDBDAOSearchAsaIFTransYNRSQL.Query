<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchAsaIFTransYNRSQL">
			<desc><![CDATA[SearchAsaIFTransYN]]></desc>
			<sql><![CDATA[
SELECT DECODE( COUNT(D.ATTR_CTNT13), 0, 'N', 'Y') AS TRANS_YN
FROM    SAP_INV_HDR H
      , SAP_INV_DTL D
      , SCO_LEGR_CD_CMB SLCC
WHERE   H.INV_SEQ = D.INV_SEQ
AND     D.DTRB_CD_CMB_SEQ = SLCC.CD_CMB_SEQ
AND     H.INV_SEQ = TO_NUMBER (@[inv_seq])
AND     SLCC.SGM_CTNT4    = ( SELECT SLD.LU_CD 
                            FROM SCO_LU_HDR SLH, SCO_LU_DTL SLD 
                            WHERE SLH.LU_APPL_CD = 'SAP' 
                            AND SLH.LU_TP_CD = 'ASA CLEARING ACCOUNT' 
                            AND SLH.LU_TP_CD = SLD.LU_TP_CD
                            AND SLD.ENBL_FLG = 'Y'
                            AND ROWNUM = 1 )  
AND     D.ATTR_CTNT13 = 'Y'			]]></sql>
			<params>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
