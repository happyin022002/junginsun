<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOSearchLoginUserEmlRSQL">
			<desc><![CDATA[Login user 의 e-mail adress 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(USR.DFLT_EML, USR.USR_EML) AS USR_EML 
  FROM COM_USER USR
 WHERE USR.USR_ID = @[usr_id]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
