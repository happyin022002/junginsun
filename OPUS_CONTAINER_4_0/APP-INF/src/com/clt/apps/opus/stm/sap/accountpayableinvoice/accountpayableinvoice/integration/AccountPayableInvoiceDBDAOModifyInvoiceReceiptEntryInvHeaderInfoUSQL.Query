<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyInvoiceReceiptEntryInvHeaderInfoUSQL">
			<desc><![CDATA[Input된 전표 ID와 변경하는 입력값을 조건으로 하여 기 존재하는 전표의 Header 내역에 해당 값을 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_HDR
SET     PAY_MZD_LU_CD = @[pay_mzd_lu_cd]
      , AP_PAY_GRP_LU_CD = @[ap_pay_grp_lu_cd]
      , INV_TERM_NM = @[inv_term_nm]
      , UPD_DT = SYSDATE
      , UPD_USR_ID = @[usr_id]
WHERE   INV_SEQ = @[inv_seq]			]]></sql>
			<params>
				<param name="pay_mzd_lu_cd" type="12" value="" out="N"/>
				<param name="ap_pay_grp_lu_cd" type="12" value="" out="N"/>
				<param name="inv_term_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
