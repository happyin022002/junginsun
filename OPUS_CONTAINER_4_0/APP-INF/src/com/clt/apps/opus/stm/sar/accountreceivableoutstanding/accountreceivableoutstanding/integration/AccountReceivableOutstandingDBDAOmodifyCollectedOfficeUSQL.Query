<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOmodifyCollectedOfficeUSQL">
			<desc><![CDATA[Receipt 된 Office 정보를 OTS Header 에 Update]]></desc>
			<sql><![CDATA[
UPDATE SAR_OTS_HDR SET
    CLT_OFC_CD = @[clt_ofc_cd]
    , UPD_USR_ID = @[upd_usr_id]
    , UPD_DT = SYSDATE
WHERE RHQ_CD = @[rhq_cd]
AND OTS_OFC_CD = @[ots_ofc_cd]
AND BL_NO = @[bl_no]
AND INV_NO = @[inv_no]			]]></sql>
			<params>
				<param name="clt_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
