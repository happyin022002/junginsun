<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivalbeAgentDBDAOremoveAgentCollectionListVODSQL">
			<desc><![CDATA[Agent Collection List remove]]></desc>
			<sql><![CDATA[
DELETE FROM SAR_AGN_CLT_RFND_MST 
 WHERE ASA_NO    = @[asa_no]
   AND BL_NO     = @[bl_no]
   AND INV_NO     = @[inv_no]
   AND CHG_TP_CD = @[chg_tp_cd]
   AND ASA_TP_CD = @[asa_tp_cd]			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
				<param name="asa_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
