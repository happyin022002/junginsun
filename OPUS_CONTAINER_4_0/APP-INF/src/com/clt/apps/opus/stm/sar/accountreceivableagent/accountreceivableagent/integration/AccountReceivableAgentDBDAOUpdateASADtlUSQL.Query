<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOUpdateASADtlUSQL">
			<desc><![CDATA[Update SAR_ASA_DTL]]></desc>
			<sql><![CDATA[
    UPDATE SAR_ASA_DTL
    SET
           UPD_DT = SYSDATE
           #if( ${asa_dtl_desc} != '' )
           , ASA_DTL_DESC = @[asa_dtl_desc]
           #end
           #if( ${asa_dr_amt} != '' )
           , ASA_DR_AMT = @[asa_dr_amt]
           #end
           #if( ${asa_cr_amt} != '' )
           , ASA_CR_AMT = @[asa_cr_amt]
           #end
           #if( ${eff_dt} != '' )
           , EFF_DT = @[eff_dt]
           #end
           #if( ${asa_dtl_tp_cd} != '' )
           , ASA_DTL_TP_CD = @[asa_dtl_tp_cd]
           #end
           #if( ${upd_usr_id} != '' )
           , UPD_USR_ID = @[upd_usr_id]
           #end
    WHERE  1=1
    AND    ASA_NO = @[asa_no]
    AND    ASA_DTL_SEQ = @[asa_dtl_seq]
			]]></sql>
			<params>
				<param name="asa_dtl_desc" type="12" value="" out="N"/>
				<param name="asa_dr_amt" type="12" value="" out="N"/>
				<param name="asa_cr_amt" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="asa_dtl_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="asa_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
