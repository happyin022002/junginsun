<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOsearchPeriodStatusRSQL">
			<desc><![CDATA[Period Status 값 조회]]></desc>
			<sql><![CDATA[
SELECT PRD_STS_CD
FROM SCO_PERIOD
WHERE PRD_APPL_CD = 'AR'
AND EFF_YRMON = SUBSTR(REPLACE(@[gl_dt], '-', ''), 1, 6)			]]></sql>
			<params>
				<param name="gl_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
