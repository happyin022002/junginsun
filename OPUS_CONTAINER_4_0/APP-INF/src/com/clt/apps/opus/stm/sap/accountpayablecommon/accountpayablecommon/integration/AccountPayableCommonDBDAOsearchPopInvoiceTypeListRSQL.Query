<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOsearchPopInvoiceTypeListRSQL">
			<desc><![CDATA[STM_SAP_0003의 검색조건 VO]]></desc>
			<sql><![CDATA[
SELECT  sld.lu_cd AS lu_cd
      , sld.lu_desc AS lu_desc
FROM    sco_lu_hdr slh
      , sco_lu_dtl sld
WHERE   slh.lu_tp_cd = sld.lu_tp_cd 
AND     slh.lu_tp_cd = 'INVOICE TYPE'
AND     NVL(sld.enbl_flg, 'Y') = 'Y'
AND     NVL(sld.lu_st_dt, TRIM(SYSDATE)) >= TRIM(SYSDATE) 
#if (${inv_tp_cd} != '')
AND sld.lu_cd LIKE '%' || @[inv_tp_cd] || '%'
#end			]]></sql>
			<params>
				<param name="inv_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
