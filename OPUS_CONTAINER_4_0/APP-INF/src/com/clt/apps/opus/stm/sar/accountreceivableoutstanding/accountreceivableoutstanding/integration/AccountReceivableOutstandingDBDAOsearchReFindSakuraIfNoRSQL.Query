<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOsearchReFindSakuraIfNoRSQL">
			<desc><![CDATA[searchReFindSakuraIfNo]]></desc>
			<sql><![CDATA[
SELECT SUBSTR( XMLAGG (XMLELEMENT (X, ',', IF_NO )
                 ORDER BY IF_NO ).EXTRACT( '//text()' ) , 2) AS IF_NO
      ,REF_NO
      ,CURR_CD
FROM 
( 
    SELECT DISTINCT SOH.IF_NO
         ,SOH.REF_NO
         ,SOC.BL_CURR_CD AS CURR_CD
      FROM SAR_OTS_HIS SOH
         ,SAR_OTS_CHG SOC
     WHERE SOH.OTS_HIS_SEQ IN ( 
        #foreach( $key IN ${ots_his_list}) 
            #if($velocityCount < $ots_his_list.size())
                '$key',
            #else
                '$key'
            #end
        #end
           ) 
       AND SOH.OTS_HIS_SEQ = SOC.OTS_HIS_SEQ
) 
GROUP BY REF_NO,CURR_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
