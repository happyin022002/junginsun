<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOSearchOriginAdjustHisSeqListRSQL">
			<desc><![CDATA[get ORIGINAL ADJUST HISTORY SEQUENCE]]></desc>
			<sql><![CDATA[
SELECT
    ADJ_HIS_SEQ
FROM
    SAR_ADJ_HIS
WHERE  ADJ_NO = @[adj_no]
AND OTS_HIS_SEQ = @[ots_his_seq]
AND CHG_TP_CD = @[chg_tp_cd]
AND ADJ_HIS_SEQ <> @[adj_his_seq]			]]></sql>
			<params>
				<param name="adj_no" type="12" value="" out="N"/>
				<param name="ots_his_seq" type="12" value="" out="N"/>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
				<param name="adj_his_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
