<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOAddInvoicePayScheduleCSQL">
			<desc><![CDATA[Header의 정보를 기준으로 전표의 지불 예정 내역인 Payment Schedule 내역을 생성]]></desc>
			<sql><![CDATA[
INSERT INTO SAP_PAY_SKD
(
   INV_SEQ
 , PAY_SKD_NO
 , PAY_RMN_AMT
 , DUE_DT
 , PAY_GRS_AMT
 , INV_HLD_FLG
 , PAY_MZD_LU_CD
 , PAY_PRIO_CD
 , PAY_STS_FLG
 , INV_BAT_SEQ
 , ATTR_CTNT1
 , ATTR_CTNT2
 , ATTR_CTNT3
 , ATTR_CTNT4
 , ATTR_CTNT5
 , ATTR_CTNT6
 , ATTR_CTNT7
 , ATTR_CTNT8
 , ATTR_CTNT9
 , ATTR_CTNT10
 , ATTR_CTNT11
 , ATTR_CTNT12
 , ATTR_CTNT13
 , ATTR_CTNT14
 , ATTR_CTNT15
 , ATTR_CATE_NM
 , XTER_BANK_ACCT_SEQ
 , PAY_BAT_RUN_SEQ
 , REMIT_VNDR_NO
 , CRE_USR_ID
 , CRE_DT
 , UPD_USR_ID
 , UPD_DT
)
VALUES
(
 @[inv_seq]
,@[pay_skd_no]
,ROUND( TO_NUMBER( REPLACE( @[pay_rmn_amt], ',', '') ) , 3 ) 
,TO_DATE( @[due_dt], 'YYYY-MM-DD')
,ROUND( TO_NUMBER( REPLACE( @[pay_grs_amt], ',', '') ) , 3 ) 
,DECODE ( @[inv_hld_flg], '0', 'N', '1', 'Y', 'N' )
,@[pay_mzd_lu_cd]
,@[pay_prio_cd]
,NVL( @[pay_sts_flg], 'N')
,@[inv_bat_seq]
,@[attr_ctnt1]
,@[attr_ctnt2]
,@[attr_ctnt3]
,@[attr_ctnt4]
,@[attr_ctnt5]
,@[attr_ctnt6]
,@[attr_ctnt7]
,@[attr_ctnt8]
,@[attr_ctnt9]
,@[attr_ctnt10]
,@[attr_ctnt11]
,@[attr_ctnt12]
,@[attr_ctnt13]
,@[attr_ctnt14]
,@[attr_ctnt15]
,@[attr_cate_nm]
,@[xter_bank_acct_seq]
,@[pay_bat_run_seq]
,@[remit_vndr_no]
,@[usr_id]
,SYSDATE
,@[usr_id]
,SYSDATE
)			]]></sql>
			<params>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="pay_skd_no" type="12" value="" out="N"/>
				<param name="pay_rmn_amt" type="12" value="" out="N"/>
				<param name="due_dt" type="12" value="" out="N"/>
				<param name="pay_grs_amt" type="12" value="" out="N"/>
				<param name="inv_hld_flg" type="12" value="" out="N"/>
				<param name="pay_mzd_lu_cd" type="12" value="" out="N"/>
				<param name="pay_prio_cd" type="12" value="" out="N"/>
				<param name="pay_sts_flg" type="12" value="" out="N"/>
				<param name="inv_bat_seq" type="12" value="" out="N"/>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="attr_ctnt3" type="12" value="" out="N"/>
				<param name="attr_ctnt4" type="12" value="" out="N"/>
				<param name="attr_ctnt5" type="12" value="" out="N"/>
				<param name="attr_ctnt6" type="12" value="" out="N"/>
				<param name="attr_ctnt7" type="12" value="" out="N"/>
				<param name="attr_ctnt8" type="12" value="" out="N"/>
				<param name="attr_ctnt9" type="12" value="" out="N"/>
				<param name="attr_ctnt10" type="12" value="" out="N"/>
				<param name="attr_ctnt11" type="12" value="" out="N"/>
				<param name="attr_ctnt12" type="12" value="" out="N"/>
				<param name="attr_ctnt13" type="12" value="" out="N"/>
				<param name="attr_ctnt14" type="12" value="" out="N"/>
				<param name="attr_ctnt15" type="12" value="" out="N"/>
				<param name="attr_cate_nm" type="12" value="" out="N"/>
				<param name="xter_bank_acct_seq" type="12" value="" out="N"/>
				<param name="pay_bat_run_seq" type="12" value="" out="N"/>
				<param name="remit_vndr_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
