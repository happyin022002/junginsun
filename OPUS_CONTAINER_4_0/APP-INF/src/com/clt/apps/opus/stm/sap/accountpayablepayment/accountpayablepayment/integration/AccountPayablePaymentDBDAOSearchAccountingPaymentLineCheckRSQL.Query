<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingPaymentLineCheckRSQL">
			<desc><![CDATA[searchAccountingPaymentLineCheck]]></desc>
			<sql><![CDATA[
SELECT  SPD.INV_PAY_SEQ AS INV_PAY_SEQ
    , SPD.INV_SEQ     AS INV_SEQ
,'' USR_ID
,'' accounting_event_id
, '' accounting_header_id
,'' ACCOUNTING_REQUEST_ID
FROM    SAP_PAY_DTL SPD
WHERE   NVL(SPD.ACCTG_PST_FLG, 'N') <> 'Y'
AND     SPD.PRNT_RVS_INV_PAY_SEQ IS NULL
AND     SPD.PAY_SEQ = @[pay_seq]
ORDER   BY SPD.INV_PAY_SEQ			]]></sql>
			<params>
				<param name="pay_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
