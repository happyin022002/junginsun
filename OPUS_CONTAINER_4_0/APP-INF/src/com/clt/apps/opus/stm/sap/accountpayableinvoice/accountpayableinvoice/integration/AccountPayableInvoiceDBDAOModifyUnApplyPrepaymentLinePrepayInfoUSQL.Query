<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyUnApplyPrepaymentLinePrepayInfoUSQL">
			<desc><![CDATA[modifyUnApplyPrepaymentLinePrepayInfo]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_DTL
SET     PPAY_RMN_AMT = NVL(PPAY_RMN_AMT, DTRB_AMT) + @[unapply_amount]
      , UPD_USR_ID = @[usr_id]
      , UPD_DT = SYSDATE
WHERE   INV_SEQ = @[prepayment_inv_seq]
AND     DTRB_LINE_NO = @[prepayment_line_no]
			]]></sql>
			<params>
				<param name="unapply_amount" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="prepayment_inv_seq" type="12" value="" out="N"/>
				<param name="prepayment_line_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
