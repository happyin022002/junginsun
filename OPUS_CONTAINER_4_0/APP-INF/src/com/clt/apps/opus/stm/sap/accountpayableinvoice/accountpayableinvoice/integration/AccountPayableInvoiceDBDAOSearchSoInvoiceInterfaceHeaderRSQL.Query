<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchSoInvoiceInterfaceHeaderRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchSoInvoiceInterfaceHeaderRSQL]]></desc>
			<sql><![CDATA[
SELECT  @[if_request_seq]               AS INV_IF_SEQ
         , AIH.CSR_NO                      AS INV_NO
         , AIH.CSR_TP_CD                   AS INV_TP_LU_CD
         , AIH.INV_DT 										 AS INV_DT
         , AIH.VNDR_NO                     AS VNDR_NO
         , AIH.CSR_AMT                     AS INV_AMT
         , AIH.CSR_CURR_CD                 AS INV_CURR_CD
         , AIH.ACT_XCH_RT                  AS INV_XCH_RT
         , '1'                             AS INV_XCH_RT_TP_CD
         , AIH.GL_DT									     AS INV_XCH_DT
         , AIH.VNDR_TERM_NM                AS INV_TERM_NM
         , AIH.INV_DESC                    AS INV_DESC
         , AIH.ATTR_CATE_NM                AS ATTR_CATE_NM
         , AIH.ATTR_CTNT1                  AS ATTR_CTNT1
         , AIH.ATTR_CTNT2                  AS ATTR_CTNT2
         , AIH.ATTR_CTNT3                  AS ATTR_CTNT3
         , AIH.ATTR_CTNT4                  AS ATTR_CTNT4
         , AIH.ATTR_CTNT5                  AS ATTR_CTNT5
         , AIH.ATTR_CTNT6                  AS ATTR_CTNT6
         , AIH.ATTR_CTNT7                  AS ATTR_CTNT7
         , AIH.ATTR_CTNT8                  AS ATTR_CTNT8
         , AIH.ATTR_CTNT9                  AS ATTR_CTNT9
         , AIH.ATTR_CTNT10                 AS ATTR_CTNT10
         , AIH.ATTR_CTNT11                 AS ATTR_CTNT11
         , AIH.ATTR_CTNT12                 AS ATTR_CTNT12
         , AIH.ATTR_CTNT13                 AS ATTR_CTNT13
         , AIH.ATTR_CTNT14                 AS ATTR_CTNT14
         , AIH.ATTR_CTNT15                 AS ATTR_CTNT15
         , ''                              AS GLO_ATTR_CATE_N
         , AIH.GLO_ATTR_CTNT1              AS GLO_ATTR_CTNT1
         , AIH.GLO_ATTR_CTNT2              AS GLO_ATTR_CTNT2
         , AIH.GLO_ATTR_CTNT3              AS GLO_ATTR_CTNT3
         , AIH.GLO_ATTR_CTNT4              AS GLO_ATTR_CTNT4
         , AIH.GLO_ATTR_CTNT5              AS GLO_ATTR_CTNT5
         , AIH.GLO_ATTR_CTNT6              AS GLO_ATTR_CTNT6
         , AIH.GLO_ATTR_CTNT7              AS GLO_ATTR_CTNT7
         , AIH.GLO_ATTR_CTNT8              AS GLO_ATTR_CTNT8
         , AIH.GLO_ATTR_CTNT9              AS GLO_ATTR_CTNT9
         , AIH.GLO_ATTR_CTNT10             AS GLO_ATTR_CTNT10
         , AIH.GLO_ATTR_CTNT11             AS GLO_ATTR_CTNT11
         , AIH.GLO_ATTR_CTNT12             AS GLO_ATTR_CTNT12
         , AIH.GLO_ATTR_CTNT13             AS GLO_ATTR_CTNT13
         , AIH.GLO_ATTR_CTNT14             AS GLO_ATTR_CTNT14
         , AIH.GLO_ATTR_CTNT15             AS GLO_ATTR_CTNT15
         , AIH.GLO_ATTR_CTNT16             AS GLO_ATTR_CTNT16
         , AIH.GLO_ATTR_CTNT17             AS GLO_ATTR_CTNT17
         , AIH.GLO_ATTR_CTNT18             AS GLO_ATTR_CTNT18
         , ''                              AS GLO_ATTR_CTNT19
         , ''                              AS GLO_ATTR_CTNT20
         , AIH.SRC_CTNT                    AS IF_SRC_NM
         , AIH.CSR_CURR_CD                 AS INV_PAY_CURR_CD
         , AIH.PAY_MZD_LU_CD               AS AP_PAY_MZD_LU_CD
         , AIH.PAY_GRP_LU_CD               AS PAY_GRP_LU_CD
         , AIH.INV_DT 										 AS INV_RCV_DT
         , AIH.GL_DT                       AS GL_DT
         , AIH.COA_CO_CD                   AS LIAB_COA_CO_CD
         , AIH.COA_RGN_CD                  AS LIAB_COA_RGN_CD
         , AIH.COA_CTR_CD                  AS LIAB_COA_CTR_CD
         , AIH.COA_ACCT_Cd                 AS LIAB_COA_ACCT_NO
         , AIH.COA_VVD_CD                  AS LIAB_COA_VVD_CD
         , AIH.COA_INTER_CO_CD             AS LIAB_COA_INTER_CO_CD
         , AIH.TJ_OFC_CD                   AS OFC_CD
         , AIH.PPD_NO                      AS PPAY_INV_NO
         , AIH.PPD_DTRB_NO                 AS PPAY_INV_LINE_NO
         , AIH.PPD_APLY_AMT                AS PPAY_APLY_AMT
         , AIH.PPD_GL_DT									 AS PPAY_APLY_GL_DT
         , ''                              AS INV_INCL_PPAY_FLG
         , AIH.INV_DT											 AS INV_TERM_DT
         , ''                              AS XTER_BANK_ACCT_SEQ
         , ''                              AS ORG_INV_NO
         , ''                              AS LIAB_CD_CMB_SEQ
         , ''                              AS INV_VAT_AMT
         , ''                              AS INV_VAT_CD
         , AIH.APRO_FLG                    AS APRO_FLG
         , NVL ( ( SELECT SV.PAY_PRIO_CD FROM SAP_VENDOR SV WHERE SV.VNDR_NO = AIH.VNDR_NO  AND ROWNUM=1 ), 99) VNDR_BANK_ACCT_PRIO_CD
         , AIH.CRE_USR_ID                  AS CREATION_USER   -- 2014.10.16 전표 생성자 정보 추가
   FROM    AP_INV_HDR AIH
   WHERE   AIH.CSR_NO = @[csr_no]
   AND     AIH.APRO_FLG = 'Y'			]]></sql>
			<params>
				<param name="if_request_seq" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
