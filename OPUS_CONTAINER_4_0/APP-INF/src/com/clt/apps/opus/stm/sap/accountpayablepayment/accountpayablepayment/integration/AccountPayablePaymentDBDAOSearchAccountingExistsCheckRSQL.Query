<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingExistsCheckRSQL">
			<desc><![CDATA[해당 조건일자까지의 자료중 지불 Batch 작업중인 자료에 대해서 점검한 후 대상이 존재하는 경우에는 에러 처리]]></desc>
			<sql><![CDATA[
SELECT  MIN(SSI.PAY_BAT_NM) AS PAY_BAT_NM
      , MIN(SISC.OFC_CD) AS OFC_CD
      , COUNT(SSI.PAY_BAT_NM) AS BATCH_CNT
FROM    SAP_SEL_INV SSI
      , SAP_INV_SEL_CRTE SISC
WHERE   SISC.PAY_BAT_SEQ = SSI.PAY_BAT_SEQ
AND     SISC.PAY_BAT_NM = SSI.PAY_BAT_NM
AND     SISC.PAY_DT <= TO_DATE(@[capture_period],'YYYYMMDD')			]]></sql>
			<params>
				<param name="capture_period" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
