<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOSearchOutstandingReceiptDtlTempRSQL">
			<desc><![CDATA[SearchOutstandingReceiptDtlTemp]]></desc>
			<sql><![CDATA[
SELECT OTS_OFC_CD
    , BL_NO
    , INV_NO
    , RCT_APLY_CHG_CD
    , RCT_APLY_SRC_CURR_CD
    , SAR_GET_FMT_MASK_FNC(RCT_APLY_SRC_CURR_CD, OTS_BAL_AMT) OTS_BAL_AMT
    , SAR_GET_FMT_MASK_FNC(RCT_APLY_SRC_CURR_CD, OTS_APLY_AMT) OTS_APLY_AMT
    , OTS_XCH_RT
    , RCT_APLY_XCH_RT
    , RCT_CURR_CD
    , SAR_GET_FMT_MASK_FNC(RCT_CURR_CD, RCT_APLY_AMT) RCT_APLY_AMT
    , DP_PRCS_KNT
    , RCT_APLY_HDR_NO RCT_APLY_HDR_SEQ
    , RCT_APLY_FLG
FROM SAR_OTS_RCT_TMP
WHERE OTS_RCT_TMP_SEQ = @[ots_rct_tmp_seq]
ORDER BY OTS_OFC_CD
        , BL_NO
        , INV_NO
        , RCT_APLY_CHG_CD
        , RCT_APLY_SRC_CURR_CD			]]></sql>
			<params>
				<param name="ots_rct_tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
