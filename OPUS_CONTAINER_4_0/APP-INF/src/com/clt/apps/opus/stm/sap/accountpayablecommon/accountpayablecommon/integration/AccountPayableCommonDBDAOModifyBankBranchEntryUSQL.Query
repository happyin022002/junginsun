<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOModifyBankBranchEntryUSQL">
			<desc><![CDATA[AccountPayableCommonDBDAOModifyBankBranchEntryUSQL]]></desc>
			<sql><![CDATA[
UPDATE SAP_BANK_BRNC
SET  BANK_NM = @[bank_nm]
    ,BANK_ALTN_NM = @[bank_altn_nm]
    ,BANK_NO = @[bank_no]
    ,BANK_BRNC_NM = @[bank_brnc_nm]
    ,BANK_BRNC_ALTN_NM = @[bank_brnc_altn_nm]
    ,BRNC_NO = @[brnc_no]
    ,BANK_BRNC_TP_NM = @[bank_brnc_tp_nm]
    ,BANK_END_DT = REPLACE(@[bank_end_dt], '-', '')
    ,BANK_BRNC_DESC = @[bank_brnc_desc]
    ,BRNC_CNT_CD = @[brnc_cnt_cd]
    ,BANK_ADDR1 = @[bank_addr1]
    ,CNTC_NM = @[cntc_nm]
    ,CNTC_TIT_NM = @[cntc_tit_nm]
    ,CNTC_PFX_CD = @[cntc_pfx_cd]
    ,CNTC_PHN_NO = @[cntc_phn_no]
    ,CNTC_EML = @[cntc_eml]
    ,UPD_USR_ID = @[upd_usr_id]
    ,UPD_DT = SYSDATE   
WHERE BANK_BRNC_SEQ = @[bank_brnc_seq]			]]></sql>
			<params>
				<param name="bank_nm" type="12" value="" out="N"/>
				<param name="bank_altn_nm" type="12" value="" out="N"/>
				<param name="bank_no" type="12" value="" out="N"/>
				<param name="bank_brnc_nm" type="12" value="" out="N"/>
				<param name="bank_brnc_altn_nm" type="12" value="" out="N"/>
				<param name="brnc_no" type="12" value="" out="N"/>
				<param name="bank_brnc_tp_nm" type="12" value="" out="N"/>
				<param name="bank_end_dt" type="12" value="" out="N"/>
				<param name="bank_brnc_desc" type="12" value="" out="N"/>
				<param name="brnc_cnt_cd" type="12" value="" out="N"/>
				<param name="bank_addr1" type="12" value="" out="N"/>
				<param name="cntc_nm" type="12" value="" out="N"/>
				<param name="cntc_tit_nm" type="12" value="" out="N"/>
				<param name="cntc_pfx_cd" type="12" value="" out="N"/>
				<param name="cntc_phn_no" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bank_brnc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
