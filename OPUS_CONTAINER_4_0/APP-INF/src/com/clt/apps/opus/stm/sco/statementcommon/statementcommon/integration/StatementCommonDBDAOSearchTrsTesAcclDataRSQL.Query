<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchTrsTesAcclDataRSQL">
			<desc><![CDATA[TRS/TES Accrula 데이타를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  EXE_YRMON
    ,	SYS_SRC_ID
    ,	REV_YRMON
    ,	ACCT_CD
    ,	ACCL_SEQ
    ,	BIZ_UT_ID
    ,	VSL_CD
    ,	SKD_VOY_NO
    ,	SKD_DIR_CD
    ,	REV_DIR_CD
    ,	ESTM_VVD_TP_CD
    ,	ESTM_IOC_DIV_CD
    ,	ESTM_BC_DIV_CD
    ,	NOD_CD
    ,	ESTM_COST_AMT
    ,	ACT_COST_AMT
    ,	ACCL_COST_AMT
    ,	BKG_NO
    ,	BKG_NO_SPLIT
    ,	CNTR_TPSZ_CD
    ,	RLANE_CD
    ,	CNTR_QTY
    ,	CRE_USR_ID
    ,	TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') AS CRE_DT
    ,	UPD_USR_ID
    ,	TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') AS UPD_DT
    ,	CXL_FLG
    ,	TO_CHAR(CXL_DT, 'YYYY-MM-DD HH24:MI:SS') AS CXL_DT
    ,	ESTM_QTY
    ,	ACT_QTY
    ,	ACCL_QTY
    ,	VNDR_NO
    ,	ESTM_UC_AMT
    ,	COA_COST_SRC_CD
    ,	ACT_PLC_CD
    ,	TO_CHAR(ACT_DT, 'YYYY-MM-DD HH24:MI:SS') AS ACT_DT
    ,	LOCL_CURR_CD
    ,	SLAN_CD
    ,	WO_NO
    ,	CTRL_OFC_CD
    ,	ERR_FLG
    ,	ERR_DESC  
FROM
#if (${job_nm} != '' && ${job_nm} == 'ACCLTRS')	
		SAC_TRSP_ACCL_COST_IF
#else   
		SAC_TML_ACCL_COST_IF
#end
WHERE   EXE_YRMON = REPLACE(@[accl_month],'-','')			]]></sql>
			<params>
				<param name="accl_month" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
