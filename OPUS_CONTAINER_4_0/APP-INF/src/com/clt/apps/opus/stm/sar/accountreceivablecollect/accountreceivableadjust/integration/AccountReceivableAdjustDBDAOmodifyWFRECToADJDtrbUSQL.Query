<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOmodifyWFRECToADJDtrbUSQL">
			<desc><![CDATA[modifyWFRECToADJDtrb]]></desc>
			<sql><![CDATA[
UPDATE SAR_CLT_DTRB SET 
(GL_CONV_XCH_RT,GL_ACCT_DR_AMT,GL_ACCT_CR_AMT)
= 
(
 SELECT GL_CONV_XCH_RT,GL_ACCT_CR_AMT,GL_ACCT_DR_AMT
  FROM SAR_CLT_DTRB
 WHERE 
     DTRB_SRC_SEQ = @[dtrb_src_seq]  
     AND DTRB_SRC_TBL_CD = @[dtrb_src_tbl_cd]
     AND DTRB_SRC_TP_CD = 'REC'
)
WHERE 
     DTRB_SRC_SEQ = @[dtrb_src_seq]  
     AND DTRB_SRC_TBL_CD = @[dtrb_src_tbl_cd]
     AND DTRB_SRC_TP_CD = 'ADJ' 			]]></sql>
			<params>
				<param name="dtrb_src_seq" type="12" value="" out="N"/>
				<param name="dtrb_src_tbl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
