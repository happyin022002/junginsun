<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostAccrualDBDAOSearchInvoiceAccrualNumberCheckRSQL">
			<desc><![CDATA[Cost Accrual 테이블의 Max Number 값 가져오기]]></desc>
			<sql><![CDATA[
SELECT  NVL(MAX(GEIE.ESTM_SEQ_NO), 0) AS ESTM_SEQ_NO
FROM    GL_ESTM_IF_ERP GEIE 
WHERE   EXE_YRMON = @[gl_yymm]  -- AP Header의 G/L DAte의 년월..
AND     SYS_SRC_ID = 'SAP'			]]></sql>
			<params>
				<param name="gl_yymm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
