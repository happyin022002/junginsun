<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASAApprovalCheckRSQL">
			<desc><![CDATA[SearchASAApprovalCheck]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(BAT_RSLT_CD),'N') AS BAT_RSLT_CD FROM 
(  
SELECT BAT_RSLT_CD
  FROM SCO_BAT_HIS
 WHERE BAT_PGM_NO = 'STM_SAR_B5002'
   AND APPL_PGM_NO = 'STM_SAR_5002'
   AND BAT_PARA_CTNT LIKE @[asa_no]||'%'
   AND BAT_RSLT_CD = 'S'
   AND ROWNUM = 1
UNION ALL
SELECT BAT_RSLT_CD
  FROM SCO_BAT_HIS
 WHERE BAT_PGM_NO = 'STM_SAR_B5002'
   AND APPL_PGM_NO = 'STM_SAR_5002'
   AND BAT_PARA_CTNT LIKE @[asa_no]||'%'
   AND BAT_RSLT_CD <> 'E'
   AND ROWNUM = 1
)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
