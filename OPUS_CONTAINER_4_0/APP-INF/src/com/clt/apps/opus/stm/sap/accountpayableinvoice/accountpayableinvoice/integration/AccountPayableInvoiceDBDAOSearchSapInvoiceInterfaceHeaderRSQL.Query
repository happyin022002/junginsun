<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchSapInvoiceInterfaceHeaderRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchSapInvoiceInterfaceHeaderRSQL]]></desc>
			<sql><![CDATA[
SELECT  SIHI.INV_IF_SEQ           AS INV_IF_SEQ
         , SIHI.INV_NO               AS INV_NO
         , SIHI.INV_TP_LU_CD         AS INV_TP_LU_CD
         , TO_CHAR(SIHI.INV_DT, 'YYYYMMDD')               AS INV_DT
         , SIHI.VNDR_NO              AS VNDR_NO
         , SIHI.INV_AMT              AS INV_AMT
         , SIHI.INV_CURR_CD          AS INV_CURR_CD
         , SIHI.INV_XCH_RT           AS INV_XCH_RT
         , SIHI.INV_XCH_RT_TP_CD     AS INV_XCH_RT_TP_CD
         , TO_CHAR(SIHI.INV_XCH_DT, 'YYYYMMDD')           AS INV_XCH_DT
         , SIHI.INV_TERM_NM          AS INV_TERM_NM
         , SIHI.INV_DESC             AS INV_DESC
         , SIHI.ATTR_CATE_NM         AS ATTR_CATE_NM
         , SIHI.ATTR_CTNT1           AS ATTR_CTNT1
         , SIHI.ATTR_CTNT2           AS ATTR_CTNT2
         , SIHI.ATTR_CTNT3           AS ATTR_CTNT3
         , SIHI.ATTR_CTNT4           AS ATTR_CTNT4
         , SIHI.ATTR_CTNT5           AS ATTR_CTNT5
         , SIHI.ATTR_CTNT6           AS ATTR_CTNT6
         , SIHI.ATTR_CTNT7           AS ATTR_CTNT7
         , SIHI.ATTR_CTNT8           AS ATTR_CTNT8
         , SIHI.ATTR_CTNT9           AS ATTR_CTNT9
         , SIHI.ATTR_CTNT10          AS ATTR_CTNT10
         , SIHI.ATTR_CTNT11          AS ATTR_CTNT11
         , SIHI.ATTR_CTNT12          AS ATTR_CTNT12
         , SIHI.ATTR_CTNT13          AS ATTR_CTNT13
         , SIHI.ATTR_CTNT14          AS ATTR_CTNT14
         , SIHI.ATTR_CTNT15          AS ATTR_CTNT15
         , SIHI.GLO_ATTR_CATE_NM     AS GLO_ATTR_CATE_N
         , SIHI.GLO_ATTR_CTNT1       AS GLO_ATTR_CTNT1
         , SIHI.GLO_ATTR_CTNT2       AS GLO_ATTR_CTNT2
         , SIHI.GLO_ATTR_CTNT3       AS GLO_ATTR_CTNT3
         , SIHI.GLO_ATTR_CTNT4       AS GLO_ATTR_CTNT4
         , SIHI.GLO_ATTR_CTNT5       AS GLO_ATTR_CTNT5
         , SIHI.GLO_ATTR_CTNT6       AS GLO_ATTR_CTNT6
         , SIHI.GLO_ATTR_CTNT7       AS GLO_ATTR_CTNT7
         , SIHI.GLO_ATTR_CTNT8       AS GLO_ATTR_CTNT8
         , SIHI.GLO_ATTR_CTNT9       AS GLO_ATTR_CTNT9
         , SIHI.GLO_ATTR_CTNT10      AS GLO_ATTR_CTNT10
         , SIHI.GLO_ATTR_CTNT11      AS GLO_ATTR_CTNT11
         , SIHI.GLO_ATTR_CTNT12      AS GLO_ATTR_CTNT12
         , SIHI.GLO_ATTR_CTNT13      AS GLO_ATTR_CTNT13
         , SIHI.GLO_ATTR_CTNT14      AS GLO_ATTR_CTNT14
         , SIHI.GLO_ATTR_CTNT15      AS GLO_ATTR_CTNT15
         , SIHI.GLO_ATTR_CTNT16      AS GLO_ATTR_CTNT16
         , SIHI.GLO_ATTR_CTNT17      AS GLO_ATTR_CTNT17
         , SIHI.GLO_ATTR_CTNT18      AS GLO_ATTR_CTNT18
         , SIHI.GLO_ATTR_CTNT19      AS GLO_ATTR_CTNT19
         , SIHI.GLO_ATTR_CTNT20      AS GLO_ATTR_CTNT20
         , SIHI.IF_SRC_NM            AS IF_SRC_NM
         , SIHI.INV_PAY_CURR_CD      AS INV_PAY_CURR_CD
         , SIHI.AP_PAY_MZD_LU_CD     AS AP_PAY_MZD_LU_CD
         , SIHI.PAY_GRP_LU_CD        AS PAY_GRP_LU_CD
         , TO_CHAR(SIHI.INV_RCV_DT, 'YYYYMMDD')           AS INV_RCV_DT
         , SIHI.GL_DT                AS GL_DT
         , SIHI.LIAB_COA_CO_CD       AS LIAB_COA_CO_CD
         , SIHI.LIAB_COA_RGN_CD      AS LIAB_COA_RGN_CD
         , SIHI.LIAB_COA_CTR_CD      AS LIAB_COA_CTR_CD
         , SIHI.LIAB_COA_ACCT_NO     AS LIAB_COA_ACCT_NO
         , SIHI.LIAB_COA_VVD_CD      AS LIAB_COA_VVD_CD
         , SIHI.LIAB_COA_INTER_CO_CD AS LIAB_COA_INTER_CO_CD
         , SIHI.OFC_CD               AS OFC_CD
         , SIHI.PPAY_INV_NO          AS PPAY_INV_NO
         , SIHI.PPAY_INV_LINE_NO     AS PPAY_INV_LINE_NO
         , SIHI.PPAY_APLY_AMT        AS PPAY_APLY_AMT
         , TO_CHAR(SIHI.PPAY_APLY_GL_DT, 'YYYYMMDD')      AS PPAY_APLY_GL_DT
         , SIHI.INV_INCL_PPAY_FLG    AS INV_INCL_PPAY_FLG
         , TO_CHAR(SIHI.INV_TERM_DT, 'YYYYMMDD')          AS INV_TERM_DT
         , SIHI.XTER_BANK_ACCT_SEQ   AS XTER_BANK_ACCT_SEQ
         , SIHI.ORG_INV_NO           AS ORG_INV_NO
         , SIHI.LIAB_CD_CMB_SEQ      AS LIAB_CD_CMB_SEQ
         , SIHI.INV_VAT_AMT          AS INV_VAT_AMT
         , SIHI.INV_VAT_CD           AS INV_VAT_CD
         , ''                        AS APRO_FLG
         , NVL ( ( SELECT SV.PAY_PRIO_CD FROM SAP_VENDOR SV WHERE SV.VNDR_NO = SIHI.VNDR_NO AND ROWNUM=1 ), 99) VNDR_BANK_ACCT_PRIO_CD
         , SIHI.CRE_USR_ID           AS CREATION_USER   -- 2014.10.16 전표 생성자 정보 추가
   FROM    SAP_INV_HDR_IF SIHI
   WHERE   SIHI.INV_NO = @[csr_no]
   AND     SIHI.INV_IF_STS_CD = 'NEW'			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
