<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOAddAccountingDetailInvHeaderCancelInfoCSQL">
			<desc><![CDATA[AddAccountingDetailInvHeaderCancelInfo]]></desc>
			<sql><![CDATA[
INSERT INTO SAP_ACCTG_EVNT_DTL 
( ACCTG_LINE_SEQ
, ACCTG_HDR_SEQ
, ACCTG_EVNT_LINE_NO
, ACCTG_LINE_TP_CD
, CURR_CD
, CONV_XCH_RT
, CONV_XCH_RT_TP_CD
, CONV_XCH_DT
, INP_DR_AMT
, INP_CR_AMT
, ACCT_DR_AMT
, ACCT_CR_AMT
, ACCTG_SRC_TBL_NM
, ACCTG_SRC_SEQ
, GL_SUB_LEGR_LNK_SEQ
, ACCTG_DESC
, ACCTG_ERR_CD
, GL_TRNS_ERR_CD
, N3PTY_NO
, N3PTY_SUB_NO
, SUB_LEGR_DOC_SEQ
, ACCTG_PGM_APPL_NM
, OFC_CD
, ATTR_CTNT1
, ATTR_CTNT2
, ATTR_CTNT3
, ATTR_CTNT4
, ATTR_CTNT5
, ATTR_CTNT6
, ATTR_CTNT7
, ATTR_CTNT8
, ATTR_CTNT9
, ATTR_CTNT10
, END_PRN_DOC_NO
, ACCTG_VAT_CD
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
, ACCT_CD_CMB_SEQ
)
SELECT
     SAP_ACCTG_EVNT_DTL_SEQ.NEXTVAL   AS ACCTG_LINE_SEQ
	,ACCTG_HDR_SEQ
	,ACCTG_EVNT_LINE_NO
	,ACCTG_LINE_TP_CD
	,CURR_CD
	,CONV_XCH_RT
	,CONV_XCH_RT_TP_CD
	,CONV_XCH_DT
	,INP_DR_AMT
	,INP_CR_AMT
	,ACCT_DR_AMT
	,ACCT_CR_AMT
	,ACCTG_SRC_TBL_NM
	,ACCTG_SRC_SEQ
	,GL_SUB_LEGR_LNK_SEQ
	,ACCTG_DESC
	,ACCTG_ERR_CD
	,GL_TRNS_ERR_CD
	,N3PTY_NO
	,N3PTY_SUB_NO
	,SUB_LEGR_DOC_SEQ
	,ACCTG_PGM_APPL_NM
	,OFC_CD
	,ATTR_CTNT1
	,ATTR_CTNT2
	,ATTR_CTNT3
	,ATTR_CTNT4
	,ATTR_CTNT5
	,ATTR_CTNT6
	,ATTR_CTNT7
	,ATTR_CTNT8
	,ATTR_CTNT9
	,ATTR_CTNT10
	,END_PRN_DOC_NO
	,ACCTG_VAT_CD
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
	,ACCT_CD_CMB_SEQ
FROM
(
      SELECT  
             @[accounting_header_id]          AS ACCTG_HDR_SEQ
            , (SELECT MAX(SAED.ACCTG_EVNT_LINE_NO) FROM SAP_ACCTG_EVNT_DTL SAED WHERE SAED.ACCTG_HDR_SEQ = @[accounting_header_id]) + 1 AS ACCTG_EVNT_LINE_NO
            , 'LIABILITY'                      AS ACCTG_LINE_TP_CD
            , SIH.INV_CURR_CD                  AS CURR_CD
            , SIH.INV_XCH_RT                   AS CONV_XCH_RT
            , SIH.INV_XCH_RT_TP_CD             AS CONV_XCH_RT_TP_CD
            , SIH.INV_XCH_DT                   AS CONV_XCH_DT
            , DECODE(SIGN(NVL(SIH.INV_CXL_AMT, SIH.INV_AMT) * (-1)), -1, NVL(SIH.INV_CXL_AMT, SIH.INV_AMT), NULL)                    AS INP_DR_AMT
            , DECODE(SIGN(NVL(SIH.INV_CXL_AMT, SIH.INV_AMT) * (-1)), -1, NULL, (NVL(SIH.INV_CXL_AMT, SIH.INV_AMT)) * (-1))           AS INP_CR_AMT
            , DECODE(SIGN(NVL(SIH.INV_CXL_AMT, SIH.INV_AMT) * (-1)), -1, ROUND((NVL(SIH.INV_CXL_AMT, SIH.INV_AMT)) * NVL(SIH.INV_XCH_RT, 1), 
                                                                        (SELECT DP_PRCS_KNT FROM MDM_CURRENCY MC WHERE MC.CURR_CD = @[functional_currency] AND DELT_FLG = 'N' AND ROWNUM=1)), NULL) AS ACCT_DR_AMT
            , DECODE(SIGN(NVL(SIH.INV_CXL_AMT, SIH.INV_AMT) * (-1)), -1, NULL, ROUND(((NVL(SIH.INV_CXL_AMT, SIH.INV_AMT)) * (-1)) * NVL(SIH.INV_XCH_RT, 1), 
                                                                        (SELECT DP_PRCS_KNT FROM MDM_CURRENCY MC WHERE MC.CURR_CD = @[functional_currency] AND DELT_FLG = 'N' AND ROWNUM=1))) AS ACCT_CR_AMT
            , 'SAP_INV_HDR'                    AS ACCTG_SRC_TBL_NM
            , SIH.INV_SEQ                      AS ACCTG_SRC_SEQ
            , ''                               AS GL_SUB_LEGR_LNK_SEQ
            , SIH.INV_DESC                     AS ACCTG_DESC
            , ''                               AS ACCTG_ERR_CD
            , ''                               AS GL_TRNS_ERR_CD
            , SIH.VNDR_NO                      AS N3PTY_NO
            , ''                               AS N3PTY_SUB_NO
            , ''                               AS SUB_LEGR_DOC_SEQ
            , 'AP'                             AS ACCTG_PGM_APPL_NM
            , SIH.OFC_CD                       AS OFC_CD
            , MV.VNDR_LGL_ENG_NM               AS ATTR_CTNT1
            , TO_CHAR(SIH.INV_SEQ)             AS ATTR_CTNT2
            , ''                               AS ATTR_CTNT3
            , ''                               AS ATTR_CTNT4
            , SIH.INV_NO                       AS ATTR_CTNT5
            , 'AP Invoices'                    AS ATTR_CTNT6
            , ''                               AS ATTR_CTNT7
            , ''                               AS ATTR_CTNT8
            , ''                               AS ATTR_CTNT9
            , 'LIABILITY'                      AS ATTR_CTNT10
            , ''                               AS END_PRN_DOC_NO
            , ''                               AS ACCTG_VAT_CD
            , @[usr_id]                        AS CRE_USR_ID
            , SYSDATE                          AS CRE_DT
            , @[usr_id]                        AS UPD_USR_ID
            , SYSDATE                          AS UPD_DT
            , SIH.LIAB_CD_CMB_SEQ              AS ACCT_CD_CMB_SEQ
      FROM    SAP_INV_HDR SIH
            , MDM_VENDOR MV
      WHERE   TO_NUMBER(SIH.VNDR_NO) = MV.VNDR_SEQ
      AND     SIH.INV_SEQ = @[inv_seq]
      AND     SIH.INV_CXL_DT IS NOT NULL 
) A			]]></sql>
			<params>
				<param name="accounting_header_id" type="12" value="" out="N"/>
				<param name="functional_currency" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
