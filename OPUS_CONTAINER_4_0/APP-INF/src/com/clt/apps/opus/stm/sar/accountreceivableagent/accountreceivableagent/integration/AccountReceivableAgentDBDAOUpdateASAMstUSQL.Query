<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOUpdateASAMstUSQL">
			<desc><![CDATA[Update SAR_ASA_MST]]></desc>
			<sql><![CDATA[
UPDATE SAR_ASA_MST
    SET
           UPD_DT = SYSDATE
           #if( ${asa_prd_fm_dt} != '' )
           , ASA_PRD_FM_DT = REPLACE(@[asa_prd_fm_dt], '-','')
           #end
           #if( ${asa_prd_to_dt} != '' )
           , ASA_PRD_TO_DT = REPLACE(@[asa_prd_to_dt], '-', '')
           #end
           #if( ${curr_cd} != '' )
           , CURR_CD = @[curr_cd]
           #end
           #if( ${act_bal_amt} != '' )
           , ACT_BAL_AMT = TO_NUMBER(@[act_bal_amt])
           #end
           #if( ${asa_fsh_dt} != '')
             #if( ${asa_fsh_dt} == 'null')   
           , ASA_FSH_DT = NULL 
             #else
           , ASA_FSH_DT = TO_DATE(@[asa_fsh_dt], 'YYYYMMDDHHMISS')
             #end
           #end
           #if( ${asa_fsh_usr_id} != '' )
             #if( ${asa_fsh_dt} == 'null')   
           , ASA_FSH_USR_ID = NULL
             #else
           , ASA_FSH_USR_ID = @[asa_fsh_usr_id]
             #end
           #end
           #if( ${asa_apro_usr_id} != '' )
           , ASA_APRO_USR_ID = @[asa_apro_usr_id]
           #end
           #if( ${asa_apro_dt} != '' )
           , ASA_APRO_DT = TO_DATE(@[asa_apro_dt], 'YYYYMMDDHHMISS')
           #end
           #if( ${asa_no_ctnt1} != '' )
           , ASA_NO_CTNT1 = @[asa_no_ctnt1]
           #end
           #if( ${asa_no_ctnt2} != '' )
           , ASA_NO_CTNT2 = @[asa_no_ctnt2]
           #end
           #if( ${asa_no_ctnt3} != '' )
           , ASA_NO_CTNT3 = @[asa_no_ctnt3]
           #end
           #if( ${asa_sts_cd} != '' )
           , ASA_STS_CD = @[asa_sts_cd]
           #end
           #if( ${pre_asa_no} != '' )
           , PRE_ASA_NO = @[pre_asa_no]
           #end
           #if( ${ofc_cd} != '' )
           , OFC_CD = @[ofc_cd]
           #end
           #if( ${upd_usr_id} != '' )
           , UPD_USR_ID = @[upd_usr_id]
           #end 
    WHERE  1=1
    AND    ASA_NO = @[asa_no]			]]></sql>
			<params>
				<param name="asa_prd_fm_dt" type="12" value="" out="N"/>
				<param name="asa_prd_to_dt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="act_bal_amt" type="12" value="" out="N"/>
				<param name="asa_fsh_dt" type="12" value="" out="N"/>
				<param name="asa_fsh_usr_id" type="12" value="" out="N"/>
				<param name="asa_apro_usr_id" type="12" value="" out="N"/>
				<param name="asa_apro_dt" type="12" value="" out="N"/>
				<param name="asa_no_ctnt1" type="12" value="" out="N"/>
				<param name="asa_no_ctnt2" type="12" value="" out="N"/>
				<param name="asa_no_ctnt3" type="12" value="" out="N"/>
				<param name="asa_sts_cd" type="12" value="" out="N"/>
				<param name="pre_asa_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
