<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchMonthlyAccrualTradeListRSQL">
			<desc><![CDATA[Accrual Trade Code]]></desc>
			<sql><![CDATA[
SELECT TRD_CD AS CODE
     , TRD_NM AS NAME
  FROM MDM_TRADE
 WHERE 1=1
   AND NVL(DELT_FLG,'N') = 'N'
 ORDER BY FM_CONTI_CD
     , TRD_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
