<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOSearchOtsCdCmbSeqRSQL">
			<desc><![CDATA[get OTS Code Combination Sequence]]></desc>
			<sql><![CDATA[
SELECT SODR.OTS_CD_CMB_SEQ
FROM   SAR_OTS_DTRB SODR
WHERE  OTS_HIS_SEQ = @[ots_his_seq]
AND    CHG_TP_CD = @[chg_tp_cd]
AND    ACCT_CLSS_CD = 'REC'
			]]></sql>
			<params>
				<param name="ots_his_seq" type="12" value="" out="N"/>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
