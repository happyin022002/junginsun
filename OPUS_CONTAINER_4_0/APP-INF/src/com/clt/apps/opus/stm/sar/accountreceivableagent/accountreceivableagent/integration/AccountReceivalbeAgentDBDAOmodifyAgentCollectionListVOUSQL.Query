<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivalbeAgentDBDAOmodifyAgentCollectionListVOUSQL">
			<desc><![CDATA[Agent Collection List modify]]></desc>
			<sql><![CDATA[
UPDATE SAR_AGN_CLT_RFND_MST
   SET PORT_CD      = @[port_cd]
      ,UPD_USR_ID  = @[upd_usr_id] 
      ,UPD_DT      = SYSDATE
 WHERE  ASA_NO = @[asa_no]
   AND BL_NO     = @[bl_no]
   AND INV_NO     = @[inv_no]
   AND CHG_TP_CD = @[chg_tp_cd]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
