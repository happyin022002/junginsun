<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOSearchPayGroupInfoRSQL">
			<desc><![CDATA[Search pay group info]]></desc>
			<sql><![CDATA[
SELECT DECODE(AP_OFC_CD, NULL, '', AP_OFC_CD || '_O/EXP') PAY_GRP_LU_CD
       , 'Refund' ATTR_CATE_NM
       , '' ATTR_CTNT7
       , '' ATTR_CTNT2
       , AP_CTR_CD
FROM MDM_ORGANIZATION MO
WHERE OFC_CD = @[ap_ofc_cd]			]]></sql>
			<params>
				<param name="ap_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
