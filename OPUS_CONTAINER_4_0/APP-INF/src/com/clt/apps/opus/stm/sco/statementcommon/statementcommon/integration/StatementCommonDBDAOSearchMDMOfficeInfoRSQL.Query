<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchMDMOfficeInfoRSQL">
			<desc><![CDATA[search MDM officeInfo]]></desc>
			<sql><![CDATA[
SELECT    OFC_CD
              , OFC_ENG_NM
              , OFC_LOCL_NM
              , OFC_ADDR
              , OFC_ZIP_CD
              , OFC_KND_CD
              , AGN_KND_CD
              , VNDR_CNT_CD
              , VNDR_SEQ
              , INTL_PHN_NO
              , OFC_PHN_NO
              , INTL_FAX_NO
              , OFC_FAX_NO
              , OFC_PSON_KNT
              , OFC_RMK
              , LOC_CD
              , BIL_CURR_CD
              , AR_CURR_CD
              , AR_CTR_CD
              , PRNT_OFC_CD
              , OPN_DT
              , CLZ_DT
              , FINC_RGN_CD
              , AR_OFC_CD
              , AR_CTRL_OFC_CD
              , AR_HD_QTR_OFC_CD
              , IB_CR_TERM_DYS
              , OB_CR_TERM_DYS
              , SUB_AGN_FLG
              , REP_CUST_CNT_CD
              , REP_CUST_SEQ
              , INV_PFX_CD
              , AP_OFC_CD
              , AP_CTRL_OFC_CD
              , AP_HO_ACCT_CD
              , AP_CTR_CD
              , AR_AGN_STL_CD
              , FX_CURR_RT
              , AP_EURO_CURR_USE_FLG
              , USA_BRK_BRNC_RQST_CTRL_OFC_CD
              , ASA_CR_TERM_DYS
              , SO_IF_CD
              , SLS_OFC_USE_FLG
              , SLS_OFC_DIV_CD
              , OFC_TAX_ID
              , OFC_RFA_SC_USE_FLG
              , COMM_IF_IND_CD
              , FAX_IP
              , BFR_OFC_CD
              , MODI_OFC_CD
              , OFC_CMMC_CD
              , OFC_TP_CD
              , PRC_OFC_CD
              , OFC_URL
              , OFC_REP_EML
              , BKG_SVR_SRCH_ROUT_CD
              , OFC_SLS_DELT_FLG
              , DOC_RCVR_HIDE_FLG
              , FINC_HIDE_FLG
              , FINC_PSDO_OFC_FLG
              , SUBS_CO_FLG
              , GL_CTR_CD
              , TEAM_MGR_NM
              , TEAM_FAX_NO
              , OFC_LOCL_LANG_ADDR
              , CRE_USR_ID
              , CRE_DT
              , UPD_USR_ID
              , UPD_DT
              , DELT_FLG
              , EAI_EVNT_DT
              , EAI_IF_ID
			  , NVL((SELECT DECODE(OTS_SMRY_CD, 'CLR', 'BL', OTS_SMRY_CD) FROM INV_AR_STUP_OFC WHERE AR_OFC_CD = @[ofc_cd]),
				    (SELECT NVL(OTS_CATE_CD, 'BL') FROM SCO_OFC_INFO WHERE OFC_CD = @[ofc_cd])) OTS_SMRY_CD
    FROM      MDM_ORGANIZATION
    WHERE     1 = 1          
      AND OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
