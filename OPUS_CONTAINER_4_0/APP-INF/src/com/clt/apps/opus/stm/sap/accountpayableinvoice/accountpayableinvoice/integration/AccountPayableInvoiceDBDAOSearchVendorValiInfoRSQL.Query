<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchVendorValiInfoRSQL">
			<desc><![CDATA[searchVendorValiInfo]]></desc>
			<sql><![CDATA[
SELECT VENDOR_SEQ  AS VALUE0 
      ,SUBS_CO_CD  AS VALUE1 
FROM MDM_VENDOR
WHERE VENDOR_SEQ = TO_NUMBER(@[vendor_no])			]]></sql>
			<params>
				<param name="vendor_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
