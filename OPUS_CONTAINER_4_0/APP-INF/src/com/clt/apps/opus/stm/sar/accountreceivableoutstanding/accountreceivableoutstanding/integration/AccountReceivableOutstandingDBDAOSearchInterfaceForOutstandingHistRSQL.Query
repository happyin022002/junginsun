<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOSearchInterfaceForOutstandingHistRSQL">
			<desc><![CDATA[Search interface for Outstanding History]]></desc>
			<sql><![CDATA[
SELECT A.IF_NO
	, A.RHQ_CD
	, DECODE(NVL(B.OTS_CD, 'ARO'), 'COU', B.REP_OTS_OFC_CD, A.AR_OFC_CD) OTS_OFC_CD
    , A.BL_NO
	, DECODE(B.OTS_CATE_CD, 'INV', NVL(A.INV_NO, '**********'), '**********') INV_NO
	, A.BL_CURR_CD CURR_CD
    , 'OTS' OTS_HIS_TP_CD
    , A.OTS_SRC_CD
	, A.IF_DT
	, A.GL_DT
	, A.OTS_AMT
	, A.IF_NO REF_NO
	, A.AR_OFC_CD INV_OFC_CD
	, A.OTS_RMK
	, A.SHP_TO_CUST_CNT_CD
    , A.SHP_TO_CUST_SEQ
    , A.BIL_TO_CUST_CNT_CD
    , A.BIL_TO_CUST_SEQ
    , A.VSL_CD
    , A.SKD_VOY_NO
    , A.DIR_CD
    , A.SVC_SCP_CD
	, A.XCH_RT_TP_CD
    , A.BKG_IO_BND_CD
	, A.GL_DT XCH_RT_DT
    , A.POL_CD
    , A.POD_CD
 	, A.REV_TP_SRC_CD
	, A.CRE_USR_ID
	, A.UPD_USR_ID
	, A.SAIL_ARR_DT SAIL_DT
	, A.VSL_CD||A.SKD_VOY_NO||A.DIR_CD||A.DIR_CD REV_VVD_CD
	, '' INV_CURR_CD
	, '' INV_LOCL_XCH_RT
	, '' INV_USD_XCH_RT
FROM SAR_OTS_IF A,
	 SCO_OFC_INFO B
WHERE A.AR_OFC_CD = B.OFC_CD(+)
AND A.OTS_IF_SEQ = @[ots_if_seq]			]]></sql>
			<params>
				<param name="ots_if_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
