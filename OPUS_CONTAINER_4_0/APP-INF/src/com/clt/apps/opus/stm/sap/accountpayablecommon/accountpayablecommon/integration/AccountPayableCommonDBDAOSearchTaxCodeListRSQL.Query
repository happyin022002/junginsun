<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchTaxCodeListRSQL">
			<desc><![CDATA[SearchTaxCodeList]]></desc>
			<sql><![CDATA[
SELECT  D.LU_CD AS TAX_NO
      , D.LU_DESC AS AP_TAX_NM
      , D.ATTR_CTNT1 AS TAX_RT
      , D.ATTR_CTNT2
      , D.ATTR_CTNT3
      , D.ATTR_CTNT4
      , D.ATTR_CTNT5      
FROM  SCO_LU_HDR H, SCO_LU_DTL D
WHERE H.LU_TP_CD = D.LU_TP_CD 
AND   H.LU_TP_CD = 'AP TAX CODE'
AND   H.LU_APPL_CD = 'SAP'
AND   D.ENBL_FLG = 'Y'
#if (${tax_no} != '') 
AND D.LU_CD = @[tax_no] 
#end
#if (${ap_tax_nm} != '') 
AND D.LU_DESC LIKE @[ap_tax_nm] || '%'
#end
ORDER BY D.DP_SEQ			]]></sql>
			<params>
				<param name="tax_no" type="12" value="" out="N"/>
				<param name="ap_tax_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
