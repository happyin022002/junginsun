<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOaddAdjustHdrListCSQL">
			<desc><![CDATA[SAR_ADJ_HDR 테이블에 insert]]></desc>
			<sql><![CDATA[
INSERT INTO  SAR_ADJ_HDR
(
     OTS_ADJ_SEQ
	,ADJ_NO
	,ADJ_DT
	,BL_NO
	,BKG_NO
	,INV_NO
	,INV_OFC_CD
	,ADJ_TP_CD
	,BIL_TO_CUST_CNT_CD
	,BIL_TO_CUST_SEQ
	,SHP_TO_CUST_CNT_CD
	,SHP_TO_CUST_SEQ
	,LOCL_VVD_CD
	,TRNK_VVD_CD
	,SAIL_DT
	,SAIL_ARR_DT
	,OBRD_DT
	,IO_BND_CD
	,DUE_DT
	,SREP_CD
	,ADJ_RMK
	,XCH_RT_TP_CD
	,XCH_RT_DT
	,CR_FLG
	,AR_TAX_IND_CD
	,AR_TJ_TP_CD
	,AR_FINC_SRC_CD
	,MAX_AR_IF_NO
	,AP_CURR_CD
	,AP_CRS_CURR_AMT
	,GAIN_AND_LSS_AMT
	,AP_OFC_CD
	,VNDR_NO
	,AP_RMK
	,RVS_FLG
	,ASA_NO
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
	,ADJ_KEY_NO
	,ADJ_OFC_CD
)
VALUES
(
	@[ots_adj_seq],
	@[adj_no],
	REPLACE(@[adj_dt], '-', ''),
	@[bl_no],
	@[bkg_no],
	@[inv_no],
	@[inv_ofc_cd],
	@[adj_tp_cd],
	@[bil_to_cust_cnt_cd],
	@[bil_to_cust_seq],
	SUBSTR(@[shp_bil_to_cust_cd], 1, 2),
	SUBSTR(@[shp_bil_to_cust_cd], 4, 6),
	@[locl_vvd_cd],
	@[trnk_vvd_cd],
	'',
	@[sail_arr_dt],
	'',
	DECODE(@[io_bnd_cd], 'O/B', 'O', 'I'),
	@[due_dt],
	@[srep_cd],
	@[adj_rmk],
	@[xch_rt_tp_cd],
	@[xch_rt_dt],
	@[cr_flg],
	'',
	'',
	@[ar_finc_src_cd],
	@[max_ar_if_no],
	@[ap_curr_cd],
	REPLACE(@[ap_crs_curr_amt], ',', ''),
	REPLACE(@[gain_and_lss_amt], ',', ''),
	@[ap_ofc_cd],
	@[vndr_no],
	@[ap_rmk],
	'N',
	@[asa_no],
	@[cre_usr_id],
	SYSDATE,
	@[cre_usr_id],
	SYSDATE,
	@[adj_key_no],
	@[adj_ofc_cd]
)			]]></sql>
			<params>
				<param name="ots_adj_seq" type="12" value="" out="N"/>
				<param name="adj_no" type="12" value="" out="N"/>
				<param name="adj_dt" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_ofc_cd" type="12" value="" out="N"/>
				<param name="adj_tp_cd" type="12" value="" out="N"/>
				<param name="bil_to_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="bil_to_cust_seq" type="12" value="" out="N"/>
				<param name="shp_bil_to_cust_cd" type="12" value="" out="N"/>
				<param name="locl_vvd_cd" type="12" value="" out="N"/>
				<param name="trnk_vvd_cd" type="12" value="" out="N"/>
				<param name="sail_arr_dt" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="due_dt" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="adj_rmk" type="12" value="" out="N"/>
				<param name="xch_rt_tp_cd" type="12" value="" out="N"/>
				<param name="xch_rt_dt" type="12" value="" out="N"/>
				<param name="cr_flg" type="12" value="" out="N"/>
				<param name="ar_finc_src_cd" type="12" value="" out="N"/>
				<param name="max_ar_if_no" type="12" value="" out="N"/>
				<param name="ap_curr_cd" type="12" value="" out="N"/>
				<param name="ap_crs_curr_amt" type="12" value="" out="N"/>
				<param name="gain_and_lss_amt" type="12" value="" out="N"/>
				<param name="ap_ofc_cd" type="12" value="" out="N"/>
				<param name="vndr_no" type="12" value="" out="N"/>
				<param name="ap_rmk" type="12" value="" out="N"/>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="adj_key_no" type="12" value="" out="N"/>
				<param name="adj_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
