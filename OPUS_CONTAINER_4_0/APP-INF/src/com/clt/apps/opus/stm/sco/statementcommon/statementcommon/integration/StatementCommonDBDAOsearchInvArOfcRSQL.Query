<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOsearchInvArOfcRSQL">
			<desc><![CDATA[searchInvArOfc]]></desc>
			<sql><![CDATA[
SELECT NVL2(MAX(AR_OFC_CD),'Y','N') AS EXIST
  FROM   INV_AR_STUP_OFC
  WHERE  AR_OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
