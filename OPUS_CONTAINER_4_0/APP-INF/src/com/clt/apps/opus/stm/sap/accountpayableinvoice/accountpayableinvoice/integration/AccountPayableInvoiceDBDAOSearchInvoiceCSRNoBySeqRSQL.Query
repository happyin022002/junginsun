<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceCSRNoBySeqRSQL">
			<desc><![CDATA[SearchInvoiceCSRNoBySeq]]></desc>
			<sql><![CDATA[
SELECT INV_NO AS CSR_NO
FROM SAP_INV_HDR
WHERE INV_SEQ = @[inv_seq]			]]></sql>
			<params>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
