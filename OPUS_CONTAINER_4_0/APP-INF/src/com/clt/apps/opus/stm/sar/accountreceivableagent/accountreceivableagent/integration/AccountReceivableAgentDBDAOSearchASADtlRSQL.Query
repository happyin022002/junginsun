<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASADtlRSQL">
			<desc><![CDATA[search ASA DETAIL]]></desc>
			<sql><![CDATA[
SELECT    
                A.ASA_NO
              , A.ASA_DTL_SEQ
              , A.ASA_DTL_DESC
              , A.ASA_DR_AMT
              , A.ASA_CR_AMT
              , A.EFF_DT
              , A.ASA_DTL_TP_CD
              , A.CRE_USR_ID
              , A.CRE_DT
              , A.UPD_USR_ID
              , A.UPD_DT
    FROM      SAR_ASA_DTL  A                
    WHERE     1 = 1      
      AND    A.ASA_NO = @[asa_no]  	  
   #if (${asa_dtl_seq} != '')
      AND    A.ASA_DTL_SEQ = @[asa_dtl_seq]  
   #end 	  
ORDER BY A.ASA_DTL_SEQ			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="asa_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
