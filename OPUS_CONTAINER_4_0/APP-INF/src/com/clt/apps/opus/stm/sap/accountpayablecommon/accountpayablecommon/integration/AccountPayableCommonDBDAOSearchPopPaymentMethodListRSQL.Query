<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchPopPaymentMethodListRSQL">
			<desc><![CDATA[SearchPopPaymentMethodList 조회]]></desc>
			<sql><![CDATA[
SELECT  SLD.lU_CD
      , SLD.LU_DESC
FROM    SCO_LU_HDR SLH
      , SCO_LU_DTL SLD
WHERE   UPPER(LU_CD) LIKE '%'||UPPER(@[lu_cd])||'%'
AND     SLH.LU_TP_CD = SLD.LU_TP_CD
AND     SLH.LU_TP_CD = 'PAYMENT METHOD'
AND     NVL(SLD.ENBL_FLG, 'Y') = 'Y'
AND     NVL(SLD.LU_ST_DT, SYSDATE) >= SYSDATE
AND     SLH.LU_APPL_CD = 'SAP'			]]></sql>
			<params>
				<param name="lu_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
