<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingDetailPayAccountedSumInfoRSQL">
			<desc><![CDATA[SearchAccountingDetailPayAccountedSumInfo]]></desc>
			<sql><![CDATA[
SELECT  SUM(NVL(SAED.ACCT_DR_AMT, 0)) AS ACCOUNTED_DR_SUM
      , SUM(NVL(SAED.ACCT_CR_AMT, 0)) AS ACCOUNTED_CR_SUM
FROM    SAP_ACCTG_EVNT_DTL SAED
WHERE   SAED.ACCTG_HDR_SEQ = @[accounting_header_id]			]]></sql>
			<params>
				<param name="accounting_header_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
