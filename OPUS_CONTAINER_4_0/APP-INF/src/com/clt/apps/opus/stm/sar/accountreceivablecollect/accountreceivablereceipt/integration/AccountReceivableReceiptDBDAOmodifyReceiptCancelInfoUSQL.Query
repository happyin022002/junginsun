<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOmodifyReceiptCancelInfoUSQL">
			<desc><![CDATA[Receipt Cancel 정보 Update]]></desc>
			<sql><![CDATA[
UPDATE SAR_RECEIPT SET
	RCT_CXL_DT = @[rct_cxl_dt]
	, RCT_CXL_CATE_CD = @[rct_cxl_cate_cd]
	, RCT_CXL_RSN_CD = @[rct_cxl_rsn_cd]
	, RCT_CXL_RMK = @[rct_cxl_rmk]
	, BAL_RCT_AMT = 0
	, RCT_STS_CD = 'CXL'
	, UPD_USR_ID = @[upd_usr_id]
	, UPD_DT = SYSDATE
WHERE RCT_SEQ = @[rct_seq]			]]></sql>
			<params>
				<param name="rct_cxl_dt" type="12" value="" out="N"/>
				<param name="rct_cxl_cate_cd" type="12" value="" out="N"/>
				<param name="rct_cxl_rsn_cd" type="12" value="" out="N"/>
				<param name="rct_cxl_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
