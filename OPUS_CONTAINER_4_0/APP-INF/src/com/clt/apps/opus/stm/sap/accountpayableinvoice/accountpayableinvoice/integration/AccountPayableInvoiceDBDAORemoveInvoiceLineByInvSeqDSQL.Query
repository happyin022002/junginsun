<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAORemoveInvoiceLineByInvSeqDSQL">
			<desc><![CDATA[SAP_INV_DTL을 INV_SEQ로 삭제]]></desc>
			<sql><![CDATA[
DELETE SAP_INV_DTL
WHERE INV_SEQ = TO_NUMBER(@[inv_seq])			]]></sql>
			<params>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
