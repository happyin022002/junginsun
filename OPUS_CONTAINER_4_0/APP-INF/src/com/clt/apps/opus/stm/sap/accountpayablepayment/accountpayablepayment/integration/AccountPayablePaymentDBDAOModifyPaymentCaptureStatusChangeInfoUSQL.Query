<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOModifyPaymentCaptureStatusChangeInfoUSQL">
			<desc><![CDATA[modifyPaymentCaptureStatusChangeInfo]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_SEL_CRTE 
SET     PAY_STS_CD = @[upd_pay_sts_cd]
      , UPD_DT = SYSDATE
      , UPD_USR_ID = @[usr_id]
WHERE   PAY_BAT_SEQ = @[pay_bat_seq]
AND     PAY_BAT_NM = @[pay_bat_nm]			]]></sql>
			<params>
				<param name="upd_pay_sts_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="pay_bat_seq" type="12" value="" out="N"/>
				<param name="pay_bat_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
