<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOaddOutstandingHistCSQL">
			<desc><![CDATA[SAR_OTS_HIS 테이블에 insert]]></desc>
			<sql><![CDATA[
INSERT INTO SAR_OTS_HIS
(
    IF_NO
    , RHQ_CD
    , OTS_OFC_CD
    , BL_NO
    , INV_NO
    , OTS_HIS_SEQ
    , CURR_CD
    , OTS_HIS_TP_CD
    , OTS_SRC_CD
    , IF_DT
    , GL_DT
    , OTS_AMT
    , USD_AMT
    , REF_NO
    , INV_OFC_CD
    , OTS_RMK
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
    , SHP_TO_CUST_CNT_CD
    , SHP_TO_CUST_SEQ
    , BIL_TO_CUST_CNT_CD
    , BIL_TO_CUST_SEQ
    , VSL_CD
    , SKD_VOY_NO
    , DIR_CD
    , SVC_SCP_CD
    , XCH_RT_TP_CD
    , LOCL_XCH_RT
    , USD_XCH_RT
    , BKG_IO_BND_CD
    , XCH_RT_DT
    , POL_CD
    , POD_CD    
	, REV_TP_SRC_CD
	, REV_VVD_CD
	, SAIL_DT
	, INV_CURR_CD
	, INV_LOCL_XCH_RT
	, INV_USD_XCH_RT
)
VALUES
(
    @[if_no]
    , @[rhq_cd]
    , @[ots_ofc_cd]
    , @[bl_no]
    , @[inv_no]
    , @[ots_his_seq]
    , @[curr_cd]
    , @[ots_his_tp_cd]
    , @[ots_src_cd]
    , GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[inv_ofc_cd])
    , @[gl_dt]
    , NVL(@[ots_amt], 0)
    , ROUND(NVL(@[ots_amt], 0) * NVL(@[usd_xch_rt], 0), 2)
    , @[ref_no]
    , @[inv_ofc_cd]
    , @[ots_rmk]
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
    , @[shp_to_cust_cnt_cd]
    , @[shp_to_cust_seq]
    , @[bil_to_cust_cnt_cd]
    , @[bil_to_cust_seq]
    , @[vsl_cd]
    , @[skd_voy_no]
    , @[dir_cd]
    , @[svc_scp_cd]
    , @[xch_rt_tp_cd]
    , NVL(@[locl_xch_rt], 0)
    , NVL(@[usd_xch_rt], 0)
    , @[bkg_io_bnd_cd]
    , @[xch_rt_dt]
    , @[pol_cd]
    , @[pod_cd]
	, @[rev_tp_src_cd]
	, @[rev_vvd_cd]
	, @[sail_dt]
	, @[inv_curr_cd]
	, @[inv_locl_xch_rt]
	, @[inv_usd_xch_rt]
)			]]></sql>
			<params>
				<param name="if_no" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="ots_his_seq" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="ots_his_tp_cd" type="12" value="" out="N"/>
				<param name="ots_src_cd" type="12" value="" out="N"/>
				<param name="inv_ofc_cd" type="12" value="" out="N"/>
				<param name="gl_dt" type="12" value="" out="N"/>
				<param name="ots_amt" type="12" value="" out="N"/>
				<param name="usd_xch_rt" type="12" value="" out="N"/>
				<param name="ref_no" type="12" value="" out="N"/>
				<param name="ots_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="shp_to_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="shp_to_cust_seq" type="12" value="" out="N"/>
				<param name="bil_to_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="bil_to_cust_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="xch_rt_tp_cd" type="12" value="" out="N"/>
				<param name="locl_xch_rt" type="12" value="" out="N"/>
				<param name="bkg_io_bnd_cd" type="12" value="" out="N"/>
				<param name="xch_rt_dt" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="rev_tp_src_cd" type="12" value="" out="N"/>
				<param name="rev_vvd_cd" type="12" value="" out="N"/>
				<param name="sail_dt" type="12" value="" out="N"/>
				<param name="inv_curr_cd" type="12" value="" out="N"/>
				<param name="inv_locl_xch_rt" type="12" value="" out="N"/>
				<param name="inv_usd_xch_rt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
