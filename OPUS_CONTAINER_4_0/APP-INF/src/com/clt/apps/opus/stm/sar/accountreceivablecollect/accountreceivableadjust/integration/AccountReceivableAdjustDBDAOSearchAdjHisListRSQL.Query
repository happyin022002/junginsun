<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOSearchAdjHisListRSQL">
			<desc><![CDATA[get Adjust History List]]></desc>
			<sql><![CDATA[
SELECT 
    ADJ_HIS_SEQ,
    ADJ_NO,
    ADJ_STS_CD,
    ADJ_AMT,
    ADJ_APLY_DT,
    ADJ_GL_DT,
    ADJ_CD_CMB_SEQ,
    CHG_TP_CD,
    ADJ_TP_CD,
    ADJ_RMK,
    GL_TRNS_SEQ,
    GL_TRNS_DT,
    ADJ_ACCT_AMT,
    ORZ_SEQ,
    ACCTG_EVNT_SEQ,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
    OTS_HIS_SEQ,
    ACCT_MTX_SEQ,
    ADJ_KEY_NO,
	ADJ_CRS_CURR_AMT,
    ADJ_CRS_CURR_CD,
	GL_CRS_CURR_AMT,
    GL_CRS_CURR_CD,
	GL_CRS_EX_RATE 
FROM
    SAR_ADJ_HIS
WHERE ADJ_HIS_SEQ = @[adj_his_seq]			]]></sql>
			<params>
				<param name="adj_his_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
