<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOsearchAdjustForOTSHeaderRSQL">
			<desc><![CDATA[Search Adjust for OTS Header]]></desc>
			<sql><![CDATA[
SELECT DISTINCT Q.RHQ_CD
   	   , Q.OTS_OFC_CD
   	   , Q.BL_NO
   	   , Q.INV_NO
   	   , P.UPD_USR_ID
FROM SAR_ADJ_HIS P,
     SAR_OTS_HIS Q
WHERE P.OTS_HIS_SEQ = Q.OTS_HIS_SEQ
AND P.ADJ_NO IN (
#foreach( $key IN ${adj_no_list}) 
	#if($velocityCount < $adj_no_list.size())
  		'$key',
	#else
		'$key'
	#end
#end
)
AND P.ADJ_STS_CD = @[sts_cd]			]]></sql>
			<params>
				<param name="sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
