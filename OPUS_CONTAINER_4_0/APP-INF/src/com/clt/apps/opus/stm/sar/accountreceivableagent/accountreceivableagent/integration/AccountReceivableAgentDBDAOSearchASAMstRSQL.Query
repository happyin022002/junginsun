<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASAMstRSQL">
			<desc><![CDATA[select SAR_ASA_MST]]></desc>
			<sql><![CDATA[
SELECT    ASA_NO
              , AGN_CD
              , ASA_PRD_FM_DT
              , ASA_PRD_TO_DT
              , CURR_CD
              , ACT_BAL_AMT
              , TO_CHAR(ASA_FSH_DT,'YYYY-MM-DD HH:MI:SS')  ASA_FSH_DT
              , ASA_FSH_USR_ID
              , ASA_APRO_USR_ID
              , TO_CHAR(ASA_APRO_DT,'YYYY-MM-DD HH:MI:SS')  ASA_APRO_DT
              , NVL(ASA_NO_CTNT1,SUBSTR(AGN_CD,0,3)) AS ASA_NO_CTNT1
              , ASA_NO_CTNT2
              , ASA_NO_CTNT3
              , ASA_STS_CD
              , PRE_ASA_NO
              , OFC_CD
              , CRE_USR_ID
              , CRE_DT
              , UPD_USR_ID
              , UPD_DT
    FROM      SAR_ASA_MST
    WHERE     1 = 1        
      AND     ASA_NO = @[asa_no]			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
