<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOModifyAPCostActivityInfoUSQL">
			<desc><![CDATA[ModifyAPCostActivityInfo]]></desc>
			<sql><![CDATA[
UPDATE SCO_AP_COST_ACT_INFO SACAI 
SET
       SACAI.ACT_COST_NM = @[act_cost_nm]
      ,SACAI.ACT_DT_NM = @[act_dt_nm]
      ,SACAI.ACT_PLC_NM = @[act_plc_nm]
      ,SACAI.ENBL_FLG = DECODE(@[enbl_flg],'1','Y','N')
      ,SACAI.UPD_USR_ID = @[usr_id]
      ,SACAI.UPD_DT = SYSDATE
      ,SACAI.ACCL_FLG = @[accl_flg]
WHERE  SACAI.SRC_MDL_CD = @[src_mdl_cd]
AND    SACAI.ACT_COST_CD = @[act_cost_cd]
AND    SACAI.CONV_ACCT_CD = @[conv_acct_cd]			]]></sql>
			<params>
				<param name="act_cost_nm" type="12" value="" out="N"/>
				<param name="act_dt_nm" type="12" value="" out="N"/>
				<param name="act_plc_nm" type="12" value="" out="N"/>
				<param name="enbl_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="accl_flg" type="12" value="" out="N"/>
				<param name="src_mdl_cd" type="12" value="" out="N"/>
				<param name="act_cost_cd" type="12" value="" out="N"/>
				<param name="conv_acct_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
