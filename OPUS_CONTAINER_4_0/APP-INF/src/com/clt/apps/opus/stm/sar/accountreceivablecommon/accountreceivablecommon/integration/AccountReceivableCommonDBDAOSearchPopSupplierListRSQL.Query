<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOSearchPopSupplierListRSQL">
			<desc><![CDATA[STM_SAP_0002- Supplier Pop List]]></desc>
			<sql><![CDATA[
SELECT
 VNDR_LGL_ENG_NM
 ,VNDR_SEQ
 ,INV_CURR_CD
 ,PAY_CURR_CD
 ,GEN_PAY_TERM_CD
 ,VNDR_CNT_CD
 ,RGST_NO
 ,TAX_ID
FROM MDM_VENDOR
WHERE 1=1
#if (${vndr_seq} != '')
  AND VNDR_SEQ LIKE @[vndr_seq]||'%' 
#end
#if (${vndr_lgl_eng_nm} != '')
  AND UPPER(VNDR_LGL_ENG_NM) LIKE '%'||UPPER(@[vndr_lgl_eng_nm])||'%'
#end
#if (${delt_flg} != '')
  AND NVL(DELT_FLG,'Y') = @[delt_flg]
#else
  AND NVL(DELT_FLG,'Y') = 'N'
#end			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vndr_lgl_eng_nm" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
