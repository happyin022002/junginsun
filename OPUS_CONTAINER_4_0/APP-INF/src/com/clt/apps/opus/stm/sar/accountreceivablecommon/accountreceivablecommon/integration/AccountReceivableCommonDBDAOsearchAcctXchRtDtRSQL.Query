<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOsearchAcctXchRtDtRSQL">
			<desc><![CDATA[searchAcctXchRtDt]]></desc>
			<sql><![CDATA[
SELECT MAX(ACCT_XCH_RT_DT) AS ACCT_XCH_RT_DT FROM SCO_MON_XCH_RT_IF SIF
WHERE ACCT_XCH_RT_DT LIKE @[ym] || '%'			]]></sql>
			<params>
				<param name="ym" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
