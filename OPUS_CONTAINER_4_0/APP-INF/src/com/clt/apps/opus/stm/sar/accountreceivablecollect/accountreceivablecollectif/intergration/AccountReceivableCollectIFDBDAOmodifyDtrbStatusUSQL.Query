<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCollectIFDBDAOmodifyDtrbStatusUSQL">
			<desc><![CDATA[modifyDtrbStatus]]></desc>
			<sql><![CDATA[
UPDATE SAR_CLT_DTRB SET
AR_IF_SEQ = @[ar_if_seq],
AR_IF_STS_CD = @[ar_if_sts_cd],
AR_IF_ERR_DESC = SUBSTR(@[ar_if_err_desc],0,500)
WHERE CLT_DTRB_SEQ =  @[clt_dtrb_seq]			]]></sql>
			<params>
				<param name="ar_if_seq" type="12" value="" out="N"/>
				<param name="ar_if_sts_cd" type="12" value="" out="N"/>
				<param name="ar_if_err_desc" type="12" value="" out="N"/>
				<param name="clt_dtrb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
