<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchPaymentBatchUniqueCheckRSQL">
			<desc><![CDATA[searchPaymentBatchUniqueCheck]]></desc>
			<sql><![CDATA[
SELECT  'E' AS DUP_CHK
      , 'Duplicated Of Payment Batch Name :' || SISC.PAY_BAT_NM
FROM    SAP_INV_SEL_CRTE SISC
WHERE   SISC.PAY_BAT_NM = @[pay_bat_nm]			]]></sql>
			<params>
				<param name="pay_bat_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
