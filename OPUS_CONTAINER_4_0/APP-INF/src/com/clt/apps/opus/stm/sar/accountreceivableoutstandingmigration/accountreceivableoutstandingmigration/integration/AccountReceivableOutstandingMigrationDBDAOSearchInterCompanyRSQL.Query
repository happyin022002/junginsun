<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingMigrationDBDAOSearchInterCompanyRSQL">
			<desc><![CDATA[Search Inter Company code]]></desc>
			<sql><![CDATA[
SELECT  NVL(SUBS_CO_CD, '00') SUBS_CO_CD
FROM    MDM_CUSTOMER
WHERE   CUST_CNT_CD = @[cust_cnt_cd]
AND     CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
