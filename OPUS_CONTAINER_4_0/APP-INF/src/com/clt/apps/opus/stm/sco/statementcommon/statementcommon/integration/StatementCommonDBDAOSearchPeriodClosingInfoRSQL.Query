<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchPeriodClosingInfoRSQL">
			<desc><![CDATA[SearchPeriodClosingInfo]]></desc>
			<sql><![CDATA[
SELECT SCP.PRD_APPL_CD
     , SCP.EFF_YRMON
     , SCP.PRD_STS_CD
	 , SCP.CRE_USR_ID
	 , SCP.CRE_DT
	 , SCP.UPD_USR_ID
	 , SCP.UPD_DT
  FROM SCO_PERIOD SCP
 WHERE SCP.PRD_APPL_CD = NVL(@[prd_appl_cd], SCP.PRD_APPL_CD)
   AND SCP.EFF_YRMON = NVL(REPLACE(@[eff_yrmon],'-',''), SCP.EFF_YRMON)
 ORDER BY SCP.PRD_APPL_CD, SCP.EFF_YRMON DESC			]]></sql>
			<params>
				<param name="prd_appl_cd" type="12" value="" out="N"/>
				<param name="eff_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
