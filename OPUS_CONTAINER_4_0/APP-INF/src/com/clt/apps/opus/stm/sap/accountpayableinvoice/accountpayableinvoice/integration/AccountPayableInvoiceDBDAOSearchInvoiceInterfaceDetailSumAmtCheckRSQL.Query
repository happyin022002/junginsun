<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceDetailSumAmtCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceDetailSumAmtCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT  SUM(AID.INV_AMT) AS INV_SUM_AMT
FROM    AP_INV_DTRB AID
WHERE   AID.CSR_NO = @[csr_no]			]]></sql>
