<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOsearchPopCenterListRSQL">
			<desc><![CDATA[STM_SAP_0033의 검색조건 VO]]></desc>
			<sql><![CDATA[
SELECT CTR_ERP_NO
     , CTR_ERP_CD || CTR_ENG_NM AS CTR_DESC
FROM   GL_CTR_ERP  
WHERE  NVL(DELT_FLG,'N') != 'Y'
#if (${f_center} != '')
AND    UPPER(CTR_ERP_NO) LIKE UPPER(@[f_center])||'%'
#end			]]></sql>
			<params>
				<param name="f_center" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
