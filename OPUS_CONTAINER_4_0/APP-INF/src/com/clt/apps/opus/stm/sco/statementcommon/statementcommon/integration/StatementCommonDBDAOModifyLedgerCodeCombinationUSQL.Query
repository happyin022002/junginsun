<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOModifyLedgerCodeCombinationUSQL">
			<desc><![CDATA[ModifyLedgerCodeCombination]]></desc>
			<sql><![CDATA[
UPDATE SCO_LEGR_CD_CMB
    SET
            UPD_DT = SYSDATE
           , UPD_USR_ID = @[usr_id]
           , LEGR_ACCT_TP_CD = NVL( (SELECT ACCTG_MNG_TP_CD FROM MDM_ACCOUNT MA WHERE MA.ACCT_CD = @[sgm_ctnt4] ), 'A' )
           , ENBL_FLG = @[enbl_flg]
           , SGM_CTNT1 = @[sgm_ctnt1]
           , SGM_CTNT2 = @[sgm_ctnt2]
           , SGM_CTNT3 = @[sgm_ctnt3]
           , SGM_CTNT4 = @[sgm_ctnt4]
           , SGM_CTNT5 = @[sgm_ctnt5]
           , SGM_CTNT6 = @[sgm_ctnt6]
           , SGM_CTNT7 = @[sgm_ctnt7]
           , SGM_CTNT8 = @[sgm_ctnt8]
           , COA_END_DT = REPLACE(@[coa_end_dt],'-','')
    WHERE  1=1
    AND    CD_CMB_SEQ = @[cd_cmb_seq]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="sgm_ctnt4" type="12" value="" out="N"/>
				<param name="enbl_flg" type="12" value="" out="N"/>
				<param name="sgm_ctnt1" type="12" value="" out="N"/>
				<param name="sgm_ctnt2" type="12" value="" out="N"/>
				<param name="sgm_ctnt3" type="12" value="" out="N"/>
				<param name="sgm_ctnt5" type="12" value="" out="N"/>
				<param name="sgm_ctnt6" type="12" value="" out="N"/>
				<param name="sgm_ctnt7" type="12" value="" out="N"/>
				<param name="sgm_ctnt8" type="12" value="" out="N"/>
				<param name="coa_end_dt" type="12" value="" out="N"/>
				<param name="cd_cmb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
