<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchAPVendorInfoListRSQL">
			<desc><![CDATA[SearchAPVendorInfoList]]></desc>
			<sql><![CDATA[
SELECT      S.VNDR_NO          AS VNDR_NO
          , M.VNDR_LGL_ENG_NM  AS VNDR_NM
          , S.LIAB_CD_CMB_SEQ  AS LIAB_CD_CMB_SEQ
          , S.PAY_CD_CMB_SEQ   AS PAY_CD_CMB_SEQ
          , L.SGM_CTNT1        AS L_COMPANY_CODE
          , L.SGM_CTNT2        AS L_REGION_CODE
          , L.SGM_CTNT3        AS L_CENTER_CODE
          , L.SGM_CTNT4        AS L_ACCOUNT_CODE
          , L.SGM_CTNT5        AS L_INTERCOMPANY_CODE
          , L.SGM_CTNT6        AS L_VVD_CODE  
          , P.SGM_CTNT1        AS P_COMPANY_CODE
          , P.SGM_CTNT2        AS P_REGION_CODE
          , P.SGM_CTNT3        AS P_CENTER_CODE
          , P.SGM_CTNT4        AS P_ACCOUNT_CODE
          , P.SGM_CTNT5        AS P_INTERCOMPANY_CODE
          , P.SGM_CTNT6        AS P_VVD_CODE 
          ,'' USR_ID 
          , S.PAY_PRIO_CD
FROM      SAP_VENDOR S
          ,MDM_VENDOR M
          ,SCO_LEGR_CD_CMB L
          ,SCO_LEGR_CD_CMB P 
WHERE   S.VNDR_NO = M.VNDR_SEQ
AND S.LIAB_CD_CMB_SEQ = L.CD_CMB_SEQ
AND S.PAY_CD_CMB_SEQ = P.CD_CMB_SEQ
#if (${vndr_no} != '') 
AND S.VNDR_NO = @[vndr_no]
#end
ORDER BY TO_NUMBER(S.VNDR_NO)			]]></sql>
			<params>
				<param name="vndr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
