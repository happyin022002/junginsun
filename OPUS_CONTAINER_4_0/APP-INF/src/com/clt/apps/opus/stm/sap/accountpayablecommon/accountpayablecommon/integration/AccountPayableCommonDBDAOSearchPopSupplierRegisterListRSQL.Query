<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchPopSupplierRegisterListRSQL">
			<desc><![CDATA[STM_SAP_0012의 검색조건 VO]]></desc>
			<sql><![CDATA[
SELECT A.RGST_NO
      ,A.VNDR_LGL_ENG_NM
      ,A.VNDR_SEQ
  FROM MDM_VENDOR A
 WHERE 1 = 1 
#if (${rgst_no} != '')
   AND UPPER(A.RGST_NO) like '%' || UPPER(@[rgst_no]) || '%'
#end 			]]></sql>
			<params>
				<param name="rgst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
