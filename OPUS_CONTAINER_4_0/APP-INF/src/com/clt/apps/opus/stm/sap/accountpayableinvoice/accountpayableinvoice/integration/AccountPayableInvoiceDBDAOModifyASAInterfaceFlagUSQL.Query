<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyASAInterfaceFlagUSQL">
			<desc><![CDATA[AR의 Interface Java 파일 Call 하여 본 테이블로 I/F 처리, 해당 결과값(성공, 실패) 가져와야 함]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_DTL SID
SET     SID.ATTR_CTNT13 = 'Y'
      , SID.UPD_DT = SYSDATE
      , SID.UPD_USR_ID = @[usr_id]
WHERE   SID.INV_DTRB_SEQ = @[inv_dtrb_seq]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_dtrb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
