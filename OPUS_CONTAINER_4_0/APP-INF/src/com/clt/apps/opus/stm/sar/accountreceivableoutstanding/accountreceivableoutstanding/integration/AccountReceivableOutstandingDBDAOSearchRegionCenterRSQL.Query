<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOSearchRegionCenterRSQL">
			<desc><![CDATA[Search Region/Center code]]></desc>
			<sql><![CDATA[
SELECT FINC_RGN_CD RGN_CD
     , AR_CTR_CD CTR_CD
  FROM MDM_ORGANIZATION 
 WHERE OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
