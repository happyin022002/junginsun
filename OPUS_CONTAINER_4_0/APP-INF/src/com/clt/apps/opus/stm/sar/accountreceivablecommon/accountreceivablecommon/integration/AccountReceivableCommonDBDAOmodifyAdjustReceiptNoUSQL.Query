<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOmodifyAdjustReceiptNoUSQL">
			<desc><![CDATA[SAR_TP_NO  테이블에 update]]></desc>
			<sql><![CDATA[
UPDATE SAR_TP_NO 
SET TP_SEQ = TP_SEQ + 1
	,UPD_USR_ID = @[usr_id]
	,UPD_DT = SYSDATE
WHERE  AR_TJ_TP_CD = @[aj_tj_tp_cd]
AND TP_OFC_CD = @[ofc_cd]  
AND TP_CRE_DT = SUBSTR(TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC((SELECT AR_OFC_CD                                           
						   											FROM MDM_ORGANIZATION                                           
						   											WHERE OFC_CD = @[l_ofc_cd]
																)), 'YYYYMMDD')
						,3,6)			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="aj_tj_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="l_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
