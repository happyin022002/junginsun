<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceSystemSourceCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceSystemSourceCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT  SLD.LU_CD   AS SYSTEM_SOURCE
FROM    SCO_LU_HDR SLH
      , SCO_LU_DTL SLD
WHERE   SLH.LU_TP_CD = SLD.LU_TP_CD
AND     SLH.LU_TP_CD = 'AP SOURCE'
AND     NVL(SLD.ENBL_FLG, 'Y') = 'Y'
AND     NVL(SLD.LU_ST_DT, TRIM(SYSDATE)) >= TRIM(SYSDATE)
AND     SLD.LU_CD = @[if_src_nm]			]]></sql>
			<params>
				<param name="if_src_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
