<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOsearchPopTermsListRSQL">
			<desc><![CDATA[STM_SAP_0033의 검색조건 VO]]></desc>
			<sql><![CDATA[
SELECT  SLD.LU_CD
      , SLD.LU_DESC
FROM    SCO_LU_HDR SLH
      , SCO_LU_DTL SLD
WHERE   SLH.LU_TP_CD = SLD.LU_TP_CD
AND     SLH.LU_TP_CD = 'AP TERMS'
AND     NVL(SLD.ENBL_FLG, 'Y') = 'Y'
AND     NVL(SLD.LU_ST_DT, TRIM(SYSDATE)) >= TRIM(SYSDATE) 
#if (${f_term} != '')
AND     UPPER(SLD.LU_CD) LIKE '%'||UPPER(@[f_term])||'%'
#end			]]></sql>
			<params>
				<param name="f_term" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
