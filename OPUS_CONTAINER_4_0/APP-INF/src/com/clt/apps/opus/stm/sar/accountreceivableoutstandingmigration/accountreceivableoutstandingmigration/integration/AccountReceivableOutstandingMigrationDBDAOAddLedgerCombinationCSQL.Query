<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingMigrationDBDAOAddLedgerCombinationCSQL">
			<desc><![CDATA[Add ledger combination info]]></desc>
			<sql><![CDATA[
INSERT INTO SCO_LEGR_CD_CMB
    (  CD_CMB_SEQ
     , COA_SEQ
     , LEGR_ACCT_TP_CD
     , ENBL_FLG
     , SGM_CTNT1
     , SGM_CTNT2
     , SGM_CTNT3
     , SGM_CTNT4
     , SGM_CTNT5
     , SGM_CTNT6
     , SGM_CTNT7
     , SGM_CTNT8
     , SGM_CTNT9
     , SGM_CTNT10
     , SGM_CTNT11
     , SGM_CTNT12
     , SGM_CTNT13
     , SGM_CTNT14
     , SGM_CTNT15
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , COA_ST_DT
     , COA_END_DT
       ) 
VALUES ( 
       SCO_LEGR_CD_CMB_SEQ.NEXTVAL
     , '1'
     , (SELECT ACCTG_MNG_TP_CD FROM MDM_ACCOUNT MA WHERE MA.ACCT_CD = @[sgm_ctnt4])
     , 'Y'
     , @[sgm_ctnt1]
     , @[sgm_ctnt2]
     , @[sgm_ctnt3]
     , @[sgm_ctnt4]
     , @[sgm_ctnt5]
     , NVL((SELECT  ATTR_CTNT3
            FROM    SCO_LU_DTL
            WHERE   LU_TP_CD = 'NO REV VVD ACCT'
            AND     @[sgm_ctnt4] BETWEEN ATTR_CTNT1 AND ATTR_CTNT2), @[sgm_ctnt6])
     , @[sgm_ctnt7]
     , @[sgm_ctnt8]
     , @[sgm_ctnt9]
     , @[sgm_ctnt10]
     , @[sgm_ctnt11]
     , @[sgm_ctnt12]
     , @[sgm_ctnt13]
     , @[sgm_ctnt14]
     , @[sgm_ctnt15]
     , 'SYSTEM'
     , SYSDATE
     , 'SYSTEM'
     , SYSDATE
     , @[coa_st_dt]
     , @[coa_end_dt]
       )			]]></sql>
			<params>
				<param name="sgm_ctnt4" type="12" value="" out="N"/>
				<param name="sgm_ctnt1" type="12" value="" out="N"/>
				<param name="sgm_ctnt2" type="12" value="" out="N"/>
				<param name="sgm_ctnt3" type="12" value="" out="N"/>
				<param name="sgm_ctnt5" type="12" value="" out="N"/>
				<param name="sgm_ctnt6" type="12" value="" out="N"/>
				<param name="sgm_ctnt7" type="12" value="" out="N"/>
				<param name="sgm_ctnt8" type="12" value="" out="N"/>
				<param name="sgm_ctnt9" type="12" value="" out="N"/>
				<param name="sgm_ctnt10" type="12" value="" out="N"/>
				<param name="sgm_ctnt11" type="12" value="" out="N"/>
				<param name="sgm_ctnt12" type="12" value="" out="N"/>
				<param name="sgm_ctnt13" type="12" value="" out="N"/>
				<param name="sgm_ctnt14" type="12" value="" out="N"/>
				<param name="sgm_ctnt15" type="12" value="" out="N"/>
				<param name="coa_st_dt" type="12" value="" out="N"/>
				<param name="coa_end_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
