<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchAcctCtnt4LookupComboListRSQL">
			<desc><![CDATA[SearchAcctCtnt4LookupComboList]]></desc>
			<sql><![CDATA[
SELECT DISTINCT LU_CD, LU_DESC
FROM   SCO_LU_DTL
WHERE  LU_TP_CD IN ('REV TYPE SRC CD', 'ACCT CTNT4')

#if (${f_acct_ctnt1} == 'ALL' or ${f_acct_ctnt1} == '')

#else
AND    ATTR_CTNT1 = (@[f_acct_ctnt1])
#end

#if (${f_acct_ctnt2} == 'STM AP' or ${f_acct_ctnt2} == 'STM AR')
AND    ATTR_CTNT2 = 'ASA'
#elseif (${f_acct_ctnt2} == 'ALL' or ${f_acct_ctnt2} == '')
    
#else
AND    ATTR_CTNT2 = (@[f_acct_ctnt2])
#end

ORDER BY LU_CD			]]></sql>
			<params>
				<param name="f_acct_ctnt1" type="12" value="" out="N"/>
				<param name="f_acct_ctnt2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
