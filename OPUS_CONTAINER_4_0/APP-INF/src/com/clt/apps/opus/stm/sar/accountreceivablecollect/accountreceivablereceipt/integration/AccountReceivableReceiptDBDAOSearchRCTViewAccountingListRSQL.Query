<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOSearchRCTViewAccountingListRSQL">
			<desc><![CDATA[Receipts View Account list]]></desc>
			<sql><![CDATA[
SELECT   SR.RCT_NO          
       , SRA.RCV_APPL_TP_CD 
       , SCD.DTRB_SRC_TP_CD
       , SOC.BL_NO  
	   , SOC.INV_NO        
       , SOC.IF_NO          
       , SOC.CHG_TP_CD      
       , SLC.SGM_CTNT1      
       , SLC.SGM_CTNT2      
       , SLC.SGM_CTNT3      
       , SLC.SGM_CTNT4      
       , SLC.SGM_CTNT5      
       , SLC.SGM_CTNT6      
       , SCD.CURR_CD        
       , SCD.INP_DR_AMT     
       , SCD.INP_CR_AMT     
       , SCD.ACCT_DR_AMT    
       , SCD.ACCT_CR_AMT    
       , SCD.CONV_XCH_RT    
       , SCD.ACCT_XCH_RT_DT
	   , SRA.GL_DT   
	   , SRA.WRTF_TP_CD WRTF_CD     
FROM     SAR_RECEIPT SR 
       , SAR_RCV_APPL SRA 
       , SAR_CLT_DTRB SCD 
       , SCO_LEGR_CD_CMB SLC 
       , SAR_OTS_CHG SOC 
WHERE  SR.RCT_SEQ = SRA.RCT_SEQ 
       AND SRA.RCV_APPL_SEQ = SCD.DTRB_SRC_SEQ 
       AND SCD.DTRB_CD_CMB_SEQ = SLC.CD_CMB_SEQ 
       AND SRA.OTS_HIS_SEQ = SOC.OTS_HIS_SEQ(+) 
       AND SRA.CHG_TP_CD = SOC.CHG_TP_CD(+) 
       AND SCD.DTRB_SRC_TBL_CD = 'RCT'        
       AND SR.RCT_NO = @[rct_no]
ORDER  BY SCD.DTRB_SRC_SEQ 
        , SCD.CLT_DTRB_SEQ			]]></sql>
			<params>
				<param name="rct_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
