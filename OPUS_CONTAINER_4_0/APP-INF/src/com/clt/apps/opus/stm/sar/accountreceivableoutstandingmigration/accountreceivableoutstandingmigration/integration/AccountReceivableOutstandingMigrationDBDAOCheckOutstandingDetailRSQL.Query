<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingMigrationDBDAOCheckOutstandingDetailRSQL">
			<desc><![CDATA[Outstanding Detail 존재여부 체크]]></desc>
			<sql><![CDATA[
SELECT  COUNT(*) CNT
FROM    OPUSADM_TMP.SAR_OTS_DTL
WHERE   RHQ_CD = @[rhq_cd]
AND     OTS_OFC_CD = @[ots_ofc_cd]
AND     BL_NO = @[bl_no]
AND     INV_NO = @[inv_no]
AND     BL_CURR_CD = @[bl_curr_cd]
AND     CHG_TP_CD = @[chg_tp_cd]			]]></sql>
			<params>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="bl_curr_cd" type="12" value="" out="N"/>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
