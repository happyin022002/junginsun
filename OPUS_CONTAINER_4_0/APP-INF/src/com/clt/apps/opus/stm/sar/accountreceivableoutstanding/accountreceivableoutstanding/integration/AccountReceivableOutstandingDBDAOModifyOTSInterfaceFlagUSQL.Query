<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOModifyOTSInterfaceFlagUSQL">
			<desc><![CDATA[Modify OTS Interface Flag]]></desc>
			<sql><![CDATA[
UPDATE SAR_OTS_IF SET
	OTS_IF_FLG = @[ots_if_flg]
WHERE OTS_IF_SEQ = @[ots_if_seq]			]]></sql>
			<params>
				<param name="ots_if_flg" type="12" value="" out="N"/>
				<param name="ots_if_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
