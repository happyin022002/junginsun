<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchSakuraInterfaceCSRInfoRSQL">
			<desc><![CDATA[Invoice Date 기준의 Sakura로 I/F 처리 할 대상의 CSR을 가져온다]]></desc>
			<sql><![CDATA[
SELECT SIH.INV_NO  AS VALUE0
FROM   SAP_INV_HDR SIH
WHERE  SIH.INV_DT >= TO_DATE(@[invfromdt], 'YYYY-MM-DD')
AND    SIH.INV_DT < TO_DATE(@[invtodt], 'YYYY-MM-DD') + 1
AND    SIH.INV_CXL_DT IS NULL 
AND    NVL(SIH.ATTR_CTNT15, 'N') = 'Y'
AND    NOT EXISTS ( SELECT 'A' FROM SAP_AP_IF SAI WHERE SAI.REF_DOC_NO = SIH.INV_NO)			]]></sql>
			<params>
				<param name="invfromdt" type="12" value="" out="N"/>
				<param name="invtodt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
