<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingDetailAmountValidateRSQL">
			<desc><![CDATA[SearchAccountingDetailAmountValidate]]></desc>
			<sql><![CDATA[
SELECT  SUM(NVL(SAED.INP_DR_AMT, 0))   AS ENTERED_DR_SUM
      , SUM(NVL(SAED.INP_CR_AMT, 0))   AS ENTERED_CR_SUM
      , SUM(NVL(SAED.ACCT_DR_AMT, 0))  AS ACCOUNTED_DR_SUM
      , SUM(NVL(SAED.ACCT_CR_AMT, 0))  AS ACCOUNTED_CR_SUM
FROM    SAP_ACCTG_EVNT_DTL SAED
WHERE   SAED.ACCTG_HDR_SEQ = @[acctg_hdr_seq]
			]]></sql>
			<params>
				<param name="acctg_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
