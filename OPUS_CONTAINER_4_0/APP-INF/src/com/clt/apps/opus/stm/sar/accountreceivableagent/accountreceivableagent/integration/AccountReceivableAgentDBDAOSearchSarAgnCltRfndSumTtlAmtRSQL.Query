<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchSarAgnCltRfndSumTtlAmtRSQL">
			<desc><![CDATA[sum agent collect , refund total amount]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(TTL_AMT), 0) AS TTL_AMT
  FROM SAR_AGN_CLT_RFND_MST
 WHERE 1 = 1
   AND ASA_NO = @[asa_no]
   AND ASA_TP_CD = @[asa_tp_cd]			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="asa_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
