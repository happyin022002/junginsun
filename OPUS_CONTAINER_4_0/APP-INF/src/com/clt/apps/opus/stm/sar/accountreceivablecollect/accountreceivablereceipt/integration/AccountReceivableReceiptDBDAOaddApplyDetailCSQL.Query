<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOaddApplyDetailCSQL">
			<desc><![CDATA[Apply Detail 정보 생성]]></desc>
			<sql><![CDATA[
INSERT INTO SAR_RCT_APLY_DTL
(
	RCT_APLY_DTL_SEQ
	, RCT_APLY_HDR_SEQ
	, RCT_SEQ
	, WRTF_CD
	, RCT_APLY_CHG_CD
	, RCT_APLY_DT
	, RCT_APLY_SRC_CURR_CD
	, OTS_APLY_AMT
	, RCT_APLY_XCH_RT
	, OTS_XCH_RT
	, RCT_CURR_CD
	, RCT_APLY_AMT
	, WRTF_RMK
	, AP_OFC_CD
	, VNDR_NO
	, AP_RMK
	, AP_GL_DT
	, RCT_APLY_FLG
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
	, OTS_BAL_AMT
)
VALUES
(
	@[rct_aply_dtl_seq]
	, @[rct_aply_hdr_seq]
	, @[rct_seq]
	, @[wrtf_cd]
	, @[rct_aply_chg_cd]
	, TO_CHAR(SYSDATE, 'YYYYMMDD')
	, @[rct_aply_src_curr_cd]
	, @[ots_aply_amt]
	, @[rct_aply_xch_rt]
	, @[ots_xch_rt]
	, @[rct_curr_cd]
	, @[rct_aply_amt]
	, @[wrtf_rmk]
	, @[ap_ofc_cd]
	, @[vndr_no]
	, @[ap_rmk]
	, @[ap_gl_dt]
	, 'Y'
	, @[cre_usr_id]
	, SYSDATE
	, @[upd_usr_id]
	, SYSDATE
	, @[ots_bal_amt]
)			]]></sql>
			<params>
				<param name="rct_aply_dtl_seq" type="12" value="" out="N"/>
				<param name="rct_aply_hdr_seq" type="12" value="" out="N"/>
				<param name="rct_seq" type="12" value="" out="N"/>
				<param name="wrtf_cd" type="12" value="" out="N"/>
				<param name="rct_aply_chg_cd" type="12" value="" out="N"/>
				<param name="rct_aply_src_curr_cd" type="12" value="" out="N"/>
				<param name="ots_aply_amt" type="12" value="" out="N"/>
				<param name="rct_aply_xch_rt" type="12" value="" out="N"/>
				<param name="ots_xch_rt" type="12" value="" out="N"/>
				<param name="rct_curr_cd" type="12" value="" out="N"/>
				<param name="rct_aply_amt" type="12" value="" out="N"/>
				<param name="wrtf_rmk" type="12" value="" out="N"/>
				<param name="ap_ofc_cd" type="12" value="" out="N"/>
				<param name="vndr_no" type="12" value="" out="N"/>
				<param name="ap_rmk" type="12" value="" out="N"/>
				<param name="ap_gl_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ots_bal_amt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
