<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingPaymentInvGainCoaInfoRSQL">
			<desc><![CDATA[searchAccountingPaymentInvGainCoaInfo]]></desc>
			<sql><![CDATA[
  SELECT  SLCC.CD_CMB_SEQ AS GAIN_COA_SEQ
  FROM    SCO_LEGR_CD_CMB SLCC
  WHERE   SLCC.SGM_CTNT1 = @[g_compnay_code]
  AND     SLCC.SGM_CTNT2 = @[coa_region_code]
  AND     SLCC.SGM_CTNT3 = @[coa_center_code]
  AND     SLCC.SGM_CTNT4 = @[g_account_code]
  AND     SLCC.SGM_CTNT5 = @[g_intercompany_code]
  AND     SLCC.SGM_CTNT6 = @[g_vvd_code]			]]></sql>
			<params>
				<param name="g_compnay_code" type="12" value="" out="N"/>
				<param name="coa_region_code" type="12" value="" out="N"/>
				<param name="coa_center_code" type="12" value="" out="N"/>
				<param name="g_account_code" type="12" value="" out="N"/>
				<param name="g_intercompany_code" type="12" value="" out="N"/>
				<param name="g_vvd_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
