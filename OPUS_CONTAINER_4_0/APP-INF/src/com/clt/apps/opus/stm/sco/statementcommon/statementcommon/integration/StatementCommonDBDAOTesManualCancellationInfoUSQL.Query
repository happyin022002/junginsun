<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOTesManualCancellationInfoUSQL">
			<desc><![CDATA[Update TesManualCancellation agingst sac_cost_accl_info]]></desc>
			<sql><![CDATA[
UPDATE SAC_COST_ACCL_INFO 
SET    ACCL_FLG = DECODE(@[cancel_flag], 'Y','Y', 'X')
     , UPD_USR_ID = @[upd_usr_id]
	 , UPD_DT = SYSDATE
WHERE (EXE_YRMON, SYS_SRC_ID, REV_YRMON, ACCT_CD, ACCL_SEQ) IN (
       SELECT EXE_YRMON, SYS_SRC_ID, REV_YRMON, ACCT_CD, ACCL_SEQ
       FROM   SAC_TML_ACCL_COST_IF
       WHERE  EXE_YRMON = @[exe_month]
       AND    SYS_SRC_ID = 'TES'
       AND    VSL_CD = @[vsl_cd]
       AND    SKD_VOY_NO = @[skd_voy_no]
       AND    SKD_DIR_CD = @[skd_dir_cd]
       AND    NVL(REV_DIR_CD,'NULL') = DECODE(@[rev_dir_cd], '', 'NULL', @[rev_dir_cd])
       AND    TO_CHAR(ACT_DT, 'YYYY-MM') = @[act_dt]
       AND    NOD_CD = @[yard_code]
       AND    VNDR_NO = @[vendor_code]
       AND    COA_COST_SRC_CD = @[cost_code]
      )			]]></sql>
			<params>
				<param name="cancel_flag" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="exe_month" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="yard_code" type="12" value="" out="N"/>
				<param name="vendor_code" type="12" value="" out="N"/>
				<param name="cost_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
