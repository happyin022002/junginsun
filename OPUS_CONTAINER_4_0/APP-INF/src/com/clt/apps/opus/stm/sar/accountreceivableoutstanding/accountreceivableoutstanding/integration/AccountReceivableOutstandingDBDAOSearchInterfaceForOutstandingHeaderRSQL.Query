<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOSearchInterfaceForOutstandingHeaderRSQL">
			<desc><![CDATA[Search interface info for Outstanding Header]]></desc>
			<sql><![CDATA[
SELECT A.RHQ_CD
	, DECODE(NVL(B.OTS_CD, 'ARO'), 'COU', B.REP_OTS_OFC_CD, A.AR_OFC_CD) OTS_OFC_CD
    , A.BL_NO
	, DECODE(B.OTS_CATE_CD, 'INV', NVL(A.INV_NO, '**********'), '**********') INV_NO
    , A.OFC_CURR_CD
    , A.OTS_SRC_CD
    , A.BIL_TO_CUST_CNT_CD
    , A.BIL_TO_CUST_SEQ
    , A.BKG_NO
    , A.BKG_NO_SPLIT
    , A.VSL_CD
    , A.SKD_VOY_NO
    , A.DIR_CD
    , A.TRNK_VVD_CD
    , A.SVC_SCP_CD
    , A.LANE_CD
    , A.SAIL_ARR_DT
    , A.BKG_IO_BND_CD
    , A.POR_CD
    , A.POL_CD
    , A.POD_CD
    , A.DEL_CD
    , A.CUST_SREP_CD
    , A.DUE_DT
    , A.STL_FLG
    , A.BKG_REF_NO
    , A.AP_AR_OFFST_NO
    , A.CR_MK_FLG
    , A.XCH_RT_TP_CD
    , A.LST_INV_NO
    , A.OTS_GRP_TP_CD
    , A.OTS_TP_CD
    , A.OTS_RMK
    , A.IF_DT
    , A.INV_DT
    , A.CLT_OFC_CD
    , A.OTS_RT_FLG
    , A.SC_NO
	, A.REV_TP_SRC_CD
	, A.SHP_TO_CUST_CNT_CD
    , A.SHP_TO_CUST_SEQ
	, A.XCH_RT_N3RD_TP_CD
	, A.GL_DT XCH_RT_DT
	, A.IF_NO MAX_AR_IF_NO 
	, A.CRE_USR_ID
	, A.UPD_USR_ID
	, A.SAIL_ARR_DT SAIL_DT
	, '' INV_CURR_CD
	, '' INV_LOCL_XCH_RT
	, '' INV_USD_XCH_RT
FROM SAR_OTS_IF A,
	 SCO_OFC_INFO B
WHERE A.AR_OFC_CD = B.OFC_CD(+)
AND A.OTS_IF_SEQ = @[ots_if_seq]			]]></sql>
			<params>
				<param name="ots_if_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
