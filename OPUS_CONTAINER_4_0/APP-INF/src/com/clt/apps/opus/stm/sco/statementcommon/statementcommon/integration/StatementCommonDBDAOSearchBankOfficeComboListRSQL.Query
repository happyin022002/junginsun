<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchBankOfficeComboListRSQL">
			<desc><![CDATA[Bank Office Combo Search]]></desc>
			<sql><![CDATA[
SELECT DISTINCT AR_OFC_CD AS OFC_CD
FROM MDM_ORGANIZATION MO
WHERE MO.AR_OFC_CD IS NOT NULL
#if(${ofc_entr_lvl_cd} == 'HO' || ${ofc_entr_lvl_cd} == 'ALL')
AND 1=1
#else
AND MO.AR_HD_QTR_OFC_CD = ( SELECT MO2.AR_HD_QTR_OFC_CD 
                               FROM MDM_ORGANIZATION MO2 
                               WHERE MO2.OFC_CD = @[mdm_ofc_cd] )
#end
AND EXISTS(SELECT 'X'
    		FROM   SAP_BANK_ACCT SBA
            WHERE  SBA.AR_OFC_CD = MO.AR_OFC_CD)
ORDER BY AR_OFC_CD			]]></sql>
			<params>
				<param name="mdm_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
