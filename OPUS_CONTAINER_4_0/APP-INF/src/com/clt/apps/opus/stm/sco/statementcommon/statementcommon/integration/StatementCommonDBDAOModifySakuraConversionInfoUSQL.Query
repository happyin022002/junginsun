<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOModifySakuraConversionInfoUSQL">
			<desc><![CDATA[ModifySakuraConversionInfo]]></desc>
			<sql><![CDATA[
UPDATE SCO_STMT_CD_CONV
   SET TGT_CD     = @[tgt_cd]
     , TGT_DESC   = @[tgt_desc]
     , USE_FLG    = @[use_flg]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT     = SYSDATE
 WHERE CONV_TP_CD = @[conv_tp_cd]
   AND SRC_CD     = @[src_cd]			]]></sql>
			<params>
				<param name="tgt_cd" type="12" value="" out="N"/>
				<param name="tgt_desc" type="12" value="" out="N"/>
				<param name="use_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="conv_tp_cd" type="12" value="" out="N"/>
				<param name="src_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
