<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOsearchOtsSourceExcludeListRSQL">
			<desc><![CDATA[Outstanding Source To Exclude  Popup]]></desc>
			<sql><![CDATA[
SELECT
   D.LU_CD
 , D.LU_DESC
FROM SCO_LU_HDR H,  SCO_LU_DTL D
WHERE H.LU_TP_CD = D.LU_TP_CD
AND   D.LU_TP_CD = 'OTS SRC CD'
AND   D.ENBL_FLG = 'Y'
ORDER BY 1,2			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
