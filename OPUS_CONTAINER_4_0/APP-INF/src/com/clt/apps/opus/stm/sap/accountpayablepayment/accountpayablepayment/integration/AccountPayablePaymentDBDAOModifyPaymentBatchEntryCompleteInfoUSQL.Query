<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOModifyPaymentBatchEntryCompleteInfoUSQL">
			<desc><![CDATA[modifyPaymentBatchEntryCompleteInfo]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_SEL_CRTE 
SET     PAY_STS_CD = 'SUCCESS'
      , PAY_DOC_NO = (SELECT COUNT(SSI.PAY_BAT_SEQ) FROM SAP_SEL_INV SSI WHERE SSI.PAY_BAT_SEQ = @[pay_bat_seq] AND SSI.PAY_BAT_NM = @[pay_bat_nm])
      , UPD_DT = SYSDATE
      , UPD_USR_ID = @[usr_id]
WHERE   PAY_BAT_SEQ = @[pay_bat_seq]
AND     PAY_BAT_NM = @[pay_bat_nm]			]]></sql>
			<params>
				<param name="pay_bat_seq" type="12" value="" out="N"/>
				<param name="pay_bat_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
