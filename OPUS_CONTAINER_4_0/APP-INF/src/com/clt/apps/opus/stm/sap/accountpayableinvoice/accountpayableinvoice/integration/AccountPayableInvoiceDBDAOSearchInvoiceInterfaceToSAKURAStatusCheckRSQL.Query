<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceToSAKURAStatusCheckRSQL">
			<desc><![CDATA[AP Invoice에 대한 SAKURA 로 Interface하기 위한 자료 처리를 했는지 여부 체크]]></desc>
			<sql><![CDATA[
SELECT  'Y' AS EXISTS_FLAG
FROM    SAP_AP_IF SAI
WHERE   SAI.REF_DOC_NO = @[inv_no]
AND     ROWNUM = 1			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
