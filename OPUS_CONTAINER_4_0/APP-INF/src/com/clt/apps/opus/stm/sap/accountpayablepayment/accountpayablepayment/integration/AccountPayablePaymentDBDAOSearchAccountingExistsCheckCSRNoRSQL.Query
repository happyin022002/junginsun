<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingExistsCheckCSRNoRSQL">
			<desc><![CDATA[SearchAccountingExistsCheckCSRNo]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN SUM(DECODE(ACCTG_PST_FLG, 'Y', 1, 0 )) > 0 THEN 'Y' ELSE 'N' END AS ACCTG_YN
FROM  SAP_INV_HDR H
     ,SAP_INV_DTL D 
WHERE H.INV_SEQ = D.INV_SEQ
AND   H.INV_NO = @[inv_no]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
