<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchGLDateAPPeriodCheckRSQL">
			<desc><![CDATA[SearchGLDateAPPeriodCheck]]></desc>
			<sql><![CDATA[
SELECT  SP.PRD_STS_CD AS CHK_PRD
FROM    SCO_PERIOD SP
WHERE   SP.EFF_YRMON = SUBSTR(REPLACE(@[gl_dt],'-'),0,6)  
AND     SP.PRD_APPL_CD = 'AP'			]]></sql>
			<params>
				<param name="gl_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
