<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASACltRfndAdjListRSQL">
			<desc><![CDATA[Search ASA adjust target list]]></desc>
			<sql><![CDATA[
SELECT          A.ASA_NO
              , A.BL_NO
              , A.INV_NO
              , A.CHG_TP_CD
              , A.ASA_CLT_SEQ
              , A.AR_OFC_CD
              , A.VVD_CD
              , A.PORT_CD
              , A.DUE_DT
              , A.ASA_TP_CD
              , A.USD_AMT
              , A.ASA_XCH_RT1
              , A.EQV_LOCL_AMT
              , A.LOCL_AMT
              , A.ASA_XCH_RT2
              , A.CHG_USD_AMT
              , A.TTL_AMT
              , A.ASA_RMK
              , A.EFF_DT              
              , A.SVC_SCP_CD
              , A.IB_OB_CD
              , A.ASA_CURR_CD
              , A.SAIL_ARR_DT
              , A.GL_YRMON
              , A.LOCL_CURR_CD
              , A.N3RD_CURR_CD1
              , A.N3RD_AMT1
              , A.N3RD_XCH_RT1
              , A.N3RD_LOCL_AMT1
              , A.N3RD_CURR_CD2
              , A.N3RD_AMT2
              , A.N3RD_XCH_RT2
              , A.N3RD_LOCL_AMT2
              , A.N3RD_CURR_CD3
              , A.N3RD_AMT3
              , A.N3RD_XCH_RT3
              , A.N3RD_LOCL_AMT3
              , A.N3RD_CURR_CD4
              , A.N3RD_AMT4
              , A.N3RD_XCH_RT4
              , A.N3RD_LOCL_AMT4
              , A.EQV_LOCL_AMT2
              , A.APRO_FLG
              , B.AGN_CD
              , B.ASA_PRD_FM_DT
              , B.ASA_PRD_TO_DT
              , B.CURR_CD
              , B.ASA_STS_CD
              , '' AS BL_CURR_CD
              , '0' AS ADJ_AMT
    FROM      SAR_AGN_CLT_RFND_MST A
            , SAR_ASA_MST B
    WHERE     A.ASA_NO = B.ASA_NO
      AND     B.ASA_STS_CD  = 'F'
      AND     A.ASA_NO = @[asa_no]			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
