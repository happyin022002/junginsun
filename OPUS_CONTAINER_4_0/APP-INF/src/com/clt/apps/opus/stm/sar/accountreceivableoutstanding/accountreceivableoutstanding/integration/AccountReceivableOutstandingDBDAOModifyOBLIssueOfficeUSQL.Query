<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOModifyOBLIssueOfficeUSQL">
			<desc><![CDATA[Modify OBL Issue Office]]></desc>
			<sql><![CDATA[
UPDATE SAR_OTS_HDR A
SET OBL_ISS_OFC_CD = (SELECT NVL(Q.OBL_ISS_OFC_CD, P.BKG_OFC_CD)
                      FROM BKG_BOOKING P,
                           BKG_BL_ISS Q
                      WHERE P.BKG_NO = Q.BKG_NO(+)
                      AND P.BKG_NO = A.BKG_NO)
    , UPD_USR_ID = @[upd_usr_id]
    , UPD_DT = SYSDATE
WHERE RHQ_CD = @[rhq_cd]
AND OTS_OFC_CD = @[ots_ofc_cd]
AND BL_NO = @[bl_no]
AND INV_NO = @[inv_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
