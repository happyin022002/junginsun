<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOSearchSarAcctCodeListRSQL">
			<desc><![CDATA[Search AR/GAIN/LOSS Account Code]]></desc>
			<sql><![CDATA[
SELECT AR_ACCT_CD,
  LEGR_XCH_DIFF_INCM_ACCT_CD,
  LEGR_XCH_DIFF_LSS_ACCT_CD
FROM SAR_ACCT_MTX SAM
WHERE SAM.ACCT_CTNT1 = 'ADJ'
  AND SAM.ACCT_TP_CD = @[adj_tp_cd]			]]></sql>
			<params>
				<param name="adj_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
