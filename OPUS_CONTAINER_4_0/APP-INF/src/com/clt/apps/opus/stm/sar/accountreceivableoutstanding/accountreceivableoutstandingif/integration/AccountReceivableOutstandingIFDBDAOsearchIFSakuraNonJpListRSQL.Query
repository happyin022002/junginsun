<sqls>
	<querys>
		<query name="AccountReceivableOutstandingIFDBDAOsearchIFSakuraNonJpListRSQL">
			<desc><![CDATA[searchIFSakuraNonJpList]]></desc>
			<sql><![CDATA[
#if (${check_if_no} == '' )
SELECT DISTINCT
       sohi.if_no AS ref_doc_no
#else

#if (${grp_yn} == 'Y' )
SELECT
	MAX(IF_SEQ_NO) IF_SEQ_NO,
	MAX(REC_ID) REC_ID,
	ACCT_CO_CD,
	IF_DOC_TP_CD,
	DOC_DT,
	PST_DT,
	REF_DOC_NO,
	DOC_HDR_CD,
	CURR_CD, 
	TAX_CALC_AUTO_FLG,
	PST_KEY_CD, 
	VAT_TAX_CD,
	SUM(LOCL_AMT) LOCL_AMT,
	SUM(DOC_AMT) DOC_AMT,
	SUM(LOCL_TAX_AMT) LOCL_TAX_AMT,
	SUM(DOC_TAX_AMT) DOC_TAX_AMT,
	ASGN_NO,
	ITM_DESC,
	PLN_DT,
	COST_CTR_CD,
	ORD_NO,
	MN_ASET_NO,
	SUB_ASET_NO,
	ASET_TJ_TP_CD,
	ASET_VAL_DT,
	GL_ACCT_NO,
	CUST_NO,
	VNDR_CRTR_ACCT_NO,
	DUE_DT_CALC_BSEL_DT,
	PAY_MZD_CD,
	STE_CNTRL_BANK_IND_CD,
	MTRL_NO,
	FUEL_LAND_QTY,
	MEAS_BSE_UT_CD,
	PFITCTR_CD,
	ALTN_ACCT_NO,
	BIZ_PRNR_REF_KEY_CD1,
	BIZ_PRNR_REF_KEY_CD2,
	LINE_ITM_REF_KEY_CD,
	INSTR_KEY_CD1,
	INSTR_KEY_CD2,
	INSTR_KEY_CD3,
	PAY_REF_CD,
	AUTOMTC_PAY_CURR_CD,
	PAY_CURR_AMT,
	CTRT_NO,
	CTRT_TP_CD,
	PAY_RSN_CD,
	CLSS_CD,
	ACT_PLC_CD,
	ENTR_EXPN_ID,
	BUD_MGMT_DIV_CD,
	ACT_DT,
	VSL_CD,
	VVL_CD,
	HUS_BANK_ID,
	PAY_BLCK_KEY_CD,
	MAX(N1ST_LODG_PORT_CD) N1ST_LODG_PORT_CD,
	MAX(N1ST_LODG_PORT_ETD_DT) N1ST_LODG_PORT_ETD_DT,
	MAX(LST_DCHG_PORT_CD) LST_DCHG_PORT_CD,
	MAX(LST_DCHG_PORT_ETA_DT) LST_DCHG_PORT_ETA_DT,
	MAX(TRD_CD) TRD_CD,
	MAX(TRNK_VVD_CD) TRNK_VVD_CD,
	REC_TP_CD,
	MAX(SLAN_CD) SLAN_CD,
	MAX(BKG_QTY) BKG_QTY,
	MAX(ASA_FLG) ASA_FLG,
    CRE_USR_ID,
    UPD_USR_ID
FROM (
#end
  -- Invoice(Non-JP)
SELECT --DISTINCT
      --  SAR_AR_IF_SEQ.NEXTVAL  as AR_IF_Seq
      NULL AS If_Seq_No
     , '' AS rec_id
     , '1000' AS acct_co_cd
     , 'H1' AS if_doc_tp_Cd
     , TO_CHAR(SOHI.IF_DT,'YYYYMMDD') AS doc_dt
     , SOHI.GL_DT AS pst_dt
     #if($check_if_nos.size() > 1)
	 	,SUBSTR(SOHI.IF_NO,1,LENGTH(SOHI.IF_NO) -1) AS REF_DOC_NO
	 #else
     	,SOHI.IF_NO AS REF_DOC_NO 
     #end 
     ,NULL AS doc_hdr_cd
      -- 20150406 source currency 기준으로 변경
     --, SOH.OFC_CURR_CD AS curr_cd
     , SODT.CURR_CD AS curr_cd
     ,'' AS tax_calc_auto_flg
     -- 20150205 , DECODE(SODT.ACCT_CLSS_CD,'REC',DECODE(SIGN(SOC.INV_AMT),1,'01',-1,'11'), 'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X',DECODE(SIGN(SOC.INV_AMT),1,'11',-1,'01'), DECODE(SOC.TJ_SRC_NM,'VAT',DECODE(SIGN(SOC.INV_AMT),1,'11',-1,'01'), DECODE(SIGN(SOC.INV_AMT),1,'50',-1,'40')))) AS pst_key_cd
     -- 20160421 PST Key 조건 수정
     , CASE
         WHEN SODT.ACCT_CLSS_CD ='REC'THEN
             DECODE(NVL(SODT.ACCT_DR_AMT, 0)+ NVL(SODT.INP_DR_AMT, 0), 0, '11', '01')
         WHEN SODT.ACCT_CLSS_CD ='REV' THEN
              CASE 
              WHEN SUBSTR(SODT.CHG_TP_CD,1,1) ='X' THEN
                   DECODE(NVL(SODT.ACCT_DR_AMT, 0)+ NVL(SODT.INP_DR_AMT, 0), 0, '11', '01')
              ELSE
                   CASE 
                   WHEN SOC.TJ_SRC_NM ='VAT' THEN
                       DECODE(NVL(SODT.ACCT_DR_AMT, 0)+ NVL(SODT.INP_DR_AMT, 0), 0, '11', '01')
                   ELSE
                       DECODE(NVL(SODT.ACCT_DR_AMT, 0)+ NVL(SODT.INP_DR_AMT, 0), 0, '50', '40')
                   END
              END
       END            AS pst_key_cd 
      --20160421

     /* 20150102
     , DECODE(SODT.ACCT_CLSS_CD,'REV','D0','') AS vat_tax_cd */
     --20150224 wskim 로직수정
     /*,DECODE(SODT.ACCT_CLSS_CD, 'REC', '', 'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','',
                        DECODE((SELECT MA.ACCT_CD
                                FROM  MDM_ACCOUNT MA
                                     ,MDM_CHARGE MC
                                WHERE MA.ACCT_CD = MC.CO_CHG_ACCT_CD
                                AND MC.CHG_CD = SODT.CHG_TP_CD
                                AND ROWNUM=1),'411111', 'B0','D0'))) AS vat_tax_cd1*/
/*     , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
               DECODE((SELECT MA.ACCT_CD
                       FROM  MDM_ACCOUNT MA
                            ,MDM_CHARGE MC
                       WHERE MA.ACCT_CD = MC.CO_CHG_ACCT_CD
                       AND MC.CHG_CD = SODT.CHG_TP_CD
                       AND ROWNUM=1),'411111', 'B0','D0')
           ELSE
               ''
       END   AS vat_tax_cd    */  
       /* 20150918 tax_cd 로직 변경 */
       /* 20151117 BL 발행 지역에 따라서 DOC FEE 관련 로직 변경 */
       /* 20151127 For Origin Doc Fee, bl issue office. for descination doc fees, POD Country */
       , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
                CASE
                  WHEN SODT.CHG_TP_CD in ('DOC','ESI','XDO') THEN
                       CASE 
                         WHEN (select FINC_RGN_CD from mdm_organization where ofc_cd = SOH.OBL_ISS_OFC_CD) =11 THEN
                                   'A3'
                         ELSE
                                   'D0'
                         END
                  WHEN SODT.CHG_TP_CD in ('DOF','XDD','XDS') THEN
                       CASE 
                         WHEN (select CNT_CD from mdm_location where loc_cd =SOH.POD_CD) ='JP' THEN
                                   'A3'
                         ELSE
                                   'D0'
                         END
                  WHEN  ((SOH.BKG_IO_BND_CD ='O' and SUBSTR(SOH.POL_CD,1,2)='JP')or (SOH.BKG_IO_BND_CD ='I' and SUBSTR(SOH.POD_CD,1,2)='JP')) THEN
                           'B0'
                  ELSE
                           'D0'
                 END
           ELSE
               ''
        END   AS vat_tax_cd                                          
    /*20150204*/       
   -- 20150406 source currency 기준으로 변경                
     --, DECODE(SOH.OFC_CURR_CD, 'JPY','',DECODE(NVL(SODT.ACCT_DR_AMT,0),0,SODT.ACCT_CR_AMT,SODT.ACCT_DR_AMT)) AS locl_amt
   /*20151216 금액의 nvl 처리 */
       , DECODE(SODT.CURR_CD, 'JPY','',DECODE(NVL(SODT.ACCT_DR_AMT,0),0,NVL(SODT.ACCT_CR_AMT,0),NVL(SODT.ACCT_DR_AMT,0)))  AS locl_amt
   /*20150119
    --  , DECODE(NVL(SODT.INP_DR_AMT,0),0,SODT.INP_CR_AMT,SODT.INP_DR_AMT)AS doc_amt */
     --20150205 wskim format mask 기준 currency 변경
     --20150210 wskim OTS Detail -> OTS History의 환율로 변경
     --, TO_NUMBER(SAR_GET_CUR_AMT_FNC(SODT.CURR_CD, ABS(NVL(SODT.INP_DR_AMT, 0) - NVL(SODT.INP_CR_AMT, 0)) * SOD.LOCL_XCH_RT)) AS doc_amt 
     -- 20150406 source currency 기준으로 변경      
     --, TO_NUMBER(SAR_GET_CUR_AMT_FNC(SOH.OFC_CURR_CD, ABS(NVL(SODT.INP_DR_AMT, 0) - NVL(SODT.INP_CR_AMT, 0)) * SOHI.LOCL_XCH_RT)) AS doc_amt
     , ABS(NVL(SODT.INP_DR_AMT, 0) - NVL(SODT.INP_CR_AMT, 0))  AS doc_amt
     ,'' AS locl_tax_amt
     ,'' AS doc_tax_amt
     , SOH.BL_NO AS asgn_no
     , DECODE(SODT.ACCT_CLSS_CD,'REC', SOH.CLT_OFC_CD||':'
              ||(SELECT USR_NM 
                 FROM COM_USER
                 WHERE USR_ID = SOH.CRE_USR_ID),'') AS itm_desc 
     , DECODE(SODT.ACCT_CLSS_CD,'REC', SOHI.SAIL_DT ,'') AS pln_dt
     /* 20150102
      , DECODE(SODT.ACCT_CLSS_CD,'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','','TBD-A101'),'') AS cost_ctr_cd -- 향후 mdm_svc_scp 에서 profit_center_code 가져옴 */
    --20150224 wskim 로직 수정
    /*, DECODE(SODT.ACCT_CLSS_CD,'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','',  
                                                                    (SELECT MODI_COST_CTR_CD 
                                                                     FROM MDM_SVC_SCP
                                                                     WHERE SVC_SCP_CD =SOH.SVC_SCP_CD)),'') AS cost_ctr_cd1*/
 /* 20160401 'OTH', SCO- SAKURA Code conversion 을 먼저 체크 하도록 수정*/
    , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
               /*   DECODE(SOHI.OTS_SRC_CD,'JOO',
                                    (SELECT MVSL.MODI_COST_CTR_CD FROM MDM_VSL_SVC_LANE MVSL WHERE MVSL.VSL_SLAN_CD = SOH.LANE_CD), 
                                     DECODE(SOHI.REV_TP_SRC_CD, 'MRD', 
                                                                DECODE(NVL(SOH.BKG_NO,'XX'), 'XX', (SELECT MVSL.MODI_COST_CTR_CD FROM MDM_VSL_SVC_LANE MVSL WHERE MVSL.VSL_SLAN_CD = SOH.LANE_CD), (SELECT MODI_COST_CTR_CD FROM MDM_SVC_SCP WHERE SVC_SCP_CD =SOHI.SVC_SCP_CD)),
                                                                (SELECT MODI_COST_CTR_CD FROM MDM_SVC_SCP WHERE SVC_SCP_CD =SOHI.SVC_SCP_CD)))*/
                    NVL(DECODE(SOHI.SVC_SCP_CD, 'OTH', NULL,
                       (SELECT MSS.MODI_COST_CTR_CD 
                        FROM   MDM_SVC_SCP MSS
                        WHERE  MSS.SVC_SCP_CD =SOHI.SVC_SCP_CD)), 
                        NVL(DECODE((SELECT SSCC.TGT_CD 
                                    FROM   SCO_STMT_CD_CONV SSCC 
                                    WHERE  SSCC.CONV_TP_CD = 'GL ACCT OF PROFIT CENTER'
                                    AND    SSCC.SRC_CD = (SELECT MA.MODI_ACCT_CD
                                                          FROM  MDM_ACCOUNT MA                          
                                                          WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
                                    AND    SSCC.USE_FLG = 'Y' 
                                    AND    SSCC.DELT_FLG = 'N' 
                                    AND ROWNUM = 1), 'ZH',(SELECT  MO.MODI_COST_CTR_CD
                                                           FROM MDM_ORGANIZATION MO
                                                           WHERE MO.Ofc_Cd= SOH.CLT_OFC_CD),
                                                          (SELECT SSCC.TGT_CD 
                                                           FROM   SCO_STMT_CD_CONV SSCC 
                                                           WHERE  SSCC.CONV_TP_CD = 'GL ACCT OF PROFIT CENTER'
                                                           AND    SSCC.SRC_CD = ( SELECT MA.MODI_ACCT_CD
                                                                                  FROM  MDM_ACCOUNT MA                          
                                                                                  WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
                                                           AND SSCC.USE_FLG = 'Y' 
                                                           AND SSCC.DELT_FLG = 'N' 
                                                           AND ROWNUM = 1)), 'A106')) 
           ELSE
               ''
       END   AS cost_ctr_cd  
            /* 20160401*/                
     ,'' AS ord_no
     ,'' AS mn_aset_no
     ,'' AS sub_aset_no
     ,'' AS aset_tj_tp_cd
     ,'' AS aset_val_dt
     /*20150127-1*/
     --20150224 wskim 로직 수정
     /*, DECODE(SODT.ACCT_CLSS_CD,'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','',  
                     (SELECT MA.MODI_ACCT_CD
                      FROM  MDM_ACCOUNT MA                          
                      WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))), '') AS gl_acct_no1*/
/*     , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
               (SELECT MA.MODI_ACCT_CD
                FROM  MDM_ACCOUNT MA                          
                WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ)          )
           ELSE
               ''
       END   AS gl_acct_no*/
       
      /*20151021 해당 국가에 대해서만 */
    , CASE
             WHEN SODT.ACCT_CLSS_CD ='REV' THEN
                 CASE
                      WHEN (SOC.TJ_SRC_NM = 'VAT' or  SUBSTR(SODT.CHG_TP_CD,1,1) = 'X') THEN
                           ''
                      WHEN (SELECT MA.MODI_ACCT_CD
                            FROM  MDM_ACCOUNT MA                          
                            WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ)) ='5360000000' THEN
                            CASE
                                WHEN (SELECT NVL(MAX('Y'),'N')
                                      FROM MDM_ORGANIZATION A, MDM_CHARGE B
                                      WHERE SUBSTR(A.LOC_CD,1,2) = B.TAX_CNT_CD
                                      AND B.CHG_CD = SODT.CHG_TP_CD
                                      AND A.OFC_CD = SOH.CLT_OFC_CD) = 'N' THEN
                                      '7001000000'
                                ELSE
                                      '5360000000'
                            END
                       ELSE
                          (SELECT MA.MODI_ACCT_CD
                            FROM  MDM_ACCOUNT MA                          
                            WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
                  END    
             ELSE
                ''
       END AS gl_acct_no
         
      /*20150122*/
    /* , DECODE(SODT.ACCT_CLSS_CD,'REC','G1'||SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000')),'REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X',
             (SELECT MODI_AGN_CD
              FROM MDM_ORGANIZATION MO
              WHERE MO.OFC_CD=SOH.CLT_OFC_CD
              AND rownum=1),''), 
             (SELECT MODI_AGN_CD
              FROM MDM_ORGANIZATION MO
              WHERE MO.OFC_CD=SOH.CLT_OFC_CD
              AND rownum=1)) AS cust_no1 */
     ,  CASE
           WHEN SODT.ACCT_CLSS_CD = 'REC' THEN
             -- 20150710 'TB'일떄는 Vendor로 한다.
             --  'G1'||SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000'))  
               DECODE(SODT.BIL_TO_CUST_CNT_CD,'TB', 
                     (SELECT 'G5'|| ML.CNT_CD || LPAD(TO_CHAR(MV.VNDR_SEQ),6, '0')
                      FROM  MDM_VENDOR MV ,
                            MDM_ORGANIZATION MO ,
                            MDM_LOCATION ML
                      WHERE  MV.OFC_CD = MO.OFC_CD(+)
                      AND    MO.LOC_CD = ML.LOC_CD(+)
                      AND    MV.RFND_PSDO_CUST_CD = SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000'))), 'G1'||SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000'))  )
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN 
               ''
           ELSE
               (SELECT MODI_AGN_CD
                FROM MDM_ORGANIZATION MO
                WHERE MO.OFC_CD=SOH.CLT_OFC_CD
                AND rownum=1)
       END      AS cust_no               
     ,'' AS vndr_crtr_acct_no
     --20150302 wskim 로직변경. v2.0
     --, DECODE(SODT.ACCT_CLSS_CD,'REC', SOH.DUE_DT, '') AS due_dt_calc_bsel_dt
     , CASE
          WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN 
              ''
          ELSE
              SOH.DUE_DT
      END      AS due_dt_calc_bsel_dt     
     ,'' AS pay_mzd_cd
     ,'' AS ste_cntrl_bank_ind_cd
     ,'' AS mtrl_no
     ,'' AS fuel_land_qty
     ,'' AS meas_bse_ut_cd
     /* 20150102
     , DECODE(SODT.ACCT_CLSS_CD,'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','','TBD-A101'),'') AS pfitctr_cd -- 향후  mdm_svc_scp 에서 profit_center_code 가져옴 */
     --20150224 wskim 로직수정
     /*, DECODE(SODT.ACCT_CLSS_CD,'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','',
                                                                    (SELECT MODI_COST_CTR_CD 
                                                                     FROM MDM_SVC_SCP
                                                                     WHERE SVC_SCP_CD =SOH.SVC_SCP_CD)),'') AS pfitctr_cd*/
     /* 20160401 'OTH', SCO- SAKURA Code conversion 을 먼저 체크 하도록 수정*/
     , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
              /* --20150820 TPB BKG NO 없는 경우 SLANE 이용하도록 변경
               DECODE(SOHI.OTS_SRC_CD,'JOO',
                                    (SELECT MVSL.MODI_COST_CTR_CD FROM MDM_VSL_SVC_LANE MVSL WHERE MVSL.VSL_SLAN_CD = SOH.LANE_CD), 
                                     DECODE(SOHI.REV_TP_SRC_CD, 'MRD', 
                                                                DECODE(NVL(SOH.BKG_NO,'XX'), 'XX', (SELECT MVSL.MODI_COST_CTR_CD FROM MDM_VSL_SVC_LANE MVSL WHERE MVSL.VSL_SLAN_CD = SOH.LANE_CD), (SELECT MODI_COST_CTR_CD FROM MDM_SVC_SCP WHERE SVC_SCP_CD =SOHI.SVC_SCP_CD)),
                                                                (SELECT MODI_COST_CTR_CD FROM MDM_SVC_SCP WHERE SVC_SCP_CD =SOHI.SVC_SCP_CD)))*/
              NVL(DECODE(SOHI.SVC_SCP_CD, 'OTH', NULL,
                       (SELECT MSS.MODI_COST_CTR_CD 
                        FROM   MDM_SVC_SCP MSS
                        WHERE  MSS.SVC_SCP_CD =SOHI.SVC_SCP_CD)), 
                        NVL(DECODE((SELECT SSCC.TGT_CD 
                                    FROM   SCO_STMT_CD_CONV SSCC 
                                    WHERE  SSCC.CONV_TP_CD = 'GL ACCT OF PROFIT CENTER'
                                    AND    SSCC.SRC_CD = (SELECT MA.MODI_ACCT_CD
                                                          FROM  MDM_ACCOUNT MA                          
                                                          WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
                                    AND    SSCC.USE_FLG = 'Y' 
                                    AND    SSCC.DELT_FLG = 'N' 
                                    AND ROWNUM = 1), 'ZH',(SELECT  MO.MODI_COST_CTR_CD
                                                           FROM MDM_ORGANIZATION MO
                                                           WHERE MO.Ofc_Cd= SOH.CLT_OFC_CD),
                                                          (SELECT SSCC.TGT_CD 
                                                           FROM   SCO_STMT_CD_CONV SSCC 
                                                           WHERE  SSCC.CONV_TP_CD = 'GL ACCT OF PROFIT CENTER'
                                                           AND    SSCC.SRC_CD = ( SELECT MA.MODI_ACCT_CD
                                                                                  FROM  MDM_ACCOUNT MA                          
                                                                                  WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
                                                           AND SSCC.USE_FLG = 'Y' 
                                                           AND SSCC.DELT_FLG = 'N' 
                                                           AND ROWNUM = 1)), 'A106')) 
           ELSE
               ''
       END   AS pfitctr_cd
     /* 20160401*/
     
     /*20150127-1*/
     --20150224 wskim 로직 수정
     /*, DECODE(SODT.ACCT_CLSS_CD,'REC',
                     (SELECT MA.MODI_ACCT_CD 
                      FROM  MDM_ACCOUNT MA                          
                      WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ)), 
                                'REV',DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','5360000000',''),'5360000000') AS altn_acct_no*/
     , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REC' THEN
               (SELECT MA.MODI_ACCT_CD 
                FROM  MDM_ACCOUNT MA                          
                WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
           WHEN SODT.ACCT_CLSS_CD = 'REV' THEN
               DECODE(SOC.TJ_SRC_NM, 'VAT', '5360000000', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','5360000000',''))
       END   AS altn_acct_no 
      /* 20150102
     , DECODE(SODT.ACCT_CLSS_CD,'REC','TBD-A101','REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','A106',''),'A160') AS biz_prnr_ref_key_cd1 -- 향후  mdm_svc_scp 에서 profit_center_code 가져옴 */
     --20150224 wskim 로직 수정
     /*, DECODE(SODT.ACCT_CLSS_CD
              ,'REC',(SELECT MODI_COST_CTR_CD 
                     FROM MDM_SVC_SCP
                     WHERE SVC_SCP_CD =SOH.SVC_SCP_CD)
              ,'REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','A106',''),'A160') AS biz_prnr_ref_key_cd1*/
    /* 20160401 'OTH'인 경우 SCO-SAKURA Code Conversion */  
     , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REC' THEN
               --20150820 TPB BKG NO 없는 경우 SLANE 이용하도록 변경
              /* DECODE(SOHI.OTS_SRC_CD,'JOO',
                                    (SELECT MVSL.MODI_COST_CTR_CD FROM MDM_VSL_SVC_LANE MVSL WHERE MVSL.VSL_SLAN_CD = SOH.LANE_CD), 
                                     DECODE(SOHI.REV_TP_SRC_CD, 'MRD', 
                                                                DECODE(NVL(SOH.BKG_NO,'XX'), 'XX', (SELECT MVSL.MODI_COST_CTR_CD FROM MDM_VSL_SVC_LANE MVSL WHERE MVSL.VSL_SLAN_CD = SOH.LANE_CD), (SELECT MODI_COST_CTR_CD FROM MDM_SVC_SCP WHERE SVC_SCP_CD =SOHI.SVC_SCP_CD)),
                                                                (SELECT MODI_COST_CTR_CD FROM MDM_SVC_SCP WHERE SVC_SCP_CD =SOHI.SVC_SCP_CD)))*/
               NVL(DECODE(SOHI.SVC_SCP_CD, 'OTH', NULL,
                       (SELECT MSS.MODI_COST_CTR_CD 
                        FROM   MDM_SVC_SCP MSS
                        WHERE  MSS.SVC_SCP_CD =SOHI.SVC_SCP_CD)), 
                        NVL(DECODE((SELECT SSCC.TGT_CD 
                                    FROM   SCO_STMT_CD_CONV SSCC 
                                    WHERE  SSCC.CONV_TP_CD = 'GL ACCT OF PROFIT CENTER'
                                    AND    SSCC.SRC_CD = (SELECT MA.MODI_ACCT_CD
                                                          FROM  MDM_ACCOUNT MA                          
                                                          WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
                                    AND    SSCC.USE_FLG = 'Y' 
                                    AND    SSCC.DELT_FLG = 'N' 
                                    AND ROWNUM = 1), 'ZH',(SELECT  MO.MODI_COST_CTR_CD
                                                           FROM MDM_ORGANIZATION MO
                                                           WHERE MO.Ofc_Cd= SOH.CLT_OFC_CD),
                                                          (SELECT SSCC.TGT_CD 
                                                           FROM   SCO_STMT_CD_CONV SSCC 
                                                           WHERE  SSCC.CONV_TP_CD = 'GL ACCT OF PROFIT CENTER'
                                                           AND    SSCC.SRC_CD = ( SELECT MA.MODI_ACCT_CD
                                                                                  FROM  MDM_ACCOUNT MA                          
                                                                                  WHERE MA.ACCT_CD = (SELECT SGM_CTNT4 from SCO_LEGR_CD_CMB where CD_CMB_SEQ =SODT.OTS_CD_CMB_SEQ))
                                                           AND SSCC.USE_FLG = 'Y' 
                                                           AND SSCC.DELT_FLG = 'N' 
                                                           AND ROWNUM = 1)), 'A106')) 
           WHEN SODT.ACCT_CLSS_CD = 'REV' THEN
               --20150514 Local charge 의 경우  'A106' 들어가도록 변경
               DECODE(SOC.TJ_SRC_NM, 'VAT', 'A106', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','A106',''))
       END   AS biz_prnr_ref_key_cd1
      /* 20160401*/
     , DECODE(SODT.ACCT_CLSS_CD,'REC','GT','') AS biz_prnr_ref_key_cd2
     ,'' AS line_itm_ref_key_cd
     ,'' AS instr_key_cd1
     ,'' AS instr_key_cd2
     ,'' AS instr_key_cd3
     /*20150127 */
     --20150224 wskim 로직 변경
     --, DECODE(SOC.TJ_SRC_NM, 'VAT', '31', DECODE(SODT.ACCT_CLSS_CD, 'REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','34',''),'')) AS pay_ref_cd
     --20150326
     ,CASE 
          WHEN SODT.ACCT_CLSS_CD = 'REV' THEN
              DECODE(SOC.TJ_SRC_NM, 'VAT', '31', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','34',''))
          ELSE 
              ''
      END   AS pay_ref_cd
      ,SODT.ACCT_CLSS_CD
      ,SOC.TJ_SRC_NM
      ,SODT.CHG_TP_CD
     /* 20150129 
     , DECODE(SODT.ACCT_CLSS_CD,'REC',DECODE(SODT.CURR_CD, FC.CURR_CD,'', FC.CURR_CD),'') AS automtc_pay_curr_cd
     , DECODE(SODT.ACCT_CLSS_CD,'REC', DECODE(SODT.CURR_CD, FC.CURR_CD,'', DECODE(SIGN(SOC.INV_AMT),1,SODT.ACCT_DR_AMT,-1,SODT.ACCT_CR_AMT)),'') AS pay_curr_amt */
     ,'' AS automtc_pay_curr_cd
     ,'' AS pay_curr_amt 
     /*20150121 
     , DECODE(SODT.ACCT_CLSS_CD,
                   'REC',
                  (SELECT MODI_VNDR_CD
                   FROM MDM_VENDOR MV, MDM_ORGANIZATION MO
                   WHERE MV.VNDR_SEQ = MO.Vndr_Seq
                   AND MO.OFC_CD=SOH.CLT_OFC_CD
                   AND rownum=1), 
                   'REV', 
                   DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X',
                                                    (SELECT MODI_VNDR_CD
                                                    FROM MDM_VENDOR MV, MDM_ORGANIZATION MO
                                                    WHERE MV.VNDR_SEQ = MO.Vndr_Seq
                                                    AND MO.OFC_CD=SOH.CLT_OFC_CD
                                                    AND rownum=1)
                  ,SODT.BIL_TO_CUST_CNT_CD||TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000')),
                  (SELECT MODI_VNDR_CD
                   FROM MDM_VENDOR MV, MDM_ORGANIZATION MO
                   WHERE MV.VNDR_SEQ = MO.Vndr_Seq
                   AND MO.OFC_CD=SOH.CLT_OFC_CD
                   AND rownum=1)) AS ctrt_no */
     --20150224 wskim 로직수정
     /*, DECODE(SODT.ACCT_CLSS_CD,
                 'REC',
                (SELECT MODI_AGN_CD
                FROM MDM_ORGANIZATION MO
                WHERE MO.OFC_CD=SOH.CLT_OFC_CD
                AND rownum=1), 
                 'REV', 
                 DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X',
                                                  (SELECT MODI_AGN_CD
                                                  FROM MDM_ORGANIZATION MO
                                                  WHERE MO.OFC_CD=SOH.CLT_OFC_CD
                                                  AND rownum=1)
                ,'G1'||SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000'))),
                (SELECT MODI_AGN_CD
                FROM MDM_ORGANIZATION MO
                WHERE MO.OFC_CD=SOH.CLT_OFC_CD
                AND rownum=1)) AS ctrt_no1*/ 
     , CASE
          WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
              /*'G1'||SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000')) */
               DECODE(SODT.BIL_TO_CUST_CNT_CD,'TB', 
                     (SELECT 'G5'|| ML.CNT_CD || LPAD(TO_CHAR(MV.VNDR_SEQ),6, '0')
                      FROM  MDM_VENDOR MV ,
                            MDM_ORGANIZATION MO ,
                            MDM_LOCATION ML
                      WHERE  MV.OFC_CD = MO.OFC_CD(+)
                      AND    MO.LOC_CD = ML.LOC_CD(+)
                      AND    MV.RFND_PSDO_CUST_CD = SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000'))), 'G1'||SODT.BIL_TO_CUST_CNT_CD||TRIM(TO_CHAR(SODT.BIL_TO_CUST_SEQ,'000000'))  )
          ELSE
              (SELECT MODI_AGN_CD
               FROM   MDM_ORGANIZATION MO
               WHERE  MO.OFC_CD=SOH.CLT_OFC_CD
               AND    rownum=1) 
       END  AS ctrt_no 
     ,'Z' AS ctrt_tp_cd
     ,'' AS pay_rsn_cd
     ,'' AS clss_cd
     --20150224 wskim 로직 수정
     /*, DECODE(SODT.ACCT_CLSS_CD,'REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X', '',
              (SELECT ML.MODI_LOC_CD
              FROM MDM_LOCATION ML
              WHERE ML.LOC_CD =  SOH.POL_CD)),'') AS act_plc_cd1 */
     , CASE
          WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
              (SELECT ML.MODI_LOC_CD
               FROM MDM_LOCATION ML
               WHERE ML.LOC_CD =  SOH.POL_CD)
          ELSE
              ''
       END   AS act_plc_cd 
     ,'' AS entr_expn_id
     ,'' AS bud_mgmt_div_cd
     --20150224 wskim 로직수정
     --, DECODE(SODT.ACCT_CLSS_CD,'REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','', SOHI.SAIL_DT) ,'') AS act_dt1
     , CASE
          WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
              SOHI.SAIL_DT
          ELSE 
              ''
       END  AS act_dt
     --20150224 wskim 로직수정
     /*, DECODE(SODT.ACCT_CLSS_CD,'REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','', 
                (SELECT MVC.MODI_VSL_CD
                 FROM MDM_VSL_CNTR MVC
                 WHERE MVC.VSL_CD = SUBSTR(SOH.TRNK_VVD_CD,1,4))),'') AS vsl_cd1  */
     , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
               (SELECT MVC.MODI_VSL_CD
                FROM MDM_VSL_CNTR MVC
                WHERE MVC.VSL_CD = SUBSTR(SOH.TRNK_VVD_CD,1,4)) 
           ELSE 
               ''
       END   AS vsl_cd 
     --20150224 wskim 로직수정
     /*, DECODE(SODT.ACCT_CLSS_CD,'REV', DECODE(SUBSTR(SODT.CHG_TP_CD,1,1),'X','',
       (SELECT MVC.MODI_VSL_CD
        FROM MDM_VSL_CNTR MVC
        WHERE MVC.VSL_CD = SUBSTR(SOH.TRNK_VVD_CD,1,4))
       ||SUBSTR(SOH.SKD_VOY_NO, 2, 3)
       ||(SELECT VSL_SLAN_DIR_SEQ
         FROM MDM_VSL_SVC_LANE_DIR
         WHERE VSL_SLAN_CD = SOH.LANE_CD
         AND VSL_SLAN_DIR_CD =SOH.DIR_CD)),'') AS vvl_Cd1 */
     , CASE
           WHEN SODT.ACCT_CLSS_CD = 'REV' AND SUBSTR(SODT.CHG_TP_CD,1,1) <> 'X' AND SOC.TJ_SRC_NM <> 'VAT' THEN
              /* (SELECT MVC.MODI_VSL_CD
                FROM MDM_VSL_CNTR MVC
                WHERE MVC.VSL_CD = SUBSTR(SOH.TRNK_VVD_CD,1,4))
               ||SUBSTR(SOH.SKD_VOY_NO, 2, 3)
               ||(SELECT VSL_SLAN_DIR_SEQ
                 FROM MDM_VSL_SVC_LANE_DIR
                 WHERE VSL_SLAN_CD = SOH.LANE_CD
                 AND VSL_SLAN_DIR_CD =SOH.DIR_CD)*/
                --20150315 trunk vvd 로직 보완
                CASE 
                   WHEN SUBSTR(SOH.TRNK_VVD_CD,1,4)in ('CNTC','CFDR','COMC') THEN
                        ''
                   ELSE
                       (SELECT MODI_VSL_CD
                        FROM MDM_VSL_CNTR
                        WHERE VSL_CD = SUBSTR(SOH.TRNK_VVD_CD,1,4)) 
                     || SUBSTR(SOH.TRNK_VVD_CD, 6, 3) 
                     || (SELECT SUBSTR(MODI_VSL_SLAN_DIR_CD,1,1)
                        FROM MDM_VSL_SVC_LANE_DIR
                        WHERE VSL_SLAN_CD = SOH.LANE_CD
                        AND VSL_SLAN_DIR_CD = SUBSTR(SOH.TRNK_VVD_CD, 9, 1) )
                   END
           ELSE
               ''
       END AS vvl_Cd   
     ,'' AS hus_bank_id
     ,'' AS pay_blck_key_cd
     ,SODT.OTS_DTRB_SEQ
     --20141229 wskim Accrual 요청 추가 시작
     ,SOH.POL_CD AS N1ST_LODG_PORT_CD
     ,(
        SELECT  MIN(B.VPS_ETD_DT)      AS POL_ETD
        FROM    BKG_VVD             A
            ,   VSK_VSL_PORT_SKD    B
        WHERE   1 = 1 
          AND   A.VSL_CD            = B.VSL_CD
          AND   A.SKD_VOY_NO        = B.SKD_VOY_NO
          AND   A.SKD_DIR_CD        = B.SKD_DIR_CD
          AND   A.POL_CD            = B.VPS_PORT_CD
          AND   A.POL_CLPT_IND_SEQ  = B.CLPT_IND_SEQ
          AND   A.BKG_NO = SOH.BL_NO
          AND   A.POL_CD = SOH.POL_CD
      ) AS N1ST_LODG_PORT_ETD_DT
     ,SOH.POD_CD AS LST_DCHG_PORT_CD 
     /*20150123*/
     ,(
        SELECT  MAX(VPS_ETA_DT)      AS POD_ETA
        FROM    BKG_VVD             A
            ,   VSK_VSL_PORT_SKD    B
        WHERE   1 = 1
          AND   A.VSL_CD            = B.VSL_CD
          AND   A.SKD_VOY_NO        = B.SKD_VOY_NO
          AND   A.SKD_DIR_CD        = B.SKD_DIR_CD
          AND   A.POD_CD            = B.VPS_PORT_CD
          AND   A.POD_CLPT_IND_SEQ  = B.CLPT_IND_SEQ
          AND   A.BKG_NO = SOH.BL_NO
          AND   A.POD_CD = SOH.POD_CD
      )  AS LST_DCHG_PORT_ETA_DT
     , (SELECT MDR.TRD_CD
       FROM   AR_MST_REV_VVD AMR,
              MDM_DTL_REV_LANE MDR 
       WHERE  AMR.RLANE_CD = MDR.RLANE_CD
       AND    AMR.VSL_CD = SUBSTR( SOHI.REV_VVD_CD ,1,4)
       AND    AMR.SKD_VOY_NO = SUBSTR( SOHI.REV_VVD_CD ,5,4)
       AND    AMR.SKD_DIR_CD = SUBSTR( SOHI.REV_VVD_CD ,9,1)
       AND    AMR.RLANE_DIR_CD = SUBSTR( SOHI.REV_VVD_CD ,10,1)
       AND    MDR.VSL_SLAN_DIR_CD = SUBSTR(SOHI.REV_VVD_CD, 9, 1)
       AND    MDR.FM_CONTI_CD = (SELECT CONTI_CD  
                                 FROM   MDM_LOCATION
                                 WHERE  LOC_CD = SOHI.POL_CD
                                )
       AND    MDR.TO_CONTI_CD = (SELECT CONTI_CD  
                                 FROM   MDM_LOCATION
                                 WHERE  LOC_CD = SOHI.POD_CD
                                )
       )   AS TRD_CD
     , SOH.TRNK_VVD_CD   AS TRNK_VVD_CD
     --20141229 wskim Accrual 요청 추가 끝
     ,SODT.ACCT_CLSS_CD AS REC_TP_CD  -- 20150129 추가 SAR_AR_IF(REC_TP_CD)에 넣어주세요
     --20150224 wskim Accrual 요청. Service Lane code 추가. SAR_AR_IF.SLAN_CD 에 추가
     ,SOH.LANE_CD       AS SLAN_CD
     --20150224 wskim Accrual 요청. BKG 수량 추가. SAR_AR_IF.BKG_QTY에 추가
     ,(SELECT  NVL(SUM(NVL(MCS.CNTR_SZ_TEU_CAPA, 0) * NVL(BQ.OP_CNTR_QTY, 0)), 0)  
       FROM    MDM_CNTR_TP_SZ  MCTS
           ,   MDM_CNTR_SZ     MCS
           ,   BKG_QUANTITY    BQ
       WHERE   1 = 1
       AND   MCTS.CNTR_SZ_CD = MCS.CNTR_SZ_CD
       AND   MCTS.CNTR_TPSZ_CD = BQ.CNTR_TPSZ_CD
       AND   BQ.BKG_NO = SOH.BL_NO)  AS BKG_QTY
     --20150224 wskim Accrual 요청. ASA Flag 추가. SAR_AR_IF.ASA_FLG에 Insert
     , 'N' AS ASA_FLG
     ,SODT.CRE_USR_ID
	 ,SODT.UPD_USR_ID 
#end
  FROM SAR_OTS_HDR SOH
     , SAR_OTS_DTL SOD
     , SAR_OTS_CHG SOC
     , SAR_OTS_HIS SOHI
     , SAR_OTS_DTRB SODT
     , (SELECT SLD.LU_CD CURR_CD
          FROM SCO_LU_HDR SLH
             ,SCO_LU_DTL SLD
         WHERE SLH.LU_APPL_CD ='SCO'
           AND SLH.LU_TP_CD ='FUNCTIONAL CURRENCY'
           AND SLH.LU_TP_CD = SLD.LU_TP_CD) FC
 WHERE SOH.RHQ_CD = SOD.RHQ_CD
   AND SOH.OTS_OFC_CD = SOD.OTS_OFC_CD
   AND SOH.BL_NO = SOD.BL_NO
   AND SOH.INV_NO = SOD.INV_NO
   AND SOD.RHQ_CD = SOC.RHQ_CD
   AND SOD.OTS_OFC_CD = SOC.OTS_OFC_CD
   AND SOD.BL_NO = SOC.BL_NO
   AND SOD.INV_NO = SOC.INV_NO
   --20150206 wskim chg_tp_cd 조건 추가
   AND SOD.CHG_TP_CD = SOC.CHG_TP_CD
   AND SOD.BL_CURR_CD = SOC.BL_CURR_CD   
   AND SOC.RHQ_CD = SOHI.RHQ_CD
   AND SOC.OTS_OFC_CD = SOHI.OTS_OFC_CD
   AND SOC.BL_NO = SOHI.BL_NO
   AND SOC.INV_NO = SOHI.INV_NO
   AND SOHI.OTS_HIS_SEQ = SODT.OTS_HIS_SEQ
   AND SOC.OTS_HIS_SEQ = SODT.OTS_HIS_SEQ
   --20150206 wskim chg_tp_cd 조건 추가
   AND SOC.CHG_TP_CD = SODT.CHG_TP_CD
   AND SOD.BL_CURR_CD = SODT.CURR_CD /*20150119 추가 */
   --20150206 wskim chg_tp_cd 조건 추가
   AND SOC.INV_AMT <> 0
   AND SOHI.REV_TP_SRC_CD <> 'AGT'
   AND SOHI.OTS_HIS_TP_CD = 'OTS'
   #if (${check_if_no} != '' )
   AND SOHI.IF_NO IN (
		#foreach( $key IN ${check_if_nos}) 
 			#if($velocityCount < $check_if_nos.size())
    			'$key',
 			#else
  				'$key'
 			#end
		#end
   ) 
   #end
   AND SODT.AR_IF_STS_CD = @[check_status]
#if (${grp_yn} == 'Y' )
) GROUP BY
 ACCT_CO_CD
,IF_DOC_TP_CD
,DOC_DT
,PST_DT
,REF_DOC_NO
,DOC_HDR_CD
,CURR_CD
,TAX_CALC_AUTO_FLG
,PST_KEY_CD
,VAT_TAX_CD
,ASGN_NO
,ITM_DESC
,PLN_DT
,COST_CTR_CD
,ORD_NO
,MN_ASET_NO
,SUB_ASET_NO
,ASET_TJ_TP_CD
,ASET_VAL_DT
,GL_ACCT_NO
,CUST_NO
,VNDR_CRTR_ACCT_NO
,DUE_DT_CALC_BSEL_DT
,PAY_MZD_CD
,STE_CNTRL_BANK_IND_CD
,MTRL_NO
,FUEL_LAND_QTY
,MEAS_BSE_UT_CD
,PFITCTR_CD
,ALTN_ACCT_NO
,BIZ_PRNR_REF_KEY_CD1
,BIZ_PRNR_REF_KEY_CD2
,LINE_ITM_REF_KEY_CD
,INSTR_KEY_CD1
,INSTR_KEY_CD2
,INSTR_KEY_CD3
,PAY_REF_CD
,AUTOMTC_PAY_CURR_CD
,PAY_CURR_AMT
,CTRT_NO
,CTRT_TP_CD
,PAY_RSN_CD
,CLSS_CD
,ACT_PLC_CD
,ENTR_EXPN_ID
,BUD_MGMT_DIV_CD
,ACT_DT
,VSL_CD
,VVL_CD
,HUS_BANK_ID
,PAY_BLCK_KEY_CD
,REC_TP_CD
,CRE_USR_ID
,UPD_USR_ID
#end			]]></sql>
