<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOModifyAPVendorInfoUSQL">
			<desc><![CDATA[ModifyAPVendorInfo]]></desc>
			<sql><![CDATA[
UPDATE SAP_VENDOR
SET
     LIAB_CD_CMB_SEQ = @[liab_cd_cmb_seq]
   , PAY_CD_CMB_SEQ = @[pay_cd_cmb_seq]
   , PAY_PRIO_CD =  @[pay_prio_cd]
   , UPD_USR_ID = @[usr_id]
   , UPD_DT = SYSDATE
WHERE  1=1
AND    VNDR_NO = @[vndr_no]			]]></sql>
			<params>
				<param name="liab_cd_cmb_seq" type="12" value="" out="N"/>
				<param name="pay_cd_cmb_seq" type="12" value="" out="N"/>
				<param name="pay_prio_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="vndr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
