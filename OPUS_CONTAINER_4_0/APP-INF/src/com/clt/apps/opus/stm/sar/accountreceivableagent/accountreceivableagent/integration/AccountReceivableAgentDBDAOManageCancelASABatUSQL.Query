<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOManageCancelASABatUSQL">
			<desc><![CDATA[Running batch 를 call 한 경우, SCO_BAT_HIS table 에 update 한다.]]></desc>
			<sql><![CDATA[
UPDATE SCO_BAT_HIS
   SET BAT_RSLT_CD = 'E'
     , BAT_RSLT_DESC = 'Batch job is Running.'
WHERE 1=1
  AND BAT_SEQ = @[bat_seq]
  AND PGM_SUB_SYS_CD = 'SAR'
  AND BAT_PGM_NO = 'STM_SAR_B5002'			]]></sql>
			<params>
				<param name="bat_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
