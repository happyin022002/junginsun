VALUES
(
 @[inv_seq]
,@[vndr_no]
,@[inv_no]
,@[inv_curr_cd]
,@[inv_pay_curr_cd]
,ROUND( TO_NUMBER( REPLACE( @[inv_amt], ',', '') ) , 3 ) 
,ROUND( TO_NUMBER( REPLACE( NVL( @[inv_pay_amt], 0 ), ',', '') ) , 3 ) 
,TO_DATE(REPLACE( @[inv_dt], '-', ''),'YYYYMMDD')
,NVL( @[ap_inv_src_cd], 'Manual Invoice Entry')
,@[inv_tp_lu_cd]
,@[inv_desc]
,@[bat_seq]
,ROUND( TO_NUMBER( REPLACE( @[inv_vat_amt], ',', '') ) , 3 ) 
,@[inv_term_nm]
,TO_DATE(REPLACE( @[inv_term_dt], '-', ''),'YYYYMMDD')
,@[pay_mzd_lu_cd]
,@[ap_pay_grp_lu_cd]
,@[liab_coa_co_cd]
,@[liab_coa_rgn_cd]
,@[liab_coa_ctr_cd]
,@[liab_coa_acct_no]
,@[liab_coa_vvd_cd]
,@[liab_coa_inter_co_cd]
,NVL( @[pay_sts_flg], 'N')
,ROUND( TO_NUMBER( REPLACE( @[inv_func_amt], ',', '') ) , 3 ) 
,@[inv_vat_cd]
,@[inv_xch_rt]
,@[inv_xch_rt_tp_cd]
,TO_DATE(REPLACE( @[inv_xch_dt], '-', ''),'YYYYMMDD')
,DECODE( @[inv_tp_lu_cd], 'PREPAYMENT', SYSDATE, TO_DATE( REPLACE( @[ery_stl_dt], '-', ''), 'YYYYMMDD')  )
,@[attr_ctnt1]
,@[attr_ctnt2]
,@[attr_ctnt3]
,@[attr_ctnt4]
,@[attr_ctnt5]
,@[attr_ctnt6]
,@[attr_ctnt7]
,@[attr_ctnt8]
,@[attr_ctnt9]
,@[attr_ctnt10]
,@[attr_ctnt11]
,@[attr_ctnt12]
,@[attr_ctnt13]
,@[attr_ctnt14]
,@[attr_ctnt15]
,@[attr_cate_nm]
,NVL( @[ap_apsts_cd],'NOT REQUIRED' ) 
,TO_DATE(REPLACE( @[inv_cxl_dt], '-', ''),'YYYYMMDD')
,@[cxl_usr_id]
,ROUND( TO_NUMBER( REPLACE( @[inv_cxl_amt], ',', '') ) , 3 ) 
,@[ofc_cd]
,@[glo_attr_cate_nm]
,@[glo_attr_ctnt1]
,@[glo_attr_ctnt2]
,@[glo_attr_ctnt3]
,@[glo_attr_ctnt4]
,@[glo_attr_ctnt5]
,@[glo_attr_ctnt6]
,@[glo_attr_ctnt7]
,@[glo_attr_ctnt8]
,@[glo_attr_ctnt9]
,@[glo_attr_ctnt10]
,@[glo_attr_ctnt11]
,@[glo_attr_ctnt12]
,@[glo_attr_ctnt13]
,@[glo_attr_ctnt14]
,@[glo_attr_ctnt15]
,@[glo_attr_ctnt16]
,@[glo_attr_ctnt17]
,@[glo_attr_ctnt18]
,@[glo_attr_ctnt19]
,@[glo_attr_ctnt20]
,ROUND( TO_NUMBER( REPLACE( @[pay_curr_inv_amt], ',', '') ) , 3 ) 
,REPLACE( @[gl_dt], '-', '')
,'N' -- default N inv_apro_rdy_flg
,@[dtrb_set_seq]
,@[xter_bank_acct_seq]
,@[usr_id]
,SYSDATE
,@[usr_id]
,SYSDATE
,@[liab_cd_cmb_seq]

)			]]></sql>
