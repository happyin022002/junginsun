<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchLookupHeaderRSQL">
			<desc><![CDATA[Lookup Header Search]]></desc>
			<sql><![CDATA[
SELECT LU_APPL_CD
     , LU_TP_CD
     , LU_DESC
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , NVL(CD_CONV_ND_FLG,'N') AS CD_CONV_ND_FLG
  FROM SCO_LU_HDR
 WHERE UPPER(LU_TP_CD) LIKE '%'||UPPER(@[lu_tp_cd])||'%'
   AND UPPER(LU_APPL_CD) LIKE UPPER(@[lu_appl_cd])||'%'
ORDER BY LU_APPL_CD, LU_TP_CD			]]></sql>
			<params>
				<param name="lu_tp_cd" type="12" value="" out="N"/>
				<param name="lu_appl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
