<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOupdateSarOtsDtlUSQL">
			<desc><![CDATA[update SAR_OTS_DTL]]></desc>
			<sql><![CDATA[
UPDATE SAR_OTS_DTL
SET
      INV_AMT = NVL(@[inv_amt], INV_UPD_DT)
    , INV_UPD_DT = NVL(@[inv_upd_dt], RCT_AMT)
    , RCT_AMT = NVL(@[rct_amt], RCT_UPD_DT)
    , RCT_UPD_DT = NVL(@[rct_upd_dt], ADJ_AMT)
    , ADJ_AMT = NVL(@[adj_amt], ADJ_UPD_DT)
    , ADJ_UPD_DT = NVL(@[adj_upd_dt], BAL_AMT)
    , BAL_AMT = NVL(@[bal_amt], BAL_UPD_DT)
    , BAL_UPD_DT = NVL(@[bal_upd_dt], WRTF_AMT)
    , WRTF_AMT = NVL(@[wrtf_amt], WRTF_UPD_DT)
    , WRTF_UPD_DT = NVL(@[wrtf_upd_dt], LOCL_XCH_RT)
    , LOCL_XCH_RT = NVL(@[locl_xch_rt], USD_XCH_RT)
    , USD_XCH_RT =  NVL(@[usd_xch_rt], BAL_LOCL_AM)
    , BAL_LOCL_AMT = NVL(@[bal_locl_amt], BAL_USD_AMT)
    , BAL_USD_AMT = NVL(@[bal_usd_amt], UPD_USR_ID)
    , UPD_USR_ID = NVL(@[upd_usr_id], UPD_DT)
    , UPD_DT = SYSDATE
WHERE 1=1
AND RHQ_CD = @[rhq_cd]
AND OTS_OFC_CD = @[ots_ofc_cd]
AND BL_NO= @[ots_ofc_cd]
AND INV_NO=@[inv_no]
AND BL_CURR_CD = @[bl_curr_cd]
AND CHG_TP_CD = @[chg_tp_cd]			]]></sql>
			<params>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="inv_upd_dt" type="12" value="" out="N"/>
				<param name="rct_amt" type="12" value="" out="N"/>
				<param name="rct_upd_dt" type="12" value="" out="N"/>
				<param name="adj_amt" type="12" value="" out="N"/>
				<param name="adj_upd_dt" type="12" value="" out="N"/>
				<param name="bal_amt" type="12" value="" out="N"/>
				<param name="bal_upd_dt" type="12" value="" out="N"/>
				<param name="wrtf_amt" type="12" value="" out="N"/>
				<param name="wrtf_upd_dt" type="12" value="" out="N"/>
				<param name="locl_xch_rt" type="12" value="" out="N"/>
				<param name="usd_xch_rt" type="12" value="" out="N"/>
				<param name="bal_locl_amt" type="12" value="" out="N"/>
				<param name="bal_usd_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="bl_curr_cd" type="12" value="" out="N"/>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
