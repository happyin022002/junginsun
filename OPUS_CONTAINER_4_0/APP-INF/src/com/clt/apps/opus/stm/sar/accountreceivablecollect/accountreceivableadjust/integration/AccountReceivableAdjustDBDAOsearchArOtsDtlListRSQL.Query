<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOsearchArOtsDtlListRSQL">
			<desc><![CDATA[Search AR OTS Detail]]></desc>
			<sql><![CDATA[
    SELECT    RHQ_CD
              , OTS_OFC_CD
              , BL_NO
              , INV_NO
              , BL_CURR_CD
              , INV_AMT
              , INV_UPD_DT
              , RCT_AMT
              , RCT_UPD_DT
              , ADJ_AMT
              , ADJ_UPD_DT
              , BAL_AMT
              , BAL_UPD_DT
              , WRTF_AMT
              , WRTF_UPD_DT
              , LOCL_XCH_RT
              , USD_XCH_RT
              , BAL_LOCL_AMT
              , BAL_USD_AMT
              , CRE_USR_ID
              , CRE_DT
              , UPD_USR_ID
              , UPD_DT
              , CHG_TP_CD
    FROM      SAR_OTS_DTL
    WHERE     1 = 1
    #if(${rhq_cd} != '')
      AND     RHQ_CD = @[rhq_cd]
    #end
    #if(${ots_ofc_cd} != '')
      AND     OTS_OFC_CD = @[ots_ofc_cd]
    #end
    #if(${bl_no} != '')
	  AND     BL_NO = @[bl_no]
    #end
    #if(${inv_no} != '')
	  AND     INV_NO = @[inv_no]
    #end
    #if(${bl_curr_cd} != '')
	  AND     BL_CURR_CD = @[bl_curr_cd]
    #end
    #if(${chg_tp_cd} != '')
	  AND     CHG_TP_CD = @[chg_tp_cd]
    #end

			]]></sql>
			<params>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="bl_curr_cd" type="12" value="" out="N"/>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
