      , '   ' AS APPL_NM 
  FROM  DUAL
UNION ALL
SELECT  'SAR' AS APPL_CD
      , 'SAR' AS APPL_NM
  FROM  DUAL
UNION ALL
SELECT  'SAP' AS APPL_CD
       , 'SAP' AS APPL_NM
  FROM  DUAL
UNION ALL
SELECT  'SCO' AS APPL_CD
      , 'SCO' AS APPL_NM 
  FROM  DUAL

			]]></sql>
