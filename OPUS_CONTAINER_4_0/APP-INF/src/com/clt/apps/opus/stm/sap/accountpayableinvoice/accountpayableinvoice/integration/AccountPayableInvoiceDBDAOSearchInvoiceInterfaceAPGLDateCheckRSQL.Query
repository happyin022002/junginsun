<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceAPGLDateCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceAPGLDateCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT  PRD_STS_CD  AS AP_PERIOD_USE_FLAG
FROM    SCO_PERIOD
WHERE   EFF_YRMON = SUBSTR(REPLACE(@[gl_dt], '-'), 0, 6) 
AND     PRD_APPL_CD = 'AP'			]]></sql>
			<params>
				<param name="gl_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
