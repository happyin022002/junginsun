<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASANoSeqRSQL">
			<desc><![CDATA[select SAR_ASA_NO_SEQ]]></desc>
			<sql><![CDATA[
    SELECT    OFC_CD
              , AGN_CD
              , ASA_PRD_YR
              , ASA_SEQ
              , CRE_USR_ID
              , CRE_DT
              , UPD_USR_ID
              , UPD_DT
    FROM      SAR_ASA_NO_SEQ
    WHERE     1 = 1
      AND OFC_CD = @[ofc_cd]
      AND AGN_CD = @[agn_cd]

			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
