<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyInvoiceHeaderCancelInfoUSQL">
			<desc><![CDATA[ModifyInvoiceHeaderCancelInfo]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_HDR
SET     INV_AMT = 0
      , INV_FUNC_AMT = 0
      , INV_CXL_AMT = INV_AMT
      , ATTR_CTNT15 = 'N'
      , INV_CXL_DT = SYSDATE
      , ATTR_CTNT5 = NVL(@[attr_ctnt5], ATTR_CTNT5)
      , CXL_USR_ID = @[usr_id]
      , UPD_DT = SYSDATE
      , UPD_USR_ID = @[usr_id]
WHERE   INV_SEQ = @[inv_seq]			]]></sql>
			<params>
				<param name="attr_ctnt5" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
