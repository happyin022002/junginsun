       ,ACCT_CTNT7 = @[acct_ctnt7]
       ,ACCT_CTNT8 = @[acct_ctnt8]
       ,ACCT_MTX_SEQ = @[acct_mtx_seq]
       ,ACCT_TP_CD = @[acct_tp_cd]
       ,ACCT_TP_NM = @[acct_tp_nm]
       ,AMT_SGN_CD = @[amt_sgn_cd]
       ,AR_ACCT_CD = @[ar_acct_cd]
