<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyPaymentScheduleUSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOModifyPaymentScheduleUSQL]]></desc>
			<sql><![CDATA[
UPDATE SAP_PAY_SKD
SET  PAY_RMN_AMT = NVL(PAY_RMN_AMT,0) - REPLACE(@[pay_amt],',','') 
    ,PAY_STS_FLG = DECODE(NVL(PAY_RMN_AMT,0) - REPLACE(@[pay_amt],',',''),0,'Y', PAY_GRS_AMT, 'N', 'P')
    ,UPD_USR_ID = @[usr_id]
    ,UPD_DT = SYSDATE    
WHERE INV_SEQ = @[inv_seq]
AND PAY_SKD_NO = @[pay_skd_no]
AND PAY_RMN_AMT <> 0			]]></sql>
			<params>
				<param name="pay_amt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="pay_skd_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
