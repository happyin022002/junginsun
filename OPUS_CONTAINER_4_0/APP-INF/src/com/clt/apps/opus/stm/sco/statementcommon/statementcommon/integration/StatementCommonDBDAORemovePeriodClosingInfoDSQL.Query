<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAORemovePeriodClosingInfoDSQL">
			<desc><![CDATA[Remove Period Closing Info]]></desc>
			<sql><![CDATA[
DELETE 
  FROM SCO_PERIOD
 WHERE PRD_APPL_CD = @[prd_appl_cd]
   AND EFF_YRMON = @[eff_yrmon]			]]></sql>
			<params>
				<param name="prd_appl_cd" type="12" value="" out="N"/>
				<param name="eff_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
