<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceCurrExchangeRateCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceCurrExchangeRateCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT  GDXR.ACCT_XCH_RT_DT  AS ACCT_XCH_RT_DT
     , GDXR.ACCT_XCH_RT_LVL AS ACCT_XCH_RT_LVL
     , GDXR.CONV_XCH_RT     AS CONV_XCH_RT
FROM    GL_DLY_XCH_RT GDXR
WHERE   GDXR.ACCT_XCH_RT_DT = REPLACE(@[gl_dt], '-', '')
AND     GDXR.ACCT_XCH_RT_LVL = '1'
AND     GDXR.FM_CURR_CD = @[inv_curr_cd]
AND     GDXR.TO_CURR_CD = @[functional_currency]			]]></sql>
			<params>
				<param name="gl_dt" type="12" value="" out="N"/>
				<param name="inv_curr_cd" type="12" value="" out="N"/>
				<param name="functional_currency" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
