<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOSearchBalRctAcctAmtRSQL">
			<desc><![CDATA[SearchBalRctAcctAmt]]></desc>
			<sql><![CDATA[
SELECT NVL(BAL_RCT_ACCT_AMT, 0) BAL_RCT_ACCT_AMT
FROM SAR_RECEIPT
WHERE RCT_SEQ = @[rct_seq]			]]></sql>
			<params>
				<param name="rct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
