<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOsearchAdjustReceiptNoRSQL">
			<desc><![CDATA[Adjust No, Receipt No 조회]]></desc>
			<sql><![CDATA[
SELECT AR_TJ_TP_CD||TP_OFC_CD||TP_CRE_DT||LPAD(TP_SEQ+1,5,'0') AS ADJ_REC_NO
FROM SAR_TP_NO   
WHERE AR_TJ_TP_CD = @[aj_tj_tp_cd]
AND TP_OFC_CD = @[ofc_cd] 
AND TP_CRE_DT = SUBSTR(TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC((SELECT AR_OFC_CD                                           
						   											FROM MDM_ORGANIZATION                                           
						   											WHERE OFC_CD = @[l_ofc_cd]
																)), 'YYYYMMDD')
						,3,6)
FOR	UPDATE	OF TP_SEQ WAIT 30			]]></sql>
			<params>
				<param name="aj_tj_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="l_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
