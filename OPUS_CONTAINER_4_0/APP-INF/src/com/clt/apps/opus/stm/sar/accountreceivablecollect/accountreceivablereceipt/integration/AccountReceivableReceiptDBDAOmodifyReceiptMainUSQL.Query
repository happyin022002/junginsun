<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOmodifyReceiptMainUSQL">
			<desc><![CDATA[Receipt Main 정보 Update]]></desc>
			<sql><![CDATA[
UPDATE SAR_RECEIPT SET
	RCT_CUST_CNT_CD = @[rct_cust_cnt_cd]
	, RCT_CUST_SEQ = TO_NUMBER(@[rct_cust_seq])
	, BAL_RCT_AMT = @[bal_rct_amt]
	, RCT_STS_CD = @[rct_sts_cd]
	, RCT_RMK = @[rct_rmk]
	, BANK_CHG_AMT = @[bank_chg_amt]
	, AGN_OFC_CD = @[agn_ofc_cd]
	, AGN_CD = @[agn_cd]
	, ASA_NO = @[asa_no]
	, UPD_USR_ID = @[upd_usr_id]
	, UPD_DT = SYSDATE
WHERE RCT_SEQ = @[rct_seq]			]]></sql>
			<params>
				<param name="rct_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="rct_cust_seq" type="12" value="" out="N"/>
				<param name="bal_rct_amt" type="12" value="" out="N"/>
				<param name="rct_sts_cd" type="12" value="" out="N"/>
				<param name="rct_rmk" type="12" value="" out="N"/>
				<param name="bank_chg_amt" type="12" value="" out="N"/>
				<param name="agn_ofc_cd" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
