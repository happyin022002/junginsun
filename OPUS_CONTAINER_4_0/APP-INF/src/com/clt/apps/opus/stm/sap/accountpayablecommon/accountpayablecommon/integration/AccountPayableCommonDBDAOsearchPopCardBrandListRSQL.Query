<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOsearchPopCardBrandListRSQL">
			<desc><![CDATA[Card Brand Popup Inquiry]]></desc>
			<sql><![CDATA[
SELECT sld.lu_cd      AS lu_cd
     , sld.lu_desc    AS lu_desc
FROM   sco_lu_hdr slh
     , sco_lu_dtl sld
WHERE  slh.lu_tp_cd = sld.lu_tp_cd 
AND    slh.lu_tp_cd = 'CARD BRAND'
AND    sld.enbl_flg = 'Y'
#if (${lu_cd} != '') 
AND    sld.lu_cd = @[lu_cd]
#end
			]]></sql>
			<params>
				<param name="lu_cd" type="12" value="KM" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
