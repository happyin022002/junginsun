<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOsearchLegrXchRtListRSQL">
			<desc><![CDATA[get Ledger exchange Rate]]></desc>
			<sql><![CDATA[
SELECT ROUND(SAR_GET_GL_XCH_RT_FNC('1', REPLACE(@[adj_dt], '-', ''), @[bl_curr_cd], @[adj_crs_curr_cd]), 6) LEGR_XCH_RT
FROM DUAL			]]></sql>
			<params>
				<param name="adj_dt" type="12" value="" out="N"/>
				<param name="bl_curr_cd" type="12" value="" out="N"/>
				<param name="adj_crs_curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
