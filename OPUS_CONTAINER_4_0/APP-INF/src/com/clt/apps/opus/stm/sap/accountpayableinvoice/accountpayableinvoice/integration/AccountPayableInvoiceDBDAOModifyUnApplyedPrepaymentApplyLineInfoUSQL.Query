<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyUnApplyedPrepaymentApplyLineInfoUSQL">
			<desc><![CDATA[modifyUnApplyedPrepaymentApplyLineInfo]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_DTL
SET     RVS_FLG = 'Y'
      , UPD_USR_ID = @[usr_id]
      , UPD_DT = SYSDATE
WHERE   INV_SEQ = @[standard_inv_seq] --(unapply view)
AND     DTRB_LINE_NO = @[standard_line_no] --(unapply view)
			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="standard_inv_seq" type="12" value="" out="N"/>
				<param name="standard_line_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
