<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchInvHdrSrcCdRSQL">
			<desc><![CDATA[SearchInvHdrSrcCd]]></desc>
			<sql><![CDATA[
SELECT AP_INV_SRC_CD
FROM SAP_INV_HDR
WHERE INV_NO = @[inv_no]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
