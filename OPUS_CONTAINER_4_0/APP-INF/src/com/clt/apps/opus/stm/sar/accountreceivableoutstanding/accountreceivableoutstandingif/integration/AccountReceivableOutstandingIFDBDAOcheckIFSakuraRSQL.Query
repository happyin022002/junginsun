<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingIFDBDAOcheckIFSakuraRSQL">
			<desc><![CDATA[checkIFSakura]]></desc>
			<sql><![CDATA[
SELECT  DECODE(MO.SO_IF_CD, 'B','BRN', 'O' ,'AGT', 'S' ,'AGT' ) BRN_AGT_DIV
       ,DECODE(SOHI.REV_TP_SRC_CD,'ASA','ASA','INV') INV_ASA_DIV 
       ,DECODE(MO.FINC_RGN_CD, 11,'JP','NN') CNT_CD
FROM SAR_OTS_HDR SOH,
     SAR_OTS_HIS SOHI,
     MDM_ORGANIZATION MO      
WHERE SOHI.IF_NO = @[check_if_no]
AND SOH.RHQ_CD= SOHI.RHQ_CD
AND SOH.OTS_OFC_CD = SOHI.OTS_OFC_CD
AND SOH.BL_NO = SOHI.BL_NO
AND SOH.INV_NO = SOHI.INV_NO
AND MO.OFC_CD = SOH.CLT_OFC_CD
AND ROWNUM = 1 			]]></sql>
			<params>
				<param name="check_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
