<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchLocalTimeRSQL">
			<desc><![CDATA[User Id에 해당하는 Office의 Local 시간을 계산하기 위한 Diff 시간 내역을 가져와 Local Sysdate로 처리]]></desc>
			<sql><![CDATA[
SELECT
	TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC((SELECT AP_OFC_CD                                           
						   FROM MDM_ORGANIZATION                                           
						   WHERE OFC_CD = @[ofc_cd])),
       'YYYYMMDD') LCL_TIME
FROM DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
