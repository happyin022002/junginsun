<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOCheckMiscLimitAmountRSQL">
			<desc><![CDATA[Check Misc Limit Amount]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM SCO_OFC_INFO
WHERE OFC_CD = @[misc_ofc_cd]
AND ABS(REPLACE(@[misc_amt], ',', '') * @[misc_xch_rt]) <= ABS(DECODE(@[misc_tp_cd], 'MI', MISC_INCM_LMT_AMT, MISC_LSS_LMT_AMT))			]]></sql>
			<params>
				<param name="misc_ofc_cd" type="12" value="" out="N"/>
				<param name="misc_amt" type="12" value="" out="N"/>
				<param name="misc_xch_rt" type="12" value="" out="N"/>
				<param name="misc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
