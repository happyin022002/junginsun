<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOsearchLastApplicationStatusRSQL">
			<desc><![CDATA[마지막 Application Status 정보를 조회]]></desc>
			<sql><![CDATA[
SELECT RCV_APPL_STS_CD
FROM SAR_RCV_APPL
WHERE RCV_APPL_SEQ = (SELECT MAX(RCV_APPL_SEQ)
					  FROM SAR_RCV_APPL
					  WHERE RCT_SEQ = @[rct_seq]
					  AND RCV_APPL_TP_CD = 'RECEIPT')			]]></sql>
			<params>
				<param name="rct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
