<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchNextPayNoRSQL">
			<desc><![CDATA[searchNextPayNo]]></desc>
			<sql><![CDATA[
SELECT  NVL(SLD.ATTR_CTNT1,0) + 1 AS PAY_NO
FROM    SCO_LU_HDR SLH
      , SCO_LU_DTL SLD
WHERE   SLH.LU_TP_CD = SLD.LU_TP_CD
AND     SLH.LU_TP_CD = 'PAYMENT METHOD'
AND     NVL(SLD.ENBL_FLG, 'Y') = 'Y'
AND     NVL(SLD.LU_ST_DT, SYSDATE) >= SYSDATE
AND     SLH.LU_APPL_CD = 'SAP'
AND     SLD.LU_CD = @[pay_mzd_lu_cd]			]]></sql>
			<params>
				<param name="pay_mzd_lu_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
