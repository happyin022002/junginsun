<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOModifyBalRctAcctAmtUSQL">
			<desc><![CDATA[ModifyBalRctAcctAmt]]></desc>
			<sql><![CDATA[
UPDATE SAR_RECEIPT
SET BAL_RCT_ACCT_AMT = NVL(@[bal_rct_acct_amt], 0)
WHERE RCT_SEQ = @[rct_seq]			]]></sql>
