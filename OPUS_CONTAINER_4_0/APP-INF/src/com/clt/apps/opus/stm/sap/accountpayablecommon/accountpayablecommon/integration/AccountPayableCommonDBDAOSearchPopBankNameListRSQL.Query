<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchPopBankNameListRSQL">
			<desc><![CDATA[SearchPopBankNameList 조회]]></desc>
			<sql><![CDATA[
SELECT BANK_NM
  FROM SAP_BANK_BRNC
 WHERE 1=1
#if (${bank_nm} != '')
   AND BANK_NM LIKE @[bank_nm]||'%'
#end			]]></sql>
			<params>
				<param name="bank_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
