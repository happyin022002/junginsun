<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOModifyBankAccountAdjustmentUSQL">
			<desc><![CDATA[Bank Account의 Balance 정보 업데이트]]></desc>
			<sql><![CDATA[
UPDATE SAP_BANK_BAL_ADJ
   SET CTRL_BGN_BAL_AMT  = @[ctrl_bgn_bal_amt]
      ,CTRL_TTL_RCT_AMT  = @[ctrl_ttl_rct_amt]
      ,CTRL_TTL_PAY_AMT  = @[ctrl_ttl_pay_amt]
      ,CTRL_ENDG_BAL_AMT = @[ctrl_endg_bal_amt]
      ,BANK_STMT_DESC    = @[bank_stmt_desc]
      ,UPD_USR_ID        = @[usr_id]
      ,UPD_DT            = SYSDATE
 WHERE BANK_ACCT_SEQ     = @[bank_acct_seq]
   AND BANK_STMT_DT      = TO_DATE(@[bank_stmt_dt], 'YYYY-MM-DD')			]]></sql>
			<params>
				<param name="ctrl_bgn_bal_amt" type="12" value="" out="N"/>
				<param name="ctrl_ttl_rct_amt" type="12" value="" out="N"/>
				<param name="ctrl_ttl_pay_amt" type="12" value="" out="N"/>
				<param name="ctrl_endg_bal_amt" type="12" value="" out="N"/>
				<param name="bank_stmt_desc" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bank_acct_seq" type="12" value="" out="N"/>
				<param name="bank_stmt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
