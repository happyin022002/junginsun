<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyAccountingDetailInvoiceLineRestoreUSQL">
			<desc><![CDATA[ModifyAccountingDetailInvoiceLineRestore]]></desc>
			<sql><![CDATA[
   UPDATE  SAP_INV_DTL SID
   SET     SID.ACCTG_PST_FLG = 'N'
         , SID.ACCTG_EVNT_SEQ = NULL
         , SID.UPD_DT = SYSDATE
         , SID.UPD_USR_ID = @[usr_id]
   WHERE   SID.ACCTG_EVNT_SEQ = @[acctg_evnt_seq]
   AND     SID.INV_SEQ IN (SELECT SAE.ACCTG_SRC_SEQ FROM SAP_ACCTG_EVNT SAE, SAP_ACCTG_EVNT_HDR SAEH 
                           WHERE  SAE.ACCTG_EVNT_SEQ = SAEH.ACCTG_EVNT_SEQ AND SAE.ACCTG_EVNT_SEQ = @[acctg_evnt_seq] AND SAEH.ACCTG_ERR_CD IS NOT NULL )
			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="acctg_evnt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
