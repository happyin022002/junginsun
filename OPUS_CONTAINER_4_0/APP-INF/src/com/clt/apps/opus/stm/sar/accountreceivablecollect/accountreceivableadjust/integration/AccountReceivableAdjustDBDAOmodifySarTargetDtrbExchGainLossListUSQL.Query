<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOmodifySarTargetDtrbExchGainLossListUSQL">
			<desc><![CDATA[modifySarTargetDtrbExchGainLossList]]></desc>
			<sql><![CDATA[
UPDATE SAR_CLT_DTRB A SET 
	    A.GL_INP_DR_AMT = A.INP_DR_AMT
	  , A.GL_INP_CR_AMT = A.INP_CR_AMT
	  , A.GL_ACCT_DR_AMT = A.ACCT_DR_AMT
	  , A.GL_ACCT_CR_AMT = A.ACCT_CR_AMT
	  , A.GL_CURR_CD = A.CURR_CD
 WHERE 
     DTRB_SRC_SEQ = @[dtrb_src_seq] 	  
     AND DTRB_SRC_TBL_CD = @[dtrb_src_tbl_cd]
     AND DTRB_SRC_TP_CD IN ('EXCH_LOSS','EXCH_GAIN','HDR_RND')			]]></sql>
			<params>
				<param name="dtrb_src_seq" type="12" value="" out="N"/>
				<param name="dtrb_src_tbl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
