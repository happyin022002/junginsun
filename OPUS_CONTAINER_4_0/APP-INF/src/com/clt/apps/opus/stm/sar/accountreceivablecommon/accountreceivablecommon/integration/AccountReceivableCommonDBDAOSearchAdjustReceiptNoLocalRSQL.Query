<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOSearchAdjustReceiptNoLocalRSQL">
			<desc><![CDATA[SearchAdjustReceiptNoLocal]]></desc>
			<sql><![CDATA[
SELECT 
     SUBSTR(TP_OFC_CD,0,3) || AR_TJ_TP_CD || LPAD(TP_SEQ+1,7,'0')   AS ADJ_REC_NO
FROM SAR_TP_NO   
WHERE AR_TJ_TP_CD = @[aj_tj_tp_cd]
AND TP_OFC_CD = @[ofc_cd] 
AND TP_CRE_DT = '99999999'
FOR	UPDATE	OF TP_SEQ WAIT 30			]]></sql>
			<params>
				<param name="aj_tj_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
