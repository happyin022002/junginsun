			<desc><![CDATA[Insert SAR_ASA_DTL]]></desc>
			<sql><![CDATA[
INSERT INTO SAR_ASA_DTL ( 
                  ASA_NO
                , ASA_DTL_SEQ
                , ASA_DTL_DESC
                , ASA_DR_AMT
                , ASA_CR_AMT
                , EFF_DT
                , ASA_DTL_TP_CD
                , CRE_USR_ID
                , CRE_DT
                , UPD_USR_ID
                , UPD_DT
    ) VALUES ( 
                  @[asa_no]
                , @[asa_dtl_seq]
                , @[asa_dtl_desc]
                , @[asa_dr_amt]
                , @[asa_cr_amt]
                , @[eff_dt]
                , @[asa_dtl_tp_cd]
                , @[cre_usr_id]
                , SYSDATE
                , @[upd_usr_id]
                , SYSDATE
    )			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
				<param name="asa_dtl_seq" type="12" value="" out="N"/>
				<param name="asa_dtl_desc" type="12" value="" out="N"/>
				<param name="asa_dr_amt" type="12" value="" out="N"/>
				<param name="asa_cr_amt" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="asa_dtl_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
