<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceCombineCombinationCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceCombineCombinationCheckRSQL]]></desc>
			<sql><![CDATA[
   SELECT  SLCC.CD_CMB_SEQ  AS CD_CMB_SEQ
         , SLCC.SGM_CTNT1   AS COMPNAY_CODE
         , SLCC.SGM_CTNT2   AS REGION_CODE
         , SLCC.SGM_CTNT3   AS CENTER_CODE
         , SLCC.SGM_CTNT4   AS ACCOUNT_CODE
         , SLCC.SGM_CTNT5   AS INTERCOMPANY_CODE
         , SLCC.SGM_CTNT6   AS VVD_CODE
   FROM    SCO_LEGR_CD_CMB SLCC
   WHERE   SLCC.ENBL_FLG = 'Y'
   AND     SLCC.SGM_CTNT1 = @[liab_coa_co_cd]
   AND     SLCC.SGM_CTNT2 = @[liab_coa_rgn_cd]
   AND     SLCC.SGM_CTNT3 = @[liab_coa_ctr_cd]
   AND     SLCC.SGM_CTNT4 = @[liab_coa_acct_no]
   AND     SLCC.SGM_CTNT5 = @[liab_coa_inter_co_cd]
   AND     SLCC.SGM_CTNT6 = @[liab_coa_vvd_cd]			]]></sql>
			<params>
				<param name="liab_coa_co_cd" type="12" value="" out="N"/>
				<param name="liab_coa_rgn_cd" type="12" value="" out="N"/>
				<param name="liab_coa_ctr_cd" type="12" value="" out="N"/>
				<param name="liab_coa_acct_no" type="12" value="" out="N"/>
				<param name="liab_coa_inter_co_cd" type="12" value="" out="N"/>
				<param name="liab_coa_vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
