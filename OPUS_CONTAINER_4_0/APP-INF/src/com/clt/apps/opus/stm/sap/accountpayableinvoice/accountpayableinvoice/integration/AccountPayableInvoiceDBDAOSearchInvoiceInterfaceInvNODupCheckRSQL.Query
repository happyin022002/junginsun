<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceInvNODupCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceInvNODupCheckRSQL]]></desc>
			<sql><![CDATA[
   SELECT SIH.INV_NO AS DUP_INV_NO
   FROM   SAP_INV_HDR SIH
   WHERE  SIH.INV_NO = @[inv_no]
   AND    ROWNUM = 1			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
