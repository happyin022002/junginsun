<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchPopSourceListRSQL">
			<desc><![CDATA[STM_SAP_0211의 검색조건 VO]]></desc>
			<sql><![CDATA[
SELECT SLD.LU_CD,
       SLD.LU_DESC
  FROM SCO_LU_HDR SLH
      ,SCO_LU_DTL SLD
 WHERE SLH.LU_TP_CD = SLD.LU_TP_CD
   AND SLH.LU_TP_CD = 'AP SOURCE'
   AND NVL(SLD.ENBL_FLG, 'Y') = 'Y'
   AND NVL(SLD.LU_ST_DT, SYSDATE) >= SYSDATE

#if (${lu_cd} != '')
   AND UPPER(SLD.LU_CD) like '%' || UPPER(@[lu_cd]) || '%'
#end			]]></sql>
			<params>
				<param name="lu_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
