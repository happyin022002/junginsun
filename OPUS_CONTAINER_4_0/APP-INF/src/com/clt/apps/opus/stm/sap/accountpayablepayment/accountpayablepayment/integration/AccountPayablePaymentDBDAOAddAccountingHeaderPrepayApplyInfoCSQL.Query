<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOAddAccountingHeaderPrepayApplyInfoCSQL">
			<desc><![CDATA[AddAccountingHeaderPrepayApplyInfo]]></desc>
			<sql><![CDATA[
      INSERT INTO SAP_ACCTG_EVNT_HDR
      ( ACCTG_HDR_SEQ
      , ACCTG_EVNT_SEQ
      , ACCTG_EVNT_CATE_CD
      , ACCTG_PRD_NM
      , ACCTG_DT
      , GL_TRNS_CD
      , GL_TRNS_RUN_SEQ
      , ACCTG_DESC
      , ACCTG_RQST_SEQ
      , ACCTG_SRC_SEQ
      , ACCTG_ERR_CD
      , GL_TRNS_ERR_CD
      , GL_RVS_FLG
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , UPD_DT
      )
      SELECT  @[accounting_header_id]  AS ACCTG_HDR_SEQ
            , @[accounting_event_id]   AS ACCTG_EVNT_SEQ
            , 'Purchase Invoices'      AS ACCTG_EVNT_CATE_CD
            , TO_CHAR(SID.ACCTG_DT, 'YYYY/MM') AS ACCTG_PRD_NM
            , SID.ACCTG_DT             AS ACCTG_DT
            , 'N'                      AS GL_TRNS_CD
            , ''                       AS GL_TRNS_RUN_SEQ
            , SIH.INV_DESC             AS ACCTG_DESC
            , @[accounting_request_id] AS ACCTG_RQST_SEQ
            , SIH.INV_SEQ              AS ACCTG_SRC_SEQ
            , ''                       AS ACCTG_ERR_CD
            , ''                       AS GL_TRNS_ERR_CD
            , 'N'                      AS GL_RVS_FLG
            , @[usr_id]                AS CRE_USR_ID
            , SYSDATE                  AS CRE_DT
            , @[usr_id]                AS UPD_USR_ID
            , SYSDATE                  AS UPD_DT
      FROM    SAP_INV_DTL SID
            , SAP_INV_HDR SIH
      WHERE   SID.INV_SEQ = SIH.INV_SEQ
      AND     SID.INV_DTRB_SEQ = @[inv_dtrb_seq]			]]></sql>
			<params>
				<param name="accounting_header_id" type="12" value="" out="N"/>
				<param name="accounting_event_id" type="12" value="" out="N"/>
				<param name="accounting_request_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_dtrb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
