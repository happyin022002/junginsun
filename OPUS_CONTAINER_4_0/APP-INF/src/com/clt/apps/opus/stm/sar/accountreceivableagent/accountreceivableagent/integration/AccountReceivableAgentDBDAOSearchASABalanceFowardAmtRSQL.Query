<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASABalanceFowardAmtRSQL">
			<desc><![CDATA[select Balance Carried Forward]]></desc>
			<sql><![CDATA[
SELECT ROUND(NVL(SUM(ASA_CR_AMT),0) - NVL(SUM(ASA_DR_AMT), 0), 2) AS BAL_AMT
FROM SAR_ASA_DTL
WHERE ASA_NO = @[asa_no]
AND  ASA_DTL_SEQ < 7			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
