<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchBankAccountCheckRSQL">
			<desc><![CDATA[SearchBankAccountCheck]]></desc>
			<sql><![CDATA[
SELECT A.BANK_ACCT_NM,
       A.BANK_ACCT_NO,
       A.BANK_ACCT_SEQ,
       A.CURR_CD,
       A.MLT_CURR_FLG,
       B.BANK_NM
  FROM SAP_BANK_ACCT A,
       SAP_BANK_BRNC B
 WHERE A.BANK_BRNC_SEQ = B.BANK_BRNC_SEQ
#if (${bank_acct_nm} != '')
   AND UPPER(A.BANK_ACCT_NM) LIKE UPPER(@[bank_acct_nm]) || '%'
#end
#if (${bank_acct_no} != '')
   AND UPPER(A.BANK_ACCT_NO) LIKE UPPER(@[bank_acct_no]) || '%'
#end
#if (${ofc_cd} != '')
   AND A.OPN_OFC_CD = @[ofc_cd]
#end			]]></sql>
			<params>
				<param name="bank_acct_nm" type="12" value="" out="N"/>
				<param name="bank_acct_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
