<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingMigrationDBDAOSearchMDMChargeForAccountRSQL">
			<desc><![CDATA[Search MDM Charge for account info]]></desc>
			<sql><![CDATA[
SELECT  CO_CHG_ACCT_CD ACCT_CD
FROM    MDM_CHARGE
WHERE   CHG_CD = @[chg_tp_cd]			]]></sql>
			<params>
				<param name="chg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
