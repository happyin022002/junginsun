<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOAddPaymentLineCSQL">
			<desc><![CDATA[AccountPayablePaymentDBDAOAddPaymentLineCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO SAP_PAY_DTL
(
  INV_PAY_SEQ,
  PAY_SEQ,
  INV_SEQ,
  ACCTG_DT,
  PAY_AMT,
  EFF_YRMON,
  ACCTG_PST_FLG,
  PAY_FUNC_AMT,
  LIAB_CD_CMB_SEQ,	
  XTER_BANK_ACCT_SEQ,
  REMIT_VNDR_NO,
  PAY_SKD_NO,
  CRE_USR_ID,
  CRE_DT,
  UPD_USR_ID,
  UPD_DT
)
VALUES (
  SAP_PAY_DTL_SEQ.NEXTVAL,
  @[pay_seq], 
  @[inv_seq],
  TO_DATE(@[acctg_dt], 'YYYY-MM-DD'),
  REPLACE(@[pay_amt],',',''),
  SUBSTR(REPLACE(@[acctg_dt],'-',''), 1, 6),
  'N',
  REPLACE(@[pay_func_amt],',',''),
  @[liab_cd_cmb_seq],
  @[xter_bank_acct_seq],
  @[remit_vndr_no],
  @[pay_skd_no],
  @[usr_id],
  SYSDATE,
  @[usr_id],
  SYSDATE
)			]]></sql>
			<params>
				<param name="pay_seq" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="acctg_dt" type="12" value="" out="N"/>
				<param name="pay_amt" type="12" value="" out="N"/>
				<param name="pay_func_amt" type="12" value="" out="N"/>
				<param name="liab_cd_cmb_seq" type="12" value="" out="N"/>
				<param name="xter_bank_acct_seq" type="12" value="" out="N"/>
				<param name="remit_vndr_no" type="12" value="" out="N"/>
				<param name="pay_skd_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
