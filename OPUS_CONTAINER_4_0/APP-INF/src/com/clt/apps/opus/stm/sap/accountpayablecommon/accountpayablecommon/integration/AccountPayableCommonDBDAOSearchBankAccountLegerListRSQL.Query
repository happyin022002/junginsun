<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchBankAccountLegerListRSQL">
			<desc><![CDATA[AccountPayableCommonDBDAOSearchBankAccountLegerListRSQL]]></desc>
			<sql><![CDATA[
SELECT
    A.BANK_ACCT_SEQ,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.ASET_CD_CMB_SEQ THEN A.ASET_CD_CMB_SEQ END) AS ASET_CD_CMB_SEQ,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.CHG_CD_CMB_SEQ THEN A.CHG_CD_CMB_SEQ END) AS CHG_CD_CMB_SEQ,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.GAIN_CD_CMB_SEQ THEN A.GAIN_CD_CMB_SEQ END) AS GAIN_CD_CMB_SEQ,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.LSS_CD_CMB_SEQ THEN A.LSS_CD_CMB_SEQ END) AS LSS_CD_CMB_SEQ,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.ASET_CD_CMB_SEQ THEN SGM_CTNT1 END) AS ASET_COA_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.ASET_CD_CMB_SEQ THEN SGM_CTNT2 END) AS ASET_COA_RGN_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.ASET_CD_CMB_SEQ THEN SGM_CTNT3 END) AS ASET_COA_CTR_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.ASET_CD_CMB_SEQ THEN SGM_CTNT4 END) AS ASET_COA_ACCT_NO,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.ASET_CD_CMB_SEQ THEN SGM_CTNT5 END) AS ASET_COA_INTER_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.ASET_CD_CMB_SEQ THEN SGM_CTNT6 END) AS ASET_COA_VVD_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.CHG_CD_CMB_SEQ THEN SGM_CTNT1 END) AS CHG_COA_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.CHG_CD_CMB_SEQ THEN SGM_CTNT2 END) AS CHG_COA_RGN_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.CHG_CD_CMB_SEQ THEN SGM_CTNT3 END) AS CHG_COA_CTR_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.CHG_CD_CMB_SEQ THEN SGM_CTNT4 END) AS CHG_COA_ACCT_NO,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.CHG_CD_CMB_SEQ THEN SGM_CTNT5 END) AS CHG_COA_INTER_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.CHG_CD_CMB_SEQ THEN SGM_CTNT6 END) AS CHG_COA_VVD_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.GAIN_CD_CMB_SEQ THEN SGM_CTNT1 END) AS GAIN_COA_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.GAIN_CD_CMB_SEQ THEN SGM_CTNT2 END) AS GAIN_COA_RGN_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.GAIN_CD_CMB_SEQ THEN SGM_CTNT3 END) AS GAIN_COA_CTR_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.GAIN_CD_CMB_SEQ THEN SGM_CTNT4 END) AS GAIN_COA_ACCT_NO,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.GAIN_CD_CMB_SEQ THEN SGM_CTNT5 END) AS GAIN_COA_INTER_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.GAIN_CD_CMB_SEQ THEN SGM_CTNT6 END) AS GAIN_COA_VVD_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.LSS_CD_CMB_SEQ THEN SGM_CTNT1 END) AS LSS_COA_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.LSS_CD_CMB_SEQ THEN SGM_CTNT2 END) AS LSS_COA_RGN_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.LSS_CD_CMB_SEQ THEN SGM_CTNT3 END) AS LSS_COA_CTR_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.LSS_CD_CMB_SEQ THEN SGM_CTNT4 END) AS LSS_COA_ACCT_NO,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.LSS_CD_CMB_SEQ THEN SGM_CTNT5 END) AS LSS_COA_INTER_CO_CD,
    MAX(CASE WHEN B.CD_CMB_SEQ = A.LSS_CD_CMB_SEQ THEN SGM_CTNT6 END) AS LSS_COA_VVD_CD
FROM SAP_BANK_ACCT A, SCO_LEGR_CD_CMB B
WHERE A.BANK_ACCT_SEQ = @[bank_acct_seq]
    AND B.CD_CMB_SEQ IN (A.ASET_CD_CMB_SEQ, A.CHG_CD_CMB_SEQ, A.GAIN_CD_CMB_SEQ, A.LSS_CD_CMB_SEQ)
GROUP BY A.BANK_ACCT_SEQ			]]></sql>
			<params>
				<param name="bank_acct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
