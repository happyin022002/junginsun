<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOsearchARAcctListRSQL">
			<desc><![CDATA[AR Acount list]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SAM.AR_ACCT_CD AS AR_ACCT_CD, MA.ACCT_ENG_NM AS ACCT_TP_NM
FROM   SAR_ACCT_MTX SAM,
       MDM_ACCOUNT MA
WHERE  SAM.AR_ACCT_CD = MA.ACCT_CD
AND    ACCT_CTNT1 = 'REC'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
