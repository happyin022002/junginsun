<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOmodifyReceivableApplTypeUSQL">
			<desc><![CDATA[Receivable Application 의 상태정보 변경]]></desc>
			<sql><![CDATA[
UPDATE SAR_RCV_APPL SET
	RCV_APPL_TP_CD = @[rcv_appl_tp_cd]
	, DP_FLG = 'N'
	, UPD_USR_ID = @[upd_usr_id]
	, UPD_DT = SYSDATE
WHERE RCV_APPL_SEQ = @[rcv_appl_seq]			]]></sql>
			<params>
				<param name="rcv_appl_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rcv_appl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
