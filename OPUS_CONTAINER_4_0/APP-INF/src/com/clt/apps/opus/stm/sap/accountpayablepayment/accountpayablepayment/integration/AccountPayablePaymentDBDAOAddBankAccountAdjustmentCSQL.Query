<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOAddBankAccountAdjustmentCSQL">
			<desc><![CDATA[Bank Account의 Balance 정보 저장]]></desc>
			<sql><![CDATA[
INSERT INTO SAP_BANK_BAL_ADJ
( BANK_ACCT_SEQ
 ,BANK_STMT_DT
 ,CTRL_BGN_BAL_AMT
 ,CTRL_TTL_RCT_AMT
 ,CTRL_TTL_PAY_AMT
 ,CTRL_ENDG_BAL_AMT
 ,BANK_STMT_DESC
 ,CRE_USR_ID
 ,CRE_DT
 ,UPD_USR_ID
 ,UPD_DT
)
VALUES
( @[bank_acct_seq]
 ,TO_DATE(@[bank_stmt_dt], 'YYYY-MM-DD')
 ,@[ctrl_bgn_bal_amt]
 ,@[ctrl_ttl_rct_amt]
 ,@[ctrl_ttl_pay_amt]
 ,@[ctrl_endg_bal_amt]
 ,@[bank_stmt_desc]
 ,@[usr_id]
 ,SYSDATE
 ,@[usr_id]
 ,SYSDATE
)			]]></sql>
			<params>
				<param name="bank_acct_seq" type="12" value="" out="N"/>
				<param name="bank_stmt_dt" type="12" value="" out="N"/>
				<param name="ctrl_bgn_bal_amt" type="12" value="" out="N"/>
				<param name="ctrl_ttl_rct_amt" type="12" value="" out="N"/>
				<param name="ctrl_ttl_pay_amt" type="12" value="" out="N"/>
				<param name="ctrl_endg_bal_amt" type="12" value="" out="N"/>
				<param name="bank_stmt_desc" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
