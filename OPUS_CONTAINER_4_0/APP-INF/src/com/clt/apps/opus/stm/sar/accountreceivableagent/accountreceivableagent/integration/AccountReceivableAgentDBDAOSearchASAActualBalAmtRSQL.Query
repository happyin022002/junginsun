<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASAActualBalAmtRSQL">
			<desc><![CDATA[select ASA Actual balance amount]]></desc>
			<sql><![CDATA[
SELECT ROUND(NVL(SUM(ASA_CR_AMT),0) - NVL(SUM(ASA_DR_AMT), 0), 2) AS ACTUAL_BAL_AMT
FROM SAR_ASA_DTL
WHERE ASA_NO = @[asa_no]
AND  ASA_DTL_SEQ IN (2 ,3, 4, 5)  			]]></sql>
			<params>
				<param name="asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
