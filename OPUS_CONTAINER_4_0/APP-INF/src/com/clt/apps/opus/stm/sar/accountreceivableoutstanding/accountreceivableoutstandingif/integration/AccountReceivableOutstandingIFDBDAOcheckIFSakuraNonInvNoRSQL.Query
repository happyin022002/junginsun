<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingIFDBDAOcheckIFSakuraNonInvNoRSQL">
			<desc><![CDATA[checkIFSakuraNonInvNo]]></desc>
			<sql><![CDATA[
SELECT count(*) AS NON_INV_NO
	FROM SAR_OTS_HIS SOHI
        ,SCO_OFC_INFO SOI
WHERE 
   SOHI.IF_NO = @[check_if_no]
   AND SOI.OFC_CD = SOHI.INV_OFC_CD
   AND SOI.OTS_CATE_CD = 'INV'
   AND SOHI.INV_NO = '**********'			]]></sql>
			<params>
				<param name="check_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
