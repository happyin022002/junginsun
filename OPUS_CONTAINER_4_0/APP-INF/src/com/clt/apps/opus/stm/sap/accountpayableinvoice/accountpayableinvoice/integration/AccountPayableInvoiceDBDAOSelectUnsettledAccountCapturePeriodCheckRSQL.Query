<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSelectUnsettledAccountCapturePeriodCheckRSQL">
			<desc><![CDATA[SelectUnsettledAccountCapturePeriodCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT  SP.PRD_STS_CD AS VALUE0 
FROM    SCO_PERIOD SP
WHERE   SP.PRD_APPL_CD = 'AP'
AND     SP.EFF_YRMON = REPLACE(@[unstl_yrmon],'-','')			]]></sql>
			<params>
				<param name="unstl_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
