<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchOfcWrtfTpComboListRSQL">
			<desc><![CDATA[OFFICE WRITE-OFF TYPE CODE 조회]]></desc>
			<sql><![CDATA[
SELECT SAM.ACCT_TP_CD
      ,SLD.LU_DESC
FROM   SAR_ACCT_MTX SAM
      ,SCO_LU_HDR SLH
      ,SCO_LU_DTL SLD
WHERE  SLH.LU_TP_CD = SLD.LU_TP_CD
AND    SLD.LU_CD    = SAM.ACCT_TP_CD
AND    TRUNC(SYSDATE) BETWEEN NVL(SLD.LU_ST_DT, TRUNC(SYSDATE)) AND NVL(SLD.LU_END_DT, SYSDATE+1)
AND    SLD.LU_TP_CD = 'WRITEOFF TYPE'
AND    SAM.ACCT_CTNT1 = 'WRTF'
AND    SAM.ACCT_TP_CD <> 'BC'
AND    NVL(SAM.ACCT_CTNT2, 'X') <> 'DFLT'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
