<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOsearchPreASAStatusRSQL">
			<desc><![CDATA[searchPreASAStatus]]></desc>
			<sql><![CDATA[
SELECT ASA_STS_CD AS PREVIOUS_ASA_STATUS
  FROM SAR_ASA_MST
 WHERE ASA_NO = @[pre_asa_no]			]]></sql>
			<params>
				<param name="pre_asa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
