<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOModifyBankAccountSupplierUSQL">
			<desc><![CDATA[Bank Account(Supplier) 정보를 업데이트]]></desc>
			<sql><![CDATA[
UPDATE SAP_BANK_ACCT
SET    BANK_ACCT_NM       = @[bank_acct_nm]
      ,CURR_CD            = @[curr_cd]
      ,BANK_ACCT_VNDR_SEQ = @[bank_acct_vndr_seq]
      ,BANK_ACCT_PRIO_CD  = NVL(@[bank_acct_prio_cd], 'N')
      ,INACT_DT           = TO_DATE(@[inact_dt], 'YYYY-MM-DD')
      ,ATTR_CTNT5         = @[attr_ctnt5]
      ,ATTR_CTNT6         = @[attr_ctnt6]
      ,IBAN_NO            = @[iban_no]
      ,ATTR_CTNT2         = @[attr_ctnt2]
      ,CNTC_AREA_CD       = @[cntc_area_cd]
      ,ATTR_CTNT7         = @[attr_ctnt7]
      ,BANK_BRNC_SEQ      = @[bank_brnc_seq]
      ,UPD_USR_ID         = @[usr_id]
      ,UPD_DT             = SYSDATE 
WHERE BANK_ACCT_SEQ = @[bank_acct_seq]			]]></sql>
			<params>
				<param name="bank_acct_nm" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="bank_acct_vndr_seq" type="12" value="" out="N"/>
				<param name="bank_acct_prio_cd" type="12" value="" out="N"/>
				<param name="inact_dt" type="12" value="" out="N"/>
				<param name="attr_ctnt5" type="12" value="" out="N"/>
				<param name="attr_ctnt6" type="12" value="" out="N"/>
				<param name="iban_no" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="cntc_area_cd" type="12" value="" out="N"/>
				<param name="attr_ctnt7" type="12" value="" out="N"/>
				<param name="bank_brnc_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bank_acct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
