<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceFunctionalAmtCalculateCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceFunctionalAmtCalculateCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT  ROUND(@[inv_amt] * @[inv_xch_rt], MC.DP_PRCS_KNT) AS FUNCTIONAL_AMOUNT
FROM    MDM_CURRENCY MC
WHERE   MC.CURR_CD = @[functional_currency]
   AND     NVL(MC.DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="inv_xch_rt" type="12" value="" out="N"/>
				<param name="functional_currency" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
