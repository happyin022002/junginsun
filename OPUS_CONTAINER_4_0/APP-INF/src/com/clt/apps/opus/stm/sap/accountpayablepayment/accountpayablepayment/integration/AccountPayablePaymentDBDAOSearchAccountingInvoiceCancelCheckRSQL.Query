<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingInvoiceCancelCheckRSQL">
			<desc><![CDATA[searchAccountingInvoiceCancelCheck]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT SIH.INV_SEQ   AS INV_SEQ
,'' USR_ID
,'' accounting_event_id
, '' accounting_header_id
,'' ACCOUNTING_REQUEST_ID
FROM    SAP_INV_HDR SIH
      , SAP_INV_DTL SID
WHERE   SIH.INV_SEQ = SID.INV_SEQ
AND     SID.ACCTG_DT >= TO_DATE(SUBSTR(@[capture_period],0,6),'YYYYMM')
AND     SID.ACCTG_DT < TO_DATE(@[capture_period],'YYYYMMDD') + 1
AND     SID.LINE_TP_LU_CD <> 'PREPAY'
AND     NVL(SID.ACCTG_PST_FLG, 'N') <> 'Y'
AND     NVL(SID.RVS_FLG, 'N') = 'Y'
AND     SID.PRNT_RVS_DTRB_SEQ IS NOT NULL			]]></sql>
			<params>
				<param name="capture_period" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
