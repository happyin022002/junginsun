<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyAPManualInvoiceAccrualCancelFlagUSQL">
			<desc><![CDATA[I/F된 SAP Manual Accrual Flag에 대해서 Cancel 하는 경우 해당 처리 Flag 원복 처리]]></desc>
			<sql><![CDATA[
UPDATE  SAP_INV_HDR
SET     GLO_ATTR_CTNT20 = 'N'
      , UPD_DT      = SYSDATE
      , UPD_USR_ID  = @[usr_id]
WHERE   INV_SEQ     = @[inv_seq]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
