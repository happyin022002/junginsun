<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOcheckBankChargeRSQL">
			<desc><![CDATA[Bank Charge 금액 체크]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM SAR_RCV_APPL
WHERE RCT_SEQ = @[rct_seq]
AND RCV_APPL_TP_CD = 'RECEIPT'
AND RCV_APPL_STS_CD = 'WRTF'
AND WRTF_TP_CD = 'BC'			]]></sql>
			<params>
				<param name="rct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
