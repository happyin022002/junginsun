<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyInvoiceInterfacePrepayApplyPayScheduleInfoUSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOModifyInvoiceInterfacePrepayApplyPayScheduleInfoUSQL]]></desc>
			<sql><![CDATA[
   UPDATE  SAP_PAY_SKD
   SET     PAY_RMN_AMT = PAY_RMN_AMT - @[ppay_aply_amt]
         , PAY_STS_FLG = DECODE(PAY_RMN_AMT - @[ppay_aply_amt], 0, 'Y', PAY_GRS_AMT, 'N', 'P')
         , UPD_USR_ID = @[usr_id]
         , UPD_DT = SYSDATE
   WHERE   INV_SEQ = @[inv_seq]
   AND     PAY_SKD_NO = 1
   AND     PAY_RMN_AMT <> 0			]]></sql>
			<params>
				<param name="ppay_aply_amt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
