<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatementCommonDBDAOSearchMonthlyAccrualAccountListRSQL">
			<desc><![CDATA[Accrual Account Code]]></desc>
			<sql><![CDATA[
SELECT A.ACCT_CD AS CODE
     , A.ACCT_ENG_NM AS NAME
  FROM MDM_ACCOUNT A
 WHERE 1=1
   AND A.JNL_CRE_FLG = 'Y'
   AND NVL(A.DELT_FLG, 'N') = 'N'
 ORDER BY A.ACCT_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
