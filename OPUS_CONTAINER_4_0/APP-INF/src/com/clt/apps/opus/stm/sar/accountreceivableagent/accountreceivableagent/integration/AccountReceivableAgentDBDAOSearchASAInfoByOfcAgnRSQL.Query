<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAgentDBDAOSearchASAInfoByOfcAgnRSQL">
			<desc><![CDATA[search ASA info]]></desc>
			<sql><![CDATA[
SELECT MAX(A.ASA_PRD_TO_DT) AS MAX_ASA_PRD_TO_DT
     , COUNT(A.ASA_NO) AS CNT_ASA_NO
     , MAX(A.ASA_NO) AS MAX_ASA_NO             
     , @[ofc_cd] AS OFC_CD
     , @[agn_cd] AS AGN_CD     
     , @[curr_cd] AS CURR_CD
     , 'N' AS OPEN_ASA_YN
     , '' AS ASA_NO
     , '' AS ASA_PRD_FM_DT
     , '' AS ASA_PRD_TO_DT
     , '' AS USER_ID
  FROM SAR_ASA_MST A
 WHERE 1 = 1
   AND A.OFC_CD = @[ofc_cd] 
   AND A.AGN_CD = @[agn_cd]
   AND A.CURR_CD = @[curr_cd]
   #if(${open_asa_yn} == 'Y')   
   AND A.ASA_STS_CD IN ('O' , 'F')
   #end			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
