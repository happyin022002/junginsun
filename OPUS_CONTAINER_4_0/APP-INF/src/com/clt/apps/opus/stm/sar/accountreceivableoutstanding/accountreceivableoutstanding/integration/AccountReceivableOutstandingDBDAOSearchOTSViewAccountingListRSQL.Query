<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOSearchOTSViewAccountingListRSQL">
			<desc><![CDATA[Search Outstanding vew accounting]]></desc>
			<sql><![CDATA[
SELECT   SOH.BL_NO 
       , SOH.IF_NO 
       , SOH.OTS_OFC_CD
       , SLC.SGM_CTNT1
       , SLC.SGM_CTNT2
       , SLC.SGM_CTNT3
       , SLC.SGM_CTNT4
       , SLC.SGM_CTNT5
       , SLC.SGM_CTNT6
       , SOD.ACCT_CLSS_CD
       , SOD.CURR_CD
       , SOD.INP_DR_AMT
       , SOD.INP_CR_AMT
       , SOD.ACCT_DR_AMT
       , SOD.ACCT_CR_AMT
       , SOD.CONV_XCH_RT
       , SOD.ACCT_XCH_RT_DT
       , SOD.CHG_TP_CD
	   , SOH.GL_DT
FROM   SAR_OTS_HIS SOH 
       , SAR_OTS_DTRB SOD 
       , SCO_LEGR_CD_CMB SLC 
WHERE  SOD.OTS_CD_CMB_SEQ = SLC.CD_CMB_SEQ 
       AND SOH.OTS_HIS_SEQ = SOD.OTS_HIS_SEQ 
       AND SOH.BL_NO = @[bl_no]
	   AND SOH.INV_NO = @[inv_no]
       AND SOH.OTS_OFC_CD= @[ots_ofc_cd] 
ORDER  BY SOH.IF_NO 
     , SOD.CHG_TP_CD 
     , SOD.ACCT_CLSS_CD			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
