<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOsearchAgentOfficeRSQL">
			<desc><![CDATA[대리점리스트 조회]]></desc>
			<sql><![CDATA[
SELECT AGTMO.OFC_CD AGT_OFC_CD
FROM   MDM_ORGANIZATION AGTMO,
       SCO_OFC_INFO  SOI
WHERE  AGTMO.OFC_CD = SOI.OFC_CD
AND    SOI.OFC_BRNC_AGN_TP_CD  = 'AGT'
#if (${ofc_entr_lvl_cd} == 'BA' || ${ofc_entr_lvl_cd} == '') 
    AND AGTMO.OFC_CD = @[ots_ofc_cd]
#elseif (${ofc_entr_lvl_cd} == 'HO' || ${ofc_entr_lvl_cd} == 'HQ')
    AND EXISTS ( SELECT 'X' 
                 FROM   MDM_ORGANIZATION MO 
                 WHERE  MO.OFC_CD = @[ots_ofc_cd]
                 AND    MO.AR_HD_QTR_OFC_CD = AGTMO.AR_HD_QTR_OFC_CD
                )
#elseif (${ofc_entr_lvl_cd} == 'ALL')    
    AND 1=1
#else
    AND 1=2
#end
ORDER BY AGT_OFC_CD ASC			]]></sql>
			<params>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
