<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableCommonDBDAOsearchPaymentLetterTitRSQL">
			<desc><![CDATA[searchPaymentLetterTit]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
     ,AR_PRN_TIT_NM
     ,AR_CR_CUST_PRN_CTNT
     ,AR_PRN_CTNT
     ,REF_EML
  FROM SCO_OFC_INFO
 WHERE OFC_CD = @[ar_ofc_cd]			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
