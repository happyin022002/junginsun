<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOSearchOpenASAListRSQL">
			<desc><![CDATA[Search open ASA list]]></desc>
			<sql><![CDATA[
SELECT SR.ASA_NO, SAM.CURR_CD
FROM SAR_RECEIPT SR,
     SAR_ASA_MST SAM
WHERE SR.RCT_NO = @[rct_no]
AND SR.ASA_NO = SAM.ASA_NO
AND SR.ASA_NO IS NOT NULL
UNION
SELECT ASA_NO, CURR_CD
  FROM SAR_ASA_MST
 WHERE AGN_CD = @[agn_cd]
   AND ASA_STS_CD = 'O'
   AND ASA_FSH_DT IS NULL
ORDER BY ASA_NO			]]></sql>
			<params>
				<param name="rct_no" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
