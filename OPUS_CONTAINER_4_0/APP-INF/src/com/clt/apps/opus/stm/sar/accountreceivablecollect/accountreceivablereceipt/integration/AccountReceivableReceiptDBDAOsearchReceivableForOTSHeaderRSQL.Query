<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOsearchReceivableForOTSHeaderRSQL">
			<desc><![CDATA[OTS Header 반영대상 Receivable 정보 조회]]></desc>
			<sql><![CDATA[
SELECT Q.RHQ_CD
   	   , Q.OTS_OFC_CD
   	   , Q.BL_NO
   	   , Q.INV_NO
   	   , Q.INV_OFC_CD CLT_OFC_CD
   	   , P.UPD_USR_ID
FROM SAR_RCV_APPL P,
     SAR_OTS_HIS Q,
     (SELECT MAX(A.RCV_APPL_SEQ) RCV_APPL_SEQ
      FROM SAR_RCV_APPL A,
           SAR_OTS_HIS B,
		   SAR_RCV_APPL_SEQ_TMP C
      WHERE A.OTS_HIS_SEQ = B.OTS_HIS_SEQ
      AND A.RCV_APPL_SEQ = C.RCV_APPL_SEQ
      GROUP BY B.RHQ_CD
               , B.OTS_OFC_CD
               , B.BL_NO
               , B.INV_NO) R
WHERE P.RCV_APPL_SEQ = R.RCV_APPL_SEQ
AND P.OTS_HIS_SEQ = Q.OTS_HIS_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
