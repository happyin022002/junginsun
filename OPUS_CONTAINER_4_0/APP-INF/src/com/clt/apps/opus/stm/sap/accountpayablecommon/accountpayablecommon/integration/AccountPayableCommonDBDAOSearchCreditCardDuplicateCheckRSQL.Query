<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchCreditCardDuplicateCheckRSQL">
			<desc><![CDATA[STM_SAP_0310 check for a duplicate card number]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT
  FROM SAP_CRD_MST SCM
 WHERE SCM.CRD_NO = REPLACE(@[crd_no], '-', '')			]]></sql>
			<params>
				<param name="crd_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
