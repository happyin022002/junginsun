<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchBankBranchCodeListRSQL">
			<desc><![CDATA[AccountPayableCommonDBDAOSearchBankBranchCodeListRSQL]]></desc>
			<sql><![CDATA[
SELECT    
    A.BANK_BRNC_SEQ
    ,A.BANK_NM
    ,A.BANK_ALTN_NM
    ,A.BANK_NO
    ,A.BANK_BRNC_NM
    ,A.BANK_BRNC_ALTN_NM
    ,A.BRNC_NO
    ,A.BANK_BRNC_TP_NM
    ,A.BANK_END_DT
    ,A.BANK_BRNC_DESC
    ,A.BRNC_CNT_CD
    ,B.CNT_NM AS  BRNC_CNT_NM
    ,A.BANK_ADDR1
    ,A.BANK_ADDR2
    ,A.CNTC_NM
    ,A.CNTC_TIT_NM
    ,A.CNTC_PFX_CD
    ,A.CNTC_PHN_NO
    ,A.CNTC_EML
FROM SAP_BANK_BRNC A, MDM_COUNTRY B
WHERE 1 = 1 
	AND A.BRNC_CNT_CD = B.CNT_CD(+)
	#if     (${bank_brnc_nm} != '')
	AND A.BANK_BRNC_NM LIKE @[bank_brnc_nm]||'%'
	#end
	#if     (${brnc_no} != '')
	AND A.BRNC_NO LIKE @[brnc_no]||'%'  
	#end
	#if     (${brnc_cnt_cd} != '')
	AND A.BRNC_CNT_CD = @[brnc_cnt_cd]
	#end
	#if     (${bank_nm} != '')
	AND A.BANK_NM LIKE @[bank_nm]||'%'
	#end
	#if     (${bank_no} != '')
	AND A.BANK_NO LIKE @[bank_no]||'%'
	#end			]]></sql>
			<params>
				<param name="bank_brnc_nm" type="12" value="" out="N"/>
				<param name="brnc_no" type="12" value="" out="N"/>
				<param name="brnc_cnt_cd" type="12" value="" out="N"/>
				<param name="bank_nm" type="12" value="" out="N"/>
				<param name="bank_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
