<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOsearchFunctionalCurrencyRateRSQL">
			<desc><![CDATA[get Functional Currency Rate]]></desc>
			<sql><![CDATA[
SELECT ROUND(CONV_XCH_RT, 6) AS CONV_XCH_RT
FROM   GL_DLY_XCH_RT
WHERE  FM_CURR_CD = @[bl_curr_cd]
AND    TO_CURR_CD = @[func_curr_cd]
AND    ACCT_XCH_RT_DT = REPLACE(@[adj_dt], '-', '')
AND    ACCT_XCH_RT_LVL = 1
AND    DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="bl_curr_cd" type="12" value="" out="N"/>
				<param name="func_curr_cd" type="12" value="" out="N"/>
				<param name="adj_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
