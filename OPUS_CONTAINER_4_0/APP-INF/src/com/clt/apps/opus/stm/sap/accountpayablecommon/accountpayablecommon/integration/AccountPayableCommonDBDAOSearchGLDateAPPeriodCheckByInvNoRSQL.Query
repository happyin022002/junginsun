<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableCommonDBDAOSearchGLDateAPPeriodCheckByInvNoRSQL">
			<desc><![CDATA[SearchGLDateAPPeriodCheckByInvNo]]></desc>
			<sql><![CDATA[
SELECT  SP.PRD_STS_CD AS CHK_PRD
FROM    SCO_PERIOD SP
WHERE   SP.EFF_YRMON = ( SELECT SUBSTR(REPLACE(H.GL_DT,'-'),0,6) FROM SAP_INV_HDR H WHERE INV_NO = @[inv_no] AND ROWNUM=1 )  
AND     SP.PRD_APPL_CD = 'AP'			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
