<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchAccountingPrepayUnApplyCheckCsrNoRSQL">
			<desc><![CDATA[SearchAccountingPrepayUnApplyCheckCsrNo]]></desc>
			<sql><![CDATA[
SELECT  SID.INV_DTRB_SEQ   AS INV_DTRB_SEQ
FROM    SAP_INV_DTL SID
       ,SAP_INV_HDR SIH
WHERE   SIH.INV_NO = @[csr_no]
AND     NVL(SID.ACCTG_PST_FLG, 'N') <> 'Y'
AND     SID.LINE_TP_LU_CD = 'PREPAY'
AND     NVL(SID.RVS_FLG, 'N') = 'Y'
AND     SID.PRNT_RVS_DTRB_SEQ IS NOT NULL			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
