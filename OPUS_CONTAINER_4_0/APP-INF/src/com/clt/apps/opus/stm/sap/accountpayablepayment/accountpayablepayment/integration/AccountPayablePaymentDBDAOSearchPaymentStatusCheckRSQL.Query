<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayablePaymentDBDAOSearchPaymentStatusCheckRSQL">
			<desc><![CDATA[AccountPayablePaymentDBDAOSearchPaymentStatusCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT
    CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END AS ACCTG_YN
FROM SAP_PAY_HDR SPH, SAP_PAY_DTL SPD
WHERE SPH.PAY_SEQ = SPD.PAY_SEQ
AND SPD.ACCTG_EVNT_SEQ IS NOT NULL
AND SPH.PAY_SEQ = @[pay_seq]
			]]></sql>
			<params>
				<param name="pay_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
