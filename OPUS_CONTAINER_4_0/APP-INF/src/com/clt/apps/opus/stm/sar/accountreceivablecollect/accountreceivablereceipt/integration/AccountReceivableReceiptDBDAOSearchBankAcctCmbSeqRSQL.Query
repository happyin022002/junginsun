<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOSearchBankAcctCmbSeqRSQL">
			<desc><![CDATA[Search bank account combination sequence]]></desc>
			<sql><![CDATA[
SELECT BANK_ACCT_NO
	   , ASET_CD_CMB_SEQ
	   , CHG_CD_CMB_SEQ
FROM SAP_BANK_ACCT
WHERE BANK_ACCT_SEQ = @[bank_acct_seq]			]]></sql>
			<params>
				<param name="bank_acct_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
