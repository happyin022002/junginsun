<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOSearchInvoiceInterfaceSOClearingAcctCheckRSQL">
			<desc><![CDATA[AccountPayableInvoiceDBDAOSearchInvoiceInterfaceSOClearingAcctCheckRSQL]]></desc>
			<sql><![CDATA[
   SELECT  COUNT(AID.DTRB_COA_ACCT_CD)  AS CLEARING_COUNT
   FROM    AP_INV_DTRB AID
   WHERE   AID.CSR_NO = @[csr_no] 
   AND     AID.DTRB_COA_ACCT_CD IN ('954111', '954113')			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
