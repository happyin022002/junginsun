<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountPayableInvoiceDBDAOModifyApplyedPrepaymentPayScheduleInfoUSQL">
			<desc><![CDATA[modifyApplyedPrepaymentPayScheduleInfo]]></desc>
			<sql><![CDATA[
UPDATE  SAP_PAY_SKD
SET     PAY_RMN_AMT = PAY_RMN_AMT - @[apply_amount]
      , PAY_STS_FLG = DECODE(PAY_RMN_AMT - @[apply_amount], 0, 'Y', PAY_GRS_AMT, 'N', 'P')
      , UPD_USR_ID = @[usr_id]
      , UPD_DT = SYSDATE
WHERE   INV_SEQ = @[standard_inv_seq]
AND     PAY_SKD_NO = @[standard_line_no]
AND     PAY_RMN_AMT <> 0			]]></sql>
			<params>
				<param name="apply_amount" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="standard_inv_seq" type="12" value="" out="N"/>
				<param name="standard_line_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
