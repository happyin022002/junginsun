<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableAdjustDBDAOmodifyAdjustInfoRvsUSQL">
			<desc><![CDATA[SAR_ADJ_HDR  테이블에 update]]></desc>
			<sql><![CDATA[
UPDATE SAR_ADJ_HDR
SET RVS_FLG = 'Y'    
    ,UPD_USR_ID = @[user_id]
    ,UPD_DT = SYSDATE
    ,ADJ_RMK = @[adj_rmk]
WHERE 1=1
AND ADJ_NO = @[adj_no]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="adj_rmk" type="12" value="" out="N"/>
				<param name="adj_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
