<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableOutstandingDBDAOSearchOTSHeaderInfoRSQL">
			<desc><![CDATA[Search OTS Header Info]]></desc>
			<sql><![CDATA[
SELECT RHQ_CD
       , OTS_OFC_CD
       , BL_NO
       , INV_NO
       , OFC_CURR_CD
       , OTS_SRC_CD
       , BIL_TO_CUST_CNT_CD
       , BIL_TO_CUST_SEQ
       , BKG_NO
       , VSL_CD
       , SKD_VOY_NO
       , DIR_CD
       , TRNK_VVD_CD
       , SVC_SCP_CD
       , LANE_CD
       , SAIL_ARR_DT
       , BKG_IO_BND_CD
       , POR_CD
       , POL_CD
       , POD_CD
       , DEL_CD
       , CUST_SREP_CD
       , DUE_DT
       , BKG_REF_NO
       , AP_AR_OFFST_NO
       , CR_MK_FLG
       , XCH_RT_TP_CD
       , INV_DT
       , CLT_OFC_CD
       , SC_NO
       , REV_TP_SRC_CD
	   , SHP_TO_CUST_CNT_CD
	   , SHP_TO_CUST_SEQ
       , XCH_RT_N3RD_TP_CD
 	   , XCH_RT_DT
	   , MAX_AR_IF_NO
	   , SAIL_DT
	   , CRE_USR_ID
	   , (SELECT UPD_USR_ID FROM INV_AR_MN WHERE AR_IF_NO = @[if_no]) UPD_USR_ID
	   , INV_CURR_CD
	   , (SELECT INV_OFC_CD 
   		  FROM SAR_OTS_HIS 
   		  WHERE IF_NO = MAX_AR_IF_NO||'1' 
   		  AND OTS_HIS_TP_CD = 'OTS') AR_OFC_CD
FROM SAR_OTS_HDR
WHERE RHQ_CD = @[rhq_cd]
AND OTS_OFC_CD = @[ots_ofc_cd]
AND BL_NO = @[bl_no]
AND INV_NO = @[inv_no]			]]></sql>
			<params>
				<param name="if_no" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ots_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
