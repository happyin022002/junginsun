(
 TO_DATE( REPLACE( @[acctg_dt], '-', ''), 'YYYYMMDD')
,@[accl_bk_pst_flg]
,NVL( @[aset_add_flg], 'N')
,NVL( @[aset_trak_flg], 'N')
,@[csh_bk_pst_flg]
