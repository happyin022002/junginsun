,@[attr_ctnt6]
,@[attr_ctnt7]
,@[attr_ctnt8]
,@[attr_ctnt9]
,@[attr_ctnt10]
,REPLACE( @[attr_ctnt11], '-', '')
,@[attr_ctnt12]
,@[attr_ctnt13]
,@[attr_ctnt14]
,@[attr_ctnt15]
,NVL ( @[attr_cate_nm], @[dtrb_coa_acct_no] ) 
,ROUND( TO_NUMBER( REPLACE( @[ppay_rmn_amt], ',', '') ) , 3 ) 
,@[glo_attr_cate_nm]
,@[glo_attr_ctnt1]
,@[glo_attr_ctnt2]
,@[glo_attr_ctnt3]
,@[glo_attr_ctnt4]
,@[glo_attr_ctnt5]
,@[glo_attr_ctnt6]
