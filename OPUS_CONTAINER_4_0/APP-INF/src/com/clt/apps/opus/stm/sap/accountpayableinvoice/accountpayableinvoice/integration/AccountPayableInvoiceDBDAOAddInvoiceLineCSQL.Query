,@[dtrb_coa_vvd_cd]
,@[dtrb_coa_inter_co_cd]
,@[inv_seq]
,@[line_tp_lu_cd]
,NVL ( @[eff_yrmon], SUBSTR(REPLACE( @[acctg_dt], '-', ''), 0, 6) ) 
,ROUND( TO_NUMBER( REPLACE( @[dtrb_amt], ',', '') ) , 3 ) 
,ROUND( TO_NUMBER( REPLACE( @[dtrb_func_amt], ',', '') ) , 3 ) 
,ROUND( TO_NUMBER( REPLACE( @[inv_rnd_amt], ',', '') ) , 3 ) 
,@[bat_seq]
,@[dtrb_desc]
,NVL (@[mtch_sts_flg], 'A')
,NVL (@[acctg_pst_flg], 'N' ) 
,@[rvs_flg]
,TO_DATE(REPLACE( @[dtrb_xch_dt], '-', ''),'YYYYMMDD')
,@[dtrb_xch_rt]
,@[dtrb_xch_rt_tp_cd]
,REPLACE(@[attr_ctnt1], chr(9), '')
,REPLACE( @[attr_ctnt2], '-', '')
