,SYSDATE
,TO_NUMBER( @[dtrb_cd_cmb_seq] )
,REPLACE( @[dtrb_func_gain_amt], ',', '')
,REPLACE( @[dtrb_func_lss_amt], ',', '')
)			]]></sql>
