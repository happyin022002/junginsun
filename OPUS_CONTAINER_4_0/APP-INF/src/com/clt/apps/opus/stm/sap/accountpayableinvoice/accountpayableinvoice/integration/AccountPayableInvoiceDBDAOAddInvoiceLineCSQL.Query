,@[glo_attr_ctnt20]
,NVL( @[dtrb_mtch_tp_nm],'NOT_MATCHED' ) 
,SAP_INV_DTL_SEQ.NEXTVAL
,@[prnt_rvs_dtrb_seq]
,@[acctg_evnt_seq]
,@[ppay_dtrb_seq]
,NVL ( @[dtrb_clss_nm],'PERMANENT' ) 
,@[dtrb_vat_cd]
,@[old_dtrb_seq]
,@[usr_id]
