<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableReceiptDBDAOSearchOTSTempTotalAmountRSQL">
			<desc><![CDATA[SearchOTSTempTotalAmount]]></desc>
			<sql><![CDATA[
SELECT SUM(RCT_APLY_AMT) TTL_APLY_AMT
FROM SAR_OTS_RCT_TMP
WHERE OTS_RCT_TMP_SEQ = @[ots_rct_tmp_seq]			]]></sql>
			<params>
				<param name="ots_rct_tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
