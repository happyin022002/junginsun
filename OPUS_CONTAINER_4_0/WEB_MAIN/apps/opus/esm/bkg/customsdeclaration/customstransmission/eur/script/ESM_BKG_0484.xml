<?xml version='1.0' ?>
<Excel>  
 <!-- IBSheetSet 요소 -->
<IBSheetSet>
     <StartRow>1</StartRow>
     <ViewCols></ViewCols>
</IBSheetSet>  
 
<!-- PageSet 요소 -->
<PageSet>
     <PaperSize>A4</PaperSize>
     <Orientation>Landscape</Orientation>
     <Margins Top="2" Header="1.3" Left="0.3" Right="0.3" Bottom="1" Footer="1.3"/>
     <Scaling>
         <Adjust Value="100">False</Adjust>
         <Fit Height="0" Width="1">True</Fit>
     </Scaling>
     <CenterOnPage Horizontal="True" Vertical="False"/>
     <RowsRepeat Row1="1" />
     <DefaultRowHeight>15</DefaultRowHeight>
     <DefaultFont Name="굴림" Bold="False" Italic="False" size="8" UnderLine="None" 
                 Strikethrough="False" Superscript="False" Subscript="False" Color="RGB(0,0,0)" />
</PageSet>  
 
<!-- ColumnWidth 요소 -->
<ColumnWidth Col1="1" Col2="End">-1</ColumnWidth>
<ColumnWidth Col1="20">50</ColumnWidth>  
<ColumnWidth Col1="23">50</ColumnWidth>  
<ColumnWidth Col1="26">50</ColumnWidth>  

<RowHeight Row1="1">50</RowHeight> 
</Excel> 
