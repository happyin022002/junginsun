<?xml version="1.0" ?>
<Excel>  
 <!-- IBSheetSet 요소 -->
<IBSheetSet>
     <StartRow>1</StartRow>
</IBSheetSet>  
  
<!-- PageSet 요소   -->
<PageSet>
     <DefaultFont Name="맑은 고딕" size="10" />
</PageSet>

<!-- ColumnWidth 요소 -->
<ColumnWidth Col1="1">4</ColumnWidth>
<ColumnWidth Col1="2" Col2="End">37.38</ColumnWidth>  

<BorderSet>
	<Range Row1="1" Col1="1" Col2="End">
		<CellFormat>
      		<Alignment Horizontal="Center" Vertical="Center"/>
			<Font Bold="True"/>
    	</CellFormat>
  	</Range>
	<Range Row1="2" Col1="1" Row2="End">
		<CellFormat>
      		<Alignment Horizontal="Center" Vertical="Center"/>
    	</CellFormat>
  	</Range>
  	<Range Row1="2" Col1="2" Row2="End" Col2="3">
   		<CellFormat>
     		<Format WrapText="true" NumberFormatLocal="G/표준"/>
   		</CellFormat>
  	</Range>
</BorderSet>
</Excel> 

