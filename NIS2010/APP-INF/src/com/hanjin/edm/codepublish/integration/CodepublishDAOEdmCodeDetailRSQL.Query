<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodepublishDAOEdmCodeDetailRSQL">
			<desc><![CDATA[select edm code value]]></desc>
			<sql><![CDATA[
SELECT   a.codeid,
b.codeid AS KEY,
REPLACE(b.NAME,'&','&'||'amp;') AS VALUE,
b.codeorder
FROM t_codedomain a, t_codevalue b
WHERE a.codeid = @[codeid] AND a.ID = b.ID
ORDER BY a.codeid, b.codeorder			]]></sql>
			<params>
				<param name="codeid" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
