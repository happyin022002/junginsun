<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodepublishDAOComIntgCdRSQL">
			<desc><![CDATA[select com_ingt_cd]]></desc>
			<sql><![CDATA[
SELECT
A.OWNR_SUB_SYS_CD,
A.INTG_CD_ID,
REPLACE(A.INTG_CD_NM,'&','&'||'amp;') INTG_CD_NM,
A.INTG_CD_LEN,
NVL(A.INTG_CD_USE_FLG,'Y') INTG_CD_USE_FLG
FROM COM_INTG_CD A
WHERE A.INTG_CD_ID IN (
#foreach($key IN ${codes})
#if($velocityCount < $codes.size())
'$key',
#else
'$key'
#end
#end
)
ORDER BY A.INTG_CD_ID			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
