<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodepublishDAOEdmCodeByCodeRSQL">
			<desc><![CDATA[select edm code list]]></desc>
			<sql><![CDATA[
SELECT a.codeid,
a.NAME,
a.definition,
a.var2,
a.datatype,
a.precision,
a.var1,
a.var3,
a.var5
FROM t_codedomain a
WHERE A.CODEID IN (
#foreach($key IN ${codes})
#if($velocityCount < $codes.size())
'$key',
#else
'$key'
#end
#end 
)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
