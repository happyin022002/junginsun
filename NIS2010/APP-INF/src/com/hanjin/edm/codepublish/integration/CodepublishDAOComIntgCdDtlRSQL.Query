<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodepublishDAOComIntgCdDtlRSQL">
			<desc><![CDATA[select from  com_intg_cd]]></desc>
			<sql><![CDATA[
SELECT B.INTG_CD_ID,
B.INTG_CD_VAL_CTNT,
REPLACE(B.INTG_CD_VAL_DP_DESC,'&','&'||'amp;') INTG_CD_VAL_DP_DESC,
B.INTG_CD_VAL_DP_SEQ,
B.APLY_ST_DT,
B.APLY_END_DT
FROM COM_INTG_CD_DTL B
WHERE B.INTG_CD_ID=@[intg_cd_id]
ORDER BY B.INTG_CD_ID, B.INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="intg_cd_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
