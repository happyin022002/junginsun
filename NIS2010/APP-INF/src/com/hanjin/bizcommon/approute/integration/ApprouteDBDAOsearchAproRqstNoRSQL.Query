<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprouteDBDAOsearchAproRqstNoRSQL">
			<desc><![CDATA[getApro_rqst_no 조회]]></desc>
			<sql><![CDATA[
SELECT CSR.APRO_RQST_NO 
FROM COM_APRO_CSR_DTL CSR 
,COM_APRO_RQST_HDR HDR 
,COM_APRO_RQST_ROUT ROUT 
WHERE HDR.APRO_RQST_NO = CSR.APRO_RQST_NO 
AND HDR.APRO_RQST_NO = ROUT.APRO_RQST_NO 
AND CSR.CSR_NO = @[csr_no]
AND (ROUT.APSTS_CD <> 'D' OR ROUT.APSTS_CD IS NULL			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
