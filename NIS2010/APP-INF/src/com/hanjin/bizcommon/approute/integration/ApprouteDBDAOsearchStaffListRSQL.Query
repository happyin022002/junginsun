<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprouteDBDAOsearchStaffListRSQL">
			<desc><![CDATA[ApprouteDBDAOsearchStaffList]]></desc>
			<sql><![CDATA[
SELECT   	
USR_NM,              
OFC_CD,          	 
PSN_ENG_NM,          	 
USR_ID          	 
FROM COM_USER    									
WHERE 1 = 1 
AND NVL(USE_FLG, 'Y') <> 'N'				
AND OFC_CD LIKE @[ofc_cd_deptsrch] || '%'
ORDER BY PSN_ENG_NM, USR_NM			]]></sql>
			<params>
				<param name="ofc_cd_deptsrch" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
