<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StateDBDAOTotalStateRSQL">
			<desc><![CDATA[totalState]]></desc>
			<sql><![CDATA[
SELECT COUNT(*)
FROM MDM_STATE
WHERE 1 = 1 
AND nvl(DELT_FLG,'N') <> 'Y'

#if (${cnt_cd} != '') 
AND cnt_cd = @[cnt_cd]
#end			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
