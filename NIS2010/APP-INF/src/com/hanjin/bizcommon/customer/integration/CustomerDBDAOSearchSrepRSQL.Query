<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerDBDAOSearchSrepRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT * FROM (
SELECT DISTINCT SREP_CD,
OFC_CD,
NVL(SREP_NM,'') SREP_NM,
SEX_CD, 
SREP_EML, 
MPHN_NO, 
ROWNUM NO
FROM MDM_SLS_REP
WHERE 1=1
#if(${ofc_cd} != '')
		AND OFC_CD LIKE @[ofc_cd] || '%'
#end

#if(${srep_cd} != '')
		AND SREP_CD LIKE @[srep_cd] || '%'
#end

#if(${srep_nm} != '')
		AND SREP_NM LIKE @[srep_nm] || '%'
#end

#if(${glo_usr_id} != '')
		AND glo_usr_id LIKE @[glo_usr_id] || '%'
#end


AND SREP_STS_CD  = 'N'

)
WHERE
NO BETWEEN @[startpart] AND @[endpart]
ORDER BY OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="srep_nm" type="12" value="" out="N"/>
				<param name="glo_usr_id" type="12" value="" out="N"/>
				<param name="startpart" type="12" value="" out="N"/>
				<param name="endpart" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
