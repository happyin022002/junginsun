<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOTotalSurchargeRSQL">
			<desc><![CDATA[TotalSurcharge]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) 
FROM MDM_COUNTRY A, MDM_SUBCONTINENT B, MDM_CONTINENT C  
WHERE 1 = 1 
  AND A.SCONTI_CD = B.SCONTI_CD 
  AND B.CONTI_CD=C.CONTI_CD 
  AND NVL(A.DELT_FLG,'N') <> 'Y' 
  AND NVL(B.DELT_FLG,'N') <> 'Y' 
  AND NVL(C.DELT_FLG,'N') <> 'Y' 

#if (${conti_cd} != '') 
AND B.conti_cd like '%' || @[conti_cd] || '%' 
#end

#if (${sconti_cd} != '') 
AND A.sconti_cd like '%' || @[sconti_cd] || '%' 
#end

#if (${cnt_cd} != '') 
AND A.cnt_cd like '%' || @[cnt_cd] || '%' 
#end

#if (${cnt_nm} != '') 
AND A.cnt_nm like '%' || @[cnt_nm] || '%' 
#end			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="A" out="N"/>
				<param name="sconti_cd" type="12" value="E" out="N"/>
				<param name="cnt_cd" type="12" value="T" out="N"/>
				<param name="cnt_nm" type="12" value="AN" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
