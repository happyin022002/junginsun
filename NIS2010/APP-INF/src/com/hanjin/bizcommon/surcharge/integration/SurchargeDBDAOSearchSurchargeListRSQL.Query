<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOSearchSurchargeListRSQL">
			<desc><![CDATA[SearchSurchargeList]]></desc>
			<sql><![CDATA[
SELECT no,
       cnt_cd,
       cnt_nm,
       conti_cd,
       conti_nm,
       sconti_cd,
       sconti_nm
 FROM (SELECT ROW_NUMBER() OVER (ORDER BY A.cnt_cd ASC) no,
              A.cnt_cd,
              A.cnt_nm,
              C.conti_cd,
              C.conti_nm,
              B.sconti_cd,
              B.sconti_nm
         FROM mdm_country A, mdm_subcontinent B, mdm_continent C
        WHERE 1 = 1 
          AND A.sconti_cd = B.sconti_cd 
          AND B.conti_cd = C.conti_cd
  		  AND nvl(A.delt_flg,'N') <> 'Y'
  		  AND nvl(B.delt_flg,'N') <> 'Y'
  		  AND nvl(C.delt_flg,'N') <> 'Y'

          #if (${conti_cd} != '') 
          AND B.conti_cd like '%' || @[conti_cd] || '%' 
          #end

          #if (${sconti_cd} != '') 
          AND A.sconti_cd like '%' || @[sconti_cd] || '%' 
          #end

          #if (${cnt_cd} != '') 
          AND A.cnt_cd like '%' || @[cnt_cd] || '%' 
          #end

          #if (${cnt_nm} != '') 
          AND A.cnt_nm like '%' || @[cnt_nm] || '%' 
          #end
		           
        ) a
 WHERE no BETWEEN @[startPart] AND @[endPart]			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="sconti_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cnt_nm" type="12" value="" out="N"/>
				<param name="startPart" type="12" value="1" out="N"/>
				<param name="endPart" type="12" value="10" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
