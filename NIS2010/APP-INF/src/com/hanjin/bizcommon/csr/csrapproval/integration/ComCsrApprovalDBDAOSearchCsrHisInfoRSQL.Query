<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ComCsrApprovalDBDAOSearchCsrHisInfoRSQL">
			<desc><![CDATA[COM_AP_CSR_HIS의 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CSR_NO
    ,CSR_APRO_TP_CD
    ,AUTO_MNL_FLG
    ,INV_STS_CD
    ,IO_BND_CD
    ,IF_DT
    ,GW_CSR_RQST_ID
    ,GW_APRO_URL_CTNT
    ,GW_APRO_RSLT_CD
    ,CORR_HIS_RMK
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
    ,APRO_USR_ID
    ,APRO_USR_NM
    ,APRO_USR_JB_TIT_NM
    ,APRO_RMK
FROM COM_AP_CSR_HIS
WHERE 1=1
AND CSR_NO = @[csr_no]
AND GW_APRO_RSLT_CD = 'Y'			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
