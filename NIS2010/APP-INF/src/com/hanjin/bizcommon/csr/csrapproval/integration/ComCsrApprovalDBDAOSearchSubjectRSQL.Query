<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ComCsrApprovalDBDAOSearchSubjectRSQL">
			<desc><![CDATA[subject 조회]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_DP_DESC
FROM COM_INTG_CD_DTL
WHERE 1=1
AND INTG_CD_ID IN ('CD03302')
AND INTG_CD_VAL_CTNT = DECODE(@[inv_sub_sys_cd],'TLL','MNR',@[inv_sub_sys_cd])
AND ROWNUM = 1 			]]></sql>
			<params>
				<param name="inv_sub_sys_cd" type="12" value="TES" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
