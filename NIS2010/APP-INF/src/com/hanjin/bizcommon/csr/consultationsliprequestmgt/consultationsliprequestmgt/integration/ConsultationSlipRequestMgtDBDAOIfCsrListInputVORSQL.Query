<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConsultationSlipRequestMgtDBDAOIfCsrListInputVORSQL">
			<desc><![CDATA[2010.08.31 김영철 [CHM-201005571-01] [VOP-PSO] 공통 CSR내 Invoice 조건 칼럼 추가요청건(INV_NO로 조회부분 수정)
2014.11.19 김영신 [CHM-201432872] 10만불 이상 G/W연동 관련 쿼리 수정]]></desc>
			<sql><![CDATA[
SELECT   B.IF_DT
       , B.CSR_NO
       , (SELECT DECODE(VNDR_CNT_CD,'KR',VNDR_LOCL_LANG_NM,VNDR_LGL_ENG_NM)
         FROM    MDM_VENDOR
         WHERE   VNDR_SEQ = B.VNDR_NO
         ) INV_DESC
       , B.CSR_CURR_CD
       , B.CSR_AMT
       , B.DUE_DT
       , B.IF_STATUS
       , B.ATTR_CTNT2
       , B.IF_ERR_RSN
       , TO_CHAR(B.VNDR_NO, '000000') VNDR_NO
       , B.VNDR_TERM_NM
       , B.AFT_ACT_FLG
       , TO_CHAR(MAX(B.INV_ISS_DT),'YYYY-MM-DD') INV_ISS_DT
       , TO_CHAR(MAX(B.INV_RCV_DT),'YYYY-MM-DD') INV_RCV_DT
       , COUNT(B.CSR_NO) NO_OF_INV
       , B.IF_FLG
       , B.RCV_ERR_FLG
       , B.APRO_RQST_NO
       , B.PAY_GRP_LU_CD
       , B.COST_OFC_CD 
       ,'' fm_eff_dt
       ,'' to_eff_dt
	   ,'' search_csr_no
	   ,'' dt_status 
	   ,'' ofc_cd
       ,'' upd_usr_id 
	   ,'' inv_sts_cd
	   ,'' inv_sub_sys_cd
	   ,'' if_no
       , B.ACCT_XCH_RT_YRMON
	   , B.CSR_USD_AMT
	   , NVL(B.CSR_APRO_TP_CD,'AL') CSR_APRO_TP_CD
       , B.CRE_USR_ID
       , ( CASE WHEN B.GW_AGMT_DOC_CFM_CD IS NOT NULL
           		THEN ( CASE WHEN B.GW_AGMT_DOC_CFM_CD = 'P' THEN 'Y'
                       WHEN B.GW_AGMT_DOC_CFM_CD = 'Y' THEN 'Y'
                       ELSE 'N'
                  END )
         	ELSE NVL(B.AGMT_DOC_CFM_CD,'N')
    		END ) AGMT_DOC_CFM_CD 
	   , NVL(B.AGMT_FILE_CFM_CD,'N') AGMT_FILE_CFM_CD   
       , (CASE WHEN
        NVL(( SELECT COUNT(F.ATCH_FILE_ID)
        		FROM COM_AP_FILE_UPLD F
        		WHERE 1=1
        		AND F.AP_FILE_DIV_CD = 'C'
        		AND F.CSR_NO = B.CSR_NO 
        		AND F.CSR_FILE_UPLD_TP_CD = 'FU'				
				AND NVL(F.DELT_FLG,'N') <> 'Y'
        	),0) > 0 THEN 'Y'    
    	  ELSE 'N'
          END ) FILE_UPLD_FLG
       , AP_COM_CHK_ALPS2GW_FNC(B.CSR_NO,@[ofc_cd]) CHK_GW_2_AL
FROM
         ( SELECT TO_CHAR(A.IF_DT,'YYYY-MM-DD') IF_DT
               , A.CSR_NO
               , H.COST_OFC_CD
               , A.CSR_CURR_CD
			   ,DECODE(H.INV_CURR_CD, 'KRW', ROUND(NVL(A.CSR_AMT,0),0)
			      				  , 'JPY', ROUND(NVL(A.CSR_AMT,0),0)
								  , ROUND(NVL(A.CSR_AMT,0),2)) CSR_AMT
               , SUBSTR(A.INV_TERM_DT, 1, 4) ||'/' ||SUBSTR(A.INV_TERM_DT, 5, 2) ||'/' ||SUBSTR(A.INV_TERM_DT, 7, 2) DUE_DT
               , CASE

                         WHEN H.INV_STS_CD    = 'J' OR H.INV_STS_CD = 'G'
                         THEN 'A/P Rejected'
                         
                         WHEN H.INV_STS_CD = 'E'
                         THEN 'I/F Error'
                         
						 WHEN H.INV_STS_CD = 'X' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'N'
						 THEN 'I/F Error Cancelled'

						 WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'N' 
                         THEN 'Approval Request Cancelled'
                         
                         WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL  AND 1 = (SELECT count(*) FROM AP_PAY_INV K WHERE K.CSR_NO  = H.CSR_NO AND K.INV_STS_CD = 'G' AND ROWNUM = 1)
                              
                         THEN 'A/P Rejected'
                         
                         
                         WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL  AND 1 = (SELECT count(*) FROM AP_PAY_INV K WHERE K.CSR_NO  = H.CSR_NO AND K.INV_STS_CD = 'B' AND ROWNUM = 1)
                              
                         THEN 'Disapproved'

						 WHEN H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N' AND A.RQST_APRO_STEP_FLG = 'Y'
                         THEN 'Requesting Approval'
                         
                         WHEN H.INV_STS_CD = 'P'
                             AND A.RCV_ERR_FLG IS NULL
                         THEN 'I/F Success'
                         
                         WHEN H.INV_STS_CD    = 'R' OR H.INV_STS_CD = 'B'
                         THEN 'Disapproved'
                         
                         WHEN H.INV_STS_CD = 'A'
                             AND A.IF_FLG IS NULL
                         THEN DECODE(A.APRO_FLG
                                   ,'Y','Sending'
                                   ,'Approval Requested')
                         
                         WHEN H.INV_STS_CD = 'D'
                         THEN 'Paid'
                         
                         WHEN H.INV_STS_CD = 'X' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'X'
                         THEN 'Approval Request Cancelled'
						 

                         ELSE H.INV_STS_CD
                 END IF_STATUS
               , A.ATTR_CTNT2
               , A.IF_ERR_RSN
               , A.VNDR_NO
               , A.VNDR_TERM_NM
               , A.AFT_ACT_FLG
               , H.INV_ISS_DT
               , C.APRO_RQST_NO
               , H.INV_RCV_DT
               , A.IF_FLG
               , A.RCV_ERR_FLG
               , H.INV_STS_CD 
               , A.PAY_GRP_LU_CD 
               , A.ACCT_XCH_RT_YRMON
			   , A.CSR_USD_AMT
			   , A.CSR_APRO_TP_CD
			   , A.CRE_USR_ID      
			   , A.AGMT_DOC_CFM_CD
			   , A.GW_AGMT_DOC_CFM_CD
			   , A.AGMT_FILE_CFM_CD         
         FROM    AP_PAY_INV H
               , COM_APRO_CSR_DTL C
#if (${dt_status} == 'IU') 
#else
               , COM_APRO_RQST_HDR R
#end
               , AP_INV_HDR A
         WHERE   1                    =1
#if (${dt_status} == 'IU' || ${dt_status} == 'AV') 
             AND H.CSR_NO             = C.CSR_NO
#else
			 AND H.CSR_NO             = C.CSR_NO
#end

#if (${dt_status} == 'IU')
#elseif (${dt_status} == 'AV')
			 AND C.APRO_RQST_NO       = R.APRO_RQST_NO
#else
			 AND C.APRO_RQST_NO       = R.APRO_RQST_NO
#end
			 AND H.CSR_NO             = A.CSR_NO
             AND H.INV_OFC_CD         = @[ofc_cd]
             AND NVL(H.DELT_FLG,'N') <> 'Y'
             AND NVL(A.CSR_APRO_TP_CD,'AL') = 'AL'
             AND NVL(A.RQST_APRO_STEP_FLG,'N') <> 'Y'
#if (${src_ctnt} == 'SO_M&R') 
			AND A.SRC_CTNT           IN ('SO_M&R', 'EQ')
#else
			AND A.SRC_CTNT           = @[src_ctnt]
#end
#if (${if_no}=='CSR') 	 
	#if (${search_csr_no}=='null' || ${search_csr_no} == '') 
		#if (${dt_status} == 'AR') --Approval Requested
 			 AND R.RQST_ST_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421
		#elseif (${dt_status} == 'RA') --Requesting Approval
			 AND (A.CSR_APRO_STEP_ASGN_RQST_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 )
		#elseif (${dt_status} == 'AV') --Approved or Disapproved
		 	 AND (
					( R.APSTS_CD IN ('C','R') 
					  AND R.RQST_END_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 )
				     OR A.IF_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421
				 )
		#elseif (${dt_status} == 'IU')  --I/F Status Updated
			 AND (A.IF_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 )
		#end
	#else
		AND A.CSR_NO LIKE '%'||@[search_csr_no]||'%'
	#end
#else
	AND H.INV_NO LIKE '%'||@[search_csr_no]||'%'
#end
#if (${if_status} == 'RA')
			--AND H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N'
            --AND A.RQST_APRO_STEP_FLG = 'Y'
            AND 1=2
#elseif (${if_status} == 'AR')
			AND H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N'
            AND A.RQST_APRO_STEP_FLG IS NULL
#elseif (${if_status} == 'SD') 
			AND A.IF_FLG IS NULL AND A.APRO_FLG = 'Y'
#elseif (${if_status} == 'SC') 
            AND H.INV_STS_CD = 'P' AND A.RCV_ERR_FLG IS NULL
#elseif (${if_status} == 'IE') 
            AND H.INV_STS_CD = 'E'
#elseif (${if_status} == 'RJ')
            AND H.INV_STS_CD = 'J'
#elseif (${if_status} == 'DA')
            AND H.INV_STS_CD = 'R'
#elseif (${if_status} == 'SP')
			AND H.INV_STS_CD = 'D'
#end
     UNION ALL
     SELECT TO_CHAR(A.IF_DT,'YYYY-MM-DD') IF_DT
               , A.CSR_NO
               , H.COST_OFC_CD
               , A.CSR_CURR_CD
			   ,DECODE(H.INV_CURR_CD, 'KRW', ROUND(NVL(A.CSR_AMT,0),0)
			      				  , 'JPY', ROUND(NVL(A.CSR_AMT,0),0)
								  , ROUND(NVL(A.CSR_AMT,0),2)) CSR_AMT
               , SUBSTR(A.INV_TERM_DT, 1, 4) ||'/' ||SUBSTR(A.INV_TERM_DT, 5, 2) ||'/' ||SUBSTR(A.INV_TERM_DT, 7, 2) DUE_DT
               , CASE

                         WHEN H.INV_STS_CD    = 'J' OR H.INV_STS_CD = 'G'
                         THEN 'A/P Rejected'
                         
                         WHEN H.INV_STS_CD = 'E'
                         THEN 'I/F Error'
                         
						 WHEN H.INV_STS_CD = 'X' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'N'
						 THEN 'I/F Error Cancelled'

						 WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'N' 
                         THEN 'Approval Request Cancelled'
                         
                         WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL  AND 1 = (SELECT count(*) FROM AP_PAY_INV K WHERE K.CSR_NO  = H.CSR_NO AND K.INV_STS_CD = 'G' AND ROWNUM = 1)
                              
                         THEN 'A/P Rejected'
                         
                         
                         WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL  AND 1 = (SELECT count(*) FROM AP_PAY_INV K WHERE K.CSR_NO  = H.CSR_NO AND K.INV_STS_CD = 'B' AND ROWNUM = 1)
                              
                         THEN 'Disapproved'

						 WHEN H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N' AND A.RQST_APRO_STEP_FLG = 'Y'
                         THEN 'Requesting Approval'
                         
                         WHEN H.INV_STS_CD = 'P'
                             AND A.RCV_ERR_FLG IS NULL
                         THEN 'I/F Success'
                         
                         WHEN H.INV_STS_CD    = 'R' OR H.INV_STS_CD = 'B'
                         THEN 'Disapproved'
                         
                         WHEN H.INV_STS_CD = 'A'
                             AND A.IF_FLG IS NULL
                         THEN DECODE(A.APRO_FLG
                                   ,'Y','Sending'
                                   ,'Approval Requested')
                         
                         WHEN H.INV_STS_CD = 'D'
                         THEN 'Paid'
                         
                         WHEN H.INV_STS_CD = 'X' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'X'
                         THEN 'Approval Request Cancelled'
						 

                         ELSE H.INV_STS_CD
                 END IF_STATUS
               , A.ATTR_CTNT2
               , A.IF_ERR_RSN
               , A.VNDR_NO
               , A.VNDR_TERM_NM
               , A.AFT_ACT_FLG
               , H.INV_ISS_DT
               , '' APRO_RQST_NO
               , H.INV_RCV_DT
               , A.IF_FLG
               , A.RCV_ERR_FLG
               , H.INV_STS_CD 
               , A.PAY_GRP_LU_CD 
               , A.ACCT_XCH_RT_YRMON
			   , A.CSR_USD_AMT
			   , A.CSR_APRO_TP_CD
			   , A.CRE_USR_ID   
			   , A.AGMT_DOC_CFM_CD
			   , A.GW_AGMT_DOC_CFM_CD
			   , A.AGMT_FILE_CFM_CD  
         FROM    AP_PAY_INV H
               , AP_INV_HDR A
         WHERE   1                    =1
			 AND H.CSR_NO             = A.CSR_NO
             AND H.INV_OFC_CD         = @[ofc_cd]
             AND NVL(H.DELT_FLG,'N') <> 'Y'
             AND NVL(A.CSR_APRO_TP_CD,'AL') = 'GW' --IN ('AL','GW')
             AND NVL(A.RQST_APRO_STEP_FLG,'N') <> 'Y'
#if (${src_ctnt} == 'SO_M&R') 
			AND A.SRC_CTNT           IN ('SO_M&R', 'EQ')
#else
			AND A.SRC_CTNT           = @[src_ctnt]
#end
#if (${if_no}=='CSR') 	 
	#if (${search_csr_no}=='null' || ${search_csr_no} == '') 
		#if (${dt_status} == 'AR') --Approval Requested
             AND ( A.CSR_APRO_STEP_ASGN_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 ) --TOBE
		#elseif (${dt_status} == 'RA') --Requesting Approval
			 AND (A.CSR_APRO_STEP_ASGN_RQST_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 )
		#elseif (${dt_status} == 'AV') --Approved or Disapproved
			 AND (( A.CSR_APRO_CMPL_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 )
			OR ( A.CSR_RJCT_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 ))
		#elseif (${dt_status} == 'IU') --I/F Status Updated
			 AND (A.IF_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 )
		#end
	#else
		AND A.CSR_NO LIKE '%'||@[search_csr_no]||'%'
	#end
#else
	AND H.INV_NO LIKE '%'||@[search_csr_no]||'%'
#end
#if (${if_status} == 'RA')
			--AND H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N'
            --AND A.RQST_APRO_STEP_FLG = 'Y'
            AND 1=2
#elseif (${if_status} == 'AR')
			AND H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N'
            AND A.RQST_APRO_STEP_FLG IS NULL
#elseif (${if_status} == 'SD') 
			AND A.IF_FLG IS NULL AND A.APRO_FLG = 'Y'
#elseif (${if_status} == 'SC') 
            AND H.INV_STS_CD = 'P' AND A.RCV_ERR_FLG IS NULL
#elseif (${if_status} == 'IE') 
            AND H.INV_STS_CD = 'E'
#elseif (${if_status} == 'RJ')
            AND H.INV_STS_CD = 'J'
#elseif (${if_status} == 'DA')
            AND H.INV_STS_CD = 'R'
#elseif (${if_status} == 'SP')
			AND H.INV_STS_CD = 'D'
#end
     UNION ALL
     SELECT TO_CHAR(A.IF_DT,'YYYY-MM-DD') IF_DT
               , A.CSR_NO
               , H.COST_OFC_CD
               , A.CSR_CURR_CD
			   ,DECODE(H.INV_CURR_CD, 'KRW', ROUND(NVL(A.CSR_AMT,0),0)
			      				  , 'JPY', ROUND(NVL(A.CSR_AMT,0),0)
								  , ROUND(NVL(A.CSR_AMT,0),2)) CSR_AMT
               , SUBSTR(A.INV_TERM_DT, 1, 4) ||'/' ||SUBSTR(A.INV_TERM_DT, 5, 2) ||'/' ||SUBSTR(A.INV_TERM_DT, 7, 2) DUE_DT
               , CASE

                         WHEN H.INV_STS_CD    = 'J' OR H.INV_STS_CD = 'G'
                         THEN 'A/P Rejected'
                         
                         WHEN H.INV_STS_CD = 'E'
                         THEN 'I/F Error'
                         
						 WHEN H.INV_STS_CD = 'X' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'N'
						 THEN 'I/F Error Cancelled'

						 WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'N' 
                         THEN 'Approval Request Cancelled'
                         
                         WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL  AND 1 = (SELECT count(*) FROM AP_PAY_INV K WHERE K.CSR_NO  = H.CSR_NO AND K.INV_STS_CD = 'G' AND ROWNUM = 1)
                              
                         THEN 'A/P Rejected'
                         
                         
                         WHEN H.INV_STS_CD = 'C' AND H.CSR_NO IS NOT NULL  AND 1 = (SELECT count(*) FROM AP_PAY_INV K WHERE K.CSR_NO  = H.CSR_NO AND K.INV_STS_CD = 'B' AND ROWNUM = 1)
                              
                         THEN 'Disapproved'

						 WHEN H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N' AND A.RQST_APRO_STEP_FLG = 'Y'
                         THEN 'Requesting Approval'
                         
                         WHEN H.INV_STS_CD = 'P'
                             AND A.RCV_ERR_FLG IS NULL
                         THEN 'I/F Success'
                         
                         WHEN H.INV_STS_CD    = 'R' OR H.INV_STS_CD = 'B'
                         THEN 'Disapproved'
                         
                         WHEN H.INV_STS_CD = 'A'
                             AND A.IF_FLG IS NULL
                         THEN DECODE(A.APRO_FLG
                                   ,'Y','Sending'
                                   ,'Approval Requested')
                         
                         WHEN H.INV_STS_CD = 'D'
                         THEN 'Paid'
                         
                         WHEN H.INV_STS_CD = 'X' AND H.CSR_NO IS NOT NULL AND A.AFT_ACT_FLG = 'X'
                         THEN 'Approval Request Cancelled'
						 

                         ELSE H.INV_STS_CD
                 END IF_STATUS
               , A.ATTR_CTNT2
               , A.IF_ERR_RSN
               , A.VNDR_NO
               , A.VNDR_TERM_NM
               , A.AFT_ACT_FLG
               , H.INV_ISS_DT
               , '' APRO_RQST_NO
               , H.INV_RCV_DT
               , A.IF_FLG
               , A.RCV_ERR_FLG
               , H.INV_STS_CD 
               , A.PAY_GRP_LU_CD 
               , A.ACCT_XCH_RT_YRMON
			   , A.CSR_USD_AMT
			   , A.CSR_APRO_TP_CD
			   , A.CRE_USR_ID   
			   , A.AGMT_DOC_CFM_CD
			   , A.GW_AGMT_DOC_CFM_CD
			   , A.AGMT_FILE_CFM_CD  
         FROM    AP_PAY_INV H
               , AP_INV_HDR A
         WHERE   1                    =1
			 AND H.CSR_NO             = A.CSR_NO
             AND H.INV_OFC_CD         = @[ofc_cd]
             AND NVL(H.DELT_FLG,'N') <> 'Y'
             --AND A.CSR_APRO_TP_CD IN ('AL','GW') --?곴??놁씠
             AND NVL(A.RQST_APRO_STEP_FLG,'N') = 'Y'
#if (${src_ctnt} == 'SO_M&R') 
			AND A.SRC_CTNT           IN ('SO_M&R', 'EQ')
#else
			AND A.SRC_CTNT           = @[src_ctnt]
#end
#if (${if_no}=='CSR') 	 
	#if (${search_csr_no}=='null' || ${search_csr_no} == '') 
		#if (${dt_status} == 'RA') --Requesting Approval
			 AND ( A.CSR_APRO_STEP_ASGN_DT IS NULL AND 
                  (A.CSR_APRO_STEP_ASGN_RQST_DT BETWEEN TO_DATE(@[fm_eff_dt],'YYYY-MM-DD') AND TO_DATE(@[to_eff_dt],'YYYY-MM-DD') + 0.99999421 ))
		#else
			 AND 1=2 
		#end
	#else
		AND A.CSR_NO LIKE '%'||@[search_csr_no]||'%'
	#end
#else
	AND H.INV_NO LIKE '%'||@[search_csr_no]||'%'
#end
#if (${if_status} == 'RA')
			AND H.INV_STS_CD = 'A' AND A.IF_FLG IS NULL AND A.APRO_FLG = 'N'
            --AND A.RQST_APRO_STEP_FLG = 'Y'
#elseif (${if_status} == 'AL')
			AND 1=1
#else
            AND 1=2
#end
         ) B
WHERE 1=1
#if (${search_tp_cd} == 'AL')
	AND NVL(B.CSR_APRO_TP_CD,'AL') = 'AL'
#elseif (${search_tp_cd} == 'GW')
	AND NVL(B.CSR_APRO_TP_CD,'AL') = 'GW'
#end
GROUP BY B.IF_DT
       , B.CSR_NO
       , B.CSR_CURR_CD
       , B.CSR_AMT
       , B.DUE_DT
       , B.IF_STATUS
       , B.ATTR_CTNT2
       , B.IF_ERR_RSN
       , B.VNDR_NO
       , B.VNDR_TERM_NM
       , B.AFT_ACT_FLG
       , B.IF_FLG
       , B.RCV_ERR_FLG
       , B.APRO_RQST_NO
       , B.PAY_GRP_LU_CD
       , B.COST_OFC_CD 
       , B.ACCT_XCH_RT_YRMON 
	   , B.CSR_USD_AMT 
       , B.AGMT_DOC_CFM_CD
	   , B.GW_AGMT_DOC_CFM_CD
	   , B.AGMT_FILE_CFM_CD
       , B.CSR_APRO_TP_CD
       , B.CRE_USR_ID
ORDER BY B.CSR_NO ASC			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
