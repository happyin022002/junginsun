<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalStepSendDBDAOLogAproCommErrMsgCSQL">
			<desc><![CDATA[공통 Error table에 Error 남기기]]></desc>
			<sql><![CDATA[
INSERT INTO COM_CSR_ERR_LOG (
CSR_ERR_LOG_SEQ
, CSR_ERR_TP_CD
, ERR_LOG_RMK    
, CRE_USR_ID     
, CRE_DT         
, UPD_USR_ID     
, UPD_DT         
) VALUES (
COM_CSR_ERR_LOG_SEQ.NEXTVAL
, @[csr_err_tp_cd] --CSR_ERR_TP_CD
, SUBSTRB(@[err_log_rmk],1,4000) --ERR_LOG_RMK    
, 'SYSTEM' --CRE_USR_ID     
, SYSDATE --CRE_DT         
, 'SYSTEM' --UPD_USR_ID     
, SYSDATE --UPD_DT        
)			]]></sql>
			<params>
				<param name="csr_err_tp_cd" type="12" value="" out="N"/>
				<param name="err_log_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
