<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalStepSendDBDAOGetAproStepSendLogListRSQL">
			<desc><![CDATA[AproStep I/F 대상 LOG 조회]]></desc>
			<sql><![CDATA[
SELECT * 
FROM COM_APRO_SND_LOG L
WHERE 1=1
AND L.SRC_CTNT = @[src_ctnt]
AND L.EXE_STS_CD = 'N'
AND L.EXE_ACT_TP_CD = @[act_tp_cd]
AND NVL(L.DELT_FLG,'N') <> 'Y'
AND EXISTS (
    SELECT 'X'
    FROM AP_INV_HDR A
    WHERE 1=1
    AND A.CSR_NO = L.CSR_NO
)
ORDER BY L.SND_LOG_SEQ DESC			]]></sql>
			<params>
				<param name="src_ctnt" type="12" value="" out="N"/>
				<param name="act_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
