<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRApprovalCommonManagementDBDAOsaveCsrAproTpCdUSQL">
			<desc><![CDATA[Approval Type(ALPS/GW)를 AP_INV_HDR에 저장한다.]]></desc>
			<sql><![CDATA[
UPDATE AP_INV_HDR
SET CSR_APRO_TP_CD = @[apro_tp_cd]
WHERE CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="apro_tp_cd" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
