<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRInvAgmtLnkDBDAODelComApFileDSQL">
			<desc><![CDATA[DELETE]]></desc>
			<sql><![CDATA[
UPDATE COM_AP_FILE_UPLD F
   SET F.DELT_FLG = 'Y',
       F.DELT_USR_ID = @[cre_usr_id],
       F.DELT_DT = SYSDATE,
       F.UPD_USR_ID = @[cre_usr_id],
       F.UPD_DT = SYSDATE
 WHERE 1=1
   AND F.ATCH_FILE_ID = @[atch_file_id]
   AND F.CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="atch_file_id" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
