<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRInvAgmtLnkDBDAOUpdComApFileUSQL">
			<desc><![CDATA[UPDATE]]></desc>
			<sql><![CDATA[
UPDATE COM_AP_FILE_UPLD F
   SET F.FILE_RMK = @[file_rmk]
 WHERE 1=1
   AND F.ATCH_FILE_ID = @[atch_file_id]
   AND F.CSR_NO = @[csr_no]
   AND F.CRE_USR_ID = @[cre_usr_id]			]]></sql>
			<params>
				<param name="file_rmk" type="12" value="" out="N"/>
				<param name="atch_file_id" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
