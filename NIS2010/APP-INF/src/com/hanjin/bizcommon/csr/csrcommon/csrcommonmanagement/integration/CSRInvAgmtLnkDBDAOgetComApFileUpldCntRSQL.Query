<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRInvAgmtLnkDBDAOgetComApFileUpldCntRSQL">
			<desc><![CDATA[COM_AP_FILE_UPLD CNT]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS ATCH_FILE_CNT
  FROM COM_AP_FILE_UPLD F,
       AP_INV_HDR A,
       COM_UPLD_FILE C,
       COM_USER U
 WHERE 1=1
   AND F.AP_FILE_DIV_CD = 'C'
   AND F.CSR_NO = @[csr_no]
   AND F.CSR_NO = A.CSR_NO
   AND F.CSR_FILE_UPLD_TP_CD = @[csr_file_upld_tp_cd]
   AND NVL(F.DELT_FLG, 'N') <> 'Y'
   AND F.FILE_SAV_ID = C.FILE_SAV_ID
   AND NVL(C.DELT_FLG, 'N') <> 'Y'
   AND F.CRE_USR_ID = U.USR_ID
   AND NVL(U.USE_FLG, 'N') = 'Y'			]]></sql>
			<params>
				<param name="csr_no" type="12" value="12SHAMBB15010500002" out="N"/>
				<param name="csr_file_upld_tp_cd" type="12" value="FU" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
