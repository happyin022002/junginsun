		<query name="AgreementNoticeDBDAOSearchSystemNameRSQL">
			<desc><![CDATA[Search System Name]]></desc>
			<sql><![CDATA[
SELECT 'ACM' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'ESM_ACM_M001') AS NM FROM DUAL -- Agent Commission Management
UNION ALL
SELECT 'CHS' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'EES_CGM_M001') AS NM FROM DUAL -- Chassis
UNION ALL
SELECT 'LSE' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'EES_LSE_M001') AS NM FROM DUAL
UNION ALL
SELECT 'FMS' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'ESM_FMS_M001') AS NM FROM DUAL
UNION ALL
SELECT 'MNR' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'EES_MNR_M001') AS NM FROM DUAL
UNION ALL
SELECT 'MGS' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'EES_CGM_M019') AS NM FROM DUAL -- M.G.SET
UNION ALL
SELECT 'TES' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'ESD_TES_M001') AS NM FROM DUAL
UNION ALL
SELECT 'TRS' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'ESD_TRS_M001') AS NM FROM DUAL
UNION ALL
SELECT 'PSO' AS CD ,(SELECT PGM_NM FROM COM_PROGRAM WHERE PGM_NO = 'VOP_PSO_M001') AS NM FROM DUAL			]]></sql>
