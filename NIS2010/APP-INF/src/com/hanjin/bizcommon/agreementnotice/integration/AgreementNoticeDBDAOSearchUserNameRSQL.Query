<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementNoticeDBDAOSearchUserNameRSQL">
			<desc><![CDATA[SearchUserName]]></desc>
			<sql><![CDATA[
SELECT USR_NM AS NM FROM COM_USER WHERE USE_FLG = 'Y' AND USR_ID = @[cd]			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
