<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchAuthPgmNmListRSQL">
			<desc><![CDATA[Authorization 0U2 Program List 조회]]></desc>
			<sql><![CDATA[
SELECT 
DISTINCT P.PGM_NO, C.PGM_NM
FROM COM_AUTH_PGM P, COM_PROGRAM C
WHERE 1=1
AND P.PGM_NO = C.PGM_NO
AND DECODE(@[sub_sys_cd],NULL,'ALL','','ALL','ALL','ALL',P.SUB_SYS_CD) = DECODE(@[sub_sys_cd],NULL,'ALL','','ALL','ALL','ALL',@[sub_sys_cd])
AND NVL(P.USE_FLG,'N') = 'Y'
AND NVL(C.USE_FLG,'N') = 'Y' 			]]></sql>
			<params>
				<param name="sub_sys_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
