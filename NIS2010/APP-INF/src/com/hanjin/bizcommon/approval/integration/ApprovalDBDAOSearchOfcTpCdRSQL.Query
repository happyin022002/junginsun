<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchOfcTpCdRSQL">
			<desc><![CDATA[Session Office code로 Office Tpye Code를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT OFC_TP_CD 
  FROM MDM_ORGANIZATION		
 WHERE DELT_FLG ='N'		
   AND OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
