<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchAuthApprovalByAuthAproRoutSeqRSQL">
			<desc><![CDATA[Auth Approval 결재 라인 조회]]></desc>
			<sql><![CDATA[
SELECT AUTH_APRO_ROUT_USR_SEQ,
AUTH_APRO_USR_ID,
AUTH_APRO_USR_NM,
AUTH_APRO_OFC_CD,
AUTH_APRO_USR_JB_TIT_NM
FROM (
 	SELECT ROW_NUMBER() OVER (ORDER BY A.AUTH_APRO_ROUT_SEQ DESC) no,
	A.AUTH_APRO_ROUT_USR_SEQ,
	A.AUTH_APRO_USR_ID,
 	A.AUTH_APRO_USR_NM,
 	B.OFC_CD AUTH_APRO_OFC_CD,
 	A.AUTH_APRO_USR_JB_TIT_NM
 	FROM COM_AUTH_APRO_DFLT_ROUT_USR A, COM_USER B
 	WHERE B.USE_FLG = 'Y'
 	--AND A.APRO_USR_ID = B.USR_ID
 	AND A.AUTH_APRO_USR_ID IN (B.USR_ID, B.EP_ID)
 	AND A.AUTH_APRO_ROUT_SEQ = @[auth_apro_rout_seq]
 	ORDER BY A.AUTH_APRO_ROUT_USR_SEQ DESC
)A			]]></sql>
			<params>
				<param name="auth_apro_rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
