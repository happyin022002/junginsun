<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchNewAproJbTitCdRSQL">
			<desc><![CDATA[PDT 우회계정 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
DECODE(AP_COM_CHECK_MGR_APRO_HIS_FNC(A.CSR_NO),'Y','N',A.APRO_USR_JB_TIT_CD) NEW_APRO_USR_JB_TIT_CD
FROM AP_INV_HDR A
WHERE A.CSR_NO = @[csr_no]
AND NVL(A.CSR_APRO_TP_CD,'AL') = 'GW'
AND A.APRO_USR_JB_TIT_CD IN ('P','B')			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
