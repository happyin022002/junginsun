<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchFileSizeRSQL">
			<desc><![CDATA[파일사이즈 조회]]></desc>
			<sql><![CDATA[
SELECT FILE_SZ_CAPA L_FILE_SZ 
FROM COM_UPLD_FILE 
WHERE FILE_SAV_ID = @[file_id] AND NVL(DELT_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="file_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
