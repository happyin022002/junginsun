<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOUpdateConfirmAuthAproProgressUSQL">
			<desc><![CDATA[Authorization 중간 결재]]></desc>
			<sql><![CDATA[
UPDATE COM_AUTH_APRO_RQST Q
SET Q.AUTH_APSTS_CD = 'P'
    , Q.CRNT_AUTH_APRO_RQST_SEQ = (Q.CRNT_AUTH_APRO_RQST_SEQ+1)
    , Q.RQST_END_DT   = NULL
	, Q.UPD_USR_ID 	= @[usr_id]
	, Q.UPD_DT = SYSDATE
WHERE Q.AUTH_APRO_RQST_NO = @[auth_apro_rqst_no] 			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="auth_apro_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
