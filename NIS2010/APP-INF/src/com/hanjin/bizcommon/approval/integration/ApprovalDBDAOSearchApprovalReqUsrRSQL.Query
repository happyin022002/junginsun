<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchApprovalReqUsrRSQL">
			<desc><![CDATA[CSR Approval Request한 User를 조화한다]]></desc>
			<sql><![CDATA[
SELECT A.CRE_USR_ID AS APRO_RQST_USR_ID
  FROM AP_INV_HDR A
 WHERE A.CSR_NO = @[csr_no]
			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
