<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchOfcCdListRSQL">
			<desc><![CDATA[Approval Inquery화면의 Office comboBox조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT LL.OFC_N5TH_LVL_CD AS OFC_CD
      ,LL.OFC_N5TH_LVL_CD AS OFC_NM
  FROM MAS_OFC_LVL LL				
 WHERE 1 = 1				
   AND (CASE (SELECT OFC_LVL				
               FROM (SELECT L.OFC_LVL				
                       FROM MAS_OFC_LVL L				
                      WHERE L.OFC_N3RD_LVL_CD IS NOT NULL				
                        AND L.OFC_N5TH_LVL_CD IS NOT NULL				
                        AND L.OFC_CD = @[ofc_cd] /* LOGIN-OFFICE CODE BINDING */				
                      ORDER BY L.OFC_APLY_TO_YRMON DESC )				
              WHERE ROWNUM = 1 )				
           WHEN '1' THEN 'XXXXX'				
           WHEN '2' THEN LL.OFC_N3RD_LVL_CD				
           WHEN '3' THEN LL.OFC_N3RD_LVL_CD				
           WHEN '4' THEN LL.OFC_CD				
           WHEN '5' THEN LL.OFC_CD				
           WHEN '6' THEN LL.OFC_CD				
           WHEN '7' THEN LL.OFC_CD				
           WHEN '9' THEN 'XXXXX'				
         ELSE 'XXXXX'				
       END = @[ofc_cd] /* LOGIN-OFFICE CODE BINDING */
       OR LL.OFC_CD = @[ofc_cd]
      )		
   AND LL.OFC_N5TH_LVL_CD IS NOT NULL
   AND LL.OFC_APLY_TO_YRMON ='999912'			
 ORDER BY LL.OFC_N5TH_LVL_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
