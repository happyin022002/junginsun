<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchCheckAproAllRSQL">
			<desc><![CDATA[입력된 결재라인이 모두 결재됐을 경우 결재라인을 저장은 막는다]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(*) = 0 THEN 'Y'
            ELSE 'N'
        END AS  APRO_ALL_FLG           
  FROM COM_APRO_RQST_ROUT A
 WHERE A.APRO_RQST_NO = @[apro_rqst_no]
   AND A.APSTS_CD IS NULL			]]></sql>
			<params>
				<param name="apro_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
