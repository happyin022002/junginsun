<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOsearchInvoiceRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT COUNT(INV_NO) CNT
  FROM TRS_TRSP_INV_WRK_HIS
 WHERE USR_EVNT_TP_CD ='D'
   AND HJL_INV_AUD_STS_CD='SV'
   AND INV_NO IN (SELECT ATTR_CTNT1 FROM AP_INV_DTRB WHERE CSR_NO = (@[csr_no]))
			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
