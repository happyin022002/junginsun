<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ApprovalDBDAOSearchExeActTpCdRSQL">
			<desc><![CDATA[ExeActTpCd 구하기]]></desc>
			<sql><![CDATA[
SELECT 
R.EXE_ACT_TP_CD
FROM AP_INV_HDR A, COM_AP_SND_MN_RULE R
WHERE 1=1
AND A.SRC_CTNT = R.SRC_CTNT
AND A.CSR_NO = @[csr_no]
AND R.AP_SND_DAT_TP_CD = 'C'
AND NVL(R.CFM_FLG,'N') = 'Y'
AND NVL(R.DELT_FLG,'N') <> 'Y'
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
