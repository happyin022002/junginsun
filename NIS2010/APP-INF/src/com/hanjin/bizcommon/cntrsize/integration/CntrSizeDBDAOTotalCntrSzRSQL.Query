<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrSizeDBDAOTotalCntrSzRSQL">
			<desc><![CDATA[Count container size]]></desc>
			<sql><![CDATA[
SELECT COUNT(*)
FROM mdm_cntr_sz        
WHERE 1 = 1
#if (${cntr_sz_cd} != '') 
AND cntr_sz_cd like @[cntr_sz_cd]
#end
#if (${cntr_sz_desc} != '') 
AND cntr_sz_desc like @[cntr_sz_desc]
#end
AND NVL(DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
