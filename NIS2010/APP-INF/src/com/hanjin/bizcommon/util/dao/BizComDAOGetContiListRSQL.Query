<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BizComDAOGetContiListRSQL">
			<desc><![CDATA[GetContiList]]></desc>
			<sql><![CDATA[
SELECT (100000 + (rownum * 10) ) as sortKey ,
       CONTI_CD as code, CONTI_NM as name
  FROM MDM_CONTINENT
 WHERE NVL(DELT_FLG, 'N') <> 'Y'
   AND CONTI_CD NOT IN ('X', 'Y')

#if (${where} != '') 
AND ${where}
#end

#if (${sortKey} > 0) 
ORDER BY ${sortKey}
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
