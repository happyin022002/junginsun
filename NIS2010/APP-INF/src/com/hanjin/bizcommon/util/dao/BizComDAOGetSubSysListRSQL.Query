<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BizComDAOGetSubSysListRSQL">
			<desc><![CDATA[GetSubSysList]]></desc>
			<sql><![CDATA[
SELECT (100000 + (rownum * 10) ) as sortKey ,
       PGM_SUB_SYS_CD as code, PGM_SUB_SYS_CD as name
  FROM COM_PGM_SUB_SYS
 WHERE PGM_APPL_CD = 'ALPS'

#if (${where} != '') 
AND ${where}
#end

#if (${sortKey} > 0) 
ORDER BY ${sortKey}
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
