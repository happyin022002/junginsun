<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BizComDAOGetSvcScopeListRSQL">
			<desc><![CDATA[GetSvcScopeList]]></desc>
			<sql><![CDATA[
SELECT (100000 + (rownum * 10) ) as sortKey ,
       T1.SVC_SCP_CD as code, T1.SVC_SCP_CD as name
  FROM MDM_SVC_SCP_LMT T1, MDM_SVC_SCP_LMT T2
 WHERE T1.ORG_DEST_CD = 'O'      
   AND   T2.ORG_DEST_CD = 'D'
   AND   T1.SVC_SCP_CD = T2.SVC_SCP_CD

#if (${where} != '') 
AND ${where}
#end

#if (${sortKey} > 0) 
ORDER BY ${sortKey}
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
