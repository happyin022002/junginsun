<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BizComDAOGetCurrencyListRSQL">
			<desc><![CDATA[GetCurrencyList]]></desc>
			<sql><![CDATA[
SELECT (100000 + (rownum * 10) ) as sortKey ,
       CURR_CD as code, CURR_CD as name
  FROM MDM_CURRENCY
 WHERE NVL(DELT_FLG, 'N') <> 'Y'

#if (${where} != '') 
AND ${where}
#end

#if (${sortKey} > 0) 
ORDER BY ${sortKey}
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
