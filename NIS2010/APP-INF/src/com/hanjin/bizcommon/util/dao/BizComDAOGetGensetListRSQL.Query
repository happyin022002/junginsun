       EQ_TPSZ_CD as code, DIFF_DESC as name
  FROM CGM_EQ_TP_SZ
 WHERE EQ_KND_CD = 'G'

#if (${where} != '') 
AND ${where}
#end

#if (${sortKey} > 0) 
ORDER BY ${sortKey}
#end			]]></sql>
