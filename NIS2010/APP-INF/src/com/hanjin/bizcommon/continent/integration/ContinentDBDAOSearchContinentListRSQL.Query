<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContinentDBDAOSearchContinentListRSQL">
			<desc><![CDATA[Continent의 모든 목록을 가져온다.]]></desc>
			<sql><![CDATA[
SELECT conti_cd,
	conti_nm
FROM mdm_continent
WHERE 1 = 1 AND nvl(delt_flg,'N') <> 'Y'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
