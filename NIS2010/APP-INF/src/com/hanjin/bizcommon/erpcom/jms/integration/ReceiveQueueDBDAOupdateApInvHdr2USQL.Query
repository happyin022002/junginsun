<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueDBDAOupdateApInvHdr2USQL">
			<desc><![CDATA[AP_INV_HDR UPDATE(2)]]></desc>
			<sql><![CDATA[
UPDATE ap_inv_hdr
   SET rcv_err_flg = SUBSTR(@[if_flag],0,1),
       rcv_err_rsn = HJSEAI_PKG.h_decode(@[if_err_rsn],'ERP','BMS')
 WHERE csr_no = @[csr_no]			]]></sql>
			<params>
				<param name="if_flag" type="12" value="" out="N"/>
				<param name="if_err_rsn" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
