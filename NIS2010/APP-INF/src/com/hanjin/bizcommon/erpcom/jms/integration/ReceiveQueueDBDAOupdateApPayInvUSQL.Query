<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueDBDAOupdateApPayInvUSQL">
			<desc><![CDATA[AP_PAY_INV UPDATE]]></desc>
			<sql><![CDATA[
UPDATE AP_PAY_INV   
   SET AP_IF_DT  = TO_DATE(@[if_date],'YYYYMMDD HH24:MI:SS') 
   	,INV_STS_CD = DECODE( SUBSTR(@[if_flag],0,1), 'Y', 'P', 'E' ) 
 WHERE CSR_NO = @[csr_no]   			]]></sql>
			<params>
				<param name="if_date" type="12" value="" out="N"/>
				<param name="if_flag" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
