<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueDBDAOupdateApPayInv2USQL">
			<desc><![CDATA[modify CSRPayInv]]></desc>
			<sql><![CDATA[
UPDATE AP_PAY_INV                   
SET AP_PAY_DT = to_date(@[hdr_pay_dt], 'YYYY-MM-DD HH24:MI:SS'), AP_PAY_AMT = @[hdr_pay_amt], AP_PAY_FLG = 'Y', INV_STS_CD = 'D'  
WHERE CSR_NO = @[hdr_csr_no]   			]]></sql>
			<params>
				<param name="hdr_pay_dt" type="12" value="" out="N"/>
				<param name="hdr_pay_amt" type="12" value="" out="N"/>
				<param name="hdr_csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
