<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueDBDAOupdateTesTmlSoHdrUSQL">
			<desc><![CDATA[TES_TML_SO_HDR Update]]></desc>
			<sql><![CDATA[
UPDATE TES_TML_SO_HDR             
SET PAY_FLG = 'Y', PAY_DT = TO_DATE(SUBSTR(@[hdr_pay_dt],1,8),'YYYY-MM-DD'), TML_INV_STS_CD = 'D'     
WHERE CSR_NO = @[hdr_csr_no]			]]></sql>
			<params>
				<param name="hdr_pay_dt" type="12" value="" out="N"/>
				<param name="hdr_csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
