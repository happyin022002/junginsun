<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueDBDAOsearchPgmIdRSQL">
			<desc><![CDATA[GET AP_PGM_NO]]></desc>
			<sql><![CDATA[
SELECT SRC_CTNT 
  FROM AP_INV_HDR 
 WHERE CSR_NO = @[csr_no] 
   AND ROWNUM = 1 			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
