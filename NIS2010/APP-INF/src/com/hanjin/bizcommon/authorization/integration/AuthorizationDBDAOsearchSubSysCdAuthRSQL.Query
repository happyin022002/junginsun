<sqls>
	<querys>
		<query name="AuthorizationDBDAOsearchSubSysCdAuthRSQL">
			<desc><![CDATA[Auth 관련 모듈 조회]]></desc>
			<sql><![CDATA[
SELECT 
DISTINCT P.SUB_SYS_CD AS SUB_SYS_CD_AUTH, P.SUB_SYS_CD AS SUB_SYS_CD
FROM COM_AUTH_PGM P
WHERE NVL(P.USE_FLG,'N') = 'Y'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
