<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationDBDAOupdateAuthAproCfmUSQL">
			<desc><![CDATA[COM_AUTH_APRO_RQST에서 CFM_FLG Update]]></desc>
			<sql><![CDATA[
UPDATE COM_AUTH_APRO_RQST
SET CFM_FLG = 'Y'
WHERE AUTH_APRO_RQST_NO = @[auth_apro_rqst_no] 			]]></sql>
			<params>
				<param name="auth_apro_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
