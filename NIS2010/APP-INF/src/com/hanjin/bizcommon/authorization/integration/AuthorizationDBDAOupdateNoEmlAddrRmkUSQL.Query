<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationDBDAOupdateNoEmlAddrRmkUSQL">
			<desc><![CDATA[E-mail이 없는 경우 헤더에 사유 update]]></desc>
			<sql><![CDATA[
UPDATE COM_AUTH_APRO_RQST
SET AUTH_APRO_RQST_RMK = SUBSTRB('No E-mail Address : '||@[no_eml_apro]||' '||AUTH_APRO_RQST_RMK, 999)
WHERE 1=1
AND AUTH_APRO_RQST_NO = @[auth_apro_rqst_no]			]]></sql>
			<params>
				<param name="no_eml_apro" type="12" value="" out="N"/>
				<param name="auth_apro_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
