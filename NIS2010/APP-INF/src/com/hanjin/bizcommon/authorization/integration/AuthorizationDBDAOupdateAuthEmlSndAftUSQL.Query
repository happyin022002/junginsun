<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationDBDAOupdateAuthEmlSndAftUSQL">
			<desc><![CDATA[Auth E-mail 발송 후 Flag, Date 변경]]></desc>
			<sql><![CDATA[
UPDATE COM_AUTH_EML_SND E
SET E.EML_SND_RSLT_FLG = 'Y'
	,E.EML_SND_DT = SYSDATE
	,E.EML_SND_NO = @[eml_snd_no]
	,E.AUTH_EML_SND_RSLT_CD = 'C'
WHERE AUTH_EML_SND_NO = @[auth_eml_snd_no]			]]></sql>
			<params>
				<param name="eml_snd_no" type="12" value="" out="N"/>
				<param name="auth_eml_snd_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
