<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationDBDAOsearchPgmNmRSQL">
			<desc><![CDATA[COM_PROGRAM 에서 PGM_NM을 조회해 온다.]]></desc>
			<sql><![CDATA[
SELECT PGM_NM
FROM COM_PROGRAM
WHERE PGM_NO = @[pgm_no]			]]></sql>
			<params>
				<param name="pgm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
