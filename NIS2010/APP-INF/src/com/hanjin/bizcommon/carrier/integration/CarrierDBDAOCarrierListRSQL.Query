<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierDBDAOCarrierListRSQL">
			<desc><![CDATA[Carrier의 모든 목록을 가져온다.]]></desc>
			<sql><![CDATA[
SELECT crr_cd,
	   crr_nm crr_full_nm,
	   '' crr_nm
FROM (SELECT ROW_NUMBER() OVER (ORDER BY crr_cd ASC) no,
	crr_cd,
	crr_nm
FROM mdm_carrier
WHERE 1 = 1   
#if (${crr_cd} != '')
AND crr_cd like @[crr_cd] || '%'
#end    
#if (${crr_nm} != '')      
AND crr_nm like '%' || @[crr_nm] || '%'
#end
AND NVL(DELT_FLG, 'N') <> 'Y'
) a	
WHERE no BETWEEN @[startpart] AND @[endpart]			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="crr_nm" type="12" value="" out="N"/>
				<param name="startpart" type="2" value="" out="N"/>
				<param name="endpart" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
