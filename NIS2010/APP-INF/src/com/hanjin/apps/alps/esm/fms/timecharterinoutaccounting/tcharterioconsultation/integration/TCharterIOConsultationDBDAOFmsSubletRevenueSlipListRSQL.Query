<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOFmsSubletRevenueSlipListRSQL">
			<desc><![CDATA[Sublet Revenue Select]]></desc>
			<sql><![CDATA[
SELECT
	B.ACCT_CD,
	B.CUST_CNT_CD,
	B.CUST_SEQ,
	B.CTR_CD,
	B.SLP_LOC_CD,
	A.EFF_DT,
	B.CSR_AMT,
	B.SLP_TP_CD,
	B.SLP_FUNC_CD,
	B.SLP_OFC_CD,
	B.SLP_SEQ_NO,
	B.CSR_CURR_CD,
	B.TRNS_CURR_CD,
	B.TRNS_AMT,
	B.INV_SEQ,
	B.SLP_ISS_DT,
	B.CSR_DESC,
	B.CSR_DESC CSR_DESC1,
	B.CSR_DESC CSR_DESC2,
	B.CSR_DESC CSR_DESC3,
	(B.VSL_CD || B.SKD_VOY_NO || B.SKD_DIR_CD || B.REV_DIR_CD) VVD_CD,
	B.TO_INV_NO,
	B.TO_INV_NO TO_INV_NO1,
	B.VVD_EFF_DT,
	B.VVD_EXP_DT,
    (B.SLP_TP_CD || B.SLP_FUNC_CD || B.SLP_OFC_CD || B.SLP_ISS_DT || B.SLP_SER_NO) CSR_NO
FROM FMS_CONSULTATION A,
	 FMS_CSUL_SLP B
WHERE A.SLP_TP_CD = B.SLP_TP_CD
  AND A.SLP_FUNC_CD = B.SLP_FUNC_CD
  AND A.SLP_OFC_CD = B.SLP_OFC_CD
  AND A.SLP_ISS_DT = B.SLP_ISS_DT
  AND A.SLP_SER_NO = B.SLP_SER_NO
  AND A.SLP_TP_CD = @[slp_tp_cd]
  AND A.SLP_FUNC_CD = @[slp_func_cd]
  AND A.SLP_OFC_CD = @[slp_ofc_cd]
  AND A.SLP_ISS_DT = SUBSTR(@[slp_iss_dt],3,6)
  AND A.SLP_SER_NO =@[slp_ser_no]			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
