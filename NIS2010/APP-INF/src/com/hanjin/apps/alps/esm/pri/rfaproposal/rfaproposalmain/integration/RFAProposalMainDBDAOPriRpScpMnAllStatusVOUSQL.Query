<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpScpMnAllStatusVOUSQL">
			<desc><![CDATA[Request Cancel 시 Status 변경]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_MN MN
   SET PROP_SCP_STS_CD = @[prop_scp_sts_cd]
   ,UPD_USR_ID          = @[upd_usr_id]
   ,UPD_DT              = SYSDATE
 WHERE PROP_NO = @[prop_no]
AND    AMDT_SEQ = @[amdt_seq]
AND EXISTS ( SELECT 1 FROM PRI_RP_SCP_AMDT_SMRY
			WHERE MN.PROP_NO = PROP_NO
			AND   MN.AMDT_SEQ = AMDT_SEQ
			AND   MN.SVC_SCP_CD = SVC_SCP_CD
			AND   AMDT_FLG = 'Y')			]]></sql>
			<params>
				<param name="prop_scp_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
