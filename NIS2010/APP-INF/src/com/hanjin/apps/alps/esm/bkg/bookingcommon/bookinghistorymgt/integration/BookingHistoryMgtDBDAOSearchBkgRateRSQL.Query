<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgRateRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgRateRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
, RT_BL_TP_CD
, FRT_TERM_CD
, BKG_CTRT_TP_CD
, PPD_RCV_OFC_CD
, PPD_PAYR_CNT_CD
, PPD_PAYR_CUST_SEQ
, CLT_OFC_CD
, CLT_PAYR_CNT_CD
, CLT_PAYR_CUST_SEQ
, REV_DIV_CD
, BKG_RT_WHF_EXPT_CD
, WHF_SHPR_RGST_NO
, TO_CHAR(RT_APLY_DT, 'YYYY-MM-DD HH24:MI:SS') RT_APLY_DT
, TO_CHAR(CGO_RCV_DT, 'YYYY-MM-DD HH24:MI:SS') CGO_RCV_DT
, DIFF_RMK
, RT_INTER_RMK
, DOC_INTER_RMK
, AUD_STS_CD
, CRE_USR_ID
, TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, UPD_USR_ID
, TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
, PRC_RT_MTCH_PATT_CD
, PRC_GEN_SPCL_RT_TP_CD
, PRC_CMDT_HDR_SEQ
, PRC_ROUT_SEQ
, GRP_RAT_CHK_FLG
  FROM BKG_RATE
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
