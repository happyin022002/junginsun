<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOSearchModifyChargeRSQL">
			<desc><![CDATA[BDRCorrectionDBDAOSearchModifyChargeRSQL]]></desc>
			<sql><![CDATA[
SELECT RATE.CHG_CD
  FROM BKG_CHG_RT     RATE 
     , BKG_CHG_RT_HIS RTHIS 
 WHERE RATE.BKG_NO   = RTHIS.BKG_NO 
   AND RATE.RT_SEQ   = RTHIS.RT_SEQ
   AND RTHIS.CORR_NO = 'TMP0000001' 
   AND RATE.BKG_NO   = @[bkg_no]
   AND (   RATE.CURR_CD      != RTHIS.CURR_CD 
        OR RATE.CHG_UT_AMT   != RTHIS.CHG_UT_AMT 
        OR RATE.RAT_AS_QTY   != RTHIS.RAT_AS_QTY 
        OR RATE.RAT_UT_CD    != RTHIS.RAT_UT_CD 
        OR RATE.CHG_AMT      != RTHIS.CHG_AMT)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
