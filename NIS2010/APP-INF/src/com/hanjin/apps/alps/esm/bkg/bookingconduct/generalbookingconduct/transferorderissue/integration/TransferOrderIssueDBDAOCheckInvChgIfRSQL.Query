<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOCheckInvChgIfRSQL">
			<desc><![CDATA[inv로 운임이 interface되었는지 확인]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y') IF_FLG
FROM DOD_DRP_OFF_CHG G, BKG_EUR_TRO T
WHERE 1 = 1
  AND G.BKG_NO = @[bkg_no]
  AND G.CNTR_NO = @[cntr_no]
  AND G.DRP_OFF_CHG_SEQ = (SELECT MAX(C.DRP_OFF_CHG_SEQ)
                           FROM  DOD_DRP_OFF_CHG C
                           WHERE 1 = 1
                             AND C.BKG_NO = G.BKG_NO
                             AND C.CNTR_NO = G.CNTR_NO)
  AND G.BKG_NO = T.BKG_NO
  AND G.CNTR_NO = T.CNTR_NO
  AND T.IO_BND_CD = 'I'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
