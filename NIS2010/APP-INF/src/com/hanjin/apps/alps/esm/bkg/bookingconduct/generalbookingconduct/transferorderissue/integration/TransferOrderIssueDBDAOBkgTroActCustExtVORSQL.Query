<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOBkgTroActCustExtVORSQL">
			<desc><![CDATA[BkgTroActCust ext select]]></desc>
			<sql><![CDATA[
SELECT 
	a.tro_act_cust_knd_cd,
	a.tro_vndr_seq,
	a.ofc_cd,
	a.tro_act_rep_seq,
	a.cnt_cd,
	a.cust_seq,
	a.vndr_seq,
	a.loc_cd,
	SUBSTR(a.ZN_CD, 6, 2) zn_cd,
	a.act_shpr_nm,
	a.act_shpr_addr,
	a.cntc_pson_nm,
	a.cntc_phn_no,
	a.cntc_fax_no,
	a.cntc_mphn_no,
	a.cntc_eml,
	a.dor_zip_id,
	a.diff_rmk, 
    '' vndr_lgl_eng_nm, 
	a.cre_usr_id,
	a.cre_dt,
	a.upd_usr_id,
	a.upd_dt
FROM bkg_tro_act_cust a
WHERE	a.tro_act_cust_knd_cd = 'C'
AND	a.cnt_cd = @[cnt_cd]
AND	a.CUST_seq = @[cust_seq]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
