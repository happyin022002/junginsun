<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOPrintFmsCsrAgmtInfoRSQL">
			<desc><![CDATA[groupware 전송 xmlData Agreement info]]></desc>
			<sql><![CDATA[
SELECT
 FC2.AGMT_DOC_NO 	AS l_assetcd  		
,FC2.AGMT_DOC_DESC	AS l_document_title 	
FROM FMS_CONSULTATION FC
    ,FMS_CONTRACT FC2
WHERE 1=1
AND FC.FLET_CTRT_NO = FC2.FLET_CTRT_NO
AND    FC.SLP_TP_CD
    || FC.SLP_FUNC_CD
    || FC.SLP_OFC_CD
    || FC.SLP_ISS_DT
    || FC.SLP_SER_NO = @[csr_no]
AND FC2.AGMT_DOC_NO IS NOT NULL   			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
