<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOmodifyCustomsResultForRemarkSnpHJSCUSQL">
			<desc><![CDATA[modifyCustomsResultForRemarkSnpHJSC]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_RSLT
SET
	CSTMS_RMK2 = @[remark2],
	AMS_LOC_CD = @[cus_amsport],
	RCV_LOC_CD = DECODE(@[in_snp],'SMLM','HOUSE',@[cus_loc]),
	BL_NVOCC_TP_CD = DECODE(@[in_snp],'SMLM','H','N'),
	VSL_CD = @[vsl_cd_m],
	SKD_VOY_NO = @[skd_voy_no],
	SKD_DIR_CD = @[skd_dir_cd]
WHERE CNT_CD = 'US'
  AND BL_NO = @[bl_no]
  AND CSTMS_SEQ = @[icr_seq]			]]></sql>
			<params>
				<param name="remark2" type="12" value="" out="N"/>
				<param name="cus_amsport" type="12" value="" out="N"/>
				<param name="in_snp" type="12" value="" out="N"/>
				<param name="cus_loc" type="12" value="" out="N"/>
				<param name="vsl_cd_m" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="icr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
