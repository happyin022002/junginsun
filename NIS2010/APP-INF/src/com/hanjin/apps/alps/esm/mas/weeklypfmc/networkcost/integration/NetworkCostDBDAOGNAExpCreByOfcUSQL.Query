<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOGNAExpCreByOfcUSQL">
			<desc><![CDATA[ ]]></desc>
			<sql><![CDATA[
UPDATE 
 MAS_GEN_EXPN_OFC_STUP 
SET 
	EXPN_USD_AMT = @[expn_usd_amt]
, UPD_USR_ID = @[upd_usr_id]
, UPD_DT = sysdate
WHERE COST_YRMON = @[cost_yrmon] AND OFC_CD = @[ofc_cd] AND LOCL_CURR_CD = @[locl_curr_cd]			]]></sql>
			<params>
				<param name="expn_usd_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
