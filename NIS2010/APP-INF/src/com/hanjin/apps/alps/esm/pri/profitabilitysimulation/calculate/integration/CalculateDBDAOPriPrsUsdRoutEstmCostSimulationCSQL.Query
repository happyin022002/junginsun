<sqls>
	<querys>
		<query name="CalculateDBDAOPriPrsUsdRoutEstmCostSimulationCSQL">
			<desc><![CDATA[Proposal Scope Copy PRI_SP_SCP_RT_ACT_CUST Insert

* History
2015.06.26 CHM-201536492 Split05-주간 MAS Open에 따른 타모듈 프로그램 적용 요청]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_PRS_USD_ROUT_ESTM_COST (
        ROUT_CS_NO
        , COA_COST_SRC_CD
        , COM_ROUT_SEQ
        , ROUT_CS_SRC_DT
        , LOC_CD
        , COND_OFC_CD
        , DIV_MEAS_CD
        , OFC_CLSS_CD
        , SLS_ACT_CD
        , RA_ACCT_CD
        , STND_COST_CD
        , COST_ASS_BSE_CD
        , SVC_TRNS_PRC_AMT
        , BKG_KNT
        , CNTR_BX_KNT
        , CNTR_TEU_QTY
        , VSL_DYS
        , COM_FLG
        , COST_CALC_RMK
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT
)
		SELECT
			   @[rout_cs_no] AS ROUT_CS_NO
			, MAS_COST_SRC_CD
			, COM_ROUT_SEQ
			, @[rout_cs_src_dt] AS ROUT_CS_SRC_DT
			, LOC_CD
			, COND_OFC_CD
			, DIV_MEAS_CD
			, OFC_CLSS_CD
			, SLS_ACT_CD
			, RA_ACCT_CD
			, STND_COST_CD
			, COST_ASS_BSE_CD
			, SVC_TRNS_PRC_AMT
			, BKG_KNT
			, CNTR_BX_KNT
			, CNTR_TEU_QTY
			, VSL_DYS
			, COM_FLG
			, COST_CALC_RMK
			, @[upd_usr_id]
			, SYSDATE
			, @[upd_usr_id]
			, SYSDATE			
		FROM MAS_COM_SVC_TRNS_PRC_PARA
		WHERE PCTL_NO = @[pctl_no]			]]></sql>
			<params>
				<param name="rout_cs_no" type="12" value="" out="N"/>
				<param name="rout_cs_src_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
