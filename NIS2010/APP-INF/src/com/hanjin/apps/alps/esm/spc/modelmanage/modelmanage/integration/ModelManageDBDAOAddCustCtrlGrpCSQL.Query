<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ModelManageDBDAOAddCustCtrlGrpCSQL">
			<desc><![CDATA[season에서 사용할 control group을 저장합니다.

2014.01.03 진마리아 [SRM-201341166] Yield Group의 확대]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_MDL_CUST_CTRL_GRP (
    TRD_CD
  , COST_YRWK
  , CUST_CTRL_CD
  , CUST_CTRL_DESC
  , CRE_USR_ID
  , CRE_DT
  , UPD_USR_ID
  , UPD_DT
) VALUES (
    @[trd_cd]
  , @[cost_yrwk]
  , @[cust_ctrl_cd]||@[cust_ctrl_sub_cd]
  , @[cust_ctrl_desc]
  , @[cre_usr_id]
  , SYSDATE
  , @[cre_usr_id]
  , SYSDATE
)			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="cost_yrwk" type="12" value="" out="N"/>
				<param name="cust_ctrl_cd" type="12" value="" out="N"/>
				<param name="cust_ctrl_sub_cd" type="12" value="" out="N"/>
				<param name="cust_ctrl_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
