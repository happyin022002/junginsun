<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupCommodityProposalDBDAOPriSpScpGrpCmdtAmdVOCSQL">
			<desc><![CDATA[SCGroupCommodityProposalDB PriSpScpGrpCmdtDtlAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_GRP_CMDT(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    GRP_CMDT_SEQ,
    PRC_GRP_CMDT_CD,
    PRC_GRP_CMDT_DESC,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
SELECT
    PROP_NO             ,
    AMDT_SEQ+1          ,
    SVC_SCP_CD          ,
    GRP_CMDT_SEQ        ,
    PRC_GRP_CMDT_CD     ,
    PRC_GRP_CMDT_DESC   ,
    @[cre_usr_id]       ,
    SYSDATE             ,
    @[upd_usr_id]       ,
    SYSDATE
FROM     
    PRI_SP_SCP_GRP_CMDT CMDT
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND EXISTS (
	SELECT 'OK'
	FROM PRI_SP_SCP_GRP_CMDT_DTL
	WHERE
		PROP_NO 		= CMDT.PROP_NO
	AND AMDT_SEQ 		= CMDT.AMDT_SEQ
	AND SVC_SCP_CD  	= CMDT.SVC_SCP_CD
	AND GRP_CMDT_SEQ	= CMDT.GRP_CMDT_SEQ
    AND SRC_INFO_CD     <> 'AD'
)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
