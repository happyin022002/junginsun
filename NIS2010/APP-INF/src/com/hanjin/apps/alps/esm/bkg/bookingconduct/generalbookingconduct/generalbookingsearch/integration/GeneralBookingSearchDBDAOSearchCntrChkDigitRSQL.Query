<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchCntrChkDigitRSQL">
			<desc><![CDATA[SearchCntrChkDigit]]></desc>
			<sql><![CDATA[
select max(substr(cntr_no, 11, 1)) chk_digit
	, CNTR_TPSZ_CD
	, CNMV_STS_CD
    , pre_sts_flg
  from mst_container
 where cntr_no like @[cntr_no]||'%'
and ACIAC_DIV_CD <> 'I'
--   and cnmv_sts_Cd <> 'XX'    
 group by CNTR_TPSZ_CD, CNMV_STS_CD, pre_sts_flg			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
