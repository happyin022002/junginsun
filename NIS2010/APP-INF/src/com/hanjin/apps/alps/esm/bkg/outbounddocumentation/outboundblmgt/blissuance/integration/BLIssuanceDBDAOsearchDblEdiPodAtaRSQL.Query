<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiPodAtaRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT 'POD_ATA:' || NVL(TO_CHAR(DECODE(C.PORT_SKD_STS_CD,'D',C.VPS_ETA_DT, (SELECT ACT_ARR_DT 
                                                                              FROM VSK_ACT_PORT_SKD
                                                                             WHERE C.VSL_CD = VSL_CD
                                                                               AND NVL(C.TURN_SKD_VOY_NO,C.SKD_VOY_NO) = SKD_VOY_NO
                                                                               AND NVL(C.TURN_SKD_DIR_CD,C.SKD_DIR_CD) = SKD_DIR_CD
                                                                               AND C.VPS_PORT_CD = VPS_PORT_CD
                                                                               AND NVL(C.TURN_CLPT_IND_SEQ,C.CLPT_IND_SEQ) = CLPT_IND_SEQ)), 'YYYYMMDDHH24MI'), ' ') || CHR(10)
FROM   BKG_VVD A, BKG_BOOKING B,VSK_VSL_PORT_SKD C
WHERE  A.BKG_NO = @[bkg_no]
   AND A.BKG_NO = B.BKG_NO
   AND A.POD_CD = B.POD_CD
   AND C.VSL_CD = A.VSL_CD
   AND C.SKD_VOY_NO = A.SKD_VOY_NO
   AND C.SKD_DIR_CD = A.SKD_DIR_CD
   AND C.VPS_PORT_CD = A.POD_CD
   AND C.CLPT_IND_SEQ = A.POD_CLPT_IND_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
