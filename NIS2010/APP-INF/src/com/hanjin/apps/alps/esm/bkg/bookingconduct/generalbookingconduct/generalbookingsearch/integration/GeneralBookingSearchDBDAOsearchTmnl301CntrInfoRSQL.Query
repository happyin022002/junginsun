<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchTmnl301CntrInfoRSQL">
			<desc><![CDATA[searchTmnl301CntrInfo]]></desc>
			<sql><![CDATA[
SELECT '{CNTR_INFO'																|| CHR(10)
	|| 'CNTN:'					|| RPAD(NVL(CNTR.CNTR_NO, ' '), 11, ' ')		|| CHR(10)
	|| 'CNTT:'					|| CNTR.CNTR_TPSZ_CD							|| CHR(10)
	|| 'SEAL:'					|| REPLACE(REPLACE(REPLACE(NVL(
		(  SELECT NVL(MIN(CNTR_SEAL_NO),' ') 
		   FROM BKG_CNTR_SEAL_NO
		   WHERE BKG_NO = CNTR.BKG_NO
		   AND CNTR_NO = CNTR.CNTR_NO
		   ),' '),CHR(32),''),CHR(10),''),CHR(13),'')	|| CHR(10)
	|| 'RIND:'                  || decode(CNTR.RC_FLG,'Y','1','0')				|| CHR(10)
	|| 'DIND:'                  || decode(CNTR.DCGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'AIND:'                  || decode(CNTR.AWK_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'BIND:'                  || decode(CNTR.BB_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'WGT_QTY:'               || CNTR.CNTR_WGT								|| CHR(10)
	|| 'WGT_TP:'                || CNTR.WGT_UT_CD								|| CHR(10)
	|| 'VGM_VRF_DATE:'          || TO_CHAR(CNTR.VGM_VRFY_DT,'RRRRMMDDHH24MI')   || CHR(10)
	|| 'VGM_WGTQTY:'            || CNTR.VGM_WGT                                 || CHR(10)
	|| 'VGM_WGTCD:'             || CNTR.VGM_WGT_UT_CD                           || CHR(10)
	|| 'VGM_METHOD:'            || CNTR.VGM_MZD_TP_CD                           || CHR(10)
	|| 'VGM_SIGNATURE:'         || REPLACE(REPLACE(REPLACE(CNTR.VGM_VRFY_SIG_CTNT,CHR(13)||CHR(10),' '),CHR(13),' '),CHR(10),' ')  || CHR(10)
	|| 'TEMP:'																	|| CHR(10)
	|| 'TUN:'																	|| CHR(10)
	|| 'TEMP_C:'																|| CHR(10)
	|| 'TUN_C:'																	|| CHR(10)
	|| 'RF_VOLTAGE:'															|| CHR(10)
	|| 'VENT:'																	|| CHR(10)
	|| 'VENT_NUM:'																|| CHR(10)
	|| 'VENT_CMH:'																|| CHR(10)
	|| 'HUMID:'																	|| CHR(10)
	|| 'GENSET:'																|| CHR(10)
	|| 'RF_REMARK:'																|| CHR(10)
	|| 'RFDRY_IND:'				|| decode(CNTR.RD_CGO_FLG,'Y','1','0')			|| CHR(10)
	|| 'RF_DRAIN:'																|| CHR(10)
	|| 'OVF:'																	|| CHR(10)
	|| 'OVR:'																	|| CHR(10)
	|| 'OVH:'																	|| CHR(10)
	|| 'OVLW:'																	|| CHR(10)
	|| 'OVRW:'																	|| CHR(10)
	|| 'OVWGT:'																	|| CHR(10)
	|| 'VOID_SLOT:'																|| CHR(10)
	|| 'STWG_REQ:'																|| CHR(10)
	|| 'TTL_DIM_LEN:'															|| CHR(10)
	|| 'TTL_DIM_WDT:'															|| CHR(10)
	|| 'TTL_DIM_HGT:'															|| CHR(10)
	|| 'TRM_TYPE:'																|| CHR(10)
	|| 'TRM_WEIGHT:'															|| CHR(10)
	|| 'TRM_HAULAGE:'															|| CHR(10)
	|| 'TRM_HMODE:'																|| CHR(10)
	|| 'TRM_PICKUP_CY:'															|| CHR(10)
	|| 'TRM_RETURN_CY:'															|| CHR(10)
	|| 'TRM_INSTRUCTION:'														|| CHR(10)
	|| 'TRM_TRAN_DT:'															|| CHR(10)
	|| 'TRM_TRAN_OFFICE:'														|| CHR(10)
	|| 'TRM_TRAN_NO:'															|| CHR(10)
	|| 'USR_ID:'																|| CHR(10)
	|| '{CNTR_TRO_DTL'															|| CHR(10)
	|| 'TRD_ADDR:'																|| CHR(10)
	|| 'TRD_DOOR_LOC:'															|| CHR(10)
	|| 'TRD_DOOR_POSTAL:'														|| CHR(10)
	|| 'TRD_DOOR_DT:'															|| CHR(10)
	|| 'TRD_PERSON:'															|| CHR(10)
	|| 'TRD_TEL:'																|| CHR(10)
	|| 'TRD_FAX:'																|| CHR(10)
	|| 'TRD_DEPARTURE:'															|| CHR(10)
	|| '}CNTR_TRO_DTL'															|| CHR(10)
	|| '}CNTR_INFO'																|| CHR(10) CNTR_INFO
  FROM BKG_BOOKING BK
        , BKG_CONTAINER CNTR
--        , BKG_CNTR_SEAL_NO SEAL
 WHERE BK.BKG_NO   	    = CNTR.BKG_NO
--   AND CNTR.BKG_NO      = SEAL.BKG_NO       (+)
--   AND CNTR.CNTR_NO     = SEAL.CNTR_NO      (+)
--   AND 1                = SEAL.CNTR_SEAL_SEQ(+)
   AND CNTR.DCGO_FLG    = 'N'
   AND CNTR.RC_FLG      = 'N'
   AND CNTR.AWK_CGO_FLG = 'N'
   AND BK.BKG_NO        = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
