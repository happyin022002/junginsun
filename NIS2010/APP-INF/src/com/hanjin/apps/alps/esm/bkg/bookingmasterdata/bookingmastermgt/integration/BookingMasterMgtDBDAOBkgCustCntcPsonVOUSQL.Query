<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOBkgCustCntcPsonVOUSQL">
			<desc><![CDATA[BookingMasterMgtDBDAOBkgCustCntcPsonVO]]></desc>
			<sql><![CDATA[
update bkg_cust_cntc_pson set 
	bkg_cntc_pson_tp_cd = @[bkg_cntc_pson_tp_cd],
	cntc_pson_nm = @[cntc_pson_nm],
	cntc_eml = @[cntc_eml],
	phn_no = @[phn_no],
    CNTC_PSON_MPHN_NO = @[cntc_pson_mphn_no],
	fax_no = @[fax_no],
	diff_rmk = @[diff_rmk],
	fwrd_cnt_cd = substr(@[fwrd_cnt_cd],1,2),
	fwrd_cust_seq = substr(@[fwrd_cnt_cd],3,6),
	upd_usr_id = @[upd_usr_id],
	upd_dt = sysdate
where	cust_cnt_cd = @[cust_cnt_cd]
and	cust_seq = @[cust_seq]
and	cntc_pson_seq = @[cntc_pson_seq]			]]></sql>
			<params>
				<param name="bkg_cntc_pson_tp_cd" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="cntc_pson_mphn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="fwrd_cnt_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="cntc_pson_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
