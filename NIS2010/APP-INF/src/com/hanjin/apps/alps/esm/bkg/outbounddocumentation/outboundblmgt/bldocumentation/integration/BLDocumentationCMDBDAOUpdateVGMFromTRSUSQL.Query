<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOUpdateVGMFromTRSUSQL">
			<desc><![CDATA[TRS --> BKG 로 VGM 업데이트]]></desc>
			<sql><![CDATA[
UPDATE BKG_CONTAINER
SET  VGM_WGT           = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN TO_NUMBER(@[vgm_wgt]) ELSE VGM_WGT END) 
    ,VGM_WGT_UT_CD     = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN DECODE(@[vgm_wgt_ut_cd], 'KGM', 'KGS','LBR','LBS', @[vgm_wgt_ut_cd]) ELSE VGM_WGT_UT_CD END)
    ,VGM_VRFY_SIG_CTNT = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN 'SML on behalf of Shipper' ELSE VGM_VRFY_SIG_CTNT END)
    ,UPD_USR_ID        = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN @[upd_usr_id] ELSE UPD_USR_ID END)
    ,UPD_DT            = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN SYSDATE ELSE UPD_DT END)
WHERE BKG_NO = @[bkg_no]
AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="vgm_wgt" type="12" value="" out="N"/>
				<param name="vgm_wgt_ut_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
