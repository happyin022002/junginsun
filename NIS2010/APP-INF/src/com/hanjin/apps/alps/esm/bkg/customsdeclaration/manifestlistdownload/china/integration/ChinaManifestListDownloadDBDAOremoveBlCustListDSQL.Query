<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOremoveBlCustListDSQL">
			<desc><![CDATA[removeBlCustList]]></desc>
			<sql><![CDATA[
DELETE	FROM BKG_CSTMS_CHN_CUST
WHERE 1=1
AND (#foreach($field_id in ${field_list}) 
     	#if($velocityCount > 1)
        OR #end      BL_NO IN ( $field_id )
     #end
)
AND	    CHN_MF_SND_IND_CD =	@[trans_mode]			]]></sql>
			<params>
				<param name="trans_mode" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
