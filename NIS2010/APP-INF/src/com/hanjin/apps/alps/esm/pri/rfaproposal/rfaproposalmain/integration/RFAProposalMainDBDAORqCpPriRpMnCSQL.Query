<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAORqCpPriRpMnCSQL">
			<desc><![CDATA[RqCpPriRpMn]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_MN (
    PROP_NO
    ,AMDT_SEQ
    ,EFF_DT
    ,EXP_DT
    ,CTRT_CUST_CNT_CD
    ,CTRT_CUST_SEQ
    ,PRC_CTRT_CUST_TP_CD
    ,CTRT_CUST_VAL_SGM_CD
    ,PROP_SREP_CD
    ,PROP_OFC_CD
    ,PROP_APRO_OFC_CD
    ,PROP_APRO_DT
    ,PROP_STS_CD
    ,RESPB_SREP_CD
    ,RESPB_SLS_OFC_CD
    ,CNTR_LOD_UT_CD
    ,TGT_MVC_QTY
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT)
WITH CUST AS (
SELECT
    CUST.CUST_CNT_CD ,
    CUST.CUST_SEQ    ,
    CUST.RVIS_CNTR_CUST_TP_CD	AS PRC_CTRT_CUST_TP_CD  ,
    CUST.VBS_CLSS_CD		AS CTRT_CUST_VAL_SGM_CD                ,
    MDM.INTG_CD_VAL_DP_DESC	AS CTRT_CUST_VAL_SGM            ,
    CUST.SREP_CD		AS RESPB_SREP_CD                       ,
    CUST.OFC_CD			AS RESPB_SLS_OFC_CD                   
FROM
    MDM_CUSTOMER CUST   ,
    MDM_CUST_ADDR ADDR  ,
    COM_INTG_CD_DTL MDM ,
    MDM_SLS_REP SREP    ,
    COM_INTG_CD_DTL CUST_TY,
    PRI_RQ_MN MN
WHERE
CUST.CUST_CNT_CD    = MN.CUST_CNT_CD
AND CUST.CUST_SEQ   = MN.CUST_SEQ
AND CUST.DELT_FLG   = 'N'
AND CUST.CUST_CNT_CD        = ADDR.CUST_CNT_CD(+)
AND CUST.CUST_SEQ           = ADDR.CUST_SEQ(+)
AND ADDR.PRMRY_CHK_FLG      = 'Y'
AND MDM.INTG_CD_ID(+)          = 'CD00698'
AND MDM.INTG_CD_VAL_CTNT(+)    = CUST.VBS_CLSS_CD
AND CUST.SREP_CD    = SREP.SREP_CD
AND CUST_TY.INTG_CD_ID      = 'CD00697'
AND CUST.RVIS_CNTR_CUST_TP_CD = CUST_TY.INTG_CD_VAL_CTNT 
AND MN.QTTN_NO = @[qttn_no]
AND MN.QTTN_VER_NO = @[qttn_ver_no]
)
SELECT   @[new_prop_no] AS PROP_NO
    ,0 AS AMDT_SEQ
    ,B.EFF_DT
    ,B.EXP_DT
    ,B.CUST_CNT_CD
    ,B.CUST_SEQ
    ,B.PRC_CUST_TP_CD
    ,C.CTRT_CUST_VAL_SGM_CD
    ,B.QTTN_SREP_CD
    ,@[qttn_ofc_cd]
    ,NULL
    ,NULL
    ,'I'
    ,C.RESPB_SREP_CD
    ,C.RESPB_SLS_OFC_CD
    ,B.CNTR_LOD_UT_CD
    ,B.ESTM_MQC_QTY
    ,@[cre_usr_id]
    ,SYSDATE
    ,@[upd_usr_id]
    ,SYSDATE
FROM	PRI_RQ_HDR A
	,	PRI_RQ_MN B
	,	CUST C
WHERE	A.QTTN_NO = B.QTTN_NO
AND	B.QTTN_NO = @[qttn_no] 
AND	B.QTTN_VER_NO = @[qttn_ver_no]
AND	B.CUST_CNT_CD = C.CUST_CNT_CD
AND	B.CUST_SEQ = C.CUST_SEQ			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="qttn_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
