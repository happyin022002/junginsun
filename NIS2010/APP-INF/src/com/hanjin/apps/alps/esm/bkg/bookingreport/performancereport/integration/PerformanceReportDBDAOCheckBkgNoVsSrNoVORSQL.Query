<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOCheckBkgNoVsSrNoVORSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
SELECT BK.BKG_NO
	   ,BK.VSL_CD || BK.SKD_VOY_NO || BK.SKD_DIR_CD AS VVD
	   ,BK.POL_CD
	   ,BK.POD_CD		
       ,SR_NO
       ,DECODE(BK.SPLIT_FLG, 'Y', 'S',
              DECODE(ISS.OBL_ISS_FLG, 'Y', 'B', 
				DECODE(NVL(SR.SR_NO,'N'), 'N', 'N', 'A'))) AS CHK_VAL
FROM   BKG_BOOKING      BK
      ,BKG_SR_CRNT_RQST SR
      ,BKG_BL_ISS       ISS
WHERE  1=1
AND    BK.BKG_NO =  @[bkg_no]
AND    BK.BKG_NO = SR.BKG_NO(+) 
AND    BK.BKG_STS_CD <> 'X' 
AND    BK.BKG_NO = ISS.BKG_NO(+) 
AND    SR_NO(+) !=  @[sr_no]
AND    ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
