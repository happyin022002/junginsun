<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RussiaManifestDownloadDBDAOsearchBkgListForFdrBlRSQL">
			<desc><![CDATA[searchBkgListForFdrBl]]></desc>
			<sql><![CDATA[
SELECT
#if ((''==${vsl_cd} || ''==${skd_voy_no} || ''==${skd_dir_cd}) && (${bkg_no} == '' && ${bl_no} == '') && ('' !=${bl_obrd_dt_from} && '' !=${bl_obrd_dt_to}))
       /*+ INDEX(DOC XAK2BKG_BL_DOC) */
#end
       BKG.BKG_NO,
       BKG.BKG_STS_CD,
       BKG.BL_NO || BKG.BL_TP_CD AS BL_NO,
       CST.CUST_CNT_CD || LPAD (CST.CUST_SEQ, 6, 0) AS SHPR_CD,
       NVL(RUS.CUST_NM, CST.CUST_NM) AS SHPR_NM,
       NVL(RUS.CUST_ADDR, CST.CUST_ADDR) AS SHPR_ADDR,
       CNE.CUST_CNT_CD || LPAD (CNE.CUST_SEQ, 6, 0) AS CNEE_CD,
       NVL(RUC.CUST_NM, CNE.CUST_NM) AS CNEE_NM,
       NVL(RUC.CUST_ADDR, CNE.CUST_ADDR) AS CNEE_ADDR,
       NFT.CUST_CNT_CD || LPAD (NFT.CUST_SEQ, 6, 0) AS NTFY_CD,
       NVL(RUN.CUST_NM, NFT.CUST_NM) AS NTFY_NM,
       NVL(RUN.CUST_ADDR, NFT.CUST_ADDR) AS NTFY_ADDR,
       NVL(RUE.CUST_NM, EXC.CUST_NM) AS EX_CUST_NM,
       BKG.VSL_CD || BKG.SKD_VOY_NO || BKG.SKD_DIR_CD AS VVD,
       BKG.POL_CD,
       BKG.POD_CD,
       BKG.POR_CD,
       BKG.DEL_CD

  FROM BKG_BOOKING BKG
       #if (''!=${vsl_cd} || ''!=${skd_voy_no} || ''!=${skd_dir_cd})
       ,BKG_VVD VVD
       #end
       ,BKG_CUSTOMER CST
       ,BKG_CUSTOMER CNE
       ,BKG_CUSTOMER NFT
       ,BKG_CUSTOMER EXC
       ,BKG_BL_ISS ISS
       ,BKG_BL_DOC DOC
       ,BKG_CSTMS_RU_CUST RUS
       ,BKG_CSTMS_RU_CUST RUC
       ,BKG_CSTMS_RU_CUST RUN
       ,BKG_CSTMS_RU_CUST RUE
       #if ('All'!=${bkg_cust_tp_cd} || ''!=${cust_cnt_cd} || ''!=${cust_seq} || ''!=${cust_nm})
       ,BKG_CUSTOMER CST2
       #end

 WHERE BKG.BKG_NO = CST.BKG_NO(+)
   AND 'S' = CST.BKG_CUST_TP_CD(+)
   AND BKG.BKG_NO = CNE.BKG_NO(+)
   AND 'C' = CNE.BKG_CUST_TP_CD(+)
   AND BKG.BKG_NO = NFT.BKG_NO(+)
   AND 'N' = NFT.BKG_CUST_TP_CD(+) 
   AND BKG.BKG_NO = EXC.BKG_NO(+)
   AND 'E' = EXC.BKG_CUST_TP_CD(+) 
   AND BKG.BL_NO = RUS.BL_NO(+)
   AND 'S' = RUS.BKG_CUST_TP_CD(+)
   AND BKG.BL_NO = RUC.BL_NO(+)
   AND 'C' = RUC.BKG_CUST_TP_CD(+)
   AND BKG.BL_NO = RUN.BL_NO(+)
   AND 'N' = RUN.BKG_CUST_TP_CD(+)  
   AND BKG.BL_NO = RUE.BL_NO(+)
   AND 'E' = RUE.BKG_CUST_TP_CD(+)
   AND BKG.BKG_NO = ISS.BKG_NO(+)
   AND BKG.BKG_NO = DOC.BKG_NO(+)
   AND BKG.BKG_STS_CD NOT IN ('A', 'X')

#if ('F'==${tp_cd})
       AND BKG.BKG_CGO_TP_CD = 'F'
#else
       AND BKG.BKG_CGO_TP_CD = 'P'
#end

#if (''!=${vsl_cd} || ''!=${skd_voy_no} || ''!=${skd_dir_cd})
   AND BKG.BKG_NO = VVD.BKG_NO
   #if (''!=${vsl_cd})
   AND @[vsl_cd] = VVD.VSL_CD  /*VVD*/
   #end
   #if (''!=${skd_voy_no})
   AND @[skd_voy_no] = VVD.SKD_VOY_NO  /*VVD*/
   #end
   #if (''!=${skd_dir_cd})
   AND @[skd_dir_cd] = VVD.SKD_DIR_CD  /*VVD*/
   #end
#end
#if ('All'!=${bkg_cust_tp_cd} || ''!=${cust_cnt_cd} || ''!=${cust_seq} || ''!=${cust_nm})
   AND CST2.BKG_NO = BKG.BKG_NO
   #if ('All'!=${bkg_cust_tp_cd})
   AND @[bkg_cust_tp_cd] = CST2.BKG_CUST_TP_CD  /*BKG_CUST_TP_CD*/
   #end
   #if (''!=${cust_cnt_cd})
   AND @[cust_cnt_cd] = CST2.CUST_CNT_CD  /*CUST_CNT_CD*/
   #end
   #if (''!=${cust_seq})
   AND CST2.CUST_SEQ = TO_NUMBER(@[cust_seq])  /*CUST_SEQ*/
   #end
   #if (''!=${cust_nm})
   AND UPPER(CST2.CUST_NM) LIKE UPPER(@[cust_nm])||'%'  /*CUST_NM*/
   #end
#end


   #if (''!=${pol_cd})
   AND VVD.POL_CD LIKE @[pol_cd]||'%'  /*POL_CD*/
   #end
   #if (''!=${pod_cd})
   AND VVD.POD_CD LIKE @[pod_cd]||'%'  /*POD_CD*/
   #end
   #if (''!=${ob_sls_ofc_cd})
   AND UPPER(BKG.OB_SLS_OFC_CD) LIKE UPPER(@[ob_sls_ofc_cd])||'%'  /*OB_SLS_OFC_CD*/
   #end
   #if (''!=${ob_srep_cd})
   AND UPPER(BKG.OB_SREP_CD) LIKE UPPER(@[ob_srep_cd])||'%'  /*OB_SREP_CD*/
   #end
   #if (''!=${bkg_no})
   AND @[bkg_no] = BKG.BKG_NO  /*BKG_NO*/
   #end
   #if (''!=${bl_no})
   AND @[bl_no] = BKG.BL_NO  /*BL_NO*/
   #end
   #if (''!=${bl_obrd_dt_from} && ''!=${bl_obrd_dt_to})
   AND DOC.BL_OBRD_DT BETWEEN TO_DATE(@[bl_obrd_dt_from],'RRRRMMDD') AND TO_DATE(@[bl_obrd_dt_to],'RRRRMMDD')  /*BL_OBRD_DT*/
   #elseif (''!=${obl_iss_dt_from} && ''!=${obl_iss_dt_to})
   AND ISS.OBL_ISS_DT BETWEEN TO_DATE(@[obl_iss_dt_from],'RRRRMMDD') AND TO_DATE(@[obl_iss_dt_to],'RRRRMMDD')  /*OBL_ISS_DT*/
   #end

ORDER BY BKG.BKG_NO			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="ob_sls_ofc_cd" type="12" value="" out="N"/>
				<param name="ob_srep_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_obrd_dt_from" type="12" value="" out="N"/>
				<param name="bl_obrd_dt_to" type="12" value="" out="N"/>
				<param name="obl_iss_dt_from" type="12" value="" out="N"/>
				<param name="obl_iss_dt_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
