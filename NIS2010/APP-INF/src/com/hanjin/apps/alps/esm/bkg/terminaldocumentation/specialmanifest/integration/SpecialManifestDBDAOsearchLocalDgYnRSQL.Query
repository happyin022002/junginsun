<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchLocalDgYnRSQL">
			<desc><![CDATA[VVD와 Port를 가지고 구주위험물쪽에 데이타가 있는지 조회한다. ('0' : 없음, 0이상 : 있음)]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*), 0, 'N', 'Y') LOCAL_SAVE_YN
FROM BKG_CSTMS_EUR_DG 
WHERE VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
AND   SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
AND   SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
AND   PORT_CD     = @[port_cd]
AND   EUR_DG_DECL_TP_CD = @[d_type] -- E:Export,I:Import,T:Transit


			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="d_type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
