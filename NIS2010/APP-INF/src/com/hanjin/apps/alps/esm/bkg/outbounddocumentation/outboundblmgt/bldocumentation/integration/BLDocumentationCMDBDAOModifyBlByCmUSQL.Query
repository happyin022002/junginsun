<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOModifyBlByCmUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
UPDATE BKG_BL_DOC_HIS
SET PCK_QTY  = @[pck_qty]
,   PCK_TP_CD = NVL(PCK_TP_CD, @[pck_tp_cd])
,   ACT_WGT = @[act_wgt]
,   MEAS_QTY = @[meas_qty]
WHERE	BKG_NO  = @[bkg_no] 
AND    CORR_NO = 'TMP0000001'
#else 
UPDATE BKG_BL_DOC 
SET PCK_QTY  = @[pck_qty]
,   PCK_TP_CD = NVL(PCK_TP_CD, @[pck_tp_cd])
,   ACT_WGT = @[act_wgt]
,   MEAS_QTY = @[meas_qty]
WHERE	BKG_NO  = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="act_wgt" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
