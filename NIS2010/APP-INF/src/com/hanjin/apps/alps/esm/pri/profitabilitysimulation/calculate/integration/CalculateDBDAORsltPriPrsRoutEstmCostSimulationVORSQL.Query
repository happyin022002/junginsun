<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CalculateDBDAORsltPriPrsRoutEstmCostSimulationVORSQL">
			<desc><![CDATA[Used Route CASE Info]]></desc>
			<sql><![CDATA[
SELECT
			ROUT_CS_NO
			, @[rout_cs_clss_no] AS ROUT_CS_CLSS_NO
			, COA_COST_SRC_CD
			, COM_ROUT_SEQ
			, LOC_CD
			, COND_OFC_CD
			, DIV_MEAS_CD
			, OFC_CLSS_CD
			, SLS_ACT_CD
			, RA_ACCT_CD
			, STND_COST_CD
			, COST_ASS_BSE_CD
			, SVC_TRNS_PRC_AMT
			, BKG_KNT
			, CNTR_BX_KNT
			, CNTR_TEU_QTY
			, VSL_DYS
			, COM_FLG
			, COST_CALC_RMK	
	FROM PRI_PRS_USD_ROUT_ESTM_COST 
	WHERE ROUT_CS_NO = @[rout_cs_no]
		AND ROUT_CS_SRC_DT = @[rout_cs_src_dt]			]]></sql>
			<params>
				<param name="rout_cs_clss_no" type="12" value="" out="N"/>
				<param name="rout_cs_no" type="12" value="" out="N"/>
				<param name="rout_cs_src_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
