<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMSummaryDBDAORsltPriAuthorizationVORSQL">
			<desc><![CDATA[권한을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
		PRC_CTRT_TP_CD
	,	SVC_SCP_CD
	,	USR_ID
	,	EFF_DT
	,	EXP_DT
	,	CRE_USR_ID
	,	CRE_DT
	,	UPD_USR_ID
	,	UPD_DT
FROM PRI_AUTHORIZATION
WHERE	PRC_CTRT_TP_CD = 'S'
	AND	USR_ID = @[usr_id]
	AND TO_CHAR(SYSDATE, 'YYYYMMDD') BETWEEN TO_CHAR(EFF_DT, 'YYYYMMDD') AND TO_CHAR(EXP_DT, 'YYYYMMDD')
	AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
