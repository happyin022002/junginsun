<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtGRICancelForIHCUSQL">
			<desc><![CDATA[Rate Calcurate cancel]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_RT A
   SET A.PROP_FRT_RT_AMT        = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PROP_FRT_RT_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PROP_FRT_RT_AMT - A.GRI_APPL_AMT)
      ,A.COFFR_FRT_RT_AMT       = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT COFFR_FRT_RT_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.COFFR_FRT_RT_AMT)
      ,A.FNL_FRT_RT_AMT         = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT FNL_FRT_RT_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.FNL_FRT_RT_AMT)
      ,A.PRS_SCG_AMT            = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_SCG_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PRS_SCG_AMT)
      ,A.PRS_RESPB_CM_UC_AMT    = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_RESPB_CM_UC_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PRS_RESPB_CM_UC_AMT)
      ,A.PRS_PFIT_CM_UC_AMT     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_PFIT_CM_UC_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PRS_PFIT_CM_UC_AMT)
      ,A.PRS_RESPB_OPFIT_UC_AMT = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_RESPB_OPFIT_UC_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PRS_RESPB_OPFIT_UC_AMT)
      ,A.PRS_RESPB_CMPB_AMT     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_RESPB_CMPB_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PROP_FRT_RT_AMT - A.GRI_APPL_AMT + A.PRS_SCG_AMT - A.PRS_RESPB_CM_UC_AMT)
      ,A.PRS_PFIT_CMPB_AMT      = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_PFIT_CMPB_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PROP_FRT_RT_AMT - A.GRI_APPL_AMT + A.PRS_SCG_AMT - A.PRS_PFIT_CM_UC_AMT)
      ,A.PRS_RESPB_OPB_AMT      = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_RESPB_OPB_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PROP_FRT_RT_AMT - A.GRI_APPL_AMT + A.PRS_SCG_AMT - A.PRS_RESPB_OPFIT_UC_AMT)
      ,A.PRS_GID_CMPB_AMT       = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_GID_CMPB_AMT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PRS_GID_CMPB_AMT)
      ,A.PRS_UPD_DT             = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT PRS_UPD_DT
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.PRS_UPD_DT)
      ,A.GRI_APPL_TP_CD         = 'N'
      ,A.GRI_APPL_AMT           = 0
      ,A.PRC_PROG_STS_CD        = DECODE(A.SRC_INFO_CD, 'AM', 'A', 'I')
      ,A.SRC_INFO_CD            = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT SRC_INFO_CD
                                           FROM PRI_RP_SCP_RT
                                          WHERE PROP_NO = A.PROP_NO
                                            AND AMDT_SEQ = A.AMDT_SEQ - 1
                                            AND SVC_SCP_CD = A.SVC_SCP_CD
                                            AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                            AND ROUT_SEQ = A.ROUT_SEQ
                                            AND RT_SEQ = A.RT_SEQ
                                            AND ROWNUM = 1)
                                        ,A.SRC_INFO_CD)
      ,A.N1ST_CMNC_AMDT_SEQ     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT N1ST_CMNC_AMDT_SEQ
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.N1ST_CMNC_AMDT_SEQ)
      ,A.FIC_GLINE_RT_AMT     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT FIC_GLINE_RT_AMT
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.FIC_GLINE_RT_AMT)
      ,A.FIC_PROP_RT_AMT     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT FIC_PROP_RT_AMT
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.FIC_PROP_RT_AMT)       
      ,A.FIC_COFFR_RT_AMT     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT FIC_COFFR_RT_AMT
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.FIC_COFFR_RT_AMT)  
       ,A.FIC_FNL_RT_AMT     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT FIC_FNL_RT_AMT
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.FIC_FNL_RT_AMT)   
        ,A.FIC_GLINE_UPD_DT     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT FIC_GLINE_UPD_DT
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.FIC_GLINE_UPD_DT)   
        ,A.OPTM_TRSP_MOD_FLG     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT OPTM_TRSP_MOD_FLG
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.OPTM_TRSP_MOD_FLG)      
        ,A.FIC_RT_USE_STS_CD     = DECODE(A.SRC_INFO_CD
                                        ,'AM'
                                        ,(SELECT FIC_RT_USE_STS_CD
                                            FROM PRI_RP_SCP_RT
                                           WHERE PROP_NO = A.PROP_NO
                                             AND AMDT_SEQ = A.AMDT_SEQ - 1
                                             AND SVC_SCP_CD = A.SVC_SCP_CD
                                             AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                                             AND ROUT_SEQ = A.ROUT_SEQ
                                             AND RT_SEQ = A.RT_SEQ
                                             AND ROWNUM = 1)
                                        ,A.FIC_RT_USE_STS_CD)                   
      ,A.UPD_USR_ID = @[upd_usr_id]
      ,A.UPD_DT = SYSDATE
 WHERE A.PROP_NO = @[prop_no]
   AND A.AMDT_SEQ = @[amdt_seq]
   AND A.SVC_SCP_CD = @[svc_scp_cd]
   AND EXISTS (SELECT 'OK' FROM PRI_RP_SCP_RT_CMDT_HDR  X1 WHERE X1.PROP_NO = A.PROP_NO AND X1.AMDT_SEQ = A.AMDT_SEQ AND X1.SVC_SCP_CD = A.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ AND NVL(X1.FIC_RT_TP_CD, 'G') = NVL(@[gen_spcl_rt_tp_cd], 'G'))
   AND A.SRC_INFO_CD <> 'AD'
   AND A.GRI_APPL_TP_CD = 'A'
#if(${v_param} != '')  
   AND NOT EXISTS(
                WITH XX AS(
#foreach($key IN ${v_param}) 
                   SELECT REGEXP_SUBSTR('$key', '[^|]+', 1,1) PROP_NO, REGEXP_SUBSTR('$key', '[^|]+', 1,2) AMDT_SEQ, REGEXP_SUBSTR('$key', '[^|]+', 1,3) SVC_SCP_CD, REGEXP_SUBSTR('$key', '[^|]+', 1, 4) CMDT_HDR_SEQ, REGEXP_SUBSTR('$key', '[^|]+', 1, 5) ROUT_SEQ, REGEXP_SUBSTR('$key', '[^|]+', 1, 6) RT_SEQ FROM DUAL
             #if($velocityCount < $v_param.size()) 
       UNION ALL
       #end
#end 
                   ) 
                   SELECT 1 FROM XX
                          WHERE XX.PROP_NO = A.PROP_NO
                             AND XX.AMDT_SEQ = A.AMDT_SEQ
                             AND XX.SVC_SCP_CD = A.SVC_SCP_CD
                             AND XX.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
                             AND XX.ROUT_SEQ = A.ROUT_SEQ
                             AND XX.RT_SEQ = A.RT_SEQ
              )
#end			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
