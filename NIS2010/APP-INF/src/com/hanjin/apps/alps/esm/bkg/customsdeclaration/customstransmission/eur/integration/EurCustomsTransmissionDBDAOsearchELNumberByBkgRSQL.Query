<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchELNumberByBkgRSQL">
			<desc><![CDATA[EL Number를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
     @[mrn_no] || @[msncfm] ELNO 
    ,NVL(BBD.PCK_QTY, 0)    ELPK
    ,NVL(BBD.PCK_TP_CD, '') ELPKU
    ,NVL(BBD.ACT_WGT, 0)    ELWT
    ,NVL(BBD.WGT_UT_CD, '') ELWTU
FROM BKG_BOOKING BB
    ,BKG_BL_DOC BBD
WHERE BB.BKG_NO = BBD.BKG_NO 
AND   BB.BKG_NO = @[bkg_no]
AND   @[bnd_cd] = 'O'
AND   @[blts] = 'R'
AND   @[bltp] = 'C'

UNION ALL

SELECT
    XPT_LIC_NO          ELNO 
    ,NVL(PCK_QTY, 0)    ELPK
    ,NVL(PCK_TP_CD, '') ELPKU
    ,NVL(MF_WGT, 0)     ELWT
    ,NVL(WGT_UT_CD, '') ELWTU
FROM BKG_XPT_IMP_LIC
WHERE BKG_NO = @[bkg_no]
AND   IO_BND_CD = 'O'
AND   @[bnd_cd] = 'O'
AND   @[blts] = 'E'
AND   @[bltp] = 'C'			]]></sql>
			<params>
				<param name="mrn_no" type="12" value="" out="N"/>
				<param name="msncfm" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bnd_cd" type="12" value="" out="N"/>
				<param name="blts" type="12" value="" out="N"/>
				<param name="bltp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
