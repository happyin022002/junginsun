<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAOPriRqRtCmdtRoutVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE PRI_RQ_RT_CMDT_ROUT SET 
	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,	ORG_CY_DOR_RT_TP_CD = @[org_cy_dor_rt_tp_cd]
,	DEST_CY_DOR_RT_TP_CD = @[dest_cy_dor_rt_tp_cd]
WHERE	QTTN_NO = @[qttn_no]
AND	QTTN_VER_NO = @[qttn_ver_no]
AND	CMDT_HDR_SEQ = @[cmdt_hdr_seq]
AND	ROUT_SEQ = @[rout_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="org_cy_dor_rt_tp_cd" type="12" value="" out="N"/>
				<param name="dest_cy_dor_rt_tp_cd" type="12" value="" out="N"/>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="rout_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
