<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshManifestListDownloadDBDAOaddManifestOutDlListCSQL">
			<desc><![CDATA[방글라데시 Manifest Outbound DownLoad List 입력]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_BD_CNTR 
       (
        VSL_CD,               		SKD_VOY_NO,         SKD_DIR_CD,         		BD_CSTMS_BND_CD,    		POL_CD,
		POD_CD,               		BL_LINE_NO,         TZ_DESC,            		TZ_DT,              		BIL_RMK,
		BIL_DT,               		XPT_DESC,           XPT_DT,             		POD_NM,             		MK_DESC,
		BKG_CNTR_QTY,         		PCK_DESC,           CSTMS_DESC,         		CNTR_NO,           	 		MLT_CNTR_SEAL_NO_DESC,
		CNTR_SZ_DESC,         		CNTR_TP_DESC,       CNTR_HGT,           		NET_WGT,            		BD_CNTR_CGO_TP_CD, 
		IMO_DESC,             		IMDG_UN_DESC,       VAT_DESC,           		CMDT_DESC,          		FDCK_DESC,
		PRSB_CD,              		SHPR_RMK,           CNEE_RMK,           		CNTR_TPSZ_CD,       		BL_NO,
		BKG_CGO_TP_CD,        		VSL_NM,	          	CNTR_WGT,           		WGT_UT_CD,          		CRE_USR_ID,
		CRE_DT,               		UPD_USR_ID,         UPD_DT
	   )
VALUES (
        SUBSTR(@[vvd],1,4),   		SUBSTR(@[vvd],5,4), SUBSTR(@[vvd],9,1), 		'O',                		@[pol_cd],
        @[pod_cd],            		@[sl_no],           @[tr_no],           		REPLACE(@[tr_date],'-',''), @[sbill_no],
        REPLACE(@[sb_date],'-',''), @[exp_no],          REPLACE(@[exp_date],'-',''),@[pod_nm],          		@[mark_no],
        @[pack_no],           		@[pack_nat],        @[goods_desc],      		@[cntr_no],         		@[seal],
        @[cntr_size],             	@[cntr_type],       @[height],          		@[weight],          		@[status],
        @[imco],              		@[un],              @[vat],             		@[cmdt_cd],         		@[off_dock],
        @[perish_cd],         		@[shipper_nm],      @[cnee_nm],         		@[cntr_tz],         		@[org_bl_no],
        @[bkg_cgo_tp_cd],     		@[vsl_nm],          @[cntr_wgt],        		@[cntr_ut_cd],      		@[cre_usr_id],
        sysdate,              		@[upd_usr_id],      sysdate
       )			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="sl_no" type="12" value="" out="N"/>
				<param name="tr_no" type="12" value="" out="N"/>
				<param name="tr_date" type="12" value="" out="N"/>
				<param name="sbill_no" type="12" value="" out="N"/>
				<param name="sb_date" type="12" value="" out="N"/>
				<param name="exp_no" type="12" value="" out="N"/>
				<param name="exp_date" type="12" value="" out="N"/>
				<param name="pod_nm" type="12" value="" out="N"/>
				<param name="mark_no" type="12" value="" out="N"/>
				<param name="pack_no" type="12" value="" out="N"/>
				<param name="pack_nat" type="12" value="" out="N"/>
				<param name="goods_desc" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="seal" type="12" value="" out="N"/>
				<param name="cntr_size" type="12" value="" out="N"/>
				<param name="cntr_type" type="12" value="" out="N"/>
				<param name="height" type="12" value="" out="N"/>
				<param name="weight" type="12" value="" out="N"/>
				<param name="status" type="12" value="" out="N"/>
				<param name="imco" type="12" value="" out="N"/>
				<param name="un" type="12" value="" out="N"/>
				<param name="vat" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="off_dock" type="12" value="" out="N"/>
				<param name="perish_cd" type="12" value="" out="N"/>
				<param name="shipper_nm" type="12" value="" out="N"/>
				<param name="cnee_nm" type="12" value="" out="N"/>
				<param name="cntr_tz" type="12" value="" out="N"/>
				<param name="org_bl_no" type="12" value="" out="N"/>
				<param name="bkg_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_nm" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="cntr_ut_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
