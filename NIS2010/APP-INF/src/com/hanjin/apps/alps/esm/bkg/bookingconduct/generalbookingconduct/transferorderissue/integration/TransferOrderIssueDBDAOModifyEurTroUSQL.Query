<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOModifyEurTroUSQL">
			<desc><![CDATA[TransferOrderIssueDBDAOModifyEurTroUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_EUR_TRO SET 
       RQST_SUB_SEQ    = @[rqst_sub_seq]
     , CNTR_NO         = @[cntr_no]
     , CNTR_TPSZ_CD    = @[cntr_tpsz_cd]
     , RC_SEQ          = @[rc_seq]
     , AWK_CGO_SEQ     = @[awk_cgo_seq]
     , HLG_TP_CD       = @[hlg_tp_cd]
     , CGO_WGT         = @[cgo_wgt]
     , CNTR_PKUP_YD_CD = UPPER(TRIM(@[cntr_pkup_yd_cd]))
     
#if (${cntr_pkup_dt} != '') 
     , CNTR_PKUP_DT = TO_DATE(@[cntr_pkup_dt]||' '||@[cntr_pkup_dt_hhmi], 'YYYY-MM-DD HH24:MI')
#else
     , CNTR_PKUP_DT = @[cntr_pkup_dt]
#end 

     , CNTR_RTN_YD_CD  = UPPER(TRIM(@[cntr_rtn_yd_cd]))
#if (${cntr_rtn_dt} != '') 
     , CNTR_RTN_DT = TO_DATE(@[cntr_rtn_dt]||' '||@[cntr_rtn_dt_hhmi], 'YYYY-MM-DD HH24:MI')
#else
     , CNTR_RTN_DT = @[cntr_rtn_dt]
#end 

     , CMDT_CD         = @[tro_cmdt_cd]
     , REP_CMDT_CD     = @[rep_cmdt_cd]
     , REP_CMDT_DESC   = @[rep_cmdt_nm]
     , BKG_TRSP_MZD_CD = @[bkg_trsp_mzd_cd]
     , SPCL_INSTR_RMK  = @[spcl_instr_rmk]
     , T1_DOC_FLG      = NVL(@[t1_doc_flg], 'N')
     , CSTMS_CLR_NO    = @[cstms_clr_no]
     , ALL_IN_RT_CD    = @[all_in_rt_cd]
     , CURR_CD         = @[curr_cd]
     , TRNS_REV_AMT    = @[trns_rev_amt]
     , NMF_TRNS_REV_AMT    = @[non_trns_rev_amt]
	 , ADD_REV_AMT     = @[add_rev_amt]
	 , ADD_REV_CHG_CD  = @[add_rev_chg_cd]
     , ADD_REV_AMT2     = @[add_rev_amt2]
	 , ADD_REV_CHG_CD2  = @[add_rev_chg_cd2]
     , ADD_REV_AMT3     = @[add_rev_amt3]
	 , ADD_REV_CHG_CD3  = @[add_rev_chg_cd3]
	 , ADD_REV_RMK     = @[add_rev_rmk]
	 , OPTM_STS_CD     = @[optm_sts_cd]
  	 , NOT_OPTM_RSN    = @[not_optm_rsn]
     , VAT_FLG         = NVL(@[vat_flg], 'N')
     , UPD_USR_ID      = @[upd_usr_id]
     , UPD_DT          = sysdate 
WHERE BKG_NO    = @[bkg_no]
  AND IO_BND_CD = @[io_bnd_cd]
  AND TRO_SEQ   = @[tro_seq]			]]></sql>
			<params>
				<param name="rqst_sub_seq" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="rc_seq" type="12" value="" out="N"/>
				<param name="awk_cgo_seq" type="12" value="" out="N"/>
				<param name="hlg_tp_cd" type="12" value="" out="N"/>
				<param name="cgo_wgt" type="12" value="" out="N"/>
				<param name="cntr_pkup_yd_cd" type="12" value="" out="N"/>
				<param name="cntr_pkup_dt" type="12" value="" out="N"/>
				<param name="cntr_pkup_dt_hhmi" type="12" value="" out="N"/>
				<param name="cntr_rtn_yd_cd" type="12" value="" out="N"/>
				<param name="cntr_rtn_dt" type="12" value="" out="N"/>
				<param name="cntr_rtn_dt_hhmi" type="12" value="" out="N"/>
				<param name="tro_cmdt_cd" type="12" value="" out="N"/>
				<param name="rep_cmdt_cd" type="12" value="" out="N"/>
				<param name="rep_cmdt_nm" type="12" value="" out="N"/>
				<param name="bkg_trsp_mzd_cd" type="12" value="" out="N"/>
				<param name="spcl_instr_rmk" type="12" value="" out="N"/>
				<param name="t1_doc_flg" type="12" value="" out="N"/>
				<param name="cstms_clr_no" type="12" value="" out="N"/>
				<param name="all_in_rt_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="trns_rev_amt" type="12" value="" out="N"/>
				<param name="non_trns_rev_amt" type="12" value="" out="N"/>
				<param name="add_rev_amt" type="12" value="" out="N"/>
				<param name="add_rev_chg_cd" type="12" value="" out="N"/>
				<param name="add_rev_amt2" type="12" value="" out="N"/>
				<param name="add_rev_chg_cd2" type="12" value="" out="N"/>
				<param name="add_rev_amt3" type="12" value="" out="N"/>
				<param name="add_rev_chg_cd3" type="12" value="" out="N"/>
				<param name="add_rev_rmk" type="12" value="" out="N"/>
				<param name="optm_sts_cd" type="12" value="" out="N"/>
				<param name="not_optm_rsn" type="12" value="" out="N"/>
				<param name="vat_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
