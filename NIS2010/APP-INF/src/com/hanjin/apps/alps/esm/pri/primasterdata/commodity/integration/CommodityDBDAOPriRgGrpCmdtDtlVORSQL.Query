<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOPriRgGrpCmdtDtlVORSQL">
			<desc><![CDATA[pri_rg_grp_cmdt_dtl 테이블 조회]]></desc>
			<sql><![CDATA[
SELECT GRP_CMDT_DTL_SEQ AS SEQ
     , PRC_CMDT_DEF_CD AS CD
     , CASE WHEN PRC_CMDT_TP_CD = 'C' THEN (SELECT CMDT_NM
                                              FROM MDM_COMMODITY
                                             WHERE CMDT_CD = PRC_CMDT_DEF_CD
                                               AND DELT_FLG = 'N')
            WHEN PRC_CMDT_TP_CD = 'R' THEN (SELECT REP_CMDT_NM
                                              FROM MDM_REP_CMDT
                                             WHERE REP_CMDT_CD = PRC_CMDT_DEF_CD
                                               AND DELT_FLG = 'N')
       END AS NM
  FROM PRI_RG_GRP_CMDT_DTL
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND GLINE_SEQ = @[gline_seq]
   AND GRP_CMDT_SEQ = @[grp_cmdt_seq]
ORDER BY PRC_CMDT_DEF_CD ASC
			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="grp_cmdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
