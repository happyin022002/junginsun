<sqls>
	<querys>
		<query name="RASCommonDBDAOSearchNoteConversionTypeCodeListRSQL">
			<desc><![CDATA[Note Conversion Type List 를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT NOTE_CONV_TP_CD AS CD,
       NOTE_CONV_TP_NM AS NM,
       '' AS ETC1,
       '' AS ETC2,
       '' AS ETC3,
       '' AS ETC4,
       (NOTE_CONV_TP_CD || '|' || NOTE_CONV_TP_NM) AS ETC5
FROM PRI_NOTE_CONV_TP
WHERE PRC_CTRT_TP_CD = 'S'
AND NOTE_CONV_TP_CD <> 'T'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
