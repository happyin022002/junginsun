<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterCustForRejectEdiRSQL">
			<desc><![CDATA[searchXterCustForRejectEdi]]></desc>
			<sql><![CDATA[
SELECT
    XTER_CUST_TP_CD					    IBCS_TP     	  
    , SCE_TOKEN_NL_FNC(NVL(CUST_NM,   ''), 1)	IBCS_NM1    	  
    , SCE_TOKEN_NL_FNC(NVL(CUST_NM,   ''), 2)	IBCS_NM2    	  
    , SCE_TOKEN_NL_FNC(NVL(CUST_ADDR, ''), 1)	IBCS_ADDR1  	  
    , SCE_TOKEN_NL_FNC(NVL(CUST_ADDR, ''), 2)	IBCS_ADDR2  	  
    , SCE_TOKEN_NL_FNC(NVL(CUST_ADDR, ''), 3)	IBCS_ADDR3  	  
    , SCE_TOKEN_NL_FNC(NVL(CNTC_NM,   ''), 1)	IBCS_C_NM1  	  
    , SCE_TOKEN_NL_FNC(NVL(CNTC_NM,   ''), 2)	IBCS_C_NM2  	  
    , CNT_CD				            CNT_CD			
    , DECODE(TO_CHAR(CUST_SEQ), '0', null, TO_CHAR(CUST_SEQ)) CUST_CD     	  
    , LOC_CTNT			                IBCS_CUST_LOC    
    , ST_NM				                IBCS_STREET      
    , LOC_CD				            IBCS_LOC_CD      
    , LOC_NM				            IBCS_LOC_NM      
    , PST_CTNT				            IBCS_ZIP_CD      
    , XTER_CUST_CNTC_TP_CD			    IBCS_C_TP        
    , CNTC_PHN_NO_CTNT				    IBCS_C_TEL       
    , CNTC_FAX_NO				        IBCS_C_FAX       
    , CNTC_EML				            IBCS_C_EMAIL   
  FROM BKG_XTER_CUST
 WHERE XTER_SNDR_ID = @[sender_id]
   AND XTER_RQST_NO = @[rqst_no]
   AND XTER_RQST_SEQ= @[rqst_seq]
   AND XTER_CUST_TP_CD IN ('S','C','F','N','A','E','R','MS', 'ST', 'SF')			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
