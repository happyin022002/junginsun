<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFANoteProposalDBDAORsltNoteInquiryListVORSQL">
			<desc><![CDATA[Inquiry 조회]]></desc>
			<sql><![CDATA[
SELECT A.PROP_NO
	 , A.AMDT_SEQ
	 , A.SVC_SCP_CD
	 , A.NOTE_TP_CD
	 , A.NOTE_SEQ
	 , A.NOTE_TIT_NM
	 , A.DP_SEQ
	 , A.UPD_USR_ID
	 , A.CRE_USR_ID
  FROM PRI_RP_SCP_NOTE A
 WHERE A.PROP_NO		= @[prop_no]
   AND A.AMDT_SEQ		= @[amdt_seq]
   AND A.SVC_SCP_CD		= @[svc_scp_cd]
   AND A.NOTE_TP_CD		= @[note_tp_cd]
   AND EXISTS ( SELECT 'X' FROM PRI_RP_SCP_NOTE_CTNT
                 WHERE PROP_NO 		= A.PROP_NO 
                   AND AMDT_SEQ 	= A.AMDT_SEQ  
                   AND SVC_SCP_CD 	= A.SVC_SCP_CD  
                   AND NOTE_SEQ 	= A.NOTE_SEQ  
                   AND NOTE_TP_CD 	= A.NOTE_TP_CD 
                   AND SRC_INFO_CD <> 'AD' )
 ORDER BY A.DP_SEQ			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="note_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
