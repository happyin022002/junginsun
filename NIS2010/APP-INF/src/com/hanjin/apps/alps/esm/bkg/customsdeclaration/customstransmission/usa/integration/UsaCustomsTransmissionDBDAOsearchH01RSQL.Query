<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchH01RSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT	
	CASE WHEN @[pol_loc] IS NULL OR LENGTH(@[pol_loc]) != 5 THEN
		RPAD('H01'||'4'||'              '||substr(@[vps_eta_dt_a],1,6)||'        '||substr(@[vps_eta_dt_a],7,6),80,' ')||CHR(10)
	ELSE
		RPAD('H01'||'9'||'              '||substr(@[vps_etd_dt],1,6)||'        '||substr(@[vps_etd_dt],7,4),80,' ')||CHR(10) 
	END H01,
	(
		SELECT	RPAD('H02                                          '||NVL(LOC_AMS_PORT_CD,' '),80,' ')||CHR(10)
		FROM MDM_LOCATION
 		WHERE LOC_CD = @[pol_loc]
	) H02
FROM DUAL			]]></sql>
			<params>
				<param name="pol_loc" type="12" value="" out="N"/>
				<param name="vps_eta_dt_a" type="12" value="" out="N"/>
				<param name="vps_etd_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
