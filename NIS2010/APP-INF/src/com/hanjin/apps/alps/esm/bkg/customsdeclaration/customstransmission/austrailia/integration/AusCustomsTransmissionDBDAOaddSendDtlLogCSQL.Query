<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOaddSendDtlLogCSQL">
			<desc><![CDATA[호주 위험물 SEND DETAIL 로그 저장]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_DG_EDI_RSPN (
	EDI_MSG_TP_ID
,	MSG_SND_NO
,	EDI_RSPN_SEQ
,	BL_NO
,	CNTR_NO
,	CNTR_CGO_SEQ
,	DG_BL_REF_NO
,	DG_ITM_REF_NO
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID   
,	UPD_DT
,   CNT_CD
) VALUES( 
	@[edi_msg_tp_id]
,	@[msg_snd_no]
,	(
		SELECT NVL(MAX(EDI_RSPN_SEQ), 0) + 1
		FROM BKG_CSTMS_DG_EDI_RSPN
		WHERE EDI_MSG_TP_ID = 'IFD'
		AND   MSG_SND_NO = @[msg_snd_no]
	)
,	@[bl_no]
,	@[cntr_no]
,	@[cntr_cgo_seq]
,	@[dg_bl_ref_no]
,	@[dg_itm_ref_no]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	'AU'
)			]]></sql>
			<params>
				<param name="edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="msg_snd_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cgo_seq" type="12" value="" out="N"/>
				<param name="dg_bl_ref_no" type="12" value="" out="N"/>
				<param name="dg_itm_ref_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
