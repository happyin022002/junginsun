<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteConversionProposalDBDAODelNoteCasRNoteDSQL">
			<desc><![CDATA[Route Note Amend Cancel시 해당 Note Conv 삭제]]></desc>
			<sql><![CDATA[
DELETE PRI_SC_NOTE_CONV A
 WHERE EXISTS (SELECT 'OK'
                 FROM PRI_SP_SCP_RT_CMDT_RNOTE B
                WHERE B.NOTE_CONV_MAPG_ID = A.NOTE_CONV_MAPG_ID
                  AND B.PROP_NO = @[prop_no]
                  AND B.AMDT_SEQ = @[amdt_seq]
                  AND B.SVC_SCP_CD = @[svc_scp_cd]
#if (${CASCADE_LEVEL} == "0") 

#elseif (${CASCADE_LEVEL} == "1") 
                  AND B.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                  AND B.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#elseif (${CASCADE_LEVEL} == "2") 
                  AND B.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                  AND B.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                  AND B.ROUT_SEQ = @[rout_seq]
#else 
                  AND B.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                  AND B.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                  AND B.ROUT_SEQ = @[rout_seq]
                  AND B.ROUT_NOTE_SEQ = @[rout_note_seq]
#end
              )			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="rout_note_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
