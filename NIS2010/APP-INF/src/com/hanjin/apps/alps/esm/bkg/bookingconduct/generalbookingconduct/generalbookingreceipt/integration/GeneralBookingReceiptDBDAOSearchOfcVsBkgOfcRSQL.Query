<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchOfcVsBkgOfcRSQL">
			<desc><![CDATA[다른 Office의 Booking을 copy할 수 없음]]></desc>
			<sql><![CDATA[
select COUNT(*) CNT
  from bkg_booking 
 where bkg_no    = @[bkg_no]
   and bkg_ofc_cd <> @[bkg_ofc_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
