<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOSearchSpaceAllocation0042ConfirmRSQL">
			<desc><![CDATA[SAQ에 Alloction Confirm하여 넘긴 Volumn((SQM_ALOC_QTA) 존재 여부 체크]]></desc>
			<sql><![CDATA[
SELECT DECODE(count(1),0,'N','Y')
   FROM SQM_ALOC_QTA SAQ
  WHERE RLANE_CD   = @[lane]
    AND VSL_CD     = substr(@[vvd],1,4)
    AND SKD_VOY_NO = substr(@[vvd],5,4)
    AND SKD_DIR_CD = substr(@[vvd],9,1)			]]></sql>
			<params>
				<param name="lane" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
