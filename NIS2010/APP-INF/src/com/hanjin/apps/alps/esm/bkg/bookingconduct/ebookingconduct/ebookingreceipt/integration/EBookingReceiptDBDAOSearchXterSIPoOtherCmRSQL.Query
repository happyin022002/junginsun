<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchXterSIPoOtherCmRSQL">
			<desc><![CDATA[alps의 export/import licens no를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(NVL(REF_SEQ,0),0,'I','U') IBFLAG,
       X.BKG_NO,
       REF_SEQ,
       X.CNTR_NO,
       X.PO_NO,
       NVL(X.ITM_NO , BR.ITM_NO)  ITM_NO,
       NVL(X.ITM_DESC, BR.ITM_DESC) ITM_DESC,
       NVL(X.PCK_QTY, BR.PCK_QTY) PCK_QTY,
       NVL(X.PCK_TP_CD, BR.PCK_TP_CD) PCK_TP_CD,
       NVL(X.CNTR_WGT, BR.CNTR_WGT) CNTR_WGT,
       NVL(X.WGT_UT_CD, BR.WGT_UT_CD) WGT_UT_CD,
       NVL(X.MEAS_QTY, BR.MEAS_QTY) MEAS_QTY,
       NVL(X.MEAS_UT_CD, BR.MEAS_UT_CD) MEAS_UT_CD
FROM (
    SELECT MST.BKG_NO
        , XC.PO_NO
        , XC.SHP_REF_NO ITM_NO
        , '' ITM_DESC
        , XC.PCK_QTY
        , XC.PCK_TP_CD
        , XC.CNTR_WGT
        , XC.WGT_UT_CD
        , XC.MEAS_QTY
        , XC.MEAS_UT_CD
        , XC.CNTR_NO
    FROM BKG_XTER_RQST_MST MST
         ,BKG_XTER_CNTR XC     
    WHERE MST.XTER_SNDR_ID  = @[xter_sndr_id]
    AND MST.XTER_RQST_NO  = @[xter_rqst_no]
    AND MST.XTER_RQST_SEQ = @[xter_rqst_seq]
    AND MST.XTER_SNDR_ID  = XC.XTER_SNDR_ID
    AND MST.XTER_RQST_NO  = XC.XTER_RQST_NO
    AND MST.XTER_RQST_SEQ = XC.XTER_RQST_SEQ
    AND XC.PO_NO IS NOT NULL) X
    ,BKG_REF_DTL BR
WHERE X.BKG_NO = BR.BKG_NO(+)
AND X.CNTR_NO = BR.CNTR_NO(+)
AND NVL(BR.DE_NO(+),'N') = 'N'
AND NVL(BR.PRT_NO(+),'N') = 'N'			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
