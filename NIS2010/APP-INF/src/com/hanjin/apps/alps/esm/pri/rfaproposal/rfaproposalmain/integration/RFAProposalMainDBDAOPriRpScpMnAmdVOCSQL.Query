<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpScpMnAmdVOCSQL">
			<desc><![CDATA[RFAProposalMainDBDAO PriRpScpMnCpVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_MN(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    EFF_DT,
    EXP_DT,
    PROP_SCP_SREP_CD,
    PROP_SCP_OFC_CD,
    PROP_SCP_STS_CD,
    PRS_XCH_RT_YRMON,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
    TGT_MVC_QTY,
    CNTR_LOD_UT_CD
)
SELECT
    PROP_NO,
    @[amdt_seq]+1,
    SVC_SCP_CD,    
	DECODE(SIGN(TO_DATE(@[eff_dt],'YYYY-MM-DD') - TRUNC(EFF_DT)), -1, EFF_DT, TO_DATE(@[eff_dt],'YYYY-MM-DD')),
	EXP_DT,
    PROP_SCP_SREP_CD,
    PROP_SCP_OFC_CD,
	PROP_SCP_STS_CD,
    PRS_XCH_RT_YRMON,
    @[cre_usr_id],
    CRE_DT,
    @[upd_usr_id],
    SYSDATE,
    TGT_MVC_QTY,
    CNTR_LOD_UT_CD
FROM 
    PRI_RP_SCP_MN
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
