<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAQuotationMainDBDAOSearchDetailCntRSQL">
			<desc><![CDATA[각 탭별 건수 조회]]></desc>
			<sql><![CDATA[
SELECT (SELECT COUNT(*)
          FROM PRI_RQ_GRP_LOC
         WHERE QTTN_NO = @[qttn_no]
           AND QTTN_VER_NO = @[qttn_ver_no]) AS GRP_LOC_CNT,
       (SELECT COUNT(*)
          FROM PRI_RQ_GRP_CMDT
         WHERE QTTN_NO = @[qttn_no]
           AND QTTN_VER_NO = @[qttn_ver_no]) AS GRP_CMDT_CNT,
       (SELECT COUNT(*)
          FROM PRI_RQ_RT_CMDT_HDR HDR, PRI_RQ_MN MN
         WHERE HDR.QTTN_NO = @[qttn_no]
           AND HDR.QTTN_VER_NO = @[qttn_ver_no]
           AND HDR.QTTN_NO = MN.QTTN_NO
           AND HDR.QTTN_VER_NO = MN.QTTN_VER_NO) AS RATE_G_CNT,
	     '0' AS RATE_CNT,
       '0' AS RATE_S_CNT
  FROM DUAL			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
