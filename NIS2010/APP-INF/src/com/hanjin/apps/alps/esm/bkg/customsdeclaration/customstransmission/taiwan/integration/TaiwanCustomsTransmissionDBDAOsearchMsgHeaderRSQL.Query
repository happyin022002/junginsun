<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TaiwanCustomsTransmissionDBDAOsearchMsgHeaderRSQL">
			<desc><![CDATA[Header cretation]]></desc>
			<sql><![CDATA[
SELECT '$$$MSGSTART:'||
    RPAD(NVL(TRIM('NISBKG'),' '),20,' ')||
    RPAD(NVL(TRIM('KORCUS'),' '),20,' ')||
    RPAD(NVL(TRIM('IFTMCS'),' '),10,' ')||
    'BKC'||
    TO_CHAR(sysdate,'rrmmdd')||
			LTRIM(TO_CHAR(BKG_CSTM_EDI_SEQ.NEXTVAL,'000009'),' ') msg_header	
FROM DUAL			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
