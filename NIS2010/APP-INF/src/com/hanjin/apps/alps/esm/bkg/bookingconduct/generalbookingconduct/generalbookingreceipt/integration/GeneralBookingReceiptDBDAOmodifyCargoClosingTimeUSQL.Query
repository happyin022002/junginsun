<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOmodifyCargoClosingTimeUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE BKG_CLZ_TM 
   SET MNL_SET_USR_ID = CASE WHEN NVL(@[mnl_set_dt], 'X') =  NVL(TO_CHAR(MNL_SET_DT,'YYYYMMDDHH24MISS'),'X') THEN CASE WHEN NVL(MNL_SET_USR_ID, 'X')='X' THEN 'SYSTEM' ELSE MNL_SET_USR_ID END
                        WHEN NVL(@[mnl_set_dt], 'X') <> NVL(TO_CHAR(MNL_SET_DT,'YYYYMMDDHH24MISS'),'X') THEN @[upd_usr_id]
                        ELSE 'SYSTEM'
                        END
     , MNL_SET_DT = TO_DATE(@[mnl_set_dt],'YYYYMMDDHH24MISS')
     , NTC_FLG = @[ntc_flg] 
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no] 
AND CLZ_TP_CD = @[clz_tp_cd]			]]></sql>
			<params>
				<param name="mnl_set_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ntc_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="clz_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
