<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOsearchHldNtcBkgStaffSetupInfoRSQL">
			<desc><![CDATA[searchHldNtcBkgStaffSetupInfo]]></desc>
			<sql><![CDATA[
SELECT BKGM.BKG_NO
      ,BKGM.BL_NO
      ,BKGM.POR_CD
      ,BKGM.POL_CD
      ,BKGM.POD_CD
      ,BKGM.DEL_CD
      ,BKGM.DOC_USR_ID
      ,COM.OFC_CD
      ,COM.USR_NM
      ,COM.USR_EML
FROM  BKG_BOOKING BKGM
     ,COM_USER COM
WHERE BKGM.BL_NO    = @[bl_no]
AND   COM.USR_ID(+) = BKGM.DOC_USR_ID			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
