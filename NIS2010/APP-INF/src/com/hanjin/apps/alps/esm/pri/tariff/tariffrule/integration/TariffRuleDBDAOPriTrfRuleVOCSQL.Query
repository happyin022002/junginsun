<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffRuleDBDAOPriTrfRuleVOCSQL">
			<desc><![CDATA[Tariff Rule 입력   ]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_RULE
(
    TRF_PFX_CD
  , TRF_NO
  , TRF_RULE_NO
  , AMDT_SEQ
  , TRF_RULE_NM
  , TRF_RULE_CTNT
  , TRF_RULE_CHG_CD
  , TRF_RULE_AMDT_TP_CD
  , EFF_DT
  , EXP_DT
  , PUB_DT
  , RQST_OFC_CD
  , APRO_OFC_CD
  , TRF_RULE_STS_CD
  , CRE_USR_ID
  , CRE_DT
  , UPD_USR_ID
  , UPD_DT
)
VALUES (
    @[trf_pfx_cd]
  , @[trf_no]
  , @[trf_rule_no]
  , @[amdt_seq]
  , @[trf_rule_nm]
  , @[trf_rule_ctnt]
  , @[trf_rule_chg_cd]
  , @[trf_rule_amdt_tp_cd]
  , TO_DATE(@[eff_dt], 'YYYYMMDD')
  , TO_DATE(@[exp_dt], 'YYYYMMDD')
  , TO_DATE(@[pub_dt], 'YYYYMMDD')
  , @[rqst_ofc_cd]
  , @[apro_ofc_cd]
  , @[trf_rule_sts_cd]
  , @[cre_usr_id]
  , SYSDATE
  , @[cre_usr_id]
  , SYSDATE
)			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_rule_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="trf_rule_nm" type="12" value="" out="N"/>
				<param name="trf_rule_ctnt" type="12" value="" out="N"/>
				<param name="trf_rule_chg_cd" type="12" value="" out="N"/>
				<param name="trf_rule_amdt_tp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="pub_dt" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="trf_rule_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
