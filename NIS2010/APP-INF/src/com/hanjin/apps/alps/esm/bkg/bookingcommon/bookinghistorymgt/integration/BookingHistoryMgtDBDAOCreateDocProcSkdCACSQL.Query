<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOCreateDocProcSkdCACSQL">
			<desc><![CDATA[copyTypeCd에 따라, BKG_DOC_PROC_SKD/BKG_DOC_PROC_SKD_HIS 를 생성]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_DOC_PROC_SKD (
    BKG_NO 
#else
INSERT INTO BKG_DOC_PROC_SKD_HIS (
    BKG_NO 
    , CORR_NO 
#end
    , BKG_DOC_PROC_TP_CD 
    , DOC_PROC_SEQ 
    , EVNT_DT 
    , EVNT_GDT 
    , EVNT_USR_ID 
    , DIFF_RMK 
    , BKG_EVNT_CD1 
    , BKG_EVNT_CD2 
    , DOC_PERF_DELT_FLG 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
    , BKG_DOC_PROC_TP_CD 
    , DOC_PROC_SEQ 
    , EVNT_DT 
    , EVNT_GDT 
    , EVNT_USR_ID 
    , DIFF_RMK 
    , BKG_EVNT_CD1 
    , BKG_EVNT_CD2 
    , DOC_PERF_DELT_FLG 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , sysdate
#if (${copy_type_cd} == 'BKG')
  FROM BKG_DOC_PROC_SKD_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
   AND BKG_DOC_PROC_TP_CD = 'CNTATC'
#else
  FROM BKG_DOC_PROC_SKD
 WHERE BKG_NO = @[bkg_no]
   AND BKG_DOC_PROC_TP_CD = 'CNTATC'
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
