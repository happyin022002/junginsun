<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOSearchDelChargeRSQL">
			<desc><![CDATA[BDRCorrectionDBDAOSearchDelChargeRSQL]]></desc>
			<sql><![CDATA[
SELECT CHG_CD
  FROM (
        SELECT RATE.BKG_NO  BKG_NO
             , RTHIS.BKG_NO BKG_NO_HIS
             , RATE.CHG_CD  CHG_CD
             , RTHIS.CHG_CD CHG_CD_HIS
          FROM BKG_CHG_RT     RATE
             , BKG_CHG_RT_HIS RTHIS
         WHERE RATE.BKG_NO      = @[bkg_no]
           AND RATE.BKG_NO      = RTHIS.BKG_NO(+)
           AND RATE.RT_SEQ      = RTHIS.RT_SEQ(+)
           AND RTHIS.CORR_NO(+) = 'TMP0000001' 
       )
 WHERE BKG_NO_HIS IS NULL 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
