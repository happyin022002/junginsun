<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOMultiSetFormDeltFlagUSQL">
			<desc><![CDATA[SPC_RPT_FOM delt_flg 값변경]]></desc>
			<sql><![CDATA[
UPDATE 
SPC_RPT_FOM
   SET DELT_FLG = 'Y'
 WHERE PGM_NO = @[pgm_no]
   AND CRE_USR_ID = @[cre_usr_id]
   AND RPT_FOM_NO = @[rpt_fom_no]			]]></sql>
			<params>
				<param name="pgm_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="rpt_fom_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
