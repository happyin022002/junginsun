<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchDgCNTRInfoRSQL">
			<desc><![CDATA[다른 항차로 이미 D/L 된 BKG이있을 경우에, Dup Error를 피하고 Update하기위해 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD
     , NVL(POL_CD,'     ') POL_CD
     , NVL(POD_CD,'     ') POD_CD
  FROM BKG_CSTMS_KR_DG_CGO
 WHERE BKG_NO           = @[bkg_no]
   AND CSTMS_DECL_TP_CD = @[cstms_decl_tp_cd]
   AND CNTR_SEQ         = @[cntr_seq]
   AND CNTR_NO          = @[cntr_no]
   AND IMDG_UN_NO       = @[imdg_un_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_seq" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
