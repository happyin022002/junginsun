<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOSearchEdiAdvJpVslSkdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT --TYPE CODE
       DECODE((SELECT JP_MSG_TP_ID
                 FROM BKG_CSTMS_ADV_JP_rcv_LOG
                WHERE JP_MSG_TP_ID = 'SATD'
                  AND VSL_CD     = SUBSTR(@[vvd], 1, 4)
                  AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
                  AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
                  AND POL_CD = @[pol_cd]
                  AND SUBSTR(RCV_KEY_DAT_CTNT, 1, 5) = '00000'
                  AND ROWNUM = 1), NULL, 9, 5) AS DATA00,
       -- SP CODE
       RPAD(' ', 16, ' ') AS DATA01,
       -- VSL CODE
       RPAD(SKD.CALL_SGN_NO, 9, ' ') AS DATA02,
       -- VOY CODE
       RPAD(SKD.SKD_VOY_NO||SKD.SKD_DIR_CD, 10, ' ') AS DATA03,
       -- CARRIER CODE
       'SMLM' AS DATA04,
       -- POL CODE
       --RPAD(SKD.POL_CD, 5, ' ') AS DATA05,
       DECODE(H1.ATTR_CTNT1, NULL, DECODE(SUBSTR(L1.LOC_CD, 1, 2), 'AW', 'AN', 'CS', 'CB', 'EU', 'ZY', 'KZ', 'KA', 'LI', 'ZY', 'RU', 'RS', 'TJ', 'TA', 'UA', 'UK', 'UZ', 'UB', 'YU', 'YE', SUBSTR(L1.LOC_CD, 1, 2))||'ZZZ', H1.ATTR_CTNT2) AS DATA05,
       -- POL SUFFIX
--       RPAD(' ', 1, ' ') AS DATA06,
       DECODE(@[pol_split_no], '', ' ', @[pol_split_no]) AS DATA06,
       -- DT OF DEPARTURE
--       TO_CHAR(SKD.ETD_DT, 'YYYYMMDD') AS DATA07,
       REPLACE(@[etd_date], '-', '') AS DATA07,
       -- TIME OF DEPARTURE
--       TO_CHAR(SKD.ETD_DT, 'HH24MI') AS DATA08,
       REPLACE(@[etd_time], ':', '') AS DATA08,
       -- GREENWICH TIME
       DECODE(SIGN(L1.GMT_HRS), -1, '-', '+')||DECODE(TRUNC(ABS(L1.GMT_HRS)/60), 0, '00', LPAD(TRUNC(ABS(L1.GMT_HRS)/60), 2, '0'))||LPAD((MOD(ABS(L1.GMT_HRS), 60)), 2, '0') AS DATA09,
       -- RELAXED AREA ID
       DECODE(JO_CD1, 'Y', 'Y', ' ') AS DATA10

  FROM BKG_CSTMS_ADV_JP_VSL_SKD SKD,
       MDM_LOCATION L1,
       BKG_CSTMS_CD_CONV_CTNT H1

 WHERE SKD.VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND SKD.POL_CD = @[pol_cd]
   AND H1.CNT_CD(+) = 'JP'
   AND H1.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
   AND SKD.POL_CD = H1.ATTR_CTNT1(+)
   AND SKD.POL_CD = L1.LOC_CD(+)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_split_no" type="12" value="" out="N"/>
				<param name="etd_date" type="12" value="" out="N"/>
				<param name="etd_time" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
