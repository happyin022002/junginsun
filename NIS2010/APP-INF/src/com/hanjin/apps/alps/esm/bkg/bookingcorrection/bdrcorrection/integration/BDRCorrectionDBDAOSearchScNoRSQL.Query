<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOSearchScNoRSQL">
			<desc><![CDATA[Booking 의 SC No. 를 조회 한다.]]></desc>
			<sql><![CDATA[
SELECT SC_NO 
#if (${ca_flg}== 'Y')
  FROM BKG_BKG_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_BOOKING
 WHERE BKG_NO  = @[bkg_no]
#end

			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
