<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BrcsCustomsTransmissionDBDAOsearchCMRSQL">
			<desc><![CDATA[CNTR DESC 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  CMDT_HS_CD D_CMDT
      , PCK_TP_CD D_PUNIT
      , PCK_QTY D_PKG
      , DECODE(NVL(WGT_UT_CD,' '),'LBS',ROUND(NVL(CNTR_MF_WGT,0)*0.4536,3),NVL(CNTR_MF_WGT,0)) D_WGT
      , DECODE(NVL(MEAS_UT_CD,' '),'CBF',ROUND(NVL(MEAS_QTY,0)*0.0283,3),NVL(MEAS_QTY,0))D_MEAS
      , CNTR_MF_GDS_DESC D_DESC
      , HAMO_TRF_CD HAMO_TRF_CD 
      , B.BRZ_CMDT_CD D_NCM_CD
      , CASE WHEN CNTR_MF_MK_DESC IS NULL THEN TO_CLOB('')
             ELSE 'D_MARK:' || replace(TO_CLOB(CNTR_MF_MK_DESC) ,chr(13)||chr(10),chr(10)||'D_MARK:')
        END D_MARK
      , B.CNTR_MF_SEQ
      , (CASE WHEN A.WPM_TRT_CD = 'A' THEN 'N/A' ELSE NVL(A.WPM_TRT_CD,' ') END) WPM
FROM    BKG_CNTR_MF_DESC A
      , BKG_CSTMS_BRZ_CNTR_MF B
      , BKG_BOOKING BB
WHERE   1 = 1
AND     BB.BKG_NO = A.BKG_NO
AND     BB.BL_NO = B.BL_NO
AND     A.CNTR_NO = B.CNTR_NO(+)  
AND     A.CNTR_MF_SEQ = B.CNTR_MF_SEQ(+)
AND     A.BKG_NO = @[bkg_no]
AND     A.CNTR_NO = @[cntr_no]
AND     A.CNTR_MF_SEQ > 0			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
