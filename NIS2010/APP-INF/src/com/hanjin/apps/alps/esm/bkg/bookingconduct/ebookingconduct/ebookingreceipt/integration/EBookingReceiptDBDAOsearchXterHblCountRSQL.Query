<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterHblCountRSQL">
			<desc><![CDATA[searchXterHblCount]]></desc>
			<sql><![CDATA[
SELECT  XTER_RQST_NO
FROM	BKG_XTER_RQST_MST
WHERE	BKG_NO = @[bkg_no]
AND		XTER_SNDR_ID = @[xter_sndr_id]
AND		XTER_BL_TP_CD = 'H'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
