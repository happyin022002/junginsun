<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArApprovalDBDAOPrintArCsrHeaderInfoRSQL">
			<desc><![CDATA[groupware 전송 xmlData Header info]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
        A.CSR_DESC AS SUBJECT
       ,'' VATREGISTNO
       ,TO_CHAR(SYSDATE,'YYYY/MM/DD') CSRDATE
       ,(A.SLP_TP_CD || A.SLP_FUNC_CD || A.SLP_OFC_CD || A.SLP_ISS_DT || A.SLP_SER_NO) CSR_NO
       ,A.SLP_OFC_CD OFFICE
       ,D.USR_NM
       ,'' BUDGET
       ,'' PERFORMANCE
       ,'' RATIO
       ,C.CUST_LGL_ENG_NM PAY_TO
       ,A.CSR_DESC DESCRIPTION
       ,'' EVIDENCE
       ,'' QTY
       ,'' INVOICE_DATE
       ,'' DUE_DATE
       ,'' AR_OFFSET_NO
       ,'SALES INVOICES' CSR_TYPE
       ,'' PAY_GROUP
       ,'' ASA_NO
       ,A.CSR_CURR_CD CURRENCY
       ,DECODE(A.RQST_AMT,'0','0.00',TO_CHAR(A.RQST_AMT,'FM999,999,999,990.00')) AS AMOUNT
       ,'' AS PYMT_AMT
       ,'N' APR_LINE
	   ,D.USR_NM AS PRPD_BY
  FROM FMS_CONSULTATION A,
       FMS_CSUL_SLP B,
       MDM_CUSTOMER C,
       MDM_VENDOR E,
       COM_USER D
 WHERE A.SLP_TP_CD = B.SLP_TP_CD 
   AND A.SLP_FUNC_CD = B.SLP_FUNC_CD
   AND A.SLP_OFC_CD = B.SLP_OFC_CD
   AND A.SLP_ISS_DT = B.SLP_ISS_DT
   AND A.SLP_SER_NO = B.SLP_SER_NO 
   AND SUBSTR(@[csr_no],1,2) = A.SLP_TP_CD
   AND SUBSTR(@[csr_no],3,1) = A.SLP_FUNC_CD
   AND SUBSTR(@[csr_no],4,6) = A.SLP_OFC_CD
   AND SUBSTR(@[csr_no],10,6) = A.SLP_ISS_DT
   AND SUBSTR(@[csr_no],16,5) = A.SLP_SER_NO
   AND B.CUST_CNT_CD = C.CUST_CNT_CD(+)
   AND B.CUST_SEQ = C.CUST_SEQ(+)
   AND B.VNDR_SEQ = E.VNDR_SEQ(+)
   AND A.CSR_USR_ID = D.USR_ID			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
