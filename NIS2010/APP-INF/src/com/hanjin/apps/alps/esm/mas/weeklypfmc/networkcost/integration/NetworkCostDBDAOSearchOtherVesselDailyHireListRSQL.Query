<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOSearchOtherVesselDailyHireListRSQL">
			<desc><![CDATA[SearchOtherVesselDailyHireList]]></desc>
			<sql><![CDATA[
SELECT D1.COST_YRMON,
       D1.COST_WK,
       D1.VSL_CD,
       D1.VSL_CLSS_CAPA,
       D1.STND_COST_CD,
       D1.CHRG_RT*100 AS CHRG_RT,
       D1.VSL_COST_AMT,
       D1.TTL_AMT,
       D1.TTL_WK_AMT,
       D1.UPD_USR_ID
  FROM MAS_OTR_VSL_DLY_HIR D1
 WHERE D1.COST_YRMON LIKE @[f_year]||'%'
   AND D1.COST_WK         = @[f_fm_wk]
   AND D1.STND_COST_CD(+) = @[f_cobcost]
#if(${f_vsl_cd} != '')
   AND D1.VSL_CD = @[f_vsl_cd]
#end
 ORDER BY D1.COST_YRMON,
       D1.COST_WK,
       D1.VSL_CD,
       D1.VSL_CLSS_CAPA			]]></sql>
			<params>
				<param name="f_year" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_cobcost" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
