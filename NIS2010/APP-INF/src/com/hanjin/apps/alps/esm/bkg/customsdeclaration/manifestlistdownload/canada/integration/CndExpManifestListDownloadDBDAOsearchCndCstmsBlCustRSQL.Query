<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndExpManifestListDownloadDBDAOsearchCndCstmsBlCustRSQL">
			<desc><![CDATA[searchCndCstmsBlCust]]></desc>
			<sql><![CDATA[
SELECT   C1.CUST_CNT_CD         AS CUST_CNT_CD1
        ,C1.CUST_SEQ            AS CUST_SEQ1
        ,C1.CUST_ZIP_ID         AS CUST_ZIP_ID1
        ,C1.CUST_CTY_NM         AS CUST_CTY_NM1
        ,C1.CUST_STE_CD         AS CUST_STE_CD1
        ,C1.CSTMS_DECL_CNT_CD   AS CSTMS_DECL_CNT_CD1
        ,C1.CUST_NM             AS CUST_NM1
        ,C1.CUST_ADDR           AS CUST_ADDR1
        ,C2.CUST_CNT_CD         AS CUST_CNT_CD2
        ,C2.CUST_SEQ            AS CUST_SEQ2
        ,C2.CUST_ZIP_ID         AS CUST_ZIP_ID2
        ,C2.CUST_CTY_NM         AS CUST_CTY_NM2
        ,C2.CUST_STE_CD         AS CUST_STE_CD2
        ,C2.CSTMS_DECL_CNT_CD   AS CSTMS_DECL_CNT_CD2
        ,C2.CUST_NM             AS CUST_NM2
        ,C2.CUST_ADDR           AS CUST_ADDR2
        ,DECODE(A.CUST_TO_ORD_FLG, 'Y', 'O', 'S') AS TO_ORD_FLG
        ,C3.CUST_CNT_CD         AS CUST_CNT_CD3
        ,C3.CUST_SEQ            AS CUST_SEQ3
        ,C3.CUST_ZIP_ID         AS CUST_ZIP_ID3
        ,C3.CUST_CTY_NM         AS CUST_CTY_NM3
        ,C3.CUST_STE_CD         AS CUST_STE_CD3
        ,C3.CSTMS_DECL_CNT_CD   AS CSTMS_DECL_CNT_CD3
        ,C3.CUST_NM             AS CUST_NM3
        ,C3.CUST_ADDR           AS CUST_ADDR3
  FROM  BKG_CSTMS_AMER_BL A
       ,BKG_CSTMS_AMER_CUST C1
	   ,BKG_CSTMS_AMER_CUST C2
	   ,BKG_CSTMS_AMER_CUST C3
 WHERE  1=1
   AND  A.CNT_CD = 'CA'
#if (${bl_no} != '')
   AND  A.BL_NO = @[bl_no]
#end
   AND  A.CNT_CD = C1.CNT_CD(+)
   AND  A.BL_NO = C1.BL_NO(+)
   AND  C1.BKG_CUST_TP_CD(+) = 'S'
   AND  A.CNT_CD = C2.CNT_CD(+)
   AND  A.BL_NO = C2.BL_NO(+)
   AND  C2.BKG_CUST_TP_CD(+) = 'C'
   AND  A.CNT_CD = C3.CNT_CD(+)
   AND  A.BL_NO = C3.BL_NO(+)
   AND  C3.BKG_CUST_TP_CD(+) = 'N'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
