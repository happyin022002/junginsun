<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GroupCommodityCMPBGuidelineDBDAOPriCmpbGrpCmdtVOAddCopyCSQL">
			<desc><![CDATA[Group commodity copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_CMPB_GRP_CMDT (
	SVC_SCP_CD
,	CRE_OFC_CD
,	GLINE_SEQ
,	GRP_CMDT_SEQ
,	PRC_GRP_CMDT_CD
,	PRC_GRP_CMDT_DESC
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT
	@[svc_scp_cd]
,	@[cre_ofc_cd]
,	@[max_gline_seq]
,	GRP_CMDT_SEQ
,	PRC_GRP_CMDT_CD
,	PRC_GRP_CMDT_DESC
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM	PRI_CMPB_GRP_CMDT
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	CRE_OFC_CD = @[cre_ofc_cd_hidden]
AND	GLINE_SEQ = @[gline_seq]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="max_gline_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd_hidden" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
