<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqListDBDAOsearchBbCgoDtlListRSQL">
			<desc><![CDATA[PRI_SCQ_BB_CGO 정보 조회]]></desc>
			<sql><![CDATA[
SELECT TTL_DIM_LEN, TTL_DIM_WDT, TTL_DIM_HGT, GRS_WGT
FROM PRI_SCQ_BB_CGO
WHERE SCQ_RQST_NO = @[scq_rqst_no]
AND SCQ_VER_NO = @[scq_ver_no]			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
