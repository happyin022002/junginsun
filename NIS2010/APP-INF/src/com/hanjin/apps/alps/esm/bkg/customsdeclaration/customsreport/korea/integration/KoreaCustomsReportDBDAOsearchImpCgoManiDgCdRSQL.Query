<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchImpCgoManiDgCdRSQL">
			<desc><![CDATA[Import Cargo Manifest Print를 위한 특수화물 코드를 조회한다. 여러건 조회되면 "\n" 해준다.]]></desc>
			<sql><![CDATA[
SELECT NVL(IMDG_UN_NO,' ') IMDG_UN_NO
  FROM BKG_DG_CGO
 WHERE BKG_NO   = @[bkg_no]
   AND DCGO_SEQ = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
