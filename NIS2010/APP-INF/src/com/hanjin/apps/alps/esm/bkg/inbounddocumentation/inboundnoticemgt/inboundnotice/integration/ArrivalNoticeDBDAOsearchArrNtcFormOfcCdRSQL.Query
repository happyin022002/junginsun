<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOsearchArrNtcFormOfcCdRSQL">
			<desc><![CDATA[Arrival Notice Form Setting에서 POD CD를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT 'ALL' as pod_cd
       ,'ALL'  as an_seq
FROM DUAL
UNION ALL
SELECT DISTINCT POD_CD as pod_cd,
                POD_CD as an_seq
  FROM BKG_ARR_NTC_WD
 WHERE AN_TP_CD = 'ARN'
   AND OFC_CD = @[ofc_cd]
   AND POD_CD <> '*'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
