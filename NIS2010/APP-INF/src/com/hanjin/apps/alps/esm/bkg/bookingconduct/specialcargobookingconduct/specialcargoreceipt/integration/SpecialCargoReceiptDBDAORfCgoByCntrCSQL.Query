<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAORfCgoByCntrCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_RF_CGO
            (BKG_NO
,            RC_SEQ
,            CNTR_TPSZ_CD
,            CNTR_NO
,            PCK_TP_CD
,            PCK_QTY
,            NET_WGT
,            GRS_WGT
,            WGT_UT_CD
,            CMDT_CD
,            CMDT_DESC
,            FDO_TEMP
,            CDO_TEMP
,            CNTR_VENT_TP_CD
,            VENT_RTO
,            HUMID_NO
,            DIFF_RMK
,            RF_DCGO_SEQ
,            PWR_SPL_CBL_FLG
,            VLTG_NO
,            CTRL_ATMS_FLG
,            MODI_ATMS_FLG
,            HUMID_CTRL_FLG
,            CNTR_DRN_CD
,            CLNG_TP_CD
,            RQST_DT
,            RQST_USR_ID
,            SPCL_CGO_APRO_CD
,            CRE_USR_ID
,            CRE_DT
,            UPD_USR_ID
,            UPD_DT
,            CNTR_VOL_QTY
,            CBM_PER_HR_QTY
            )
    SELECT @[tgt_bkg_no]
,          (SELECT NVL(MAX(RC_SEQ), 0)
            FROM   BKG_RF_CGO
            WHERE  BKG_NO = @[tgt_bkg_no]) + 1 AS RC_SEQ
,          CNTR_TPSZ_CD
,          CNTR_NO
,          PCK_TP_CD
,          PCK_QTY
,          NET_WGT
,          GRS_WGT
,          WGT_UT_CD
,          CMDT_CD
,          CMDT_DESC
,          FDO_TEMP
,          CDO_TEMP
,          CNTR_VENT_TP_CD
,          VENT_RTO
,          HUMID_NO
,          DIFF_RMK
,          RF_DCGO_SEQ
,          PWR_SPL_CBL_FLG
,          VLTG_NO
,          CTRL_ATMS_FLG
,          MODI_ATMS_FLG
,          HUMID_CTRL_FLG
,          CNTR_DRN_CD
,          CLNG_TP_CD
,          RQST_DT
,          RQST_USR_ID
,          SPCL_CGO_APRO_CD
,          @[cre_usr_id]
,          SYSDATE
,          @[cre_usr_id]
,          SYSDATE
,          @[tgt_cntr_vol]
,          CBM_PER_HR_QTY
    FROM   BKG_RF_CGO
    WHERE  BKG_NO = @[src_bkg_no]
    AND    CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="tgt_bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="tgt_cntr_vol" type="12" value="" out="N"/>
				<param name="src_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
