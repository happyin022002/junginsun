<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchContentsForSelfMailRSQL">
			<desc><![CDATA[email에 들어갈 데이터를 가져온다]]></desc>
			<sql><![CDATA[
SELECT BL_NO
	  ,VSL_CD
	  ,SKD_VOY_NO
	  ,SKD_DIR_CD
      ,POR_CD
	  ,POL_CD
	  ,POD_CD
	  ,DEL_CD 
#if (${ca_flg} == 'Y') 
FROM BKG_BKG_HIS
#else
FROM BKG_BOOKING
#end
WHERE BKG_NO = @[bkg_no]
#if (${ca_flg} == 'Y') 
AND CORR_NO = (SELECT MAX(CORR_NO) FROM BKG_BKG_HIS WHERE BKG_NO = @[bkg_no])
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
