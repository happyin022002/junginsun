<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOSearchEdiAdvJpCustomerRenewal2017RSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
    DATA00, DATA01, DATA02, DATA03, DATA04, DATA05
  , DATA06, DATA07, DATA08, DATA09, DATA10
  , CASE WHEN TO_ORDER_FLAG > 0 THEN RPAD('TO ORDER',70, ' ')
         ELSE RPAD(DATA11,70, ' ')
    END AS DATA11    
    
  , CASE WHEN TO_ORDER_FLAG > 0 THEN RPAD('TO ORDER',175, ' ') /* NAME이 TO ORDER 이면 주소도 TO ORDER */
         ELSE RPAD(DATA12,175, ' ')
    END AS DATA12
  , DATA13, DATA14, DATA15
  
  
  , DATA16, DATA17    
  , CASE WHEN TO_ORDER_FLAG > 0 THEN DATA28                    /* NAME이 TO ORDER 이면 Notify Country Code */
         ELSE DATA18
    END AS DATA18
    
  , CASE WHEN TO_ORDER_FLAG > 0 THEN RPAD('TO ORDER',14, ' ') /* NAME이 TO ORDER 이면 전화번호도 TO ORDER */
         ELSE RPAD(REGEXP_REPLACE(DATA19,'[^0-9]',''),14, ' ')
    END AS DATA19
  , DATA20
  
  , DATA21, DATA22, DATA23, DATA24, DATA25
  , DATA26, DATA27, DATA28, DATA29, DATA30
  , DATA31, DATA32, DATA33, DATA34, DATA35
  , DATA36, DATA37, DATA38, DATA39
FROM (

        SELECT --34 Shipper Code  (17)
               RPAD(' ', 17, ' ') AS DATA00,
               --35 Shipper Name
               RPAD(SUBSTR(BKG_SPCLCHAR_CONV_FNC(NVL(A.CUST_NM, ' '), 'J'), 1, 70), 70, ' ') AS DATA01,
               --36 Shipper ADDR
               RPAD(SUBSTR(BKG_SPCLCHAR_CONV_FNC(NVL(A.CUST_ADDR, ' '), 'J'), 1, 175), 175, ' ') AS DATA02,
               --37 Shipper 주소 1/4(Street and number/P.O.Box)  (70)   C
               RPAD(' ', 70, ' ') AS DATA03,
               --38 Shipper 주소 2/4(Street and number/P.O.Box)  (35)   C
               RPAD(' ', 35, ' ') AS DATA04,
               --39 Shipper 주소 3/4(City name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA05,
               --40 Shipper 주소 4/4(Country sub-entity, name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA06,
               --41 Shipper ZIP
               RPAD(SUBSTR(NVL(A.CUST_ZIP_ID, ' '), 1, 9), 9, ' ') AS DATA07,
               --42 Shipper Country
               RPAD(SUBSTR(NVL(A.CUST_CNT_CD, ' '), 1, 2), 2, ' ') AS DATA08,
               --43 Shipper Phone No
               RPAD(SUBSTR(REGEXP_REPLACE(A.PHN_NO,'[^0-9]',''), 
                           1, 14), 
                     14, ' ') AS DATA09,
               ----------------------------------------------------------------------------------------------------------------
               --44 Consignee Code  (17)
               RPAD(' ', 17, ' ') AS DATA10,
               --45 Consignee Name
               RPAD(SUBSTR(BKG_SPCLCHAR_CONV_FNC(NVL(B.CUST_NM, ' '), 'J'), 1, 70), 70, ' ') AS DATA11,
               --TO_ORDER_FLAG
               INSTR(REPLACE(B.CUST_NM,' ',''),'TOORDER') AS TO_ORDER_FLAG,
               --46 Consignee ADDR
               RPAD(SUBSTR(BKG_SPCLCHAR_CONV_FNC(NVL(B.CUST_ADDR, ' '), 'J'), 1, 175), 175, ' ') AS DATA12,
               --47 Consignee 주소 1/4(Street and number/P.O.Box)  (70)   C
               RPAD(' ', 70, ' ') AS DATA13,
               --48 Consignee 주소 2/4(Street and number/P.O.Box)  (35)   C
               RPAD(' ', 35, ' ') AS DATA14,
               --44 Consignee 주소 3/4(City name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA15,
               --50 Consignee 주소 4/4(Country sub-entity, name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA16,
               --51 Consignee ZIP
               RPAD(SUBSTR(NVL(B.CUST_ZIP_ID, ' '), 1, 9), 9, ' ') AS DATA17,
               --52 Consignee Country
               RPAD(SUBSTR(NVL(B.CUST_CNT_CD, ' '), 1, 2), 2, ' ') AS DATA18,
               --53 Consignee Phone No
               RPAD(SUBSTR(REGEXP_REPLACE(B.PHN_NO,'[^0-9]',''), 
                           1, 14), 
                    14, ' ') AS DATA19,
               -----------------------------------------------------------------------------------------------------------------
               --54 Notify Code  (17)
               RPAD(' ', 17, ' ') AS DATA20,
               --55 Notify Name
               RPAD(SUBSTR(BKG_SPCLCHAR_CONV_FNC(NVL(C.CUST_NM, ' '), 'J'), 1, 70), 70, ' ') AS DATA21,
               --56 Notify ADDR
               RPAD(SUBSTR(BKG_SPCLCHAR_CONV_FNC(NVL(C.CUST_ADDR, ' '), 'J'), 1, 175), 175, ' ') AS DATA22,
               --57 Notify 주소 1/4(Street and number/P.O.Box)  (70)   C
               RPAD(' ', 70, ' ') AS DATA23,
               --58 화물 발송인 주소 2/4(Street and number/P.O.Box)  (35)   C
               RPAD(' ', 35, ' ') AS DATA24,
               --59 화물 발송인 주소 3/4(City name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA25,
               --60 화물 발송인 주소 4/4(Country sub-entity, name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA26,
               --61 Notify Party Postal Code (Postcode identification)
               RPAD(SUBSTR(NVL(C.CUST_ZIP_ID, ' '), 1, 9), 9, ' ') AS DATA27,
               --62 Notify Party Country Code (Country, coded)
               RPAD(SUBSTR(NVL(C.CUST_CNT_CD, ' '), 1, 2), 2, ' ') AS DATA28,
               --63 Notify Party Telephone Number
               RPAD(SUBSTR(REGEXP_REPLACE(C.PHN_NO,'[^0-9]',''), 
                           1, 14), 
                    14, ' ') AS DATA29,
               -----------------------------------------------------------------------------------------------------------------
               --64 Notify Code  (12)
               RPAD(' ', 17, ' ') AS DATA30,
               --65 Notify Name
               RPAD(' ', 70, ' ') AS DATA31,
               --66 Notify ADDR
               RPAD(' ', 175, ' ') AS DATA32,
               --67 Notify 주소 1/4(Street and number/P.O.Box)  (70)   C
               RPAD(' ', 70, ' ') AS DATA33,
               --68 화물 발송인 주소 2/4(Street and number/P.O.Box)  (35)   C
               RPAD(' ', 35, ' ') AS DATA34,
               --69 화물 발송인 주소 3/4(City name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA35,
               --70 화물 발송인 주소 4/4(Country sub-entity, name)  (35)   C
               RPAD(' ', 35, ' ') AS DATA36,
               --71 Shipper ZIP
               RPAD(' ', 9, ' ') AS DATA37,
               --72 Shipper Country
               RPAD(' ', 2, ' ') AS DATA38,
               --73 Shipper Phone No
               RPAD(' ', 14, ' ') AS DATA39
               
          FROM BKG_CSTMS_ADV_JP_CUST A,
               BKG_CSTMS_ADV_JP_CUST B,
               BKG_CSTMS_ADV_JP_CUST C

         WHERE 1 = 1
           AND A.BL_NO = @[bl_no]
           AND A.BL_SPLIT_NO = NVL(@[bl_split_no], '  ')
           AND A.BKG_CUST_TP_CD = 'S'
           AND A.BL_NO = B.BL_NO
           AND A.BL_SPLIT_NO = B.BL_SPLIT_NO
           AND B.BKG_CUST_TP_CD = 'C'
           AND A.BL_NO = C.BL_NO
           AND A.BL_SPLIT_NO = C.BL_SPLIT_NO
           AND C.BKG_CUST_TP_CD = 'N'
)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
