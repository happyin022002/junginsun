<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchPreMFRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) BL_COUNT 
FROM BKG_CSTMS_ADV_BL BL
     ,BKG_CSTMS_ADV_RCV_LOG R_LOG
     ,BKG_CSTMS_ADV_EDI_BL_RSPN EDI
WHERE   1=1
AND     BL.BL_NO = EDI.BL_NO
AND     BL.CNT_CD = EDI.CNT_CD
AND     R_LOG.CNT_CD     = EDI.CNT_CD
AND     R_LOG.CRR_BAT_NO = EDI.CRR_BAT_NO
AND     BL.CNT_CD     = 'US'
AND     BL.MF_SND_DT IS NOT NULL
AND     BL.BL_NO IN (
                        SELECT PRE_MF_NO
                          FROM BKG_CSTMS_ADV_BL
                         WHERE CNT_CD = 'US'
                           AND BL_NO = @[bl_no]
                    )			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
