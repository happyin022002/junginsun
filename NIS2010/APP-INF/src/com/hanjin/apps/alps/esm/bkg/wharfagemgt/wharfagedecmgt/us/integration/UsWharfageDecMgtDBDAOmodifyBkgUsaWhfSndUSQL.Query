<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOmodifyBkgUsaWhfSndUSQL">
			<desc><![CDATA[modifyBkgUsaWhfSnd]]></desc>
			<sql><![CDATA[
UPDATE BKG_USA_WHF_SND SET 
	VSL_NM = @[vsl_nm]
,	VSL_VOY_DIR_NO = @[vsl_voy_dir_no]
,	BRTH_DESC = @[brth_desc]
,	DEP_DT = TO_DATE(@[dep_dt],'YYYY-MM-DD')
,	ARR_DT = TO_DATE(@[arr_dt],'YYYY-MM-DD')
,	BIL_RCV_PTY_NM = @[bil_rcv_pty_nm]
,	BIL_SND_PTY_NM = @[bil_snd_pty_nm]
,	SND_RMK = @[snd_rmk]
,	WHF_DC_RT = @[whf_dc_rt]
,	DDCT_AMT = @[ddct_amt]
,	LOCL_UPD_DT = SYSDATE
,	UPD_USR_ID = @[cre_usr_id]
,	UPD_DT = SYSDATE
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	PORT_CD = @[port_cd]
AND	IO_BND_CD = @[io_bnd_cd]
			]]></sql>
			<params>
				<param name="vsl_nm" type="12" value="" out="N"/>
				<param name="vsl_voy_dir_no" type="12" value="" out="N"/>
				<param name="brth_desc" type="12" value="" out="N"/>
				<param name="dep_dt" type="12" value="" out="N"/>
				<param name="arr_dt" type="12" value="" out="N"/>
				<param name="bil_rcv_pty_nm" type="12" value="" out="N"/>
				<param name="bil_snd_pty_nm" type="12" value="" out="N"/>
				<param name="snd_rmk" type="12" value="" out="N"/>
				<param name="whf_dc_rt" type="6" value="" out="N"/>
				<param name="ddct_amt" type="6" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
