<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateProposalDBDAOPriSpBlplCopyVOCSQL">
			<desc><![CDATA[pri_sp_blpl을 guideline에서 copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_BLPL (
	PROP_NO
,	AMDT_SEQ
,	BLPL_SEQ
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	BLPL_TIT_NM
,	DP_SEQ
,	N1ST_CMNC_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT 
	@[prop_no]
,	@[amdt_seq]
,	DENSE_RANK() OVER (ORDER BY BLPL_SEQ) BLPL_SEQ
,	'I'
,	'GC'
,	BLPL_TIT_NM
,	DENSE_RANK() OVER (ORDER BY DP_SEQ) DP_SEQ
,	@[amdt_seq]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM PRI_SG_BLPL
WHERE BLPL_HDR_SEQ = @[blpl_hdr_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="blpl_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
