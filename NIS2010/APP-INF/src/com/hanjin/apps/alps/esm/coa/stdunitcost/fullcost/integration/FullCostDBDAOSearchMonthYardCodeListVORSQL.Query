<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FullCostDBDAOSearchMonthYardCodeListVORSQL">
			<desc><![CDATA[PRD_PORT_TML_MTX, MDM_YARD 테이블의 데이터 조회 
]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.TML_CD AS NOD_CD	
      ,B.YD_NM
      ,A.VSL_SLAN_CD AS SLAN_CD
      ,A.SKD_DIR_CD
      ,'' AS SEL_CHECK
  FROM PRD_PORT_TML_MTX A,MDM_YARD B
 WHERE A.TML_CD = B.YD_CD

  #if (${f_loc_cd} != '') 
  AND A.PORT_CD  = @[f_loc_cd]
 #end

  #if (${f_slan_cd} != '') 
  AND A.VSL_SLAN_CD = @[f_slan_cd]
 #end

ORDER BY NOD_CD	
        ,SLAN_CD
        ,SKD_DIR_CD			]]></sql>
			<params>
				<param name="f_loc_cd" type="12" value="" out="N"/>
				<param name="f_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
