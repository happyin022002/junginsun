<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchOfcCdRSQL">
			<desc><![CDATA[SearchOfcCd]]></desc>
			<sql><![CDATA[
select decode(@[iss_ofc_cd],null,'Y',decode(count(*),0,'N','Y')) as valFlg
 from mdm_organization
where ofc_cd=NVL(@[iss_ofc_cd],'')
and delt_flg = 'N'			]]></sql>
			<params>
				<param name="iss_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
