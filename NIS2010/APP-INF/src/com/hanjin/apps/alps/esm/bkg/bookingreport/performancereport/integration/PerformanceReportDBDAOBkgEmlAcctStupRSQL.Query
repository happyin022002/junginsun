<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOBkgEmlAcctStupRSQL">
			<desc><![CDATA[BkgEmlAcctStup]]></desc>
			<sql><![CDATA[
SELECT 
	EML_CPY_TO_EML
,	EML_ACCT_SEQ
,	BKG_OFC_CD
,	EML_PRIO_NO
,	INCL_SUB_OFC_FLG
,	DPCS_EML_SVC_KND_CD
,	DPCS_EML_STND_GRP_TP_CD
,	VBS_CTNT
,	POL_DPCS_EML_LOC_GRP_TP_CD
,	POL_CD
,	DEL_DPCS_EML_LOC_GRP_TP_CD
,	DEL_CD
,	DELT_FLG
,	CRE_USR_ID
,	TO_CHAR(CRE_DT,'YYYY-MM-DD') CRE_DT
,	UPD_USR_ID
,	TO_CHAR(UPD_DT,'YYYY-MM-DD') UPD_DT
,	RGN_OFC_CD
FROM BKG_EML_ACCT_STUP
WHERE	1=1
AND	NVL(DELT_FLG,'N') = 'N'
#if (${bkg_ofc_cd} != '') 
AND	BKG_OFC_CD = @[bkg_ofc_cd]
#end			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
