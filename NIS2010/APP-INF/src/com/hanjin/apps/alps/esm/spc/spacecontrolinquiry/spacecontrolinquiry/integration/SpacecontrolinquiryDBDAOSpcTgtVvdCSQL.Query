<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAOSpcTgtVvdCSQL">
			<desc><![CDATA[한국지점에서 리포트할 VVD List 저장]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_TGT_VVD (
    TRD_CD
  , RLANE_CD
  , VSL_CD
  , SKD_VOY_NO
  , SKD_DIR_CD
  , BSE_DT
  , CRE_USR_ID
  , CRE_DT
)
SELECT   @[trd_cd]
       , @[rlane_cd]
       , SUBSTR(@[vvd], 1, 4)
       , SUBSTR(@[vvd], 5, 4)
       , SUBSTR(@[vvd], 9)
       , TO_CHAR(SYSDATE, 'YYYYMMDD')
       , @[cre_usr_id]
       , SYSDATE
  FROM DUAL			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
