<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOModifyHandlingOfficeUSQL">
			<desc><![CDATA[E-BKG Handling Office 를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_HNDL_OFC_STUP
   SET POL_CD      = @[pol_cd]
     , CMDT_NM     = @[cmdt_nm]
     , HNDL_OFC_CD = @[hndl_ofc_cd]
     , UPD_USR_ID  = @[upd_usr_id]     
     , UPD_DT      = SYSDATE
 WHERE BKG_HNDL_OFC_SEQ = @[bkg_hndl_ofc_seq]			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="cmdt_nm" type="12" value="" out="N"/>
				<param name="hndl_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_hndl_ofc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
