<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOremoveBkgRefDtlAfterSplitDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_REF_DTL
WHERE	BKG_NO = @[bkg_no]
AND	CNTR_NO = @[cntr_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
