<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateQuotationDBDAORsltPriSurchargeAdjustListVORSQL">
			<desc><![CDATA[PRS-Surcharge Adjust 조회   ]]></desc>
			<sql><![CDATA[
SELECT qttn_no, 
    qttn_ver_no, 
	gen_spcl_rt_tp_cd,
    scg_adj_seq, 
    prc_cmdt_tp_cd, 
    prc_cmdt_def_cd, 
    org_loc_tp_cd, 
    org_loc_def_cd, 
    org_via_loc_tp_cd, 
    org_via_loc_def_cd, 
    dest_via_loc_tp_cd, 
    dest_via_loc_def_cd, 
    dest_loc_tp_cd, 
    dest_loc_def_cd, 
    prc_rcv_term_cd, 
    prc_de_term_cd, 
    bkg_rat_ut_cd, 
    prc_cgo_tp_cd, 
    chg_cd, 
    curr_cd, 
    adj_scg_amt, 
    adj_scg_usd_amt, 
    cre_usr_id, 
    cre_dt, 
    upd_usr_id, 
    upd_dt 
FROM pri_sq_scg_adj
WHERE qttn_no = @[qttn_no]
    and qttn_ver_no = @[qttn_ver_no]
	and gen_spcl_rt_tp_cd=@[gen_spcl_rt_tp_cd]
ORDER BY prc_cmdt_def_cd,org_loc_def_cd,org_via_loc_def_cd,dest_via_loc_def_cd,dest_loc_def_cd			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
