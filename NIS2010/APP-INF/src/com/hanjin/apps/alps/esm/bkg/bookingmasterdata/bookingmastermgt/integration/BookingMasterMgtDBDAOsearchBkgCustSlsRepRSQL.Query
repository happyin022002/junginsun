<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOsearchBkgCustSlsRepRSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
SELECT 
	SREP_CD
,	CUST_CNT_CD
,	CUST_SEQ
,	DELT_FLG
,	DP_SEQ
,	SREP_CUST_CLSS_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_CUST_SLS_REP
WHERE	SREP_CD = @[srep_cd]
AND	CUST_CNT_CD = @[cust_cnt_cd]
AND	CUST_SEQ = @[cust_seq]
			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
