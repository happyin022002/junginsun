<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationAssignmentDBDAOPriTradeAuthorizationVOCSQL">
			<desc><![CDATA[Trade Authorization Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_AUTHORIZATION (
      PRC_CTRT_TP_CD
    , SVC_SCP_CD
    , USR_ID
    , EFF_DT
    , EXP_DT
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
	, TRD_CD
)
(
SELECT 
	@[prc_ctrt_tp_cd] AS PRC_CTRT_TP_CD
	, SVC_SCP_CD
	, @[usr_id] AS USR_ID
	, TO_DATE(@[eff_dt],'YYYYMMDD') AS EFF_DT
	, TO_DATE('99991231','YYYYMMDD') AS EXP_DT
	, @[cre_usr_id] AS CRE_USR_ID
	, SYSDATE AS CRE_DT
	, @[cre_usr_id] AS UPD_USR_ID
	, SYSDATE AS UPD_DT
	, @[trd_cd]
FROM PRI_SCG_TRD_SVC_SCP_MAPG
WHERE TRD_CD = @[trd_cd]

)			]]></sql>
			<params>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
