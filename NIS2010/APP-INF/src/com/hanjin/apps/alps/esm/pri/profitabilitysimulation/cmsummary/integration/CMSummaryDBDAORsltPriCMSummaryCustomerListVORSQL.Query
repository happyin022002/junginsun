<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMSummaryDBDAORsltPriCMSummaryCustomerListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.CUST_CNT_CD||LPAD(A.CUST_SEQ, 6, '0') AS CUST_CD, OFC_CD,
  A.CUST_CNT_CD,
  A.CUST_SEQ,
  A.CUST_LGL_ENG_NM ,
 '' AS CUST_LIST,
 '' AS search_type
FROM MDM_CUSTOMER A
WHERE A.DELT_FLG = 'N'
	#if (${cust_cnt_cd} != '') 
  	AND A.CUST_CNT_CD = @[cust_cnt_cd]
	#end
	#if (${cust_lgl_eng_nm} != '') 
		AND A.CUST_LGL_ENG_NM LIKE '%' || UPPER(@[cust_lgl_eng_nm]) || '%'
	#end

	#if (${cust_seq} != '') 
		AND A.CUST_SEQ = @[cust_seq]
	#end
	#if( $cust_list.size()  != 0 )
		AND (A.CUST_CNT_CD , A.CUST_SEQ) NOT IN (
 		#foreach( ${key} in ${cust_list}) 
			#if($velocityCount != 1 ) 
				UNION ALL
			#end
			SELECT 
					substr('$key',1,2),substr('$key',3)
			FROM DUAL
		#end
		)
	#end

AND A.NMD_CUST_FLG = 'N'			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_lgl_eng_nm" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
