<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOMultiDailyHireUSQL">
			<desc><![CDATA[MultiDailyHire UPDATE]]></desc>
			<sql><![CDATA[
UPDATE MAS_CHRG_VSL_DLY_HIR SET
   CHRG_DHIR_AMT = @[chrg_dhir_amt]
  ,UPD_USR_ID    = @[upd_usr_id]
  ,UPD_DT        = SYSDATE
WHERE COST_YRMON = @[cost_yrmon]
  AND VSL_CD     = @[vsl_cd]			]]></sql>
			<params>
				<param name="chrg_dhir_amt" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="cost_yrmon" type="12" value="" out="Y"/>
				<param name="vsl_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
