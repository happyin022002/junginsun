<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOmodifyBkgUsaWhfExptCmdtUSQL">
			<desc><![CDATA[modifyBkgUsaWhfExptCmdt]]></desc>
			<sql><![CDATA[
UPDATE BKG_USA_WHF_EXPT_CMDT SET 
	KW_NM1 = @[kw_nm1]
,	KW_NM2 = @[kw_nm2]
,	CMDT_DESC = @[cmdt_desc]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	PORT_CD = @[port_cd]
AND	CMDT_SEQ = @[cmdt_seq]
			]]></sql>
			<params>
				<param name="kw_nm1" type="12" value="" out="N"/>
				<param name="kw_nm2" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="cmdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
