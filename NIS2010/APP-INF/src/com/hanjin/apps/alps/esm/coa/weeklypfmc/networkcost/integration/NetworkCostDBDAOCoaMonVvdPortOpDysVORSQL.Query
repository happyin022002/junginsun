<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOCoaMonVvdPortOpDysVORSQL">
			<desc><![CDATA[Lane Transit Time 2 조회   ]]></desc>
			<sql><![CDATA[
SELECT SUBSTR (A1.COST_YRMON, 0, 4)||'-'||A1.COST_WK AS COST_YRWK,
       A5.VOP_CD,
       A2.VSL_CD,
       A2.SKD_VOY_NO,
       A2.DIR_CD,
       (SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03217' AND INTG_CD_VAL_CTNT = D.HUL_BND_CD) HUL_BND_CD,
       A4.CONTI_NM,
       A2.SLAN_CD,
       A2.TRD_CD,
       A2.RLANE_CD,
       A2.LOC_CD,
       A2.VSL_DBL_CALL_SEQ,
       A2.CLPT_SEQ,
       A2.PORT_DYS,
       A2.SEA_DYS,
       A2.TTL_TZ_DYS,
       A2.VVD_RMK,
       A2.IOC_CD
  FROM COA_MON_VVD A1,
       COA_VSL_RGST A5,
       COA_MON_VVD_PORT_OP_DYS A2,
       MDM_LOCATION A3,
       MDM_CONTINENT A4,
	   COA_LANE_RGST D
 WHERE A1.TRD_CD = A2.TRD_CD
   AND A1.RLANE_CD = A2.RLANE_CD
   AND A1.IOC_CD = A2.IOC_CD
   AND A1.VSL_CD = A2.VSL_CD
   AND A1.SKD_VOY_NO = A2.SKD_VOY_NO
   AND A1.DIR_CD = A2.DIR_CD
   AND A1.DELT_FLG <> 'Y'
   AND A2.LOC_CD = A3.LOC_CD
   AND A3.CONTI_CD = A4.CONTI_CD
   AND A1.VSL_CD = A5.VSL_CD(+)
   AND A2.NEW_OP_DYS_FLG = 'Y' 
   AND A2.TRD_CD          = D.TRD_CD
   AND A2.RLANE_CD        = D.RLANE_CD
   AND A2.IOC_CD          = D.IOC_CD
   AND A2.DIR_CD          = D.DIR_CD
   AND A1.N1ST_LODG_PORT_ETD_DT BETWEEN A5.VSL_APLY_FM_DT(+) AND A5.VSL_APLY_TO_DT(+)
#if (${f_seltrade} != '')
   AND A1.TRD_CD = @[f_seltrade]
#end
#if (${f_selrlane} != '')
   AND A1.RLANE_CD = @[f_selrlane]
#end
#if (${f_seldir} != '')
   AND A1.DIR_CD = @[f_seldir]
#end
#if (${f_selioc} != '')
   AND A1.IOC_CD = @[f_selioc]
#end
#if (${f_vsl_cd} != '')
   AND A1.VSL_CD = @[f_vsl_cd]
#end
#if (${f_skd_voy_no} != '')
   AND A1.SKD_VOY_NO = @[f_skd_voy_no]
#end
#if (${f_dir_cd} != '')
   AND A1.DIR_CD = @[f_dir_cd]
#end
#if (${f_chkprd} == 'M')
   AND A1.COST_YRMON BETWEEN @[f_year]||@[f_fm_mon] AND @[f_year]||@[f_to_mon]
#elseif (${f_chkprd} == 'W')
   AND A1.SLS_YRMON LIKE @[f_year]||'%'
   AND A1.COST_WK BETWEEN @[f_fm_wk] AND @[f_to_wk]
#end
ORDER BY COST_YRWK,
         A2.TRD_CD,
         A2.RLANE_CD,
         A2.VSL_CD,
         A2.SKD_VOY_NO,
         A2.DIR_CD,
         A2.CLPT_SEQ			]]></sql>
			<params>
				<param name="f_seltrade" type="12" value="" out="N"/>
				<param name="f_selrlane" type="12" value="" out="N"/>
				<param name="f_seldir" type="12" value="" out="N"/>
				<param name="f_selioc" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
				<param name="f_skd_voy_no" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_year" type="12" value="" out="N"/>
				<param name="f_fm_mon" type="12" value="" out="N"/>
				<param name="f_to_mon" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
