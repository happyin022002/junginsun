<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24CustomsTransmissionDBDAOSearchBlRouteCountryOBRSQL">
			<desc><![CDATA[Eur24CustomsTransmissionDBDAOSearchBlRouteCountryOBRSQL]]></desc>
			<sql><![CDATA[
/*  Eur24BlRouteCntListVOs Eur24CustomsTransmissionDBDAOSearchBlRouteCountryOB ( blNo) */
SELECT 
   
    SUBSTR(VPS_PORT_CD,1,2) ROUTE_CNT
    , MIN(CLPT_SEQ)
    
FROM
    (
    SELECT MIN(DECODE(A.POL_CD, B.VPS_PORT_CD, CLPT_SEQ)) POL_SEQ ,
      MAX(DECODE(A.POD_CD, B.VPS_PORT_CD, CLPT_SEQ)) POD_SEQ
    FROM BKG_CSTMS_EUR_IO_BL A,
         VSK_VSL_PORT_SKD B
   WHERE A.BND_TP_CD = 'O'
	 AND A.BL_NO=@[bl_no]
     AND A.VSL_CD = @[vsl_cd]
     AND A.SKD_VOY_NO = @[skd_voy_no]
     AND A.SKD_DIR_CD = @[skd_dir_cd]
     AND A.VSL_CD = B.VSL_CD
     AND A.SKD_VOY_NO = B.SKD_VOY_NO
     AND A.SKD_DIR_cD = B.SKD_dIR_CD 

    ) A
  ,VSK_VSL_PORT_SKD B
  WHERE B.VSL_CD = @[vsl_cd]
    AND B.SKD_VOY_NO = @[skd_voy_no]
    AND B.SKD_DIR_CD = @[skd_dir_cd]
  AND B.CLPT_SEQ BETWEEN A. POL_SEQ AND A.POD_SEQ
GROUP BY SUBSTR(VPS_PORT_CD,1,2) 
ORDER BY 2 			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
