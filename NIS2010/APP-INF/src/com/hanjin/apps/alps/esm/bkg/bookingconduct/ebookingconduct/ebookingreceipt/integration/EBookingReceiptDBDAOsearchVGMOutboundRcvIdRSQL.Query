<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchVGMOutboundRcvIdRSQL">
			<desc><![CDATA[searchVGMOutboundRcvId]]></desc>
			<sql><![CDATA[
WITH RCV_ID AS 
       (SELECT SUBSTR(MIN(KIND), 2) NTC_KND_CD, EDI_RCV_ID
          FROM 
               (--VERMAS
                SELECT DISTINCT '1VM' KIND, EY.RCVR_TRD_PRNR_ID AS EDI_RCV_ID
                     , EY.PRNR_PORT_CD AS PORT_CD
                     , EY.PRNR_SUB_LNK_CD AS YD_CD
                  FROM BKG_EDI_TRD_PRNR_SUB_LNK EY
                     , BKG_EDI_SUB_LNK_MSG MSG
                     , BKG_BOOKING BK
                     , BKG_VVD     VVD
                 WHERE BK.BKG_NO   = @[bkg_no]
                   AND BK.BKG_NO   = VVD.BKG_NO
                   AND EY.TRD_PRNR_SUB_LNK_SEQ = MSG.TRD_PRNR_SUB_LNK_SEQ
                   AND EY.PRNR_SUB_LNK_DIV_CD = '1'
                   AND MSG.MSG_TP_DESC        = '1'
                   AND MSG.EDI_MSG_TP_ID      = 'VERMAS'
                   AND (   (EY.PRNR_SUB_LNK_CD = BK.POL_NOD_CD  AND MSG.EDI_MSG_IND_CD IN ('13')) --입력된 찾아진 POL과 같거나
                        OR (EY.PRNR_SUB_LNK_CD <> BK.POL_NOD_CD AND EY.PRNR_SUB_LNK_CD = VVD.POL_YD_CD AND MSG.EDI_MSG_IND_CD IN ('14'))
                           -- T/S POL 이거나 
                       )
                ) 
          GROUP BY EDI_RCV_ID)
, SENT_ID AS (SELECT NTC_KND_CD, EDI_ID AS EDI_RCV_ID, TML_NTC_SND_STS_CD, HIS_SEQ--TYPE, ID별 마지막 전송 기록
                FROM BKG_NTC_HIS A
                WHERE BKG_NO      = @[bkg_no]
                 AND NTC_VIA_CD  = 'E'
                 AND NTC_KND_CD  IN ('VM')
                 AND HIS_SEQ = (SELECT MAX(HIS_SEQ)
                                  FROM BKG_NTC_HIS HIS
                                 WHERE A.BKG_NO = HIS.BKG_NO
                                   AND HIS.NTC_VIA_CD = 'E'
                                   AND A.NTC_KND_CD = HIS.NTC_KND_CD
                                   AND A.EDI_ID = HIS.EDI_ID))
SELECT NTC_KND_CD AS KIND, EDI_RCV_ID, '1' BRAC_CD   --CANCEL로 전송 한다
  FROM (SELECT SENT_ID.NTC_KND_CD, SENT_ID.EDI_RCV_ID--기존 전송처가 
          FROM SENT_ID, RCV_ID
                , BKG_EDI_TRD_PRNR_SUB_LNK EY
                , BKG_EDI_SUB_LNK_MSG      MSG
         WHERE SENT_ID.TML_NTC_SND_STS_CD <> '1'     --기존 전송처가 CANCE이면 제외
                        AND SENT_ID.TML_NTC_SND_STS_CD IS NOT NULL--기존 전송처의 STATUS가 불분명할 때도 제외
                        AND SENT_ID.EDI_RCV_ID = EY.RCVR_TRD_PRNR_ID--자동 전송처의 경우에만 cancel도 다시 전송
                        and EY.TRD_PRNR_SUB_LNK_SEQ = MSG.TRD_PRNR_SUB_LNK_SEQ
           AND EY.PRNR_SUB_LNK_DIV_CD = '1'
                        AND MSG.MSG_TP_DESC                      = '1'
           AND MSG.EDI_MSG_TP_ID      = 'VERMAS'
           AND msg.EDI_MSG_IND_CD     IN ('13','14')
                        AND @[brac_cd] <> '1' --CANCEL일 때는 제외
         MINUS
        SELECT NTC_KND_CD, EDI_RCV_ID--새 전송처에 없을 경우
          FROM RCV_ID)
UNION ALL
SELECT NTC_KND_CD AS KIND, EDI_RCV_ID, '9' BRAC_CD --NEW로 전송
  FROM (SELECT NTC_KND_CD, EDI_RCV_ID--새 전송처가 
          FROM RCV_ID
                     WHERE @[brac_cd] <> '1'        --CANCEL일 때는 제외
         MINUS
        SELECT NTC_KND_CD, EDI_RCV_ID--기존 전송처에 없을 경우
          FROM SENT_ID)        
 UNION ALL
SELECT DISTINCT RCV_ID.NTC_KND_CD AS KIND, RCV_ID.EDI_RCV_ID, DECODE(SENT_ID.TML_NTC_SND_STS_CD, '1', '9', '5') --마지막 전송이 R일 경우 'N'
  FROM RCV_ID, SENT_ID
WHERE RCV_ID.NTC_KND_CD = SENT_ID.NTC_KND_CD--기존 전송처에
   AND RCV_ID.EDI_RCV_ID = SENT_ID.EDI_RCV_ID--재전송을 경우			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="brac_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
