<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOmodifyBkgBlDocAfterSplitUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_DOC SET 
	PCK_QTY = @[pck_qty]
,	MEAS_QTY = @[meas_qty]
,	ACT_WGT = @[act_wgt]
,   UPD_USR_ID = @[usr_id]
,   UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="pck_qty" type="6" value="" out="N"/>
				<param name="meas_qty" type="6" value="" out="N"/>
				<param name="act_wgt" type="6" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
