<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqListDBDAOaddPriScqAtchFileCSQL">
			<desc><![CDATA[addPriScqAtchFile]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SCQ_ATCH_FILE
     ( SCQ_RQST_NO    
     , SPCL_CGO_TP_CD
     , FILE_SAV_ID   
     , ORG_FILE_NM   
     , CRE_USR_ID    
     , CRE_DT        
     , UPD_USR_ID    
     , UPD_DT ) 
VALUES
     ( @[scq_rqst_no]    
     , @[spcl_cgo_tp_cd]
     , @[file_sav_id]   
     , (SELECT FILE_UPLD_NM
          FROM COM_UPLD_FILE
         WHERE FILE_SAV_ID = @[file_sav_id]
           AND PGM_SUB_SYS_CD = 'PRI')     
     , @[cre_usr_id]    
     , SYSDATE        
     , @[cre_usr_id]    
     , SYSDATE )			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
