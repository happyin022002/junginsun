<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOCheckSlsRepCodeRSQL">
			<desc><![CDATA[Sales Rep check]]></desc>
			<sql><![CDATA[
SELECT SREP_CD 
       ,MSR.SREP_NM
       ,MSR.OFC_CD
       ,MSR.MPHN_NO
       ,MSR.SREP_EML
FROM MDM_SLS_REP MSR
WHERE 1=1
AND SREP_CD = @[srep_cd]
AND SREP_STS_CD = 'N'			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
