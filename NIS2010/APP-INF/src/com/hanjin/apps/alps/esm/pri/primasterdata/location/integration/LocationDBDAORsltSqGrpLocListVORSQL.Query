<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAORsltSqGrpLocListVORSQL">
			<desc><![CDATA[PRI_SQ_GRP_LOC 조회]]></desc>
			<sql><![CDATA[
SELECT QTTN_NO
  	 , QTTN_VER_NO
  	 , GRP_LOC_SEQ
	 , PRC_GRP_LOC_CD CD
     , PRC_GRP_LOC_DESC NM
  FROM PRI_SQ_GRP_LOC
 WHERE QTTN_NO = @[qttn_no]
   AND QTTN_VER_NO = @[qttn_ver_no]
 ORDER BY PRC_GRP_LOC_CD ASC			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
