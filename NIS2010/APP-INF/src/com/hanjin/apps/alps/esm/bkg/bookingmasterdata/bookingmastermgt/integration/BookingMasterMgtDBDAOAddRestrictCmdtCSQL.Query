<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOAddRestrictCmdtCSQL">
			<desc><![CDATA[Import Restricted Commodities Set-up(INSERT)]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_IMP_RSTR_CMDT (
	RGN_OFC_CD,
	CNT_CD,
	LOC_CD,
	DP_SEQ,
	TS_FLG,
	FROB_FLG,
	RSTR_CMDT_NM,
	PROHI_CMDT_NM,
	EFF_DT,
	EXP_DT,
	WEB_SITE_URL,
	INTER_RMK,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	RSTR_CMDT_GRP_NM
) VALUES( 
	@[rgn_ofc_cd],
	@[cnt_cd],
	DECODE(@[loc_cd],'ALL','*****',@[loc_cd]),
	(SELECT NVL(MAX(DP_SEQ)+1, 1) 
		FROM BKG_IMP_RSTR_CMDT 
	   WHERE RGN_OFC_CD = @[rgn_ofc_cd] 
		 AND CNT_CD     = @[cnt_cd]
		 AND LOC_CD     = DECODE(@[loc_cd],'ALL','*****',@[loc_cd])),
	DECODE(NVL(@[ts_flg], 0), 0, 'N', 'Y'),
	DECODE(NVL(@[frob_flg], 0), 0, 'N', 'Y'),
	@[rstr_cmdt_nm],
	@[prohi_cmdt_nm],
	TO_DATE(@[eff_dt], 'YYYY-MM-DD HH24:MI:SS'),
	TO_DATE(@[exp_dt], 'YYYY-MM-DD HH24:MI:SS'),
	@[web_site_url],
	@[inter_rmk],
	@[cre_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE,
	@[rstr_cmdt_grp_nm]
)			]]></sql>
			<params>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="ts_flg" type="12" value="" out="N"/>
				<param name="frob_flg" type="12" value="" out="N"/>
				<param name="rstr_cmdt_nm" type="12" value="" out="N"/>
				<param name="prohi_cmdt_nm" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="web_site_url" type="12" value="" out="N"/>
				<param name="inter_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rstr_cmdt_grp_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
