<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOVslVslSkdCdRSQL">
			<desc><![CDATA[VSK_VSL_SKD 의 코드조합을 조회]]></desc>
			<sql><![CDATA[
SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD CD
     , VSL_CD||SKD_VOY_NO||SKD_DIR_CD NM
  FROM VSK_VSL_SKD
 WHERE VSL_CD = @[cd]
   AND SKD_VOY_NO = @[etc1]
   AND SKD_DIR_CD = @[etc2]			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
