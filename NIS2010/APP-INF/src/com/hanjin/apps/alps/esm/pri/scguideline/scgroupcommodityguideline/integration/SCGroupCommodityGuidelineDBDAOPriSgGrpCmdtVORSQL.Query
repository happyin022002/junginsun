<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupCommodityGuidelineDBDAOPriSgGrpCmdtVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	SVC_SCP_CD,
	GLINE_SEQ,
	PRC_CUST_TP_CD,
	GRP_CMDT_SEQ,
	PRC_GRP_CMDT_CD,
	PRC_GRP_CMDT_DESC
FROM PRI_SG_GRP_CMDT
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	GLINE_SEQ = @[gline_seq]
AND	PRC_CUST_TP_CD = @[prc_cust_tp_cd]
ORDER BY PRC_GRP_CMDT_CD			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="2" value="" out="N"/>
				<param name="prc_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
