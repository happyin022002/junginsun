<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchOfmMsnInfoRSQL">
			<desc><![CDATA[MSN Info를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(KR_CSTMS_BL_TP_CD,' ') KR_CSTMS_BL_TP_CD
     , NVL(MRN_BL_TS_CD,' ') MRN_BL_TS_CD
     , NVL(CSTMS_CLR_TP_CD,' ') CSTMS_CLR_TP_CD
     , NVL(CSTMS_DCHG_LOC_WH_CD,' ') CSTMS_DHG_LOC_WH_CD
     , NVL(CSTMS_CLR_LOC_CD,' ') CSTMS_CLR_LOC_CD
     , NVL(CSTMS_CLR_WH_CD,' ') CSTMS_CLR_WH_CD
FROM BKG_CSTMS_KR_MF_SEQ_NO
WHERE BKG_NO = @[bkg_no]
AND MF_REF_NO = @[mrn_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="mrn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
