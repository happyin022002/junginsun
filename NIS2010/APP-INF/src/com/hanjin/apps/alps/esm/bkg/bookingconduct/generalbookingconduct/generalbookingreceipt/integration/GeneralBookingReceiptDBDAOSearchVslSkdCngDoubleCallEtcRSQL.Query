<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchVslSkdCngDoubleCallEtcRSQL">
			<desc><![CDATA[Double Calling Port 의 Yard 변경 Notice 의 ETC 데이터를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SLAN_CD  
     , YD_CD
     , TO_CHAR(VPS_ETA_DT,'YYYY-MM-DD HH24:MI') VPS_ETA_DT
     , TO_CHAR(VPS_ETB_DT,'YYYY-MM-DD HH24:MI') VPS_ETB_DT
     , TO_CHAR(VPS_ETD_DT,'YYYY-MM-DD HH24:MI') VPS_ETD_DT 
  FROM VSK_VSL_PORT_SKD
 WHERE VSL_CD     = SUBSTR(@[vvd],1,4)
   AND SKD_VOY_NO = SUBSTR(@[vvd],5,4)
   AND SKD_DIR_CD = SUBSTR(@[vvd],9,1)
   AND VPS_PORT_CD = @[port_cd]
 ORDER BY CLPT_SEQ, CLPT_IND_SEQ			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
