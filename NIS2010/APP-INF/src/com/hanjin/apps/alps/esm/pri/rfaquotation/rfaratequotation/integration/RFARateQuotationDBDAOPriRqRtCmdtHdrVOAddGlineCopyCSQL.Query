<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAOPriRqRtCmdtHdrVOAddGlineCopyCSQL">
			<desc><![CDATA[gline copy insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RQ_RT_CMDT_HDR (
	QTTN_NO
,	QTTN_VER_NO
,	CMDT_HDR_SEQ
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	FIC_RT_TP_CD
)
SELECT  @[qttn_no]
,       @[qttn_ver_no]
,       CMDT_HDR_SEQ
,       'GC'
,       @[cre_usr_id]
,       SYSDATE
,       @[upd_usr_id]
,       SYSDATE
,		FIC_RT_TP_CD
FROM    PRI_RG_RT_CMDT_HDR
WHERE   1=1
AND    (SVC_SCP_CD, GLINE_SEQ) =  ( 
									SELECT  SVC_SCP_CD
                                    ,       GLINE_SEQ
                                      FROM (
                                            SELECT B.SVC_SCP_CD
                                                 , B.GLINE_SEQ
                                                 , ROW_NUMBER() OVER (ORDER BY B.EFF_DT DESC) AS SEQ
                                             FROM  PRI_RG_MN B
                                             WHERE B.SVC_SCP_CD = @[svc_scp_cd]
                                             AND   B.CFM_FLG = 'Y'
                                             AND   TO_CHAR(B.EFF_DT,'YYYY-MM-DD') <= @[eff_dt]
            								 AND   TO_CHAR(B.EXP_DT,'YYYY-MM-DD') >= @[eff_dt]
                                            )
                                       WHERE SEQ = 1
                                   )			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
