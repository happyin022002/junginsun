<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterSISrepEmlRSQL">
			<desc><![CDATA[RSQL]]></desc>
			<sql><![CDATA[
SELECT NVL(MR.SREP_EML,MR2.SREP_EML) SREP_EML 
  FROM MDM_SLS_REP MR,
       MDM_SLS_REP MR2,
       BKG_BOOKING BK
 WHERE BK.CTRT_SREP_CD = MR.SREP_CD(+)
   AND BK.OB_SREP_CD = MR2.SREP_CD(+)
   AND BK.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
