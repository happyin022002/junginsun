<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NoteConversionDBDAOPriNoteConvGrpLocDtlVODSQL">
			<desc><![CDATA[Group Location detail delete]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_NOTE_CONV_GRP_LOC_DTL
WHERE PRC_CTRT_TP_CD =  @[prc_ctrt_tp_cd]
  AND NOTE_CONV_GRP_LOC_CD = @[note_conv_grp_loc_cd]
  AND LOC_CD = @[loc_cd]
			]]></sql>
			<params>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="note_conv_grp_loc_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
