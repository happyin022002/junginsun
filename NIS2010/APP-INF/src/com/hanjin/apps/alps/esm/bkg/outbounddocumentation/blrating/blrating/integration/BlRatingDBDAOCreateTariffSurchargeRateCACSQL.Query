<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCreateTariffSurchargeRateCACSQL">
			<desc><![CDATA[CAì‹œ Tariff Surcharge Rate Insert]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_TRF_SCG_RT (
    BKG_NO 
#else
INSERT INTO BKG_TRF_SCG_RT_HIS (
    BKG_NO 
    , CORR_NO 
#end
    , RT_SEQ 
    , FRT_TERM_CD
    , CGO_CATE_CD 
    , CHG_CD 
    , CURR_CD 
    , RAT_UT_CD 
    , RAT_AS_QTY 
    , CHG_UT_AMT 
    , CHG_AMT 
    , RCV_TERM_CD 
    , DE_TERM_CD 
    , FRT_INCL_XCLD_DIV_CD   
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
        , RT_SEQ 
        , FRT_TERM_CD 
        , CGO_CATE_CD  
        , CHG_CD 
        , CURR_CD 
        , RAT_UT_CD 
        , RAT_AS_QTY 
        , CHG_UT_AMT 
        , CHG_AMT 
        , RCV_TERM_CD 
        , DE_TERM_CD 
        , FRT_INCL_XCLD_DIV_CD   
        , CRE_USR_ID 
        , CRE_DT 
        , UPD_USR_ID 
        , sysdate
#if (${copy_type_cd} == 'BKG')
  FROM BKG_TRF_SCG_RT_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_TRF_SCG_RT
 WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
