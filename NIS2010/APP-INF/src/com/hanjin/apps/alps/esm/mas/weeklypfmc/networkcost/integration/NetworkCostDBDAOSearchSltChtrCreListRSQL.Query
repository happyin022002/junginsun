<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOSearchSltChtrCreListRSQL">
			<desc><![CDATA[SearchSltChtrCreList SELECT]]></desc>
			<sql><![CDATA[
SELECT
    (CASE WHEN no = 1 THEN 'J'
          WHEN no = 2 THEN 'J'
          WHEN no = 3 THEN 'S'
     END) AS SLT_TP_CD
   ,(CASE WHEN no = 1 THEN 'Joint / Indepent Operation'
          WHEN no = 2 THEN 'Joint / Indepent Operation'
          WHEN no = 3 THEN 'Slot Charter Lane'
     END) AS SLT_TP_NM
   ,(CASE WHEN no = 1 THEN 'SML'
          WHEN no = 2 THEN 'OTH'
          WHEN no = 3 THEN 'OTH'
     END) AS VOP_CD
   ,(CASE WHEN no = 1 THEN ''
          WHEN no = 2 THEN ''
          WHEN no = 3 THEN ''
     END) AS INCM_BZC_CHTR_CD
   ,(CASE WHEN no = 1 THEN ''
          WHEN no = 2 THEN ''
          WHEN no = 3 THEN ''
     END) AS INCM_SUB_LSE_CD
   ,(CASE WHEN no = 1 THEN ''
          WHEN no = 2 THEN ''
          WHEN no = 3 THEN ''
     END) AS INCM_CRS_CHTR_CD
   ,(CASE WHEN no = 1 THEN ''
          WHEN no = 2 THEN 'C'
          WHEN no = 3 THEN 'C'
     END) AS EXPN_BZC_CHTR_CD
   ,(CASE WHEN no = 1 THEN ''
          WHEN no = 2 THEN ''
          WHEN no = 3 THEN ''
     END) AS EXPN_SUB_CHTR_CD
   ,(CASE WHEN no = 1 THEN ''
          WHEN no = 2 THEN ''
          WHEN no = 3 THEN ''
     END) AS EXPN_CRS_CHTR_CD
   ,'R' AS OP_CRE_STS_CD
   ,'Y' AS CRE_SLCT_FLG
  FROM DUAL
      ,(SELECT CPY_NO AS no FROM COM_CPY_NO WHERE CPY_NO BETWEEN 1 AND 3)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
