<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommAgreementDBDAOSearchSurchargeCntRSQL">
			<desc><![CDATA[Search Surcharge Cnt]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM PRI_SCG_RT
WHERE CHG_CD = @[chg_cd]
AND RAT_UT_CD = 'BL'
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
