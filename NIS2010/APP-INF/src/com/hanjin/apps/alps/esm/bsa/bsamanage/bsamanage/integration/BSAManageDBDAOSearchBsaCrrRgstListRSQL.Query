<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOSearchBsaCrrRgstListRSQL">
			<desc><![CDATA[BSAManageDBDAOSearchBsaCrrRgstListRSQL.Query]]></desc>
			<sql><![CDATA[
SELECT  
      A.BSA_OP_JB_CD 
     ,(SELECT 
            NVL(BSA_OP_JB_DESC,'') 
       FROM   
            BSA_OP_JB 
       WHERE  
            BSA_OP_CD = 'J' 
        AND BSA_OP_JB_CD = A.BSA_OP_JB_CD 
       ) AS BSA_OP_JB_NM 
      ,A.CRR_CD 
FROM    
     BSA_CRR_RGST A 
WHERE  
    ((A.BSA_OP_JB_CD = '001' AND A.CRR_CD <> 'SML') 
       OR A.BSA_OP_JB_CD  IN ('002','004')) 
 AND A.APLY_FLG = 'Y' 
GROUP BY 
         A.BSA_OP_JB_CD, 
         A.CRR_CD 
ORDER BY 
         A.BSA_OP_JB_CD, 
         A.CRR_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
