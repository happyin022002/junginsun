<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDDAOSearchUserNameRSQL">
			<desc><![CDATA[com_user에서 user_nm을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
	FAX_NO
,	USR_ID
,	USR_NM
,	USR_PWD
,	USE_FLG
,	MPHN_NO
,	USR_EML
,	CNT_CD
,	LANG_TP_CD
,	GMT_TMZN_CD
,	CNT_DT_FMT_CD
,	CNT_NO_FMT_CD
,	XTN_PHN_NO
--,	MN_SCRN_OPT_ID
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	OFC_CD
,	USR_LOCL_NM
,	USR_AUTH_TP_CD
,	JB_ENG_NM
,	PSN_ENG_NM
,	GRD_ENG_NM
FROM COM_USER
WHERE	USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
