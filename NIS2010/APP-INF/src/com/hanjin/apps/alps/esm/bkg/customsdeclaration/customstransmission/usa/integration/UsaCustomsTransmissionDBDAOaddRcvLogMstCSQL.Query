<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOaddRcvLogMstCSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신]]></desc>
			<sql><![CDATA[
INSERT 
  INTO BKG_CSTMS_ADV_RCV_LOG
      (
       CNT_CD
      ,IO_BND_CD
      ,RCV_DT,RCV_SEQ
      ,RCV_MSG_TP_ID
      ,VSL_CD
      ,SKD_VOY_NO
      ,SKD_DIR_CD
      ,POD_CD
      ,POL_CD
      ,CSTMS_BAT_NO
      ,CRR_BAT_NO
      ,SCAC_CD
      ,CRE_USR_ID
      ,UPD_USR_ID
)
VALUES
      (
       'US'
      ,'I'
      ,TO_DATE(@[ir_date],'RRMMDDHH24MISS')
      ,@[ir_seq]

#if (${is_baplie_rc} == 'Y') 
	  , 'RB'
#else 
      ,@[ir_type]
#end
      ,@[vsl_cd]
      ,@[skd_voy_no]
      ,@[skd_dir_cd]
      ,@[pod_loc]
      ,@[pol_cd]
      ,@[ir_batch_no]
      ,@[crr_bat_no]
      ,@[in_snp]
      ,'SYSTEM'
      ,'SYSTEM'
      )			]]></sql>
			<params>
				<param name="ir_date" type="12" value="" out="N"/>
				<param name="ir_seq" type="12" value="" out="N"/>
				<param name="ir_type" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_loc" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="ir_batch_no" type="12" value="" out="N"/>
				<param name="crr_bat_no" type="12" value="" out="N"/>
				<param name="in_snp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
