<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORqCpPriRpScpRtRoutPntCSQL">
			<desc><![CDATA[RqCpPriRpScpRtRoutPnt]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_RT_ROUT_PNT (
      PROP_NO
    , AMDT_SEQ
    , SVC_SCP_CD
    , CMDT_HDR_SEQ
    , ROUT_SEQ
    , ORG_DEST_TP_CD
    , ROUT_PNT_SEQ
    , ROUT_PNT_LOC_TP_CD
    , ROUT_PNT_LOC_DEF_CD
    , PRC_TRSP_MOD_CD
    , RCV_DE_TERM_CD
    , PRC_PROG_STS_CD
    , SRC_INFO_CD
    , N1ST_CMNC_AMDT_SEQ
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
	, FIC_ROUT_CMB_TP_CD
	, BSE_PORT_LOC_CD
)
WITH CMDT_HDR AS (
    SELECT A.QTTN_NO
         , A.QTTN_VER_NO
         , A.CMDT_HDR_SEQ AS OLD_CMDT_HDR_SEQ
         , DENSE_RANK() OVER (PARTITION BY A.QTTN_NO, A.QTTN_VER_NO
                              ORDER BY A.QTTN_NO, A.QTTN_VER_NO, A.CMDT_HDR_SEQ) AS CMDT_HDR_SEQ
    FROM PRI_RQ_RT_CMDT_HDR A
    WHERE A.QTTN_NO = @[qttn_no]
    AND   A.QTTN_VER_NO = @[qttn_ver_no]
    AND   (
        EXISTS (
            SELECT 'X'
            FROM PRI_RQ_RT_CMDT B
            WHERE B.QTTN_NO = A.QTTN_NO
            AND   B.QTTN_VER_NO = A.QTTN_VER_NO
            AND   B.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RQ_RT_CMDT_ROUT E
            WHERE E.QTTN_NO = A.QTTN_NO
            AND   E.QTTN_VER_NO = A.QTTN_VER_NO
            AND   E.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
            AND   (
                EXISTS (
                    SELECT 'X'
                    FROM PRI_RQ_RT_ROUT_PNT F
                    WHERE F.QTTN_NO = E.QTTN_NO
                    AND   F.QTTN_VER_NO = E.QTTN_VER_NO
                    AND   F.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
                    AND   F.ROUT_SEQ = E.ROUT_SEQ
                )
                OR EXISTS (
                    SELECT 'X'
                    FROM PRI_RQ_RT_ROUT_VIA G
                    WHERE G.QTTN_NO = E.QTTN_NO
                    AND   G.QTTN_VER_NO = E.QTTN_VER_NO
                    AND   G.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
                    AND   G.ROUT_SEQ = E.ROUT_SEQ
                )
                OR EXISTS (
                    SELECT 'X'
                    FROM PRI_RQ_RT J
                    WHERE J.QTTN_NO = E.QTTN_NO
                    AND   J.QTTN_VER_NO = E.QTTN_VER_NO
                    AND   J.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
                    AND   J.ROUT_SEQ = E.ROUT_SEQ
                )
            )
        )
    )
)
, CMDT_ROUT AS (
    SELECT L.QTTN_NO
         , L.QTTN_VER_NO
         , K.OLD_CMDT_HDR_SEQ
         , K.CMDT_HDR_SEQ
         , L.ROUT_SEQ AS OLD_ROUT_SEQ
         , DENSE_RANK() OVER (PARTITION BY L.QTTN_NO, L.QTTN_VER_NO, K.OLD_CMDT_HDR_SEQ
                              ORDER BY L.QTTN_NO, L.QTTN_VER_NO, K.OLD_CMDT_HDR_SEQ, L.ROUT_SEQ) AS ROUT_SEQ
    FROM CMDT_HDR K
        ,PRI_RQ_RT_CMDT_ROUT L
    WHERE L.QTTN_NO = K.QTTN_NO
    AND   L.QTTN_VER_NO = K.QTTN_VER_NO
    AND   L.CMDT_HDR_SEQ = K.OLD_CMDT_HDR_SEQ
    AND   (
        EXISTS (
            SELECT 'X'
            FROM PRI_RQ_RT_ROUT_PNT F
            WHERE F.QTTN_NO = L.QTTN_NO
            AND   F.QTTN_VER_NO = L.QTTN_VER_NO
            AND   F.CMDT_HDR_SEQ = L.CMDT_HDR_SEQ
            AND   F.ROUT_SEQ = L.ROUT_SEQ
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RQ_RT_ROUT_VIA G
            WHERE G.QTTN_NO = L.QTTN_NO
            AND   G.QTTN_VER_NO = L.QTTN_VER_NO
            AND   G.CMDT_HDR_SEQ = L.CMDT_HDR_SEQ
            AND   G.ROUT_SEQ = L.ROUT_SEQ
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RQ_RT J
            WHERE J.QTTN_NO = L.QTTN_NO
            AND   J.QTTN_VER_NO = L.QTTN_VER_NO
            AND   J.CMDT_HDR_SEQ = L.CMDT_HDR_SEQ
            AND   J.ROUT_SEQ = L.ROUT_SEQ
        )
    )
)
SELECT @[new_prop_no] AS PROP_NO
     , 0 AS AMDT_SEQ
     , Z.SVC_SCP_CD
     , R.CMDT_HDR_SEQ
     , R.ROUT_SEQ
     , S.ORG_DEST_TP_CD
     , DENSE_RANK() OVER (PARTITION BY S.QTTN_NO, S.QTTN_VER_NO
                                     , S.CMDT_HDR_SEQ, S.ROUT_SEQ, S.ORG_DEST_TP_CD
                          ORDER BY S.QTTN_NO, S.QTTN_VER_NO
                                 , S.CMDT_HDR_SEQ, S.ROUT_SEQ, S.ORG_DEST_TP_CD, S.ROUT_PNT_SEQ) AS ROUT_PNT_SEQ
     , S.ROUT_PNT_LOC_TP_CD
     , S.ROUT_PNT_LOC_DEF_CD
     , S.PRC_TRSP_MOD_CD
     , S.RCV_DE_TERM_CD
     , 'I' AS PRC_PROG_STS_CD
     , S.SRC_INFO_CD
     , 0 AS N1ST_CMNC_AMDT_SEQ
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
	 , S.FIC_ROUT_CMB_TP_CD
	 , S.BSE_PORT_LOC_CD
FROM CMDT_ROUT R
    ,PRI_RQ_RT_ROUT_PNT S
    ,PRI_RQ_MN Z
WHERE S.QTTN_NO = R.QTTN_NO
AND   S.QTTN_VER_NO = R.QTTN_VER_NO
AND   S.CMDT_HDR_SEQ = R.OLD_CMDT_HDR_SEQ
AND   S.ROUT_SEQ = R.OLD_ROUT_SEQ
AND   S.QTTN_NO = Z.QTTN_NO
AND   S.QTTN_VER_NO = Z.QTTN_VER_NO			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
