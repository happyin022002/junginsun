<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EbookingReceiptDBDAOselectBkgRqstNoByBkgNoRSQL">
			<desc><![CDATA[BkgNo로 BkgRqst 정보를 조회한다.]]></desc>
			<sql><![CDATA[
select XTER_SNDR_ID SENDER_ID
	  ,XTER_RQST_NO RQST_NO
	  ,XTER_RQST_SEQ RQST_SEQ
  from bkg_xter_rqst_mst mst
 where bkg_no = @[bkg_no]
   and doc_tp_cd = 'B'
   and rqst_dt = (select max(rqst_dt)
                    from bkg_xter_rqst_mst mst2
                   where mst.bkg_no = mst2.bkg_no
                     and mst.doc_tp_cd = mst2.doc_tp_cd)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
