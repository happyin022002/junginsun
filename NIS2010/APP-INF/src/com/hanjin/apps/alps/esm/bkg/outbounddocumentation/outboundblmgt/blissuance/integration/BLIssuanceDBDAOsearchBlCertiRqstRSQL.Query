<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchBlCertiRqstRSQL">
			<desc><![CDATA[BLIssuanceDBDAOsearchBlCertiRqstRSQL]]></desc>
			<sql><![CDATA[
SELECT *
  FROM (
    SELECT BKG.BKG_NO
          ,BL.BL_NO
          ,BL.BL_CERTI_SEQ
          ,BL.CO_NM
          ,BL.POL_CD
          ,BL.BL_CERTI_STS_CD
          ,TO_CHAR(BL.RQST_DT, 'YYYY-MM-DD HH24:MI') AS RQST_DT
          ,'' AS ACT_DT 
          ,DTL.INTG_CD_VAL_DESC AS BL_CERTI_STS
          ,BKG.OB_SREP_CD
          ,BL.BL_NO || '_' || BL.BL_CERTI_SEQ AS FILE_NM
          ,NVL(BL.BL_CERTI_PRN_FLG,'N') AS BL_CERTI_PRN_FLG
      FROM BKG_BL_CERTI BL
          ,BKG_BOOKING BKG
          ,COM_INTG_CD_DTL DTL
     WHERE BKG.BL_NO = BL.BL_NO
       AND DTL.INTG_CD_ID = 'CD03408'
       AND BL.BL_CERTI_STS_CD = DTL.INTG_CD_VAL_CTNT
       AND BL.BL_CERTI_STS_CD = 'R'
    #if (${bl_no} != '') 
       AND BL.BL_NO = @[bl_no]
       AND BL.TMPLT_FLG = 'N'
    #end
    #if (${ob_srep_cd} != '') 
       AND BKG.OB_SREP_CD LIKE UPPER(@[ob_srep_cd])||'%'
    #end
    #if (${vsl_eng_nm} != '') 
       AND BL.VSL_NM LIKE '%'||UPPER(@[vsl_eng_nm])||'%'
    #end
    #if (${pol_cd} != '') 
       AND BL.POL_CD = @[pol_cd]
    #end
    #if (${co_nm} != '') 
       AND BL.CO_NM LIKE '%'||@[co_nm]||'%'
    #end
    #if (${bl_no} == '') 
        #if (${rqst_from_dt} != '' && ${rqst_to_dt} != '') 
                AND BL.RQST_DT >= TO_DATE(@[rqst_from_dt], 'YYYY-MM-DD')
                AND BL.RQST_DT < TO_DATE(@[rqst_to_dt], 'YYYY-MM-DD') + 1

        #end
    #end
      AND 1 = DECODE(@[bl_certi_sts_cd],'All',1,'R',1,2)
    UNION ALL
    SELECT BKG.BKG_NO
          ,BL.BL_NO
          ,BL.BL_CERTI_SEQ
          ,BL.CO_NM
          ,BL.POL_CD
          ,BL.BL_CERTI_STS_CD
          ,TO_CHAR(BL.RQST_DT, 'YYYY-MM-DD HH24:MI') AS RQST_DT
          ,TO_CHAR(BL.APRO_DT, 'YYYY-MM-DD HH24:MI') AS ACT_DT 
          ,DTL.INTG_CD_VAL_DESC AS BL_CERTI_STS
          ,BKG.OB_SREP_CD
          ,BL.BL_NO || '_' || BL.BL_CERTI_SEQ AS FILE_NM
          ,NVL(BL.BL_CERTI_PRN_FLG,'N') AS BL_CERTI_PRN_FLG
      FROM BKG_BL_CERTI BL
          ,BKG_BOOKING BKG
          ,COM_INTG_CD_DTL DTL
     WHERE BKG.BL_NO = BL.BL_NO
       AND DTL.INTG_CD_ID = 'CD03408'
       AND BL.BL_CERTI_STS_CD = DTL.INTG_CD_VAL_CTNT
       AND BL.BL_CERTI_STS_CD = 'A'
    #if (${bl_no} != '') 
       AND BL.BL_NO = @[bl_no]
       AND BL.TMPLT_FLG = 'N'
    #end
    #if (${ob_srep_cd} != '') 
       AND BKG.OB_SREP_CD LIKE UPPER(@[ob_srep_cd])||'%'
    #end
    #if (${vsl_eng_nm} != '') 
       AND BL.VSL_NM LIKE '%'||UPPER(@[vsl_eng_nm])||'%'
    #end
    #if (${pol_cd} != '') 
       AND BL.POL_CD = @[pol_cd]
    #end
    #if (${co_nm} != '') 
       AND BL.CO_NM LIKE '%'||@[co_nm]||'%'
    #end
    #if (${bl_no} == '') 
        #if (${rqst_from_dt} != '' && ${rqst_to_dt} != '') 
                AND BL.RQST_DT >= TO_DATE(@[rqst_from_dt], 'YYYY-MM-DD')
                AND BL.RQST_DT < TO_DATE(@[rqst_to_dt], 'YYYY-MM-DD') + 1
        #end
    #end
      AND 1 = DECODE(@[bl_certi_sts_cd],'All',1,'A',1,2)
    UNION ALL
    SELECT BKG.BKG_NO
          ,BL.BL_NO
          ,BL.BL_CERTI_SEQ
          ,BL.CO_NM
          ,BL.POL_CD
          ,BL.BL_CERTI_STS_CD
          ,TO_CHAR(BL.RQST_DT, 'YYYY-MM-DD HH24:MI') AS RQST_DT
          ,TO_CHAR(BL.RJCT_DT, 'YYYY-MM-DD HH24:MI') AS ACT_DT 
          ,DTL.INTG_CD_VAL_DESC AS BL_CERTI_STS
          ,BKG.OB_SREP_CD
          ,BL.BL_NO || '_' || BL.BL_CERTI_SEQ AS FILE_NM
          ,NVL(BL.BL_CERTI_PRN_FLG,'N') AS BL_CERTI_PRN_FLG
      FROM BKG_BL_CERTI BL
          ,BKG_BOOKING BKG
          ,COM_INTG_CD_DTL DTL
     WHERE BKG.BL_NO = BL.BL_NO
       AND DTL.INTG_CD_ID = 'CD03408'
       AND BL.BL_CERTI_STS_CD = DTL.INTG_CD_VAL_CTNT
       AND BL.BL_CERTI_STS_CD = 'X'
    #if (${bl_no} != '') 
       AND BL.BL_NO = @[bl_no]
       AND BL.TMPLT_FLG = 'N'
    #end
    #if (${ob_srep_cd} != '') 
       AND BKG.OB_SREP_CD LIKE UPPER(@[ob_srep_cd])||'%'
    #end
    #if (${vsl_eng_nm} != '') 
       AND BL.VSL_NM LIKE '%'||UPPER(@[vsl_eng_nm])||'%'
    #end
    #if (${pol_cd} != '') 
       AND BL.POL_CD = @[pol_cd]
    #end
    #if (${co_nm} != '') 
       AND BL.CO_NM LIKE '%'||@[co_nm]||'%'
    #end
    #if (${bl_no} == '') 
        #if (${rqst_from_dt} != '' && ${rqst_to_dt} != '') 
                AND BL.RQST_DT >= TO_DATE(@[rqst_from_dt], 'YYYY-MM-DD')
                AND BL.RQST_DT < TO_DATE(@[rqst_to_dt], 'YYYY-MM-DD') + 1
        #end
    #end
      AND 1 = DECODE(@[bl_certi_sts_cd],'All',1,'X',1,2)
    UNION ALL
    SELECT BKG.BKG_NO
          ,BL.BL_NO
          ,BL.BL_CERTI_SEQ
          ,BL.CO_NM
          ,BL.POL_CD
          ,BL.BL_CERTI_STS_CD
          ,TO_CHAR(BL.ISS_RQST_DT, 'YYYY-MM-DD HH24:MI') AS RQST_DT
          ,'' AS ACT_DT 
          ,DTL.INTG_CD_VAL_DESC AS BL_CERTI_STS
          ,BKG.OB_SREP_CD
          ,BL.BL_NO || '_' || BL.BL_CERTI_SEQ AS FILE_NM
          ,NVL(BL.BL_CERTI_PRN_FLG,'N') AS BL_CERTI_PRN_FLG
      FROM BKG_BL_CERTI BL
          ,BKG_BOOKING BKG
          ,COM_INTG_CD_DTL DTL
     WHERE BKG.BL_NO = BL.BL_NO
       AND DTL.INTG_CD_ID = 'CD03408'
       AND BL.BL_CERTI_STS_CD = DTL.INTG_CD_VAL_CTNT
       AND BL.BL_CERTI_STS_CD = 'I'
    #if (${bl_no} != '') 
       AND BL.BL_NO = @[bl_no]
       AND BL.TMPLT_FLG = 'N'
    #end
    #if (${ob_srep_cd} != '') 
       AND BKG.OB_SREP_CD LIKE UPPER(@[ob_srep_cd])||'%'
    #end
    #if (${vsl_eng_nm} != '') 
       AND BL.VSL_NM LIKE '%'||UPPER(@[vsl_eng_nm])||'%'
    #end
    #if (${pol_cd} != '') 
       AND BL.POL_CD = @[pol_cd]
    #end
    #if (${co_nm} != '') 
       AND BL.CO_NM LIKE '%'||@[co_nm]||'%'
    #end
    #if (${bl_no} == '') 
        #if (${rqst_from_dt} != '' && ${rqst_to_dt} != '') 
                AND BL.ISS_RQST_DT >= TO_DATE(@[rqst_from_dt], 'YYYY-MM-DD')
                AND BL.ISS_RQST_DT < TO_DATE(@[rqst_to_dt], 'YYYY-MM-DD') + 1
        #end
    #end
      AND 1 = DECODE(@[bl_certi_sts_cd],'All',1,'I',1,'BL',1,2)
    UNION ALL
    SELECT BKG.BKG_NO
          ,BL.BL_NO
          ,BL.BL_CERTI_SEQ
          ,BL.CO_NM
          ,BL.POL_CD
          ,BL.BL_CERTI_STS_CD
          ,TO_CHAR(BL.ISS_RQST_DT, 'YYYY-MM-DD HH24:MI') AS RQST_DT
          ,TO_CHAR(BL.CMPL_DT, 'YYYY-MM-DD HH24:MI') AS ACT_DT 
          ,DTL.INTG_CD_VAL_DESC AS BL_CERTI_STS
          ,BKG.OB_SREP_CD
          ,BL.BL_NO || '_' || BL.BL_CERTI_SEQ AS FILE_NM
          ,NVL(BL.BL_CERTI_PRN_FLG,'N') AS BL_CERTI_PRN_FLG
      FROM BKG_BL_CERTI BL
          ,BKG_BOOKING BKG
          ,COM_INTG_CD_DTL DTL
     WHERE BKG.BL_NO = BL.BL_NO
       AND DTL.INTG_CD_ID = 'CD03408'
       AND BL.BL_CERTI_STS_CD = DTL.INTG_CD_VAL_CTNT
       AND BL.BL_CERTI_STS_CD = 'C'
    #if (${bl_no} != '') 
       AND BL.BL_NO = @[bl_no]
       AND BL.TMPLT_FLG = 'N'
    #end
    #if (${ob_srep_cd} != '') 
       AND BKG.OB_SREP_CD LIKE UPPER(@[ob_srep_cd])||'%'
    #end
    #if (${vsl_eng_nm} != '') 
       AND BL.VSL_NM LIKE '%'||UPPER(@[vsl_eng_nm])||'%'
    #end
    #if (${pol_cd} != '') 
       AND BL.POL_CD = @[pol_cd]
    #end
    #if (${co_nm} != '') 
       AND BL.CO_NM LIKE '%'||@[co_nm]||'%'
    #end
    #if (${bl_no} == '') 
        #if (${rqst_from_dt} != '' && ${rqst_to_dt} != '') 
                AND BL.ISS_RQST_DT >= TO_DATE(@[rqst_from_dt], 'YYYY-MM-DD')
                AND BL.ISS_RQST_DT < TO_DATE(@[rqst_to_dt], 'YYYY-MM-DD') + 1
        #end
    #end
      AND 1 = DECODE(@[bl_certi_sts_cd],'All',1,'C',1,'BL',1,2)
  )
  ORDER BY BL_NO, BL_CERTI_SEQ			]]></sql>
			<params>
				<param name="bl_no" type="12" value=" " out="N"/>
				<param name="ob_srep_cd" type="12" value="" out="N"/>
				<param name="vsl_eng_nm" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="co_nm" type="12" value="" out="N"/>
				<param name="rqst_from_dt" type="12" value="" out="N"/>
				<param name="rqst_to_dt" type="12" value="" out="N"/>
				<param name="bl_certi_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
