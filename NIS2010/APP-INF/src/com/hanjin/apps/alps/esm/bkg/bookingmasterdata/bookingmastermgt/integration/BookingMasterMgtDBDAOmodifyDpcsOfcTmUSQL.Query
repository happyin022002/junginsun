SET DOC_WRK_ST_HRMNT = @[doc_wrk_st_hrmnt],
	DOC_WRK_END_HRMNT = @[doc_wrk_end_hrmnt],
	DOC_WRK_OVN_FLG = @[doc_wrk_ovn_flg],
	DOC_WRK_TM_RMK = @[doc_wrk_tm_rmk],
	UPD_USR_ID = @[upd_usr_id],
	UPD_DT	= SYSDATE
WHERE BKG_OFC_CD = @[bkg_ofc_cd]			]]></sql>
			<params>
				<param name="doc_wrk_st_hrmnt" type="12" value="" out="N"/>
				<param name="doc_wrk_end_hrmnt" type="12" value="" out="N"/>
				<param name="doc_wrk_ovn_flg" type="12" value="" out="N"/>
				<param name="doc_wrk_tm_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
