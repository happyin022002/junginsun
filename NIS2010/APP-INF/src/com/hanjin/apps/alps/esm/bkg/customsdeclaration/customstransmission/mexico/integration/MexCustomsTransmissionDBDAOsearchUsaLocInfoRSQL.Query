<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MexCustomsTransmissionDBDAOsearchUsaLocInfoRSQL">
			<desc><![CDATA[DWKIM, 0370, OUTVO: MxUsaLocVO]]></desc>
			<sql><![CDATA[
SELECT
    DECODE(IB.LOCL_CLR_IPI_MVMT_FLG, 'Y', 'Y', IB.CSTMS_CLR_TP_CD) local_ipi
    ,NVL(L.LOC_CD, '     ') loc_cd
    ,DECODE(LENGTH(RTRIM(L.LOC_AMS_PORT_CD)), 4, 'D', 5, 'K', 'N') dkn
	,RPAD(RTRIM(L.LOC_AMS_PORT_CD), 5) loc_ams_port_cd
    ,L.LOC_NM loc_nm
FROM BKG_CSTMS_ADV_BL BL, BKG_CSTMS_ADV_IBD IB
    ,MDM_LOCATION L
WHERE
	BL.CNT_CD = 'US'
	AND	BL.BL_NO = @[bl_no]
    AND BL.CNT_CD = IB.CNT_CD
    AND BL.BL_NO = IB.BL_NO
    AND BL.HUB_LOC_CD = L.LOC_CD(+)
	AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
