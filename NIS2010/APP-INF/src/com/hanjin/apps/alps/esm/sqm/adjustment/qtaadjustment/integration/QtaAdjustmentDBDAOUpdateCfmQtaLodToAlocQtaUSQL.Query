<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOUpdateCfmQtaLodToAlocQtaUSQL">
			<desc><![CDATA[Apply 당시 SQM_CFM_QTA 테이블의 LOD_QTY를 SQM_ALOC_QTA 테이블의 CFM_LOD_QTY 에 넣음]]></desc>
			<sql><![CDATA[
MERGE INTO SQM_ALOC_QTA X
USING (
        SELECT BSE_YR
              ,TRD_CD
              ,RLANE_CD
              ,DIR_CD
              ,VSL_CD
              ,SKD_VOY_NO
              ,SKD_DIR_CD
              ,RGN_OFC_CD
              ,LOD_QTY
          FROM SQM_CFM_QTA
         WHERE 1=1
           AND BSE_TP_CD     = 'Q'
           AND BSE_YR        = @[bse_yr]
		   AND SUBSTR(QTA_RLSE_VER_NO,-2) = '02'
           AND OFC_VW_CD     = 'L'
           AND TRD_CD        = @[trd_cd]
		   AND RLANE_CD      = @[rlane_cd]
           AND SQM_CNG_TP_CD <> 'A'
           AND VSL_CD        = @[vsl_cd]
           AND SKD_VOY_NO    = @[skd_voy_no]
           AND SKD_DIR_CD    = @[skd_dir_cd]
       ) Y
ON  (
         X.BSE_YR     = Y.BSE_YR     
     AND X.TRD_CD     = Y.TRD_CD     
     AND X.RLANE_CD   = Y.RLANE_CD   
     AND X.DIR_CD     = Y.DIR_CD     
     AND X.VSL_CD     = Y.VSL_CD
     AND X.SKD_VOY_NO = Y.SKD_VOY_NO 
     AND X.SKD_DIR_CD = Y.SKD_DIR_CD     
     AND X.RGN_OFC_CD = Y.RGN_OFC_CD 
    )
WHEN MATCHED THEN
     UPDATE SET X.CFM_LOD_QTY   = Y.LOD_QTY     
               ,X.UPD_USR_ID    = @[upd_usr_id]
               ,X.UPD_DT        = SYSDATE			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
