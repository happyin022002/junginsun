<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAYearlyPlanDBDAOInsertBsaBudSltPrcCrrCSQL">
			<desc><![CDATA[BSA_BUD_SLT_PRC_CRR 데이타를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BSA_BUD_SLT_PRC_CRR (
    TRD_CD            ,
    RLANE_CD          ,
    DIR_CD            ,
    BSA_SLT_COST_TP_CD,
    SLT_PRC_SEQ       ,
    BSA_OP_JB_CD      ,
    CRR_CD            ,
    BSA_SLT_PRC_FM_DT ,
    BSA_SLT_PRC_TO_DT ,
    SLT_PRC_CAPA      ,
    CRE_USR_ID        ,
    CRE_DT            ,
    UPD_USR_ID        ,
    UPD_DT
)
  SELECT B.TRD_CD            ,
         B.RLANE_CD          ,
         B.DIR_CD            ,
         B.BSA_SLT_COST_TP_CD,
         B.SLT_PRC_SEQ       ,
         B.BSA_OP_JB_CD      ,
         B.CRR_CD            ,
         B.BSA_SLT_PRC_FM_DT ,
         B.BSA_SLT_PRC_TO_DT ,
         B.SLT_PRC_CAPA      ,
         @[cre_usr_id]       ,
         SYSDATE             ,
         @[upd_usr_id]       ,
         SYSDATE
    FROM BSA_BUD_SLT_PRC A,
         BSA_SLT_PRC_CRR B
   WHERE 1=1
     AND A.TRD_CD             = B.TRD_CD
     AND A.RLANE_CD           = B.RLANE_CD
     AND A.DIR_CD             = B.DIR_CD
     AND A.BSA_SLT_COST_TP_CD = B.BSA_SLT_COST_TP_CD
     AND A.SLT_PRC_SEQ        = B.SLT_PRC_SEQ
     
#if (${trd_cd} != 'All')
     AND A.TRD_CD             = @[trd_cd]
#end

#if (${rlane_cd} != 'All')
     AND A.RLANE_CD           = @[rlane_cd]
#end			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
