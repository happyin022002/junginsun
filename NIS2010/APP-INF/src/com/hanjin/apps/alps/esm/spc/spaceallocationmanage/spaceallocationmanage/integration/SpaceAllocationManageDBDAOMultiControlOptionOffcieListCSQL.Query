<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOMultiControlOptionOffcieListCSQL">
			<desc><![CDATA[Lane Control Option Offcie List 에 값 입력시 사용]]></desc>
			<sql><![CDATA[
MERGE INTO SPC_ALOC_LANE_CTRL_OFC D1
USING (
    SELECT @[trd_cd] TRD_CD
         , @[sub_trd_cd] SUB_TRD_CD
         , @[rlane_cd] RLANE_CD
         , @[dir_cd] DIR_CD
         , @[aloc_ctrl_tp_cd] ALOC_CTRL_TP_CD
         , @[ctrl_loc_acct_cd] CTRL_LOC_ACCT_CD
         , @[upd_usr_id] CRE_USR_ID
		 , @[ofc_cd] OFC_CD
         , SYSDATE CRE_DT
         , @[upd_usr_id] UPD_USR_ID
         , SYSDATE UPD_DT
      FROM DUAL
) D2
ON ( D1.TRD_CD = D2.TRD_CD
    AND D1.SUB_TRD_CD = D2.SUB_TRD_CD
    AND D1.RLANE_CD = D2.RLANE_CD
    AND D1.DIR_CD = D2.DIR_CD
    AND D1.ALOC_CTRL_TP_CD = D2.ALOC_CTRL_TP_CD
    AND D1.CTRL_LOC_ACCT_CD = D2.CTRL_LOC_ACCT_CD 
	AND D1.OFC_CD = D2.OFC_CD 
    )
WHEN NOT MATCHED THEN
INSERT 
(
TRD_CD,
SUB_TRD_CD,
RLANE_CD,
DIR_CD,
ALOC_CTRL_TP_CD,
CTRL_LOC_ACCT_CD,
OFC_CD,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID,
UPD_DT
)
VALUES (
@[trd_cd],
@[sub_trd_cd],
@[rlane_cd],
@[dir_cd],
@[aloc_ctrl_tp_cd],
@[ctrl_loc_acct_cd],
@[ofc_cd],
@[cre_usr_id],
SYSDATE,
@[upd_usr_id],
SYSDATE
)			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="aloc_ctrl_tp_cd" type="12" value="" out="N"/>
				<param name="ctrl_loc_acct_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
