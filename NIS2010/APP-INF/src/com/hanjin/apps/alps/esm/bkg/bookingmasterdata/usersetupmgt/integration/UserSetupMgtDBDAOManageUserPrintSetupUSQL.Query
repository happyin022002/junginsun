<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOManageUserPrintSetupUSQL">
			<desc><![CDATA[UserSetupMgtDBDAOManageUserPrintSetupUSQL]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_USR_DFLT_SET TA
    USING (
            SELECT  @[usr_id] USR_ID 
                   , @[bl_face_prn_dvc_nm] BL_FACE_PRN_DVC_NM
                   , @[bl_ridr_prn_dvc_nm] BL_RIDR_PRN_DVC_NM
                   , @[bl_prn_dvc_nm] BL_PRN_DVC_NM
            FROM   DUAL
          ) TB
    ON(TA.USR_ID   = TB.USR_ID )
 WHEN MATCHED THEN 
    UPDATE SET    BL_FACE_PRN_DVC_NM = TB.BL_FACE_PRN_DVC_NM 
                , BL_RIDR_PRN_DVC_NM = TB.BL_RIDR_PRN_DVC_NM
                , BL_PRN_DVC_NM      = TB.BL_PRN_DVC_NM
                , CRE_USR_ID         = TB.USR_ID
                , CRE_DT             = SYSDATE  
                , UPD_USR_ID         = TB.USR_ID 
                , UPD_DT             = SYSDATE
 WHEN NOT MATCHED THEN 
    INSERT (  USR_ID,    BL_FACE_PRN_DVC_NM,    BL_RIDR_PRN_DVC_NM,    BL_PRN_DVC_NM,     CRE_USR_ID, CRE_DT,  UPD_USR_ID, UPD_DT ) 
    VALUES (  TB.USR_ID, TB.BL_FACE_PRN_DVC_NM, TB.BL_RIDR_PRN_DVC_NM, TB.BL_PRN_DVC_NM,  TB.USR_ID,  SYSDATE, TB.USR_ID,  SYSDATE)			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bl_face_prn_dvc_nm" type="12" value="" out="N"/>
				<param name="bl_ridr_prn_dvc_nm" type="12" value="" out="N"/>
				<param name="bl_prn_dvc_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
