<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshOdcyReqDBDAOSearchShippingRequestRSQL">
			<desc><![CDATA[SearchShippingRequest]]></desc>
			<sql><![CDATA[
SELECT RQST.BKG_NO
     , RQST.CNTR_NO
     , RQST.CNTR_SEQ
     , RQST.CNTR_TPSZ_CD
     , RQST.CNTR_SEAL_NO
     , RQST.PCK_QTY
     , RQST.PCK_TP_CD
     , RQST.CNTR_WGT
     , RQST.WGT_UT_CD
     , RQST.MEAS_QTY
     , RQST.MEAS_UT_CD
     , RQST.CNTR_VOL_QTY
     , RQST.VNDR_SEQ
     , BKGVVD.SLAN_CD SLANE_CD
     , BKGVVD.VSL_CD||BKGVVD.SKD_VOY_NO||BKGVVD.SKD_DIR_CD AS VVD
     , BKG.POL_CD
FROM BKG_CUST_SHP_RQST RQST
   , BKG_BOOKING BKG
   , BKG_VVD BKGVVD
WHERE 1=1
AND BKG.BKG_NO = RQST.BKG_NO
AND RQST.BKG_NO = BKGVVD.BKG_NO
AND BKG.POL_CD = BKGVVD.POL_CD
#if(${bkg_no} != '')
  AND RQST.BKG_NO = @[bkg_no]
#end
#if(${pol_cd} != '')
  AND BKG.POL_CD = @[pol_cd]
#end
#if(${cntr_no} != '')
  AND RQST.CNTR_NO = @[cntr_no]
#end
#if(${vndr_seq} !='')
  AND RQST.VNDR_SEQ = @[vndr_seq]
#end
#if(${upd_dt_from} !='' && ${upd_dt_to} !='') 
  AND RQST.UPD_DT BETWEEN TO_DATE(@[upd_dt_from],'YYYY-MM-DD') AND TO_DATE(@[upd_dt_to],'YYYY-MM-DD') +0.99999
#end
AND RQST.CRE_USR_ID = 'SPPUSR'
AND RQST.UPD_USR_ID = 'SPPUSR'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="upd_dt_from" type="12" value="" out="N"/>
				<param name="upd_dt_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
