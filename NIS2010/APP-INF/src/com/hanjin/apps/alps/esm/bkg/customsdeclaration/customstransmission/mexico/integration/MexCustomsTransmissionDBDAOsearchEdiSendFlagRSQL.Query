<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MexCustomsTransmissionDBDAOsearchEdiSendFlagRSQL">
			<desc><![CDATA[dwkim, 0370, outvo : None]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) 
FROM
	BKG_NTC_HIS
WHERE
	BKG_NO = @[bkg_no]
	-- 20030930 modified by thhong be_ind = 'IM' -> be_ind = :be_ind_tmp
	AND NTC_KND_CD = 'IM'
	AND EDI_ID(+) = 'MEXCUS'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
