<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchEdi301CntrRfInfoRSQL">
			<desc><![CDATA[searchEdi301CntrRfInfo]]></desc>
			<sql><![CDATA[
SELECT '{CNTR_INFO'																|| CHR(10)
	|| 'CNTN:'					|| RPAD(NVL(Cntr.CNTR_NO, ' '), 11, ' ')		|| CHR(10)
	|| 'CNTT:'					|| rf.CNTR_TPSZ_CD								|| CHR(10)
	|| 'SEAL:'					|| REPLACE(REPLACE(REPLACE(NVL(seal.CNTR_SEAL_NO,' '),CHR(32),''),CHR(10),''),CHR(13),'')	|| CHR(10)
	|| 'RIND:'                  || DECODE(Cntr.RC_FLG,'N','0','Y','1',CNTR.RC_FLG)|| CHR(10)
	|| 'DIND:'                  || DECODE(Cntr.DCGO_FLG,'N','0','Y','1',CNTR.DCGO_FLG)|| CHR(10)
	|| 'AIND:'                  || DECODE(Cntr.AWK_CGO_FLG,'N','0','Y','1',CNTR.AWK_CGO_FLG)|| CHR(10)
	|| 'BIND:'                  || DECODE(Cntr.BB_CGO_FLG,'N','0','Y','1',CNTR.BB_CGO_FLG)|| CHR(10)
	|| 'PKG_QTY:'               || cntr.pck_qty                                 || CHR(10)
	|| 'PKG_TP:'                || cntr.pck_tp_cd                               || CHR(10)
	|| 'MEA_QTY:'               || cntr.meas_qty                                || CHR(10)
	|| 'MEA_TP:'                || cntr.meas_ut_cd                              || CHR(10)
	|| 'WGT_QTY:'               || Cntr.CNTR_WGT								|| CHR(10)
	|| 'WGT_TP:'                || Cntr.WGT_UT_CD								|| CHR(10)
	|| 'TEMP:'					|| RF.FDO_TEMP									|| CHR(10)
	|| 'TUN:'					|| 'F'											|| CHR(10)
	|| 'TEMP_C:'				|| RF.CDO_TEMP									|| CHR(10)
	|| 'TUN_C:'					|| 'C'											|| CHR(10)
	|| 'RF_VOLTAGE:'			|| NVL(RF.VLTG_NO,0)							|| CHR(10)
	|| 'VENT:'					|| CASE WHEN NVL(RF.VENT_RTO,0) = 0 THEN 'C'
										WHEN NVL(RF.VENT_RTO,0) > 0 AND NVL(RF.VENT_RTO,0) < 35 THEN 'Q'
										WHEN NVL(RF.VENT_RTO,0) >= 35 AND NVL(RF.VENT_RTO,0) < 65 THEN 'H'
										WHEN NVL(RF.VENT_RTO,0) >= 65 AND NVL(RF.VENT_RTO,0) < 100 THEN 'T'
										WHEN NVL(RF.VENT_RTO,0) = 100 THEN 'O' ELSE 'M' END		|| CHR(10)
	|| 'HUMID:'					|| RF.HUMID_NO									|| CHR(10)
	|| 'GENSET:'				|| RF.PWR_SPL_CBL_FLG                           || CHR(10)
	|| 'RF_REMARK:'				|| REPLACE(RF.DIFF_RMK, chr(13)||chr(10), ' ')	|| CHR(10)
	|| 'RFDRY_IND:'				|| DECODE(bk.rd_cgo_FLG,'N','0','Y','1',BK.RD_CGO_FLG)|| CHR(10)
	|| 'OVF:'																	|| CHR(10)
	|| 'OVR:'																	|| CHR(10)
	|| 'OVH:'																	|| CHR(10)
	|| 'OVLW:'																	|| CHR(10)
	|| 'OVRW:'																	|| CHR(10)
	|| 'OVWGT:'																	|| CHR(10)
	|| 'VOID_SLOT:'																|| CHR(10)
	|| 'STWG_REQ:'																|| CHR(10)
	|| 'TTL_DIM_LEN:'															|| CHR(10)
	|| 'TTL_DIM_WDT:'															|| CHR(10)
	|| 'TTL_DIM_HGT:'															|| CHR(10)
	|| 'TRM_TYPE:'																|| CHR(10)
	|| 'TRM_WEIGHT:'															|| CHR(10)
	|| 'TRM_HAULAGE:'															|| CHR(10)
	|| 'TRM_HMODE:'																|| CHR(10)
	|| 'TRM_PICKUP_CY:'															|| CHR(10)
	|| 'TRM_RETURN_CY:'															|| CHR(10)
	|| 'TRM_INSTRUCTION:'														|| CHR(10)
	|| 'TRM_TRAN_DT:'															|| CHR(10)
	|| 'TRM_TRAN_OFFICE:'														|| CHR(10)
	|| 'TRM_TRAN_NO:'															|| CHR(10)
	|| 'USR_ID:'																|| CHR(10)
	|| 'CNTR_RCV_TERM:'			||CNTR.RCV_TERM_CD								|| CHR(10)
	|| 'CNTR_DLV_TERM:'			||CNTR.DE_TERM_CD								|| CHR(10)
	|| '{CNTR_TRO_DTL'															|| CHR(10)
	|| 'TRD_ADDR:'																|| CHR(10)
	|| 'TRD_DOOR_LOC:'															|| CHR(10)
	|| 'TRD_DOOR_POSTAL:'														|| CHR(10)
	|| 'TRD_DOOR_DT:'															|| CHR(10)
	|| 'TRD_PERSON:'															|| CHR(10)
	|| 'TRD_TEL:'																|| CHR(10)
	|| 'TRD_FAX:'																|| CHR(10)
	|| 'TRD_ACTSHIP:'															|| CHR(10)
	|| 'TRD_REMARK:'															|| CHR(10)
	|| '}CNTR_TRO_DTL'															|| CHR(10)
	|| '}CNTR_INFO'																 CNTR_RF_INFO
  FROM BKG_BOOKING bk
        , BKG_RF_CGO RF
        , BKG_CONTAINER cntr
        , BKG_CNTR_SEAL_NO seal
 WHERE bk.bkg_no        = RF.bkg_no
   AND RF.bkg_no        = Cntr.BKG_NO  (+) 
   AND RF.CNTR_NO       = Cntr.CNTR_NO (+) 
   AND cntr.BKG_NO      = seal.BKG_NO       (+)
   AND cntr.CNTR_NO     = seal.CNTR_NO      (+)
   AND 1                = seal.CNTR_SEAL_SEQ(+)
   AND bk.BKG_NO  = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
