<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOAddTariffIhcRateDataCSQL">
			<desc><![CDATA[PRI_TRF_IHC_RT
2013.02.08 [CHM-201322859] 서미진 TRS weight IF 시 추가
2013.03.04 [CHM-201323352] 전윤주 AOC 테이블의 delt_flg 확인 로직 추가
2015.03.05 [CHM-201534279] 최성환  Pricing FeederIHC tariff 45 칼럼 추가]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_IHC_RT
  (SVC_SCP_CD,
   ORG_DEST_TP_CD,
   IHC_TRF_NO,
   AMDT_SEQ,
   IHC_CGO_TP_CD,
   RT_SEQ,
   N1ST_CMNC_AMDT_SEQ,
   PNT_LOC_CD,
   BSE_PORT_LOC_CD,
   HUB_LOC_CD,
   PNT_NOD_CD,
   HUB_NOD_CD,
   BSE_PORT_NOD_CD,
   IHC_COST_LOC_GRP_NO,
   RCV_DE_TERM_CD,
   PRC_TRSP_MOD_CD,
   GLINE_20FT_FRT_RT_AMT,
   GLINE_40FT_FRT_RT_AMT,
   GLINE_45FT_FRT_RT_AMT,	--45'추가
   COST_20FT_FRT_RT_AMT,
   COST_40FT_FRT_RT_AMT,
   COST_45FT_FRT_RT_AMT,	--45'추가
   LOCL_CURR_COST_20FT_FRT_RT_AMT,
   LOCL_CURR_COST_40FT_FRT_RT_AMT,
   LOCL_CURR_COST_45FT_FRT_RT_AMT,	--45'추가
   TRSP_20FT_COST_AMT,
   TRSP_40FT_COST_AMT,
   TRSP_45FT_COST_AMT,	--45'추가 
   MTY_TRSP_20FT_COST_AMT,
   MTY_TRSP_40FT_COST_AMT,
   MTY_TRSP_45FT_COST_AMT,	--45'추가 
   TML_20FT_COST_AMT,
   TML_40FT_COST_AMT,
   TML_45FT_COST_AMT,	--45'추가
   MB_20FT_RTO,
   MB_40FT_RTO,
   MB_45FT_RTO,		--45'추가
   GLINE_DG_20FT_FRT_RT_AMT,
   GLINE_DG_40FT_FRT_RT_AMT,
   GLINE_DG_45FT_FRT_RT_AMT,	--45'추가
   GLINE_OVR_WGT_20FT_FRT_RT_AMT,
   GLINE_OVR_WGT_40FT_FRT_RT_AMT,
   GLINE_OVR_WGT_45FT_FRT_RT_AMT,	--45'추가
   ORG_COST_20FT_FRT_RT_AMT,
   ORG_COST_40FT_FRT_RT_AMT,
   ORG_COST_45FT_FRT_RT_AMT,	--45'추가
   OPTM_TRSP_MOD_FLG,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   SRC_INFO_CD,
   PRC_TRF_CRE_TP_CD,
   LOCL_CURR_CD,
   TRSP_20FT_AGMT_WGT,
   TRSP_40FT_AGMT_WGT,
   TRSP_45FT_AGMT_WGT,	--45'추가
   GLINE_LOCL_CURR_20FT_AMT,
   GLINE_LOCL_CURR_40FT_AMT,
   GLINE_LOCL_CURR_45FT_AMT	--45'추가
  )
  SELECT SVC_SCP_CD,
         ORG_DEST_TP_CD,
         IHC_TRF_NO,
         AMDT_SEQ,
         'DR' AS IHC_CGO_TP_CD,
         ROWNUM RT_SEQ,
         N1ST_CMNC_AMDT_SEQ,
         PNT_LOC_CD,
         BSE_PORT_LOC_CD,
         HUB_LOC_CD,
         PNT_NOD_CD,
         HUB_NOD_CD,
         BSE_PORT_NOD_CD,
         IHC_COST_LOC_GRP_NO,
         RCV_DE_TERM_CD,
         PRC_TRSP_MOD_CD,
         GLINE_20FT_FRT_RT_AMT,
         GLINE_40FT_FRT_RT_AMT,
	   	 GLINE_45FT_FRT_RT_AMT, --45'추가 
         COST_20FT_FRT_RT_AMT,
         COST_40FT_FRT_RT_AMT,
		 COST_45FT_FRT_RT_AMT,	---45'추가 
         LOCL_CURR_COST_20FT_FRT_RT_AMT,
         LOCL_CURR_COST_40FT_FRT_RT_AMT,
		 LOCL_CURR_COST_45FT_FRT_RT_AMT,	---45'추가 
         TRSP_20FT_COST_AMT,
         TRSP_40FT_COST_AMT,
		 TRSP_45FT_COST_AMT,	---45'추가 
         MTY_TRSP_20FT_COST_AMT,
         MTY_TRSP_40FT_COST_AMT,
		 MTY_TRSP_45FT_COST_AMT,	---45'추가 
         TML_20FT_COST_AMT,
         TML_40FT_COST_AMT,
		 TML_45FT_COST_AMT,	---45'추가 
         MB_20FT_RTO,
         MB_40FT_RTO,
		 MB_45FT_RTO,	---45'추가 
         GLINE_DG_20FT_FRT_RT_AMT,
         GLINE_DG_40FT_FRT_RT_AMT,
		 GLINE_DG_45FT_FRT_RT_AMT,	---45'추가 
         GLINE_OVR_WGT_20FT_FRT_RT_AMT,
         GLINE_OVR_WGT_40FT_FRT_RT_AMT,
		 GLINE_OVR_WGT_45FT_FRT_RT_AMT,	---45'추가 
         ORG_COST_20FT_FRT_RT_AMT,
         ORG_COST_40FT_FRT_RT_AMT,
		 ORG_COST_45FT_FRT_RT_AMT,	---45'추가 
         OPTM_TRSP_MOD_FLG,
         CRE_USR_ID,
         CRE_DT,
         UPD_USR_ID,
         UPD_DT,
         SRC_INFO_CD,
	     PRC_TRF_CRE_TP_CD,
         CURR_CD,
         TRSP_20FT_AGMT_WGT,
         TRSP_40FT_AGMT_WGT,
		 TRSP_45FT_AGMT_WGT,	---45'추가 
         LOCL_CURR_COST_20FT_FRT_RT_AMT AS GLINE_LOCL_CURR_20FT_AMT,
         LOCL_CURR_COST_40FT_FRT_RT_AMT AS GLINE_LOCL_CURR_40FT_AMT,
		 LOCL_CURR_COST_45FT_FRT_RT_AMT AS GLINE_LOCL_CURR_45FT_AMT	---45'추가 

    FROM (SELECT G1.*
            FROM (
              SELECT   @[svc_scp_cd] SVC_SCP_CD,
                       @[ihc_trf_no] IHC_TRF_NO,
                       @[org_dest_tp_cd] ORG_DEST_TP_CD,         
                       '0' AMDT_SEQ,
                       '0' N1ST_CMNC_AMDT_SEQ,
                       SUBSTR(A.LOC_NOD_CD, 1, 5) PNT_LOC_CD,
                       SUBSTR(A.PORT_NOD_CD, 1, 5) BSE_PORT_LOC_CD,
                       SUBSTR(A.HUB_NOD_CD, 1, 5) HUB_LOC_CD,
                       A.LOC_NOD_CD PNT_NOD_CD,
                       A.HUB_NOD_CD HUB_NOD_CD,
                       A.PORT_NOD_CD BSE_PORT_NOD_CD,
                       A.LOC_GRP_NO IHC_COST_LOC_GRP_NO,
                       A.RCV_DE_TERM_CD RCV_DE_TERM_CD,
                       CASE
                           WHEN A.TRSP_CRR_MOD_CD IN ( 'WD','WR','RW') THEN 'B'
                           WHEN A.TRSP_CRR_MOD_CD = 'RD' THEN 'R'
                           WHEN A.TRSP_CRR_MOD_CD ='TD' THEN 'T'
                           WHEN A.TRSP_CRR_MOD_CD IN ('WT', 'TW') THEN 'U'
                           WHEN A.TRSP_CRR_MOD_CD IN ('RT', 'TR') THEN 'A'
                       END  PRC_TRSP_MOD_CD,
                       TPB_GET_USD_AMT_FNC(A.INLND_20FT_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) GLINE_20FT_FRT_RT_AMT,
                       TPB_GET_USD_AMT_FNC(A.INLND_40FT_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) GLINE_40FT_FRT_RT_AMT,
					   TPB_GET_USD_AMT_FNC(A.INLND_45FT_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) GLINE_45FT_FRT_RT_AMT,	--45'추가
                       TPB_GET_USD_AMT_FNC(A.INLND_20FT_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) COST_20FT_FRT_RT_AMT,
                       TPB_GET_USD_AMT_FNC(A.INLND_40FT_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) COST_40FT_FRT_RT_AMT,
					   TPB_GET_USD_AMT_FNC(A.INLND_45FT_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) COST_45FT_FRT_RT_AMT,	--45'추가
                       A.INLND_20FT_TTL_AMT LOCL_CURR_COST_20FT_FRT_RT_AMT,
                       A.INLND_40FT_TTL_AMT LOCL_CURR_COST_40FT_FRT_RT_AMT,
 					   A.INLND_45FT_TTL_AMT LOCL_CURR_COST_45FT_FRT_RT_AMT,	--45'추가
                       TPB_GET_USD_AMT_FNC(A.TRSP_20FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) TRSP_20FT_COST_AMT,
                       TPB_GET_USD_AMT_FNC(A.TRSP_40FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) TRSP_40FT_COST_AMT,
					   TPB_GET_USD_AMT_FNC(A.TRSP_45FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) TRSP_45FT_COST_AMT,	--45'추가
                       TPB_GET_USD_AMT_FNC(A.MTY_TRSP_20FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) MTY_TRSP_20FT_COST_AMT,
                       TPB_GET_USD_AMT_FNC(A.MTY_TRSP_40FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) MTY_TRSP_40FT_COST_AMT,
					   TPB_GET_USD_AMT_FNC(A.MTY_TRSP_45FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) MTY_TRSP_45FT_COST_AMT,	--45'추가
                       TPB_GET_USD_AMT_FNC(A.TML_20FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) TML_20FT_COST_AMT,
                       TPB_GET_USD_AMT_FNC(A.TML_40FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) TML_40FT_COST_AMT,
					   TPB_GET_USD_AMT_FNC(A.TML_45FT_TTL_COST_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) TML_45FT_COST_AMT,	--45'추가
                       A.MB_20FT_RTO MB_20FT_RTO,
                       A.MB_40FT_RTO MB_40FT_RTO,     
					   A.MB_45FT_RTO MB_45FT_RTO,  --45'추가     
                       NULL GLINE_DG_20FT_FRT_RT_AMT,
                       NULL GLINE_DG_40FT_FRT_RT_AMT,
					   NULL GLINE_DG_45FT_FRT_RT_AMT,	--45'추가
                       NULL GLINE_OVR_WGT_20FT_FRT_RT_AMT,
                       NULL GLINE_OVR_WGT_40FT_FRT_RT_AMT,
					   NULL GLINE_OVR_WGT_45FT_FRT_RT_AMT,	--45'추가
                       TPB_GET_USD_AMT_FNC(A.INLND_20FT_ORG_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) ORG_COST_20FT_FRT_RT_AMT,
                       TPB_GET_USD_AMT_FNC(A.INLND_40FT_ORG_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) ORG_COST_40FT_FRT_RT_AMT,
					   TPB_GET_USD_AMT_FNC(A.INLND_45FT_ORG_TTL_AMT, A.CURR_CD, F.ACCT_XCH_RT_YRMON) ORG_COST_45FT_FRT_RT_AMT,	--45'추가
                       NVL(A.COST_SEL_ROUT_FLG, 'N') OPTM_TRSP_MOD_FLG,
                       @[cre_usr_id] CRE_USR_ID,
                       SYSDATE CRE_DT,
                       @[upd_usr_id] UPD_USR_ID,
                       SYSDATE UPD_DT,
                       'NW' SRC_INFO_CD,
					   'I' PRC_TRF_CRE_TP_CD,
                       CURR_CD,
                       CASE 
                           WHEN TRSP_20FT_COST_SYS_SRC_CD <> 'A' AND DECODE(A.INLND_ROUT_CMB_FLG,'N',A.N1ST_VNDR_20FT_AGMT_WGT,GREATEST(A.N1ST_VNDR_20FT_AGMT_WGT, A.N2ND_VNDR_20FT_AGMT_WGT)) IS NULL THEN 999999.99
                           ELSE DECODE(A.INLND_ROUT_CMB_FLG,'N',A.N1ST_VNDR_20FT_AGMT_WGT,GREATEST(A.N1ST_VNDR_20FT_AGMT_WGT, A.N2ND_VNDR_20FT_AGMT_WGT))
                       END TRSP_20FT_AGMT_WGT,
                       CASE 
                           WHEN TRSP_40FT_COST_SYS_SRC_CD <> 'A' AND DECODE(A.INLND_ROUT_CMB_FLG,'N',A.N1ST_VNDR_40FT_AGMT_WGT,GREATEST(A.N1ST_VNDR_40FT_AGMT_WGT, A.N2ND_VNDR_40FT_AGMT_WGT)) IS NULL THEN 999999.99
                           ELSE DECODE(A.INLND_ROUT_CMB_FLG,'N',A.N1ST_VNDR_40FT_AGMT_WGT,GREATEST(A.N1ST_VNDR_40FT_AGMT_WGT, A.N2ND_VNDR_40FT_AGMT_WGT))
                       END TRSP_40FT_AGMT_WGT,
					   CASE 
                           WHEN TRSP_45FT_COST_SYS_SRC_CD <> 'A' AND DECODE(A.INLND_ROUT_CMB_FLG,'N',A.N1ST_VNDR_45FT_AGMT_WGT,GREATEST(A.N1ST_VNDR_45FT_AGMT_WGT, A.N2ND_VNDR_45FT_AGMT_WGT)) IS NULL THEN 999999.99
                           ELSE DECODE(A.INLND_ROUT_CMB_FLG,'N',A.N1ST_VNDR_45FT_AGMT_WGT,GREATEST(A.N1ST_VNDR_45FT_AGMT_WGT, A.N2ND_VNDR_45FT_AGMT_WGT))
                       END TRSP_45FT_AGMT_WGT	--45'추가
                  FROM   
                            AOC_EUR_INLND_TRF_DTL A,
                        (SELECT @[svc_scp_cd] SVC_SCP_CD,  TO_DATE(MAX(R.ACCT_XCH_RT_YRMON), 'YYYYMM') ACCT_XCH_RT_YRMON
                           FROM GL_MON_XCH_RT R
                          WHERE R.ACCT_XCH_RT_LVL = '1'
                                AND DELT_FLG = 'N') F
                 WHERE A.COST_TRF_NO = @[cost_trf_no]
                   AND NVL(A.DELT_FLG, 'N') <> 'Y'                   
                 ) G1
           ORDER BY OPTM_TRSP_MOD_FLG DESC,
                    PNT_LOC_CD,
                    BSE_PORT_LOC_CD,
                    RCV_DE_TERM_CD
) VV1


			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
