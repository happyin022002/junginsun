<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOmodifyBaplieAlarmSetupUSQL">
			<desc><![CDATA[modifyBaplieAlarmSetup : update]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_STWG_ALRM_STUP SET 
	SLAN_CD = @[slan_cd]
,	LST_PORT_CD = @[lst_port_cd]
,	RCVR_EML = @[rcvr_eml]
,	EML_SND_USR_ID = @[eml_snd_usr_id]
,	EML_SND_HRS = @[eml_snd_hrs]
,	UPD_USR_ID = @[user_id]
,	UPD_DT = SYSDATE
WHERE	SLAN_CD = @[hidden_slan_cd]
AND	LST_PORT_CD = @[hidden_lst_port_cd]
AND	RCVR_EML = @[hidden_rcvr_eml]
AND	CNT_CD = @[cnt_cd]			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="lst_port_cd" type="12" value="" out="N"/>
				<param name="rcvr_eml" type="12" value="" out="N"/>
				<param name="eml_snd_usr_id" type="12" value="" out="N"/>
				<param name="eml_snd_hrs" type="2" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="hidden_slan_cd" type="12" value="" out="N"/>
				<param name="hidden_lst_port_cd" type="12" value="" out="N"/>
				<param name="hidden_rcvr_eml" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
