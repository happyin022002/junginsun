<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOSearchIhcBKGListForAuditRSQL">
			<desc><![CDATA[심사를 위한 IHC BKG 리스트를 조회한다.]]></desc>
			<sql><![CDATA[
WITH BKG AS
(
SELECT R.BKG_NO,
       TO_CHAR(R.RT_APLY_DT, 'YYYYMMDD') RT_APLY_DT,
       R.BKG_CTRT_TP_CD,
       CASE WHEN R.BKG_CTRT_TP_CD = 'R' THEN B.RFA_NO
            WHEN R.BKG_CTRT_TP_CD = 'S' THEN B.SC_NO
            WHEN R.BKG_CTRT_TP_CD = 'T' THEN B.TAA_NO
       END CTRT_NO,
       B.POR_CD,
       B.POL_CD,
       B.POD_CD,
       B.DEL_CD,
       B.RCV_TERM_CD,
       B.DE_TERM_CD,
       C.CNTR_NO,
       C.CNTR_TPSZ_CD,
       C.CNTR_WGT,
       (SELECT CURR_CD||' / '||
               TRIM(TO_CHAR(CHG_UT_AMT, '999,999,999.00'))||' / '||
               TRIM(TO_CHAR(RAT_AS_QTY, '999.00'))||' / '||
               RAT_UT_CD||' / '||
               TRIM(TO_CHAR(CHG_AMT, '999,999,999.00'))
        FROM BKG_CHG_RT OIH
        WHERE BKG_NO = C.BKG_NO
        AND CHG_CD = 'OIH'
        AND FRT_INCL_XCLD_DIV_CD = 'N'
        AND (RAT_UT_CD = C.CNTR_TPSZ_CD
            OR RAT_UT_CD IN ('BX', 'BL')
            OR (
                RAT_UT_CD IN ('20', '40', 'HC', '45', '53')
                AND ( SELECT A.CNTR_SZ_CD FROM PRI_RAT_UT A WHERE A.RAT_UT_CD = OIH.RAT_UT_CD AND ROWNUM=1) = U.CNTR_SZ_CD
                )
            )
        AND ROWNUM = 1) OIH,
       (SELECT CURR_CD||' / '||
               TRIM(TO_CHAR(CHG_UT_AMT, '999,999,999.00'))||' / '||
               TRIM(TO_CHAR(RAT_AS_QTY, '999.00'))||' / '||
               RAT_UT_CD||' / '||
               TRIM(TO_CHAR(CHG_AMT, '999,999,999.00'))
        FROM BKG_CHG_RT DIH
        WHERE BKG_NO = C.BKG_NO
        AND CHG_CD = 'DIH'
        AND FRT_INCL_XCLD_DIV_CD = 'N'
        AND (RAT_UT_CD = C.CNTR_TPSZ_CD
            OR RAT_UT_CD IN ('BX', 'BL')
            OR (
                RAT_UT_CD IN ('20', '40', 'HC', '45', '53')
                AND ( SELECT A.CNTR_SZ_CD FROM PRI_RAT_UT A WHERE A.RAT_UT_CD = DIH.RAT_UT_CD AND ROWNUM=1) = U.CNTR_SZ_CD
                )
            )
        AND ROWNUM = 1) DIH,
       (SELECT CURR_CD||' / '||
               TRIM(TO_CHAR(CHG_UT_AMT, '999,999,999.00'))||' / '||
               TRIM(TO_CHAR(RAT_AS_QTY, '999.00'))||' / '||
               RAT_UT_CD||' / '||
               TRIM(TO_CHAR(CHG_AMT, '999,999,999.00'))
        FROM BKG_CHG_RT CYR
        WHERE BKG_NO = C.BKG_NO
        AND CHG_CD = 'CYR'
        AND FRT_INCL_XCLD_DIV_CD = 'N'
        AND (RAT_UT_CD = C.CNTR_TPSZ_CD
            OR RAT_UT_CD IN ('BX', 'BL')
            OR (
                RAT_UT_CD IN ('20', '40', 'HC', '45', '53')
                AND ( SELECT A.CNTR_SZ_CD FROM PRI_RAT_UT A WHERE A.RAT_UT_CD = CYR.RAT_UT_CD AND ROWNUM=1) = U.CNTR_SZ_CD
                )
            )
        AND ROWNUM = 1) CYR,
        COUNT(DISTINCT R.BKG_NO) OVER (PARTITION BY 1) BKG_CNT,
        '' BKG_RHQ_CD,
        '' SVC_SCP_CD,
        '' FM_DT,
        '' TO_DT,
        NVL(DECODE(R.IDA_IHC_AUD_STS_CD,'E','U','Y','S',R.IDA_IHC_AUD_STS_CD),'N') AUD_STS_CD,
        TO_CHAR(R.IDA_AUD_BAT_DT, 'YYYYMMDD') REV_AUD_DT,
        B.INTER_RMK,
        B.XTER_RMK,
        (SELECT MAX(RDN_NO)
          FROM BKG_REV_DR_NOTE
          WHERE BKG_NO = B.BKG_NO) AS RDN_NO
FROM BKG_RATE R, BKG_BOOKING B, 
     BKG_CONTAINER C, PRI_RAT_UT U,
    (SELECT	OFC_CD 
     FROM	MDM_ORGANIZATION A
     START WITH	A.OFC_CD = @[bkg_rhq_cd]
     CONNECT BY	PRIOR A.OFC_CD	= A.PRNT_OFC_CD) O
WHERE R.RT_APLY_DT BETWEEN TO_DATE(@[fm_dt],'YYYY-MM-DD') AND TO_DATE(@[to_dt],'YYYY-MM-DD') + 0.99999
AND R.BKG_NO = B.BKG_NO
AND B.BKG_STS_CD <> 'X'
AND B.BKG_CGO_TP_CD = 'F'
#if (${so_bnd} == 'Outbound')
AND B.POR_CD <> B.POL_CD 
#else
AND B.POD_CD <> B.DEL_CD 
#end
AND R.BKG_NO = C.BKG_NO
AND C.CNTR_TPSZ_CD = U.RAT_UT_CD
AND B.BKG_OFC_CD = O.OFC_CD
#if (${svc_scp_cd} != '')
AND B.SVC_SCP_CD = @[svc_scp_cd]
#end
AND B.POR_CD LIKE @[por_cd]||'%'
AND B.POL_CD LIKE @[pol_cd]||'%'
AND B.POD_CD LIKE @[pod_cd]||'%'
AND B.DEL_CD LIKE @[del_cd]||'%'
#if (${aud_sts_cd} != '')
AND NVL(R.IDA_IHC_AUD_STS_CD,'N') = DECODE(@[aud_sts_cd],'S','Y','U','E','N')
#end
)
SELECT BKG.BKG_NO,
       BKG.RT_APLY_DT,
       BKG.BKG_CTRT_TP_CD,
       BKG.CTRT_NO,
       BKG.POR_CD,
       BKG.POL_CD,
       BKG.POD_CD,
       BKG.DEL_CD,
       BKG.RCV_TERM_CD,
       BKG.DE_TERM_CD,
       BKG.CNTR_NO,
       BKG.CNTR_TPSZ_CD,
       BKG.CNTR_WGT,
       BKG.OIH,
       BKG.DIH,
       BKG.CYR,
       BKG.BKG_RHQ_CD,
       BKG.SVC_SCP_CD,
       BKG.FM_DT,
       BKG.TO_DT,
       BKG.BKG_CNT,
       SO.SO_NO,
       SO.FM_NOD_CD,
       SO.TO_NOD_CD,
       (SELECT INTG_CD_VAL_DP_DESC
         FROM COM_INTG_CD_DTL
        WHERE INTG_CD_ID = 'CD03279'
          AND INTG_CD_VAL_CTNT = BKG.AUD_STS_CD) AUD_STS_CD,
       BKG.REV_AUD_DT,
       BKG.INTER_RMK,
       BKG.XTER_RMK,
       (SELECT INTG_CD_VAL_DP_DESC
         FROM COM_INTG_CD_DTL
        WHERE INTG_CD_ID = 'CD00594'
          AND INTG_CD_VAL_CTNT = SO.TRSP_COST_DTL_MOD_CD) TRSP_COST_DTL_MOD,
       BKG.RDN_NO,
       COUNT(DISTINCT BKG.BKG_NO) OVER (PARTITION BY 1) TTL_BKG_CNT
   FROM BKG
   ,(SELECT BKG_NO
             ,EQ_NO
             ,TRSP_SO_OFC_CTY_CD || TRSP_SO_SEQ AS SO_NO
             ,FM_NOD_CD
             ,TO_NOD_CD
             ,SUBSTR(FM_NOD_CD,1,5) AS POR_CD 
             ,SUBSTR(TO_NOD_CD,1,5) AS POD_CD 
             ,TRSP_COST_DTL_MOD_CD
        FROM TRS_TRSP_SVC_ORD
       WHERE TRSP_COST_DTL_MOD_CD IN ('CY','DR')
         AND DELT_FLG = 'N') SO
  WHERE BKG.BKG_NO = SO.BKG_NO(+)
AND BKG.CNTR_NO = SO.EQ_NO(+)
#if (${so_bnd} == 'Outbound')
AND BKG.POR_CD = SO.POR_CD(+)
#else
AND BKG.POD_CD = SO.POD_CD(+)
#end
			]]></sql>
			<params>
				<param name="bkg_rhq_cd" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="aud_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
