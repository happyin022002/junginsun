<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOHblForMndRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT A.BKG_NO
,      A.HBL_SEQ
,      A.BL_MK_DESC
,      A.BL_GDS_DESC
,      (select CNTR_MF_GDS_DESC from BKG_CNTR_MF_DESC where bkg_no=A.BKG_NO and CNTR_MF_NO=a.CNTR_MF_NO and rownum=1) CNTR_MF_GDS_DESC
,      A.CRE_USR_ID
,      A.UPD_USR_ID
FROM   BKG_HBL A
WHERE  A.BKG_NO = @[bkg_no]
ORDER BY A.HBL_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
