<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOaddAutoMACAMDSndLogCSQL">
			<desc><![CDATA[Master LOG Table로 전송일, VVD, B/L Info. 저장]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_KR_SND_LOG 
    (
        MSG_LOG_TP_ID ,
        SND_DT ,
        OFC_CD,
        FLT_FILE_REF_NO ,
        TRSM_USR_ID ,
        VSL_CD,
        SKD_VOY_NO,
        SKD_DIR_CD,
        POL_CD ,
        BL_NO,
        SMT_AMD_NO ,
        CORR_CD1 ,
        MF_RCVR_USR_ID ,
        MF_SND_SEQ,
        CRE_USR_ID ,
        CRE_DT ,
        UPD_USR_ID ,
        UPD_DT )
    SELECT  '6SK',
        TO_DATE(@[snd_dt], 'YYYYMMDD HH24:MI:SS'),
        @[ofc_cd],
        @[flt_file_ref_no],
		@[usr_id],
        SUBSTR(@[vvd_cd], 1, 4),
        SUBSTR(@[vvd_cd], 5, 4),
        SUBSTR(@[vvd_cd], 9, 1),
        @[pol_cd],
        @[bl_no],
        @[sub_no],
        'B' ,
        RPAD('KCS4G002', 20),
        @[snd_seq],
        @[usr_id],
        SYSDATE ,
        @[usr_id],
        SYSDATE 
    FROM    DUAL			]]></sql>
			<params>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="sub_no" type="12" value="" out="N"/>
				<param name="snd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
