<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOmodifyCstmsCdConvDescUSQL">
			<desc><![CDATA[modifyCstmsCdConvDesc]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_CD_CONV_DESC
SET CSTMS_CD_DESC = @[cstms_cd_desc],
	ATTR_NM1 = @[attr_nm1],
	ATTR_NM2 = @[attr_nm2],
	ATTR_NM3 = @[attr_nm3],
	ATTR_NM4 = @[attr_nm4],
	ATTR_NM5 = @[attr_nm5],
	UPD_USR_ID = @[user_id],
	UPD_DT = SYSDATE
WHERE CNT_CD = @[cnt_cd]
AND CSTMS_DIV_ID = @[cstms_div_id]			]]></sql>
			<params>
				<param name="cstms_cd_desc" type="12" value="" out="N"/>
				<param name="attr_nm1" type="12" value="" out="N"/>
				<param name="attr_nm2" type="12" value="" out="N"/>
				<param name="attr_nm3" type="12" value="" out="N"/>
				<param name="attr_nm4" type="12" value="" out="N"/>
				<param name="attr_nm5" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_div_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
