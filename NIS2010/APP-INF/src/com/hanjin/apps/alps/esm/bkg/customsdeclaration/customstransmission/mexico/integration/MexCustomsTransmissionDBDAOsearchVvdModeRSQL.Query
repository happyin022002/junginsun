<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MexCustomsTransmissionDBDAOsearchVvdModeRSQL">
			<desc><![CDATA[DWKIM, 0370, OUTVO : MxVvdModeVO]]></desc>
			<sql><![CDATA[
SELECT VSL_PRE_PST_CD 
FROM BKG_VVD
WHERE
	BKG_NO = @[bkg_no]
	AND VSL_CD      = SUBSTR(@[vvd],1,4)
	AND SKD_VOY_NO   = SUBSTR(@[vvd],5,4)
	AND SKD_DIR_CD  = SUBSTR(@[vvd],9,1)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
