<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOsearchWhfExptInfoRSQL">
			<desc><![CDATA[Wharfage 정보 조회]]></desc>
			<sql><![CDATA[
SELECT BKG_RT_WHF_EXPT_CD
     , DECODE(BKG_RT_WHF_EXPT_CD,'S',WHF_SHPR_RGST_NO,' ') WHF_SHPR_RGST_NO
  FROM BKG_RATE
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
