<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchUsHandlingOfcRSQL">
			<desc><![CDATA[EBookingReceiptDBDAOSearchUsHandlingOfc]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
FROM (
SELECT 1 RANK
       ,HNDL_OFC_CD AS OFC_CD
  FROM BKG_HNDL_OFC_STUP
 WHERE upper(@[cmdt_nm]) LIKE '%'||CMDT_NM||'%' 
   AND CMDT_NM IS NOT NULL
   AND @[pol_cd] LIKE 'US%'
 UNION
SELECT 2 RANK
       ,HNDL_OFC_CD AS OFC_CD
  FROM BKG_HNDL_OFC_STUP
 WHERE POL_CD = @[pol_cd]
ORDER BY RANK
)
WHERE ROWNUM =1			]]></sql>
			<params>
				<param name="cmdt_nm" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
