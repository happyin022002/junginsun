<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOmodifyBlKrWhfExptUSQL">
			<desc><![CDATA[u]]></desc>
			<sql><![CDATA[
UPDATE BKG_RATE
     SET BKG_RT_WHF_EXPT_CD = @[kr_whf_expt_cd],
          WHF_SHPR_RGST_NO = DECODE(@[kr_whf_expt_cd], 'S', @[registno], NULL),
          UPD_USR_ID = @[upd_usr_id],
          UPD_DT = SYSDATE
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="kr_whf_expt_cd" type="12" value="" out="N"/>
				<param name="registno" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
