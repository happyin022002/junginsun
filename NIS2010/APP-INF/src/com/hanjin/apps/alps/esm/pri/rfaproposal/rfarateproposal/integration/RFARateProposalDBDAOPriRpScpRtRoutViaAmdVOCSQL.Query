<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtRoutViaAmdVOCSQL">
			<desc><![CDATA[RFARateProposalDBDAOPriRpScpRtRoutViaAmdVOCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_RT_ROUT_VIA(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    CMDT_HDR_SEQ,
    ROUT_SEQ,
    ORG_DEST_TP_CD,
    ROUT_VIA_SEQ,
    ROUT_VIA_PORT_TP_CD,
    ROUT_VIA_PORT_DEF_CD,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    PROP_NO                 ,
    AMDT_SEQ+1              ,
    SVC_SCP_CD              ,
    CMDT_HDR_SEQ            ,
    ROUT_SEQ                ,
    ORG_DEST_TP_CD          ,
    ROUT_VIA_SEQ            ,
    ROUT_VIA_PORT_TP_CD     ,
    ROUT_VIA_PORT_DEF_CD    ,
    PRC_PROG_STS_CD         ,
    SRC_INFO_CD             ,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    @[cre_usr_id]           ,
    SYSDATE                 ,
    @[upd_usr_id]           ,
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ
FROM 
    PRI_RP_SCP_RT_ROUT_VIA VIA
WHERE
    PROP_NO     = @[prop_no]
AND AMDT_SEQ    = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'
AND EXISTS ( 
    SELECT 'OK'
    FROM    
        PRI_RP_SCP_RT
    WHERE   
        PROP_NO             = VIA.PROP_NO
    AND AMDT_SEQ            = VIA.AMDT_SEQ
    AND SVC_SCP_CD          = VIA.SVC_SCP_CD
    AND CMDT_HDR_SEQ        = VIA.CMDT_HDR_SEQ
    AND ROUT_SEQ            = VIA.ROUT_SEQ 
	AND SRC_INFO_CD			<> 'AD'   
)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
