		<query name="PRICommonDBDAOSearchAocTariffCountryListRSQL">
			<desc><![CDATA[AocTariffCountryList 조회]]></desc>
			<sql><![CDATA[
SELECT A1.CNT_CD	AS CD
      ,A2.CNT_NM	AS NM
FROM AOC_TRF_CURR A1
    ,MDM_COUNTRY A2
WHERE RHQ_CD IN ('HAMRU', 'NYCRA')
  AND A1.CNT_CD = A2.CNT_CD			]]></sql>
