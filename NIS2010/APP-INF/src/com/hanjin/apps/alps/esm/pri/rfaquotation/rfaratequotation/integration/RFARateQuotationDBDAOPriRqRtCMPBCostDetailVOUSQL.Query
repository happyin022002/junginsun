<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAOPriRqRtCMPBCostDetailVOUSQL">
			<desc><![CDATA[pri_rq_rt를 일괄 조정한다.]]></desc>
			<sql><![CDATA[

UPDATE  PRI_RQ_RT
   SET  PRS_RESPB_CMPB_AMT = QTTN_RT_AMT + PRS_SCG_AMT - PRS_RESPB_CM_UC_AMT
       ,PRS_PFIT_CMPB_AMT = QTTN_RT_AMT + PRS_SCG_AMT - PRS_PFIT_CM_UC_AMT
       ,PRS_RESPB_OPB_AMT = QTTN_RT_AMT + PRS_SCG_AMT - PRS_RESPB_OPFIT_UC_AMT
 WHERE  QTTN_NO = @[qttn_no]
   AND  QTTN_VER_NO = @[qttn_ver_no]
   AND  CMDT_HDR_SEQ = @[cmdt_hdr_seq]
   AND  ROUT_SEQ = @[rout_seq]
   AND  RT_SEQ = @[rt_seq]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
