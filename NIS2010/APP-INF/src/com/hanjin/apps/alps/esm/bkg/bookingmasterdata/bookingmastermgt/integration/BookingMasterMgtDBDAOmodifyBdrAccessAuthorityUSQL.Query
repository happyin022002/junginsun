<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOmodifyBdrAccessAuthorityUSQL">
			<desc><![CDATA[BdrAccessAuthority를 수정하는 쿼리]]></desc>
			<sql><![CDATA[
UPDATE BKG_HRD_CDG_CTNT SET 


ATTR_CTNT1 = @[usr_id],
ATTR_CTNT2 = @[open_auth],
ATTR_CTNT3 = @[close_auth],
UPD_USR_ID = @[upd_usr_id],
UPD_DT = SYSDATE

WHERE	HRD_CDG_ID ='BDR_MAN_USER'
AND HRD_CDG_ID_SEQ = @[hrd_cdg_id_seq]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="open_auth" type="12" value="" out="N"/>
				<param name="close_auth" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="hrd_cdg_id_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
