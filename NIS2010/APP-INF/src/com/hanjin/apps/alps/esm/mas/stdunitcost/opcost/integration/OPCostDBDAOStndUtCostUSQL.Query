<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPCostDBDAOStndUtCostUSQL">
			<desc><![CDATA[StndUtCost
2015.03.26 컬럼 속성명 변경으로 수정()]]></desc>
			<sql><![CDATA[
UPDATE MAS_STND_UT_COST
   SET TEU_UC_AMT  = @[teu_uc_amt]
     , UPD_USR_ID   = @[upd_usr_id]
     , UPD_DT       = SYSDATE
 WHERE COST_YR = @[cost_yr]
   AND BSE_QTR_CD = @[cost_qtr_cd]
   AND TRD_CD     = @[trd_cd]
   AND SUB_TRD_CD = @[sub_trd_cd]
   AND RLANE_CD   = @[rlane_cd]
   AND DIR_CD     = @[dir_cd]
   AND HUL_BND_CD = @[hul_bnd_cd]
   AND STND_COST_CD = @[stnd_cost_cd]			]]></sql>
			<params>
				<param name="teu_uc_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yr" type="12" value="" out="N"/>
				<param name="cost_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="hul_bnd_cd" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
