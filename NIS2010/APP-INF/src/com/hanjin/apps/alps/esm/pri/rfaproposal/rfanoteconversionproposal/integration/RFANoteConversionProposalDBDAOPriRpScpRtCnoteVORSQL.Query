<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFANoteConversionProposalDBDAOPriRpScpRtCnoteVORSQL">
			<desc><![CDATA[PRI_RP_SCP_RT_CNOTE 조회]]></desc>
			<sql><![CDATA[
SELECT A.PROP_NO
	 , A.AMDT_SEQ
	 , A.SVC_SCP_CD
	 , A.GEN_SPCL_RT_TP_CD
	 , A.CMDT_HDR_SEQ
	 , A.CMDT_NOTE_SEQ
	 , A.NOTE_CLSS_CD
	 , A.CHG_CD
	 , A.NOTE_CTNT
	 , A.NOTE_CONV_MAPG_ID
	 , A.NOTE_CHG_TP_CD
	 , A.PRC_PROG_STS_CD
	 , A.SRC_INFO_CD
	 , A.N1ST_CMNC_DT
  FROM PRI_RP_SCP_RT_CNOTE A
     , COM_INTG_CD_DTL B
 WHERE PROP_NO = @[prop_no]
   AND AMDT_SEQ = @[amdt_seq]
   AND SVC_SCP_CD = @[svc_scp_cd]
   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
   AND A.NOTE_CLSS_CD = B.INTG_CD_VAL_CTNT(+)
   AND B.INTG_CD_ID = 'CD01711'
 ORDER BY B.INTG_CD_VAL_DP_SEQ, CHG_CD			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
