<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOModifyBkgTroDtlByBkgUSQL">
			<desc><![CDATA[TRO KR Update - Return CY]]></desc>
			<sql><![CDATA[
UPDATE BKG_TRO_DTL SET 
  RTN_LOC_CD = (select substr(FULL_RTN_YD_CD, 1, 5) from bkg_booking where bkg_no = @[bkg_no])
  ,RTN_YD_CD = (select FULL_RTN_YD_CD from bkg_booking where bkg_no = @[bkg_no])
  ,UPD_USR_ID = @[usr_id]
  ,UPD_DT = sysdate 
WHERE BKG_NO      = @[bkg_no]
  AND IO_BND_CD   = 'O'
  AND RTN_TRO_FLG = 'N'
  AND NVL(CXL_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
