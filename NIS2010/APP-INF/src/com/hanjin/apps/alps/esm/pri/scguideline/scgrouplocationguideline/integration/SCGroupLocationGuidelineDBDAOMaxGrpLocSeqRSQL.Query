<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupLocationGuidelineDBDAOMaxGrpLocSeqRSQL">
			<desc><![CDATA[grp_loc_seq 채번]]></desc>
			<sql><![CDATA[
SELECT 
       NVL((SELECT /*+ INDEX_DESC(a XPKPRI_SG_GRP_LOC) */
                   grp_loc_seq
              FROM pri_sg_grp_loc a
             WHERE svc_scp_cd = @[svc_scp_cd]
               AND gline_seq = @[gline_seq]
               AND rownum = 1), 0) + 1 AS next_seq
FROM DUAL			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="n"/>
				<param name="gline_seq" type="2" value="" out="n"/>
			</params>
		</query>
	</querys>
</sqls>
