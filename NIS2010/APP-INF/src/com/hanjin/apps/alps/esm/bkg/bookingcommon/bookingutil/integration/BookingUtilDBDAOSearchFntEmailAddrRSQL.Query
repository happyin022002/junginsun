<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchFntEmailAddrRSQL">
			<desc><![CDATA[BookingUtilDBDAOSearchFntEmailAddrRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_GET_FNT_RCV_EML_FNC('',BL.BKG_NO,BL.UPD_USR_ID,'FNT_EML') AS EMAIL FROM BKG_BL_DOC BL
WHERE BL.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
