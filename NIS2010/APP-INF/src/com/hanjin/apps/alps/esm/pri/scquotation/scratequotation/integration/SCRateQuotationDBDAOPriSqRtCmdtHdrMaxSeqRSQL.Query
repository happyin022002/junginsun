<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateQuotationDBDAOPriSqRtCmdtHdrMaxSeqRSQL">
			<desc><![CDATA[max seq select]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(MAX(CMDT_HDR_SEQ)+1,1) AS MAX_SEQ
FROM PRI_SQ_RT_CMDT_HDR
WHERE	QTTN_NO = @[qttn_no]
AND	QTTN_VER_NO = @[qttn_ver_no]
AND	GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd] 
			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
