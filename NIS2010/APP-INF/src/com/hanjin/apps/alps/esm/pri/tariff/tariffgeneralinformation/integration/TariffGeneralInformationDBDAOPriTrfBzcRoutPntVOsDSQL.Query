<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffGeneralInformationDBDAOPriTrfBzcRoutPntVOsDSQL">
			<desc><![CDATA[Tariff Basic Route Point 정보를 Delete 한다.]]></desc>
			<sql><![CDATA[
DELETE
  FROM  PRI_TRF_BZC_ROUT_PNT
 WHERE  1 = 1
   AND  TRF_PFX_CD           = @[trf_pfx_cd]          
   AND  TRF_NO               = @[trf_no]              
   AND  AMDT_SEQ             = @[amdt_seq]            
   AND  ORG_DEST_TP_CD       = @[org_dest_tp_cd]      
   AND  TRF_BZC_ROUT_PNT_SEQ = @[trf_bzc_rout_pnt_seq]			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="trf_bzc_rout_pnt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
