<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentDBDAOSearchQtaAdjustmentListRSQL">
			<desc><![CDATA[SKD Adjustment by VVD 정보를 조회합니다.

* History
2014.01.02 이혜민 [CHM-201328060-01] SQM 데이터 조회시 현재날짜와 조건날짜 비교하여 다른 테이블에서 조회되도록 수정
2015.06.15 김용습 [CHM-201536164] 주간 MAS Open에 따른 타모듈 프로그램 적용 요청
2015.11.06 김용습 [CHM-201538494] [CSR 전환건] SKD Adjustment by VVD 화면 보완 (Trade Direction, Adjusting VVD Select, BSA Flag 칼럼 추가) ]]></desc>
			<sql><![CDATA[
SELECT A1.TRD_CD
      ,A1.RLANE_CD
      ,A1.SUB_TRD_CD
      ,A1.DIR_CD
      ,DECODE(MAS.HUL_BND_CD,'BH','B/H','H/H') HUL_BND_CD
      ,A1.IOC_CD
      ,A1.BSE_MON
      ,A1.BSE_WK
      ,A1.VVD
      ,A1.FNL_BSA_CAPA
      ,A1.MAS_BSE_MON
      ,A1.MAS_BSE_WK
      ,A1.MAS_VVD
      ,A1.MAS_FNL_BSA_CAPA
      ,CASE
            WHEN A1.FLAG = 'U' AND A1.FNL_BSA_CAPA <> A1.MAS_FNL_BSA_CAPA THEN 'B'
            WHEN A1.FLAG = 'U' AND A1.BSE_MON      <> A1.MAS_BSE_MON      THEN 'M'
            WHEN A1.FLAG = 'U' AND A1.BSE_WK       <> A1.MAS_BSE_WK       THEN 'W'
                                                                 ELSE A1.FLAG
       END AS STS_FLAG
      ,A1.FLAG
      ,DECODE((SELECT MAS_MON_VVD.BSA_ZR_FLG 
            FROM MAS_MON_VVD 
            WHERE MAS_MON_VVD.TRD_CD = A1.TRD_CD
                AND MAS_MON_VVD.RLANE_CD = A1.RLANE_CD
                AND MAS_MON_VVD.SUB_TRD_CD = A1.SUB_TRD_CD
                AND MAS_MON_VVD.DIR_CD = A1.DIR_CD
                AND MAS_MON_VVD.IOC_CD = A1.IOC_CD
--                AND MAS_MON_VVD.COST_YRMON = NVL(MAS_COST_YRMON, A1.SLS_YRMON) 
                AND MAS_MON_VVD.VSL_CD||MAS_MON_VVD.SKD_VOY_NO||MAS_MON_VVD.DIR_CD = NVL(MAS_VVD,VVD)), 'Y', 'Y', 'N', '', '') AS BSA_ZR_FLG
  FROM (
           SELECT O_TRD_CD          AS TRD_CD
                 ,O_RLANE_CD        AS RLANE_CD
                 ,O_SUB_TRD_CD      AS SUB_TRD_CD
                 ,O_DIR_CD          AS DIR_CD
                 ,O_IOC_CD          AS IOC_CD
                 ,O_VVD
                 ,MAX(BSE_MON)      AS BSE_MON
                 ,MAX(BSE_WK)       AS BSE_WK
                 ,MAX(VVD)          AS VVD
                 ,MAX(FNL_BSA_CAPA) AS FNL_BSA_CAPA
                 ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') THEN '' ELSE SLS_MON                END) AS MAS_BSE_MON
                 ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') THEN '' ELSE COST_WK                END) AS MAS_BSE_WK
                 ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') THEN '' ELSE MAS_VVD                END) AS MAS_VVD
                 ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') THEN '' ELSE FNL_HJS_BSA_CAPA || '' END) AS MAS_FNL_BSA_CAPA
                 ,CASE WHEN MAX(SQM_INFO) = MAX(MAS_INFO) THEN 'R'
                       WHEN NVL(MAX(SQM_INFO), '*') = '*' THEN 'I'
                       WHEN NVL(MAX(MAS_INFO), '*') = '*' THEN 'D'
                                                          ELSE 'U'
                  END AS FLAG
                 ,MAX(SQM_INFO) AS SQM_INFO
                 ,MAX(MAS_INFO) AS MAS_INFO
             FROM (
                     SELECT B1.TRD_CD
                           ,B1.DIR_CD
                           ,B1.SUB_TRD_CD
                           ,B1.RLANE_CD
                           ,B1.BSE_MON
                           ,B1.BSE_WK
                           ,B1.VSL_CD || B1.SKD_VOY_NO || B1.DIR_CD AS VVD
                           ,B1.FNL_BSA_CAPA
                           ,SUBSTR(B2.SLS_YRMON, -2) AS SLS_MON
                           ,B2.COST_WK
                           ,B2.VSL_CD || B2.SKD_VOY_NO || B2.DIR_CD AS MAS_VVD
                           ,B2.FNL_HJS_BSA_CAPA
                           ,B1.TRD_CD || B1.SUB_TRD_CD || B1.RLANE_CD || B1.DIR_CD || B1.VSL_CD || B1.SKD_VOY_NO || B1.DIR_CD || B1.BSE_MON || B1.BSE_WK || B1.FNL_BSA_CAPA AS SQM_INFO
                           ,B2.TRD_CD || B2.SUB_TRD_CD || B2.RLANE_CD || B2.DIR_CD || B2.VSL_CD || B2.SKD_VOY_NO || B2.DIR_CD || SUBSTR(B2.SLS_YRMON,-2) || B2.COST_WK || B2.FNL_HJS_BSA_CAPA AS MAS_INFO
                           ,NVL(B1.TRD_CD         , B2.TRD_CD)                AS O_TRD_CD
                           ,NVL(B1.RLANE_CD       , B2.RLANE_CD)              AS O_RLANE_CD
                           ,NVL(B1.SUB_TRD_CD     , B2.SUB_TRD_CD)            AS O_SUB_TRD_CD
                           ,NVL(B1.IOC_CD         , B2.IOC_CD)                AS O_IOC_CD
                           ,NVL(B1.DIR_CD         , B2.DIR_CD)                AS O_DIR_CD
                           ,NVL(B1.BSE_MON        , SUBSTR(B2.SLS_YRMON, -2)) AS O_BSE_MON
                           ,NVL(B1.BSE_WK         , B2.COST_WK)               AS O_BSE_WK
                           ,NVL(B1.VSL_CD || B1.SKD_VOY_NO || B1.DIR_CD, B2.VSL_CD || B2.SKD_VOY_NO || B2.DIR_CD) AS O_VVD
                       FROM (
                              SELECT 
                                     TRD_CD
                                    ,DIR_CD
                                    ,SUB_TRD_CD
                                    ,RLANE_CD
                                    ,BSE_MON
                                    ,BSE_WK
                                    ,VSL_CD
                                    ,SKD_VOY_NO
                                    ,FNL_BSA_CAPA
                                    ,IOC_CD
                                FROM SQM_SPCL_TGT_VVD
                               WHERE BSE_TP_CD  = @[f_bse_tp_cd]
                                 AND BSE_YR     = @[f_bse_yr]
                                 AND BSE_QTR_CD = @[f_bse_qtr_cd]
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
                                 AND TRD_CD     = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                                 AND RLANE_CD   = @[f_rlane_cd]
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                                 AND DIR_CD     = @[f_dir_cd]
#end
                            ) B1
                            FULL OUTER JOIN
                            (
                              SELECT 
                                     A1.TRD_CD
                                    ,A1.SUB_TRD_CD
                                    ,A1.RLANE_CD
                                    ,A1.DIR_CD
                                    ,A1.VSL_CD
                                    ,A1.SKD_VOY_NO
                                    ,A1.SLS_YRMON
                                    ,A1.COST_WK
                                    ,A1.IOC_CD
                                    ,NVL(A2.FNL_HJS_BSA_CAPA, 0) AS FNL_HJS_BSA_CAPA
                                FROM MAS_MON_VVD       A1
                                    ,BSA_VVD_MST       A2
                                    ,(
                                       SELECT DISTINCT
                                              BSE_TP_CD
                                             ,BSE_YR
                                             ,BSE_QTR_CD
                                             ,TRD_CD
                                             ,RLANE_CD
                                             ,DIR_CD
#if (${f_cond_tp} == '1')
                                       -- 해당 분기의 LANE-OFFICE RELATION SETTING 한 노선의 정보를 걸어준다
                                       -- (2013년간 데이터를 제외한 모든 분기를 걸어주면됨 현재는 현재 분기와 미래 판매목표만 걸어주고 있음)
                                         FROM SQM_QTA_LANE_OFC
#else
                                       -- 2013년도 데이터는  LANE-OFFICE RELATION SETTING 정보가 없기 때문에 TARGET VVD 정보를 걸어서 노선정보를 가져온다.
                                         FROM SQM_SPCL_TGT_VVD
#end                                             
                                        WHERE 1=1
                                          AND BSE_TP_CD      = @[f_bse_tp_cd]
                                          AND BSE_YR         = @[f_bse_yr]
                                          AND BSE_QTR_CD     = @[f_bse_qtr_cd]
#if (${f_cond_tp} == '1')
                                          AND OFC_VW_CD      = 'L'
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
                                          AND TRD_CD         = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                                          AND RLANE_CD       = @[f_rlane_cd]
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                                          AND DIR_CD         = @[f_dir_cd]
#end
                                       ) A3
                               WHERE A1.TRD_CD      = A2.TRD_CD     (+)
                                 AND A1.RLANE_CD    = A2.RLANE_CD   (+)
                                 AND A1.VSL_CD      = A2.VSL_CD     (+)
                                 AND A1.SKD_VOY_NO  = A2.SKD_VOY_NO (+)
                                 AND A1.DIR_CD      = A2.SKD_DIR_CD (+)
                                 AND A1.IOC_CD      = A2.IOC_CD     (+)
                                 AND A1.TRD_CD      = A3.TRD_CD
                                 AND A1.RLANE_CD    = A3.RLANE_CD
                                 AND A1.DIR_CD      = A3.DIR_CD
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
                                 AND A1.TRD_CD      = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                                 AND A1.RLANE_CD    = @[f_rlane_cd]
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                                 AND A1.DIR_CD      = @[f_dir_cd]
#end
                                 AND A1.DELT_FLG    = 'N'
                                 AND A1.IOC_CD      = DECODE(A1.RLANE_CD, 'RBCCO', 'I', A1.IOC_CD) -- RBCCO 노선은 IOC_CD = 'I' 인것만
                                 -- 2013년 SLS_YRMON 이후는 Week로 걸어준다(2013년도는 SAQ 이행데이터로 기준이 월로 되어 있기 때문)
                                 AND CASE WHEN '2013' = A3.BSE_YR THEN A1.SLS_YRMON ELSE SUBSTR(A1.SLS_YRMON, 1, 4)||A1.COST_WK END 
                                     BETWEEN CASE WHEN '2013' = A3.BSE_YR THEN @[f_bse_yr]||DECODE(@[f_bse_qtr_cd],'1Q','01','2Q','04','3Q','07','4Q','10') ELSE @[f_bse_yr]||@[f_fm_wk] END 
                                         AND CASE WHEN '2013' = A3.BSE_YR THEN @[f_bse_yr]||DECODE(@[f_bse_qtr_cd],'1Q','03','2Q','06','3Q','09','4Q','12') ELSE @[f_bse_yr]||@[f_to_wk] END 
                                 AND NOT EXISTS ( SELECT 1
                                                    FROM SQM_QTA_TGT_VVD S1
                                                   WHERE S1.BSE_TP_CD  = @[f_bse_tp_cd]
                                                     AND S1.BSE_YR     = @[f_bse_yr]
                                                     AND S1.BSE_QTR_CD = @[f_bse_qtr_cd]
                                                     AND S1.DELT_FLG   = 'Y'
                                                     AND A1.TRD_CD     = S1.TRD_CD
                                                     AND A1.RLANE_CD   = S1.RLANE_CD
                                                     AND A1.VSL_CD     = S1.VSL_CD
                                                     AND A1.SKD_VOY_NO = S1.SKD_VOY_NO
                                                     AND A1.DIR_CD     = S1.DIR_CD
                                                )
                            ) B2
                         ON B1.TRD_CD          = B2.TRD_CD
                        AND B1.SUB_TRD_CD      = B2.SUB_TRD_CD
                        AND B1.DIR_CD          = B2.DIR_CD
                        AND B1.RLANE_CD        = B2.RLANE_CD
                        AND B1.IOC_CD          = B2.IOC_CD
                        AND B1.VSL_CD          = B2.VSL_CD
                        AND B1.SKD_VOY_NO      = B2.SKD_VOY_NO
                  )
         GROUP BY O_TRD_CD
                 ,O_RLANE_CD
                 ,O_SUB_TRD_CD
                 ,O_DIR_CD
                 ,O_IOC_CD
                 ,O_VVD
         ) A1, MAS_LANE_RGST MAS
WHERE A1.TRD_CD         = MAS.TRD_CD
AND A1.SUB_TRD_CD       = MAS.SUB_TRD_CD
AND A1.RLANE_CD         = MAS.RLANE_CD
AND A1.DIR_CD           = MAS.DIR_CD
AND A1.IOC_CD           = MAS.IOC_CD
#if (${f_trd_dir} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
AND MAS.HUL_BND_CD       = @[f_hul_bnd_cd]
#end
ORDER BY TRD_CD
        ,RLANE_CD
        ,DIR_CD
        ,NVL(MAS_BSE_MON, BSE_MON)
        ,NVL(BSE_WK, MAS_BSE_WK)			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
				<param name="f_hul_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
