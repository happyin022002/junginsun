<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOremoveBkgCodCostDSQL">
			<desc><![CDATA[bkg_cod_cost에서 해당 bkg_no, bkg_no_split, cod_rqst_seq에 맞는 row들를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_COD_COST
WHERE	BKG_NO = @[bkg_no]
AND	COD_RQST_SEQ = @[cod_rqst_seq]
#if (${cost_cd_rqst_seq} !='')
	AND COST_CD_RQST_SEQ = @[cost_cd_rqst_seq]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
				<param name="cost_cd_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
