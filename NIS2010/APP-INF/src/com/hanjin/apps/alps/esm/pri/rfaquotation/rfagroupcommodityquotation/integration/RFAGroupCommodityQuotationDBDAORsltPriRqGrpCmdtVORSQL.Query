		<query name="RFAGroupCommodityQuotationDBDAORsltPriRqGrpCmdtVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	QTTN_NO
,	QTTN_VER_NO
,	GRP_CMDT_SEQ
,	PRC_GRP_CMDT_CD
,	PRC_GRP_CMDT_DESC
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM PRI_RQ_GRP_CMDT
WHERE	QTTN_NO = @[qttn_no]
AND	QTTN_VER_NO = @[qttn_ver_no]
ORDER BY PRC_GRP_CMDT_CD			]]></sql>
			<params>
