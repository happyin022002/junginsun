<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchTSRouteForEqualPortRSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOSearchTSRouteForEqualPort]]></desc>
			<sql><![CDATA[
SELECT  count(1) CNT
FROM    bkg_eqlz_port
WHERE   loc_cd = @[pod_cd]     
AND     eqlz_port_cd = @[pol_cd]			]]></sql>
			<params>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
