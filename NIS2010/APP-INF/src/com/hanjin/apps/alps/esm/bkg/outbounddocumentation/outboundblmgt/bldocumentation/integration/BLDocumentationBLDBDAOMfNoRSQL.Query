<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOMfNoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
SELECT BKG_STS_CD
,      VSL_CD || SKD_VOY_NO || SKD_DIR_CD VVD
,      POR_CD
,      POL_CD
,      POD_CD
,      DEL_CD
,      PRE_RLY_PORT_CD
,      PST_RLY_PORT_CD
,      USA_CSTMS_FILE_CD
,      CND_CSTMS_FILE_CD
FROM   BKG_BKG_HIS
WHERE  BKG_NO = @[bkg_no]
AND    CORR_NO = 'TMP0000001'
#else 
SELECT BKG_STS_CD
,      VSL_CD || SKD_VOY_NO || SKD_DIR_CD VVD
,      POR_CD
,      POL_CD
,      POD_CD
,      DEL_CD
,      PRE_RLY_PORT_CD
,      PST_RLY_PORT_CD
,      USA_CSTMS_FILE_CD
,      CND_CSTMS_FILE_CD
FROM   BKG_BOOKING
WHERE  BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
