<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchBkgCstmsEurDgListRSQL">
			<desc><![CDATA[searchBkgCstmsEurDgList]]></desc>
			<sql><![CDATA[
SELECT  A.EUR_DG_DECL_TP_CD
       ,A.VSL_CD
       ,A.SKD_VOY_NO
       ,A.SKD_DIR_CD
       ,A.PORT_CD
       ,A.BKG_NO
       ,A.CNTR_NO
       ,A.CNTR_CGO_SEQ
  FROM  BKG_CSTMS_EUR_DG A
 WHERE  1 = 1
#if (${sch_type} == 'bkg_no') 
   AND  A.BKG_NO = @[bkg_no]
#elseif (${sch_type} != 'bl_no') 
   AND  A.BL_NO = @[bl_no]
#end
#if (${eur_dg_decl_tp_cd} != '') 
   AND  A.EUR_DG_DECL_TP_CD = @[eur_dg_decl_tp_cd]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="eur_dg_decl_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
