<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TAAProposalDBDAOPriTaaMnConfirmUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE PRI_TAA_MN SET 
 	CFM_FLG = @[cfm_flg]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
#if (${cfm_flg} == 'Y') 
,	CFM_DT = SYSDATE
#else
,   CFM_DT = NULL
#end
WHERE TAA_PROP_NO = @[taa_prop_no]
AND	  AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="taa_prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
