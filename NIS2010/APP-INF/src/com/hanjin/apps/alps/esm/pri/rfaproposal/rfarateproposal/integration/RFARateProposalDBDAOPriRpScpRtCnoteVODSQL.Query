<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtCnoteVODSQL">
			<desc><![CDATA[CMDT삭제]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_RP_SCP_RT_CNOTE
WHERE	PROP_NO = @[prop_no]
AND	AMDT_SEQ = @[amdt_seq]
AND	SVC_SCP_CD = @[svc_scp_cd]
#if (${CASCADE_LEVEL} == "0") 

#elseif (${CASCADE_LEVEL} == "1") 
AND	CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#else 
AND	CMDT_HDR_SEQ = @[cmdt_hdr_seq]
AND	CMDT_NOTE_SEQ = @[cmdt_note_seq]
#end
#if (${IS_DEL_AMEND} == "Y") 
AND SRC_INFO_CD IN ('NW', 'GC', 'GM', 'PC', 'PM')
AND PRC_PROG_STS_CD = 'I'
#end			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="cmdt_note_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
