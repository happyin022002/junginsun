<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOCheckTurkeyTaxIdRSQL">
			<desc><![CDATA[POD, DEL이 터키인 BKG에 대해 Consignee와 Notify Tax Id 유무를 판단]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y')
SELECT 'Y'
FROM BKG_XPT_IMP_LIC_HIS
WHERE IO_BND_CD = 'I'
AND CNT_CD = 'TR'
AND (TR_CNEE_TAX_ID IS NOT NULL OR TR_NTFY_TAX_ID IS NOT NULL)
AND BKG_NO = @[bkg_no]
AND CORR_NO = 'TMP0000001'
#else
SELECT 'Y'
FROM BKG_XPT_IMP_LIC
WHERE IO_BND_CD = 'I'
AND CNT_CD = 'TR'
AND (TR_CNEE_TAX_ID IS NOT NULL OR TR_NTFY_TAX_ID IS NOT NULL)
AND BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
