			<desc><![CDATA[ModifyAgentRateDetail]]></desc>
			<sql><![CDATA[
UPDATE ACM_AGN_AGMT_DTL
   SET OFC_SET_TP_CD = @[ofc_set_tp_cd],
       OFC_CVRG_CD = @[ofc_cvrg_cd],
       OFC_CD = @[ofc_cd],
       OFT_PAY_TERM_CD = @[oft_pay_term_cd],
       FULL_MTY_CD = @[full_mty_cd],
       CURR_CD = @[curr_cd],
       COMM_FX_AMT = @[comm_fx_amt],
       COMM_PAY_TERM_CD = @[comm_pay_term_cd],
       REV_DIV_CD = @[rev_div_cd],
       COMM_RT = @[comm_rt],
       HLG_DDCT_ORG_FLG = @[hlg_ddct_org_flg],
       HLG_DDCT_DEST_FLG = @[hlg_ddct_dest_flg],
       FDRG_DDCT_ORG_FLG = @[fdrg_ddct_org_flg],
       FDRG_DDCT_DEST_FLG = @[fdrg_ddct_dest_flg],
	   -- 2017.08.18 Charge Commission 추가 -- start
	   CHG_COMM_DIV_CD = @[chg_comm_div_cd],
	   CHG_COMM_RT = @[chg_comm_rt],
	   CHG_COMM_OTR_AMT = @[chg_comm_otr_amt], 
	   CHG_COMM_CURR_CD = @[chg_comm_curr_cd],
	   CHG_COMM_PAY_TERM_CD = @[chg_comm_pay_term_cd], 
	   -- 2017.08.18 Charge Commission 추가 -- end
       UPD_USR_ID = @[usr_id],
       UPD_DT = SYSDATE
 WHERE AGN_CD = @[agn_cd]
   AND AGN_AGMT_NO = @[agn_agmt_no]
   AND IO_BND_CD = @[io_bnd_cd]
   AND AC_TP_CD = @[ac_tp_cd]
   AND AGN_AGMT_SEQ = @[agn_agmt_seq]			]]></sql>
			<params>
				<param name="ofc_set_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cvrg_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="oft_pay_term_cd" type="12" value="" out="N"/>
