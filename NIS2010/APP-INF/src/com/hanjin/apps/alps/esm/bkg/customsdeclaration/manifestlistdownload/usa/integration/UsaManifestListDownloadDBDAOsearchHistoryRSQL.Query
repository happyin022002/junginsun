<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchHistoryRSQL">
			<desc><![CDATA[UsaBlHistoryVO]]></desc>
			<sql><![CDATA[
SELECT *
FROM (      
    SELECT  H.HIS_TP_ID
		   ,H.HIS_SEQ
		   ,D.HIS_SUB_SEQ
           ,D.HIS_ITM_CTNT
           ,D.PRE_CTNT
           ,D.CRNT_CTNT
           ,U.USR_NM
           ,U.OFC_CD
           ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC ('KRACY', H.UPD_DT, 'USNYC'), 'yyyy-mm-dd hh24:mi:ss') CRE_DT
           ,H.CRE_USR_ID
           ,ROW_NUMBER() OVER(ORDER BY H.CRE_DT, H.HIS_SEQ, D.HIS_SUB_SEQ) AS RNUM
           ,COUNT(*) OVER() AS TOTAL
    FROM    BKG_CSTMS_IBD_HIS H
           ,BKG_CSTMS_IBD_HIS_DTL D
           ,COM_USER U
    WHERE   1=1
    AND     H.CNT_CD = @[cnt_cd]
    AND     H.BL_NO = @[bl_no]
    AND     H.CNT_CD = D.CNT_CD
    AND     H.BL_NO = D.BL_NO
    AND     H.HIS_SEQ = D.HIS_SEQ
    AND     H.CRE_USR_ID = U.USR_ID(+)
)
WHERE RNUM BETWEEN @[start_no] AND @[end_no]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="start_no" type="12" value="" out="N"/>
				<param name="end_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
