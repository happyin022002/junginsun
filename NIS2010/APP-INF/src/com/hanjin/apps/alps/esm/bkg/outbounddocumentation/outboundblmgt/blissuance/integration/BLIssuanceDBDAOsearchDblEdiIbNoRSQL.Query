<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiIbNoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT MAX(MST.XTER_SNDR_ID) EDI_SENDER, MAX(A.IB_NO) IB_NO
FROM (
        SELECT MAX(DECODE(BKG_REF_TP_CD, 'ESRF', CUST_REF_NO_CTNT)) IB_NO
        FROM   BKG_REFERENCE
        WHERE  BKG_NO = @[bkg_no]
        GROUP BY BKG_NO
) A,
BKG_XTER_RQST_MST MST
WHERE MST.BKG_NO = @[bkg_no]
  AND MST.XTER_RQST_NO = A.IB_NO
GROUP BY MST.XTER_SNDR_ID			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
