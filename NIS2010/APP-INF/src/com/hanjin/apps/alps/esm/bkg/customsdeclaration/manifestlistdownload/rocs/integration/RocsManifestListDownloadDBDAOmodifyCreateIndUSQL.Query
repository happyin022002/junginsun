<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOmodifyCreateIndUSQL">
			<desc><![CDATA[BL Create]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_RTM_VSL
   SET VSL_CALL_REF_STS_CD = 'Y'
       , CSTMS_DECL_USR_ID = @[user_id]
       , BL_CRE_DT = sysdate
WHERE  VSL_CALL_REF_NO = @[crn_number]
AND VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]
#if ( ${pod_clpt_ind_seq} != '')
AND NVL(POD_CLPT_IND_SEQ,'1') = @[pod_clpt_ind_seq]
#end			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="crn_number" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
