<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCntrEtcInfoRSQL">
			<desc><![CDATA[RSQL]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
SELECT A.BKG_NO
,      A.BL_NO || NVL(A.BL_TP_CD, DECODE(C.OBL_SRND_FLG, 'Y', 'S', NULL)) AS BL_NO
,      A.BL_TP_CD
,      A.BKG_STS_CD
,      A.BKG_CGO_TP_CD
,	   A.STWG_CD
,      A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD T_VVD
,      A.VSL_CD
,      A.SKD_VOY_NO
,      A.SKD_DIR_CD
,      A.POR_CD||SUBSTR(A.POR_NOD_CD,6,2) POR_CD
,      A.POL_CD||SUBSTR(A.POL_NOD_CD,6,2) POL_CD
,      A.POD_CD||SUBSTR(A.POD_NOD_CD,6,2) POD_CD
,      A.DEL_CD||SUBSTR(A.DEL_NOD_CD,6,2) DEL_CD
,      A.RCV_TERM_CD
,      A.DE_TERM_CD
,      DECODE(B.BKG_DOC_PROC_TP_CD, 'CNTCFM', B.EVNT_USR_ID, '') EVNT_USR_ID
,      DECODE(B.BKG_DOC_PROC_TP_CD, 'CNTCFM', TO_CHAR(B.EVNT_DT, 'YYYY-MM-DD'), '') EVNT_DT
,      DECODE(B.BKG_DOC_PROC_TP_CD, 'CNTCFM', 'Y', 'N') FNL_CFM_FLG
,      '' MODIFY_FNL_CFM_FLG
,      (SELECT WGT_UT_CD FROM BKG_CNTR_HIS WHERE BKG_NO=A.BKG_NO AND CORR_NO = 'TMP0000001' AND ROWNUM=1) BKG_WGT_UT_CD
,      (SELECT MEAS_UT_CD FROM BKG_CNTR_HIS WHERE BKG_NO=A.BKG_NO AND CORR_NO = 'TMP0000001' AND ROWNUM=1) BKG_MEAS_UT_CD
,      (SELECT TO_CHAR(MAX(CGO_RCV_DT), 'YYYY-MM-DD') FROM BKG_CNTR_HIS WHERE BKG_NO=A.BKG_NO AND CORR_NO = 'TMP0000001') CGO_RCV_DT
,      (SELECT NVL2(CORR_NO, 'Y', 'N') FROM BKG_BL_DOC_HIS WHERE BKG_NO=A.BKG_NO AND CORR_NO = 'TMP0000001') CORR_FLG
,      (SELECT BDR_FLG FROM BKG_BL_DOC_HIS WHERE BKG_NO=A.BKG_NO AND CORR_NO = 'TMP0000001') BDR_FLG
,      'N' NOT_UPDATED_FLG
,      (SELECT 'Y' FROM BKG_VVD_HIS WHERE BKG_NO=A.BKG_NO AND CORR_NO='TMP0000001' AND (POL_CD LIKE 'CN%' OR POD_CD LIKE 'CN%') AND ROWNUM=1) CN_FLG

,      (SELECT 'Y' FROM BKG_BKG_HIS BK, BKG_VVD_HIS VVD  WHERE BK.BKG_NO =  A.BKG_NO
                                                AND BK.BKG_NO = VVD.BKG_NO
                                                AND (BK.DEL_CD LIKE 'CN%' OR BK.POD_CD LIKE 'CN%' OR VVD.POD_CD LIKE 'CN%')
                                                                                AND BK.POD_CD != 'CNHKG' 
                                                                                AND BK.DEL_CD != 'CNHKG'
                                                                                AND  BK.CORR_NO = 'TMP0000001'
                                                                                AND  VVD.CORR_NO = 'TMP0000001'
                                                AND  NOT EXISTS ( SELECT 'X' FROM BKG_VVD_HIS V , BKG_BKG_HIS BKG
                                                                  WHERE V.BKG_NO = BK.BKG_NO  
                                                                                                            AND BKG.BKG_NO = BK.BKG_NO  
                                                                                                          AND ((V.POL_CD LIKE 'CN%' AND V.POD_CD LIKE 'CN%') OR V.POD_CD = 'CNHKG')
                                                                                                  AND BKG.CORR_NO = 'TMP0000001'
                                                                                                            AND V.CORR_NO = 'TMP0000001')

                                                AND ROWNUM = 1) CN_DIR_FLG
,      A.FLEX_HGT_FLG
,      A.CRE_USR_ID
,      A.UPD_USR_ID
FROM   BKG_BKG_HIS A
,      (SELECT *
        FROM (SELECT BKG_NO
        ,            BKG_DOC_PROC_TP_CD
        ,            DOC_PROC_SEQ
        ,            EVNT_USR_ID
        ,            EVNT_DT
        ,            UPD_DT
        ,            row_number() over(partition by BKG_NO order by upd_dt desc, rownum desc, rowid desc) row_no
              FROM   BKG_DOC_PROC_SKD_HIS
              WHERE  BKG_NO = @[bkg_no]
              AND    CORR_NO = 'TMP0000001'
              AND    BKG_DOC_PROC_TP_CD IN('CNTCFM', 'CNTRLS')
              AND    DOC_PERF_DELT_FLG = 'N') 
        WHERE  row_no = 1) B
,      BKG_BL_ISS_HIS C
WHERE  A.BKG_NO = B.BKG_NO(+)
AND    A.BKG_NO = C.BKG_NO(+)
AND    A.CORR_NO = C.CORR_NO(+)
AND    A.BKG_NO = @[bkg_no]
AND    A.CORR_NO = 'TMP0000001'
#else 
SELECT A.BKG_NO
,      A.BL_NO || NVL(A.BL_TP_CD, DECODE(C.OBL_SRND_FLG, 'Y', 'S', NULL)) AS BL_NO
,      A.BL_TP_CD
,      A.BKG_STS_CD
,      A.BKG_CGO_TP_CD
,	   A.STWG_CD
,      A.VSL_CD || A.SKD_VOY_NO || A.SKD_DIR_CD T_VVD
,      A.VSL_CD
,      A.SKD_VOY_NO
,      A.SKD_DIR_CD
,      A.POR_CD||SUBSTR(A.POR_NOD_CD,6,2) POR_CD
,      A.POL_CD||SUBSTR(A.POL_NOD_CD,6,2) POL_CD
,      A.POD_CD||SUBSTR(A.POD_NOD_CD,6,2) POD_CD
,      A.DEL_CD||SUBSTR(A.DEL_NOD_CD,6,2) DEL_CD
,      A.RCV_TERM_CD
,      A.DE_TERM_CD
,      DECODE(B.BKG_DOC_PROC_TP_CD, 'CNTCFM', B.EVNT_USR_ID, '') EVNT_USR_ID
,      DECODE(B.BKG_DOC_PROC_TP_CD, 'CNTCFM', TO_CHAR(B.EVNT_DT, 'YYYY-MM-DD'), '') EVNT_DT
,      DECODE(B.BKG_DOC_PROC_TP_CD, 'CNTCFM', 'Y', 'N') FNL_CFM_FLG
,      '' MODIFY_FNL_CFM_FLG
,      (SELECT WGT_UT_CD FROM BKG_CONTAINER WHERE BKG_NO=A.BKG_NO AND ROWNUM=1) BKG_WGT_UT_CD
,      (SELECT MEAS_UT_CD FROM BKG_CONTAINER WHERE BKG_NO=A.BKG_NO AND ROWNUM=1) BKG_MEAS_UT_CD
,      (SELECT TO_CHAR(MAX(CGO_RCV_DT), 'YYYY-MM-DD') FROM BKG_CONTAINER WHERE BKG_NO=A.BKG_NO) CGO_RCV_DT
,      (SELECT NVL2(CORR_NO, 'Y', 'N') FROM BKG_BL_DOC WHERE BKG_NO=A.BKG_NO) CORR_FLG
,      (SELECT BDR_FLG FROM BKG_BL_DOC WHERE BKG_NO=A.BKG_NO) BDR_FLG
,      'N' NOT_UPDATED_FLG
,      (SELECT 'Y' FROM BKG_VVD WHERE BKG_NO=A.BKG_NO AND (POL_CD LIKE 'CN%' OR POD_CD LIKE 'CN%') AND ROWNUM=1) CN_FLG
,      (SELECT 'Y' FROM BKG_BOOKING BK, BKG_VVD VVD  WHERE BK.BKG_NO =  A.BKG_NO
                                                AND BK.BKG_NO = VVD.BKG_NO
                                                AND (BK.DEL_CD LIKE 'CN%' OR BK.POD_CD LIKE 'CN%' OR VVD.POD_CD LIKE 'CN%')
                                                                                AND BK.POD_CD != 'CNHKG' 
                                                                             AND BK.DEL_CD != 'CNHKG' 
                                                AND NOT EXISTS ( SELECT 'X' FROM BKG_VVD V, BKG_BOOKING BKG 
                                                                  WHERE V.BKG_NO = BK.BKG_NO  
                                                                                                  AND BKG.BKG_NO = BK.BKG_NO
                                                                  AND ((V.POL_CD LIKE 'CN%' AND V.POD_CD LIKE 'CN%') OR V.POD_CD = 'CNHKG')
                                                                 )
                                           
                                                AND ROWNUM = 1) CN_DIR_FLG

,      A.FLEX_HGT_FLG
,      A.CRE_USR_ID
,      A.UPD_USR_ID
FROM   BKG_BOOKING A
,      (SELECT *
        FROM (SELECT BKG_NO
,              BKG_DOC_PROC_TP_CD
,              DOC_PROC_SEQ
,              EVNT_USR_ID
,              EVNT_DT
,              UPD_DT
,              row_number() over(partition by BKG_NO order by upd_dt desc, rownum desc, rowid desc) row_no
        FROM   BKG_DOC_PROC_SKD
        WHERE  BKG_NO = @[bkg_no]
        AND    BKG_DOC_PROC_TP_CD IN('CNTCFM', 'CNTRLS')
        AND    DOC_PERF_DELT_FLG = 'N') 
        WHERE  row_no = 1) B
,      BKG_BL_ISS C
WHERE  A.BKG_NO = B.BKG_NO(+)
AND    A.BKG_NO = C.BKG_NO(+)
AND    A.BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="KORZC055229" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
