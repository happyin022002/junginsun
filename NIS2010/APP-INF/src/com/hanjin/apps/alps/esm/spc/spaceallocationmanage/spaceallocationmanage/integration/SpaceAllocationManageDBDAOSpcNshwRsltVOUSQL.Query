<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOSpcNshwRsltVOUSQL">
			<desc><![CDATA[2015.12.14 이혜민 [CHM-201539247] No-Show Summary 신규 컬럼 개발 요청(Fcast & Alloc)]]></desc>
			<sql><![CDATA[
UPDATE SPC_NSHW_RSLT
   SET FCAST_LOD_QTY = @[fcast_lod_qty],
       ALOC_LOD_QTY  = @[aloc_lod_qty] ,
       MIN_LOD_QTY   = LEAST(NVL(@[fcast_lod_qty], 0), NVL(@[aloc_lod_qty], 0)),
       UPD_USR_ID    = @[upd_usr_id]   ,
       UPD_DT        = SYSDATE
 WHERE ALOC_DDCT_BSE_CD = @[aloc_ddct_bse_cd]
   AND OFC_KND_CD       = @[ofc_knd_cd]
   AND RLANE_CD         = @[rlane_cd]
   AND DIR_CD           = @[dir_cd]
   AND VSL_CD           = @[vsl_cd]
   AND SKD_VOY_NO       = @[skd_voy_no]
   AND SKD_DIR_CD       = @[skd_dir_cd]
   AND SLS_OFC_CD       = @[sls_ofc_cd]
   AND POL_YD_CD        = @[pol_yd_cd]
   AND TRD_CD			= @[trade]			]]></sql>
			<params>
				<param name="fcast_lod_qty" type="12" value="" out="N"/>
				<param name="aloc_lod_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="aloc_ddct_bse_cd" type="12" value="" out="N"/>
				<param name="ofc_knd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="sls_ofc_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
