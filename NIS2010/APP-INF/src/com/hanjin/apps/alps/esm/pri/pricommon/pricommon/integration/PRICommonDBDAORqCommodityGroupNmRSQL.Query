<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORqCommodityGroupNmRSQL">
			<desc><![CDATA[RqCommodityGroupNm]]></desc>
			<sql><![CDATA[
SELECT PRC_GRP_CMDT_DESC AS NM
  FROM PRI_RQ_GRP_CMDT
 WHERE QTTN_NO = @[etc1]
   AND QTTN_VER_NO = @[etc2]
   AND PRC_GRP_CMDT_CD = @[cd]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
