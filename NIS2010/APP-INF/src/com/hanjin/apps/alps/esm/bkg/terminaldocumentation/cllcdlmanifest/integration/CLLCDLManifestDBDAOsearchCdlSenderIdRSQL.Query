<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCdlSenderIdRSQL">
			<desc><![CDATA[searchCdlSenderId]]></desc>
			<sql><![CDATA[
SELECT SNDR_TRD_PRNR_ID EDI_SND_ID
  FROM BKG_EDI_TRD_PRNR_SUB_LNK EY,
       BKG_EDI_SUB_LNK_MSG MSG
   WHERE EY.TRD_PRNR_SUB_LNK_SEQ = MSG.TRD_PRNR_SUB_LNK_SEQ
     AND EDI_MSG_TP_ID = 'COPRAR'
     AND EDI_MSG_IND_CD = '8'
     AND MSG_TP_DESC = '1'
     AND PRNR_SUB_LNK_CD = @[in_yd_cd]
     AND RCVR_TRD_PRNR_ID = @[in_rcv_id]			]]></sql>
			<params>
				<param name="in_yd_cd" type="12" value="" out="N"/>
				<param name="in_rcv_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
