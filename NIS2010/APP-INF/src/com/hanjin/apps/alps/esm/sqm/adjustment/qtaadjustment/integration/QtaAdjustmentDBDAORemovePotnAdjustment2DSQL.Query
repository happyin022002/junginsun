<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAORemovePotnAdjustment2DSQL">
			<desc><![CDATA[QTA adjustment by VVD for IAS Sector에서 VVD 데이터 삭제할 때 20162Q까지만 작동하는 로직

2016.04.22 CHM-201641278 [SQM] IAS Trade 판매목표 수립 Portion 연결 시스템 수정 요청 CSR]]></desc>
			<sql><![CDATA[
DELETE
  FROM SQM_CFM_QTA X
 WHERE 1=1
   AND (@[f_bse_yr]||@[f_bse_qtr_cd] IN ('20161Q', '20162Q') OR @[f_bse_yr] < '2016') --2016 2Q까지만 작동하게 함. 2016 3Q부터 IAS는 OVERALL, SECTOR 이원화하여 관리
#if (${f_gubun} == 'HO' || ${f_gubun} == 'RHQ')
   AND X.SQM_CNG_TP_CD   = 'I'
#end
   AND (X.QTA_RLSE_VER_NO,X.BSE_TP_CD,X.BSE_YR,X.BSE_QTR_CD,X.OFC_VW_CD,X.QTA_TGT_CD,X.TRD_CD,X.RLANE_CD,X.DIR_CD,X.VSL_CD,X.SKD_VOY_NO,X.SKD_DIR_CD,X.RHQ_CD)
    IN (
            SELECT A1.QTA_RLSE_VER_NO
                  ,A1.BSE_TP_CD
                  ,A1.BSE_YR
                  ,A1.BSE_QTR_CD
                  ,A1.OFC_VW_CD
                  ,A1.QTA_TGT_CD
                  ,A1.TRD_CD
                  ,A1.RLANE_CD
                  ,A1.DIR_CD
                  ,A1.VSL_CD
                  ,A1.SKD_VOY_NO
                  ,A1.SKD_DIR_CD
                  ,A1.RHQ_CD
              FROM SQM_CFM_QTA A1
                  ,SQM_DAT_RLT A2
                  ,MAS_LANE_RGST A3
             WHERE 1=1
               AND A1.BSE_TP_CD       = A2.BSE_TP_CD
               AND A1.BSE_YR          = A2.BSE_YR
               AND A1.BSE_QTR_CD      = A2.BSE_QTR_CD
               AND A1.OFC_VW_CD       = A2.OFC_VW_CD
               AND DECODE(A1.BSE_YR||A1.BSE_QTR_CD, '20141Q', DECODE(A1.RGN_OFC_CD, 'SELSC', 'SHARC', 'TYOSC', 'SHARC', A1.RHQ_CD), A1.RHQ_CD) = A2.RHQ_CD
               AND A1.TRD_CD          = A2.TRD_CD
               AND A1.CONV_DIR_CD     = A2.CONV_DIR_CD
#if (${f_gubun} == 'HO' || ${f_gubun} == 'RHQ')
               AND A1.SQM_CNG_TP_CD   = 'I'
#end
               AND A1.TRD_CD          = A3.TRD_CD
               AND A1.RLANE_CD        = A3.RLANE_CD
               AND A1.DIR_CD          = A3.DIR_CD
               AND A3.DELT_FLG        = 'N'

               AND A1.QTA_RLSE_VER_NO like '%02'
               AND A1.BSE_TP_CD       = @[f_bse_tp_cd]
               AND A1.BSE_YR          = @[f_bse_yr]
               AND A1.BSE_QTR_CD      = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
#if (${f_gubun} == 'HO')
               AND A2.TEAM_CD         = (SELECT CASE WHEN @[ofc_cd] IN (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255') 
                                                            THEN A2.TEAM_CD
                                                       ELSE @[ofc_cd]
                                              END TEAM_CD
                                         FROM DUAL)
#elseif (${f_gubun} == 'RHQ')
               AND A2.RHQ_CD          = (SELECT CASE WHEN @[ofc_cd] IN (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255') 
                                                            THEN A2.RHQ_CD
                                                       ELSE @[ofc_cd]
                                              END RHQ_CD
                                         FROM DUAL)
#end
#if (${f_ofc_vw_cd} != '' && ${f_ofc_vw_cd} != 'All')
               AND A1.OFC_VW_CD       = @[f_ofc_vw_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
               AND A1.TRD_CD          = @[f_trd_cd]
#end
#if (${rlane_cd} != '' && ${rlane_cd} != 'All')
               AND A1.RLANE_CD        = @[rlane_cd] -- Adjustment by VVD
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
               AND A1.RLANE_CD        IN (${f_rlane_cd})  -- Portion Adjustment by HO/RHQ
#end
#if (${f_dir_cd} != '' && ${f_dir_cd} != 'All')
               AND A1.DIR_CD          = DECODE(@[adj_vvd], 'Y', @[dir_cd], @[f_dir_cd]) -- QTA Adjustment by VVD for IAS Sector에서 Adjusting VVD select시 체크시에는 검색조건의 dir_cd이 아닌 row의 dir_cd를 가져오도록 함
#end
#if (${f_click} != 'on' && ${f_conv_dir_cd} != '' && ${f_conv_dir_cd} != 'All')
               AND A1.CONV_DIR_CD     = @[f_conv_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
               AND A3.HUL_BND_CD      = @[f_trd_dir]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
               AND A2.OB_DIV_CD       = @[f_ob_div_cd]
#end
#if (${vvd} != '')
               AND A1.VSL_CD || A1.SKD_VOY_NO || A1.SKD_DIR_CD = @[vvd]
#end
           )			]]></sql>
			<params>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="adj_vvd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_conv_dir_cd" type="12" value="" out="N"/>
				<param name="f_trd_dir" type="12" value="" out="N"/>
				<param name="f_ob_div_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
