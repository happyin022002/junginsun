<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentDBDAOAddKpiCreationEditNewOfcCSQL">
			<desc><![CDATA[AddKpiCreationEditNewOfc]]></desc>
			<sql><![CDATA[
INSERT INTO SQM_SPCL_NEW_OFC (
         BSE_TP_CD
        ,BSE_YR
        ,BSE_QTR_CD
        ,SPCL_TGT_CD
        ,TRD_CD
        ,RLANE_CD
        ,DIR_CD
        ,RGN_OFC_CD
        ,RHQ_CD
        ,CONV_DIR_CD
        ,SRC_RGN_OFC_CD
        ,SRC_RHQ_CD
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
        )
VALUES (
         @[bse_tp_cd]
        ,@[bse_yr]
        ,DECODE(@[bse_tp_cd], 'Q', @[bse_qtr_cd], '00')
        ,@[spcl_tgt_cd]
        ,@[trd_cd]
        ,@[rlane_cd]
        ,NVL((SELECT DIR_CD 
                FROM SQM_DIR_CONV 
               WHERE 1=1
                 AND BSE_TP_CD   = @[bse_tp_cd]
                 AND BSE_YR      = @[bse_yr]
                 AND BSE_QTR_CD  = DECODE(@[bse_tp_cd], 'Q', @[bse_qtr_cd], '00')
                 AND TRD_CD      = @[trd_cd]
                 AND RLANE_CD    = @[rlane_cd]
                 AND CONV_DIR_CD = @[conv_dir_cd]
              ), @[conv_dir_cd])
        ,@[rgn_ofc_cd]
        ,@[rhq_cd]
        ,@[conv_dir_cd]
        ,@[src_rgn_ofc_cd]
        ,@[src_rhq_cd]
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[upd_usr_id]
        ,SYSDATE
        )			]]></sql>
			<params>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="spcl_tgt_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="conv_dir_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="src_rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="src_rhq_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
