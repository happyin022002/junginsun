<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAORemoveBkgClzTmDSQL">
			<desc><![CDATA[RemoveBkgClzTm]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CLZ_TM
WHERE	BKG_NO = @[bkg_no]
#if (${mode_cd}== 'R')
AND	CLZ_TP_CD in ('F','O')
#elseif (${mode_cd}== 'L')
AND	CLZ_TP_CD in ('R', 'T')
#elseif (${mode_cd}== 'A')
AND	CLZ_TP_CD in ('F', 'O', 'R', 'T')
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
