<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOmodifyBkgKrWhfBrthUSQL">
			<desc><![CDATA[U]]></desc>
			<sql><![CDATA[
UPDATE BKG_KR_WHF_BRTH
SET
BRTH_CD    = @[brth_cd],
BRTH_KR_NM = @[brth_kr_nm],
UPD_USR_ID = @[upd_usr_id],
UPD_DT     =  SYSDATE
WHERE PORT_CD = @[port_cd]
AND BRTH_CD   = @[brth_cd]			]]></sql>
			<params>
				<param name="brth_cd" type="12" value="" out="N"/>
				<param name="brth_kr_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
