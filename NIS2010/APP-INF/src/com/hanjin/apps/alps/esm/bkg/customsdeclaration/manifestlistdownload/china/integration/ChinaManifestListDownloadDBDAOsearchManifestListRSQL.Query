<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOsearchManifestListRSQL">
			<desc><![CDATA[ChinaManifestListDetailVO]]></desc>
			<sql><![CDATA[
WITH EDI_INFO AS (
    SELECT  * FROM (
        SELECT  DISTINCT
                DENSE_RANK() OVER(PARTITION BY BL.BL_NO ORDER BY SLOG.MF_SND_DT DESC) AS RNUM, 
                B.BKG_NO,
                SLOG.MF_SND_DT,
                SLOG.EDI_REF_ID
        FROM    BKG_CSTMS_CHN_SND_LOG_BL BL,
                BKG_CSTMS_CHN_SND_LOG SLOG,
                BKG_BOOKING B
        WHERE   SLOG.CHN_MF_SND_IND_CD  = @[trans_mode]
        AND     SLOG.VSL_CD 	 = SUBSTR(@[vvd],1,4)
        AND     SLOG.SKD_VOY_NO  = SUBSTR(@[vvd],5,4)
        AND     SLOG.SKD_DIR_CD  = SUBSTR(@[vvd],9,1)
        AND     BL.EDI_REF_ID    = SLOG.EDI_REF_ID
        AND     BL.BL_NO 	 	 = B.BL_NO
        AND     NVL(B.CRR_SOC_FLG, 'N' )='N'
    )
    WHERE   RNUM = 1
)
SELECT  INFO.BKG_NO,
        INFO.BL_NO,
        INFO.POL_CD,
        INFO.POD_CD,
        INFO.DEL_CD,
        INFO.PCK_QTY,
        INFO.PCK_TP_CD,
        INFO.ACT_WGT,
        INFO.WGT_UT_CD,
        INFO.SHPR_NM,
        INFO.SHPR_ADDR,
		INFO.SHPR_CNT,
		INFO.SHPR_ST_PO,
		INFO.SHPR_RGST_NO,
		INFO.SHPR_FAX,
		INFO.SHPR_EML,
		INFO.SHPR_PHN,
        INFO.CNEE_NM,
        INFO.CNEE_ADDR,
		INFO.CNEE_CNT,
		INFO.CNEE_ST_PO,
		INFO.CNEE_RGST_NO,
		INFO.CNEE_FAX,
		INFO.CNEE_EML,
		INFO.CNEE_PHN,
        INFO.NTFY_NM,
        INFO.NTFY_ADDR,
		INFO.NTFY_CNT,
		INFO.NTFY_ST_PO,
		INFO.NTFY_RGST_NO,
		INFO.NTFY_FAX,
		INFO.NTFY_EML,
		INFO.NTFY_PHN,
        INFO.BKG_CGO_TP_CD,
        INFO.TR,
        INFO.DCGO_FLG, 
        INFO.RC_FLG,
        INFO.DL_FLG,
		INFO.MF_DL_DT,
        INFO.CNTR_CNT,
        --SLOG.TRSM_MSG_TP_ID,
        DECODE(SLOG.TRSM_MSG_TP_ID,'9','Original','0','Original','5','Change','3','Delete') TRSM_MSG_TP_ID,
        TO_CHAR(SLOG.MF_SND_DT, 'YYYYMMDDHH24MISS') MF_SND_DT,
		MIN(DECODE(TRIM(SEAL.CNTR_SEAL_NO),NULL,'N','Y')) SEAL_NO_FLG,
		MIN(DECODE(TRIM(SEAL.SEAL_KND_CD), NULL, 'N', 'Y')) SEAL_KND_FLG,
		MIN(DECODE(TRIM(SEAL.SEAL_PTY_TP_CD),NULL,'N','Y')) SEALER_CD_FLG,		
        --SUM(DECODE(CNTR.CNTR_NO,NULL,0,1)) CNTR_CNT,
		-- VSL Info
		'' AS CALL_SGN_NO,
		'' AS PRE_PORT,
		'' AS NXT_PORT,
		'' AS VPS_ETA_DT,
		'' AS VPS_ETD_DT,
		'' AS VPS_ETB_DT,
		'' AS VSL_ENG_NM,
		-- 조회 조건
		@[vvd] 			 AS VVD,
		@[bkg_cgo_tp_cd] AS BKG_CGO_TP_CD,
		@[loc_cd] 		 AS LOC_CD,
		@[trans_mode] 	 AS TRANS_MODE,
		-- 총 개수
		'' AS TOTAL,
       MIN( CASE WHEN DECODE(SUBSTR(CNTR.CNTR_TPSZ_CD, 1, 1), 'T', 1, 'F', 1, 'A', 1, 0) = 0        --일반CNTR TYPE인 경우
                 THEN 
                      (CASE WHEN (DECODE(TRIM(SEAL.CNTR_SEAL_NO), NULL, 'N', 'Y')
                                ||DECODE(TRIM(SEAL.SEAL_PTY_TP_CD), NULL, 'N', 'Y')
                                ||DECODE(TRIM(SEAL.SEAL_KND_CD), NULL, 'N', 'Y')) = 'YYY'
                            THEN 'Y'
                            ELSE 'N'
                       END)
                 ELSE                                                                               --(T,F,A) CNTR TYPE인 경우 
                      'Y'
            END             
       ) AS DL_CHK_FLG
  FROM (
            SELECT  B.BKG_NO BKG_NO,
					(select count(cntr_no) from bkg_container cc where b.bkg_no = cc.bkg_no group by cc.bkg_no) CNTR_CNT,
                    B.BL_NO BL_NO,
                    B.POL_CD POL_CD,
                    B.POD_CD POD_CD,
                    B.DEL_CD DEL_CD,
                    DECODE(NVL(BL.PCK_QTY,0),0,'N','Y') PCK_QTY,
                    DECODE(BL.PCK_TP_CD,NULL,'N','Y') PCK_TP_CD,
                    DECODE(NVL(BL.ACT_WGT,0),0,'N','Y') ACT_WGT,
                    DECODE(BL.WGT_UT_CD,NULL,'N','Y') WGT_UT_CD,
                    DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.CUST_NM)), 0, 'N',NULL,'N', 'Y')) SHPR_NM,
                    DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.CUST_ADDR)), 0, 'N',NULL,'N', 'Y')) SHPR_ADDR,
					DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.CSTMS_DECL_CNT_CD)),    0, DECODE(LENGTH(TRIM(S.CUST_CNT_CD)),    0, 'N'
																																  , NULL, 'N'
																													      				, 'Y'
																						 		      )
																						, NULL, DECODE(LENGTH(TRIM(S.CUST_CNT_CD)),    0, 'N'
																																  , NULL, 'N'
																														  				, 'Y'
																								 	  )
  																						, 'Y'
													  )
						  ) SHPR_CNT,
					DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.EUR_CSTMS_ST_NM)), 0, 'N',NULL, 'N', 'Y')) SHPR_ST_PO,
                    DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.EORI_NO)), 0, 'N',NULL,'N', 'Y')) SHPR_RGST_NO,
					DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.CUST_FAX_NO)),   0, CASE WHEN LENGTH(TRIM(S.CUST_EML)) <> 0
													   									  	    OR LENGTH(TRIM(S.CUST_PHN_NO)) <> 0
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																				  ,NULL, CASE WHEN LENGTH(TRIM(S.CUST_EML)) <> 0
													   									  	    OR LENGTH(TRIM(S.CUST_PHN_NO)) <> 0
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																					   , 'Y'
													  )
						  ) SHPR_FAX, 
					DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.CUST_EML)),   0, CASE WHEN LENGTH(TRIM(S.CUST_FAX_NO)) <> 0
													   									     OR LENGTH(TRIM(S.CUST_PHN_NO)) <> 0
                                                     								       THEN '-'
																						   ELSE 'N'
																					   END
																			   ,NULL, CASE WHEN LENGTH(TRIM(S.CUST_FAX_NO)) <> 0
													   									  	 OR LENGTH(TRIM(S.CUST_PHN_NO)) <> 0
                                                     								       THEN '-'
																						   ELSE 'N'
																					   END
																					, 'Y'
													  )
						  ) SHPR_EML,
					DECODE(S.BKG_CUST_TP_CD,'S',DECODE(LENGTH(TRIM(S.CUST_PHN_NO)),   0, CASE WHEN LENGTH(TRIM(S.CUST_FAX_NO)) <> 0
													   									        OR LENGTH(TRIM(S.CUST_EML)) <> 0
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																			      ,NULL, CASE WHEN LENGTH(TRIM(S.CUST_FAX_NO)) <> 0
													   									  	    OR LENGTH(TRIM(S.CUST_EML)) <> 0
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																					   , 'Y'
													  )
						  ) SHPR_PHN,
                    DECODE(C.BKG_CUST_TP_CD,'C',DECODE(LENGTH(TRIM(C.CUST_NM)), 0, 'N',NULL,'N', 'Y')) CNEE_NM,
                    DECODE(C.BKG_CUST_TP_CD,'C',DECODE(B.CUST_TO_ORD_FLG,'Y','Y',DECODE(LENGTH(TRIM(C.CUST_ADDR)), 0, 'N',NULL,'N', 'Y'))) CNEE_ADDR,
					DECODE(C.BKG_CUST_TP_CD,'C',DECODE(LENGTH(TRIM(C.CSTMS_DECL_CNT_CD)),    0, DECODE(LENGTH(TRIM(C.CUST_CNT_CD)),    0, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																																									  ,'N'
																																				)
																																  , NULL, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																																									  ,'N'
																																				)
																													  			  		, 'Y'
																				  					  )
																	      				, NULL, DECODE(LENGTH(TRIM(C.CUST_CNT_CD)),    0, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																																									  ,'N'
																																				)
																																  , NULL, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																																									  ,'N'
																																				)
																																	   	, 'Y'
																									  )
																				  					  
																						, 'Y'
													  )
						  ) CNEE_CNT,
					DECODE(C.BKG_CUST_TP_CD,'C',DECODE(LENGTH(TRIM(C.EUR_CSTMS_ST_NM)),    0, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																												          ,'N'
																							        )
																				      , NULL, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																												          ,'N'
																							        )
																				            , 'Y'
													  )
						  ) CNEE_ST_PO,
					DECODE(C.BKG_CUST_TP_CD,'C',DECODE(LENGTH(TRIM(C.EORI_NO)),    0, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																										          ,'N'
																							)
																			  , NULL, DECODE(B.CUST_TO_ORD_FLG,'Y','-'
																										          ,'N'
																							)
																			        , 'Y'
													  )
						  ) CNEE_RGST_NO,
                    DECODE(C.BKG_CUST_TP_CD,'C',DECODE(LENGTH(TRIM(C.CUST_FAX_NO)),   0, CASE WHEN LENGTH(TRIM(C.CUST_EML)) <> 0
													   									  	    OR LENGTH(TRIM(C.CUST_PHN_NO)) <> 0
																							    OR B.CUST_TO_ORD_FLG = 'Y'
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																				  ,NULL, CASE WHEN LENGTH(TRIM(C.CUST_EML)) <> 0
													   									  	    OR LENGTH(TRIM(C.CUST_PHN_NO)) <> 0
																							    OR B.CUST_TO_ORD_FLG = 'Y'
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																					   , 'Y'
													  )
						  ) CNEE_FAX,		
                    DECODE(C.BKG_CUST_TP_CD,'C',DECODE(LENGTH(TRIM(C.CUST_EML)),   0, CASE WHEN LENGTH(TRIM(C.CUST_FAX_NO)) <> 0
													   									  	 OR LENGTH(TRIM(C.CUST_PHN_NO)) <> 0
																							 OR B.CUST_TO_ORD_FLG = 'Y'
                                                     								       THEN '-'
																						   ELSE 'N'
																					   END
																			   ,NULL, CASE WHEN LENGTH(TRIM(C.CUST_FAX_NO)) <> 0
													   									  	 OR LENGTH(TRIM(C.CUST_PHN_NO)) <> 0
																							 OR B.CUST_TO_ORD_FLG = 'Y'
                                                     								       THEN '-'
																						   ELSE 'N'
																					   END
																					 , 'Y'
													  )
						  ) CNEE_EML,
                    DECODE(C.BKG_CUST_TP_CD,'C',DECODE(LENGTH(TRIM(C.CUST_PHN_NO)),   0, CASE WHEN LENGTH(TRIM(C.CUST_FAX_NO)) <> 0
													   									        OR LENGTH(TRIM(C.CUST_EML)) <> 0
																							    OR B.CUST_TO_ORD_FLG = 'Y'
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																			      ,NULL, CASE WHEN LENGTH(TRIM(C.CUST_FAX_NO)) <> 0
													   									  	    OR LENGTH(TRIM(C.CUST_EML)) <> 0
																							    OR B.CUST_TO_ORD_FLG = 'Y'
                                                     								          THEN '-'
																						      ELSE 'N'
																					      END
																					   , 'Y'
													  )
						  ) CNEE_PHN,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(LENGTH(TRIM(N.CUST_NM)), 0, 'N',NULL,'N', 'Y')) NTFY_NM,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(B.SAM_CNEE_NTFY_FLG,'Y','Y',DECODE(LENGTH(TRIM(N.CUST_ADDR)), 0, 'N',NULL,'N', 'Y'))) NTFY_ADDR,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(LENGTH(TRIM(N.CSTMS_DECL_CNT_CD)), 	 0,DECODE(LENGTH(TRIM(N.CUST_CNT_CD)),    0, DECODE(B.CUST_TO_ORD_FLG,'N','-'
																																									 ,'N'
																																			   )
																																 , NULL, DECODE(B.CUST_TO_ORD_FLG,'N','-'
																																									 ,'N'
																																			   )
				 																													   , 'Y'
																				  					 )
																						, NULL,DECODE(LENGTH(TRIM(N.CUST_CNT_CD)),    0, DECODE(B.CUST_TO_ORD_FLG,'N','-'
 																																									 ,'N'
																																			   )
																																 , NULL, DECODE(B.CUST_TO_ORD_FLG,'N','-'
 																																									 ,'N'
																																			   )
																																	   , 'Y'
																				  					 )
																									 
																						, 'Y'
													  )
						  ) NTFY_CNT,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(LENGTH(TRIM(N.EUR_CSTMS_ST_NM)),   0, DECODE(B.CUST_TO_ORD_FLG,'N','-'
																													     ,'N'
																							       )
																				      ,NULL, DECODE(B.CUST_TO_ORD_FLG,'N','-'
																													     ,'N'
																							       )
																				           , 'Y'
													  )
						  ) NTFY_ST_PO,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(LENGTH(TRIM(N.EORI_NO)),   0, DECODE(B.CUST_TO_ORD_FLG,'N','-'
																												 ,'N'
																							       )
																			  ,NULL, DECODE(B.CUST_TO_ORD_FLG,'N','-'
																												 ,'N'
																							       )
																				   , 'Y'
													  )
						  ) NTFY_RGST_NO,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(LENGTH(TRIM(N.CUST_FAX_NO)),    0, CASE WHEN LENGTH(TRIM(N.CUST_EML)) <> 0
													   									         OR LENGTH(TRIM(N.CUST_PHN_NO)) <> 0
																							     OR B.CUST_TO_ORD_FLG = 'N'
                                                     								           THEN '-'
																						       ELSE 'N'
																					       END
																				  , NULL, CASE WHEN LENGTH(TRIM(N.CUST_EML)) <> 0
													   									         OR LENGTH(TRIM(N.CUST_PHN_NO)) <> 0
																							     OR B.CUST_TO_ORD_FLG = 'N'
                                                     								           THEN '-'
																						       ELSE 'N'
																					       END
																				        , 'Y'
													  )
						  ) NTFY_FAX,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(LENGTH(TRIM(N.CUST_EML)),    0, CASE WHEN LENGTH(TRIM(N.CUST_FAX_NO)) <> 0
													   									      OR LENGTH(TRIM(N.CUST_PHN_NO)) <> 0
																							  OR B.CUST_TO_ORD_FLG = 'N'
                                                     								        THEN '-'
																						    ELSE 'N'
																					    END
																			   , NULL, CASE WHEN LENGTH(TRIM(N.CUST_FAX_NO)) <> 0
													   									      OR LENGTH(TRIM(N.CUST_PHN_NO)) <> 0
																							  OR B.CUST_TO_ORD_FLG = 'N'
                                                     								        THEN '-'
																						    ELSE 'N'
																					    END
																				     , 'Y'
													  )
						  ) NTFY_EML,
                    DECODE(N.BKG_CUST_TP_CD,'N',DECODE(LENGTH(TRIM(N.CUST_PHN_NO)),    0, CASE WHEN LENGTH(TRIM(N.CUST_FAX_NO)) <> 0
													   									         OR LENGTH(TRIM(N.CUST_EML)) <> 0
																							     OR B.CUST_TO_ORD_FLG = 'N'
                                                     								           THEN '-'
																						       ELSE 'N'
																					       END
																			      , NULL, CASE WHEN LENGTH(TRIM(N.CUST_FAX_NO)) <> 0
													   									         OR LENGTH(TRIM(N.CUST_EML)) <> 0
																							     OR B.CUST_TO_ORD_FLG = 'N'
                                                     								           THEN '-'
																						       ELSE 'N'
																					       END
																				        , 'Y'
													  )
						  ) NTFY_PHN,
                    B.BKG_CGO_TP_CD,
                    DECODE(@[trans_mode], 'D', DECODE(B.POD_CD, @[loc_cd], 'I', 'T'), DECODE(B.POL_CD, @[loc_cd], 'E', 'R')) TR,
                    DECODE(B.DCGO_FLG,'N','N','Y') DCGO_FLG, 
                    DECODE(B.RC_FLG,'N','N','Y') RC_FLG,
                    CASE WHEN ( SELECT  COUNT(*) CNT                        
                                FROM    BKG_CSTMS_CHN_BL                     
                                WHERE   CHN_MF_SND_IND_CD = @[trans_mode]     
                                AND     VSL_CD 	   = SUBSTR(@[vvd],1,4)            
                                AND     SKD_VOY_NO = SUBSTR(@[vvd],5,4)         
                                AND     SKD_DIR_CD = SUBSTR(@[vvd],9,1)         
                                AND     BKG_POL_CD = BV.POL_CD                    
                                AND     BKG_POD_CD = BV.POD_CD                    
                                AND     BL_NO = B.BL_NO ) > 0 THEN 'Y'        
                         ELSE 'N' 
                    END DL_FLG,
					( SELECT  MF_DL_DT
					FROM    BKG_CSTMS_CHN_DL_HIS
					WHERE   CHN_MF_SND_IND_CD = @[trans_mode]
					AND     VSL_CD 	   = SUBSTR(@[vvd],1,4)
					AND     SKD_VOY_NO = SUBSTR(@[vvd],5,4)
					AND     SKD_DIR_CD = SUBSTR(@[vvd],9,1)
					AND     BL_NO = B.BL_NO 
					AND     DELT_FLG = 'N') MF_DL_DT,
                    EDI_INFO.EDI_REF_ID
                    --(SELECT  MAX(SLOG.EDI_REF_ID)                        
                    -- FROM    BKG_CSTMS_CHN_SND_LOG_BL BL,               
                    --         BKG_CSTMS_CHN_SND_LOG SLOG			
                    -- WHERE   BL.EDI_REF_ID = SLOG.EDI_REF_ID		
                    -- AND     SLOG.CHN_MF_SND_IND_CD = [trans_mode]		
                    -- AND     SLOG.VSL_CD 	 = SUBSTR([vvd],1,4)		
                    -- AND     SLOG.SKD_VOY_NO = SUBSTR([vvd],5,4)		
                    -- AND     SLOG.SKD_DIR_CD = SUBSTR([vvd],9,1)		
                    -- AND     SLOG.BKG_POL_CD = BV.POL_CD		
                    -- AND     BL.BL_NO = B.BL_NO	
					--) EDI_REF_ID                        
            FROM    BKG_VVD BV, 
                    BKG_BOOKING B, 
                    BKG_BL_DOC BL,
                    BKG_CUSTOMER S, 
                    BKG_CUSTOMER C, 
                    BKG_CUSTOMER N,
                	EDI_INFO
            WHERE   BV.VSL_CD           =    SUBSTR(@[vvd],1,4)
            AND     BV.SKD_VOY_NO       =    SUBSTR(@[vvd],5,4)
            AND     BV.SKD_DIR_CD       =    SUBSTR(@[vvd],9,1)
            AND     BV.BKG_NO           =    B.BKG_NO
            AND     BV.BKG_NO           =    BL.BKG_NO
            AND     B.BKG_STS_CD        NOT IN    ('X','S')
            AND     DECODE(B.BKG_CGO_TP_CD,'P','P','F') LIKE @[bkg_cgo_tp_cd]||'%'
            AND     DECODE(@[trans_mode],'D',BV.POD_CD,'O',BV.POL_CD, BV.POL_CD) = @[loc_cd]
            AND     BV.POD_CD           LIKE    DECODE(@[trans_mode],'D','%','O','CN%','%')
            AND     BV.POD_CD           <>      DECODE(@[trans_mode],'P',' ','CNHKG')/*24HR ?????? ???????? PRE-STOWAGE?? ????*/
            AND     B.BKG_NO            =    S.BKG_NO(+)
            AND     B.BKG_NO            =    C.BKG_NO(+)
            AND     B.BKG_NO            =    N.BKG_NO(+)
            AND     S.BKG_CUST_TP_CD(+) =    'S'
            AND     C.BKG_CUST_TP_CD(+) =    'C'
            AND     N.BKG_CUST_TP_CD(+) =    'N' 
        	AND     B.BKG_NO            =    EDI_INFO.BKG_NO(+)
            AND     NVL(B.CRR_SOC_FLG, 'N' )='N'
       ) INFO,
         BKG_CONTAINER CNTR,
         BKG_CNTR_SEAL_NO SEAL,
         BKG_CSTMS_CHN_SND_LOG SLOG
   WHERE INFO.BKG_NO          = CNTR.BKG_NO(+)
     AND CNTR.BKG_NO          = SEAL.BKG_NO(+)
     AND CNTR.CNTR_NO         = SEAL.CNTR_NO(+)
     AND INFO.EDI_REF_ID      = SLOG.EDI_REF_ID(+)
#if (${trans_type} == 'local') 
	 AND INFO.TR IN ('E','I')
#elseif (${trans_type} == 'ts') 
	 AND INFO.TR IN ('R','T')
#end
#if (${err_div} == 'err')
	 AND (INFO.PCK_QTY = 'N' OR INFO.PCK_TP_CD = 'N' OR INFO.ACT_WGT = 'N' OR 
		  INFO.WGT_UT_CD = 'N' OR SEAL.CNTR_SEAL_NO IS NULL OR SEAL.SEAL_KND_CD IS NULL OR
		  SEAL.SEAL_PTY_TP_CD IS NULL OR INFO.SHPR_NM = 'N' OR INFO.SHPR_ADDR = 'N' OR
		  INFO.SHPR_CNT = 'N' OR INFO.SHPR_ST_PO = 'N' OR INFO.SHPR_RGST_NO = 'N' OR
		  INFO.SHPR_FAX = 'N' OR INFO.SHPR_EML = 'N' OR INFO.SHPR_PHN = 'N' OR 
		  INFO.CNEE_NM = 'N' OR INFO.CNEE_ADDR = 'N' OR INFO.CNEE_CNT = 'N' OR
		  INFO.CNEE_ST_PO = 'N' OR INFO.CNEE_RGST_NO = 'N' OR INFO.CNEE_FAX = 'N' OR
		  INFO.CNEE_EML = 'N' OR INFO.CNEE_PHN = 'N' OR INFO.NTFY_NM = 'N' OR
          INFO.NTFY_ADDR = 'N' OR INFO.NTFY_CNT = 'N' OR INFO.NTFY_ST_PO = 'N' OR
		  INFO.NTFY_RGST_NO = 'N' OR INFO.NTFY_FAX = 'N' OR INFO.NTFY_EML = 'N' OR
		  INFO.NTFY_PHN = 'N' OR INFO.CNTR_CNT = 0)
#end
GROUP BY INFO.BKG_NO, 
         INFO.BL_NO,
         INFO.POL_CD,
         INFO.POD_CD,
