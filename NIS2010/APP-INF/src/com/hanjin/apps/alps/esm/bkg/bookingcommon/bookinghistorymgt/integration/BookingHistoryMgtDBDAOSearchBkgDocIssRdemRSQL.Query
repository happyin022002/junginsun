<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgDocIssRdemRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgDocIssRdemRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
      ,HIS_SEQ
      ,EVNT_OFC_CD
      ,ISS_RDEM_KNT
      ,EVNT_USR_ID
      ,TO_CHAR(EVNT_DT, 'YYYY-MM-DD HH24:MI:SS') AS EVNT_DT
      ,OBL_RDEM_CFM_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
  FROM BKG_DOC_ISS_RDEM
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
