<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOSearchCheckValidAfilRSQL">
			<desc><![CDATA[S/C Main Customer 의 Type 이 Shipper's Association 의 경우
입력된 Actual Customer 가 현재 회차안에서 유효한 Affiliate 인지 체크 한다.]]></desc>
			<sql><![CDATA[
SELECT	SUM ( 1 ) AS CNT
FROM	PRI_SP_AFIL A
WHERE	A.PROP_NO  = @[prop_no]
AND		A.AMDT_SEQ = @[amdt_seq]
AND		A.CUST_CNT_CD = @[etc1]
AND		A.CUST_SEQ    = @[etc2]
AND		A.SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
