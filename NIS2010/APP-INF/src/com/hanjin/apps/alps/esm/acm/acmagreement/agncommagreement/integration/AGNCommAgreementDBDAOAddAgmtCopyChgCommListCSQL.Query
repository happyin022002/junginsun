<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommAgreementDBDAOAddAgmtCopyChgCommListCSQL">
			<desc><![CDATA[Add Agmt Copy Chg Comm List]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_AGN_AGMT_CHG_COMM
      (AGN_CD,
       AGN_AGMT_NO,
       IO_BND_CD,
       AC_TP_CD,
       AGN_AGMT_SEQ,
       AGN_AGMT_CHG_SEQ,
       CHG_CD,
       CRE_USR_ID,
       CRE_DT,
       UPD_USR_ID,
       UPD_DT)

SELECT @[agn_cd] AS AGN_CD,
       @[new_agmt_no] AS AGN_AGMT_NO,
       IO_BND_CD,
       AC_TP_CD,
       AGN_AGMT_SEQ,
       AGN_AGMT_CHG_SEQ,
       CHG_CD,
       @[usr_id] AS CRE_USR_ID,
       SYSDATE AS CRE_DT,
       @[usr_id] AS UPD_USR_ID,
       SYSDATE AS UPD_DT
  FROM ACM_AGN_AGMT_CHG_COMM
 WHERE AGN_AGMT_NO = @[agn_agmt_no]			]]></sql>
			<params>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="new_agmt_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="agn_agmt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
