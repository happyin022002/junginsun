<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOValidatePkgTypeRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT PCK_NM
  FROM MDM_PCK_TP
 WHERE PCK_CD = @[pck_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="pck_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
