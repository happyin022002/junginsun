<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOModifyGroupRatingListUSQL">
			<desc><![CDATA[Group & Multi B/L Rating Self Check저장]]></desc>
			<sql><![CDATA[
UPDATE BKG_RATE
SET GRP_RAT_CHK_FLG = DECODE(@[grp_rat_chk_flg],'1','Y','N'),
    UPD_USR_ID = @[usr_id],
    UPD_DT = SYSDATE
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="grp_rat_chk_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
