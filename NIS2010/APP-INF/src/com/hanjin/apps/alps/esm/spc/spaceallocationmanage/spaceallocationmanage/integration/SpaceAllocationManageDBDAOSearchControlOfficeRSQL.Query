<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOSearchControlOfficeRSQL">
			<desc><![CDATA[Control Option Registration(0052) 에서 Trade, Lane, Bound, Control 값으로 Office List 검색]]></desc>
			<sql><![CDATA[
SELECT RGN_OFC_CD 
FROM SQM_QTA_LANE_OFC
WHERE 1 = 1
	AND BSE_TP_CD = 'Q' --fix
	AND BSE_YR = @[bse_yr] --'2014'
	AND BSE_QTR_CD =@[bse_qtr_cd] --'1Q' -- (1Q:1-3MM / 2Q:4-6MM /3Q:7-9MM/ 4Q:10-12MM)
	AND OFC_VW_CD = 'L' --fix
	AND SQM_ACT_FLG = 'Y' --fix 
	AND TRD_CD = @[trd_cd] --'TPS'
	AND RLANE_CD = @[rlane_cd] --'PNHTP'
	AND DIR_CD = @[dir_cd] --'E'			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
