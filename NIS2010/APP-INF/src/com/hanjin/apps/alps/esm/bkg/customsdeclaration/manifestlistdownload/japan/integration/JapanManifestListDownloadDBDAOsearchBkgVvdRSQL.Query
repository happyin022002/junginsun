<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchBkgVvdRSQL">
			<desc><![CDATA[searchBkgVvd]]></desc>
			<sql><![CDATA[
SELECT 
	VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD_CD,  
	POD_CD
FROM 
	BKG_VVD
WHERE BKG_NO = @[bkg_no]
AND POL_CD = @[pod_cd]				]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
