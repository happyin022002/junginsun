<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommCalculationDBDAOSearchBkgCalculationRSQL">
			<desc><![CDATA[SearchBkgCalculation]]></desc>
			<sql><![CDATA[
SELECT B.BKG_NO,
  B.BL_NO,
  B.BKG_STS_CD,
  TO_CHAR(B.POL_ETD_DT, 'YYYY-MM-DD hh:mi:ss') AS POL_ETD_DT,
  TO_CHAR(I.UPD_DT, 'YYYY-MM-DD hh:mi:ss') AS UPD_DT,
  REV_VVD_CD AS REV_VVD_CD
FROM BKG_BOOKING B ,
  ACM_AGN_BKG_INFO I
WHERE 1=1
       AND B.BKG_NO IN (${bkg_no})
  AND B.BKG_NO = I.BKG_NO (+)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
