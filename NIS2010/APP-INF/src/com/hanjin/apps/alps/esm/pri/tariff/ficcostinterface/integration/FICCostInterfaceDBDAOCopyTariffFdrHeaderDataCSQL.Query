<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOCopyTariffFdrHeaderDataCSQL">
			<desc><![CDATA[CopyTariffFdrHeaderData]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_FDR_HDR
  (SVC_SCP_CD,
   ORG_DEST_TP_CD,
   FDR_TRF_NO,
   RHQ_CD, 
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT)
SELECT @[svc_scp_cd]
     , @[org_dest_tp_cd]
     , @[fdr_trf_no] 
     , HDR.RHQ_CD
     , @[cre_usr_id]
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
 FROM PRI_TRF_FDR_HDR HDR
 WHERE 1 = 1
   AND HDR.SVC_SCP_CD     = @[ori_svc_scp_cd]
   AND HDR.ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND HDR.FDR_TRF_NO     = @[ori_fdr_trf_no]   
   AND HDR.RHQ_CD         = @[rhq_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ori_svc_scp_cd" type="12" value="" out="N"/>
				<param name="ori_fdr_trf_no" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
