<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAYearlyPlanDBDAOInsertBsaBudJntOpBzcCSQL">
			<desc><![CDATA[BSA_BUD_JNT_OP_BZC 데이타를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BSA_BUD_JNT_OP_BZC (
    TRD_CD              ,
    RLANE_CD            ,
    DIR_CD              ,
    VOP_CD              ,
    VSL_CAPA            ,
    BSA_SEQ             ,
    VVD_CD              ,
    BSA_FM_DT           ,
    BSA_TO_DT           ,
    BSA_CAPA            ,
    FNL_HJS_BSA_CAPA    ,
    HJS_BSA_BFR_SUB_CAPA,
    JO_DESC             ,
    OWNR_VSL_WGT        ,
    CRE_USR_ID          ,
    CRE_DT              ,
    UPD_USR_ID          ,
    UPD_DT
)
  SELECT TRD_CD              ,
         RLANE_CD            ,
         DIR_CD              ,
         VOP_CD              ,
         VSL_CAPA            ,
         BSA_SEQ             ,
         VVD_CD              ,
         BSA_FM_DT           ,
         BSA_TO_DT           ,
         BSA_CAPA            ,
         FNL_HJS_BSA_CAPA    ,
         HJS_BSA_BFR_SUB_CAPA,
         JO_DESC             ,
         OWNR_VSL_WGT        ,
         @[cre_usr_id]       ,
         SYSDATE             ,
         @[upd_usr_id]       ,
         SYSDATE
    FROM BSA_JNT_OP_BZC
   WHERE 1=1
     AND BSA_FM_DT <= @[bsa_to_dt]
     AND BSA_TO_DT >= @[bsa_fm_dt]

#if (${trd_cd} != 'All')
     AND TRD_CD     = @[trd_cd]
#end

#if (${rlane_cd} != 'All')
     AND RLANE_CD   = @[rlane_cd]
#end			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bsa_fm_dt" type="12" value="" out="N"/>
				<param name="bsa_to_dt" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
