<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteProposalDBDAORsltCtrtCluzListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
     PM.PROP_NO
,    PM.AMDT_SEQ
,    CM.SVC_SCP_CD
,    CM.GLINE_SEQ
,    CM.CTRT_CLUZ_SEQ
,    CM.NOTE_CLSS_CD
,    CD.CTRT_CLUZ_DTL_SEQ
,    CD.CHG_CD
,    CD.CTRT_CLUZ_CTNT
FROM  PRI_SP_SCP_MN PM
    , PRI_SG_MN GM
    , PRI_SG_CTRT_CLUZ CM
    , PRI_SG_CTRT_CLUZ_DTL CD
WHERE PM.PROP_NO = @[prop_no]
AND   PM.AMDT_SEQ = @[amdt_seq]
AND   PM.SVC_SCP_CD = @[svc_scp_cd]
AND   GM.SVC_SCP_CD = PM.SVC_SCP_CD
AND   GM.CFM_FLG = 'Y'
AND   PM.EFF_DT BETWEEN GM.EFF_DT AND GM.EXP_DT 
AND   CM.SVC_SCP_CD = GM.SVC_SCP_CD
AND   CM.GLINE_SEQ = GM.GLINE_SEQ
AND   CM.NOTE_CLSS_CD = @[note_clss_cd]
AND   CD.SVC_SCP_CD = CM.SVC_SCP_CD
AND   CD.GLINE_SEQ = CM.GLINE_SEQ
AND   CD.CTRT_CLUZ_SEQ = CM.CTRT_CLUZ_SEQ
#if (${chg_cd} != '') 
AND   CD.CHG_CD = @[chg_cd]
#end
ORDER BY CM.SVC_SCP_CD
       , CM.GLINE_SEQ
       , CM.CTRT_CLUZ_SEQ
       , CD.CTRT_CLUZ_DTL_SEQ			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="note_clss_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
