<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOaddResultCntrCSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신, BKG_CSTMS_ADV_CNTR_RSLT 입력.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_CNTR_RSLT
(
     CNT_CD
    ,BL_NO
    ,CSTMS_SEQ
    ,CNTR_NO
    ,RCV_MSG_TP_ID
    ,DSPO_CD
    ,CNTR_QTY
    ,ENTR_TP_NO
    ,ENTR_NO
    ,RCV_DT
    ,CSTMS_CLR_CD
    ,RAIL_REF_NO
    ,CSTMS_BAT_NO
    ,CSTMS_RMK1
    ,CRE_USR_ID
    ,UPD_USR_ID
)
VALUES
(
     'US'
    ,@[bl_no]
    ,@[icr_seq]
    ,@[in_cntr]
    ,@[ir_type]
    ,@[icr_code]
    ,TO_NUMBER(@[icr_qty])
    ,@[icr_et_tp]
    ,@[icr_et_no]
    ,TO_DATE(@[ir_date], 'rrmmddhh24miss')
    ,@[new_cntr_c_flag]
    ,@[in_snp] || @[in_nvobl]
    ,@[ir_batch_no]
    ,@[cntr_hold_remark]
    ,'SYSTEM'
    ,'SYSTEM'
)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="icr_seq" type="12" value="" out="N"/>
				<param name="in_cntr" type="12" value="" out="N"/>
				<param name="ir_type" type="12" value="" out="N"/>
				<param name="icr_code" type="12" value="" out="N"/>
				<param name="icr_qty" type="12" value="" out="N"/>
				<param name="icr_et_tp" type="12" value="" out="N"/>
				<param name="icr_et_no" type="12" value="" out="N"/>
				<param name="ir_date" type="12" value="" out="N"/>
				<param name="new_cntr_c_flag" type="12" value="" out="N"/>
				<param name="in_snp" type="12" value="" out="N"/>
				<param name="in_nvobl" type="12" value="" out="N"/>
				<param name="ir_batch_no" type="12" value="" out="N"/>
				<param name="cntr_hold_remark" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
