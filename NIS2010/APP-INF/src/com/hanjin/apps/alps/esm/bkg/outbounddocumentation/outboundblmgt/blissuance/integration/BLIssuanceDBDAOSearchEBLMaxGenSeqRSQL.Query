<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchEBLMaxGenSeqRSQL">
			<desc><![CDATA[BLIssuanceDBDAOSearchEBLMaxGenSeq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(BKG_EBL_SEQ)+1,1) MAX_EBL_HIS_SEQ
  FROM BKG_EBL_GEN_NTC
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
