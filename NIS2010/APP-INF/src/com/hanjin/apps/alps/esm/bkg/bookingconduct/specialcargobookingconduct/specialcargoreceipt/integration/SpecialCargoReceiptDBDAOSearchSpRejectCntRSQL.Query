<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOSearchSpRejectCntRSQL">
			<desc><![CDATA[BKG에 대한 REJECT 요청의 개수]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) 
FROM
-- checking spcl_cgo_tp
#if (${spcl_cgo_tp} == 'A')
	     BKG_AWK_CGO SP
#elseif (${spcl_cgo_tp} == 'B')
		 BKG_BB_CGO SP
#elseif (${spcl_cgo_tp} == 'D')
		 BKG_DG_CGO SP
#elseif (${spcl_cgo_tp} == 'R')
		 BKG_RF_CGO SP

#end

WHERE SP.BKG_NO = @[bkg_no]
AND SP.SPCL_CGO_APRO_CD = 'N'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
