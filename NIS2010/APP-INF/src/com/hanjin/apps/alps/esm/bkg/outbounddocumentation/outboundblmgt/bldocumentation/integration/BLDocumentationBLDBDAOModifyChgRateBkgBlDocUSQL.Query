<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOModifyChgRateBkgBlDocUSQL">
			<desc><![CDATA[ModifyChgRateBkgBlDoc]]></desc>
			<sql><![CDATA[
#if (${caflag} == 'Y') 
UPDATE 
	BKG_BL_DOC_HIS
SET 
	BL_CVRD_TP_CD = @[bl_cvrd_tp_cd],
	MST_CVRD_BL_NO =  @[mst_cvrd_bl],
	COBIZ_AUTH_NO = @[cobiz_auth_no]
WHERE  
	BKG_NO =  @[bkg_no]
	AND   CORR_NO = 'TMP0000001'
#else 
UPDATE 
	BKG_BL_DOC 
SET 
	BL_CVRD_TP_CD = @[bl_cvrd_tp_cd],
	MST_CVRD_BL_NO =  @[mst_cvrd_bl],
	COBIZ_AUTH_NO = @[cobiz_auth_no]
WHERE  
	BKG_NO =  @[bkg_no]
#end			]]></sql>
			<params>
				<param name="bl_cvrd_tp_cd" type="12" value="" out="N"/>
				<param name="mst_cvrd_bl" type="12" value="" out="N"/>
				<param name="cobiz_auth_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
