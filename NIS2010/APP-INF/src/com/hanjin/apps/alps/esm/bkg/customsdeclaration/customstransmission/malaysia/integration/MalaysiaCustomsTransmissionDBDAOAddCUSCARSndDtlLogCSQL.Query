<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MalaysiaCustomsTransmissionDBDAOAddCUSCARSndDtlLogCSQL">
			<desc><![CDATA[2012.02.02 변종건 [CHM-201215473-01] Malaysis (MYTPP) Customs Manifest 전송 기능 추가 요청]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_MY_SND_LOG_DTL
(
     MY_MF_SND_IND_CD
    ,SND_DT
    ,FLT_FILE_REF_NO
    ,MF_SND_SEQ
    ,LOG_DTL_SEQ
    ,EDI_SND_MSG
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
)
VALUES(
     @[my_mf_snd_ind_cd]
    ,SYSDATE
    ,@[flt_file_ref_no]
    ,(
        SELECT NVL(MAX(MF_SND_SEQ),0)
        FROM   BKG_CSTMS_MY_SND_LOG
        WHERE  1 = 1
        AND    MY_MF_SND_IND_CD = @[my_mf_snd_ind_cd]
        AND    FLT_FILE_REF_NO = @[flt_file_ref_no]
     )
    ,@[log_dtl_seq]
    ,@[edi_snd_msg]
    ,@[usr_id]
    ,SYSDATE
    ,@[usr_id]
    ,SYSDATE
)			]]></sql>
			<params>
				<param name="my_mf_snd_ind_cd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="log_dtl_seq" type="12" value="" out="N"/>
				<param name="edi_snd_msg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
