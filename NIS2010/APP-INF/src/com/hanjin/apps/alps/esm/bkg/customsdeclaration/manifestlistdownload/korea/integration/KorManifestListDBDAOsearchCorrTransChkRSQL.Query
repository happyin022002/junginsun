<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchCorrTransChkRSQL">
			<desc><![CDATA[전송여부 체크]]></desc>
			<sql><![CDATA[
SELECT  DECODE(AMDT_SND_DT,NULL,'N','Y') TRANS_CHK
FROM    BKG_CSTMS_KR_CORR
WHERE   SMT_AMD_NO = (SELECT MAX(SMT_AMD_NO)
                      FROM   BKG_CSTMS_KR_CORR
                      WHERE  BL_NO            = @[bl_no]
                      AND    PORT_CD          = @[port_cd]
                      AND    CSTMS_DECL_TP_CD = @[cstms_decl_tp_cd])			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
