<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOSearchInterfaceStatusDetailListVORSQL">
			<desc><![CDATA[Invoice Interface Status]]></desc>
			<sql><![CDATA[
SELECT 
	A.SLP_TP_CD  ORG_SLP_TP_CD
,	A.SLP_FUNC_CD ORG_SLP_FUNC_CD
,	A.SLP_OFC_CD ORG_SLP_OFC_CD
,	A.SLP_ISS_DT ORG_SLP_ISS_DT
,	A.SLP_SER_NO ORG_SLP_SER_NO
,	A.SLP_SEQ_NO ORG_SLP_SEQ_NO
,	A.ACCT_CD
,	B.EFF_DT
,	A.CTR_CD
,	A.SLP_LOC_CD
,	A.CSR_CURR_CD
,	A.CSR_AMT
,	A.CSR_DESC
,	(SELECT VNDR_CNT_CD FROM MDM_VENDOR WHERE VNDR_SEQ = A.VNDR_SEQ) VNDR_CNT_CD
,	A.VNDR_SEQ
,	A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD||A.REV_DIR_CD VVD_CD
,   NVL(A.LSG_GR_NO,'')||NVL(A.AP_SLP_TP_CD||A.AP_SLP_FUNC_CD||A.AP_SLP_OFC_CD||A.AP_SLP_ISS_DT||A.AP_SLP_SER_NO||A.AP_SLP_SEQ_NO,'')||NVL(A.SLP_KEY_NO,'') KEY_NUMBER
,	A.SLP_TP_CD SLP_TP_CD
,	A.SLP_FUNC_CD SLP_FUNC_CD
,	'' SLP_OFC_CD
,	TO_CHAR(SYSDATE, 'YYMMDD') SLP_ISS_DT
,   '' SLP_SER_NO
,   '' SLP_SEQ_NO
,	'' CXL_DESC
,   '' CRE_USR_ID
,   '' UPD_USR_ID
,   CASE WHEN A.SLP_FUNC_CD = 'S' AND A.ORG_SLP_FUNC_CD = 'P' THEN A.ORG_SLP_TP_CD END PRE_SLP_TP_CD
,   CASE WHEN A.SLP_FUNC_CD = 'S' AND A.ORG_SLP_FUNC_CD = 'P' THEN A.ORG_SLP_FUNC_CD END PRE_SLP_FUNC_CD
,   CASE WHEN A.SLP_FUNC_CD = 'S' AND A.ORG_SLP_FUNC_CD = 'P' THEN A.ORG_SLP_OFC_CD END PRE_SLP_OFC_CD
,   CASE WHEN A.SLP_FUNC_CD = 'S' AND A.ORG_SLP_FUNC_CD = 'P' THEN A.ORG_ISS_DT END PRE_SLP_ISS_DT
,   CASE WHEN A.SLP_FUNC_CD = 'S' AND A.ORG_SLP_FUNC_CD = 'P' THEN A.ORG_SLP_SER_NO END PRE_SLP_SER_NO
,   CASE WHEN A.SLP_FUNC_CD = 'S' AND A.ORG_SLP_FUNC_CD = 'P' THEN A.ORG_SLP_SEQ_NO END PRE_SLP_SEQ_NO
FROM FMS_CONSULTATION B, FMS_CSUL_SLP A
WHERE 	A.SLP_TP_CD 	= B.SLP_TP_CD
AND   	A.SLP_FUNC_CD 	= B.SLP_FUNC_CD
AND   	A.SLP_OFC_CD 	= B.SLP_OFC_CD
AND   	A.SLP_ISS_DT 	= B.SLP_ISS_DT
AND   	A.SLP_SER_NO 	= B.SLP_SER_NO
AND		A.SLP_TP_CD 	= SUBSTR(@[csr_no],1,2)
AND		A.SLP_FUNC_CD 	= SUBSTR(@[csr_no],3,1)
AND		A.SLP_OFC_CD 	= SUBSTR(@[csr_no],4,6)
AND		A.SLP_ISS_DT 	= SUBSTR(@[csr_no],10,6)
AND		A.SLP_SER_NO 	= SUBSTR(@[csr_no],16,5)			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
