<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAORsltAcceptListRSQL">
			<desc><![CDATA[accept data  조회]]></desc>
			<sql><![CDATA[
SELECT PROP_NO
FROM   (SELECT PROP_NO
        FROM   PRI_SP_AMDT_SMRY
        WHERE  PROP_NO  = @[prop_no]
        AND    AMDT_SEQ = @[amdt_seq]
        AND    ACPT_FLG = 'N'
        AND    AMDT_FLG = 'Y'
        AND    ROWNUM = 1
        UNION ALL
        SELECT PROP_NO
        FROM   PRI_SP_SCP_AMDT_SMRY
        WHERE  PROP_NO  = @[prop_no]
        AND    AMDT_SEQ = @[amdt_seq]
        AND    ACPT_FLG = 'N'
        AND    AMDT_FLG = 'Y'
        AND    ROWNUM = 1)
WHERE  ROWNUM = 1			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
