<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationAssignmentDBDAOAuthAproOfcCSQL">
			<desc><![CDATA[PRI_AUTH_APRO_OFC 테이블 Merge]]></desc>
			<sql><![CDATA[
MERGE INTO PRI_AUTH_APRO_OFC
    USING (SELECT @[auth_apro_ofc_cd] NEW_AUTH_APRO_OFC_CD
     			 ,@[prc_ctrt_tp_cd] NEW_PRC_CTRT_TP_CD
    			 ,@[prc_ofc_auth_tp_cd] NEW_PRC_OFC_AUTH_TP_CD
    		 FROM DUAL)
    ON (AUTH_APRO_OFC_CD = NEW_AUTH_APRO_OFC_CD AND PRC_CTRT_TP_CD = NEW_PRC_CTRT_TP_CD AND PRC_OFC_AUTH_TP_CD = NEW_PRC_OFC_AUTH_TP_CD)
    WHEN MATCHED THEN 
    UPDATE SET AUTH_APRO_USE_FLG =  @[auth_apro_use_flg] , UPD_USR_ID = @[upd_usr_id], UPD_DT = SYSDATE
    WHEN NOT MATCHED THEN
      INSERT(PRC_CTRT_TP_CD
            , PRC_OFC_AUTH_TP_CD
            , AUTH_APRO_OFC_CD
            , AUTH_APRO_USE_FLG
            , CRE_USR_ID
            , CRE_DT
            , UPD_USR_ID
            , UPD_DT
      )
      VALUES( @[prc_ctrt_tp_cd]
            , @[prc_ofc_auth_tp_cd]
            , @[auth_apro_ofc_cd]
            , @[auth_apro_use_flg]
            , @[cre_usr_id]
            , SYSDATE
            , @[upd_usr_id]
            , SYSDATE
            )			]]></sql>
			<params>
				<param name="auth_apro_ofc_cd" type="12" value="" out="N"/>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="prc_ofc_auth_tp_cd" type="12" value="" out="N"/>
				<param name="auth_apro_use_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
