<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOcopyHblCustByBkgCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
insert into bkg_hbl_cust
        (BKG_NO
        , HBL_SEQ
        , BKG_CUST_TP_CD
        , CUST_CNT_CD
        , CUST_SEQ
        , CUST_NM
        , CUST_ADDR
        , CUST_ZIP_ID
        , CTY_NM
        , STE_CD
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT)
select @[targetBkg] BKG_NO
        , hcust.HBL_SEQ + (SELECT NVL(MAX(hbl_seq), 0)
							 FROM bkg_hbl_cust 
                            WHERE bkg_no = @[targetBkg])
        , hcust.BKG_CUST_TP_CD
        , hcust.CUST_CNT_CD
        , hcust.CUST_SEQ
        , hcust.CUST_NM
        , hcust.CUST_ADDR
        , hcust.CUST_ZIP_ID
        , hcust.CTY_NM
        , hcust.STE_CD
        , @[usr_id] CRE_USR_ID
        , sysdate CRE_DT
        , @[usr_id] UPD_USR_ID
        , sysdate UPD_DT
 from bkg_hbl_cust hcust
where hcust.bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="targetBkg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
