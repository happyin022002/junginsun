<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOBkgIbCustCntcUSQL">
			<desc><![CDATA[....]]></desc>
			<sql><![CDATA[
UPDATE bkg_ib_cust_cntc
    SET fax_no = @[fax_no]
        ,fax_snd_flg = decode(@[fax_snd_flg],1,'N',0,'Y')
        ,cntc_eml    = @[cntc_eml]
        ,eml_snd_flg = decode(@[eml_snd_flg],1,'N',0,'Y')
        ,phn_no      = @[phn_no]
        ,mphn_no     = @[mphn_no]
        ,cntc_rmk    = @[cntc_rmk]
        ,upd_locl_dt  = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[evnt_ofc_cd])    
        ,upd_locl_gdt = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', sysdate, 'GMT' )
		,upd_dt      = sysdate
		,upd_usr_id  = @[upd_usr_id]        
  WHERE ofc_cd = @[ofc_cd]
    AND cust_cnt_cd = @[cust_cnt_cd]
    AND cust_seq = @[cust_seq]
    AND cust_cntc_tp_cd = @[cust_cntc_tp_cd]			]]></sql>
			<params>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="fax_snd_flg" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="eml_snd_flg" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="mphn_no" type="12" value="" out="N"/>
				<param name="cntc_rmk" type="12" value="" out="N"/>
				<param name="evnt_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_cntc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
