<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostManageDBDAOSearchNewLaneOfficeCmcbListRSQL">
			<desc><![CDATA[New Lane - Office의 Cmcb를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CST.BSE_TP_CD
      ,CST.BSE_YR
      ,CST.BSE_QTR_CD
      ,DECODE(CST.OFC_VW_CD,'L','LOADING','C','CONTRACT') OFC_VW_CD
      ,CST.TRD_CD
      ,CST.RLANE_CD
      ,CST.DIR_CD
      ,CST.RGN_OFC_CD
      ,CST.RHQ_CD
      ,CST.GID_PA_CM_UC_AMT
      ,CST.GID_RA_CM_UC_AMT
      ,CST.PA_CM_UC_AMT
      ,CST.RA_CM_UC_AMT
 FROM SQM_QTA_NEW_LANE_OFC_COST CST
     ,SQM_QTA_LANE_OFC OFC
WHERE CST.BSE_TP_CD  = @[f_bse_tp_cd]
  AND CST.BSE_YR     = @[f_bse_yr]
#if (${f_bse_tp_cd} == 'Y')
  AND CST.BSE_QTR_CD = '00'
#else
  AND CST.BSE_QTR_CD = @[f_bse_qtr_cd]
#end
  AND CST.OFC_VW_CD  = @[f_ofc_vw_cd]
  AND CST.TRD_CD     = @[f_trd_cd]
  AND CST.DIR_CD     = @[f_dir_cd]
  AND CST.RLANE_CD   = @[f_rlane_cd]
  AND CST.BSE_TP_CD  = OFC.BSE_TP_CD
  AND CST.BSE_YR     = OFC.BSE_YR
  AND CST.BSE_QTR_CD = OFC.BSE_QTR_CD
  AND CST.OFC_VW_CD  = OFC.OFC_VW_CD
  AND CST.TRD_CD     = OFC.TRD_CD
  AND CST.DIR_CD     = OFC.DIR_CD
  AND CST.RLANE_CD   = OFC.RLANE_CD
  AND CST.RGN_OFC_CD = OFC.RGN_OFC_CD
  AND CST.RHQ_CD     = OFC.RHQ_CD    
  AND OFC.SQM_ACT_FLG= 'Y' 
ORDER BY TRD_CD
        ,RLANE_CD
        ,DIR_CD
        ,RHQ_CD
        ,RGN_OFC_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
