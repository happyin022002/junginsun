<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOBkgUsrTmpltVORSQL">
			<desc><![CDATA[select   ]]></desc>
			<sql><![CDATA[
SELECT USR_ID
	  ,TMPLT_TP_CD
	  ,TMPLT_SEQ
	  ,TMPLT_HDR_NM
	  ,TMPLT_CTNT
  FROM BKG_USR_TMPLT
 WHERE USR_ID = @[usr_id]

#if (${tmplt_tp_cd} == 'R')
   AND TMPLT_TP_CD = @[tmplt_tp_cd]
#else
   AND TMPLT_TP_CD IN ('M', 'D', 'T', 'V')

    #if (${tmplt_tp_cd} != '')
   AND	TMPLT_TP_CD = @[tmplt_tp_cd]
    #end
#end			]]></sql>
			<params>
				<param name="usr_id" type="12" value="NIS2010" out="N"/>
				<param name="tmplt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
