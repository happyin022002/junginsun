<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOSearchVVDByBlRSQL">
			<desc><![CDATA[BL No.로 VVD정보(Vessel name, Lloyd No.포함)]]></desc>
			<sql><![CDATA[
SELECT BKG.VSL_CD,
  BKG.SKD_VOY_NO,
  BKG.SKD_DIR_CD,
  BKG.VSL_CD||BKG.SKD_VOY_NO||BKG.SKD_DIR_CD VVD,
  VSL.VSL_ENG_NM,
  VSL.LLOYD_NO,
  BKG.CSTMS_YD_CD
FROM BKG_CSTMS_EUR_BL BKG, MDM_VSL_CNTR VSL
WHERE BL_NO = @[bl_no]
  AND BKG.VSL_CD = VSL.VSL_CD			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
