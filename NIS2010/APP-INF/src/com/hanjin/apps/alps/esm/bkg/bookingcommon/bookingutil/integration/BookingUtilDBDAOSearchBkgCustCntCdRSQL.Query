<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchBkgCustCntCdRSQL">
			<desc><![CDATA[BKG의 Customer Country Code를 조회함]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD
  FROM BKG_CUSTOMER
 WHERE BKG_NO = @[bkg_no]
   AND BKG_CUST_TP_CD = @[bkg_cust_tp_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
