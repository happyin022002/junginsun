<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchLocCdByOfcCd2RSQL">
			<desc><![CDATA[특정 Location Code 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SUBSTR(LOC_CD,0,2) LOC_CD
FROM MDM_ORGANIZATION
WHERE 1=1
#if (${ofc_cd} != '') 
AND	OFC_CD IN ( ${ofc_cd} )
#else
AND	OFC_CD = @[ofc_cd]
#end
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
