<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOMultiSimReportMasterCSQL">
			<desc><![CDATA[report master 입력]]></desc>
			<sql><![CDATA[
INSERT INTO COA_SIM_RPT_MST
		    (SELECT @[sim_dt] SIM_DT
		           ,@[sim_no] SIM_NO
		           ,@[sect_no] SECT_NO
		           ,@[sim_rpt_no] SIM_RPT_NO
		           ,@[ldf_rto] / 100 LDF_RTO
		           ,@[grs_rpb_rev] GRS_RPB_REV
		           ,@[bnk_cost_amt] BNK_COST_AMT
		           ,@[sim_rmk] SIM_RMK
		           ,@[cre_usr_id] CRE_USR_ID
		           ,SYSDATE CRE_DT
		           ,@[upd_usr_id] UPD_USR_ID
		           ,SYSDATE UPD_DT
		       FROM DUAL)			]]></sql>
			<params>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="sect_no" type="12" value="" out="N"/>
				<param name="sim_rpt_no" type="12" value="" out="N"/>
				<param name="ldf_rto" type="12" value="" out="N"/>
				<param name="grs_rpb_rev" type="12" value="" out="N"/>
				<param name="bnk_cost_amt" type="12" value="" out="N"/>
				<param name="sim_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
