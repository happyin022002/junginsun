<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQBalanceDBDAOMasCntrRepoCrVOUSQL">
			<desc><![CDATA[mas_cntr_repo_cr 조회   ]]></desc>
			<sql><![CDATA[
UPDATE MAS_CNTR_REPO_CR
   SET EQ_REPO_CR_RTO = @[eq_repo_cr_rto]/100              
     , UPD_USR_ID = @[upd_usr_id]                
     , UPD_DT = SYSDATE                
 WHERE COST_YRMON = @[cost_yrmon]              
   AND CNTR_TPSZ_CD = REPLACE(@[cntr_tpsz_cd], 'RD', 'R')
   AND REPO_LVL_GRP_CD = @[repo_lvl_grp_cd]			]]></sql>
			<params>
				<param name="eq_repo_cr_rto" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="repo_lvl_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
