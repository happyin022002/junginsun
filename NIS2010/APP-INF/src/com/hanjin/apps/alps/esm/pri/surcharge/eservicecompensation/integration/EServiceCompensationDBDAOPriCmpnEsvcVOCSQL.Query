<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EServiceCompensationDBDAOPriCmpnEsvcVOCSQL">
			<desc><![CDATA[E-SVC Compensation 데이터 입력]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_CMPN_ESVC (
    SVC_SCP_CD
,   CMPN_SEQ
,   ORG_RGN_CD
,   DEST_RGN_CD
,   PRC_CTRT_TP_CD
,   SC_NO
,   RFA_NO
,   CHG_CD
,   CURR_CD
,   FLT_PCT_TP_CD
,   DC_AMT
,   EFF_DT
,   EXP_DT
,   CMPN_RMK
,   CRE_USR_ID
,   CRE_DT
,   UPD_USR_ID
,   UPD_DT
) VALUES(
    @[svc_scp_cd]
,   @[cmpn_seq]
,   @[org_rgn_cd]
,   @[dest_rgn_cd]
,   @[prc_ctrt_tp_cd]
,   @[sc_no]
,   @[rfa_no]
,   @[chg_cd]
,   @[curr_cd]
,   DECODE(LENGTH(TRIM(@[curr_cd])),0,'P','F')
,   NVL2(@[curr_cd],@[dc_amt],@[dc_per])
,   TO_DATE(@[eff_dt],'YYYY-MM-DD')
,   TO_DATE(@[exp_dt],'YYYY-MM-DD')
,   @[cmpn_rmk]
,   @[cre_usr_id]
,   SYSDATE
,   @[upd_usr_id]
,   SYSDATE
)			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="AFE" out="N"/>
				<param name="cmpn_seq" type="12" value="" out="N"/>
				<param name="org_rgn_cd" type="12" value="" out="N"/>
				<param name="dest_rgn_cd" type="12" value="" out="N"/>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="dc_amt" type="6" value="" out="N"/>
				<param name="dc_per" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cmpn_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
