<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOUpdateCfmTgtVvdSupplyUSQL">
			<desc><![CDATA[QtaAdjustmentDBDAOUpdateCfmTgtVvdSupplyUSQL]]></desc>
			<sql><![CDATA[
UPDATE SQM_CFM_TGT_VVD
SET FNL_BSA_CAPA = @[fnl_bsa_capa]
WHERE QTA_RLSE_VER_NO = @[qta_rlse_ver_no]
AND BSE_TP_CD = 'Q'
AND BSE_YR = '20'||SUBSTR(@[qta_rlse_ver_no],1,2)
AND BSE_QTR_CD = SUBSTR(@[qta_rlse_ver_no], 3, 2)
AND QTA_TGT_CD = 'D'
AND TRD_CD = @[trd_cd]
AND RLANE_CD = @[rlane_cd]
AND DIR_CD = @[dir_cd]
AND VSL_CD||SKD_VOY_NO||SKD_DIR_CD = @[vvd]			]]></sql>
			<params>
				<param name="fnl_bsa_capa" type="12" value="" out="N"/>
				<param name="qta_rlse_ver_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
