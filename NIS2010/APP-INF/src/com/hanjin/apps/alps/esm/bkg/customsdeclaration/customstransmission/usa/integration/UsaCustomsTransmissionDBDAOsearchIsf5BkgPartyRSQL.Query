<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchIsf5BkgPartyRSQL">
			<desc><![CDATA[dwkim, 미세관 ISF5송신, outVO : None, 연관 VO: UsaIsf5ResultVO VO생성금지!!]]></desc>
			<sql><![CDATA[
SELECT BKG_SPCLCHAR_CONV_FNC(RPAD(SCE_TOKEN_NL_FNC(RTRIM(CS.CUST_NM),1),35,' '), 'M') ENTT_NAME
      ,BKG_SPCLCHAR_CONV_FNC(RPAD(SCE_TOKEN_NL_FNC(RTRIM(CS.CUST_ADDR),1),35,' '), 'M') ADD_INFO
      ,BKG_SPCLCHAR_CONV_FNC(RPAD(NVL(SCE_TOKEN_NL_FNC(RTRIM(CS.CUST_ADDR),2),'.'),35,' '), 'M') ADD_INFO_2
      ,BKG_SPCLCHAR_CONV_FNC(RPAD(SUBSTR(DECODE(RTRIM(CS.CUST_CTY_NM),NULL,DECODE(RTRIM(BS.CUST_CTY_NM),NULL,L1.LOC_NM,RTRIM(BS.CUST_CTY_NM)),RTRIM(CS.CUST_CTY_NM)),1,35),35,' '), 'M') ADD_CT
      ,BKG_SPCLCHAR_CONV_FNC(RPAD(DECODE(RTRIM(CS.CUST_CNT_CD),NULL,DECODE(RTRIM(BS.CUST_CNT_CD),NULL,SUBSTR(CB.POR_CD,1,2),RTRIM(BS.CUST_CNT_CD)),RTRIM(CS.CUST_CNT_CD)),2,' '), 'M') ADD_CNTRY
  FROM BKG_CSTMS_ADV_BL CB
      ,BKG_CSTMS_ADV_CUST CS
      ,BKG_CUSTOMER BS
      ,MDM_LOCATION L1
 WHERE CB.CNT_CD = 'US'
   AND CB.BL_NO = @[bl_no]
   AND CB.CNT_CD = CS.CNT_CD
   AND CB.BL_NO = CS.BL_NO
   AND CS.BKG_CUST_TP_CD    = 'S'
   AND CB.BKG_NO = BS.BKG_NO (+)
   AND BS.BKG_CUST_TP_CD(+) = 'S'
   AND CB.POR_CD = L1.LOC_CD			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
