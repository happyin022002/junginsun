<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyExpAccpDtAtCntrUSQL">
			<desc><![CDATA[미세관 응답메세지 수신 NVOCC 중 BKG_CSTMS_ADV_CNTR 를 갱신한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_CNTR 
   SET XPT_ACPT_DT = TO_DATE(@[acp_date], 'RRMMDD')
 WHERE CNT_CD = 'US'
   AND BL_NO = @[bl_no]
#if (${icr_code} == '52') 
   AND CNTR_NO = @[cntr_no]
   AND @[icr_code] = 52
#end			]]></sql>
			<params>
				<param name="acp_date" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="icr_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
