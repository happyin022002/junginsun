<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOSearchAddCreateLanePairCostCntRSQL">
			<desc><![CDATA[Add Creation 할 노선의 sctr_pair_cost Data Cnt를 조회한다. (data cnt가 0이 아닐경우 cost 생성)]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT 
FROM SQM_SCTR_PAIR_COST
WHERE 1=1
AND BSE_TP_CD   = @[f_bse_tp_cd]
AND BSE_YR      = @[f_bse_yr]
AND BSE_QTR_CD  = DECODE(@[f_bse_tp_cd],'Y','00',@[f_bse_qtr_cd])
AND RLANE_CD    = @[f_rlane_cd]
AND SUB_TRD_CD  = @[f_sub_trd_cd]			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_sub_trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
