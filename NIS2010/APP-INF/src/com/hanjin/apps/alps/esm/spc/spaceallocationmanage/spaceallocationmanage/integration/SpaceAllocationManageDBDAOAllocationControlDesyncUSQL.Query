<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOAllocationControlDesyncUSQL">
			<desc><![CDATA[Control By HO, RHQ, Main Office 의 Control 중 Desync 동작]]></desc>
			<sql><![CDATA[
--update
UPDATE SPC_ALOC_CTRL_OPT
SET 
	BKG_CTRL_ACCT_GRP_APLY_FLG=NULL
	,BKG_CTRL_ACCT_GRP_FCAST_FLG=NULL
	,BKG_CTRL_ACCT_GRP_RTO=NULL
	,BKG_CTRL_MST_TBL_APLY_FLG=NULL
	,BKG_CTRL_MST_TBL_FCAST_FLG=NULL
	,BKG_CTRL_MST_TBL_FCAST_RTO=NULL
	,BKG_CTRL_ALOC_APLY_FLG=NULL
	,BKG_CTRL_ALOC_FCAST_FLG=NULL
	,BKG_CTRL_ALOC_FCAST_RTO=NULL
WHERE 1=1
	AND RLANE_CD=@[rlane_cd]
	AND DIR_CD=@[dir_cd]
	AND VSL_CD=@[vsl_cd]
	AND SKD_VOY_NO=@[skd_voy_no]
	AND SKD_DIR_CD=@[skd_dir_cd]
	AND REP_TRD_CD=SPC_GET_REP_TRD_FNC(@[rlane_cd])
	AND REP_SUB_TRD_CD=SPC_GET_REP_SUB_TRD_FNC(@[rlane_cd])			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
