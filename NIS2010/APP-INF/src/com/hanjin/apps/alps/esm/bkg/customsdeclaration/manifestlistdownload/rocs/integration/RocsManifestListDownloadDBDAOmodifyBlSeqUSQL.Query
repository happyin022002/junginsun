<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOmodifyBlSeqUSQL">
			<desc><![CDATA[데이터 확인을 위한 BL List에서 B/L Seq를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_RTM_BL
   SET IB_FILE_SEQ				=	@[ib_file_seq]
 WHERE VSL_CALL_REF_NO			=	@[frm_crn_number]
   AND BKG_NO					=	@[bkg_no]			]]></sql>
			<params>
				<param name="ib_file_seq" type="12" value="" out="N"/>
				<param name="frm_crn_number" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
