<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffCodeDBDAOSearchTariffCodeRSQL">
			<desc><![CDATA[입력한 Tariff Code에 해당하는 Tariff Scope 조회]]></desc>
			<sql><![CDATA[
SELECT P.SVC_SCP_CD, M.SVC_SCP_NM
FROM MDM_SVC_SCP M,
     PRI_SVC_SCP_TRF P
WHERE P.TRF_PFX_CD = SUBSTR(@[trf_pfx_cd],1,4)
  AND P.SVC_SCP_CD = M.SVC_SCP_CD
  AND P.TRF_NO 	   = @[trf_no]			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
