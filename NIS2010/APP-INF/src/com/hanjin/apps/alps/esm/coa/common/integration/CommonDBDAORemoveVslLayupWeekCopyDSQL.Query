<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAORemoveVslLayupWeekCopyDSQL">
			<desc><![CDATA[Manual Detail Cost 테이블 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM COA_MNL_DTL_COST 
WHERE 1=1
	AND COST_YRMON LIKE SUBSTR(REPLACE(@[f_tar_week],'-',''),1,4)||'%'
    AND COST_WK = SUBSTR(REPLACE(@[f_tar_week],'-',''),5,2)
	AND RLANE_CD = @[rlane_cd]			]]></sql>
			<params>
				<param name="f_tar_week" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
