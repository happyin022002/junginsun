<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchAROfcIsCheckRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchAROfcIsCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT 
  FROM BKG_CHG_RT A,MDM_ORGANIZATION B, MDM_LOCATION C 
 WHERE A.BKG_NO = @[bkg_no]
   AND A.FRT_TERM_CD = 'C' 
   AND A.FRT_INCL_XCLD_DIV_CD = 'N' 
   AND NVL(A.N3PTY_RCV_OFC_CD, @[ppd_ofc_cd]) = @[ppd_ofc_cd] 
   AND NVL(A.N3PTY_RCV_OFC_CD, @[ppd_ofc_cd]) = B.OFC_CD 
   AND B.LOC_CD = C.LOC_CD 
   AND C.CONTI_CD IN ('F', 'E') 
   AND ROWNUM = 1 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ppd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
