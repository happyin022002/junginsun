<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchBbCgoRSQL">
			<desc><![CDATA[searchBbCgo]]></desc>
			<sql><![CDATA[
SELECT (
    SELECT 'BB :'||' '||SUBSTR(RTRIM(REPLACE(PP, 'P0')||REPLACE(LL, 'L0')||REPLACE(WW, 'W0')||REPLACE(HH, 'H0')), 1, 50) AA
    FROM (
        SELECT 'PK'||TO_CHAR(PCK_QTY||PCK_TP_CD||',') PP,
          'L'||TO_CHAR(ROUND(DIM_LEN)) LL,
          'W'||TO_CHAR(ROUND(DIM_WDT)) WW,
          'H'||TO_CHAR(ROUND(DIM_HGT)) HH
        FROM BKG_BB_CGO
        WHERE BKG_NO = @[bkg_no]
          AND BB_CGO_SEQ = 1 ) ) BB_EXIST
FROM BKG_CONTAINER A
WHERE A.BKG_NO = @[bkg_no]
  AND A.CNTR_NO = @[cntr_no]
  AND A.BB_CGO_FLG = 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
