<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchInfoForGrpMsnRSQL">
			<desc><![CDATA[Group MSN정보조회를 위한 정보 search]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SEQ.MF_REF_NO GRP_MRN
     , SEQ.MRN_CHK_NO GRP_MRN_CHK
     , SEQ.VSL_CD||SEQ.SKD_VOY_NO||SEQ.SKD_DIR_CD GRP_VVD
     , VVD.POL_CD GRP_POL
     , VVD.POD_CD GRP_POD
     , NVL(TO_CHAR(VSL1.VPS_ETD_DT,'YYYYMMDD'),' ') GRP_ETD
     , NVL(TO_CHAR(VSL2.VPS_ETA_DT,'YYYYMMDD'),' ') GRP_ETA
	 ,(
    	SELECT NVL(TO_CHAR(VSL3.VPS_ETA_DT, 'YYYY-MM-DD HH24:MI'), ' ')
    	FROM VSK_VSL_PORT_SKD VSL3
    	WHERE VVD.VSL_CD = VSL3.VSL_CD
      	AND VVD.SKD_VOY_NO = VSL3.SKD_VOY_NO
      	AND VVD.SKD_DIR_CD = VSL3.SKD_DIR_CD
      	AND VVD.POD_CD = VSL3.VPS_PORT_CD
        AND VSL3.CLPT_IND_SEQ  =  '1'
      	AND ROWNUM = 1) GRP_ETA_DTL
     , SEQ.UPD_USR_ID
     , SEQ.UPD_DT
     , SEQ.VIA_WEB_FLG
     , SEQ.VIA_WEB_DT
FROM BKG_VVD VVD,  VSK_VSL_PORT_SKD  VSL1,  VSK_VSL_PORT_SKD  VSL2
     ,(SELECT BKG_NO, MF_REF_NO, MRN_CHK_NO, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, UPD_USR_ID, NVL(TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI'),' ') UPD_DT
, VIA_WEB_FLG, NVL(TO_CHAR(WEB_IF_DT,'YYYY-MM-DD HH24:MI'),' ') VIA_WEB_DT
     FROM BKG_CSTMS_KR_MF_SEQ_NO
     WHERE BKG_NO = @[bkg_no]) SEQ
WHERE SEQ.VSL_CD   = VVD.VSL_CD
AND SEQ.SKD_VOY_NO = VVD.SKD_VOY_NO
AND SEQ.SKD_DIR_CD = VVD.SKD_DIR_CD
AND SEQ.BKG_NO     = VVD.BKG_NO
AND VVD.VSL_CD         =  VSL1.VSL_CD
AND VVD.SKD_VOY_NO     =  VSL1.SKD_VOY_NO
AND VVD.SKD_DIR_CD     =  VSL1.SKD_DIR_CD
AND VVD.POL_CD         =  VSL1.VPS_PORT_CD
AND VSL1.CLPT_IND_SEQ  =  '1'
AND VVD.VSL_CD         =  VSL2.VSL_CD
AND VVD.SKD_VOY_NO     =  VSL2.SKD_VOY_NO
AND VVD.SKD_DIR_CD     =  VSL2.SKD_DIR_CD
AND VVD.POD_CD         =  VSL2.VPS_PORT_CD
AND VSL2.VPS_ETA_DT    >  VSL1.VPS_ETD_DT			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
