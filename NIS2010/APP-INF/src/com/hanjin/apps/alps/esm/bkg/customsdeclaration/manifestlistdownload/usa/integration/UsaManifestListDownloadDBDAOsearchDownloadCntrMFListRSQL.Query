<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchDownloadCntrMFListRSQL">
			<desc><![CDATA[UsaManifestListDownloadDBDAOsearchDownloadCntrMFListRSQL]]></desc>
			<sql><![CDATA[
SELECT *
FROM (
		SELECT  CNT.CNTR_NO
		       ,BKG.BKG_NO
		       ,BKG.BL_NO
			   ,CM.CNTR_MF_WGT AS CNTR_MF_WGT 
		FROM    BKG_VVD VVD
		       ,BKG_BOOKING BKG
		       ,BKG_CONTAINER CNT
		       ,BKG_CNTR_MF_DESC CM
		WHERE   1=1
		AND     VVD.VSL_CD      = @[vsl_cd]
		AND     VVD.SKD_VOY_NO  = @[skd_voy_no]
		AND     VVD.SKD_DIR_CD  = @[skd_dir_cd]
		#if (${all_pol} != 'Y' and ${pol_cd} != '')
		AND     VVD.POL_CD      = @[pol_cd]
		#end
		AND     VVD.POD_CD      = @[pod_cd]
		#if (${bkg_no} != '')
		AND     VVD.BKG_NO 		= @[bkg_no] 
		#end
		#if (${bkg_cgo_tp_cd} == 'F') 
		AND  	BKG.BKG_CGO_TP_CD IN ('F', 'R')
		#else
		AND  	BKG.BKG_CGO_TP_CD IN ('P')
		#end
		AND     VVD.BKG_NO = BKG.BKG_NO
		AND     BKG.BKG_NO = CNT.BKG_NO(+)
		--AND     BKG.BKG_STS_CD IN ('F','W')
		AND     NVL(BKG.BKG_STS_CD,' ') NOT IN ('X','A')
		AND     BKG.BL_NO IS NOT NULL
		AND     CNT.CNTR_NO = CM.CNTR_NO
        AND     BKG.BKG_NO = CM.BKG_NO
)
ORDER BY BL_NO			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
