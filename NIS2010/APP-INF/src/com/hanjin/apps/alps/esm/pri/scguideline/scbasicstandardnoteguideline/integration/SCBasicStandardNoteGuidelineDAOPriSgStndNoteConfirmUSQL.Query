<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBasicStandardNoteGuidelineDAOPriSgStndNoteConfirmUSQL">
			<desc><![CDATA[컨폼]]></desc>
			<sql><![CDATA[
update pri_sg_stnd_note_hdr set 
	cfm_flg = DECODE(@[cfm_flg],'No','N','Yes','Y',@[cfm_flg]),
	cfm_usr_id = @[cfm_usr_id]
where	svc_scp_cd = @[svc_scp_cd]
and	note_hdr_seq = @[note_hdr_seq]			]]></sql>
			<params>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="cfm_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="note_hdr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
