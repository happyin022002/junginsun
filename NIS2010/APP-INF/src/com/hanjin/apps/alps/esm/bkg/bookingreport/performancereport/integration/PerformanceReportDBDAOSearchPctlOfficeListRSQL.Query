<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchPctlOfficeListRSQL">
			<desc><![CDATA[Port Closing Inquiry[ESM_BKG_0914]
PCT (Port Closing Time) 현황 조회 화면 -1.Office_List]]></desc>
			<sql><![CDATA[
#if (${dt_type} == 'PCT')
    SELECT /*+ ALL_ROWS */
           OLA.REGION RHQ_CD
          ,OLA.GSO BKG_OFC_CD
          ,OLA.OFC_CD B_OFC
          ,TRIM(TO_CHAR(COUNT(BK.BKG_NO))) TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BK.BKG_STS_CD, 'F', 1, 'S', 1, 0)))) FIRM_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BK.BKG_STS_CD, 'W', 1, 0)))) WAIT_TTL
		  ,TRIM(TO_CHAR(SUM(CASE WHEN BK.CUST_TO_ORD_FLG ='Y' AND (NVL(BN.CUST_SEQ,0) <> 0) THEN 1 
                                 WHEN BC.BKG_CUST_TP_CD = 'C' AND (NVL(BC.CUST_SEQ,0) <> 0 OR NVL(BK.INDIV_PSON_FLG,'N') ='Y') THEN 1 ELSE 0 END)))CNEE_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BKG_RATE_RESULT_FNC(BK.BKG_NO), 'F', 1, 0)))) RFIRM_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BKG_RATE_RESULT_FNC(BK.BKG_NO), 'N', 1, 0)))) NULL_TTL
          ,'' BKG_STS_CD
          ,'' BKG_RT_STS_CD
          ,'' REV_STATUS
          ,'' BKG_STAFF
          ,'' FR_DT
          ,'' TO_DT
          ,'' POL_CD
          ,'' VVD_CD
          ,'' GSO
          ,'' DT_TYPE  --PCT,ETD
          ,'0'  S_BL_TTL
          ,'0'  O_BL_TTL
          ,'0'  S_CNEE_TTL
          ,'0'  O_CNEE_TTL
          ,'0'  S_NTFY_TTL
          ,'0'  O_NTFY_TTL
          ,''   BL_TYPE
          ,'0'   BL_TTL
          ,'0'   NTFY_TTL
          ,''   DOWN_TP_O
          ,''   DOWN_TP_B
          ,''   DOWN_TP_T
          ,SUM(BK.CNTR_CFM) AS CNTR_CFM
          ,COUNT(BK.BKG_NO) - SUM(BK.CNTR_CFM) AS CNTR_NONCFM
      FROM (
            SELECT BK.BKG_NO
                  ,BK.BKG_STS_CD
                  ,BK.BKG_OFC_CD
                  ,(SELECT NVL(COUNT(*),0)
                      FROM BKG_DOC_PROC_SKD
                     WHERE BKG_DOC_PROC_TP_CD IN('CNTCFM', 'CNTRLS')
                       AND DOC_PERF_DELT_FLG = 'N'
                       AND BKG_NO = BK.BKG_NO
                       AND ROWNUM = 1
                   ) AS CNTR_CFM
				,BK.INDIV_PSON_FLG
				,BK.CUST_TO_ORD_FLG
              FROM BKG_BOOKING BK
             WHERE 1=1
               AND BK.BKG_STS_CD <> 'X'
            #if (${bkg_ofc_cd} != '')
               AND BK.BKG_OFC_CD = @[bkg_ofc_cd]
            #end
            #if (${bkg_sts_cd} != '')
               AND BK.BKG_STS_CD = @[bkg_sts_cd]
            #end
            #if (${bkg_rt_sts_cd} != '' && ${bkg_rt_sts_cd} != 'A')
               AND BKG_RATE_RESULT_FNC(BK.BKG_NO)  = @[bkg_rt_sts_cd]
            #end
            #if (${bkg_staff} != '')
               AND BK.DOC_USR_ID LIKE @[bkg_staff] || '%%'
            #end
            #if (${fr_dt} != '')
               AND BK.PORT_CLZ_DT   >= TO_DATE(@[fr_dt], 'YYYY-MM-DD')
            #end
            #if (${to_dt} != '')
               AND BK.PORT_CLZ_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
            #end
            #if (${pol_cd} != '')
               AND BK.POL_CD = @[pol_cd]
            #end
            #if (${vvd_cd} != '')
               AND BK.VSL_CD =  SUBSTR(@[vvd_cd],1,4)
               AND BK.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
               AND BK.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
            #end
            #if (${slan_cd} != 'All')
               AND BK.SLAN_CD = @[slan_cd]
            #end
           ) BK
          ,BKG_CUSTOMER BC
          ,BKG_CUSTOMER BN
          ,BKG_VVD VVD
          ,VSK_VSL_PORT_SKD VPS
          ,(SELECT ROWNUM
                  ,REGION
                  ,GSO
                  ,OFC_CD
              FROM BKG_OFC_LVL_V
            ) OLA
    #if (${rev_status} != '')
          ,(
            SELECT S.*
                  ,DECODE(NVL(REV_LANE, 'N'), 'N', 'N', 'Y') REV_STATUS
              FROM MDM_VSL_SVC_LANE S
                  ,(
                    SELECT DISTINCT SUBSTR(RLANE_CD, 1, 3) REV_LANE
                      FROM MDM_DTL_REV_LANE
                     WHERE DELT_FLG = 'N'
                   ) R
             WHERE S.VSL_SLAN_CD = R.REV_LANE(+)
           ) REV
    #end
     WHERE BK.BKG_OFC_CD = OLA.OFC_CD
       AND OLA.REGION IS NOT NULL
       AND BC.BKG_NO = BK.BKG_NO
       AND BN.BKG_NO = BK.BKG_NO
       AND BC.BKG_CUST_TP_CD = 'C'
       AND BN.BKG_CUST_TP_CD = 'N'
       AND VVD.BKG_NO = BK.BKG_NO
       AND VPS.VSL_CD = VVD.VSL_CD
       AND VPS.SKD_VOY_NO = VVD.SKD_VOY_NO
       AND VPS.SKD_DIR_CD = VVD.SKD_DIR_CD
       AND VPS.VPS_PORT_CD = VVD.POL_CD
       AND VPS.CLPT_IND_SEQ = 1
       AND VVD.VSL_PRE_PST_CD || VVD.VSL_SEQ = (SELECT /*+ INDEX(VVD XPKBKG_VVD) */
                                                       BV.VSL_PRE_PST_CD || BV.VSL_SEQ
                                                  FROM BKG_VVD BV
                                                 WHERE BV.BKG_NO = VVD.BKG_NO
                                                   AND ROWNUM =1)
    #if (${rev_status} != '')
       AND VVD.SLAN_CD = REV.VSL_SLAN_CD(+)
    #end
    #if (${rhq_cd} != '')
       AND OLA.REGION = @[rhq_cd]
    #end
    #if (${gso} != '')
       AND OLA.GSO = @[gso]
    #end
    #if (${rev_status} != '')
       AND NVL(REV.REV_STATUS, 'N') = @[rev_status]
    #end
    #if (${cntr_cfm} == 'Y')
       AND BK.CNTR_CFM > 0
    #elseif (${cntr_cfm} == 'N')
       AND BK.CNTR_CFM = 0
    #end
    GROUP BY OLA.REGION, OLA.GSO, OLA.OFC_CD
    ORDER BY OLA.REGION, OLA.GSO, OLA.OFC_CD

--------------------------------------------------------------------------------

#elseif (${dt_type} =='ETD')

    SELECT REGION RHQ_CD
          ,GSO BKG_OFC_CD
          ,OFC_CD B_OFC
          ,TRIM(TO_CHAR(COUNT(TTL))) TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BKG_STS_CD, 'F', 1, 'S', 1, 0)))) FIRM_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BKG_STS_CD, 'W', 1, 0)))) WAIT_TTL
          ,TRIM(TO_CHAR(SUM(CASE WHEN BKG_CUST_TP_CD = 'C' AND (NVL(CUST_SEQ,0) <> 0 OR NVL(INDIV_PSON_FLG,'N') ='Y') THEN 1 ELSE 0 END)))CNEE_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BKG_RATE_RESULT_FNC(TTL), 'F', 1, 0)))) RFIRM_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BKG_RATE_RESULT_FNC(TTL), 'N', 1, 0)))) NULL_TTL
          ,SUM(CNTR_CFM) AS CNTR_CFM
          ,COUNT(TTL) - SUM(CNTR_CFM) AS CNTR_NONCFM
      FROM (
            SELECT /*+ ALL_ROWS */
                   BK.BKG_NO TTL
                  ,BK.BKG_STS_CD
                  ,BKG_RATE_RESULT_FNC(BK.BKG_NO)  BKG_RT_STS_CD
                  ,BK.CNTR_CFM
                  ,OLA.REGION REGION
                  ,OLA.GSO GSO
                  ,OLA.OFC_CD
                  ,C.BKG_CUST_TP_CD
                  ,C.CUST_CNT_CD
				  ,C.CUST_SEQ
				  ,BK.INDIV_PSON_FLG
              FROM (
		            SELECT BK.BKG_NO
		                  ,BK.BKG_STS_CD
		                  ,BK.BKG_OFC_CD
		                  ,BK.POL_CD
		                  ,(SELECT COUNT(*)
		                      FROM BKG_DOC_PROC_SKD
		                     WHERE BKG_DOC_PROC_TP_CD IN('CNTCFM', 'CNTRLS')
		                       AND DOC_PERF_DELT_FLG = 'N'
		                       AND BKG_NO = BK.BKG_NO
		                       AND ROWNUM = 1
		                   ) AS CNTR_CFM
						  ,BK.INDIV_PSON_FLG
		              FROM BKG_BOOKING BK
		             WHERE 1=1
		               AND BK.BKG_STS_CD <> 'X'
		               AND BK.BKG_CGO_TP_CD <> 'P'
		            #if (${bkg_ofc_cd} != '')
		               AND BK.BKG_OFC_CD = @[bkg_ofc_cd]
		            #end
		            #if (${bkg_sts_cd} != '')
		               AND BK.BKG_STS_CD = @[bkg_sts_cd]
		            #end
		            #if (${bkg_rt_sts_cd} != '' && ${bkg_rt_sts_cd} != 'A')
		               AND BKG_RATE_RESULT_FNC(BK.BKG_NO)  = @[bkg_rt_sts_cd]
		            #end
		            #if (${bkg_staff} != '')
		               AND BK.DOC_USR_ID LIKE @[bkg_staff] || '%%'
		            #end
		            #if (${pol_cd} != '')
		               AND BK.POL_CD = @[pol_cd]
		            #end
		            #if (${vvd_cd} != '')
		               AND BK.VSL_CD =  SUBSTR(@[vvd_cd],1,4)
		               AND BK.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
		               AND BK.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
		            #end
		            #if (${slan_cd} != 'All')
		               AND BK.SLAN_CD = @[slan_cd]
                    #end
                   ) BK
                  ,BKG_CUSTOMER C
                  ,BKG_VVD         VVD
                  ,VSK_VSL_PORT_SKD VPS
                  ,(SELECT ROWNUM
                          ,REGION
                          ,GSO
                          ,OFC_CD
                      FROM BKG_OFC_LVL_V
                   ) OLA
                #if (${rev_status} != '')
                  ,(
                    SELECT S.*, DECODE(NVL(REV_LANE, 'N'), 'N', 'N', 'Y') REV_STATUS
                      FROM MDM_VSL_SVC_LANE S
                          ,(
                            SELECT DISTINCT SUBSTR(RLANE_CD, 1, 3) REV_LANE
                              FROM MDM_DTL_REV_LANE
                             WHERE DELT_FLG = 'N'
                            ) R
                     WHERE S.VSL_SLAN_CD = R.REV_LANE(+)) REV
                #end
             WHERE BK.BKG_OFC_CD = OLA.OFC_CD
               AND OLA.REGION IS NOT NULL
               AND C.BKG_NO = BK.BKG_NO
               AND C.BKG_CUST_TP_CD = 'C'
               AND BK.BKG_NO           =  VVD.BKG_NO
               AND BK.POL_CD           =  VVD.POL_CD
               AND VPS.VSL_CD          =  VVD.VSL_CD
               AND VPS.SKD_VOY_NO      =  VVD.SKD_VOY_NO
               AND VPS.SKD_DIR_CD      =  VVD.SKD_DIR_CD
               AND VPS.CLPT_IND_SEQ    =  VVD.POL_CLPT_IND_SEQ
               AND VPS.VPS_PORT_CD     =  VVD.POL_CD
            #if (${fr_dt} != '')
               AND VPS.VPS_ETD_DT >= TO_DATE(@[fr_dt], 'YYYY-MM-DD')
            #end
            #if (${to_dt} != '')
               AND VPS.VPS_ETD_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
            #end
            #if (${rhq_cd} != '')
               AND OLA.REGION = @[rhq_cd]
            #end
            #if (${gso} != '')
               AND OLA.GSO = @[gso]
            #end
            #if (${rev_status} != '')
               AND NVL(REV.REV_STATUS, 'N') = @[rev_status]
            #end
            #if (${rev_status} != '')
               AND VPS.SLAN_CD = REV.VSL_SLAN_CD
            #end
		    #if (${cntr_cfm} == 'Y')
		       AND BK.CNTR_CFM > 0
		    #elseif (${cntr_cfm} == 'N')
		       AND BK.CNTR_CFM = 0
		    #end
            )
    GROUP BY REGION, GSO, OFC_CD
    ORDER BY REGION, GSO, OFC_CD
#end			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_sts_cd" type="12" value="" out="N"/>
				<param name="bkg_rt_sts_cd" type="12" value="" out="N"/>
				<param name="bkg_staff" type="12" value="" out="N"/>
				<param name="fr_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="gso" type="12" value="" out="N"/>
				<param name="rev_status" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
