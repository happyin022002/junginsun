<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOSearchUCbyCustomerListRSQL">
			<desc><![CDATA[[CSR 1795] MAS chassis 비용 로직 추정 방식 변경 - 기초 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT COST_YRMON, CTRT_NO, SCC_CD, CTRT_SEQ
     , USA_IO_BND_CD, BKG_RCV_DE_TERM_CD, STND_UC_AMT, DELT_FLG
     , CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT
FROM   MAS_CHSS_EXPT_CUST_LIST
WHERE  COST_YRMON  BETWEEN SUBSTR(@[f_fm_mon],1,6)
                   AND     SUBSTR(@[f_to_mon],1,6)
  AND  DELT_FLG = 'N'
#if (${f_sc_no} != '')
	AND    CTRT_NO = NVL(@[f_sc_no], CTRT_NO)
#end
ORDER BY COST_YRMON, CTRT_NO, SCC_CD			]]></sql>
			<params>
				<param name="f_fm_mon" type="12" value="" out="N"/>
				<param name="f_to_mon" type="12" value="" out="N"/>
				<param name="f_sc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
