<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOManageHJSSalesAmountUSQL">
			<desc><![CDATA[Sales/Slot Cht-out sheet 수정]]></desc>
			<sql><![CDATA[
UPDATE	MAS_VVD_HIR X
SET		X.NTWK_HIR_COST_AMT = DECODE(X.STND_COST_CD,
									'53101000', @[amt_1_01],
                                	'53102000', @[amt_1_02],
                                	'53200000', @[amt_1_03],
									'54100000', @[amt_1_04],
                                	'54250000', @[amt_1_05],
                                	'54300000', @[amt_1_06],
                                	'54200000', @[amt_1_07],
                                	'54150000', @[amt_1_08],
                                	'54450000', @[amt_1_09],
                                	'54180000', @[amt_1_10],
                                	'54550000', @[amt_1_11],
                                	'54350000', @[amt_1_12],
                                	'54400000', @[amt_1_13],
                                	'72100000', @[amt_1_14]),
		X.UPD_USR_ID		= @[upd_usr_id]
WHERE	1 = 1
AND		X.TRD_CD		= @[trd_cd]
AND		X.RLANE_CD		= @[rlane_cd]
AND		X.IOC_CD		= @[ioc_cd]
AND		X.VSL_CD		= @[vsl_cd]
AND		X.SKD_VOY_NO	= @[skd_voy_no]
AND		X.DIR_CD		= @[dir_cd]			]]></sql>
			<params>
				<param name="amt_1_01" type="12" value="" out="N"/>
				<param name="amt_1_02" type="12" value="" out="N"/>
				<param name="amt_1_03" type="12" value="" out="N"/>
				<param name="amt_1_04" type="12" value="" out="N"/>
				<param name="amt_1_05" type="12" value="" out="N"/>
				<param name="amt_1_06" type="12" value="" out="N"/>
				<param name="amt_1_07" type="12" value="" out="N"/>
				<param name="amt_1_08" type="12" value="" out="N"/>
				<param name="amt_1_09" type="12" value="" out="N"/>
				<param name="amt_1_10" type="12" value="" out="N"/>
				<param name="amt_1_11" type="12" value="" out="N"/>
				<param name="amt_1_12" type="12" value="" out="N"/>
				<param name="amt_1_13" type="12" value="" out="N"/>
				<param name="amt_1_14" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
