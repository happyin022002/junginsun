<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDBDAOSearchExchangeRateRSQL">
			<desc><![CDATA[ExternalFinderDBDAOSearchExchangeRateRSQL]]></desc>
			<sql><![CDATA[
SELECT USD_LOCL_XCH_RT					
  FROM GL_MON_XCH_RT GM 					
 WHERE ACCT_XCH_RT_YRMON = @[acct_xch_rt_yrmon]			
   AND ACCT_XCH_RT_LVL = '1'		
   AND CURR_CD = 'KRW'			]]></sql>
			<params>
				<param name="acct_xch_rt_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
