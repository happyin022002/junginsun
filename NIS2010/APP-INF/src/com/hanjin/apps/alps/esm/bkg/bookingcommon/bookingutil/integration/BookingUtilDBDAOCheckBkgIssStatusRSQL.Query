<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOCheckBkgIssStatusRSQL">
			<desc><![CDATA[Check B/L status]]></desc>
			<sql><![CDATA[
select decode(OBL_ISS_FLG,'Y','I', decode(BL_PRF_SHPR_FLG,'Y','C','')) STATUS
from bkg_bl_iss
where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
