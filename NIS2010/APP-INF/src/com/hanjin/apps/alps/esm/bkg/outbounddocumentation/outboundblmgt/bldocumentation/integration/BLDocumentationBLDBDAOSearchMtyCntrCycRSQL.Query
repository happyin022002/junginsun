<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchMtyCntrCycRSQL">
			<desc><![CDATA[Container Cycle No를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT nvl(cntr.cnmv_cyc_no, 0) cnmv_cyc_no
    , nvl(cntr.CNMV_ID_NO, 0) CNMV_ID_NO
    , nvl(cntr.CNMV_YR, ' ') CNMV_YR
    , nvl(cntr.CNMV_STS_CD, ' ') CNMV_STS_CD
    , nvl(cntr.ORG_FM_LOC_CD, ' ') ORG_FM_LOC_CD
FROM  bkg_booking BK, bkg_container cntr
WHERE BK.bkg_no               = @[bkg_no]
AND   NVL(BK.bkg_sts_cd, ' ') <> 'S'
AND   BK.bkg_no 		      = cntr.bkg_no	
AND   cntr.cntr_no            = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
