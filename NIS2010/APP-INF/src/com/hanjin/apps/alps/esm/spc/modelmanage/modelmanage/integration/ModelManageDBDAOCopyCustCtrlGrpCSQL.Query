<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ModelManageDBDAOCopyCustCtrlGrpCSQL">
			<desc><![CDATA[season에서 사용할 control group을 COPY합니다.

2014.01.03 진마리아 [SRM-201341166] Yield Group의 확대]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_MDL_CUST_CTRL_GRP (
    TRD_CD
  , COST_YRWK
  , CUST_CTRL_CD
  , CUST_CTRL_DESC
  , CRE_USR_ID
  , CRE_DT
  , UPD_USR_ID
  , UPD_DT
)
SELECT
    TRD_CD
  , @[new_cost_yrwk]
  , CUST_CTRL_CD
  , CUST_CTRL_DESC
  , @[cre_usr_id]
  , sysdate
  , @[cre_usr_id]
  , sysdate
FROM SPC_MDL_CUST_CTRL_GRP
WHERE 	    TRD_CD = @[trd_cd]
  and COST_YRWK = @[cost_yrwk]
    
  			]]></sql>
			<params>
				<param name="new_cost_yrwk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="cost_yrwk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
