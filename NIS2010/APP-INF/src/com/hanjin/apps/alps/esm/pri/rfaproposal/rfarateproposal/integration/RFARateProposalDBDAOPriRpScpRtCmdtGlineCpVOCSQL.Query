<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtCmdtGlineCpVOCSQL">
			<desc><![CDATA[CMDT입력]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_RT_CMDT (
	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	CMDT_HDR_SEQ
,	CMDT_SEQ
,	PRC_CMDT_TP_CD
,	PRC_CMDT_DEF_CD
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	N1ST_CMNC_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT @[prop_no]
      ,@[amdt_seq]
      ,@[svc_scp_cd]
      ,DENSE_RANK() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ ORDER BY CMDT_HDR_SEQ) AS CMDT_HDR_SEQ
      ,DENSE_RANK() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ ORDER BY CMDT_SEQ) AS CMDT_SEQ
      ,PRC_CMDT_TP_CD
      ,PRC_CMDT_DEF_CD
      ,'I'
      ,'GC'
      ,@[amdt_seq]
      ,@[cre_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE
  FROM (
SELECT C.SVC_SCP_CD, C.GLINE_SEQ, C.CMDT_HDR_SEQ, C.CMDT_SEQ, C.PRC_CMDT_TP_CD, C.PRC_CMDT_DEF_CD
  FROM (SELECT T.SVC_SCP_CD, T.GLINE_SEQ, RANK() OVER(PARTITION BY SVC_SCP_CD ORDER BY EFF_DT DESC) AS RNK
          FROM PRI_RG_MN T
         WHERE T.SVC_SCP_CD = @[svc_scp_cd]
           AND TO_DATE(@[eff_dt], 'YYYY-MM-DD') BETWEEN T.EFF_DT AND T.EXP_DT
           AND T.CFM_FLG = 'Y') A
      ,PRI_RG_RT_CMDT_HDR B
      ,PRI_RG_RT_CMDT C
 WHERE A.SVC_SCP_CD = B.SVC_SCP_CD
   AND A.GLINE_SEQ = B.GLINE_SEQ
   AND B.SVC_SCP_CD = C.SVC_SCP_CD
   AND B.GLINE_SEQ = C.GLINE_SEQ
   AND B.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ
   AND A.RNK = 1
   AND TO_DATE(@[eff_dt], 'YYYY-MM-DD') BETWEEN B.EFF_DT AND B.EXP_DT) T
			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
