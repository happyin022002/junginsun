<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchSREmlAtchFileListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	ATCH.SR_NO
,	ATCH.FAX_LOG_REF_NO
,	ATCH.SR_KND_CD
,	ATCH.EML_ATCH_FILE_SEQ
#if (${conv_pdf_flg} == 'Y')
,   '/a_dpcs/mail/module/BKG/' || DECODE(NVL(EML.FAX_SVR_OFC_CD,' '),' ',EML.RCV_OFC_CD,EML.FAX_SVR_OFC_CD) || '/' || ATCH.CONV_ATCH_FILE_PATH_CTNT AS ATCH_FILE_PATH_CTNT
,	ATCH.CONV_EML_ATCH_FILE_NM AS EML_ATCH_FILE_NM
#else 
,	'/a_dpcs/mail/module/BKG/' || DECODE(NVL(EML.FAX_SVR_OFC_CD,' '),' ',EML.RCV_OFC_CD,EML.FAX_SVR_OFC_CD) || '/' || ATCH.ATCH_FILE_PATH_CTNT AS ATCH_FILE_PATH_CTNT
,	ATCH.EML_ATCH_FILE_NM
#end

,	ATCH.CRE_USR_ID
,	ATCH.CRE_DT
,	ATCH.UPD_USR_ID
,	ATCH.UPD_DT
,	ATCH.EML_ATCH_FILE_SZ_CAPA
,	EML.EML_SUBJ_CTNT
,	EML.EML_ORG_SUBJ_CTNT
FROM BKG_SR_EML_ATCH_FILE ATCH
,	BKG_SR_FAX EML
WHERE	1=1
AND	ATCH.SR_NO(+)	= EML.SR_NO
AND	ATCH.FAX_LOG_REF_NO(+) = EML.FAX_LOG_REF_NO
AND	ATCH.SR_KND_CD(+) = EML.SR_KND_CD
AND	EML.SR_NO = @[sr_no]
AND	EML.FAX_LOG_REF_NO = @[fax_log_ref_no]
AND	EML.SR_KND_CD = @[sr_knd_cd]			]]></sql>
			<params>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="fax_log_ref_no" type="12" value="" out="N"/>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
