<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORfaLocNmRSQL">
			<desc><![CDATA[RFA Location이름 조회]]></desc>
			<sql><![CDATA[
SELECT A.LOC_CD AS CD
      ,A.LOC_NM AS NM
      ,A.SCONTI_CD AS ETC1
      ,(SELECT SCONTI_NM
          FROM MDM_SUBCONTINENT
         WHERE SCONTI_CD = A.SCONTI_CD
           AND ROWNUM = 1) AS ETC2
  FROM MDM_LOCATION A
 WHERE A.LOC_CD = @[cd]
   AND A.DELT_FLG = 'N'
   AND (@[etc4] IS NULL OR @[etc4] = 'B' OR EXISTS (SELECT 'OK'
                                                      FROM MDM_SVC_SCP_LMT S
                                                     WHERE S.RGN_CD = A.RGN_CD
                                                       AND S.SVC_SCP_CD = @[svc_scp_cd]
                                                       AND S.ORG_DEST_CD = @[etc4]
                                                       AND S.DELT_FLG = 'N'))			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
				<param name="etc4" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
