<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOInsertUsIHCGuidelineDetailCSQL">
			<desc><![CDATA[Insert US IHC Guideline Detail]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_IHC_RT
  (SVC_SCP_CD,
   ORG_DEST_TP_CD,
   IHC_TRF_NO,
   AMDT_SEQ,
   IHC_CGO_TP_CD,
   RT_SEQ,
   N1ST_CMNC_AMDT_SEQ,
   PNT_LOC_CD,
   BSE_PORT_LOC_CD,
   HUB_LOC_CD,
   PNT_NOD_CD,
   HUB_NOD_CD,
   BSE_PORT_NOD_CD,
   IHC_COST_LOC_GRP_NO,
   RCV_DE_TERM_CD,
   PRC_TRSP_MOD_CD,
   GLINE_20FT_RAIL_FRT_RT_AMT,
   GLINE_40FT_RAIL_FRT_RT_AMT,
   GLINE_20FT_TRK_FRT_RT_AMT,
   GLINE_40FT_TRK_FRT_RT_AMT,
   GLINE_20FT_FRT_RT_AMT,
   GLINE_40FT_FRT_RT_AMT,
   OPTM_TRSP_MOD_FLG,
   SRC_INFO_CD,
   IHC_RT_RMK,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   USA_COST_TRF_SVC_MOD_CD,
   PRC_TRF_CRE_TP_CD,
   LOCL_CURR_CD)
SELECT  @[svc_scp_cd],
    	@[org_dest_tp_cd],
    	@[ihc_trf_no],
    	@[amdt_seq],
   	    @[ihc_cgo_tp_cd],
      	NVL(MAX(RT_SEQ), 0) + 1,
        @[n1st_cmnc_amdt_seq],
        @[pnt_loc_cd],
        @[bse_port_loc_cd],
        @[hub_loc_cd],
        @[pnt_nod_cd],
        @[hub_nod_cd],
        @[bse_port_nod_cd],
        @[ihc_cost_loc_grp_no],
        @[rcv_de_term_cd],
        @[prc_trsp_mod_cd],
        @[gline_20ft_rail_frt_rt_amt],
        @[gline_40ft_rail_frt_rt_amt],
        @[gline_20ft_trk_frt_rt_amt],
        @[gline_40ft_trk_frt_rt_amt],
        @[gline_20ft_frt_rt_amt],
        @[gline_40ft_frt_rt_amt],
        @[optm_trsp_mod_flg],
        @[src_info_cd],
        @[ihc_rt_rmk],
        @[cre_usr_id],
        SYSDATE,
        @[upd_usr_id],
        SYSDATE,
        @[usa_cost_trf_svc_mod_cd],
        @[prc_trf_cre_tp_cd],
        @[locl_curr_cd]
  FROM PRI_TRF_IHC_RT
 WHERE SVC_SCP_CD       = @[svc_scp_cd] 
   AND ORG_DEST_TP_CD   = @[org_dest_tp_cd]
   AND IHC_TRF_NO       = @[ihc_trf_no]
   AND AMDT_SEQ         = @[amdt_seq]
   AND IHC_CGO_TP_CD    = @[ihc_cgo_tp_cd]      			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="ihc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="n1st_cmnc_amdt_seq" type="12" value="" out="N"/>
				<param name="pnt_loc_cd" type="12" value="" out="N"/>
				<param name="bse_port_loc_cd" type="12" value="" out="N"/>
				<param name="hub_loc_cd" type="12" value="" out="N"/>
				<param name="pnt_nod_cd" type="12" value="" out="N"/>
				<param name="hub_nod_cd" type="12" value="" out="N"/>
				<param name="bse_port_nod_cd" type="12" value="" out="N"/>
				<param name="ihc_cost_loc_grp_no" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="prc_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="gline_20ft_rail_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_40ft_rail_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_20ft_trk_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_40ft_trk_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_20ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_40ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="optm_trsp_mod_flg" type="12" value="" out="N"/>
				<param name="src_info_cd" type="12" value="" out="N"/>
				<param name="ihc_rt_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usa_cost_trf_svc_mod_cd" type="12" value="" out="N"/>
				<param name="prc_trf_cre_tp_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
