<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOEBookingBkgDgCgoUSQL">
			<desc><![CDATA[EBookingBkgDgCgo]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')

UPDATE BKG_DG_CGO_HIS SET 
	CNTR_NO = @[cntr_no]
,	DG_CNTR_SEQ = @[dg_cntr_seq]
,	CNTR_CGO_SEQ = @[cntr_cgo_seq]
,	CNTR_TPSZ_CD = @[cntr_tpsz_cd]
,	IMDG_UN_NO = @[imdg_un_no]
,	IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
,	PRP_SHP_NM = @[prp_shp_nm]
,	HZD_DESC = @[hzd_desc]
,	FLSH_PNT_CDO_TEMP = @[flsh_pnt_cdo_temp]
,	MRN_POLUT_FLG = @[mrn_polut_flg]
,	GRS_WGT = @[grs_wgt]
,	NET_WGT = @[net_wgt]
,	WGT_UT_CD = @[wgt_ut_cd]
,	EMER_CNTC_PSON_NM = @[emer_cntc_pson_nm]
,	DCGO_STS_CD = @[dcgo_sts_cd]
,	IMDG_LMT_QTY_FLG = @[imdg_lmt_qty_flg]
,	SPCL_STWG_RQST_DESC = @[spcl_stwg_rqst_desc]
,	EMS_NO = (SELECT MAX(IMDG_EMER_NO) FROM SCG_IMDG_UN_NO WHERE IMDG_UN_NO = @[imdg_un_no] AND IMDG_CLSS_CD = @[imdg_clss_cd])
,	EMER_RSPN_GID_NO = (SELECT MAX(EMER_RSPN_GID_NO) FROM SCG_IMDG_UN_NO WHERE IMDG_UN_NO = @[imdg_un_no] AND IMDG_CLSS_CD = @[imdg_clss_cd])
,	EMER_RSPN_GID_CHR_NO = (SELECT MAX(EMER_RSPN_GID_CHR_NO) FROM SCG_IMDG_UN_NO WHERE IMDG_UN_NO = @[imdg_un_no] AND IMDG_CLSS_CD = @[imdg_clss_cd])
,	HZD_CTNT = (
            SELECT SUBSTR(XMLAGG(XMLELEMENT(NM, '|' || M.IMDG_SEGR_GRP_NM) ORDER BY M.IMDG_SEGR_GRP_NO).EXTRACT('//text()'), 2)
            FROM   SCG_IMDG_SEGR_GRP_DTL D
                  ,SCG_IMDG_SEGR_GRP M
            WHERE  1 = 1
            AND    D.IMDG_SEGR_GRP_NO = M.IMDG_SEGR_GRP_NO
            AND    D.IMDG_UN_NO = @[imdg_un_no]
         )
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,   IMDG_COMP_GRP_CD = @[imdg_comp_grp_cd]
,   EMER_CNTC_PHN_NO_CTNT = @[emer_cntc_phn_no_ctnt]
, OUT_IMDG_PCK_CD1 = @[out_imdg_pck_cd1]
, OUT_IMDG_PCK_QTY1 = @[out_imdg_pck_qty1]
,   PSA_NO = @[psa_no]
WHERE	BKG_NO = @[bkg_no]
AND	DCGO_SEQ = @[dcgo_seq]
AND NVL(SPCL_CGO_APRO_CD,' ') NOT IN ('R', 'Y', 'N')
AND	CORR_NO = 'TMP0000001'

#else

UPDATE BKG_DG_CGO SET 
	CNTR_NO = @[cntr_no]
,	DG_CNTR_SEQ = @[dg_cntr_seq]
,	CNTR_CGO_SEQ = @[cntr_cgo_seq]
,	CNTR_TPSZ_CD = @[cntr_tpsz_cd]
,	IMDG_UN_NO = @[imdg_un_no]

,	IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
,	PRP_SHP_NM = @[prp_shp_nm]
,	HZD_DESC = @[hzd_desc]
,	FLSH_PNT_CDO_TEMP = @[flsh_pnt_cdo_temp]
,	MRN_POLUT_FLG = @[mrn_polut_flg]
,	GRS_WGT = @[grs_wgt]
,	NET_WGT = @[net_wgt]
,	WGT_UT_CD = @[wgt_ut_cd]
,	EMER_CNTC_PSON_NM = @[emer_cntc_pson_nm]
,	DCGO_STS_CD = @[dcgo_sts_cd]
,	IMDG_LMT_QTY_FLG = @[imdg_lmt_qty_flg]
,	SPCL_STWG_RQST_DESC = @[spcl_stwg_rqst_desc]
,	EMS_NO = (SELECT MAX(IMDG_EMER_NO) FROM SCG_IMDG_UN_NO WHERE IMDG_UN_NO = @[imdg_un_no] AND IMDG_CLSS_CD = @[imdg_clss_cd])
,	EMER_RSPN_GID_NO = (SELECT MAX(EMER_RSPN_GID_NO) FROM SCG_IMDG_UN_NO WHERE IMDG_UN_NO = @[imdg_un_no] AND IMDG_CLSS_CD = @[imdg_clss_cd])
,	EMER_RSPN_GID_CHR_NO = (SELECT MAX(EMER_RSPN_GID_CHR_NO) FROM SCG_IMDG_UN_NO WHERE IMDG_UN_NO = @[imdg_un_no] AND IMDG_CLSS_CD = @[imdg_clss_cd])
,	HZD_CTNT = (
            SELECT SUBSTR(XMLAGG(XMLELEMENT(NM, '|' || M.IMDG_SEGR_GRP_NM) ORDER BY M.IMDG_SEGR_GRP_NO).EXTRACT('//text()'), 2)
            FROM   SCG_IMDG_SEGR_GRP_DTL D
                  ,SCG_IMDG_SEGR_GRP M
            WHERE  1 = 1
            AND    D.IMDG_SEGR_GRP_NO = M.IMDG_SEGR_GRP_NO
            AND    D.IMDG_UN_NO = @[imdg_un_no]
         )
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,   IMDG_COMP_GRP_CD = @[imdg_comp_grp_cd]
,   EMER_CNTC_PHN_NO_CTNT = @[emer_cntc_phn_no_ctnt]
, OUT_IMDG_PCK_CD1 = @[out_imdg_pck_cd1]
, OUT_IMDG_PCK_QTY1 = @[out_imdg_pck_qty1]
,   PSA_NO = @[psa_no]
WHERE	BKG_NO = @[bkg_no]
AND NVL(SPCL_CGO_APRO_CD,' ') NOT IN ('R', 'Y', 'N')
AND	DCGO_SEQ = @[dcgo_seq]

#end			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="dg_cntr_seq" type="12" value="" out="N"/>
				<param name="cntr_cgo_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
				<param name="prp_shp_nm" type="12" value="" out="N"/>
				<param name="hzd_desc" type="12" value="" out="N"/>
				<param name="flsh_pnt_cdo_temp" type="6" value="" out="N"/>
				<param name="mrn_polut_flg" type="12" value="" out="N"/>
				<param name="grs_wgt" type="6" value="" out="N"/>
				<param name="net_wgt" type="6" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="emer_cntc_pson_nm" type="12" value="" out="N"/>
				<param name="dcgo_sts_cd" type="12" value="" out="N"/>
				<param name="imdg_lmt_qty_flg" type="12" value="" out="N"/>
				<param name="spcl_stwg_rqst_desc" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="imdg_comp_grp_cd" type="12" value="" out="N"/>
				<param name="emer_cntc_phn_no_ctnt" type="12" value="" out="N"/>
				<param name="out_imdg_pck_cd1" type="12" value="" out="N"/>
				<param name="out_imdg_pck_qty1" type="12" value="" out="N"/>
				<param name="psa_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dcgo_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
