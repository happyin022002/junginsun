<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOLocByBkgRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT A1.SYS_AREA_GRP_ID
FROM   COM_SYS_AREA_GRP_ID A1, BKG_BOOKING A2
WHERE  A2.BKG_NO = @[bkg_no]
AND    A2.POL_CD LIKE A1.CNT_CD || '%'
AND    A1.CO_IND_CD = 'H'
AND    A1.SVR_USD_FLG = 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
