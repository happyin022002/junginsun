<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqBreakbulkDBDAOmodifyPriScqBbCgoUSQL">
			<desc><![CDATA[modifyPriScqBbCgo]]></desc>
			<sql><![CDATA[
UPDATE PRI_SCQ_BB_CGO                       
   SET CMDT_CD     = @[cmdt_cd]
     , TTL_DIM_LEN = @[ttl_dim_len]
     , TTL_DIM_WDT = @[ttl_dim_wdt]
     , TTL_DIM_HGT = @[ttl_dim_hgt]
     , GRS_WGT     = @[grs_wgt]
     , WGT_UT_CD   = @[wgt_ut_cd]                  
     , UPD_USR_ID  = @[upd_usr_id]      
     , UPD_DT      = SYSDATE            
 WHERE SCQ_RQST_NO = @[scq_rqst_no]          
   AND SCQ_VER_NO  = @[scq_ver_no]           
   AND CGO_SEQ     = @[cgo_seq]			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="ttl_dim_len" type="12" value="" out="N"/>
				<param name="ttl_dim_wdt" type="12" value="" out="N"/>
				<param name="ttl_dim_hgt" type="12" value="" out="N"/>
				<param name="grs_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no" type="12" value="" out="N"/>
				<param name="cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
