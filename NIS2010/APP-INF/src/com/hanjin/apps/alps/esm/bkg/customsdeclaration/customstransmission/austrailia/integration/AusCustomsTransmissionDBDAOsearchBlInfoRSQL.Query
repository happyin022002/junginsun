<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchBlInfoRSQL">
			<desc><![CDATA[호주 위험물 컨테이너 & bl 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
   BL_NO
   , '' SHIPPING_REF
   ,MAX(POL_CD) POL_CD
   ,MAX(POD_CD) POD_CD
   , @[d_type] AS D_TYPE
   , @[vvd_cd] AS VVD_CD
   , @[port_cd] AS PORT_CD
FROM BKG_CSTMS_DG A
WHERE  A.DG_DECL_TP_CD  = @[d_type]
AND   A.VSL_CD          = SUBSTR(@[vvd_cd], 1, 4)
AND   A.SKD_VOY_NO      = SUBSTR(@[vvd_cd], 5, 4)
AND   A.SKD_DIR_CD      = SUBSTR(@[vvd_cd], 9, 1)
AND   A.PORT_CD         =  @[port_cd]
AND	  A.CNT_CD ='AU'
GROUP BY BL_NO
ORDER BY BL_NO   			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
