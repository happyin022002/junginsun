<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchEdoCntrPtyTrspRSQL">
			<desc><![CDATA[UI_BKG_0133에서 사용하는 SQL문이다.]]></desc>
			<sql><![CDATA[
SELECT *
FROM ( SELECT 
              NVL(EC.EDO_RQST_NO,@[rqstNo])  AS EDO_RQST_NO
            , BC.CNTR_NO                     AS CNTR_NO
            , EDO_CNTR_PTY_TRSP_SEQ
            , RANK() OVER ( PARTITION BY BC.CNTR_NO ORDER BY EDO_CNTR_PTY_TRSP_SEQ DESC) SEQ    
            , EC.EDO_ACK_CD                  AS EDO_ACK_CD 
			, PTY_BIZ_NO                     AS PTY_BIZ_NO
            , PTY_NM                         AS PTY_NM
            , PTY_CNTC_PSON_NM               AS PTY_CNTC_PSON_NM
            , PTY_PHN_NO                     AS PTY_PHN_NO
            , PTY_FAX_NO                     AS PTY_FAX_NO 
            , PTY_EML                        AS PTY_EML 
            , ACT_SHPR_NM                    AS ACT_SHPR_NM 
            , ACT_SHPR_PHN_NO                AS ACT_SHPR_PHN_NO
            , DEP_AREA_NM                    AS DEP_AREA_NM
            , ARR_AREA_NM                    AS ARR_AREA_NM 

           FROM   BKG_CONTAINER BC, BKG_EDO_CNTR_PTY_TRSP EC
         WHERE EC.CNTR_NO(+)     = BC.CNTR_NO
           AND EC.EDO_RQST_NO(+) = @[rqstNo]
           AND BC.BKG_NO         = (SELECT BKG_NO
                                      FROM BKG_EDO_MST EDOMST
                                     WHERE EDOMST.EDO_RQST_NO = @[rqstNo]
                                       AND ROWNUM =1
                                    )

     )
WHERE SEQ = 1 /* 가장 최근 수신된 것 가져옴*/			]]></sql>
			<params>
				<param name="rqstNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
