<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOcheckHoldRemvQtyRSQL">
			<desc><![CDATA[checkHoldRemvQty]]></desc>
			<sql><![CDATA[
SELECT NVL(SUM(
               CASE WHEN TB.HOLD_QTY <= TB.REMV_QTY THEN 0
                    ELSE 1
                END
          ),0) AS HOLD_REMV_QTY
  FROM (
		SELECT SUM(HOLD.CNTR_QTY) AS HOLD_QTY
		      ,(
				SELECT SUM(RS1.CNTR_QTY)
				  FROM BKG_CSTMS_ADV_RSLT RS1
				 WHERE RS1.CNT_CD = 'US'
				   AND RS1.BL_NO = @[bl_no]
				   AND HOLD.REMV_CD LIKE '%' ||  RS1.DSPO_CD || '%'
				   AND NVL(RS1.RSND_FLG, 'N') <> 'Y'
				   AND RS1.BL_NVOCC_TP_CD = 'M'
				) REMV_QTY
		      ,HOLD.DSPO_CD
		  FROM (
				SELECT RS.CNTR_QTY
				      ,RS.DSPO_CD
				      ,CD.ATTR_CTNT4 AS REMV_CD
				  FROM BKG_CSTMS_ADV_RSLT RS
				      ,BKG_CSTMS_CD_CONV_CTNT CD
				 WHERE RS.CNT_CD = 'US'
				   AND RS.BL_NO = @[bl_no]
				   AND RS.CNT_CD = CD.CNT_CD
				   AND RS.DSPO_CD = CD.ATTR_CTNT3
				   AND NVL(RS.RSND_FLG,'N') <> 'Y'
				   AND RS.BL_NVOCC_TP_CD = 'M'
				   AND CD.CSTMS_DIV_ID = 'CARGO_RLS_H_CD'
#if (${cstms_seq} != '') 
                   AND RS.CSTMS_SEQ NOT IN (${cstms_seq})
#end
				) HOLD
		GROUP BY HOLD.DSPO_CD, HOLD.REMV_CD
		) TB			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
