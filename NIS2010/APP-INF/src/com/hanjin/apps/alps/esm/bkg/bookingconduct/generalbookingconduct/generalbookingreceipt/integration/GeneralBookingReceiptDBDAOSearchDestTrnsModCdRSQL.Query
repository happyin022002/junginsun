<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchDestTrnsModCdRSQL">
			<desc><![CDATA[dest_trns_mod_cd를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DEST_TRNS_MOD_CD 
FROM BKG_BOOKING 
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
