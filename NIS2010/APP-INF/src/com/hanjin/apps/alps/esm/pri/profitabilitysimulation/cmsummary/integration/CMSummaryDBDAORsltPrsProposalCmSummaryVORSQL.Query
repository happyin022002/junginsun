		, MN.CTRT_CUST_CNT_CD AS CUST_CNT_CD
		, MN.CTRT_CUST_SEQ AS CUST_SEQ
		, MN.TAA_PROP_NO
		, MN.AMDT_SEQ
		, MN.EFF_DT AS CTRT_EFF_DT, MN.EXP_DT AS CTRT_EXP_DT
		, (TO_DATE(TO_CHAR(MN.EXP_DT,'YYYYMMDD'),'YYYYMMDD')- TO_DATE(TO_CHAR(MN.EFF_DT,'YYYYMMDD'),'YYYYMMDD')) / 7 AS WEEK_NEW	-- WEEK(NEW)
		, CUST.CUST_LGL_ENG_NM AS CTRT_PTY_NM--PTY.CTRT_PTY_NM --  -- CUSTOMER NAME
