<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOmodifyBkgHrdCdgCtntUSQL">
			<desc><![CDATA[u]]></desc>
			<sql><![CDATA[
UPDATE BKG_HRD_CDG_CTNT
SET
 ATTR_CTNT2 = @[attr_ctnt2]
,ATTR_CTNT3 = @[attr_ctnt3]
,UPD_USR_ID = @[upd_usr_id]
,UPD_DT     = SYSDATE
WHERE 1=1
AND HRD_CDG_ID = @[hrd_cdg_id]
AND HRD_CDG_ID_SEQ = @[hrd_cdg_id_seq]
			]]></sql>
			<params>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="attr_ctnt3" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="hrd_cdg_id" type="12" value="" out="N"/>
				<param name="hrd_cdg_id_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
