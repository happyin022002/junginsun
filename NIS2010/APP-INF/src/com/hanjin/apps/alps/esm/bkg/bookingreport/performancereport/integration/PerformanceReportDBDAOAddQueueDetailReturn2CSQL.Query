<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOAddQueueDetailReturn2CSQL">
			<desc><![CDATA[PerformanceReportDBDAOAddQueueDetailReturn2CSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_SR_AMD_RSN
(   SR_KND_CD,
    SR_NO,
    BKG_NO,
    SR_AMD_TP_CD,
    SR_AMD_SEQ,
    SR_AMD_RSN_TP_CD, SR_AMD_RSN_CD, SR_AMD_RSN_SEQ, /* 향후 삭제*/
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
VALUES
(
	 SUBSTR(@[src_cd],1,1),
     @[sr_no],
     @[bkg_no],
     @[sr_knd_cd]/* 0421의 SR_KND_CD*//* 파라메터로 SR_AMD_TP_CD 받아옴 */,
    (SELECT NVL(MAX(SR_AMD_SEQ) + 1,0) 
                      FROM BKG_SR_AMD_RSN
                     WHERE SR_KND_CD    = @[src_cd] /* 0421의 SRC_CD*/
			  						   AND SR_NO        = @[sr_no]
										   AND BKG_NO       = @[bkg_no]
										   AND SR_AMD_TP_CD = @[sr_knd_cd]/* 0421의 SR_KND_CD*/
     )
    ,
    ' ', ' ', 0, --NULL, NULL , 0, /* 향후 삭제*/
     @[usr_id],
     SYSDATE,
     @[usr_id],
     SYSDATE   
)			]]></sql>
			<params>
				<param name="src_cd" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
