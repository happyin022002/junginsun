<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchSpclCgoSumByMlbRSQL">
			<desc><![CDATA[searchSpclCgoSumByMlb]]></desc>
			<sql><![CDATA[
SELECT *
FROM (

#if (${in_pgm_no} == 'ESM_BKG_0951') 
/**************************** ESM_BKG_0951에서 조회된 경우  *****************************/

SELECT	
	
	BLCK_STWG_CD GUBUN_CD,     
	DG.POD_CD GUBUN_CD2,                                                   
	DECODE(DG.POD_CD, 'USLGB', '1', 2) ORDER_GUBUN,
	DECODE(BLCK_STWG_CD, 'LOC', '0', 'ONE', '1', 'TWO', '2', 'THR', '3', 'FOR', '4', 'FIV', '4.5', 'SEV', '4.7', 'HOT','5','TRS', '6', BLCK_STWG_CD) ORDER_GUBUN2,
	DECODE(SUM(LOCAL_DG_40 + LOCAL_DG_45),0,'',SUM(LOCAL_DG_40 + LOCAL_DG_45))	LOCAL_DG_40,
	DECODE(SUM(LOCAL_DG_20),0,'',SUM(LOCAL_DG_20)) LOCAL_DG_20,
	DECODE(SUM(LOCAL_DG_40H),0,'',SUM(LOCAL_DG_40H)) LOCAL_DG_40H,

	DECODE(SUM(LOCAL_RF_40 + LOCAL_RF_45),0,'',SUM(LOCAL_RF_40 + LOCAL_RF_45))	LOCAL_RF_40,
	DECODE(SUM(LOCAL_RF_20),0,'',SUM(LOCAL_RF_20)) LOCAL_RF_20,
	DECODE(SUM(LOCAL_RF_40H_5 + LOCAL_RF_40H_8 + LOCAL_RF_40H_9),0,'',SUM(LOCAL_RF_40H_5 + LOCAL_RF_40H_8 + LOCAL_RF_40H_9)) LOCAL_RF_40H,

	DECODE(SUM(LOCAL_FR_40 + LOCAL_RF_45),0,'',SUM(LOCAL_FR_40 + LOCAL_RF_45))	LOCAL_FR_40,
	DECODE(SUM(LOCAL_FR_20),0,'',SUM(LOCAL_FR_20)) LOCAL_FR_20,
	DECODE(SUM(LOCAL_FR_40H),0,'',SUM(LOCAL_FR_40H)) LOCAL_FR_40H,

	DECODE(SUM(LOCAL_OT_40),0,'',SUM(LOCAL_OT_40)) LOCAL_OT_40,
	DECODE(SUM(LOCAL_OT_20),0,'',SUM(LOCAL_OT_20))	LOCAL_OT_20,
    DECODE(SUM(LOCAL_OT_40H),0,'',SUM(LOCAL_OT_40H)) LOCAL_OT_40H,

	DECODE(SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45),0,'',SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45))	LOCAL_PCOD_40,
	DECODE(SUM(LOCAL_PCOD_20),0,'',SUM(LOCAL_PCOD_20)) LOCAL_PCOD_20,

	DECODE(SUM(TS_DG_40 + TS_DG_45),0,'',SUM(TS_DG_40 + TS_DG_45))	TS_DG_40,
	DECODE(SUM(TS_DG_20),0,'',SUM(TS_DG_20)) TS_DG_20,
	DECODE(SUM(TS_DG_40H),0,'',SUM(TS_DG_40H)) TS_DG_40H,

	DECODE(SUM(TS_RF_40 + TS_RF_45),0,'',SUM(TS_RF_40 + TS_RF_45))	TS_RF_40,
	DECODE(SUM(TS_RF_20),0,'',SUM(TS_RF_20)) TS_RF_20,
	DECODE(SUM(TS_RF_40H_5 + TS_RF_40H_8 + TS_RF_40H_9),0,'',SUM(TS_RF_40H_5 + TS_RF_40H_8 + TS_RF_40H_9)) TS_RF_40H,

	DECODE(SUM(TS_FR_40 + TS_FR_45),0,'',SUM(TS_FR_40 + TS_FR_45))	TS_FR_40,
	DECODE(SUM(TS_FR_20),0,'',SUM(TS_FR_20)) TS_FR_20,
	DECODE(SUM(TS_FR_40H),0,'',SUM(TS_FR_40H)) TS_FR_40H,


	DECODE(SUM(TS_OT_40),0,'',SUM(TS_OT_40))	TS_OT_40,
	DECODE(SUM(TS_OT_20),0,'',SUM(TS_OT_20))	TS_OT_20,
    DECODE(SUM(TS_OT_40H),0,'',SUM(TS_OT_40H))	TS_OT_40H,


	DECODE(SUM(TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45),0,'',SUM(TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45))	TS_PCOD_40,
	DECODE(SUM(TS_PCOD_20),0,'',SUM(TS_PCOD_20))	TS_PCOD_20,

	DECODE(SUM(LOCAL_DG_40 + LOCAL_DG_45 + TS_DG_40 + TS_DG_45),0,'',SUM(LOCAL_DG_40 + LOCAL_DG_45 + TS_DG_40 + TS_DG_45)) SUM_DG_40,
	DECODE(SUM(LOCAL_DG_20 + TS_DG_20),0,'',SUM(LOCAL_DG_20 + TS_DG_20)) SUM_DG_20,
	DECODE(SUM(LOCAL_DG_40H + TS_DG_40H),0,'',SUM(LOCAL_DG_40H + TS_DG_40H)) SUM_DG_40H,

	DECODE(SUM(LOCAL_RF_40 + LOCAL_RF_45 + TS_RF_40 + TS_RF_45),0,'',SUM(LOCAL_RF_40 + LOCAL_RF_45 + TS_RF_40 + TS_RF_45)) SUM_RF_40,
	DECODE(SUM(LOCAL_RF_20 + TS_RF_20),0,'',SUM(LOCAL_RF_20 + TS_RF_20)) SUM_RF_20,
	DECODE(SUM(LOCAL_RF_40H_5 + LOCAL_RF_40H_8 + LOCAL_RF_40H_9 + TS_RF_40H_5 + TS_RF_40H_8 + TS_RF_40H_9),0,'',SUM(LOCAL_RF_40H_5 + LOCAL_RF_40H_8 + LOCAL_RF_40H_9 + TS_RF_40H_5 + TS_RF_40H_8 + TS_RF_40H_9)) SUM_RF_40H,

	DECODE(SUM(LOCAL_FR_40 + LOCAL_FR_45 + TS_FR_40 + TS_FR_45),0,'',SUM(LOCAL_FR_40 + LOCAL_FR_45 + TS_FR_40 + TS_FR_45)) SUM_FR_40,
	DECODE(SUM(LOCAL_FR_20 + TS_FR_20),0,'',SUM(LOCAL_FR_20 + TS_FR_20)) SUM_FR_20,
	DECODE(SUM(LOCAL_FR_40H + TS_FR_40H),0,'',SUM(LOCAL_FR_40H + TS_FR_40H)) SUM_FR_40H,

	DECODE(SUM(LOCAL_OT_40 + TS_OT_40),0,'',SUM(LOCAL_OT_40 + TS_OT_40)) SUM_OT_40,
	DECODE(SUM(LOCAL_OT_20 + TS_OT_20),0,'',SUM(LOCAL_OT_20 + TS_OT_20)) SUM_OT_20,
    DECODE(SUM(LOCAL_OT_40H + TS_OT_40H),0,'',SUM(LOCAL_OT_40H + TS_OT_40H)) SUM_OT_40H,

	DECODE(SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45 + TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45),0,'',SUM(LOCAL_PCOD_40 + LOCAL_PCOD_40H + LOCAL_PCOD_45 + TS_PCOD_40 + TS_PCOD_40H + TS_PCOD_45)) SUM_PCOD_40,
	DECODE(SUM(LOCAL_PCOD_20 + TS_PCOD_20),0,'',SUM(LOCAL_PCOD_20 + TS_PCOD_20)) SUM_PCOD_20
FROM 
    	(SELECT NVL(LO.UN_LOC_CD,BV.POD_CD) POD_CD
    		, BK.BKG_NO BKG_NO
            , DECODE(MAX( DECODE(NVL(BV.POD_CD,' '), 'USSEA', DECODE(BV.SLAN_CD,'PSX',DECODE(DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'ONE','SE1','SEA'),DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC')), 
        							'USOAK', DECODE(BK.BLCK_STWG_CD, 'OA1', 'OA1' ,'' , '', 'OAK'), 
        							'USLGB', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','7','SEV','T','TRS', 'LOC'), 
        							'CAVAN', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'')), NULL, MAX(DECODE(BK.POD_CD,'XXXXX','XXXXX',NVL(LO.UN_LOC_CD,BV.POD_CD))), 
        			 MAX( DECODE(NVL(BV.POD_CD,' '), 'USSEA', DECODE(BV.SLAN_CD,'PSX',DECODE(DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'ONE','SE1','SEA'),DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC')), 
        							'USOAK', DECODE(BK.BLCK_STWG_CD, 'OA1', 'OA1' ,'', '', 'OAK'), 
        							'USLGB', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','7','SEV','T','TRS', 'LOC'), 
        							'CAVAN', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),''))) BLCK_STWG_CD

            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))       LOCAL_DG_20
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',0,'5',0,DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0)),0)) LOCAL_DG_40
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'5',DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))       LOCAL_DG_40H
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))       LOCAL_DG_45
            
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))       TS_DG_20
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',0,'5',0,DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0)))) TS_DG_40
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'5',DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))       TS_DG_40H
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',DECODE(BQ.DCGO_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))       TS_DG_45

    	FROM BKG_BOOKING BK
    		, BKG_VVD BV
    		, VSK_VSL_PORT_SKD VPS
    		, BKG_DG_CGO DG
    		, MDM_LOCATION LO
    		, BKG_QTY_DTL BQ
    	WHERE BK.BKG_NO = BV.BKG_NO
    	  AND BV.VSL_CD = VPS.VSL_CD
    	  AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
    	  AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
    	  AND BV.POD_CD = VPS.VPS_PORT_CD
    	  AND VPS.VPS_PORT_CD = LO.LOC_CD
    	  AND BK.BKG_NO = DG.BKG_NO(+)

		  AND BV.VSL_CD     = @[in_vsl_cd]
		  AND BV.SKD_VOY_NO = @[in_skd_voy_no]
		  AND BV.SKD_DIR_CD = @[in_skd_dir_cd]
		  AND BV.POL_CD     = @[in_pol_cd]
		  AND SUBSTR(BV.POL_YD_CD, 6, 2) like @[in_pol_yd_cd]||'%'

    	  AND BK.BKG_STS_CD IN ( 'F','W')
    	  AND VPS.CLPT_IND_SEQ='1'
    	  AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
    	  AND BK.BKG_NO = BQ.BKG_NO
    	GROUP BY NVL(LO.UN_LOC_CD,BV.POD_CD)
    		, BK.BKG_NO) DG, 
    
    	(SELECT NVL(LO.UN_LOC_CD,BV.POD_CD) POD_CD
    		, BK.BKG_NO BKG_NO
    
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'R2',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))        LOCAL_RF_20
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'R4',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))        LOCAL_RF_40
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'R5',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))        LOCAL_RF_40H_5
	    , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'R8',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))        LOCAL_RF_40H_8
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'R9',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))        LOCAL_RF_40H_9
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'R7',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0),0))        LOCAL_RF_45
            
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(BQ.CNTR_TPSZ_CD,'R2',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))        TS_RF_20
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(BQ.CNTR_TPSZ_CD,'R4',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))        TS_RF_40
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(BQ.CNTR_TPSZ_CD,'R5',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))        TS_RF_40H_5
	    , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(BQ.CNTR_TPSZ_CD,'R8',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))        TS_RF_40H_8
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(BQ.CNTR_TPSZ_CD,'R9',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))        TS_RF_40H_9
            , MAX(DECODE(BV.POL_CD,BK.POL_CD, 0,DECODE(BQ.CNTR_TPSZ_CD,'R7',DECODE(BQ.RC_FLG,'Y',BQ.OP_CNTR_QTY,0),0)))        TS_RF_45
            
    	FROM BKG_BOOKING BK
    		, BKG_VVD BV
    		, VSK_VSL_PORT_SKD VPS
    		, BKG_RF_CGO RF
    		, MDM_LOCATION LO
    		, BKG_QTY_DTL BQ
    	WHERE BK.BKG_NO = BV.BKG_NO
    	  AND BV.VSL_CD = VPS.VSL_CD
    	  AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
    	  AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
    	  AND BV.POD_CD = VPS.VPS_PORT_CD
    	  AND VPS.VPS_PORT_CD = LO.LOC_CD
    	  AND BK.BKG_NO = RF.BKG_NO(+)

		  AND BV.VSL_CD     = @[in_vsl_cd]
		  AND BV.SKD_VOY_NO = @[in_skd_voy_no]
		  AND BV.SKD_DIR_CD = @[in_skd_dir_cd]
		  AND BV.POL_CD     = @[in_pol_cd]
		  AND SUBSTR(BV.POL_YD_CD, 6, 2) like @[in_pol_yd_cd]||'%'

    	  AND BK.BKG_STS_CD IN ( 'F','W')
    	  AND VPS.CLPT_IND_SEQ='1'
    	  AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
    	  AND BK.BKG_NO = BQ.BKG_NO
    	GROUP BY NVL(LO.UN_LOC_CD,BV.POD_CD)
    		, BK.BKG_NO 
    	) RF,
    	(SELECT NVL(LO.UN_LOC_CD,BV.POD_CD) POD_CD
    		, BK.BKG_NO BKG_NO
    
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'F2',BQ.OP_CNTR_QTY,'P2',BQ.OP_CNTR_QTY,'A2',BQ.OP_CNTR_QTY,0),0)) LOCAL_FR_20
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'F4',BQ.OP_CNTR_QTY,'P4',BQ.OP_CNTR_QTY,'A4',BQ.OP_CNTR_QTY,0),0)) LOCAL_FR_40
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'F5',BQ.OP_CNTR_QTY,0),0)) LOCAL_FR_40H
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'F7',BQ.OP_CNTR_QTY,0),0)) LOCAL_FR_45
    
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'O2',BQ.OP_CNTR_QTY,'S2',BQ.OP_CNTR_QTY,0),0)) LOCAL_OT_20
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'O4',BQ.OP_CNTR_QTY,'S4',BQ.OP_CNTR_QTY,0),0)) LOCAL_OT_40
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(BQ.CNTR_TPSZ_CD,'O5',BQ.OP_CNTR_QTY,'S5',BQ.OP_CNTR_QTY,0),0)) LOCAL_OT_40H
    		
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0)),0)) LOCAL_PCOD_20
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0)),0)) LOCAL_PCOD_40
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0)),0)) LOCAL_PCOD_40H
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7', BQ.OP_CNTR_QTY,0),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7', BQ.OP_CNTR_QTY,0),0)),0)) LOCAL_PCOD_45
        
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(BQ.CNTR_TPSZ_CD,'F2',BQ.OP_CNTR_QTY,'P2',BQ.OP_CNTR_QTY,'A2',BQ.OP_CNTR_QTY,0))) TS_FR_20
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(BQ.CNTR_TPSZ_CD,'F4',BQ.OP_CNTR_QTY,'P4',BQ.OP_CNTR_QTY,'A4',BQ.OP_CNTR_QTY,0))) TS_FR_40
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(BQ.CNTR_TPSZ_CD,'F5',BQ.OP_CNTR_QTY,0))) TS_FR_40H
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(BQ.CNTR_TPSZ_CD,'F7',BQ.OP_CNTR_QTY,0))) TS_FR_45
    
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(BQ.CNTR_TPSZ_CD,'O2',BQ.OP_CNTR_QTY,'S2',BQ.OP_CNTR_QTY,0))) TS_OT_20
    		, SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(BQ.CNTR_TPSZ_CD,'O4',BQ.OP_CNTR_QTY,'S4',BQ.OP_CNTR_QTY,0))) TS_OT_40
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(BQ.CNTR_TPSZ_CD,'O5',BQ.OP_CNTR_QTY,'S5',BQ.OP_CNTR_QTY,0))) TS_OT_40H
    		
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0)))) TS_PCOD_20
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0)))) TS_PCOD_40
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0)))) TS_PCOD_40H
            , SUM(DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7', BQ.OP_CNTR_QTY,0),0),0,'',DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7', BQ.OP_CNTR_QTY,0),0)))) TS_PCOD_45
    
    	FROM BKG_BOOKING BK
    		, BKG_VVD BV
    		, VSK_VSL_PORT_SKD VPS
    		, BKG_QUANTITY BQ
    		, MDM_LOCATION LO
    		, MDM_COMMODITY CM
    	WHERE BK.BKG_NO = BV.BKG_NO
    	  AND BV.VSL_CD = VPS.VSL_CD
    	  AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
    	  AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
    	  AND BV.POD_CD = VPS.VPS_PORT_CD
    	  AND BK.BKG_NO = BQ.BKG_NO
    	  AND VPS.VPS_PORT_CD = LO.LOC_CD

		  AND BV.VSL_CD     = @[in_vsl_cd]
		  AND BV.SKD_VOY_NO = @[in_skd_voy_no]
		  AND BV.SKD_DIR_CD = @[in_skd_dir_cd]
		  AND BV.POL_CD     = @[in_pol_cd]
		  AND SUBSTR(BV.POL_YD_CD, 6, 2) like @[in_pol_yd_cd]||'%'

    	  AND BK.BKG_STS_CD IN ( 'F','W')
    	  AND VPS.CLPT_IND_SEQ='1'
    	  AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
    	  AND BK.CMDT_CD = CM.CMDT_CD
    	  AND NVL(CM.DELT_FLG,'N')='N'
    	GROUP BY NVL(LO.UN_LOC_CD,BV.POD_CD) 
    		, BK.BKG_NO) BQ
    WHERE DG. BKG_NO = RF.BKG_NO
    AND DG. BKG_NO = BQ.BKG_NO
