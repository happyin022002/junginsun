<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DailyForecastManageDBDAOSpcDlyFcastCustSwitchCSQL">
			<desc><![CDATA[전임 S.REP이 등록한 Forecast 를 승계한다.
[CHM-201322502-01] SPC 프로젝트 - 성수기 선복운영 개선을 위한 T/F추진]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_DLY_FCAST_CUST
(
    RLANE_CD,
    DIR_CD,
    VSL_CD, 
    SKD_VOY_NO, 
    SKD_DIR_CD, 
    IOC_TS_CD, 
    SREP_USR_ID, 
    CUST_CNT_CD, 
    CUST_SEQ , 
    FCAST_SEQ, 
    SC_NO    , 
    POL_YD_CD, 
    POD_YD_CD, 
    FCAST_OFC_CD, 
    FCAST_CUST_TP_CD, 
    IOC_CD, 
    REP_TRD_CD, 
    REP_SUB_TRD_CD, 
    TRD_CD, 
    SUB_TRD_CD, 
    SLS_RHQ_CD, 
    SLS_AQ_CD, 
    SLS_RGN_OFC_CD, 
    SLS_OFC_CD, 
    FCAST_TTL_QTY, 
    FCAST_40FT_HC_QTY, 
    FCAST_45FT_HC_QTY, 
    FCAST_RF_QTY, 
    FCAST_TTL_WGT, 
    CFM_TTL_QTY, 
    CFM_40FT_HC_QTY, 
    CFM_45FT_HC_QTY, 
    CFM_RF_QTY, 
    CFM_TTL_WGT, 
    CTRL_LVL_CD, 
    MODI_GDT, 
    CFM_USR_ID, 
    CFM_DT, 
    USD_BKG_TTL_QTY, 
    USD_BKG_20FT_QTY, 
    USD_BKG_40FT_QTY, 
    USD_BKG_40FT_HC_QTY, 
    USD_BKG_45FT_HC_QTY, 
    USD_BKG_RF_QTY, 
    USD_BKG_TTL_WGT, 
    CRE_USR_ID, 
    CRE_DT, 
    UPD_USR_ID, 
    UPD_DT, 
    POL_IND_SEQ, 
    POD_IND_SEQ, 
    POL_YD_IND_SEQ, 
    POD_YD_IND_SEQ, 
    FCAST_53FT_QTY, 
    CFM_53FT_QTY, 
    USD_BKG_53FT_QTY, 
    FCAST_20FT_QTY, 
    FCAST_40FT_QTY
)
SELECT A.RLANE_CD,
       A.DIR_CD,
       A.VSL_CD, 
       A.SKD_VOY_NO, 
       A.SKD_DIR_CD, 
       A.IOC_TS_CD, 
       @[salesRep] AS SREP_CD, 
       A.CUST_CNT_CD, 
       A.CUST_SEQ, 
       A.FCAST_SEQ, 
       A.SC_NO, 
       A.POL_YD_CD, 
       A.POD_YD_CD, 
       A.FCAST_OFC_CD, 
       A.FCAST_CUST_TP_CD, 
       A.IOC_CD, 
       A.REP_TRD_CD, 
       A.REP_SUB_TRD_CD, 
       A.TRD_CD, 
       A.SUB_TRD_CD, 
       A.SLS_RHQ_CD, 
       A.SLS_AQ_CD, 
       A.SLS_RGN_OFC_CD, 
       A.SLS_OFC_CD, 
       A.FCAST_TTL_QTY, 
       A.FCAST_40FT_HC_QTY, 
       A.FCAST_45FT_HC_QTY, 
       A.FCAST_RF_QTY, 
       A.FCAST_TTL_WGT, 
       A.CFM_TTL_QTY, 
       A.CFM_40FT_HC_QTY, 
       A.CFM_45FT_HC_QTY, 
       A.CFM_RF_QTY, 
       A.CFM_TTL_WGT, 
       A.CTRL_LVL_CD, 
       CAST(SYS_EXTRACT_UTC(TO_TIMESTAMP(SYSDATE, 'YYYY/MM/DD HH24:MI:SS')) AS DATE) AS MODI_GDT, 
       A.CFM_USR_ID, 
       SYSDATE CFM_DT, 
       A.USD_BKG_TTL_QTY, 
       A.USD_BKG_20FT_QTY, 
       A.USD_BKG_40FT_QTY, 
       A.USD_BKG_40FT_HC_QTY, 
       A.USD_BKG_45FT_HC_QTY, 
       A.USD_BKG_RF_QTY, 
       A.USD_BKG_TTL_WGT, 
       'SYSTEM' CRE_USR_ID, 
       SYSDATE CRE_DT, 
       'SYSTEM' UPD_USR_ID, 
       SYSDATE UPD_DT, 
       A.POL_IND_SEQ, 
       A.POD_IND_SEQ, 
       A.POL_YD_IND_SEQ, 
       A.POD_YD_IND_SEQ, 
       A.FCAST_53FT_QTY, 
       A.CFM_53FT_QTY, 
       A.USD_BKG_53FT_QTY, 
       A.FCAST_20FT_QTY, 
       A.FCAST_40FT_QTY
  FROM SPC_DLY_FCAST_CUST A,
       MAS_MON_VVD        V
 WHERE V.TRD_CD      = @[trd_cd]
#if(${sub_trd_cd} != '')
   AND V.SUB_TRD_CD  = @[sub_trd_cd]
#end
#if(${rlane_cd} != '')
   AND V.RLANE_CD    IN (${rlane_cd})
#end
   AND V.IOC_CD      = DECODE(@[ioc_ts_cd], 'T', 'I', @[ioc_ts_cd])
#if(${dir_cd} != '')
   AND V.DIR_CD      = @[dir_cd]
#end
   AND V.DELT_FLG    = 'N'
   AND V.TRD_CD      = A.TRD_CD
   AND V.RLANE_CD    = A.RLANE_CD
   AND V.DIR_CD      = A.DIR_CD
   AND V.SUB_TRD_CD  = A.SUB_TRD_CD
   AND V.VSL_CD      = A.VSL_CD
   AND V.SKD_VOY_NO  = A.SKD_VOY_NO
   AND V.DIR_CD      = A.SKD_DIR_CD
   AND A.IOC_TS_CD   = @[ioc_ts_cd]
   AND SUBSTR(V.SLS_YRMON, 1, 4)||V.COST_WK >= REPLACE(@[cost_wk], '-', '')
   AND A.CUST_CNT_CD = @[cust_cnt_cd]
   AND A.CUST_SEQ    = TO_NUMBER(@[cust_seq])
   AND A.SREP_USR_ID = @[srep_cd]			]]></sql>
			<params>
				<param name="salesRep" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="ioc_ts_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="cost_wk" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
