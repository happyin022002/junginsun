<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchCNTRCountRSQL">
			<desc><![CDATA[Container Validation Check]]></desc>
			<sql><![CDATA[
SELECT COUNT(CNTR_NO) CNT
  FROM BKG_VVD V, BKG_CONTAINER C
 WHERE V.VSL_CD        = @[vsl_cd]
   AND V.SKD_VOY_NO    = @[skd_voy_no]
   AND V.SKD_DIR_CD    = @[skd_dir_cd]
   AND V.BKG_NO        = C.BKG_NO
   AND C.CNTR_NO       = @[cntr_no]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
