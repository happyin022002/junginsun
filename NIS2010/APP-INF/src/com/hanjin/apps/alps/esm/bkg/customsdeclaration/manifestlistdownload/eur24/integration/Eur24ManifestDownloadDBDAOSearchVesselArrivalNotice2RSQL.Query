<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOSearchVesselArrivalNotice2RSQL">
			<desc><![CDATA[구주24 Vessel Arrival Notice 2]]></desc>
			<sql><![CDATA[
SELECT VPS.VSL_CD||VPS.SKD_VOY_NO||VPS.SKD_DIR_CD VVD,
  VPS.VSL_CD,
  VPS.SKD_VOY_NO,
  VPS.SKD_DIR_CD,
  EU.CVY_REF_NO,
  EU.CVY_REF_NO CVY_REF_NO_HIDDEN,
  VSL.VSL_ENG_NM,
  VSL.LLOYD_NO,
  VSL.PICLB_DESC,
  '' N1ST_CLPT_CD,
  TO_CHAR(VPS.VPS_ETA_DT, 'YYYY-MM-DD HH24:MI') ETA_DT,
  TO_CHAR(VPS.VPS_ETD_DT, 'YYYY-MM-DD HH24:MI') ETD_DT,
  TO_CHAR(ACT.ACT_ARR_DT, 'YYYY-MM-DD HH24:MI') ATA_DT,
  EU.CSTMS_PORT_CD_BL CSTMS_PORT_CD,
  EU.CSTMS_YD_CD,
  EU.NEW_PORT RVIS_N1ST_CLPT_CD,
  (SELECT A.EUR_CSTMS_OFC_ID
    FROM BKG_CSTMS_EUR_CD_STUP A
    WHERE A.PORT_CD = EU.CSTMS_PORT_CD
      AND DECODE(A.TML_CD, 'ALL' , EU.CSTMS_YD_CD, A.TML_CD)= EU.CSTMS_YD_CD
      AND ROWNUM=1 ) AS N1ST_PORT_OFC_CD,
  (SELECT A.EUR_CSTMS_OFC_ID
    FROM BKG_CSTMS_EUR_CD_STUP A
    WHERE A.PORT_CD = SUBSTR(EU.NEW_PORT, 1, 5)
      AND DECODE(A.TML_CD, 'ALL' , EU.NEW_PORT , A.TML_CD)= EU.NEW_PORT
      AND ROWNUM=1 ) AS N1ST_PORT_OFC_CD_NEW ,
  VPS.YD_CD TML_CD,
  YARD.YD_NM TML_NM,
  NVL( EU.LST_CLPT_CD, (
        SELECT /*+ INDEX_DESC(K XAK4VSK_VSL_PORT_SKD) */VPS_PORT_CD
        FROM VSK_VSL_PORT_SKD K
        WHERE K.VSL_CD = VPS.VSL_CD
          AND K.SKD_VOY_NO = VPS.SKD_VOY_NO
          AND K.SKD_DIR_CD = VPS.SKD_DIR_CD
          AND K.CLPT_SEQ < VPS.CLPT_SEQ
          AND ROWNUM = 1 ) ) LST_CLPT_CD,
  NVL(EU.NXT_CLPT_CD, (
        SELECT /*+ INDEX(K XAK4VSK_VSL_PORT_SKD) */VPS_PORT_CD
        FROM VSK_VSL_PORT_SKD K
        WHERE K.VSL_CD = VPS.VSL_CD
          AND K.SKD_VOY_NO = VPS.SKD_VOY_NO
          AND K.SKD_DIR_CD = VPS.SKD_DIR_CD
          AND K.CLPT_SEQ > VPS.CLPT_SEQ
          AND ROWNUM = 1 ) ) NXT_CLPT_CD,
  EU.ARR_PORT_CD,
  VSL.RGST_NO RGST_NO,
  TO_CHAR(VSL.RGST_DT, 'YYYY-MM-DD') RGST_DT,
  VSL.RGST_PORT_CD RGST_PORT_CD,
  VSL.GRS_RGST_TONG_WGT GRS_RGST_TONG_WGT,
  VSL.NET_RGST_TONG_WGT NET_RGST_TONG_WGT,
  DECODE(EU.CSTMS_PORT_CD, '', 'I', 'U') IBFLAG,
  EU.SND_USR_ID,
  EU.SND_DT,
  '' OFC_CD,
  EU.ACK,
  EU.RESULT
FROM VSK_VSL_PORT_SKD VPS,
  VSK_ACT_PORT_SKD ACT,
  MDM_VSL_CNTR VSL,
  MDM_YARD YARD,
  MDM_LOCATION LOC,
  BKG_VSL_DCHG_YD DCHG,
  (
    SELECT A.VSL_CD,
      A.SKD_VOY_NO,
      A.SKD_DIR_CD,
      A.CSTMS_PORT_CD,
      MAX(A.CSTMS_YD_CD) CSTMS_YD_CD,
      MAX(B.CSTMS_PORT_CD) CSTMS_PORT_CD_BL,
      MAX(B.CSTMS_YD_CD) CSTMS_YD_CD_BL,
      MAX(A.CVY_REF_NO) CVY_REF_NO,
      MAX(RVIS_N1ST_CLPT_CD) NEW_PORT,
      MAX(LST_CLPT_CD) LST_CLPT_CD,
      MAX(NXT_CLPT_CD) NXT_CLPT_CD,
      MAX(A.ARR_PORT_CD) ARR_PORT_CD,
      MAX(SND_DT) SND_DT,
      MAX(SND_USR_ID) SND_USR_ID,
      MAX(RCV.ACK_RCV_STS_CD) ACK,
      MAX(RCV.EUR_CSTMS_ACK_CD) RESULT
    FROM BKG_CSTMS_EUR_BL B,
      BKG_CSTMS_EUR_VSL A,
      BKG_CSTMS_ADV_EUR_SND SND,
      BKG_CSTMS_ADV_EUR_RCV RCV
    WHERE A.VSL_CD = B.VSL_CD(+)
      AND A.SKD_VOY_NO = B.SKD_VOY_NO(+)
      AND A.SKD_DIR_CD = B.SKD_DIR_CD(+)
      AND A.CSTMS_PORT_CD = B.CSTMS_PORT_CD(+)
      AND A.MSG_SND_NO = SND.MSG_SND_NO(+)
	  AND SND.MSG_SND_NO = RCV.MSG_RCV_NO(+)
    GROUP BY A.VSL_CD, A.SKD_VOY_NO, A.SKD_DIR_CD, A.CSTMS_PORT_CD ) EU
WHERE VPS.VSL_CD = SUBSTR(@[vvd], 1, 4)
  AND VPS.VSL_CD = VSL.VSL_CD
  AND VPS.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
  AND VPS.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
  AND VPS.VSL_CD = DCHG.VSL_CD(+)
  AND VPS.SKD_VOY_NO = DCHG.SKD_VOY_NO(+)
  AND VPS.SKD_DIR_CD = DCHG.SKD_DIR_CD(+)
  AND VPS.VSL_CD = ACT.VSL_CD(+)
  AND VPS.SKD_VOY_NO = ACT.SKD_VOY_NO(+)
  AND VPS.SKD_DIR_CD = ACT.SKD_DIR_CD(+)
  AND VPS.VPS_PORT_CD = ACT.VPS_PORT_CD(+)
  AND VPS.YD_CD = DCHG.YD_CD(+)
  AND VPS.VPS_PORT_CD = LOC.LOC_CD
  AND VPS.YD_CD = YARD.YD_CD
  AND VPS.VPS_PORT_CD = @[cstms_port_cd]
  AND VPS.VSL_CD = EU.VSL_CD(+)
  AND VPS.SKD_VOY_NO = EU.SKD_VOY_NO(+)
  AND VPS.SKD_DIR_CD = EU.SKD_DIR_CD(+)
  AND VPS.VPS_PORT_CD = EU.CSTMS_PORT_CD(+)
  AND VPS.ROWID = (
    SELECT ROWID
    FROM (
        SELECT ROWID
        FROM VSK_VSL_PORT_SKD A
        WHERE A.VSL_CD = SUBSTR(@[vvd], 1, 4)
          AND A.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
          AND A.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
		  AND NVL(SKD_CNG_STS_CD,'X')  <> 'S'
          AND SUBSTR(A.VPS_PORT_CD, 1, 2) IN (
            SELECT ATTR_CTNT1
            FROM BKG_CSTMS_CD_CONV_CTNT B
            WHERE B.CSTMS_DIV_ID = 'EU_MEMBER_CNT' )
        ORDER BY CLPT_SEQ )
    WHERE ROWNUM=1 )			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
