<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOLocationGroupNmRSQL">
			<desc><![CDATA[Location Group Name select]]></desc>
			<sql><![CDATA[
SELECT PRC_GRP_LOC_DESC
FROM PRI_SG_GRP_LOC
WHERE	SVC_SCP_CD = @[etc1]
AND	GLINE_SEQ = @[etc2]
AND PRC_GRP_LOC_CD = @[cd]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
