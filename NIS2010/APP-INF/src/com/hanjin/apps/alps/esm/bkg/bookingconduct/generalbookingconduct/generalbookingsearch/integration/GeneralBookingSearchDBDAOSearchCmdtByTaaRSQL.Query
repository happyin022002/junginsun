<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchCmdtByTaaRSQL">
			<desc><![CDATA[TAA 계약상의 Commodity 정보를 조회한다.]]></desc>
			<sql><![CDATA[
select distinct cmdt.cmdt_cd, cmdt.cmdt_nm cmdt_nm, taa.SVC_SCP_CD, cmdt.rep_cmdt_cd
  from PRI_TRI_MN main, mdm_commodity cmdt, pri_taa_tri_list list, pri_taa_mn taa 
 where list.taa_prop_no     = taa.taa_prop_no
   and list.amdt_seq        = taa.amdt_seq
   and main.tri_prop_no     = list.tri_prop_no 
   and list.taa_prop_no     = @[prop_no]
   and list.amdt_seq        = @[amdt_seq]
   and taa.svc_scp_cd       = @[svc_scp_cd]
   and main.cmdt_cd         = cmdt.cmdt_cd

#if (${cmdt_cd} != '')
   and cmdt.cmdt_cd    like @[cmdt_cd]||'%' --입력했을 때만
#end
#if (${cmdt_nm} != '')
   and cmdt.cmdt_nm    like @[cmdt_nm]||'%'
#end			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="cmdt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
