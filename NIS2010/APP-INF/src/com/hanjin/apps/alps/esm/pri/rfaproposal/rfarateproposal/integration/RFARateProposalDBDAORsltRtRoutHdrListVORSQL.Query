<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltRtRoutHdrListVORSQL">
			<desc><![CDATA[Rate - Route조회]]></desc>
			<sql><![CDATA[
SELECT A.PROP_NO
      ,A.AMDT_SEQ
      ,A.SVC_SCP_CD
      ,A.CMDT_HDR_SEQ
      ,A.ROUT_SEQ
      ,B.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS ORG_ROUT_PNT_LOC_DEF_CD_PROP
      ,C.ROUT_VIA_PORT_DEF_CD AS ORG_ROUT_VIA_PORT_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS ORG_ROUT_VIA_PORT_DEF_CD_PROP
      ,D.ROUT_VIA_PORT_DEF_CD AS DEST_ROUT_VIA_PORT_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS DEST_ROUT_VIA_PORT_DEF_CD_PROP
      ,E.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS DEST_ROUT_PNT_LOC_DEF_CD_PROP
      ,REPLACE(F.NOTE_CTNT, '^|^', CHR(13)) AS NOTE_CTNT
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS NOTE_CTNT_PROP
      ,REPLACE(F.NOTE_CTNT, '^|^', CHR(13) || CHR(13)) AS NOTE_CTNT_TOOLTIP
      ,(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) + NVL(H.ND_CNT, 0)) AS ND_CNT
      ,(NVL(B.NA_CNT, 0) + NVL(C.NA_CNT, 0) + NVL(D.NA_CNT, 0) + NVL(E.NA_CNT, 0) + NVL(F.NA_CNT, 0) + NVL(H.NA_CNT, 0)) AS NA_CNT
      ,(NVL(B.UP_CNT, 0) + NVL(C.UP_CNT, 0) + NVL(D.UP_CNT, 0) + NVL(E.UP_CNT, 0) + NVL(F.UP_CNT, 0) + NVL(H.UP_CNT, 0)) AS UP_CNT
      ,DECODE(A.AMDT_SEQ
             ,0
             ,DECODE(NVL(B.NA_CNT, 0) + NVL(C.NA_CNT, 0) + NVL(D.NA_CNT, 0) + NVL(E.NA_CNT, 0) + NVL(F.NA_CNT, 0) +
                     NVL(H.NA_CNT, 0)
                    ,0
                    ,'color:blue;'
                    ,'')
             ,DECODE(NVL(B.NA_CNT, 0) + NVL(C.NA_CNT, 0) + NVL(D.NA_CNT, 0) + NVL(E.NA_CNT, 0) + NVL(F.NA_CNT, 0) +
                     NVL(H.NA_CNT, 0)
                    ,0
                    ,DECODE(NVL(B.UP_CNT, 0) + NVL(C.UP_CNT, 0) + NVL(D.UP_CNT, 0) + NVL(E.UP_CNT, 0) + NVL(F.UP_CNT, 0) +
                            NVL(H.UP_CNT, 0)
                           ,0
                           ,''
                           ,'color:blue;')
                    ,'color:red;')) AS ROW_PROPERTIES
      ,A.NOTE_DP_SEQ
      ,A.N1ST_CMNC_AMDT_SEQ
      ,A.N1ST_CMNC_AMDT_SEQ AS ORG_N1ST_CMNC_AMDT_SEQ
      ,A.CRE_USR_ID
      ,A.CRE_DT
      ,A.UPD_USR_ID
      ,A.UPD_DT
      ,@[note_dp_seq] || '.' || ROW_NUMBER() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.CMDT_HDR_SEQ ORDER BY B.SORT_KEY, E.SORT_KEY, C.SORT_KEY, D.SORT_KEY) AS RN
      , '' as FIC_RT_USE_STS_CD
      , A.ORG_CY_DOR_RT_TP_CD
      , A.DEST_CY_DOR_RT_TP_CD
      , B.BSE_PORT_LOC_CD ORG_BSE_PORT_LOC_CD
      , E.BSE_PORT_LOC_CD DEST_BSE_PORT_LOC_CD
  FROM PRI_RP_SCP_RT_CMDT_ROUT A
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
              ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD, ', ')), 3) AS ROUT_PNT_LOC_DEF_CD              
			  ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(BSE_PORT_LOC_CD, ', ')), 3) AS BSE_PORT_LOC_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_DEF_CD
                      ,ROUT_PNT_LOC_TP_CD
                      ,RCV_DE_TERM_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
					  ,BSE_PORT_LOC_CD
                      ,N1ST_CMNC_AMDT_SEQ
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                  FROM PRI_RP_SCP_RT_ROUT_PNT
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'O')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) B
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
              ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_DEF_CD
                      ,ROUT_VIA_PORT_TP_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,N1ST_CMNC_AMDT_SEQ
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                  FROM PRI_RP_SCP_RT_ROUT_VIA
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'O')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) C
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
              ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_DEF_CD
                      ,ROUT_VIA_PORT_TP_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,N1ST_CMNC_AMDT_SEQ
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                  FROM PRI_RP_SCP_RT_ROUT_VIA
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'D')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) D
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
              ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD, ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
			  ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(BSE_PORT_LOC_CD, ', ')), 3) AS BSE_PORT_LOC_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_DEF_CD
                      ,ROUT_PNT_LOC_TP_CD
                      ,RCV_DE_TERM_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
					  ,BSE_PORT_LOC_CD
                      ,N1ST_CMNC_AMDT_SEQ
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                  FROM PRI_RP_SCP_RT_ROUT_PNT
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'D')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) E
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
              ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(NOTE_CTNT, '^|^')), 4) AS NOTE_CTNT
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,SVC_SCP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,SUBSTRB(NOTE_CTNT,0,497)||CASE WHEN LENGTHB(NOTE_CTNT)<497 THEN '' ELSE '...' END AS NOTE_CTNT
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,N1ST_CMNC_AMDT_SEQ
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ROUT_NOTE_SEQ) AS RN
                  FROM PRI_RP_SCP_RT_CMDT_RNOTE
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq])
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ) F
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
              ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
          FROM PRI_RP_SCP_RT
         WHERE PROP_NO = @[prop_no]
           AND AMDT_SEQ = @[amdt_seq]
           AND SVC_SCP_CD = @[svc_scp_cd]
           AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ) H
 WHERE A.PROP_NO = B.PROP_NO(+)
   AND A.AMDT_SEQ = B.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = B.SVC_SCP_CD(+)
   AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = B.ROUT_SEQ(+)
   AND A.PROP_NO = C.PROP_NO(+)
   AND A.AMDT_SEQ = C.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = C.SVC_SCP_CD(+)
   AND A.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = C.ROUT_SEQ(+)
   AND A.PROP_NO = D.PROP_NO(+)
   AND A.AMDT_SEQ = D.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = D.SVC_SCP_CD(+)
   AND A.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = D.ROUT_SEQ(+)
   AND A.PROP_NO = E.PROP_NO(+)
   AND A.AMDT_SEQ = E.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = E.SVC_SCP_CD(+)
   AND A.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = E.ROUT_SEQ(+)
   AND A.PROP_NO = F.PROP_NO(+)
   AND A.AMDT_SEQ = F.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = F.SVC_SCP_CD(+)
   AND A.CMDT_HDR_SEQ = F.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = F.ROUT_SEQ(+)
   AND A.PROP_NO = H.PROP_NO(+)
   AND A.AMDT_SEQ = H.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = H.SVC_SCP_CD(+)
   AND A.CMDT_HDR_SEQ = H.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = H.ROUT_SEQ(+)
   AND A.PROP_NO = @[prop_no]
   AND A.AMDT_SEQ = @[amdt_seq]
   AND A.SVC_SCP_CD = @[svc_scp_cd]
   AND A.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#if (${IS_STYLE} == 'Y') 
   AND A.ROUT_SEQ = @[rout_seq]
#end
 ORDER BY B.SORT_KEY
         ,E.SORT_KEY
         ,C.SORT_KEY
         ,D.SORT_KEY			]]></sql>
			<params>
				<param name="note_dp_seq" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="TES090001" out="N"/>
				<param name="amdt_seq" type="2" value="2" out="N"/>
				<param name="svc_scp_cd" type="12" value="ACE" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="1" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
