<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAORsltCmpbGrpLocListVORSQL">
			<desc><![CDATA[PRI_CMPB_GRP_LOC 조회]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
     , CRE_OFC_CD
     , GLINE_SEQ
     , (GRP_LOC_SEQ||'|'||CRE_OFC_CD||'|'||GLINE_SEQ) GRP_LOC_SEQ
     , PRC_GRP_LOC_CD CD
     , PRC_GRP_LOC_DESC NM
  FROM PRI_CMPB_GRP_LOC
 WHERE SVC_SCP_CD = @[svc_scp_cd]

#if (${cre_ofc_cd} != '') 
	AND CRE_OFC_CD = @[cre_ofc_cd]
#end

#if (${gline_seq} != '') 
	AND GLINE_SEQ = @[gline_seq]
#end

 	ORDER BY PRC_GRP_LOC_CD ASC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
