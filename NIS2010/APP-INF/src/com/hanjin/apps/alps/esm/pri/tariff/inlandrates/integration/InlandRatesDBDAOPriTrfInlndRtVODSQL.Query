<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRatesDBDAOPriTrfInlndRtVODSQL">
			<desc><![CDATA[inland Rate 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_TRF_INLND_RT
 WHERE TRF_PFX_CD		= @[trf_pfx_cd]
   AND TRF_NO 			= @[trf_no]
   AND TRF_INLND_SEQ 	= @[trf_inlnd_seq]
   AND AMDT_SEQ 		= @[amdt_seq]
#if (${main_del_yn} == 'N') 
   AND TRF_INLND_RT_SEQ = @[trf_inlnd_rt_seq]
#end			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_inlnd_seq" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="trf_inlnd_rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
