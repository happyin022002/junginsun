<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOMasMonVvdVOChkUSQL">
			<desc><![CDATA[ MAS_MON_VVD Check List 수정   ]]></desc>
			<sql><![CDATA[
UPDATE MAS_MON_VVD 
   SET WKY_MNL_FLG = NVL2(@[wky_mnl_flg], 'M', 'P')
      ,WKY_TGT_FLG  = 'Y'
      ,COST_YRMON   = @[cost_yrmon]
      ,SLS_YRMON    = @[sls_yrmon] 
      ,COST_WK      = @[cost_wk]
      ,UPD_USR_ID   = @[upd_usr_id]
      ,UPD_DT       = SYSDATE 
 WHERE 1=1 
   AND TRD_CD     IN (CASE 
                         WHEN @[trd_cd] = 'AES' 
							THEN 'IES'
						 WHEN @[trd_cd] = 'IES' 
							THEN 'AES'
                         ELSE
                              ''
                      END
						,@[trd_cd])
   AND RLANE_CD     = @[rlane_cd] 
   AND VSL_CD       = @[vsl_cd] 
   AND SKD_VOY_NO   = @[skd_voy_no] 
   AND DIR_CD       = @[dir_cd]			]]></sql>
			<params>
				<param name="wky_mnl_flg" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="sls_yrmon" type="12" value="" out="N"/>
				<param name="cost_wk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
