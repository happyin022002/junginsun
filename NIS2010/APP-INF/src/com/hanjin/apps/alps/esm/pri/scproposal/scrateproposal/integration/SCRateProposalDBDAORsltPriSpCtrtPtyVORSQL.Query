<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAORsltPriSpCtrtPtyVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT NVL(A.REAL_CUST_CNT_CD, B.CUST_CNT_CD) CUST_CNT_CD, NVL(REAL_CUST_SEQ,B.CUST_SEQ ) CUST_SEQ
	   ,B.PROP_NO, B.AMDT_SEQ, B.PRC_CTRT_PTY_TP_CD
	   ,B.CTRT_CUST_VAL_SGM_CD, B.CTRT_CUST_SREP_CD, B.CTRT_CUST_SLS_OFC_CD, B.CTRT_PTY_NM
       , B.CTRT_PTY_ADDR, B.CTRT_PTY_SGN_NM, B.CTRT_PTY_SGN_TIT_NM
	   ,B.PRC_PROG_STS_CD, B.SRC_INFO_CD, B.N1ST_CMNC_AMDT_SEQ, B.ACPT_USR_ID
	   , B.ACPT_OFC_CD, B.ACPT_DT, B.CRE_USR_ID, B.CRE_DT, B.UPD_USR_ID, B.UPD_DT
  FROM PRI_SP_CTRT_PTY B, PRI_SP_MN A 
 WHERE A.PROP_NO =  @[prop_no]
   AND A.AMDT_SEQ = @[amdt_seq]
   AND A.PROP_NO = B.PROP_NO 
   AND A.AMDT_SEQ = B.AMDT_SEQ 
   AND PRC_CTRT_PTY_TP_CD = 'C' 			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
