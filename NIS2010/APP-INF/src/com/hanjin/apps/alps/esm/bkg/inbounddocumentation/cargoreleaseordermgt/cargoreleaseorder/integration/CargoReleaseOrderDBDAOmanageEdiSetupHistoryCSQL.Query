<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOmanageEdiSetupHistoryCSQL">
			<desc><![CDATA[EDI Setup History 저장]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_EDI_YD_HIS (
	YD_CD
	,EDI_YD_HIS_SEQ
	,PORT_CD
	,EDI_RCV_ID
	,SLAN_CD1
	,SLAN_CD2
	,SLAN_CD3
	,SLAN_CD4
	,SLAN_CD5
	,SLAN_CD6
	,SLAN_CD7
	,SLAN_CD8
	,SLAN_CD9 
	,SLAN_CD10
	,EDI_SND_ID
	,FULL_RLSE_EDI_CD
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT )
SELECT 
	YD_CD
    ,LTRIM(TO_CHAR(BKG_EDI_YD_HIS_SEQ.NEXTVAL,'009'))  
	,PORT_CD
	,EDI_RCV_ID
	,SLAN_CD1
	,SLAN_CD2
	,SLAN_CD3
	,SLAN_CD4
	,SLAN_CD5
	,SLAN_CD6
	,SLAN_CD7
	,SLAN_CD8
	,SLAN_CD9
	,SLAN_CD10
	,EDI_SND_ID
	,FULL_RLSE_EDI_CD
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT 
FROM BKG_EDI_YD
WHERE YD_CD = @[yd_cd]			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
