<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOMaxCmdtHdrSeqRSQL">
			<desc><![CDATA[조건에 일치하는 최대 Commmodity Header Sequence를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(CMDT_HDR_SEQ), 0)+1 CMDT_HDR_SEQ FROM PRI_RP_SCP_RT_CMDT_HDR
WHERE 1=1
AND PROP_NO=@[prop_no]
AND AMDT_SEQ=@[amdt_seq]
AND SVC_SCP_CD=@[svc_scp_cd]
			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
