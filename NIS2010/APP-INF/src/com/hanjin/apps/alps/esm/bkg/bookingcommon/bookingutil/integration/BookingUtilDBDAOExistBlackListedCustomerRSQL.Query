<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOExistBlackListedCustomerRSQL">
			<desc><![CDATA[ExistBlackListedCustomer]]></desc>
			<sql><![CDATA[
SELECT B.CUST_CNT_CD
      ,B.CUST_SEQ
      ,A.CUST_RLSE_CTRL_RMK
      ,NVL(A.CR_CLT_OFC_CD, B.OFC_CD) AR_OFC
      ,C.SREP_NM  
  FROM MDM_CR_CUST A
      ,MDM_CUSTOMER B
      ,MDM_SLS_REP C     
 WHERE (B.CUST_CNT_CD, B.CUST_SEQ) IN (SELECT CUST_CNT_CD, CUST_SEQ 
                                         FROM BKG_BOOKING A, BKG_CUSTOMER B
                                        WHERE A.BKG_NO = B.BKG_NO
                                          AND A.BKG_NO = @[input_text]
                                          AND B.BKG_CUST_TP_CD IN ('S', DECODE(A.CUST_TO_ORD_FLG, 'Y', 'N', 'C')))
   AND NVL(A.CUST_RLSE_CTRL_FLG, 'N') = 'Y'
   AND A.CUST_CNT_CD(+) = B.CUST_CNT_CD
   AND A.CUST_SEQ(+) = B.CUST_SEQ
   AND B.SREP_CD = C.SREP_CD(+)
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
