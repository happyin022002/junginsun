<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOCoaMonVvdPortOpDysVOUSQL">
			<desc><![CDATA[Lane Transit Time 2 업데이트]]></desc>
			<sql><![CDATA[
UPDATE COA_MON_VVD_PORT_OP_DYS SET
     SEA_DYS      = @[sea_dys]
    ,PORT_DYS     = @[port_dys]
    ,TTL_TZ_DYS   = @[ttl_tz_dys]
    ,UPD_USR_ID   = @[upd_usr_id]
    ,UPD_DT       = SYSDATE
 WHERE TRD_CD            = @[trd_cd]
   AND RLANE_CD          = @[rlane_cd]
   AND IOC_CD            = @[ioc_cd]
   AND VSL_CD            = @[vsl_cd]
   AND SKD_VOY_NO        = @[skd_voy_no]
   AND DIR_CD            = @[dir_cd]
   AND LOC_CD            = @[loc_cd]
   AND VSL_DBL_CALL_SEQ  = @[vsl_dbl_call_seq]
   AND NEW_OP_DYS_FLG 	 = 'Y'			]]></sql>
			<params>
				<param name="sea_dys" type="12" value="" out="N"/>
				<param name="port_dys" type="12" value="" out="N"/>
				<param name="ttl_tz_dys" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="vsl_dbl_call_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
