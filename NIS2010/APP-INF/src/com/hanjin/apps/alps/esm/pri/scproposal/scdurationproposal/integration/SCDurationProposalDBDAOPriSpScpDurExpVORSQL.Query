<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCDurationProposalDBDAOPriSpScpDurExpVORSQL">
			<desc><![CDATA[main duration 변경시 자동으로 변경된 scope을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD FROM PRI_SP_SCP_DUR
WHERE PROP_NO = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND CTRT_EXP_DT <= TO_DATE(@[ctrt_exp_dt], 'YYYY-MM-DD')			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="ctrt_exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
