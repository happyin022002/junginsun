<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAORemoveVvdCADSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAORemoveVvdCADSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
DELETE 
  FROM BKG_VVD
 WHERE BKG_NO  = @[bkg_no]
#elseif (${copy_type_cd} == 'SPLIT_MASTER')
DELETE
  FROM BKG_VVD_HIS
 WHERE BKG_NO = @[bkg_no]
   AND CORR_NO = @[ca_no]
#else 
DELETE 
  FROM BKG_VVD_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ca_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
