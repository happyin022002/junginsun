		<query name="CMPBGuidelineDBDAOPriCmpbGlineAmtVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE PRI_CMPB_GLINE_AMT SET 
	MQC_RNG_TO_QTY = @[mqc_rng_to_qty]
,	RAT_UT_CD = @[rat_ut_cd]
,	PRC_CGO_TP_CD = @[prc_cgo_tp_cd]
,	CURR_CD = @[curr_cd]
,	CMPB_AMT = @[cmpb_amt]
,	SVC_SCP_CD = @[svc_scp_cd]
,	CRE_OFC_CD = @[cre_ofc_cd]
,	GLINE_SEQ = @[gline_seq]
,	PRS_CUST_TP_CD = @[prs_cust_tp_cd]
,	BSE_SEQ = @[bse_seq]
,	CMPB_SEQ = @[cmpb_seq]
,	MQC_RNG_FM_QTY = @[mqc_rng_fm_qty]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	CRE_OFC_CD = @[cre_ofc_cd]
AND	GLINE_SEQ = @[gline_seq]
AND	PRS_CUST_TP_CD = @[prs_cust_tp_cd]
AND	BSE_SEQ = @[bse_seq]
AND	CMPB_SEQ = @[cmpb_seq]			]]></sql>
			<params>
				<param name="mqc_rng_to_qty" type="2" value="" out="N"/>
				<param name="rat_ut_cd" type="12" value="" out="N"/>
				<param name="prc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="cmpb_amt" type="6" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="2" value="" out="N"/>
				<param name="prs_cust_tp_cd" type="12" value="" out="N"/>
				<param name="bse_seq" type="2" value="" out="N"/>
				<param name="cmpb_seq" type="2" value="" out="N"/>
				<param name="mqc_rng_fm_qty" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
