--                   ,'0000000'        AS POL_CD    
--                   ,'0000000'        AS POD_CD
                   , A2.POL_CD        AS POL_CD    
                   , A2.POD_CD        AS POD_CD
                   ,'OTHERS' AS DEL_CD
--                   ,   0                AS POL_SEQ    
--                   ,   0                AS POD_SEQ    
--                   ,   NULL             AS POL_SKIP  
--                    ,  NULL             AS POD_SKIP   
                   , A2.POL_SEQ              AS POL_SEQ    
                   , A2.POD_SEQ              AS POD_SEQ    
                   , A2.POL_SKIP             AS POL_SKIP  
                   , A2.POD_SKIP             AS POD_SKIP   
                   , 0 AS BKG_QUOTA
                   , 0 AS BKG_QTA_CMB
                   , A1.GUIDE
                   , OFC_TTL_GUIDE
                   , CTRL_TTL_GUIDE
                   , GUIDE_TTL
                   , 0 AS FC_TEU
                   , 0 AS FC_HC
                   , 0 AS FC_45
                   , 0 AS FC_53
                   , 0 AS FC_RF
                   , 0 AS FC_WGT
                   , 0 AS AP_TEU
                   , 0 AS AP_HC
                   , 0 AS AP_45
                   , 0 AS AP_53
                   , 0 AS AP_RF
                   , 0 AS AP_WGT
                   , 0 AS BT_TEU
                   , 0 AS BT_20
                   , 0 AS BT_40
                   , 0 AS BT_HC
                   , 0 AS BT_45
                   , 0 AS BT_53
                   , 0 AS BT_RF
                   , 0 AS BT_WGT, 0 AS BKG_VOL_VGM, 0 AS BKG_WGT_VGM
                   , 0 AS BS_WGT--2015.03.12
                   , 0 AS BS_TEU--2015.03.12
                   , 0 AS CM_OP
                   , 0 AS CM_OC
                   , 0 AS CM_VL
                   , 0 AS FC_D2  
                   , 0 AS FC_D4
                   , 0 AS FC_RD
                   , 0 AS BT_D2
                   , 0 AS BT_D4
                   , 0 AS BT_RD
                   , 0 AS AP_D2
                   , 0 AS AP_D4  
                   , 0 AS AP_RD  
                   , 'N' AS PAST
                   , A1.LST_LODG_PORT_ETD_DT
                   , A1.LST_LODG_PORT_CD
                   , NULL AS CFM_FLG
                   , NULL AS SPC_CTRL_ALOC_RMK
                   , NULL AS SPC_CTRL_ALOC_POL_RMK
                   , NULL AS SPC_CTRL_ALOC_POD_RMK
                FROM (SELECT V.REP_TRD_CD
                           , V.REP_SUB_TRD_CD
                           , V.TRD_CD
                           , V.SUB_TRD_CD
                           , V.IOC_CD
                           , L.CUST_CTRL_CD
                           , V.OFC_CD AS BASE_RHQ_CD
                           , L.SLS_RHQ_CD AS RHQ_CD
                           , L.SLS_AQ_CD AS AREA_CD
                           , L.SLS_RGN_OFC_CD AS OFC_CD
                           --, RLANE_ADJ_QTY AS GUIDE
                           , V.LST_LODG_PORT_ETD_DT
                             , V.LST_LODG_PORT_CD
                             , SUM(RLANE_ADJ_QTY) OVER (PARTITION BY L.TRD_CD, L.SUB_TRD_CD, L.RLANE_CD, L.SLS_RGN_OFC_CD, L.CUST_CTRL_CD) AS GUIDE
							 , SUM(RLANE_ADJ_QTY) OVER (PARTITION BY L.TRD_CD, L.SUB_TRD_CD, L.RLANE_CD, L.SLS_RGN_OFC_CD) OFC_TTL_GUIDE
                             , SUM(RLANE_ADJ_QTY) OVER (PARTITION BY L.TRD_CD, L.SUB_TRD_CD, L.RLANE_CD, L.CUST_CTRL_CD) CTRL_TTL_GUIDE 
                             , SUM(RLANE_ADJ_QTY) OVER (PARTITION BY L.TRD_CD, L.SUB_TRD_CD, L.RLANE_CD) AS GUIDE_TTL
                          FROM SPC_MDL_CUST_REV_LANE L
                             , VVDS V
							 , SPC_OFC_LVL           O
                         WHERE L.COST_YRWK = V.SEASON
                           AND L.VER_SEQ = V.VER_SEQ
                           AND L.TRD_CD = V.TRD_CD
                           AND L.SUB_TRD_CD = V.SUB_TRD_CD
                           AND L.RLANE_CD = V.RLANE_CD
                           AND V.HH_FLG = 'Y'
                           AND ( L.SLS_RHQ_CD = V.OFC_CD
                                    OR L.SLS_RHQ_CD = 'SINRS')
                           AND NVL(L.RLANE_ADJ_QTY,0)>0
						   AND O.OFC_CD     = L.SLS_RGN_OFC_CD
						  AND V.COST_YR || V.COST_WK BETWEEN O.OFC_APLY_FM_YRWK AND O.OFC_APLY_TO_YRWK) A1
                     , (SELECT DISTINCT  VPP.REP_TRD_CD
                             ,VPP.REP_SUB_TRD_CD
                             ,VPP.RLANE_CD
                             ,VPP.DIR_CD
                             ,VPP.TRD_CD
                             ,VPP.SUB_TRD_CD
                             ,MAP.IOC_TS_CD
                             ,MAP.SLS_RHQ_CD
                             ,MAP.SLS_OFC_CD
                             ,MAP.SLS_AQ_CD
                             ,MAP.SLS_RGN_OFC_CD
                             ,VPP.POL_CD
                             ,VPP.POD_CD
                             ,VPP.POL_SEQ
                             ,VPP.POD_SEQ
                             ,VPP.POL_SKIP
                             ,VPP.POD_SKIP
                          FROM VVD_POL_POD VPP
                             , SPC_FCAST_OFC_POL_MAPG MAP
                         WHERE VPP.REP_TRD_CD = MAP.REP_TRD_CD
                           AND VPP.REP_SUB_TRD_CD = MAP.REP_SUB_TRD_CD
                           AND VPP.RLANE_CD = MAP.RLANE_CD
                           AND VPP.DIR_CD = MAP.DIR_CD
                           AND VPP.TRD_CD = MAP.TRD_CD
                           AND VPP.SUB_TRD_CD = MAP.SUB_TRD_CD
                           AND VPP.COST_YR || VPP.COST_WK >= MAP.BSE_YRWK
                           AND VPP.HH_FLG = 'Y'
                           AND VPP.POL_CD LIKE MAP.POL_CD||'%' ) A2
                 WHERE A1.REP_TRD_CD = A2.REP_TRD_CD
                   AND A1.REP_SUB_TRD_CD = A2.REP_SUB_TRD_CD
                   AND A1.TRD_CD = A2.TRD_CD
                   AND A1.SUB_TRD_CD = A2.SUB_TRD_CD
                   AND A1.IOC_CD = A2.IOC_TS_CD
                   AND A1.OFC_CD = A2.SLS_RGN_OFC_CD
           )
  GROUP BY IOC_CD              ,
           CUST_CTRL_CD        ,
           BASE_RHQ_CD         ,
           RHQ_CD              ,
           AREA_CD             ,
           OFC_CD              ,
           US_MOD              ,
           ACCOUNT_CD          ,
           POL_CD              ,
           POL_SEQ             ,
           POD_CD              ,
           DEL_CD              ,
           POD_SEQ             ,
           POL_SKIP            ,
           POD_SKIP            ,
           LST_LODG_PORT_ETD_DT,
           LST_LODG_PORT_CD
    HAVING SUBSTR(IOC_CD, 2, 1)        = 'T'
         OR SUM(DECODE(FLG, 'A', 1, 0)) = 0
         OR SUM(DECODE(FLG, 'A', 0, 1)) > 0
         OR SUM(AP_TEU)                 > 0
         OR SUM(AP_WGT)                 > 0
)

#if (${all_pol} == 'Y')
--IAS SECTOR 판매 활성화를 위한 POL-POD PAIR 로직 START
, CMPR AS (
    SELECT T.RGN_OFC_CD AS OFC_CD, 
           --T.POL_CD AS POL, 
           T.IOC_TS_CD, 
           T.CUST_CTRL_CD,
           V.POL_CD, 
           V.POL_SEQ, 
           V.POD_CD, 
           V.POD_SEQ,
           V.PAST                ,
           V.LST_LODG_PORT_CD    ,
           V.LST_LODG_PORT_ETD_DT,   
           V.REP_TRD_CD    ,
           V.REP_SUB_TRD_CD,
           V.TRD_CD        ,
           V.SUB_TRD_CD    , 
           V.RLANE_CD           ,
           V.OFC_CD   AS BASE_RHQ_CD,
           OV.N2ND_PRNT_OFC_CD AS RHQ_CD     ,
           OV.N3RD_PRNT_OFC_CD AS AREA_CD    ,
           V.POL_SKIP  ,
           V.POD_SKIP  ,
           (SELECT DECODE(COUNT(*),0,0,1)
              FROM SPC_ALOC_CUST_POL_POD K
             WHERE K.RLANE_CD = V.RLANE_CD
               AND K.DIR_CD = V.DIR_CD
               AND K.VSL_CD = V.VSL_CD
               AND K.SKD_VOY_NO = V.SKD_VOY_NO
               AND K.SKD_DIR_CD = V.SKD_DIR_CD
               AND K.SLS_OFC_CD = T.SUB_OFC_CD
               AND K.CUST_CTRL_CD = T.CUST_CTRL_CD
               AND K.POL_YD_CD = V.POL_CD
               AND K.POD_YD_CD = V.POD_CD
               AND K.TS_FLG = DECODE(T.IOC_TS_CD,'T','Y','N')
               AND K.IOC_CD = DECODE(T.IOC_TS_CD,'T','I',T.IOC_TS_CD)
           ) AS AP_MD,
           1 AS NQTA_MD,
           1 AS NCFM_MD,
           1 AS NMDL_MD
           -- 2014.08.13
           --V.US_MOD,
           --V.ACCOUNT_CD,
           --V.DEL_CD
    FROM (
              SELECT M.SLS_OFC_CD     AS SUB_OFC_CD,
                     M.SLS_RGN_OFC_CD AS RGN_OFC_CD,
                     M.POL_CD,
                     M.IOC_TS_CD ,
                     P.CUST_CTRL_CD
                FROM REP_VVDS               V,
                     SPC_FCAST_OFC_POL_MAPG M,
                     (SELECT distinct a.ioc_cd, a.ofc_cd, a.cust_ctrl_cd FROM ALL_DATA A WHERE A.IOC_CD<>'O') P
               WHERE M.REP_TRD_CD     = V.REP_TRD_CD
                 AND M.REP_SUB_TRD_CD = V.REP_SUB_TRD_CD
--                 AND M.TRD_CD         = V.TRD_CD
--                 AND M.SUB_TRD_CD     = V.SUB_TRD_CD
                 AND M.RLANE_CD       = V.RLANE_CD
                 AND M.DIR_CD         = V.DIR_CD
                 AND M.IOC_TS_CD      = P.IOC_CD
                 AND M.SLS_OFC_CD     = P.OFC_CD
                 AND M.BSE_YRWK       = ( SELECT /*+ INDEX_DESC(MI XPKSPC_FCAST_OFC_POL_MAPG) */
                                                 MI.BSE_YRWK
                                            FROM SPC_FCAST_OFC_POL_MAPG MI
                                           WHERE MI.REP_TRD_CD     = M.REP_TRD_CD
                                             AND MI.REP_SUB_TRD_CD = M.REP_SUB_TRD_CD
                                             AND MI.RLANE_CD       = M.RLANE_CD
                                             AND MI.DIR_CD         = M.DIR_CD
                                             AND MI.IOC_TS_CD      = M.IOC_TS_CD
                                             AND MI.SLS_OFC_CD     = M.SLS_OFC_CD
                                             AND MI.BSE_YRWK      <= SUBSTR(V.SLS_YRMON,1,4)||V.COST_WK
                                             AND ROWNUM = 1 )
              UNION ALL
              SELECT M.SLS_OFC_CD      AS SUB_OFC_CD,
                     M.SLS_RGN_OFC_CD  AS RGN_OFC_CD,
                     M.POL_CD,
                     M.IOC_TS_CD,
                     P.CUST_CTRL_CD
                FROM REP_VVDS                   V,
                     SPC_IRR_FCAST_OFC_POL_MAPG M,
                     (SELECT distinct a.ioc_cd, a.ofc_cd, a.cust_ctrl_cd FROM ALL_DATA A WHERE A.IOC_CD<>'O') P
               WHERE M.REP_TRD_CD     = V.REP_TRD_CD
                 AND M.REP_SUB_TRD_CD = V.REP_SUB_TRD_CD
--                 AND M.TRD_CD         = V.TRD_CD
--                 AND M.SUB_TRD_CD     = V.SUB_TRD_CD
                 AND M.RLANE_CD       = V.RLANE_CD
                 AND M.DIR_CD         = V.DIR_CD
                 AND M.IOC_TS_CD      = P.IOC_CD
                 AND M.SLS_OFC_CD     = P.OFC_CD
                 AND M.VSL_CD         = V.VSL_CD
                 AND M.SKD_VOY_NO     = V.SKD_VOY_NO
                 AND M.SKD_DIR_CD     = V.SKD_DIR_CD
    ) T, 
--  2014.08.13
    VVD_POL_POD   V,
--  VVD_POL_POD_ALL V,
    SPC_OFC_LVL   OV 
    WHERE T.POL_CD = SUBSTR( V.POL_CD ,1,5)
      AND V.IOC_CD = DECODE(T.IOC_TS_CD,'T','I',T.IOC_TS_CD)
      AND T.RGN_OFC_CD = OV.OFC_CD
      AND V.COST_YR || V.COST_WK BETWEEN OV.OFC_APLY_FM_YRWK AND OV.OFC_APLY_TO_YRWK
#if (${office} == '')
      AND (    V.POL_CONTI = V.OFC_CONTI
            OR V.OFC_CD    = OV.N2ND_PRNT_OFC_CD
            OR V.RLANE_CD = 'WAFIE' 
            OR OV.N2ND_PRNT_OFC_CD = 'SINRS' )
#else
      AND (    V.POL_CONTI = V.OFC_CONTI
            OR V.OFC_CD    = OV.N2ND_PRNT_OFC_CD
            OR V.RLANE_CD = 'WAFIE' 
            OR (V.OFC_CD = 'SINRS' AND V.RLANE_CD = 'WAXIA') )
#end
)
, CTRL_GRP AS (
SELECT S.SEASON, CUST_CNT_CD||LPAD(CUST_SEQ, 6, '0') AS CUST_CD, CUST_CTRL_CD
                   FROM SPC_MDL_CUST_CTRL M,
                        (SELECT DISTINCT SEASON
                        FROM VSL_PORT_SKD
                        ) S
                   WHERE M.TRD_CD = TRD_CD
                     AND M.COST_YRWK = SUBSTR(S.SEASON, 1, 6)
                     AND M.VER_SEQ   = SUBSTR(S.SEASON, 8)
UNION ALL
SELECT DISTINCT S.SEASON, 'OTHERS', CUST_CTRL_CD
                   FROM SPC_MDL_CUST_CTRL M,
                        (SELECT DISTINCT SEASON
                        FROM VSL_PORT_SKD
                        ) S
                   WHERE M.TRD_CD = TRD_CD
                     AND M.COST_YRWK = SUBSTR(S.SEASON, 1, 6)
                     AND M.VER_SEQ   = SUBSTR(S.SEASON, 8)
)
