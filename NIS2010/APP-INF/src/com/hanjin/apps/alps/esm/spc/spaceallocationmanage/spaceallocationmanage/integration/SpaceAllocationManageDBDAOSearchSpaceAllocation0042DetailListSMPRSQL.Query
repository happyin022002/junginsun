    FROM (
            SELECT DECODE(IOC_CD, 'E', 0, 1) * DECODE(NQTA_MD * NCFM_MD, 0, 0, 1) * DECODE(DECODE(REP_TRD_CD, TRD_CD, DECODE(CTRL_LVL, 'O', 1, 'U', 2, 'A', 3, 'L', 4, 'D', 5, 'T', 6) , 5), LVL, 1, 0) AS EDIT,
                   REP_TRD_CD      ,
