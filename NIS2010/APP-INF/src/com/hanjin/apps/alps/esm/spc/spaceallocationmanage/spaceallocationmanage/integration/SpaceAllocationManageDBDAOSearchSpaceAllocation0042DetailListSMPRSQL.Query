	     CASE 
			 WHEN CUST_CTRL_CD = '+' THEN BKG_QUOTA
             WHEN RHQ_CD IS NULL AND CUST_CTRL_CD = 'C' THEN BKG_QUOTA - MAX(GUIDE_TTL) OVER() 
             WHEN RHQ_CD IS NULL AND CUST_CTRL_CD <> 'C' THEN CTRL_TTL_GUIDE
             WHEN RHQ_CD IS NOT NULL AND CUST_CTRL_CD <> 'C' AND US_MOD = '+' THEN GUIDE
--            WHEN RHQ_CD IS NOT NULL AND CUST_CTRL_CD = 'C' AND US_MOD = '+' THEN BKG_QUOTA - GUIDE_TTL2
             WHEN RHQ_CD IS NOT NULL AND CUST_CTRL_CD = 'C' AND US_MOD = '+' THEN BKG_QUOTA - OFC_TTL_GUIDE
			 ELSE 0
         END AS GUIDE,
