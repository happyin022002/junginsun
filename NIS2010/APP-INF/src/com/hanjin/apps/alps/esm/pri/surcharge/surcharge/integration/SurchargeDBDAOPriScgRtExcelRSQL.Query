<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOPriScgRtExcelRSQL">
			<desc><![CDATA[[CHM-201431588] Surcharge Inquiry 화면의 surcharge 개정 이력 관리 추가
2015.04.10 전지예 [CHM-201535041] Arrival Date 항목 추가
2015.05.18 전지예 [CHM-201535941] [Surcharge Inquiry] 조회화면 중 Arrival Date 컬럼 추가]]></desc>
			<sql><![CDATA[
SELECT 
       SVC_SCP_CD
     , CHG_CD
     , POR_DEF_CD
     , POL_DEF_CD
     , POD_DEF_CD
     , DEL_DEF_CD
     , RAT_UT_CD
     , PRC_CGO_TP_CD
     , SCG_IMDG_CLSS_CD
     , PSA_NO
     , CURR_CD
     , SCG_AMT
     , PAY_TERM_CD
     , EFF_DT
     , EXP_DT
     , CTRT_DT
     , ARR_DT
     , ACT_RAT_FLG
     , PRN_HDN_FLG
     , CNT_CD
     , STE_CD
     , CNL_TZ_CD
     , MIN_CGO_WGT
     , MAX_CGO_WGT     
     , ORG_TRSP_MOD_CD
     , DEST_TRSP_MOD_CD
     , PRC_RCV_TERM_CD
     , PRC_DE_TERM_CD
     , PRC_HNGR_BAR_TP_CD
     , SUB_TRD_CD
     , VSL_SLAN_CD
     , DIR_CALL_FLG
     , TML_CD
     , CMDT_CD
     , IO_GA_CD
     , TS_PORT_CD
     , SOC_FLG
     , CG_GRP_CMDT_CD
     , FD_GRD_FLG
     , RC_AIR_COND_TP_CD         
     , USA_SVC_MOD_CD
     , WDR_FLG
     , UPD_DT
     , SCG_CRTE_DY_KNT
     , SCG_PRD_TP_CD
     , SCG_PRD_CRTE_CD
     , UPD_USR_NM
     , SCG_RMK  
FROM (
SELECT '0' AS SEQ_ORDER
     , 'Scope' AS SVC_SCP_CD
     , 'Charge' AS CHG_CD
     , 'POR' AS POR_DEF_CD
     , 'POL' AS POL_DEF_CD
     , 'POD' AS POD_DEF_CD
     , 'DEL' AS DEL_DEF_CD
     , 'PER' AS RAT_UT_CD
     , 'Cargo Type' AS PRC_CGO_TP_CD
     , 'IMDG Class' AS SCG_IMDG_CLSS_CD
     , 'PSA Group' AS PSA_NO
     , 'Cur.' AS CURR_CD
     , 'Amount/Percentage(%)' AS SCG_AMT
     , 'Pay Term' AS PAY_TERM_CD
     , '     Effective Date     ' AS EFF_DT
     , 'Expiration Date     ' AS EXP_DT
     , 'Contract Date     ' AS CTRT_DT
     , 'Arrival Date     ' AS ARR_DT
     , 'Auto' AS ACT_RAT_FLG
     , 'Hide' AS PRN_HDN_FLG
     , 'Country' AS CNT_CD
     , 'State' AS STE_CD
     , 'Canal' AS CNL_TZ_CD
     , 'Weight MIN <= TON' AS MIN_CGO_WGT
     , 'Weight MAX > TON' AS MAX_CGO_WGT     
     , 'Trans. Mode Origin' AS ORG_TRSP_MOD_CD
     , 'Trans. Mode Dest' AS DEST_TRSP_MOD_CD
     , 'R/D Term Origin' AS PRC_RCV_TERM_CD
     , 'R/D Term Dest' AS PRC_DE_TERM_CD
     , 'Bar Type' AS PRC_HNGR_BAR_TP_CD
     , 'Sub trade' AS  SUB_TRD_CD
     , 'Lane' AS VSL_SLAN_CD
     , 'Direct Call' AS DIR_CALL_FLG
     , 'Terminal' AS TML_CD
     , 'Commodity' AS CMDT_CD
     , 'In/Out Gauge' AS IO_GA_CD
     , 'T/S Port' AS TS_PORT_CD
     , 'Shippers Own Container(S.O.C)' AS SOC_FLG
     , 'Commodity Group' AS CG_GRP_CMDT_CD
     , 'Food Grade' AS FD_GRD_FLG
     , 'RF Condition' AS RC_AIR_COND_TP_CD         
     , 'US Service Mode' AS USA_SVC_MOD_CD
     , 'Del Mark' AS WDR_FLG
     , 'Update Date      ' AS  UPD_DT
     , 'Days' AS SCG_CRTE_DY_KNT
     , 'Period Type' AS SCG_PRD_TP_CD
     , 'Period Criteria' AS SCG_PRD_CRTE_CD
     , 'User Name' AS UPD_USR_NM
     , 'Remark(s)' AS SCG_RMK     
FROM DUAL  
UNION ALL     
SELECT '1' AS SEQ_ORDER
     , A.SVC_SCP_CD AS SVC_SCP_CD
     , A.CHG_CD AS CHG_CD
     , A.POR_DEF_CD AS POR_DEF_CD
     , A.POL_DEF_CD AS POL_DEF_CD
     , A.POD_DEF_CD AS POD_DEF_CD
     , A.DEL_DEF_CD AS DEL_DEF_CD
     , A.RAT_UT_CD AS RAT_UT_CD
     , A.PRC_CGO_TP_CD AS PRC_CGO_TP_CD
     , A.SCG_IMDG_CLSS_CD AS SCG_IMDG_CLSS_CD
     , A.PSA_NO AS PSA_NO
     , A.CURR_CD AS CURR_CD
     , '' || (CASE (SELECT FLT_PCT_TP_CD FROM PRI_SCG_PRF WHERE SVC_SCP_CD = A.SVC_SCP_CD AND CHG_CD = A.CHG_CD) 
          WHEN 'F' THEN DECODE(SCG_AMT, 0, TO_CHAR(SCG_AMT), TO_CHAR(SCG_AMT, '999,999,999.99'))
          ELSE TO_CHAR(SCG_AMT) || '%'
       END) AS SCG_AMT
    -- , A.PAY_TERM_CD AS PAY_TERM_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01713' AND INTG_CD_VAL_CTNT(+) = A.PAY_TERM_CD) AS PAY_TERM_CD
     , TO_CHAR(A.EFF_DT, 'YYYY-MM-DD') AS EFF_DT
     , DECODE(TO_CHAR(A.EXP_DT, 'YYYYMMDD'), '99991231', NULL, TO_CHAR(A.EXP_DT, 'YYYY-MM-DD')) AS EXP_DT   
     , TO_CHAR(A.CTRT_DT, 'YYYY-MM-DD') AS CTRT_DT
     , TO_CHAR(A.ARR_DT, 'YYYY-MM-DD') AS ARR_DT
     , DECODE(A.ACT_RAT_FLG, 'Y', 'YES', 'N', 'NO') AS ACT_RAT_FLG
     , A.PRN_HDN_FLG AS PRN_HDN_FLG
     , A.CNT_CD AS CNT_CD
     , A.STE_CD AS STE_CD
     , A.CNL_TZ_CD AS CNL_TZ_CD
     , '' || A.MIN_CGO_WGT AS MIN_CGO_WGT
     , '' || A.MAX_CGO_WGT AS MAX_CGO_WGT
--     , A.ORG_TRSP_MOD_CD AS ORG_TRSP_MOD_CD
--     , A.DEST_TRSP_MOD_CD AS DEST_TRSP_MOD_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01720' AND INTG_CD_VAL_CTNT(+) = A.ORG_TRSP_MOD_CD) AS ORG_TRSP_MOD_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01720' AND INTG_CD_VAL_CTNT(+) = A.DEST_TRSP_MOD_CD) AS DEST_TRSP_MOD_CD
     
--     , A.PRC_RCV_TERM_CD AS PRC_RCV_TERM_CD
--     , A.PRC_DE_TERM_CD AS PRC_DE_TERM_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD02070' AND INTG_CD_VAL_CTNT(+) = A.PRC_RCV_TERM_CD) AS PRC_RCV_TERM_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD02071' AND INTG_CD_VAL_CTNT(+) = A.PRC_DE_TERM_CD) AS PRC_DE_TERM_CD
     
--     , A.PRC_HNGR_BAR_TP_CD AS PRC_HNGR_BAR_TP_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01708' AND INTG_CD_VAL_CTNT(+) = A.PRC_HNGR_BAR_TP_CD) AS PRC_HNGR_BAR_TP_CD
     , A.SUB_TRD_CD AS SUB_TRD_CD
     , A.VSL_SLAN_CD AS VSL_SLAN_CD   
     , DECODE(A.DIR_CALL_FLG, 'Y', 'YES', 'N', 'NO') AS DIR_CALL_FLG
     , A.TML_CD AS TML_CD
     , A.CMDT_CD AS CMDT_CD
--     , A.IO_GA_CD AS IO_GA_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD02142' AND INTG_CD_VAL_CTNT(+) = A.IO_GA_CD) AS IO_GA_CD
     , A.TS_PORT_CD AS TS_PORT_CD
     , DECODE(A.SOC_FLG, 'Y', 'YES', 'N', 'NO') AS SOC_FLG
     , A.SCG_GRP_CMDT_CD AS SCG_GRP_CMDT_CD
     , DECODE(A.FD_GRD_FLG, 'Y', 'YES', 'N', 'NO') AS FD_GRD_FLG
--     , A.RC_AIR_COND_TP_CD AS RC_AIR_COND_TP_CD    
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03187' AND INTG_CD_VAL_CTNT(+) = A.RC_AIR_COND_TP_CD) AS RC_AIR_COND_TP_CD
--     , A.USA_SVC_MOD_CD AS USA_SVC_MOD_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01729' AND INTG_CD_VAL_CTNT(+) = A.USA_SVC_MOD_CD) AS USA_SVC_MOD_CD
--     , DECODE(A.WDR_FLG, 'Y', '1', 'N', '0') AS WDR_FLG
     , A.WDR_FLG AS WDR_FLG
     , TO_CHAR(A.UPD_DT, 'YYYY-MM-DD') AS UPD_DT
     , '' || A.SCG_CRTE_DY_KNT AS SCG_CRTE_DY_KNT
--     , A.SCG_PRD_TP_CD AS SCG_PRD_TP_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD02955' AND INTG_CD_VAL_CTNT(+) = A.SCG_PRD_TP_CD) AS SCG_PRD_TP_CD
--     , A.SCG_PRD_CRTE_CD AS SCG_PRD_CRTE_CD
     ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD02957' AND INTG_CD_VAL_CTNT(+) = A.SCG_PRD_CRTE_CD) AS SCG_PRD_CRTE_CD
     , (SELECT USR_NM FROM COM_USER WHERE USR_ID = A.UPD_USR_ID) AS UPD_USR_NM
     , TRIM(A.SCG_RMK) AS SCG_RMK
FROM PRI_SCG_RT A
#if (${por_def_cd} != '')
     , (SELECT POR_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS POR_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS POR_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS POR_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT @[por_def_cd] AS POR_DEF_CD
                  FROM DUAL
                 WHERE LENGTH (@[por_def_cd]) = 4
                UNION 
                SELECT 'NULL' AS POR_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS POR_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT POR_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS POR_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS POR_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS POR_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[por_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[por_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                  WHERE A.DTL_LOC_DEF_CD = B.POR_DEF_CD
				 #if (${svc_scp_cd} != '') 
				   AND A.SVC_SCP_CD = @[svc_scp_cd]
				 #end
				 #if (${chg_cd} != '') 
                   AND A.CHG_CD = @[chg_cd]
				 #end
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) B
#end
#if (${pol_def_cd} != '')
     , (SELECT POL_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS POL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS POL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS POL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT @[pol_def_cd] AS POL_DEF_CD
                  FROM DUAL
                 WHERE LENGTH (@[pol_def_cd]) = 4
                UNION 
                SELECT 'NULL' AS POL_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS POL_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT POL_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS POL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS POL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS POL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pol_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pol_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                  WHERE A.DTL_LOC_DEF_CD = B.POL_DEF_CD
				 #if (${svc_scp_cd} != '') 
				   AND A.SVC_SCP_CD = @[svc_scp_cd]
				 #end
				 #if (${chg_cd} != '') 
                   AND A.CHG_CD = @[chg_cd]
				 #end
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) C
#end
#if (${pod_def_cd} != '')
     , (SELECT POD_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS POD_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS POD_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS POD_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT @[pod_def_cd] AS POD_DEF_CD
                  FROM DUAL
                 WHERE LENGTH (@[pod_def_cd]) = 4
                UNION 
                SELECT 'NULL' AS POD_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS POD_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT POD_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS POD_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS POD_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS POD_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[pod_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[pod_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                  WHERE A.DTL_LOC_DEF_CD = B.POD_DEF_CD
				 #if (${svc_scp_cd} != '') 
				   AND A.SVC_SCP_CD = @[svc_scp_cd]
				 #end
				 #if (${chg_cd} != '') 
                   AND A.CHG_CD = @[chg_cd]
				 #end
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) D
#end
#if (${del_def_cd} != '')
     , (SELECT DEL_DEF_CD
          FROM (
                SELECT DISTINCT(LOC_CD) AS DEL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(RGN_CD) AS DEL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                   AND DELT_FLG = 'N'
                UNION
                SELECT DISTINCT(CNT_CD) AS DEL_DEF_CD
                  FROM MDM_LOCATION
                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                   AND DELT_FLG = 'N'
                UNION 
                SELECT @[del_def_cd] AS DEL_DEF_CD
                  FROM DUAL
                 WHERE LENGTH (@[del_def_cd]) = 4
                UNION 
                SELECT 'NULL' AS DEL_DEF_CD
                  FROM DUAL
               )
         UNION
         SELECT A1.SCG_GRP_LOC_CD AS DEL_DEF_CD
          FROM PRI_SCG_GRP_LOC A1
             , (
                SELECT SVC_SCP_CD
                     , CHG_CD
                     , GRP_LOC_SEQ
                  FROM PRI_SCG_GRP_LOC_DTL A
                     , (SELECT DEL_DEF_CD
                          FROM (
                                SELECT DISTINCT(LOC_CD) AS DEL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(RGN_CD) AS DEL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                                   AND DELT_FLG = 'N'
                                UNION
                                SELECT DISTINCT(CNT_CD) AS DEL_DEF_CD
                                  FROM MDM_LOCATION
                                 WHERE DECODE(LENGTH(@[del_def_cd]), 5, LOC_CD, 3, RGN_CD, 2, CNT_CD) = @[del_def_cd]
                                   AND DELT_FLG = 'N'
                               )
                       ) B
                 WHERE A.DTL_LOC_DEF_CD = B.DEL_DEF_CD
				 #if (${svc_scp_cd} != '') 
				   AND A.SVC_SCP_CD = @[svc_scp_cd]
				 #end
				 #if (${chg_cd} != '') 
                   AND A.CHG_CD = @[chg_cd]
				 #end
               ) B1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
           AND A1.CHG_CD = B1.CHG_CD
           AND A1.GRP_LOC_SEQ = B1.GRP_LOC_SEQ
           AND A1.DELT_FLG = 'N'      
       ) E
#end
#if (${cntr_sz_cd} != '')
   , PRI_RAT_UT F
#end   
WHERE A.DELT_FLG = 'N'
#if (${svc_scp_cd} != '') 
AND A.SVC_SCP_CD = @[svc_scp_cd]
#end
#if (${chg_cd} != '') 
AND	A.CHG_CD = @[chg_cd]
#end
#if (${por_def_cd} != '') 
   AND NVL(A.POR_DEF_CD, 'NULL') = B.POR_DEF_CD
#end
#if (${pol_def_cd} != '') 
   AND NVL(A.POL_DEF_CD, 'NULL') = C.POL_DEF_CD
#end
#if (${pod_def_cd} != '') 
   AND NVL(A.POD_DEF_CD, 'NULL') = D.POD_DEF_CD
#end
#if (${del_def_cd} != '')
   AND NVL(A.DEL_DEF_CD, 'NULL') = E.DEL_DEF_CD
#end
#if (${prc_rcv_term_cd} != '')
AND	NVL(A.PRC_RCV_TERM_CD, 'NULL') IN (@[prc_rcv_term_cd], 'NULL')
#end
#if (${prc_de_term_cd} != '')
AND	NVL(A.PRC_DE_TERM_CD, 'NULL') IN (@[prc_de_term_cd], 'NULL')
#end
#if (${eff_dt} != '')
AND TO_DATE(REPLACE(@[eff_dt],'-',''), 'YYYYMMDD') <= A.EXP_DT  
AND TO_DATE(REPLACE(@[exp_dt],'-',''), 'YYYYMMDD') >= A.EFF_DT 
#end
#if (${upd_dt} != '')
AND	A.UPD_DT BETWEEN TO_DATE(REPLACE(@[upd_dt],'-','')||'000000', 'YYYYMMDDHH24MISS') AND TO_DATE(REPLACE(@[upd_dt],'-','')||'235959', 'YYYYMMDDHH24MISS')
#end
#if (${prc_cgo_tp_cd} != '')
AND	NVL(A.PRC_CGO_TP_CD, 'NULL') IN (@[prc_cgo_tp_cd], 'NULL')
#end
#if (${scg_imdg_clss_cd} != '')
AND NVL(A.SCG_IMDG_CLSS_CD, 'NULL') IN (@[scg_imdg_clss_cd], 'NULL')
#end
#if (${rat_ut_cd} != '')
AND A.RAT_UT_CD IN (
					SELECT   @[rat_ut_cd] RAT_UT_CD FROM DUAL
#if (${is_num} == 'Y')
					UNION
					SELECT   RAT_UT_CD
					FROM     PRI_RAT_UT
					WHERE    CNTR_SZ_CD = (SELECT   CNTR_SZ_CD
                       					   FROM     PRI_RAT_UT
                       					   WHERE    RAT_UT_CD = @[rat_ut_cd])
#else
					UNION
					SELECT RAT_UT_CD
					FROM(
						SELECT   RAT_UT_CD, RANK() OVER (ORDER BY RAT_UT_CD) RNUM
						FROM     PRI_RAT_UT
						WHERE    CNTR_SZ_CD = (SELECT   CNTR_SZ_CD
                       					   		FROM     PRI_RAT_UT
                       					  		 WHERE    RAT_UT_CD = @[rat_ut_cd])
						)
					WHERE RNUM = 1

#end
					UNION ALL
					SELECT   RAT_UT_CD
					FROM     PRI_RAT_UT
					WHERE    CNTR_SZ_CD IS NULL
     				AND (SELECT   CNTR_SZ_CD
          				 FROM     PRI_RAT_UT
          				 WHERE    RAT_UT_CD = @[rat_ut_cd]) IS NOT NULL
					)
#end
#if (${cntr_sz_cd} != '')
AND F.CNTR_SZ_CD = @[cntr_sz_cd]
AND F.DELT_FLG = 'N'
AND A.RAT_UT_CD = F.RAT_UT_CD
#end
#if (${wdr_flg} == '')
AND A.WDR_FLG = 'N'
#end
#if (${type_cd} == 'L')
AND A.SVC_SCP_CD IN
				(
				SELECT   A.SVC_SCP_CD
				FROM     MDM_SVC_SCP A
        			   ,(SELECT   DISTINCT (B.SVC_SCP_CD) AS SVC_SCP_CD
          				 FROM     MDM_LOCATION A, MDM_SVC_SCP_LMT B
          				 WHERE    A.LOC_CD = @[por_def_cd]
               			 AND A.DELT_FLG = 'N'
               			 AND B.RGN_CD = A.RGN_CD
               		  	 AND B.ORG_DEST_CD = 'O'
               			 AND B.DELT_FLG = 'N') O
        			   ,(SELECT   DISTINCT (B.SVC_SCP_CD) AS SVC_SCP_CD
          				 FROM     MDM_LOCATION A, MDM_SVC_SCP_LMT B
          				 WHERE    A.LOC_CD = @[del_def_cd]
               			 AND A.DELT_FLG = 'N'
               			 AND B.RGN_CD = A.RGN_CD
               			 AND B.ORG_DEST_CD = 'D'
               			 AND B.DELT_FLG = 'N') D
				WHERE    A.SVC_SCP_CD = O.SVC_SCP_CD
     			AND O.SVC_SCP_CD = D.SVC_SCP_CD
     			AND A.DELT_FLG = 'N'
				)
#elseif(${type_cd} == 'R')
AND A.SVC_SCP_CD IN
				(
                SELECT   A.SVC_SCP_CD
                FROM     MDM_SVC_SCP A
                        ,(SELECT   DISTINCT (SVC_SCP_CD) AS SVC_SCP_CD
                          FROM     MDM_SVC_SCP_LMT
                          WHERE    RGN_CD = @[por_def_cd]
                               AND ORG_DEST_CD = 'O'
                               AND DELT_FLG = 'N') O
                        ,(SELECT   DISTINCT (SVC_SCP_CD) AS SVC_SCP_CD
                          FROM     MDM_SVC_SCP_LMT
                          WHERE    RGN_CD = @[del_def_cd]
                               AND ORG_DEST_CD = 'D'
                               AND DELT_FLG = 'N') D
                WHERE    A.SVC_SCP_CD = O.SVC_SCP_CD
                     AND O.SVC_SCP_CD = D.SVC_SCP_CD
                     AND A.DELT_FLG = 'N'
				)
#elseif (${type_cd} == 'C')
AND A.SVC_SCP_CD IN
				(				
				SELECT   A.SVC_SCP_CD
                FROM     MDM_SVC_SCP A
                        ,(SELECT   DISTINCT (C.SVC_SCP_CD) AS SVC_SCP_CD
                          FROM     MDM_COUNTRY A, MDM_REGION B, MDM_SVC_SCP_LMT C
                          WHERE    A.CNT_CD = @[por_def_cd]
                               AND A.DELT_FLG = 'N'
                               AND B.CNT_CD = A.CNT_CD
                               AND B.DELT_FLG = 'N'
                               AND C.RGN_CD = B.RGN_CD
                               AND C.ORG_DEST_CD = 'O'
                               AND C.DELT_FLG = 'N') O
                        ,(SELECT   DISTINCT (C.SVC_SCP_CD) AS SVC_SCP_CD
                          FROM     MDM_COUNTRY A, MDM_REGION B, MDM_SVC_SCP_LMT C
                          WHERE    A.CNT_CD = @[del_def_cd]
                               AND A.DELT_FLG = 'N'
                               AND B.CNT_CD = A.CNT_CD
                               AND B.DELT_FLG = 'N'
                               AND C.RGN_CD = B.RGN_CD
                               AND C.ORG_DEST_CD = 'D'
                               AND C.DELT_FLG = 'N') D
                WHERE    A.SVC_SCP_CD = O.SVC_SCP_CD
                     AND O.SVC_SCP_CD = D.SVC_SCP_CD
                     AND A.DELT_FLG = 'N'
				)
#end
)
ORDER BY 
       SEQ_ORDER
     , SVC_SCP_CD
     , CHG_CD
     , POR_DEF_CD
     , POL_DEF_CD
     , POD_DEF_CD
     , DEL_DEF_CD
     , RAT_UT_CD
     , PRC_CGO_TP_CD
     , SCG_IMDG_CLSS_CD
     , PSA_NO
     , CURR_CD
     , PAY_TERM_CD
     , EFF_DT
     , EXP_DT
     , CTRT_DT
     , ARR_DT
     , ACT_RAT_FLG
     , PRN_HDN_FLG
     , CNT_CD
     , STE_CD
     , CNL_TZ_CD
     , MIN_CGO_WGT
     , MAX_CGO_WGT     
     , ORG_TRSP_MOD_CD
     , DEST_TRSP_MOD_CD
     , PRC_RCV_TERM_CD
     , PRC_DE_TERM_CD
     , PRC_HNGR_BAR_TP_CD
     , SUB_TRD_CD
     , VSL_SLAN_CD
     , DIR_CALL_FLG
     , TML_CD
     , CMDT_CD
     , IO_GA_CD
     , TS_PORT_CD
     , SOC_FLG
     , CG_GRP_CMDT_CD
     , FD_GRD_FLG
     , RC_AIR_COND_TP_CD         
     , USA_SVC_MOD_CD
     , WDR_FLG
     , UPD_DT
     , SCG_CRTE_DY_KNT
     , SCG_PRD_TP_CD
     , SCG_PRD_CRTE_CD
     , UPD_USR_NM
     , SCG_RMK			]]></sql>
			<params>
				<param name="por_def_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="pol_def_cd" type="12" value="" out="N"/>
				<param name="pod_def_cd" type="12" value="" out="N"/>
				<param name="del_def_cd" type="12" value="" out="N"/>
				<param name="prc_rcv_term_cd" type="12" value="" out="N"/>
				<param name="prc_de_term_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="prc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="scg_imdg_clss_cd" type="12" value="" out="N"/>
				<param name="rat_ut_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
