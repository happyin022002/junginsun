<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchCustNmBlockWordRSQL">
			<desc><![CDATA[금지 Customer Name 을 조회하여 이를 포함하면 save blocking 한다]]></desc>
			<sql><![CDATA[
SELECT BLCK_KW_NM
  FROM BKG_BLCK_KW_LIST
 WHERE BLCK_KW_TP_CD='BLA'
   AND ' '||UPPER(@[cust_nm])||' ' LIKE '% '||UPPER(BLCK_KW_NM)||' %'    
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cust_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
