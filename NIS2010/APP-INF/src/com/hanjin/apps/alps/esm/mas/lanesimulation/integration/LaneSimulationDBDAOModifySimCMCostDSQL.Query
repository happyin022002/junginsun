<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOModifySimCMCostDSQL">
			<desc><![CDATA[CmCost 삭제]]></desc>
			<sql><![CDATA[
DELETE 
FROM MAS_SIM_CTRB_MGN_COST 
WHERE SIM_DT  = @[f_sim_dt]  
  AND SIM_NO  = @[f_sim_no]  
  AND SECT_NO = @[f_sect_no]  
  AND SGRP_COST_CD = @[f_sgrp_cost_cd]			]]></sql>
			<params>
				<param name="f_sim_dt" type="12" value="" out="N"/>
				<param name="f_sim_no" type="12" value="" out="N"/>
				<param name="f_sect_no" type="12" value="" out="N"/>
				<param name="f_sgrp_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
