<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOremoveCustAdvisoryNoticeByUploadDSQL">
			<desc><![CDATA[기존에 등록된 VVD별 Customer Advisory Notice 대상 정보를 삭제처리한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CUST_AVC_NTC_BL
WHERE BL_NO IN ( SELECT 	DISTINCT BK.BL_NO
                 FROM BKG_BOOKING BK,
                          BKG_VVD     BV
                 WHERE BV.VSL_CD     = SUBSTR(@[vvd], 1,4)
                 AND   BV.SKD_VOY_NO = SUBSTR(@[vvd], 5,4)
                 AND   BV.SKD_DIR_CD IN (${dir_sts_cd})
                 AND   BK.BKG_NO     = BV.BKG_NO
                 AND   BK.BKG_STS_CD <>'X'
                )
AND   SRC_DAT_TP_CD  = 'E'			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
