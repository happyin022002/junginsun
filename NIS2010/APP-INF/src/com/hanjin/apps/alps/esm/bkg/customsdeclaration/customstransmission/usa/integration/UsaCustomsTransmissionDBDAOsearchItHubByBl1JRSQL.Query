<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchItHubByBl1JRSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신 outVO : None, 연관VO: UsaResultVO]]></desc>
			<sql><![CDATA[
SELECT NVL(B.HUB_LOC_CD,'USMKC')  cus_loc
FROM
    BKG_CSTMS_ADV_RSLT R, 
    BKG_CSTMS_ADV_BL B
WHERE B.CNT_CD = 'US'
  AND B.BL_NO = @[bl_no]
  AND R.BL_NO = B.BL_NO
  AND R.CNT_CD = B.CNT_CD
  AND R.DSPO_CD = '1J'
  AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
