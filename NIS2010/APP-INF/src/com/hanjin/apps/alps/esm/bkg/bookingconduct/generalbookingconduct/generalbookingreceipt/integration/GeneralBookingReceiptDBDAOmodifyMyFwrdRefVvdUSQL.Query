   MY_FWRD_CD = @[my_fwrd_ref_cd]
 , MY_FWRD_VSL_DESC = @[my_fwrd_vsl_desc]
 , upd_usr_id = @[usr_id]
 , upd_dt = sysdate
WHERE BKG_NO = @[bkg_no]			]]></sql>
