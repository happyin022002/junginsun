<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchRestrictCmdtFileRSQL">
			<desc><![CDATA[BKG_IMP_IMG_STO 조회]]></desc>
			<sql><![CDATA[
SELECT
RGN_OFC_CD
,DECODE(LOC_CD,'*****','ALL',LOC_CD) LOC_CD
,DP_SEQ
,IMG_SEQ
,FILE_NM
,FILE_SIZE
,FILE_SAV_ID
FROM (
SELECT
MIN(IMG.RGN_OFC_CD) AS RGN_OFC_CD
,MIN(IMG.LOC_CD) AS LOC_CD
,MIN(IMG.DP_SEQ) AS DP_SEQ
,MIN(IMG.IMG_SEQ) AS IMG_SEQ
,MIN(IMG.FILE_NM) AS FILE_NM
,MIN(UPLD.FILE_SZ_CAPA) AS FILE_SIZE
,IMG.FILE_SAV_ID AS FILE_SAV_ID
FROM
BKG_IMP_IMG_STO IMG, COM_UPLD_FILE UPLD
WHERE    IMG.FILE_SAV_ID = UPLD.FILE_SAV_ID(+)
AND IMG.RGN_OFC_CD = @[rgn_ofc_cd]
AND IMG.LOC_CD     = DECODE(@[loc_cd],'ALL','*****',@[loc_cd])
AND IMG.CNT_CD     = @[cnt_cd]
AND IMG.DP_SEQ     = @[dp_seq]
GROUP BY IMG.FILE_SAV_ID
) MAIN_TABLE			]]></sql>
			<params>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="dp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
