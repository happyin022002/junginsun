<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOSearchIHCDgOverWeightFlagRSQL">
			<desc><![CDATA[2013.08.01 전윤주 [CHM-201326002] Overweight, DG Service flag가 'Y' 인 경우에만 버튼 색을 파란색으로 표시]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN SPCL_COUNT >= 1 THEN 'Y'
            ELSE 'N'
       END OVR_WGT_CGO_SVC_FLG
 FROM(
    SELECT COUNT(*) SPCL_COUNT
     FROM PRI_TRF_IHC_SPCL_CGO_RT
    WHERE 1=1
      AND SVC_SCP_CD = @[svc_scp_cd]
      AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
      AND IHC_TRF_NO = @[ihc_trf_no]
      AND (DCGO_SVC_FLG = 'Y' OR OVR_WGT_CGO_SVC_FLG = 'Y') -- service flag가 Y인 경우만 파란색으로 표시
    )			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
