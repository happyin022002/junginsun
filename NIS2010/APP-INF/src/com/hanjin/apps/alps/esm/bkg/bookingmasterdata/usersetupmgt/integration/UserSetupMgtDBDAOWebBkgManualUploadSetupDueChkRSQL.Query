<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOWebBkgManualUploadSetupDueChkRSQL">
			<desc><![CDATA[Web Booking Manual Upload Setup Table 등록 항목이 중복되는지 체크]]></desc>
			<sql><![CDATA[
SELECT *
FROM BKG_SYS_UPLD_BLCK_STUP 
WHERE DELT_FLG = 'N'
#if(${bkg_ofc_cd} != '')
    AND BKG_OFC_CD = @[bkg_ofc_cd]
#end
#if(${vsl_slan_cd} != '')
    AND VSL_SLAN_CD = @[vsl_slan_cd]
#else    
    AND VSL_SLAN_CD IS null
#end
#if(${vvd} != '')
    AND VSL_CD = substr(@[vvd], 1, 4)
    AND SKD_VOY_NO = substr(@[vvd], 5, 4)
    AND DIR_CD = substr(@[vvd], 9, 1)
#else    
    AND VSL_CD IS null
    AND SKD_VOY_NO IS null
    AND DIR_CD IS null
#end
#if(${cust_cnt_cd} != '')
    AND CUST_CNT_CD = SUBSTR(@[cust_cnt_cd],1,2)
    AND CUST_SEQ = SUBSTR(@[cust_cnt_cd],3)
#else    
    AND CUST_CNT_CD IS null
    AND CUST_SEQ IS null
#end
#if(${pol_cnt_cd} != '')
    AND POL_CNT_CD = @[pol_cnt_cd]
#else    
    AND POL_CNT_CD IS null
#end
#if(${pol_cd} != '')
    AND POL_CD = @[pol_cd]
#else    
    AND POL_CD IS null
#end
#if(${pod_cnt_cd} != '')
    AND POD_CNT_CD = @[pod_cnt_cd]
#else    
    AND POD_CNT_CD IS null
#end
#if(${pod_cd} != '')
    AND POD_CD = @[pod_cd]
#else    
    AND POD_CD IS null
#end
#if(${xter_rmk} != '')
    AND XTER_RMK = @[xter_rmk]
#else    
    AND XTER_RMK IS null
#end
#if(${lodg_dir_cd} != '')
    AND LODG_DIR_CD = @[lodg_dir_cd]
#else    
    AND LODG_DIR_CD IS null
#end			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="pol_cnt_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cnt_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="xter_rmk" type="12" value="" out="N"/>
				<param name="lodg_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
