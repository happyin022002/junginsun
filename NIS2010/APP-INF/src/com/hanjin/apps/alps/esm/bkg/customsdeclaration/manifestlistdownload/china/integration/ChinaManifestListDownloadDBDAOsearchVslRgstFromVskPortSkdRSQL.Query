<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOsearchVslRgstFromVskPortSkdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT VVD, 
       SLAN_CD,
       CRR_CD,
       VSL_ENG_NM,
       LLOYD_NO,
       VSL_CD,
       IB_SKD_VOY_NO,
       IB_SKD_DIR_NM,
       OB_SKD_VOY_NO,
       OB_SKD_DIR_NM,
       MAX(ETA_DT) AS ETA_DT,
       MAX(ETB_DT) AS ETB_DT,
       MAX(ETD_DT) AS ETD_DT,
       MF_RMK

  FROM (
        SELECT DISTINCT
               SKD.VSL_CD||SKD.SKD_VOY_NO||SKD.SKD_DIR_CD AS VVD,
               SKD.SLAN_CD,
               DECODE((SELECT T1.ACT_CRR_CD
                         FROM VSK_VSL_SKD T1
                        WHERE 1 = 1
                          AND SKD.VSL_CD = T1.VSL_CD
                          AND SKD.SKD_VOY_NO = T1.SKD_VOY_NO
                          AND SKD.SKD_DIR_CD = T1.SKD_DIR_CD),
                      NULL,
                      CNTR.CRR_CD,
                      (SELECT T1.ACT_CRR_CD
                         FROM VSK_VSL_SKD T1
                        WHERE 1 = 1
                          AND SKD.VSL_CD = T1.VSL_CD
                          AND SKD.SKD_VOY_NO = T1.SKD_VOY_NO
                          AND SKD.SKD_DIR_CD = T1.SKD_DIR_CD)
                     ) AS CRR_CD,
               CNTR.VSL_ENG_NM,
               CNTR.LLOYD_NO,
               SKD.VSL_CD,
               (SELECT CHN.IB_SKD_VOY_NO
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD= CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS IB_SKD_VOY_NO,
               (SELECT CHN.IB_SKD_DIR_NM
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS IB_SKD_DIR_NM,
               (SELECT CHN.OB_SKD_VOY_NO
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS OB_SKD_VOY_NO,
               (SELECT CHN.OB_SKD_DIR_NM
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS OB_SKD_DIR_NM,
               TO_CHAR(SKD.VPS_ETA_DT, 'YYYY-MM-DD') AS ETA_DT,
               TO_CHAR(SKD.VPS_ETB_DT, 'YYYY-MM-DD') AS ETB_DT,
               TO_CHAR(SKD.VPS_ETD_DT, 'YYYY-MM-DD') AS ETD_DT,
               (SELECT CHN.MF_RMK
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS MF_RMK

          FROM VSK_VSL_PORT_SKD SKD,
               MDM_VSL_SVC_LANE LANE,
               MDM_VSL_CNTR CNTR

         WHERE SKD.VPS_PORT_CD = @[port_cd]
#if (${date_fm} != '' && ${date_to} != '')
           AND SKD.VPS_ETB_DT BETWEEN TO_DATE(@[date_fm], 'YYYY-MM-DD') AND TO_DATE(@[date_to], 'YYYY-MM-DD') + 0.99999
#end
#if (${vvd} != '')
           AND SKD.VSL_CD = SUBSTR(@[vvd], 1, 4)
           AND SKD.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
           AND SKD.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
#end
           AND SKD.SLAN_CD = LANE.VSL_SLAN_CD(+)
           AND LANE.VSL_SVC_TP_CD <> 'O'
           AND SKD.VSL_CD = CNTR.VSL_CD(+))

 WHERE 1 = 1
#if (${crr_cd} != '')
   AND CRR_CD = @[crr_cd]
#end
#if (${slan_cd} != '')
   AND SLAN_CD = @[slan_cd]
#end

 GROUP BY VVD,
          SLAN_CD,
          CRR_CD,
          VSL_ENG_NM,
          LLOYD_NO,
          VSL_CD,
          IB_SKD_VOY_NO,
          IB_SKD_DIR_NM,
          OB_SKD_VOY_NO,
          OB_SKD_DIR_NM,
          MF_RMK


UNION ALL


SELECT VVD,
       SLAN_CD,
       CRR_CD,
       VSL_ENG_NM,
       LLOYD_NO,
       VSL_CD,
       IB_SKD_VOY_NO,
       IB_SKD_DIR_NM,
       OB_SKD_VOY_NO,
       OB_SKD_DIR_NM,
       MAX(ETA_DT) AS ETA_DT,
       MAX(ETB_DT) AS ETB_DT,
       MAX(ETD_DT) AS ETD_DT,
       MF_RMK

  FROM (
        SELECT DISTINCT
               SKD.VSL_CD||SKD.SKD_VOY_NO||SKD.SKD_DIR_CD AS VVD,
               SKD.SLAN_CD,
               DECODE((SELECT T1.ACT_CRR_CD
                         FROM VSK_VSL_SKD T1
                        WHERE 1 = 1
                          AND SKD.VSL_CD = T1.VSL_CD
                          AND SKD.SKD_VOY_NO = T1.SKD_VOY_NO
                          AND SKD.SKD_DIR_CD = T1.SKD_DIR_CD),
                      NULL,
                      CNTR.CRR_CD,
                      (SELECT T1.ACT_CRR_CD
                         FROM VSK_VSL_SKD T1
                        WHERE 1 = 1
                          AND SKD.VSL_CD = T1.VSL_CD
                          AND SKD.SKD_VOY_NO = T1.SKD_VOY_NO
                          AND SKD.SKD_DIR_CD = T1.SKD_DIR_CD)
                     ) AS CRR_CD,
               CNTR.VSL_ENG_NM,
               CNTR.LLOYD_NO,
               SKD.VSL_CD,
               (SELECT CHN.IB_SKD_VOY_NO
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD= CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS IB_SKD_VOY_NO,
               (SELECT CHN.IB_SKD_DIR_NM
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS IB_SKD_DIR_NM,
               (SELECT CHN.OB_SKD_VOY_NO
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS OB_SKD_VOY_NO,
               (SELECT CHN.OB_SKD_DIR_NM
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS OB_SKD_DIR_NM,
               TO_CHAR(SKD.VPS_ETA_DT, 'YYYY-MM-DD') AS ETA_DT,
               TO_CHAR(SKD.VPS_ETB_DT, 'YYYY-MM-DD') AS ETB_DT,
               TO_CHAR(SKD.VPS_ETD_DT, 'YYYY-MM-DD') AS ETD_DT,
               (SELECT CHN.MF_RMK
                  FROM BKG_CSTMS_CHN_CORR_VVD CHN
                 WHERE SKD.VSL_CD = CHN.VSL_CD
                   AND SKD.SKD_VOY_NO = CHN.SKD_VOY_NO
                   AND SKD.SKD_DIR_CD = CHN.SKD_DIR_CD
                   AND SKD.VPS_PORT_CD = CHN.PORT_CD) AS MF_RMK

          FROM VSK_VSL_PORT_SKD SKD,
               MDM_VSL_SVC_LANE LANE,
               MDM_VSL_CNTR CNTR

         WHERE SKD.VPS_PORT_CD = @[port_cd]
#if (${date_fm} != '' && ${date_to} != '')
           AND SKD.VPS_ETB_DT BETWEEN TO_DATE(@[date_fm], 'YYYY-MM-DD') AND TO_DATE(@[date_to], 'YYYY-MM-DD') + 0.99999
#end
#if (${vvd} != '')
           AND SKD.VSL_CD = SUBSTR(@[vvd], 1, 4)
           AND SKD.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
           AND SKD.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
#end
           AND SKD.SLAN_CD = LANE.VSL_SLAN_CD(+)
           AND LANE.VSL_SLAN_CD IN ('ACJ', 'AKC')
           AND SKD.VSL_CD = CNTR.VSL_CD(+))

 WHERE 1 = 1
#if (${crr_cd} != '')
   AND CRR_CD = @[crr_cd]
#end
#if (${slan_cd} != '')
   AND SLAN_CD = @[slan_cd]
#end

 GROUP BY VVD,
          SLAN_CD,
          CRR_CD,
          VSL_ENG_NM,
          LLOYD_NO,
          VSL_CD,
          IB_SKD_VOY_NO,
          IB_SKD_DIR_NM,
          OB_SKD_VOY_NO,
          OB_SKD_DIR_NM,
          MF_RMK

 ORDER BY ETA_DT			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="date_fm" type="12" value="" out="N"/>
				<param name="date_to" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
