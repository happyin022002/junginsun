<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGuidelineMainDBDAOChkGlEffDtRSQL">
			<desc><![CDATA[기간이 유효한지 검색]]></desc>
			<sql><![CDATA[
SELECT gline_seq
  FROM pri_sg_mn
 WHERE svc_scp_cd = @[svc_scp_cd]
#if (${HAS_GLINE_SEQ} == 'Y')
   AND gline_seq <> @[gline_seq]
#end
#if (${BACKUP} == 'Y')
   AND (to_char(eff_dt, 'YYYY-MM-DD') BETWEEN @[eff_dt] AND @[exp_dt]
        OR to_char(exp_dt, 'YYYY-MM-DD') BETWEEN @[eff_dt] AND @[exp_dt]
        OR (to_char(eff_dt, 'YYYY-MM-DD') <= @[eff_dt] AND to_char(exp_dt, 'YYYY-MM-DD') >= @[exp_dt]))
#end
   AND to_char(eff_dt, 'YYYY-MM-DD') = @[eff_dt]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
