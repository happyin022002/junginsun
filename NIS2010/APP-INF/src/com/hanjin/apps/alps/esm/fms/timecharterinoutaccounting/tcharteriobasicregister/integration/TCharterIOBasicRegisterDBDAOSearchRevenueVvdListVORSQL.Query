<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDBDAOSearchRevenueVvdListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	REV_DIR_CD
,	REV_YRMON
,	SLAN_CD
,	RLANE_CD
,	VST_DT
,	VED_DT
,	VSL_CD||SKD_VOY_NO||SKD_DIR_CD||REV_DIR_CD VVD_CD
,	CRE_USR_ID
,	UPD_USR_ID
FROM FMS_VVD
WHERE	REV_YRMON = @[rev_yrmon]
AND	SLAN_CD like @[slan_cd]||'%'
AND	RLANE_CD like @[rlane_cd]||'%'			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
