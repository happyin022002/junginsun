<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAORsltSpScpGrpLocDtlListVORSQL">
			<desc><![CDATA[RsltSpScpGrpLocDtlListVO]]></desc>
			<sql><![CDATA[
SELECT A.LOC_CD
	 , B.LOC_NM
	 , B.SCONTI_CD
	 , B.RGN_CD
  FROM PRI_SP_SCP_GRP_LOC_DTL A
     , MDM_LOCATION B
 WHERE A.LOC_CD = B.LOC_CD
   AND A.SVC_SCP_CD = @[svc_scp_cd]
   AND A.AMDT_SEQ = @[amdt_seq]
   AND A.PROP_NO = @[prop_no]
   AND A.GRP_LOC_SEQ = @[combo_grp_loc_cd]
 ORDER BY A.LOC_CD ASC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="combo_grp_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
