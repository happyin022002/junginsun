<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupCommodityGuidelineDBDAOPriRgGrpCmdtExcelRSQL">
			<desc><![CDATA[EXCEL 다운로드]]></desc>
			<sql><![CDATA[
SELECT B.PRC_GRP_CMDT_CD, 
       B.PRC_GRP_CMDT_DESC,
       A.PRC_CMDT_DEF_CD,
       CASE WHEN A.PRC_CMDT_TP_CD = 'C' THEN C.CMDT_NM
            WHEN A.PRC_CMDT_TP_CD = 'R' THEN D.REP_CMDT_NM
            END CMDT_NM
FROM PRI_RG_GRP_CMDT_DTL A
	 , PRI_RG_GRP_CMDT B
	 , MDM_COMMODITY C
	 , MDM_REP_CMDT D
WHERE B.SVC_SCP_CD 		= A.SVC_SCP_CD
AND   B.GLINE_SEQ 		= A.GLINE_SEQ
AND   B.GRP_CMDT_SEQ 	= A.GRP_CMDT_SEQ
AND A.PRC_CMDT_DEF_CD = C.CMDT_CD(+)
AND A.PRC_CMDT_DEF_CD = D.REP_CMDT_CD(+)
AND   B.SVC_SCP_CD 		= @[svc_scp_cd]
AND   B.GLINE_SEQ 		= @[gline_seq]
ORDER BY B.PRC_GRP_CMDT_CD, A.PRC_CMDT_DEF_CD
			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
