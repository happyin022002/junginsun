<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOaddFiledCancelProposalCSQL">
			<desc><![CDATA[* 2012.04.18 이석준[CHM-201217045-01] S/C Filed Cancel 기능및 조회 기능 추가]]></desc>
			<sql><![CDATA[
insert into PRI_SP_FILE_CXL_HIS
(FILE_CXL_SEQ,
 RQST_DT,RQST_USR_NM,RQST_USR_ID,RQST_OFC_CD,CRE_USR_NM,CRE_USR_ID,FILE_CXL_RSN,
 SC_NO,PROP_NO,AMDT_SEQ,CRE_DT,UPD_USR_ID,UPD_DT)
SELECT  nvl((SELECT MAX(FILE_CXL_SEQ) FROM PRI_SP_FILE_CXL_HIS)+1,1),
        to_date(@[rqst_dt],'yyyy-mm-dd') rqst_dt,@[rqst_usr_nm],@[rqst_usr_id],@[rqst_ofc_cd],@[cre_usr_nm],@[cre_usr_id],@[file_cxl_rsn],
        @[sc_no],@[prop_no],@[amdt_seq],sysdate,@[upd_usr_id],sysdate
from dual			]]></sql>
			<params>
				<param name="rqst_dt" type="12" value="" out="N"/>
				<param name="rqst_usr_nm" type="12" value="" out="N"/>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="file_cxl_rsn" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
