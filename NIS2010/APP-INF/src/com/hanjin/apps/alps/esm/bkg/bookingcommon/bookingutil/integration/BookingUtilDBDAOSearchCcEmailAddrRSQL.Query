<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchCcEmailAddrRSQL">
			<desc><![CDATA[User ID별 CC EMAIL 가져오기]]></desc>
			<sql><![CDATA[
SELECT	BKG_USER_RCV_EMAIL_FNC(@[usr_id],@[ntc_knd_cd]) AS EMAIL
FROM	DUAL			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ntc_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
