<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAORsltNewRoutCaseNoRSQL">
			<desc><![CDATA[CmdtHdrSeq 구하기]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(ROUT_CS_NO),0) +1 AS ROUT_CS_NO , @[para_info_ctnt] AS ROUT_CS_SRC_DT
FROM (
    select max(ROUT_CS_NO) AS ROUT_CS_NO
    from PRI_PRS_ROUT_CS
	WHERE ROUT_CS_CLSS_NO = @[prs_bat_id]
    UNION ALL
    select max(ROUT_CS_NO)
    from PRI_PRS_USD_ROUT_CS_INFO 
    where ROUT_CS_SRC_DT = @[para_info_ctnt]  

)			]]></sql>
			<params>
				<param name="para_info_ctnt" type="12" value="" out="N"/>
				<param name="prs_bat_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
