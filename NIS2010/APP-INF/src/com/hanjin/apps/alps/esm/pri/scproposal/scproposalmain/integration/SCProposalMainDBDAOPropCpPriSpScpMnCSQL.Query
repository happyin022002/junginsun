<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOPropCpPriSpScpMnCSQL">
			<desc><![CDATA[Proposal Scope Copy PRI_SP_SCP_MN Insert
2012.02.03 이석준[CHM-201215685] CHSS_EXPT_FLG, GRI_APPL_FLG, NEW_SCG_FLG 추가 컬럼 조회 반영]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_MN (
      PROP_NO
    , AMDT_SEQ
    , SVC_SCP_CD
    , EFF_DT
    , EXP_DT
    , PROP_SCP_SREP_CD
    , PROP_SCP_OFC_CD
    , PROP_SCP_APRO_OFC_CD
    , PROP_SCP_STS_CD
    , NOTE_HDR_SEQ
    , CHSS_EXPT_FLG
    , GRI_APPL_FLG
    , NEW_SCG_FLG
    , CRE_USR_ID
    , CRE_DT 
    , UPD_USR_ID
    , UPD_DT
)    
SELECT @[new_prop_no] AS PROP_NO
     , 0 AS AMDT_SEQ
     , SVC_SCP_CD
     , TO_DATE('99991231', 'YYYYMMDD') AS EFF_DT
     , TO_DATE('99991231', 'YYYYMMDD') AS EXP_DT
     , PROP_SCP_SREP_CD
     , PROP_SCP_OFC_CD
     , PROP_SCP_APRO_OFC_CD
     , 'I' AS PROP_SCP_STS_CD
     , NULL AS NOTE_HDR_SEQ
     , CHSS_EXPT_FLG
     , GRI_APPL_FLG
     , NEW_SCG_FLG
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM PRI_SP_SCP_MN
WHERE PROP_NO = @[prop_no]
AND   AMDT_SEQ = @[amdt_seq]
AND   SVC_SCP_CD = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
