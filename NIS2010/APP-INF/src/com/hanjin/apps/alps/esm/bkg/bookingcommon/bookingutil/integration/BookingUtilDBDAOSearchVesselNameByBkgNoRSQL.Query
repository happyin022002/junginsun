<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchVesselNameByBkgNoRSQL">
			<desc><![CDATA[Receipt Notice 메일 내용에 포함 될 Vessel Name 과 Lane 을 조회 한다.]]></desc>
			<sql><![CDATA[
SELECT ( SELECT VSL_ENG_NM 
           FROM MDM_VSL_CNTR 
          WHERE VSL_CD = BK.VSL_CD 
            AND DELT_FLG = 'N'
            AND ROWNUM = 1 ) 
       || ' ' || BK.SKD_VOY_NO||BK.SKD_DIR_CD  
       || ' ('|| BK.SLAN_CD || ')' AS VSL_ENG_NM    
  FROM BKG_BOOKING BK
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
