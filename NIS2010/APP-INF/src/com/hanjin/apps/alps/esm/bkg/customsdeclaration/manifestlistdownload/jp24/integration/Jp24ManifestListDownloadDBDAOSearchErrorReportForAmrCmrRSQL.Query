<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOSearchErrorReportForAmrCmrRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(C.ATTR_CTNT1, 3) AS ATTR_CTNT1,
       C.ATTR_CTNT2,
       (SELECT REPLACE(M.ATTR_CTNT4, '\r', '\n')
          FROM BKG_CSTMS_CD_CONV_CTNT M
         WHERE M.ATTR_CTNT1 = C.ATTR_CTNT1
           AND ROWNUM = 1) AS ATTR_CTNT4,
       (SELECT REPLACE(M.ATTR_CTNT5, '\r', '\n')
          FROM BKG_CSTMS_CD_CONV_CTNT M
         WHERE M.ATTR_CTNT1 = C.ATTR_CTNT1
           AND ROWNUM = 1) AS ATTR_CTNT5

  FROM BKG_CSTMS_CD_CONV_CTNT C,
       (SELECT RCV_KEY_DAT_CTNT ERR_CD
          FROM BKG_CSTMS_ADV_JP_RCV_LOG
         WHERE BKG_NO = @[bl_no]
           AND JP_MSG_TP_ID = @[jp_msg_tp_id]
           AND TO_CHAR(RCV_DT, 'YYYYMMDDHH24MI') = (SELECT MAX(TO_CHAR(RCV_DT, 'YYYYMMDDHH24MI'))
                                                      FROM BKG_CSTMS_ADV_JP_RCV_LOG
                                                     WHERE BKG_NO = @[bl_no]
                                                       AND JP_MSG_TP_ID = @[jp_msg_tp_id])) RSLT

 WHERE C.CSTMS_DIV_ID = 'JP24_ERR_CD_DTL'
   AND DELT_FLG = 'N'
   AND (C.ATTR_CTNT1 = DECODE(@[jp_msg_tp_id], 'SAMR', 'A-', 'SCMR', 'C-')||SUBSTR(RSLT.ERR_CD, 1, 5) OR
        C.ATTR_CTNT1 = 'T-'||SUBSTR(RSLT.ERR_CD, 1, 5))
   AND C.ATTR_CTNT3 = SUBSTR(RSLT.ERR_CD, 7, 3)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="jp_msg_tp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
