<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMSummaryDBDAOInRevenueLaneVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT
		A1.RLANE_CD 
,       B1.RLANE_NM
--,       A1.TRD_CD
--,       A1.SUB_TRD_CD
  FROM  MDM_DTL_REV_LANE A1
,       MDM_REV_LANE B1
 WHERE  A1.RLANE_CD = B1.RLANE_CD  
#if (${rlane_cd} != '')
 AND    A1.RLANE_CD LIKE UPPER(@[rlane_cd]) || '%'
#end    
#if (${rlane_nm} != '')
 AND    B1.RLANE_NM LIKE UPPER(@[rlane_nm]) || '%'
#end
#if (${trd_cd} != '')
 AND    A1.TRD_CD = @[trd_cd]
#end    
#if (${sub_trd_cd} != '')
 AND    A1.SUB_TRD_CD = @[sub_trd_cd]
#end
 AND    B1.VSL_TP_CD = 'C' 
 AND    B1.DELT_FLG = 'N'
 AND    A1.DELT_FLG = 'N'
--and		rownum < 10
ORDER 	BY A1.RLANE_CD, B1.RLANE_NM			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="rlane_nm" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
