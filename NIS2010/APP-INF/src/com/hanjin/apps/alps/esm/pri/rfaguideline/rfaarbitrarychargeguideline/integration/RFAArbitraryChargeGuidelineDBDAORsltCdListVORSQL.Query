<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAArbitraryChargeGuidelineDBDAORsltCdListVORSQL">
			<desc><![CDATA[공통코드 조회 체크]]></desc>
			<sql><![CDATA[
#if (${code_tp} == 'LOC') 
SELECT LOC_CD CD
	 , LOC_NM NM
  FROM MDM_LOCATION
 WHERE LOC_CD = @[cd]
   AND DELT_FLG = 'N'

#elseif (${code_tp} == 'GRP') 
SELECT PRC_GRP_LOC_CD CD
     , PRC_GRP_LOC_DESC NM
  FROM PRI_RG_GRP_LOC
 WHERE SVC_SCP_CD 		= @[etc1]
   AND GLINE_SEQ 		= @[etc2]
   AND PRC_GRP_LOC_CD 	= @[cd]

#end			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
