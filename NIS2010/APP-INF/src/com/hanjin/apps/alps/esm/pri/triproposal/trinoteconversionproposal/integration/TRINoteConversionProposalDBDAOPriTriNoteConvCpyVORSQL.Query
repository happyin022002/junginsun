<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRINoteConversionProposalDBDAOPriTriNoteConvCpyVORSQL">
			<desc><![CDATA[붙혀넣기]]></desc>
			<sql><![CDATA[
SELECT '' NOTE_CONV_MAPG_ID
		, '0' NOTE_CONV_SEQ
		, A.NOTE_CONV_TP_CD
		, A.TRF_PFX_CD
		, A.TRF_NO
		, A.TRI_PROP_NO
		, A.AMDT_SEQ
		, CASE WHEN A.CHG_RULE_TP_CD = 'C' THEN NOTE_CONV_CHG_CD
			   WHEN A.CHG_RULE_TP_CD = 'R' THEN NOTE_CONV_RULE_CD
			   END CHG_RULE_DEF_CD
		, A.CHG_RULE_TP_CD
		, A.NOTE_CONV_CHG_CD
		, A.NOTE_CONV_RULE_CD
		, TO_CHAR(A.EFF_DT,'YYYYMMDD') EFF_DT
		, TO_CHAR(A.EXP_DT,'YYYYMMDD') EXP_DT
		, A.RULE_APPL_CHG_TP_CD
		, A.RT_APPL_TP_CD
		, A.RT_OP_CD
		, A.CURR_CD
		, A.FRT_RT_AMT
		, A.PAY_TERM_CD
		, A.BKG_RAT_UT_CD
		, A.BKG_PRC_CGO_TP_CD
		, A.BKG_IMDG_CLSS_CD
		, A.BKG_CMDT_TP_CD
		, A.BKG_CMDT_DEF_CD
		, A.BKG_USA_SVC_MOD_CD
		, A.BKG_POR_TP_CD
		, DECODE(A.BKG_POR_TP_CD, 'T', SUBSTR(A.BKG_POR_DEF_CD, 3), A.BKG_POR_DEF_CD) BKG_POR_DEF_CD
		, A.BKG_POL_TP_CD
		, DECODE(A.BKG_POL_TP_CD, 'T', SUBSTR(A.BKG_POL_DEF_CD, 3), A.BKG_POL_DEF_CD) BKG_POL_DEF_CD
		, A.BKG_POD_TP_CD
		, DECODE(A.BKG_POD_TP_CD, 'T', SUBSTR(A.BKG_POD_DEF_CD, 3), A.BKG_POD_DEF_CD) BKG_POD_DEF_CD
		, A.BKG_DEL_TP_CD
		, DECODE(A.BKG_DEL_TP_CD, 'T', SUBSTR(A.BKG_DEL_DEF_CD, 3), A.BKG_DEL_DEF_CD) BKG_DEL_DEF_CD
		, A.BKG_RCV_TERM_CD
		, A.BKG_DE_TERM_CD
		, A.BKG_MIN_CGO_WGT
		, A.BKG_MAX_CGO_WGT
		, A.BKG_SOC_FLG
		, A.BKG_TS_PORT_TP_CD
		, A.BKG_TS_PORT_DEF_CD
		, A.BKG_DIR_CALL_FLG
		, A.BKG_HNGR_BAR_TP_CD
		, A.CONV_RAT_UT_CD
		, A.CONV_PRC_CGO_TP_CD
		, A.CRE_USR_ID
		, A.CRE_DT
		, A.UPD_USR_ID
		, A.UPD_DT
		, DECODE(A.BKG_POR_TP_CD, 'T', SUBSTR(A.BKG_POR_DEF_CD, 1, 2), '') BKG_POR_CNT_CD
		, DECODE(A.BKG_POL_TP_CD, 'T', SUBSTR(A.BKG_POL_DEF_CD, 1, 2), '') BKG_POL_CNT_CD
		, DECODE(A.BKG_POD_TP_CD, 'T', SUBSTR(A.BKG_POD_DEF_CD, 1, 2), '') BKG_POD_CNT_CD
		, DECODE(A.BKG_DEL_TP_CD, 'T', SUBSTR(A.BKG_DEL_DEF_CD, 1, 2), '') BKG_DEL_CNT_CD
	 FROM PRI_TRI_NOTE_CONV_CPY A
	WHERE A.USR_ID = @[cre_usr_id]
	  AND A.NOTE_CONV_TP_CD = @[note_conv_tp_cd]
	ORDER BY A.BKG_PRC_CGO_TP_CD	ASC NULLS FIRST
		, A.BKG_RAT_UT_CD			ASC NULLS FIRST
		, A.PAY_TERM_CD				ASC NULLS FIRST
		, A.CURR_CD			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="note_conv_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
