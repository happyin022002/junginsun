<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchRateAmountRSQL">
			<desc><![CDATA[Rate 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  
	NVL(CHG_CD, '') FCTYPE 
	,NVL(CHG_UT_AMT, 0) RATE 
	,NVL(RAT_AS_QTY, 0) REVENUETON 
	,DECODE(FRT_TERM_CD,'P', NVL(CHG_AMT,0),0.0) PPD 
	,DECODE(FRT_TERM_CD,'C', NVL(CHG_AMT,0),0.0) CCT 
	,NVL(CURR_CD, '') CURRENCYCODE 
	,'' EXCHRATE 
	,NVL(TRF_ITM_NO, '') TARIFF 
	,NVL(RAT_UT_CD, '') PERTYPE 
	,NVL(N3PTY_RCV_OFC_CD, '') RATEOFC 
FROM  BKG_CHG_RT 
WHERE BKG_NO          		=   @[bkg_no]
AND  FRT_INCL_XCLD_DIV_CD   =   'N'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
