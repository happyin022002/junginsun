<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOAddEdiSndLogCSQL">
			<desc><![CDATA[EDI 전송 로그 정보를 기록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_IB_EDI_SND_LOG ( BKG_NO
                                ,EDI_SND_SEQ
                                ,FLT_FILE_REF_NO
                                ,CNTR_NO
                                ,DO_EDI_TP_CD
                                ,MSG_TP_NM
                                ,MSG_TP_ID
                                ,EDI_EVNT_USR_ID
                                ,EDI_EVNT_OFC_CD
                                ,EDI_EVNT_DT
                                ,EDI_EVNT_GDT
                                ,EDI_SND_MSG_CTNT
                                ,CRE_USR_ID
                                ,CRE_DT
                                ,UPD_USR_ID
                                ,UPD_DT
)                                
VALUES( @[bkg_no]
      ,(SELECT NVL(MAX(EDI_SND_SEQ)+ 1, 1) FROM BKG_IB_EDI_SND_LOG WHERE BKG_NO = @[bkg_no])  
      ,@[flt_file_ref_no]
      ,NVL(@[cntr_no],'XXXXXXXXXXXX')
      ,@[do_edi_tp_cd]
      ,@[msg_tp_nm]
      ,@[msg_tp_id]
      ,@[edi_evnt_usr_id]
      ,@[edi_evnt_ofc_cd]
      ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[edi_evnt_ofc_cd])
      ,GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,'GMT') 
      ,@[edi_snd_msg_ctnt]
      ,@[cre_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE  
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="do_edi_tp_cd" type="12" value="" out="N"/>
				<param name="msg_tp_nm" type="12" value="" out="N"/>
				<param name="msg_tp_id" type="12" value="" out="N"/>
				<param name="edi_evnt_usr_id" type="12" value="" out="N"/>
				<param name="edi_evnt_ofc_cd" type="12" value="" out="N"/>
				<param name="edi_snd_msg_ctnt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
