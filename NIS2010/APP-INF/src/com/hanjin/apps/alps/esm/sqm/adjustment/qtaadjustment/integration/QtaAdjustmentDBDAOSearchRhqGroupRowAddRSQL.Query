<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOSearchRhqGroupRowAddRSQL">
			<desc><![CDATA[[Portion Adjustment by Head Office]를 [RHQ Group Row Add] 합니다
[Portion Adjustment by RHQ]를 [RHQ Group Row Add] 합니다

* 2015.06.15 김용습 [CHM-201536164] 주간 MAS Open에 따른 타모듈 프로그램 적용 요청
20160422 CHM-201641278 [SQM] IAS Trade 판매목표 수립 Portion 연결 시스템 수정 요청 CSR]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
       A1.BSE_YR
      ,A1.BSE_QTR_CD
      ,DECODE(A1.OFC_VW_CD,'C','CONTRACT','LOADING')    AS OFC_VW_CD
      ,DECODE(A3.OB_DIV_CD,'N','N.OB','O/B')            AS OB_DIV_CD
      ,DECODE(A4.HUL_BND_CD,'HH','H/H','B/H')           AS HUL_BND_CD
      ,A1.TRD_CD
      ,A1.SUB_TRD_CD
      ,A1.RLANE_CD
      ,NVL(A5.CONV_DIR_CD, A1.DIR_CD) AS CONV_DIR_CD
      ,A1.DIR_CD
      ,A1.RHQ_CD
#if (${f_gubun} == 'RHQ')
      ,A1.RGN_OFC_CD
#end
      ,A2.LOD_POTN_RTO
      ,A2.REV_POTN_RTO
      ,DENSE_RANK() OVER(PARTITION BY A1.BSE_TP_CD, A1.BSE_YR, A1.BSE_QTR_CD, A1.OFC_VW_CD, A2.QTA_STEP_CD, A1.TRD_CD, A1.RLANE_CD --, A2.RHQ_CD
                         ORDER BY A1.TRD_CD, A1.RLANE_CD, A2.CONV_DIR_CD
#if (${f_gubun} == 'RHQ')
      								, A2.RHQ_CD
#end
                         ) GRP_NO
  FROM SQM_QTA_LANE_OFC A1
      ,SQM_QTA_POTN_MGMT A2
      ,SQM_DAT_RLT A3
      ,MAS_LANE_RGST A4
      ,SQM_DIR_CONV A5
 WHERE 1=1
   AND A1.BSE_TP_CD    = A5.BSE_TP_CD(+)
   AND A1.BSE_YR       = A5.BSE_YR(+)
   AND A1.BSE_QTR_CD   = A5.BSE_QTR_CD(+)
   AND A1.TRD_CD       = A5.TRD_CD(+)
   AND A1.RLANE_CD     = A5.RLANE_CD(+)
   AND A1.DIR_CD       = A5.DIR_CD(+)

   AND A1.TRD_CD       = A4.TRD_CD
   AND A1.RLANE_CD     = A4.RLANE_CD
   AND A1.DIR_CD       = A4.DIR_CD
   AND A4.DELT_FLG     = 'N'

   AND A1.BSE_TP_CD    = A3.BSE_TP_CD
   AND A1.BSE_YR       = A3.BSE_YR
   AND A1.BSE_QTR_CD   = A3.BSE_QTR_CD
   AND A1.OFC_VW_CD    = A3.OFC_VW_CD
   AND A1.TRD_CD       = A3.TRD_CD
   AND NVL(A5.CONV_DIR_CD,A1.DIR_CD)  = A3.CONV_DIR_CD
   AND A1.RHQ_CD       = A3.RHQ_CD

   AND A1.BSE_TP_CD    = A2.BSE_TP_CD(+)
   AND A1.BSE_YR       = A2.BSE_YR(+)
   AND A1.BSE_QTR_CD   = A2.BSE_QTR_CD(+)
   AND A1.OFC_VW_CD    = A2.OFC_VW_CD(+)
   AND A1.RHQ_CD       = A2.RHQ_CD(+)
#if (${f_gubun} == 'RHQ')
   AND A1.RGN_OFC_CD   = A2.RGN_OFC_CD(+)
#end
   AND A1.TRD_CD       = A2.TRD_CD(+)
   AND A1.RLANE_CD     = A2.RLANE_CD(+)
   AND A1.DIR_CD       = A2.DIR_CD(+)

   AND A1.SQM_ACT_FLG  = 'Y'
   AND A1.BSE_TP_CD    = @[f_bse_tp_cd]
   AND A1.BSE_YR       = @[f_bse_yr]
   AND A1.BSE_QTR_CD   = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
   AND A1.OFC_VW_CD    = @[f_ofc_vw_cd]
#if (${f_gubun} == 'HO')
   AND A2.QTA_STEP_CD(+)  = @[f_qta_step_cd]
   AND A3.TEAM_CD      = (SELECT CASE WHEN @[ofc_cd] IN (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255')
                                                       THEN A3.TEAM_CD
                                                  ELSE @[ofc_cd]
                                                END TEAM_CD
                                          FROM DUAL)
#elseif (${f_gubun} == 'RHQ')
   AND A2.QTA_STEP_CD(+)  <> '02'
   AND A3.RHQ_CD       = (SELECT CASE WHEN @[ofc_cd] IN (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255')
                                                       THEN A3.RHQ_CD
                                                  ELSE DECODE(@[ofc_cd], 'SELCMI', A3.RHQ_CD, @[ofc_cd])
                                                END RHQ_CD
                                          FROM DUAL)
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
   AND A1.TRD_CD       = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
   AND A1.RLANE_CD     IN (${f_rlane_cd})
#end
#if (${f_click} != 'on' && ${f_conv_dir_cd} != '' && ${f_conv_dir_cd} != 'All')
   AND NVL(A5.CONV_DIR_CD,A1.DIR_CD)  = @[f_conv_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
   AND A4.HUL_BND_CD  = @[f_trd_dir]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
   AND A3.OB_DIV_CD   = @[f_ob_div_cd]
#end
#if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
   AND A1.RGN_OFC_CD   = @[f_rgn_ofc_cd]
#end
 ORDER BY A1.TRD_CD
         ,A1.SUB_TRD_CD
         ,A1.RLANE_CD
         ,NVL(A5.CONV_DIR_CD, A1.DIR_CD)
         ,A1.RHQ_CD
#if (${f_gubun} == 'RHQ')
         ,A1.RGN_OFC_CD
#end			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_qta_step_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_conv_dir_cd" type="12" value="" out="N"/>
				<param name="f_trd_dir" type="12" value="" out="N"/>
				<param name="f_ob_div_cd" type="12" value="" out="N"/>
				<param name="f_rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
