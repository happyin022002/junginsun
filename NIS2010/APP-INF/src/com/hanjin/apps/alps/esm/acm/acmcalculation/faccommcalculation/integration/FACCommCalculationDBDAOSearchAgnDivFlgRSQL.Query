		<query name="FACCommCalculationDBDAOSearchAgnDivFlgRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchAgnDivFlgRSQL]]></desc>
			<sql><![CDATA[
SELECT DECODE (LTRIM (OFC_TP_CD), 'BA', 'Y', 'OT', 'Y', 'N') AGN_DIV_FLG 
  FROM MDM_ORGANIZATION 
 WHERE OFC_CD = @[ar_ofc_cd]			]]></sql>
