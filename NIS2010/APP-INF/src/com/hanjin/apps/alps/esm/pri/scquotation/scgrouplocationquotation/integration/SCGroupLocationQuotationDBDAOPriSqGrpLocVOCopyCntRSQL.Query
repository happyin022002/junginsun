<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupLocationQuotationDBDAOPriSqGrpLocVOCopyCntRSQL">
			<desc><![CDATA[gline 카피 전에 저장할 데이터가 있는 지 조회]]></desc>
			<sql><![CDATA[
SELECT  COUNT(*) AS COPY_CNT
FROM    PRI_SG_GRP_LOC
WHERE   1=1
AND    (SVC_SCP_CD, GLINE_SEQ) = ( SELECT SVC_SCP_CD, GLINE_SEQ
									FROM   (                                 
                                            SELECT SVC_SCP_CD, GLINE_SEQ, MAX(EFF_DT)
                                            FROM   PRI_SG_MN
                                            WHERE  SVC_SCP_CD = @[svc_scp_cd]
                                            AND    TO_CHAR(EFF_DT,'YYYY-MM-DD') <= @[eff_dt]
                                            AND    TO_CHAR(EFF_DT,'YYYY-MM-DD') >= @[eff_dt]
                                            AND    CFM_FLG = 'Y' 
                                            GROUP BY SVC_SCP_CD, GLINE_SEQ
                                           )
                                  )
AND     PRC_CUST_TP_CD = @[prc_cust_tp_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="prc_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
