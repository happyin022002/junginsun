<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchDgInfoinquiryRSQL">
			<desc><![CDATA[구주위험물 테이블들에서 데이타를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT 
        A.EUR_DG_DECL_TP_CD
        ,A.VSL_CD
        ,A.SKD_VOY_NO
        ,A.SKD_DIR_CD
        ,A.PORT_CD
        ,A.BL_NO
		,BB.BKG_NO
        ,A.CNTR_NO
        ,A.CNTR_CGO_SEQ
        ,A.CNTR_TPSZ_CD         -- 컨테이너 타입 사이즈
        ,(
            SELECT CNTR_TPSZ_ISO_CD 
            FROM MDM_CNTR_TP_SZ 
            WHERE CNTR_TPSZ_CD = A.CNTR_TPSZ_CD
        ) CNTR_TPSZ_ISO_CD      -- ISO 값
        ,A.EUR_DCGO_MRN_POLUT_CD DCGO_MRN_POLUT_CD    -- Marine Pollutant  
        ,A.IMDG_CLSS_CD         -- Class
        ,A.IMDG_SUBS_RSK_LBL_CD1 -- Sub label 1
        ,A.IMDG_SUBS_RSK_LBL_CD2 -- Sub label 2
        ,A.IMDG_SUBS_RSK_LBL_CD3 -- Sub label 3
        ,A.IMDG_SUBS_RSK_LBL_CD4 -- Sub label 4
        ,A.IMDG_LMT_QTY_FLG     -- Limited quantity
        ,A.EMER_RSPN_GID_NO     -- Page           
        ,A.HCDG_FLG             -- High Consequence dangerous goods
        ,A.IMDG_UN_NO           -- UN No. 
        ,A.IMDG_UN_NO_SEQ       -- UN No SEQ. 
        ,SIUN.IMDG_COMP_GRP_CD
        ,A.IN_IMDG_PCK_QTY1     -- Inner Package Qty
        ,A.IN_IMDG_PCK_CD1      -- Inner Package Code
        ,A.FLSH_PNT_CDO_TEMP    -- Flash Point
        ,A.EUR_INR_PCK_DESC     -- Inner Package Description
        ,A.IMDG_PCK_GRP_CD      -- Package Group
        ,A.OUT_IMDG_PCK_QTY1     
        ,A.OUT_IMDG_PCK_CD1
        ,A.EMS_NO               -- EMS No.
        ,A.EUR_OUTR_PCK_DESC    -- Outer Package Description
        ,A.NET_WGT              -- Net Weight
        
        ,A.MFAG_NO              -- MFAG
        
        ,A.CELL_PSN_NO          -- CELL_PSN_NO
        ,A.GRS_WGT              -- GRS_WGT
        ,A.EUR_PCK_DESC         -- Packages
        ,A.PRP_SHP_NM           -- Substance
        ,A.HZD_DESC             -- Hazardous Contents
        ,A.BRTH_YD_CD           -- Berth Code
        ,A.BRTH_YD_NM           -- Berth Name
        ,A.ANR_FWRD_ID          -- Forwarder Code
        ,D.FWRD_NM              -- Forwarder Name
        ,TO_CHAR(A.CRR_DT, 'YYYY-MM-DD') CRR_DT -- On-Carriage Date
        ,A.XTD_STAY_PRMT_NO     -- Extended Stay Permit
        ,A.DIFF_RMK             -- Additional Remark(s)
        ,A.ANR_SPCL_TP_ID       -- Belgian Codes for Type of special UN numbers
        ,A.ANR_CRR_TP_CD        -- Carriage Type
        ,A.FDR_SVC_RQST_NO      -- SSR for Feeder transshipment
        ,A.FDR_VSL_NM           -- Feeder Name
        ,A.FDR_VSL_LLOYD_NO     -- Feeder Lloyd No
        ,A.FDR_VVD_ID           -- Feeder VVD
	    ,A.NET_EXPLO_WGT		-- Net Explosive Weight 
        --,L.ACK_RCV_STS_CD

        ,(
            CASE
                WHEN L.MSG_SND_NO IS NULL AND ACK_RCV_STS_CD IS NULL THEN ''
                WHEN L.MSG_SND_NO IS NOT NULL AND ACK_RCV_STS_CD IS NULL THEN 'P'
                ELSE L.ACK_RCV_STS_CD
            END
          ) ACK_RCV_STS_CD


        ,L.CSTMS_ERR_ID || '|' || L.CSTMS_ERR_MSG AS CSTMS_ERR_MSG

		,'' D_TYPE
		,'' VVD_CD
		,'' SAVE_TYPE
		,'' CRE_USR_ID
		,'' UPD_USR_ID
        
  FROM  BKG_CSTMS_EUR_DG A
       ,BKG_CSTMS_EUR_DG_FWRD D
	   ,BKG_BOOKING BB

       ,(
        
            SELECT 
                 A.MSG_SND_NO
				#if (${call_gubun} == 'ESM_BKG_0965') 
                ,B.BL_NO
                ,B.CNTR_NO
				#end
                ,C.ACK_RCV_STS_CD
				,D.CSTMS_ERR_ID
                ,D.CSTMS_ERR_MSG
                ,A.EUR_DG_DECL_TP_CD
            	,A.VSL_CD
            	,A.SKD_VOY_NO
            	,A.SKD_DIR_CD
            	,A.PORT_CD
            FROM BKG_CSTMS_EUR_DG_SND A
				#if (${call_gubun} == 'ESM_BKG_0965') 
                ,BKG_CSTMS_EUR_DG_EDI_RSPN B
				#end
                ,BKG_CSTMS_EUR_DG_RCV C
                ,BKG_CSTMS_EUR_DG_RCV_ERR D
    
            WHERE 1=1
            AND   A.EUR_EDI_MSG_TP_ID   = C.EUR_EDI_MSG_TP_ID(+)
            AND   A.MSG_SND_NO          = C.MSG_RCV_NO(+)
            AND   C.EUR_EDI_MSG_TP_ID   = D.EUR_EDI_MSG_TP_ID(+)
            AND   C.MSG_RCV_NO          = D.MSG_RCV_NO(+)
            AND   C.RCV_LOG_SEQ         = D.RCV_LOG_SEQ(+)
            AND   A.EUR_DG_DECL_TP_CD = @[d_type]
            AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
            AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
            AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
            AND   A.PORT_CD     = @[port_cd]
            AND   A.EUR_EDI_MSG_TP_ID ='IFD'
			#if (${call_gubun} == 'ESM_BKG_0965') 
            AND   A.EUR_EDI_MSG_TP_ID   = B.EUR_EDI_MSG_TP_ID(+)
            AND   A.MSG_SND_NO          = B.MSG_SND_NO(+)
            AND   B.BL_NO        = @[bl_no]
            AND   B.CNTR_NO      = @[cntr_no]
            AND   B.CNTR_CGO_SEQ = @[cntr_cgo_seq]
			#end
            AND   A.SND_DT = (
                                SELECT 
                                     MAX(A.SND_DT)
                                FROM BKG_CSTMS_EUR_DG_SND A
									#if (${call_gubun} == 'ESM_BKG_0965') 
                                    ,BKG_CSTMS_EUR_DG_EDI_RSPN B
									#end
                                    ,BKG_CSTMS_EUR_DG_RCV C
                                    ,BKG_CSTMS_EUR_DG_RCV_ERR D
    
                                WHERE 1=1
                                AND   A.EUR_EDI_MSG_TP_ID   = C.EUR_EDI_MSG_TP_ID(+)
                                AND   A.MSG_SND_NO          = C.MSG_RCV_NO(+)
                                AND   C.EUR_EDI_MSG_TP_ID   = D.EUR_EDI_MSG_TP_ID(+)
                                AND   C.MSG_RCV_NO          = D.MSG_RCV_NO(+)
                                AND   C.RCV_LOG_SEQ         = D.RCV_LOG_SEQ(+)
                                AND   A.EUR_DG_DECL_TP_CD = @[d_type]
                                AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                                AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                                AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                                AND   A.PORT_CD     = @[port_cd]
                                AND   A.EUR_EDI_MSG_TP_ID ='IFD'
								#if (${call_gubun} == 'ESM_BKG_0965') 
                                AND   A.EUR_EDI_MSG_TP_ID   = B.EUR_EDI_MSG_TP_ID(+)
                                AND   A.MSG_SND_NO          = B.MSG_SND_NO(+)
                                AND   B.BL_NO        = @[bl_no]
                                AND   B.CNTR_NO      = @[cntr_no]
                                AND   B.CNTR_CGO_SEQ = @[cntr_cgo_seq]
								#end
                             )
        
       ) L
       ,SCG_IMDG_UN_NO SIUN

 WHERE  1=1

   AND  A.BL_NO = BB.BL_NO
   AND  A.ANR_FWRD_ID = D.ANR_FWRD_ID(+)

   AND  A.IMDG_UN_NO       = SIUN.IMDG_UN_NO(+)
   AND  A.IMDG_UN_NO_SEQ   = SIUN.IMDG_UN_NO_SEQ(+)

#if (${call_gubun} == 'ESM_BKG_0965') 
   AND  A.BL_NO = L.BL_NO(+)
   AND  A.CNTR_NO = L.CNTR_NO(+)
#end   

   AND  A.EUR_DG_DECL_TP_CD = @[d_type]
   AND  A.VSL_CD            = SUBSTR(@[vvd_cd], 1, 4)
   AND  A.SKD_VOY_NO        = SUBSTR(@[vvd_cd], 5, 4)
   AND  A.SKD_DIR_CD        = SUBSTR(@[vvd_cd], 9, 1)
   AND  A.PORT_CD           = @[port_cd]
   AND  A.EUR_DG_DECL_TP_CD = L.EUR_DG_DECL_TP_CD(+)
   AND  A.VSL_CD            = L.VSL_CD(+)
   AND  A.SKD_VOY_NO        = L.SKD_VOY_NO(+)
   AND  A.SKD_DIR_CD        = L.SKD_DIR_CD(+)
   AND  A.PORT_CD           = L.PORT_CD(+)
   AND  A.BL_NO             = @[bl_no]
   AND  A.CNTR_NO           = @[cntr_no]
   AND  A.CNTR_CGO_SEQ      = @[cntr_cgo_seq]			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
