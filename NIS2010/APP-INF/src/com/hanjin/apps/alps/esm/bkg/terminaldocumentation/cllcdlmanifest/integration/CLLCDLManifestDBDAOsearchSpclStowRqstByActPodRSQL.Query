<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchSpclStowRqstByActPodRSQL">
			<desc><![CDATA[searchSpclStowRqstByActPod]]></desc>
			<sql><![CDATA[
SELECT GUBUN_CD3, '' GUBUN_CD2, '' GUBUN_CD, '' BLCK_STWG_CD
,SUM(OD_20) OD_20, SUM(OD_40) OD_40, SUM(OD_40H) OD_40H
,SUM(ODET_20) ODET_20, SUM(ODET_40) ODET_40, SUM(ODET_40H) ODET_40H
,SUM(ODHD_20) ODHD_20, SUM(ODHD_40) ODHD_40, SUM(ODHD_40H) ODHD_40H
,SUM(ODTB_20) ODTB_20, SUM(ODTB_40) ODTB_40, SUM(ODTB_40H) ODTB_40H
,SUM(ODTS_20) ODTS_20, SUM(ODTS_40) ODTS_40, SUM(ODTS_40H) ODTS_40H
,SUM(UD_20) UD_20, SUM(UD_40) UD_40, SUM(UD_40H) UD_40H
,SUM(UDAB_20) UDAB_20, SUM(UDAB_40) UDAB_40, SUM(UDAB_40H) UDAB_40H
,SUM(UDAV_20) UDAV_20, SUM(UDAV_40) UDAV_40, SUM(UDAV_40H) UDAV_40H
,SUM(UDBW_20) UDBW_20, SUM(UDBW_40) UDBW_40, SUM(UDBW_40H) UDBW_40H
,SUM(UDHG_20) UDHG_20, SUM(UDHG_40) UDHG_40, SUM(UDHG_40H) UDHG_40H
,SUM(UDTS_20) UDTS_20, SUM(UDTS_40) UDTS_40, SUM(UDTS_40H) UDTS_40H
,SUM(PCOD_20) PCOD_20, SUM(PCOD_40) PCOD_40, SUM(PCOD_40H) PCOD_40H
FROM
(SELECT NVL(LO.UN_LOC_CD,BK.POD_CD) GUBUN_CD3
, DECODE(BK.STWG_CD,'OD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) OD_20
, DECODE(BK.STWG_CD,'OD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) OD_40
, DECODE(BK.STWG_CD,'OD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) OD_40H
  
, DECODE(BK.STWG_CD,'ODET',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) ODET_20
, DECODE(BK.STWG_CD,'ODET',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) ODET_40
, DECODE(BK.STWG_CD,'ODET',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) ODET_40H
  
, DECODE(BK.STWG_CD,'ODHD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) ODHD_20
, DECODE(BK.STWG_CD,'ODHD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) ODHD_40
, DECODE(BK.STWG_CD,'ODHD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) ODHD_40H
  
, DECODE(BK.STWG_CD,'ODTB',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) ODTB_20
, DECODE(BK.STWG_CD,'ODTB',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) ODTB_40
, DECODE(BK.STWG_CD,'ODTB',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) ODTB_40H
  
, DECODE(BK.STWG_CD,'ODTS',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) ODTS_20
, DECODE(BK.STWG_CD,'ODTS',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) ODTS_40
, DECODE(BK.STWG_CD,'ODTS',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) ODTS_40H
  
, DECODE(BK.STWG_CD,'UD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) UD_20
, DECODE(BK.STWG_CD,'UD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) UD_40
, DECODE(BK.STWG_CD,'UD',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) UD_40H
  
, DECODE(BK.STWG_CD,'UDAB',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) UDAB_20
, DECODE(BK.STWG_CD,'UDAB',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) UDAB_40
, DECODE(BK.STWG_CD,'UDAB',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) UDAB_40H
  
, DECODE(BK.STWG_CD,'UDAV',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) UDAV_20
, DECODE(BK.STWG_CD,'UDAV',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) UDAV_40
, DECODE(BK.STWG_CD,'UDAV',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) UDAV_40H
  
, DECODE(BK.STWG_CD,'UDBW',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) UDBW_20
, DECODE(BK.STWG_CD,'UDBW',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) UDBW_40
, DECODE(BK.STWG_CD,'UDBW',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) UDBW_40H
  
, DECODE(BK.STWG_CD,'UDHG',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) UDHG_20
, DECODE(BK.STWG_CD,'UDHG',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) UDHG_40
, DECODE(BK.STWG_CD,'UDHG',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) UDHG_40H
  
, DECODE(BK.STWG_CD,'UDTS',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) UDTS_20
, DECODE(BK.STWG_CD,'UDTS',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) UDTS_40
, DECODE(BK.STWG_CD,'UDTS',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) UDTS_40H
  
, DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0) PCOD_20
, DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'4', BQ.OP_CNTR_QTY,0),0) PCOD_40
, DECODE(CM.REP_IMDG_LVL_CD,'P',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0,  BQ.OP_CNTR_QTY),0) PCOD_40H
, VPS.CLPT_SEQ CLPT_SEQ
FROM BKG_BOOKING BK
	, BKG_VVD BV
	, BKG_QUANTITY BQ
	, VSK_VSL_PORT_SKD VPS
	, MDM_COMMODITY CM
	, MDM_LOCATION LO
	, BKG_CSTMS_TML_KR_CLL CLL
	, BKG_CONTAINER BC
WHERE BK.BKG_NO = BV.BKG_NO
  AND BK.BKG_NO = BQ.BKG_NO
  AND BV.VSL_CD = VPS.VSL_CD
  AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
  AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
  AND BK.POD_CD = VPS.VPS_PORT_CD
  AND BK.CMDT_CD = CM.CMDT_CD
  AND VPS.VPS_PORT_CD = LO.LOC_CD
  AND NVL(CM.DELT_FLG,'N')='N'
  AND BV.VSL_CD = SUBSTR(@[in_vvd_cd],1,4)
  AND BV.SKD_VOY_NO = SUBSTR(@[in_vvd_cd],5,4)
  AND BV.SKD_DIR_CD = SUBSTR(@[in_vvd_cd],9,1)
  AND BV.POL_CD = @[in_pol_cd]
  AND BK.BKG_STS_CD IN ( 'F','W')
  AND VPS.CLPT_IND_SEQ = '1'
  AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
  AND CLL.CNTR_LIST_NO = BV.VSL_CD||SUBSTR(BV.SKD_VOY_NO,2,3)||BV.SKD_DIR_CD||SUBSTR(BV.POL_CD,3,3)
  AND CLL.BKG_NO = BK.BKG_NO
  AND BK.BKG_NO = BC.BKG_NO
  AND BC.DCGO_FLG = 'N'
  AND BC.RC_FLG = 'N'
  AND BC.AWK_CGO_FLG = 'N'
  --AND CLL.POL_YD_CD = BV.POL_YD_CD
GROUP BY NVL(LO.UN_LOC_CD,BK.POD_CD), BK.STWG_CD, BQ.CNTR_TPSZ_CD, BQ.OP_CNTR_QTY, CM.REP_IMDG_LVL_CD, BK.BKG_NO, VPS.CLPT_SEQ)
GROUP BY GUBUN_CD3, CLPT_SEQ
ORDER BY CLPT_SEQ			]]></sql>
			<params>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
