<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgencyCommissionDBDAOMultiAgnOtrCommCostMonthCopyCSQL">
			<desc><![CDATA[2017.05.15 송민석 ERP MAS의 Phase out에 따라 영향 받는 화면에 대한 수정 프로젝트 1차]]></desc>
			<sql><![CDATA[
INSERT  INTO MAS_OTR_COMM (
COST_YRMON
, CNTR_TPSZ_CD
, COMM_LOC_CD
, MAS_COST_SRC_CD
, STND_COST_USD_AMT
, BKG_TTL_QTY
, OTR_COMM_TTL_AMT
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT 
)
SELECT @[f_tar_mon] as COST_YRMON
, CNTR_TPSZ_CD
, COMM_LOC_CD
, MAS_COST_SRC_CD
, STND_COST_USD_AMT
, BKG_TTL_QTY
, OTR_COMM_TTL_AMT
        , @[user_id] CRE_USR_ID
        , SYSDATE CRE_DT
        , @[user_id] UPD_USR_ID
        , SYSDATE UPD_DT
  FROM MAS_OTR_COMM
 WHERE COST_YRMON = @[f_src_mon]			]]></sql>
			<params>
				<param name="f_tar_mon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_src_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
