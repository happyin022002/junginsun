<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQBalanceDBDAOMasCntrRepoShtgInfoVOUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE  MAS_CNTR_REPO_SHTG_INFO
   SET  IMBAL_AMT = @[imbal_amt] 
	   ,MB_AMT = @[mb_amt] / 100 
	   ,EQ_REPO_CR_RTO = @[eq_repo_cr_rto]/100
       ,UPD_USR_ID = @[upd_usr_id]
       ,UPD_DT = SYSDATE
 WHERE  COST_YRMON = @[cost_yrmon]
   AND  CNTR_TPSZ_CD = REPLACE(@[cntr_tpsz_cd], 'RD', 'R')
   AND  ECC_CD = @[ecc_cd]			]]></sql>
			<params>
				<param name="imbal_amt" type="12" value="" out="N"/>
				<param name="mb_amt" type="12" value="" out="N"/>
				<param name="eq_repo_cr_rto" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
