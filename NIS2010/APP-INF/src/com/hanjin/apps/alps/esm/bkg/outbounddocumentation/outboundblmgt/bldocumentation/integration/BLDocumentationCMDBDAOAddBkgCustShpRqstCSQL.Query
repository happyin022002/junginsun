<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOAddBkgCustShpRqstCSQL">
			<desc><![CDATA[BkgCustShpRqst저장 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CUST_SHP_RQST
(BKG_NO,
CNTR_NO,
CNTR_SEQ,
CNTR_TPSZ_CD,
CNTR_SEAL_NO,
PCK_QTY,
PCK_TP_CD,
CNTR_WGT,
WGT_UT_CD,
MEAS_QTY,
MEAS_UT_CD,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID,
UPD_DT,
CNTR_VOL_QTY
)
VALUES (
 @[bkg_no]
,@[cntr_no]
,(SELECT NVL(MAX(CNTR_SEQ)+1, 1) AS CNTR_SEQ
    FROM BKG_CUST_SHP_RQST
   WHERE BKG_NO = @[bkg_no]
     AND CNTR_NO= @[cntr_no])
,@[cntr_tpsz_cd]
,@[cntr_seal_no]
,@[pck_qty]
,@[pck_tp_cd]
,@[cntr_wgt]
,@[wgt_ut_cd]
,@[meas_qty]
,@[meas_ut_cd]
,@[cre_usr_id]
,sysdate
,@[upd_usr_id]
,sysdate
,@[cntr_vol_qty]

)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_seal_no" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="meas_ut_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_vol_qty" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
