<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSplitCombineDBDAOsearchBkgCustForCombineRSQL">
			<desc><![CDATA[searchBkgCustForCombine]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD, CUST_SEQ
  FROM BKG_CUSTOMER
WHERE BKG_NO = @[bkg_no]
  AND CUST_CNT_CD IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
