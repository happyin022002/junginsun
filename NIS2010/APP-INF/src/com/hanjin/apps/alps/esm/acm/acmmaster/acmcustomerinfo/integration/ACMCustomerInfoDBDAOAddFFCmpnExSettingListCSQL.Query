<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCustomerInfoDBDAOAddFFCmpnExSettingListCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_FF_EXCLU_SET
       (FF_CNT_CD,
        FF_SEQ,
        SHPR_CNT_CD,
        SHPR_SEQ,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (SUBSTR(@[ff_cnt_seq], 1, 2),
        TO_NUMBER(SUBSTR(@[ff_cnt_seq], 3)),
        SUBSTR(@[shpr_cnt_seq], 1, 2),
        TO_NUMBER(SUBSTR(@[shpr_cnt_seq], 3)),
        @[usr_id],
        SYSDATE,
        @[usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="ff_cnt_seq" type="12" value="" out="N"/>
				<param name="shpr_cnt_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
