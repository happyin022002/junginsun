<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltAllCurrencyListRSQL">
			<desc><![CDATA[MDM_CURRENCY의 전체 조회]]></desc>
			<sql><![CDATA[
SELECT 
    CURR_CD AS CD
   ,CURR_CD AS NM
   ,CURR_NM AS ETC1
FROM MDM_CURRENCY
WHERE DELT_FLG = 'N'
#if (${etc1} != '')
AND CURR_CD = @[etc1]
#end
ORDER BY CD			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
