<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchSrEmailRSQL">
			<desc><![CDATA[선적 요청서 (S/R) 제출 data 조회]]></desc>
			<sql><![CDATA[
SELECT
    (SELECT VSL_ENG_NM FROM MDM_VSL_CNTR VSL
     WHERE DELT_FLG ='N'
     AND VSL.VSL_CD = BK.VSL_CD
     AND ROWNUM = 1 )||' '||BK.SKD_VOY_NO||BK.SKD_DIR_CD VSL_NM,
     BK.BKG_NO BKG_NO,
     BK.POL_CD||' / '||BK.POD_CD POL_POD_CD,
     TO_CHAR(VVD.VPS_ETB_DT,'YYYY-MM-DD HH24:MI') || ' / ' || TO_CHAR(VVD.VPS_ETD_DT,'YYYY-MM-DD HH24:MI') ETB_ETD_DT,
     --NVL(PRD_GET_CCT_FNC(VVD.YD_CD,VVD.SLAN_CD,VVD.SKD_DIR_CD, 'F', VVD.VPS_ETB_DT, VVD.VPS_ETD_DT ) , VVD.VPS_ETB_DT -10/24)  CARGO,
     CASE WHEN MNL_SET_DT IS NOT NULL THEN NVL(TO_CHAR(MNL_SET_DT,'YYYY-MM-DD HH24:MI'),' ')
          WHEN 'KR' = SUBSTR(BK.POL_CD,1,2)
           AND BKG_GET_DCT_CUT_OFF_FNC(VVD.VSL_CD||VVD.SKD_VOY_NO||VVD.SKD_DIR_CD, VVD.POL_CD, TO_CHAR(VVD.POL_CLPT_IND_SEQ), BK.POD_CD) IS NOT NULL
          THEN NVL(TO_CHAR(BKG_GET_DCT_CUT_OFF_FNC(VVD.VSL_CD||VVD.SKD_VOY_NO||VVD.SKD_DIR_CD, VVD.POL_CD, TO_CHAR(VVD.POL_CLPT_IND_SEQ), BK.POD_CD),'YYYY-MM-DD HH24:MI'),' ')  
          ELSE NVL(TO_CHAR(SYS_SET_DT,'YYYY-MM-DD HH24:MI'),' ') 
          END AS DOC,
	 --NVL((SELECT DECODE(TO_CHAR(MNL_SET_DT,'YYYY-MM-DD HH24:MI'), NULL, TO_CHAR(SYS_SET_DT,'YYYY-MM-DD HH24:MI'),TO_CHAR(MNL_SET_DT,'YYYY-MM-DD HH24:MI')) FROM BKG_CLZ_TM WHERE BKG_NO = BK.BKG_NO AND CLZ_TP_CD ='T' AND ROWNUM = 1 ),' ') CARGO,
	 NVL(TO_CHAR((SELECT CASE WHEN 'KR' = (SELECT SUBSTR(POL_CD,1,2) FROM BKG_BOOKING WHERE BKG_NO =  @[bkg_no] ) THEN ETB -0.41667 
													 ELSE CCT
													 END CCT
													 FROM (select PRD_GET_CCT_FNC(skd.pol_nod_cd, SKD.SLAN_CD, SKD.SLAN_DIR_CD, SKD.CGO_TP_CD, SKD.VPS_ETB_DT, SKD.VPS_ETD_DT, SKD.VSL_CD||SKD.SKD_VOY_NO||SKD.SKD_DIR_CD) CCT, SKD.VPS_ETB_DT ETB
                                                     from (SELECT SKD.SLAN_CD SLAN_CD
                                                                 , VVD.SKD_DIR_CD SLAN_DIR_CD
                                                                 , CASE WHEN RC_FLG   = 'Y' THEN 'RF'
                                                                        WHEN DCGO_FLG = 'Y' THEN 'DG'
                                                                        WHEN DCGO_FLG = 'N' AND RC_FLG = 'N' AND AWK_CGO_FLG = 'N' AND BB_CGO_FLG = 'N' THEN 'DR'
                                                                        ELSE 'AL' END CGO_TP_CD
                                                                 , VPS_ETB_DT
                                                                 , VPS_ETD_DT
                                                                 , pol_nod_cd
                                                                 , VVD.VSL_CD
                                                                 , VVD.SKD_VOY_NO
                                                                 , VVD.SKD_DIR_CD
                                                               FROM BKG_BOOKING BK, BKG_VVD VVD, VSK_VSL_PORT_SKD SKD
                                                              WHERE BK.BKG_NO = VVD.BKG_NO
                                                                AND BK.POL_CD = VVD.POL_CD
                                                                AND VVD.VSL_PRE_PST_CD IN ('S', 'T')
																AND VVD.VSL_CD	   NOT IN ('SMXX','SMYY','SMZZ')
                                                                AND VVD.VSL_CD     = SKD.VSL_CD
                                                                AND VVD.SKD_VOY_NO = SKD.SKD_VOY_NO
                                                                AND VVD.SKD_DIR_CD = SKD.SKD_DIR_CD
                                                                AND VVD.POL_CD     = SKD.VPS_PORT_CD
                                                                AND NVL(VVD.POL_CLPT_IND_SEQ, 1) = SKD.CLPT_IND_SEQ
                                                                AND BK.BKG_NO      = @[bkg_no]) skd)),'YYYY-MM-DD HH24:MI'),' ') CARGO,
     '' DUM1,
     '' DUM2,
     NVL(BK.XTER_RMK,' ') REMARK,
     NVL((SELECT 'Y' FROM COM_USER WHERE USR_ID = @[usr_id] AND OFC_CD IN('SZPSC','HKGSC', 'CANSO','ZHOSO')),'N') AS CN_OFC_FLG,
	 NVL(TO_CHAR((SELECT NVL(MNL_SET_DT,SYS_SET_DT) FROM BKG_CLZ_TM WHERE BKG_NO = TM.BKG_NO AND CLZ_TP_CD ='V'),'YYYY-MM-DD HH24:MI'),' ') VGM_CUT_OFF
 FROM BKG_BOOKING BK,
      BKG_CLZ_TM TM,
      (SELECT VVD.BKG_NO ,VVD.POL_CD, VVD.POL_CLPT_IND_SEQ, VVD.VSL_CD, VVD.SKD_VOY_NO, VVD.SKD_DIR_CD, VSK.VPS_ETB_DT VPS_ETB_DT ,VSK.VPS_ETD_DT VPS_ETD_DT, VSK.SLAN_CD SLAN_CD, VSK.YD_CD YD_CD
        FROM BKG_VVD VVD
            ,VSK_VSL_PORT_SKD VSK
       WHERE 1=1
         AND VVD.VSL_CD = VSK.VSL_CD
         AND VVD.SKD_VOY_NO = VSK.SKD_VOY_NO
         AND VVD.SKD_DIR_CD = VSK.SKD_DIR_CD
         AND VVD.POL_CD = VSK.VPS_PORT_CD
         AND VVD.POL_CLPT_IND_SEQ = VSK.CLPT_IND_SEQ
         AND VVD.VSL_PRE_PST_CD||VVD.VSL_SEQ = ( SELECT MIN(VSL_PRE_PST_CD||VSL_SEQ) FROM BKG_VVD VVD2 WHERE VVD2.BKG_NO = VVD.BKG_NO)) VVD         
WHERE BK.BKG_NO = @[bkg_no]
AND BKG_STS_CD <>'X'
AND BK.BKG_NO = VVD.BKG_NO
AND BK.BKG_NO = TM.BKG_NO(+)
AND TM.CLZ_TP_CD(+) = 'D'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
