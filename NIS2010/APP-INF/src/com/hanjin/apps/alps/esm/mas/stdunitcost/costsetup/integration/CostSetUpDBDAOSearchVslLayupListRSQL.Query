    ,MIN(DP_SEQ) AS DP_SEQ
    ,MERGE_CD
    ,ROW_SEQ
FROM (
	SELECT (SUBSTR(COST_YRMON,1,4)||COST_WK) AS COST_YRMON
    	,COST_WK
    	,RLANE_CD
    	,'' AS VSL_CD_TTL
    	,VSL_CD
    	,STND_COST_CD
