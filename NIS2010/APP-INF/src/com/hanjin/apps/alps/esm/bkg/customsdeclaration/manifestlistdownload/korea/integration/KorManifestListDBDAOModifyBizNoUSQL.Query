<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOModifyBizNoUSQL">
			<desc><![CDATA[BizNo Update]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_KR_BL
   SET BIZ_RGST_NO = @[biz_no]
 WHERE BKG_NO = @[bkg_no]
   AND CSTMS_DECL_TP_CD = DECODE(@[expt_kcd_tp],'R','R','T','T',@[kcd_tp])
   AND DMST_PORT_CD = @[kt_port]
   AND TRNS_SEQ = @[kt_seq]			]]></sql>
			<params>
				<param name="biz_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="expt_kcd_tp" type="12" value="" out="N"/>
				<param name="kcd_tp" type="12" value="" out="N"/>
				<param name="kt_port" type="12" value="" out="N"/>
				<param name="kt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
