<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchSlsOfcCdBySRepCdRSQL">
			<desc><![CDATA[SLS_REP_CD 로 OFC_CD 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SLS.OFC_CD PRNT_OFC_CD 
  FROM MDM_SLS_REP SLS
 WHERE SLS.SREP_CD = @[srep_cd]			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
