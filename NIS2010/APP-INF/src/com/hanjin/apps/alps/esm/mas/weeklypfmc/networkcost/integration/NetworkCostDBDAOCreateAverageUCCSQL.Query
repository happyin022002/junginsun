<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOCreateAverageUCCSQL">
			<desc><![CDATA[--Average UC단가를 생성
History-------------------------------
2010.11.08 이행지 [CHM-201006375-01] Trunk IPC와 Ocean간 내부거래 신규 추가로 인한 계정은 제외되어서 생성되도록 수정
                         [CHM-201007231-01] - 생성로직에서 OP_LANE_TP_CD Group by에 포함되도록 수정]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_OP_AVG_UT_COST
(
	COST_YRMON
      , TRD_CD
      , RLANE_CD
      , STND_COST_CD
      , DIR_CD
      , COST_USE_TP_CD
      , OP_UC_AMT
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , UPD_DT
)
 SELECT COST_YRMON
      , TRD_CD
      , RLANE_CD
      , DECODE(B.NO, 1, '43102011', 2, '53101000', 3, '53102000', 4, '53200000', 5, '54100000'
                   , 6, '54250000', 7, '54300000', 8, '54200000', 9, '54150000', 10 , '54450000'
		   , 11, '54180000', 12, '54550000', 13, '54350000', 14, '54400000') STND_COST_CD
      , DIR_CD
      , 'A' COST_USE_TP_CD
      , DECODE(B.NO, 1, SPC_INCOME, 2, AMT_01, 3, AMT_02, 4, AMT_03, 5, AMT_04
                   , 6, AMT_05, 7, AMT_06, 8, AMT_07, 9, AMT_08, 10 , AMT_09
		   , 11, AMT_10, 12, AMT_11, 13, AMT_12, 14, AMT_13) OP_UC_AMT
      , @[cre_usr_id] CRE_USR_ID
      , SYSDATE CRE_DT
      , @[cre_usr_id] UPD_USR_ID
      , SYSDATE UPD_DT
   FROM 
        (
        --5단계
        --'Trade' / 'Lane' / 'BD별' 최종 단가를 생성. 이 단가는 2,3,4 계산 과정을 통해 해당 노선의 1Cycle에 대한 선박별 가중치가 반영되어 있는 단가.
        --이렇게 만들어진 해당 R.Month의 단가를 각 PFMC 주차 VVD의 BSA에 곱해주어 계정별 수입/비용 생성. 이후 과정 동일

         SELECT COST_YRMON
              , TRD_CD
              , RLANE_CD
              , DIR_CD
              , FREQ_NO
              , HJS_BSA_CAPA
              , DECODE(HJS_BSA_CAPA, 0, 0, SPC_INCOME / HJS_BSA_CAPA) AS SPC_INCOME
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_01 / HJS_BSA_CAPA)     AS AMT_01
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_02 / HJS_BSA_CAPA)     AS AMT_02
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_03 / HJS_BSA_CAPA)     AS AMT_03
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_04 / HJS_BSA_CAPA)     AS AMT_04
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_05 / HJS_BSA_CAPA)     AS AMT_05
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_06 / HJS_BSA_CAPA)     AS AMT_06
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_07 / HJS_BSA_CAPA)     AS AMT_07
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_08 / HJS_BSA_CAPA)     AS AMT_08
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_09 / HJS_BSA_CAPA)     AS AMT_09
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_10 / HJS_BSA_CAPA)     AS AMT_10
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_11 / HJS_BSA_CAPA)     AS AMT_11
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_12 / HJS_BSA_CAPA)     AS AMT_12
              , DECODE(HJS_BSA_CAPA, 0, 0, AMT_13 / HJS_BSA_CAPA)     AS AMT_13
           FROM
                (
                --4단계
                --3번 계산 과정까지 하여,
                --1Cycle에 대한, 개별 노선들의 선박별 가중치 반영 작업을 완료.  Pivot을 통하여 mix 하는 과정임

                 SELECT COST_YRMON
                      , TRD_CD
                      , RLANE_CD
                      , DIR_CD
                      , SUM(FREQ_NO)      AS FREQ_NO
                      , SUM(HJS_BSA_CAPA) AS HJS_BSA_CAPA
                      , SUM(SPC_INCOME)   AS SPC_INCOME
                      , SUM(AMT_01)       AS AMT_01
                      , SUM(AMT_02)       AS AMT_02
                      , SUM(AMT_03)       AS AMT_03
                      , SUM(AMT_04)       AS AMT_04
                      , SUM(AMT_05)       AS AMT_05
                      , SUM(AMT_06)       AS AMT_06
                      , SUM(AMT_07)       AS AMT_07
                      , SUM(AMT_08)       AS AMT_08
                      , SUM(AMT_09)       AS AMT_09
                      , SUM(AMT_10)       AS AMT_10
                      , SUM(AMT_11)       AS AMT_11
                      , SUM(AMT_12)       AS AMT_12
                      , SUM(AMT_13)       AS AMT_13
                   FROM
                        (
                        --3단계
                        --2번 계산 과정에서 1척당 비용을 구했으니, 기존에 입력해 두었던 선박척수 Table의 정보와 매칭하여
                        --1 Cycle당 수입 / 비용을 구할 수가 있다.
                        --예를 들어, TPS-AWHTP노선의 경우 'OWN'과 'CHT' 두 Type의 선박이 투입되며, OWN 7척 / CHT 3척하여 총 10척이 1Cycle을 이룬다고 선박척수 Table에 입력해 놓음.

                         SELECT A.COST_YRMON
                              , A.TRD_CD
                              , A.RLANE_CD
                              , A.DIR_CD
                              , A.VSL_OSHP_CD
                              , B.FREQ_NO
                              , (A.HJS_BSA_CAPA * B.FREQ_NO) AS HJS_BSA_CAPA
                              , (A.SPC_INCOME * B.FREQ_NO)   AS SPC_INCOME
                              , (A.AMT_01 * B.FREQ_NO)       AS AMT_01
                              , (A.AMT_02 * B.FREQ_NO)       AS AMT_02
                              , (A.AMT_03 * B.FREQ_NO)       AS AMT_03
                              , (A.AMT_04 * B.FREQ_NO)       AS AMT_04
                              , (A.AMT_05 * B.FREQ_NO)       AS AMT_05
                              , (A.AMT_06 * B.FREQ_NO)       AS AMT_06
                              , (A.AMT_07 * B.FREQ_NO)       AS AMT_07
                              , (A.AMT_08 * B.FREQ_NO)       AS AMT_08
                              , (A.AMT_09 * B.FREQ_NO)       AS AMT_09
                              , (A.AMT_10 * B.FREQ_NO)       AS AMT_10
                              , (A.AMT_11 * B.FREQ_NO)       AS AMT_11
                              , (A.AMT_12 * B.FREQ_NO)       AS AMT_12
                              , (A.AMT_13 * B.FREQ_NO)       AS AMT_13
                           FROM
                                (
                                --2단계
                                --1번 계산 과정에서 확보한 확보한 해당 기간동안의 모든 수입 비용을 가지고
                                --'Trade별' / 'Lane별' / 'Bound별' / 'Vessel Type별' 1척당 비용을 계산한다. 해당 기간 동안의 선박척수 (BSA가 0이 아닌 선박척수) 로 나누어 주면 됨

                                 SELECT COST_YRMON
                                      , TRD_CD
                                      , RLANE_CD
                                      , DIR_CD
                                      , VSL_OSHP_CD
                                      , (HJS_BSA_CAPA / CNT) AS HJS_BSA_CAPA
                                      , (CNT / CNT)          AS CNT
                                      , (SPC_INCOME / CNT)   AS SPC_INCOME
                                      , (AMT_01 / CNT)       AS AMT_01
                                      , (AMT_02 / CNT)       AS AMT_02
                                      , (AMT_03 / CNT)       AS AMT_03
                                      , (AMT_04 / CNT)       AS AMT_04
                                      , (AMT_05 / CNT)       AS AMT_05
                                      , (AMT_06 / CNT)       AS AMT_06
                                      , (AMT_07 / CNT)       AS AMT_07
                                      , (AMT_08 / CNT)       AS AMT_08
                                      , (AMT_09 / CNT)       AS AMT_09
                                      , (AMT_10 / CNT)       AS AMT_10
                                      , (AMT_11 / CNT)       AS AMT_11
                                      , (AMT_12 / CNT)       AS AMT_12
                                      , (AMT_13 / CNT)       AS AMT_13
                                   FROM
                                        (SELECT COST_YRMON
                                              , TRD_CD
                                              , RLANE_CD
                                              , DIR_CD
                                              , VSL_OSHP_CD
                                              , SUM(FNL_HJS_BSA_CAPA) AS HJS_BSA_CAPA
                                              -- 1cycle의 척수 
                                              , COUNT(*) CNT
                                              , SUM(SPC_INCOME) SPC_INCOME
                                              , SUM(AMT_01 ) AS AMT_01
                                              , SUM(AMT_02)  AS AMT_02
                                              , SUM(AMT_03)  AS AMT_03
                                              , SUM(AMT_04)  AS AMT_04
                                              , SUM(AMT_05)  AS AMT_05
                                              , SUM(AMT_06)  AS AMT_06
                                              , SUM(AMT_07)  AS AMT_07
                                              , SUM(AMT_08)  AS AMT_08
                                              , SUM(AMT_09)  AS AMT_09
                                              , SUM(AMT_10)  AS AMT_10
                                              , SUM(AMT_11)  AS AMT_11
                                              , SUM(AMT_12)  AS AMT_12
                                              , SUM(AMT_13)  AS AMT_13
                                           FROM
                                                (
                                                -- 1단계:
                                                -- Create Lane Table에서 지정한 OP4 단가 생성 대상 노선들 (Lane Avg U/C) 의 / Create시 지정한 WK 에 해당하는 SML Sales Cht/Out의 모든 Data를 긁어온다.
                                                -- 단, Final SML BSA가 0인 VVD는 대상에서 제외한다. 비용이 들어있어도 제외한다. 그랬을 시 아래와 같은 모양새의  Data 확보 완료.
                                                -- 'Trade별' / 'Lane별' / 'Bound별' / Operator ('Vessel Type별')
                                                -- '1번 계산 과정'의 하기 Raw Data를 OP4 단가 메인 화면의 Raw Data Down을 누르면 다운받을 수 있게 함. 

                                                 SELECT REPLACE(@[f_cost_yrmon],'-','') AS COST_YRMON
                                                      , A.COST_WK
                                                      , A.TRD_CD
                                                      , A.RLANE_CD
                                                      , A.VSL_CD
                                                      , A.SKD_VOY_NO
                                                      , A.DIR_CD
                                                      , NVL(V.VSL_OSHP_CD, 'OTH') VSL_OSHP_CD
                                                      , B.FNL_HJS_BSA_CAPA
                                                      , NVL(B.INCM_BZC_CHTR_AMT, 0)+NVL(B.INCM_SUB_CHTR_AMT, 0)+NVL(B.INCM_CRS_CHTR_AMT, 0) AS SPC_INCOME --Space Charter Revenue
                                                      , C.AMT_01
                                                      , C.AMT_02
                                                      , C.AMT_03
                                                      , C.AMT_04
                                                      , C.AMT_05
                                                      , C.AMT_06
                                                      , C.AMT_07
                                                      , C.AMT_08
                                                      , C.AMT_09
                                                      , C.AMT_10
                                                      , C.AMT_11
                                                      , C.AMT_12
                                                      , C.AMT_13
                                                   FROM MAS_MON_VVD A
                                                      , BSA_VVD_MST B
                                                      , (SELECT A1.TRD_CD                                                         AS TRD_CD
                                                              , A1.RLANE_CD                                                       AS RLANE_CD
                                                              , A1.IOC_CD                                                         AS IOC_CD
                                                              , A1.VSL_CD                                                         AS VSL_CD
                                                              , A1.SKD_VOY_NO                                                     AS SKD_VOY_NO
                                                              , A1.DIR_CD                                                         AS DIR_CD
                                                              , SUM(TS_UC_AMT)                                                    AS TS_UC_AMT
                                                              , SUM(DECODE(A1.STND_COST_CD, '53101000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_01
                                                              , SUM(DECODE(A1.STND_COST_CD, '53102000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_02
                                                              , SUM(DECODE(A1.STND_COST_CD, '53200000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_03
                                                              , SUM(DECODE(A1.STND_COST_CD, '54100000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_04
                                                              , SUM(DECODE(A1.STND_COST_CD, '54250000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_05
                                                              , SUM(DECODE(A1.STND_COST_CD, '54300000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_06
                                                              , SUM(DECODE(A1.STND_COST_CD, '54200000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_07
                                                              , SUM(DECODE(A1.STND_COST_CD, '54150000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_08
                                                              , SUM(DECODE(A1.STND_COST_CD, '54450000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_09
                                                              , SUM(DECODE(A1.STND_COST_CD, '54180000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_10
                                                              , SUM(DECODE(A1.STND_COST_CD, '54550000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_11
                                                              , SUM(DECODE(A1.STND_COST_CD, '54350000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_12
                                                              , SUM(DECODE(A1.STND_COST_CD, '54400000', A1.NTWK_HIR_COST_AMT, 0)) AS AMT_13
                                                           FROM MAS_VVD_HIR A1
                                                       GROUP BY A1.TRD_CD
                                                              , A1.RLANE_CD
                                                              , A1.IOC_CD
                                                              , A1.VSL_CD
                                                              , A1.SKD_VOY_NO
                                                              , A1.DIR_CD
                                                        ) C
                                                      , MAS_LANE_RGST R
                                                      , MAS_VSL_RGST V
                                                  WHERE A.TRD_CD     = B.TRD_CD
                                                    AND A.RLANE_CD   = B.RLANE_CD
                                                    AND A.IOC_CD     = B.IOC_CD
                                                    AND A.VSL_CD     = B.VSL_CD
                                                    AND A.SKD_VOY_NO = B.SKD_VOY_NO
                                                    AND A.DIR_CD     = B.SKD_DIR_CD
                                                    AND A.TRD_CD     = C.TRD_CD
                                                    AND A.RLANE_CD   = C.RLANE_CD
                                                    AND A.IOC_CD     = C.IOC_CD
                                                    AND A.VSL_CD     = C.VSL_CD
                                                    AND A.SKD_VOY_NO = C.SKD_VOY_NO
                                                    AND A.DIR_CD     = C.DIR_CD
                                                    AND A.TRD_CD     = R.TRD_CD
                                                    AND A.RLANE_CD   = R.RLANE_CD
                                                    AND A.IOC_CD     = R.IOC_CD
                                                    AND A.DIR_CD     = R.DIR_CD
                                                    AND R.DELT_FLG   = 'N'
                                                    --   AND R.OP_LANE_TP_CD      = 'LA' -- Lane Avg U/C -> 배부시에 적용

                                                    AND A.VSL_CD   = V.VSL_CD(+)
                                                    AND V.LST_FLG(+)  = 'Y'
                                                    AND V.DELT_FLG(+) = 'N'
                                                    AND A.DELT_FLG = 'N'
                                                    AND SUBSTR(A.SLS_YRMON,1,4)||A.COST_WK BETWEEN REPLACE(@[f_fm_yrwk],'-','') AND REPLACE(@[f_to_yrwk],'-','')
                                                    AND B.FNL_HJS_BSA_CAPA > 0
                                                    AND A.SUB_TRD_CD <> 'IP'
                                                )
                                       GROUP BY COST_YRMON
                                              , TRD_CD
                                              , RLANE_CD
                                              , DIR_CD
                                              , VSL_OSHP_CD
                                        )
                                ) A
                              , MAS_LANE_VSL_TP_FREQ B
                          WHERE A.COST_YRMON  = B.COST_YRMON(+)
                            AND A.TRD_CD      = B.TRD_CD(+)
                            AND A.RLANE_CD    = B.RLANE_CD(+)
                            AND A.VSL_OSHP_CD = B.VSL_OSHP_CD(+)
                        )
               GROUP BY COST_YRMON
                      , TRD_CD
                      , RLANE_CD
                      , DIR_CD
                )
        ) A
      , (SELECT LEVEL NO
           FROM DUAL
                CONNECT BY LEVEL <= 14
        ) B			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
				<param name="f_fm_yrwk" type="12" value="" out="N"/>
				<param name="f_to_yrwk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
