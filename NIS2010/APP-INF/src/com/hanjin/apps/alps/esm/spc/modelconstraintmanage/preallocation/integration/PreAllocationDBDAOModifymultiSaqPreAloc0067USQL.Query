<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PreAllocationDBDAOModifymultiSaqPreAloc0067USQL">
			<desc><![CDATA[pre-allocation update]]></desc>
			<sql><![CDATA[
      UPDATE  SAQ_PRE_ALOC SET 
	 	     BSE_YR = @[bse_yr], 
		     BSE_MON = @[bse_mon], 
		     RLANE_CD = @[rlane_cd], 
		     DIR_CD = @[dir_cd], 
		     VSL_CLSS_CAPA = @[vsl_clss_capa], 
		     PORT_CD = @[port_cd], 
		     TO_TRD_CD = @[to_trd_cd], 
		     TO_DIR_CD = @[to_dir_cd], 
		     REP_TRD_CD = @[rep_trd_cd], 
		     SPC_ALOC_QTY = @[spc_aloc_qty], 
		     UPD_USR_ID = @[upd_usr_id],
		     UPD_DT = SYSDATE
 	WHERE  BSE_YR = @[bse_yr] 
	    AND   BSE_MON = @[bse_mon] 
	    AND   RLANE_CD = @[rlane_cd] 
	    AND   DIR_CD = @[dir_cd] 
	    AND   VSL_CLSS_CAPA = @[vsl_clss_capa] 
	    AND   PORT_CD = @[port_cd] 
	    AND   TO_TRD_CD = @[to_trd_cd]  
	    AND   TO_DIR_CD = @[to_dir_cd]			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_mon" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_capa" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="to_trd_cd" type="12" value="" out="N"/>
				<param name="to_dir_cd" type="12" value="" out="N"/>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="spc_aloc_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
