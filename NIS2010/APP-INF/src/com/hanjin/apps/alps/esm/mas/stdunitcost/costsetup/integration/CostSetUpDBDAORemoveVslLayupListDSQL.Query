<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostSetUpDBDAORemoveVslLayupListDSQL">
			<desc><![CDATA[MAS_MNL_DTL_COST 테이블의 데이터 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MAS_MNL_DTL_COST
WHERE COST_YRMON LIKE SUBSTR(REPLACE(@[f_yearweek],'-',''),1,4)||'%'
    AND COST_WK = SUBSTR(REPLACE(@[f_yearweek],'-',''),5,2)
    AND RLANE_CD = @[rlane_cd]			]]></sql>
			<params>
				<param name="f_yearweek" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
