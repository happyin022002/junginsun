<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqBreakbulkDBDAOsearchPriScqBbNewRqstNoRSQL">
			<desc><![CDATA[searchPriScqBbNewRqstNo]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(@[rqst_ofc_cd],1,3)||TO_CHAR(SYSDATE,'YYYYMMDD')||LPAD(NVL(MAX(SUBSTR(SCQ_RQST_NO,12,4)),0)+1,4,'0') AS SCQ_RQST_NO
  FROM PRI_SCQ_BB_MN
 WHERE SUBSTR(SCQ_RQST_NO,1,11) = SUBSTR(@[rqst_ofc_cd],1,3)||TO_CHAR(SYSDATE,'YYYYMMDD')			]]></sql>
			<params>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
