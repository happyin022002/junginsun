<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchBKGCNTRListRSQL">
			<desc><![CDATA[한국세관 CNTR에 Insert하기위해 조회함]]></desc>
			<sql><![CDATA[
#if(${bound} == 'O' && ${type} == 'P' && ${c_bl_no} == ${bkg_no})
SELECT *
FROM
(
#end
SELECT 	B_CNTR_NO
     	, B_CNTRTS_CD
     	, MAX(B_CNTR_SEAL_NO1) B_CNTR_SEAL_NO1
     	, MAX(B_CNTR_SEAL_NO2) B_CNTR_SEAL_NO2
     	, B_CNTR_PKG_QTY
     	, B_CNTR_PKG_CD
     	, B_CNTR_WGT_QTY
     	, B_CNTR_WGT_TP
     	, B_CNTR_MEA_QTY
     	, B_CNTR_MEA_TP
	 	, B_BB_CGO_FLG
#if(${bound} == 'O' && ${type} == 'P' && ${c_bl_no} == ${bkg_no})
		, MAX((	SELECT  BL.CSTMS_BL_NO
    		FROM    BKG_CSTMS_KR_BL BL,
            		BKG_CSTMS_KR_CNTR CNTR            
    		WHERE   BL.CSTMS_BL_NO      = CNTR.CSTMS_BL_NO
    		AND     BL.CSTMS_DECL_TP_CD = CNTR.CSTMS_DECL_TP_CD
    		AND     BL.TRNS_SEQ         = CNTR.TRNS_SEQ
    		AND     CNTR.CNTR_NO        = B_CNTR_NO
    		AND     BL.CSTMS_DECL_TP_CD = 'T'
    		AND     BL.BKG_CGO_TP_CD 	= 'P'
    		AND     BL.KR_CSTMS_BND_CD 	= 'T'
			AND		BL.MF_SND_DT		= (	SELECT  MAX(MF_SND_DT)
    										FROM    BKG_CSTMS_KR_BL BL,
            										BKG_CSTMS_KR_CNTR CNTR1           
    										WHERE   BL.CSTMS_BL_NO      = CNTR1.CSTMS_BL_NO
    										AND     BL.CSTMS_DECL_TP_CD = CNTR1.CSTMS_DECL_TP_CD
    										AND     BL.TRNS_SEQ         = CNTR1.TRNS_SEQ
    										AND     CNTR1.CNTR_NO       = CNTR.CNTR_NO
    										AND     BL.MF_SND_DT        IS NOT NULL
											)
    		AND     BL.MF_SND_DT        IS NOT NULL
    		)) X_CNTR
#end
FROM	(	
			SELECT 	NVL(CNTR.CNTR_NO,' ') B_CNTR_NO 
        			, NVL(CNTR.CNTR_TPSZ_CD,' ') B_CNTRTS_CD 
        			, NVL(DECODE(SEAL.CNTR_SEAL_SEQ,1,SEAL.CNTR_SEAL_NO),' ') B_CNTR_SEAL_NO1
        			, NVL(DECODE(SEAL.CNTR_SEAL_SEQ,2,SEAL.CNTR_SEAL_NO),' ') B_CNTR_SEAL_NO2
        			, NVL(CNTR.PCK_QTY,0) B_CNTR_PKG_QTY 
        			, NVL(CNTR.PCK_TP_CD,' ') B_CNTR_PKG_CD 
        			, TRUNC(DECODE(NVL(CNTR.WGT_UT_CD,'  '),'LBS',ROUND(NVL(CNTR.CNTR_WGT,0)*0.4536,3),NVL(CNTR.CNTR_WGT,0)),2) B_CNTR_WGT_QTY 
        			, DECODE(NVL(CNTR.WGT_UT_CD,''),'LBS','KGS',NVL(CNTR.WGT_UT_CD,'')) B_CNTR_WGT_TP 
        			, NVL(CNTR.MEAS_QTY,0) B_CNTR_MEA_QTY 
        			, NVL(CNTR.MEAS_UT_CD,'') B_CNTR_MEA_TP
					, NVL(CNTR.BB_CGO_FLG, 'N') B_BB_CGO_FLG 
        	FROM 	BKG_CONTAINER CNTR, BKG_CNTR_SEAL_NO SEAL 
        	WHERE 	CNTR.BKG_NO  = @[bkg_no]
        	AND 	CNTR.BKG_NO  = SEAL.BKG_NO(+) 
        	AND 	CNTR.CNTR_NO = SEAL.CNTR_NO(+) 
        	ORDER BY NVL(CNTR.CNTR_NO,' ') 
		) TBL1
#if(${c_bl_no} != '' && ${c_bl_no} != ${bkg_no})
		,(
        	SELECT  CNTR.CNTR_NO
        	FROM    BKG_CSTMS_KR_BL BL, 
                	BKG_CSTMS_KR_CNTR CNTR            
        	WHERE   BL.BKG_NO = CNTR.BKG_NO
        	AND     BL.CSTMS_BL_NO      = CNTR.CSTMS_BL_NO
        	AND     BL.CSTMS_DECL_TP_CD = CNTR.CSTMS_DECL_TP_CD
        	AND     BL.TRNS_SEQ         = CNTR.TRNS_SEQ
        	AND     BL.CSTMS_BL_NO   	= @[c_bl_no]
        	AND     BL.CSTMS_DECL_TP_CD = 'T'
        	AND     BL.BKG_CGO_TP_CD 	= 'P'
        	AND     BL.KR_CSTMS_BND_CD 	= 'T'
       	) TBL2
#end
WHERE	1=1
#if(${c_bl_no} != '' && ${c_bl_no} != ${bkg_no})
AND		TBL1.B_CNTR_NO 	= TBL2.CNTR_NO
#end
GROUP BY B_CNTR_NO,B_CNTRTS_CD,B_CNTR_PKG_QTY,B_CNTR_PKG_CD,B_CNTR_WGT_QTY,B_CNTR_WGT_TP,B_CNTR_MEA_QTY,B_CNTR_MEA_TP,B_BB_CGO_FLG
ORDER BY B_CNTR_NO
#if(${bound} == 'O' && ${type} == 'P' && ${c_bl_no} == ${bkg_no})
) WHERE X_CNTR IS NULL
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="c_bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
