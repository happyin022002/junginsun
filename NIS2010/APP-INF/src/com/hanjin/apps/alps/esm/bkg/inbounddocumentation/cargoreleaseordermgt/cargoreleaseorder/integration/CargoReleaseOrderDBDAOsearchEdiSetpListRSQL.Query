<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchEdiSetpListRSQL">
			<desc><![CDATA[EDI Setup 조회]]></desc>
			<sql><![CDATA[
SELECT 
	YD_CD
    ,PORT_CD
	,SUBSTR(YD_CD,6,2) POD_YD_NO
	,SUBSTR(YD_CD,0,5) POD_CD 
	,PORT_CD POD_CD_CPY
	,EDI_RCV_ID
	,EDI_SND_ID
	,CASE WHEN FULL_RLSE_EDI_CD IN (1,2) THEN FULL_RLSE_EDI_CD ELSE '' END FULL_RLSE_EDI_CD
	,(SELECT YD_NM FROM MDM_YARD WHERE YD_CD = A.YD_CD ) YD_DESC
	,( SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = A.PORT_CD ) POD_DESC
	,NVL(SLAN_CD1,'') SLAN_CD1
	,NVL(SLAN_CD2,'') SLAN_CD2
	,NVL(SLAN_CD3,'') SLAN_CD3
	,NVL(SLAN_CD4,'') SLAN_CD4
	,NVL(SLAN_CD5,'') SLAN_CD5
	,NVL(SLAN_CD6,'') SLAN_CD6
	,NVL(SLAN_CD7,'') SLAN_CD7
	,NVL(SLAN_CD8,'') SLAN_CD8
	,NVL(SLAN_CD9,'') SLAN_CD9
	,NVL(SLAN_CD10,'') SLAN_CD10
	,CRE_USR_ID
	,UPD_USR_ID
	,TO_CHAR(CRE_DT,'YYYY-MM-DD HH24:MI') CRE_DT
	,TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI') UPD_DT
FROM BKG_EDI_YD A
WHERE 1=1
#if (${yd_cd} != '') 
AND YD_CD LIKE @[yd_cd]||'%'
#end     
#if (${port_cd} != '') 
AND PORT_CD = @[port_cd]
#end
#if (${edi_rcv_id} != '') 
AND EDI_RCV_ID = @[edi_rcv_id]
#end
#if (${edi_snd_id} != '') 
AND EDI_SND_ID = @[edi_snd_id]
#end
#if (${full_rlse_edi_cd} != '') 
	#if (${full_rlse_edi_cd} == '1' || ${full_rlse_edi_cd} == '2') 
		AND FULL_RLSE_EDI_CD = @[full_rlse_edi_cd]
    #elseif (${full_rlse_edi_cd} == '0') 
		AND FULL_RLSE_EDI_CD NOT IN (1,2)
	#end
#end
ORDER BY YD_CD			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="edi_rcv_id" type="12" value="" out="N"/>
				<param name="edi_snd_id" type="12" value="" out="N"/>
				<param name="full_rlse_edi_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
