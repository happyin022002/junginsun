<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanningDBDAOSearchRbcLaneQtaListRSQL">
			<desc><![CDATA[RBC Lane Qta list를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT RBC.BSE_TP_CD
      ,RBC.BSE_YR
      ,RBC.BSE_QTR_CD
      ,DECODE(RBC.OFC_VW_CD, 'C', 'CONTRACT', 'LOADING') OFC_VW_CD
      ,RBC.TRD_CD
      ,RBC.RLANE_CD
      ,RBC.DIR_CD
      ,RBC.RGN_OFC_CD
      ,RBC.RHQ_CD
      ,RBC.SUB_TRD_CD
      ,RBC.GID_LOD_QTY
      ,RBC.GID_GRS_REV
      ,RBC.GID_PA_CM_AMT
      ,RBC.GID_RA_CM_AMT
      ,RBC.LOD_QTY
      ,RBC.GRS_REV
      ,RBC.PA_CM_AMT
      ,RBC.RA_CM_AMT
 FROM SQM_QTA_RBC RBC
     ,SQM_QTA_LANE_OFC OFC
WHERE RBC.BSE_TP_CD    = @[f_bse_tp_cd]
  AND RBC.BSE_YR       = @[f_bse_yr]
  AND RBC.BSE_QTR_CD   = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
  AND RBC.OFC_VW_CD    = @[f_ofc_vw_cd]
#if (${f_rhq_cd} != '' && ${f_rhq_cd} != 'All')
  AND RBC.RHQ_CD       = @[f_rhq_cd]
#end
#if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
  AND RBC.RGN_OFC_CD   = @[f_rgn_ofc_cd]
#end
  AND RBC.BSE_TP_CD    = OFC.BSE_TP_CD
  AND RBC.BSE_YR       = OFC.BSE_YR
  AND RBC.BSE_QTR_CD   = OFC.BSE_QTR_CD
  AND RBC.OFC_VW_CD    = OFC.OFC_VW_CD
  AND RBC.TRD_CD       = OFC.TRD_CD
  AND RBC.RLANE_CD     = OFC.RLANE_CD
  AND RBC.DIR_CD       = OFC.DIR_CD
  AND RBC.RGN_OFC_CD   = OFC.RGN_OFC_CD
  AND OFC.SQM_ACT_FLG  = 'Y'
ORDER BY RBC.TRD_CD
        ,RBC.SUB_TRD_CD
        ,RBC.RLANE_CD
        ,RBC.DIR_CD
        ,RBC.RHQ_CD
        ,RBC.RGN_OFC_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_rhq_cd" type="12" value="" out="N"/>
				<param name="f_rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
