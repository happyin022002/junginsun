<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchBkgCloseRSQL">
			<desc><![CDATA[해당 bkg이 close 되었는지 조회한다.]]></desc>
			<sql><![CDATA[
select decode(BKG_CLZ_STS_CD, 'C', 'Y', 'N') BKG_CLZ_FLG
  from bkg_booking bk, bkg_vvd vvd, BKG_COFF_TM coff
 where bk.bkg_no = @[bkg_no]
   and bk.bkg_no = vvd.bkg_no
   and bk.pol_cd = vvd.pol_cd
   and vvd.vsl_pre_pst_cd in ('S','T')
   and vvd.vsl_cd           = coff.vsl_cd (+)
   and vvd.skd_voy_no       = coff.skd_voy_no(+)
   and vvd.skd_dir_cd       = coff.skd_dir_cd(+)
   and vvd.pol_cd           = coff.pol_cd(+)
   and vvd.pol_clpt_ind_seq = coff.clpt_ind_seq(+)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
