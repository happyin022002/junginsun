<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAOManageSimCommAddBatchListCSQL">
			<desc><![CDATA[대상 Simulation Agreement 를 Batch 에 입력한다.

* 2014.06.03 박다은 [CHM-201430120] Agent Comm. Mass simulation 에 기능 추가]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_CALC_BAT
              (
                BAT_RCV_DT
               ,BAT_RCV_SEQ
               ,BKG_NO
               ,COMM_TP_CD 
               ,BAT_FLG
               ,BAT_ITM_NM
               ,BAT_DESC 
               ,CALC_FM_DT 
               ,CALC_TO_DT 
               ,AGN_AGMT_NO
               ,CRE_USR_ID
               ,CRE_DT
               ,UPD_USR_ID
               ,UPD_DT
               )
    VALUES (
               TO_CHAR(SYSDATE,'YYYYMMDD')
              ,ACM_CALC_BAT_SEQ.NEXTVAL
              ,@[sim_agmt_no]
              ,'I'
              ,'N'
              ,@[sim_agmt_sml_no]
              ,'MassSim(S)_'||@[sim_agmt_no]||'_'||to_char(sysdate,'yymmddhh24miss')
              ,REPLACE(@[bkg_date_fm],'-','')
              ,REPLACE(@[bkg_date_to],'-','')
              ,@[sim_agmt_no]
              ,@[usr_id]
              ,SYSDATE
              ,@[usr_id]
              ,SYSDATE
              )			]]></sql>
			<params>
				<param name="sim_agmt_no" type="12" value="" out="N"/>
				<param name="sim_agmt_sml_no" type="12" value="" out="N"/>
				<param name="bkg_date_fm" type="12" value="" out="N"/>
				<param name="bkg_date_to" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
