<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOmodifyApInvHdrUSQL">
			<desc><![CDATA[Delete & Resubmit 대상전표의 AP_INV_HDR 테이블의 APRO_FLG = 'N'으로 변경한다  ]]></desc>
			<sql><![CDATA[
UPDATE AP_INV_HDR
   SET APRO_FLG = 'N'
 WHERE CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
