<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMSummaryDBDAORsltPrsCheckRegionCodeVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT RGN_CD,RGN_NM,CNT_CD
FROM MDM_REGION
WHERE DELT_FLG = 'N'

		#if(${cnt_cd} != '')
			AND CNT_CD = @[cnt_cd]
		#end
		#if(${rgn_cd} != '')
			AND RGN_CD = @[rgn_cd]
		#end
			  			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
