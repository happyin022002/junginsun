<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOMstRsltRnoteNoteConvListRSQL">
			<desc><![CDATA[Master RFA에서 Conversion 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT A.NOTE_CONV_MAPG_ID
      ,A.NOTE_CONV_SEQ
      ,A.NOTE_CONV_TP_CD
      ,A.SVC_SCP_CD
      ,A.PROP_NO
      ,A.AMDT_SEQ
      ,CASE
         WHEN A.CHG_RULE_TP_CD = 'C' THEN
          NOTE_CONV_CHG_CD
         WHEN A.CHG_RULE_TP_CD = 'R' THEN
          NOTE_CONV_RULE_CD
       END CHG_RULE_DEF_CD
      ,A.CHG_RULE_TP_CD
      ,A.NOTE_CONV_CHG_CD
      ,A.NOTE_CONV_RULE_CD
      ,(SELECT TO_CHAR(EFF_DT, 'YYYYMMDD')
          FROM PRI_RP_SCP_MN
         WHERE PROP_NO = A.PROP_NO
           AND AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ
           AND SVC_SCP_CD = A.SVC_SCP_CD) AS EFF_DT
      ,(SELECT CASE
                 WHEN M.AMDT_SEQ = A.AMDT_SEQ THEN
                  TO_CHAR(M.EXP_DT, 'YYYYMMDD')
                 WHEN M.EFF_DT <= N.EXP_DT THEN
                  TO_CHAR(M.EFF_DT - 1, 'YYYYMMDD')
                 ELSE
                  TO_CHAR(N.EXP_DT, 'YYYYMMDD')
               END AS EXP_DT
          FROM PRI_RP_SCP_MN M, PRI_RP_SCP_MN N
         WHERE M.PROP_NO = A.PROP_NO
           AND N.PROP_NO = A.PROP_NO
           AND M.AMDT_SEQ = @[amdt_seq]
           AND N.AMDT_SEQ = M.AMDT_SEQ - DECODE(@[amdt_seq], A.AMDT_SEQ, 0, 0, 0, 1)
           AND M.SVC_SCP_CD = A.SVC_SCP_CD
           AND N.SVC_SCP_CD = A.SVC_SCP_CD) AS EXP_DT
      ,A.RT_APPL_TP_CD
      ,A.RT_OP_CD
      ,A.CURR_CD
      ,A.FRT_RT_AMT
      ,A.PAY_TERM_CD
      ,A.BKG_RAT_UT_CD
      ,A.BKG_PRC_CGO_TP_CD
      ,A.BKG_IMDG_CLSS_CD
      ,A.BKG_CMDT_TP_CD
      ,A.BKG_CMDT_DEF_CD
      ,A.BKG_POR_TP_CD
	  , DECODE(A.BKG_POR_TP_CD, 'T', SUBSTR(A.BKG_POR_DEF_CD, 3), A.BKG_POR_DEF_CD) BKG_POR_DEF_CD
	  , A.BKG_POL_TP_CD
	  , DECODE(A.BKG_POL_TP_CD, 'T', SUBSTR(A.BKG_POL_DEF_CD, 3), A.BKG_POL_DEF_CD) BKG_POL_DEF_CD
	  , A.BKG_POD_TP_CD
	  , DECODE(A.BKG_POD_TP_CD, 'T', SUBSTR(A.BKG_POD_DEF_CD, 3), A.BKG_POD_DEF_CD) BKG_POD_DEF_CD
	  , A.BKG_DEL_TP_CD
	  , DECODE(A.BKG_DEL_TP_CD, 'T', SUBSTR(A.BKG_DEL_DEF_CD, 3), A.BKG_DEL_DEF_CD) BKG_DEL_DEF_CD
      ,A.BKG_SLAN_CD
      ,(A.BKG_VSL_CD || A.BKG_SKD_VOY_NO || A.BKG_SKD_DIR_CD) BKG_VVD_CD
      ,A.BKG_VSL_CD
      ,A.BKG_SKD_VOY_NO
      ,A.BKG_SKD_DIR_CD
      ,A.BKG_SOC_FLG
      ,A.BKG_TS_PORT_TP_CD
      ,A.BKG_TS_PORT_DEF_CD
      ,A.BKG_DIR_CALL_FLG
      ,A.BKG_MAX_CGO_WGT
      ,A.BKG_MIN_CGO_WGT
      ,A.BKG_HNGR_BAR_TP_CD
      ,TO_CHAR(A.UPD_DT, 'YYYYMMDD') UPD_DT
      ,(SELECT USR_NM FROM COM_USER WHERE USR_ID = A.UPD_USR_ID) USR_NM
	
	  , DECODE(A.BKG_POR_TP_CD, 'T', SUBSTR(A.BKG_POR_DEF_CD, 1, 2), '') BKG_POR_CNT_CD
	  , DECODE(A.BKG_POL_TP_CD, 'T', SUBSTR(A.BKG_POL_DEF_CD, 1, 2), '') BKG_POL_CNT_CD
	  , DECODE(A.BKG_POD_TP_CD, 'T', SUBSTR(A.BKG_POD_DEF_CD, 1, 2), '') BKG_POD_CNT_CD
	  , DECODE(A.BKG_DEL_TP_CD, 'T', SUBSTR(A.BKG_DEL_DEF_CD, 1, 2), '') BKG_DEL_CNT_CD
      ,A.BKG_YD_CD
      ,A.BKG_ESVC_TP_CD

      ,PRC_PROG_STS_CD
      ,(SELECT INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID = 'CD01719'
           AND INTG_CD_VAL_CTNT = PRC_PROG_STS_CD
           AND ROWNUM = 1) AS PRC_PROG_STS_NM
      ,SRC_INFO_CD
      ,(SELECT INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID = 'CD02198'
           AND INTG_CD_VAL_CTNT = SRC_INFO_CD
           AND ROWNUM = 1) AS SRC_INFO_NM
      ,N1ST_CMNC_AMDT_SEQ
      ,NEXT_N1ST_CMNC_AMDT_SEQ
  FROM (
    SELECT NOTE_CONV_MAPG_ID
          ,NOTE_CONV_SEQ
          ,NOTE_CONV_TP_CD
          ,SVC_SCP_CD
          ,PROP_NO
          ,AMDT_SEQ
          ,CHG_RULE_TP_CD
          ,NOTE_CONV_CHG_CD
          ,NOTE_CONV_RULE_CD
          ,EFF_DT
          ,EXP_DT
          ,RT_APPL_TP_CD
          ,RT_OP_CD
          ,CURR_CD
          ,FRT_RT_AMT
          ,PAY_TERM_CD
          ,BKG_RAT_UT_CD
          ,BKG_PRC_CGO_TP_CD
          ,BKG_IMDG_CLSS_CD
          ,BKG_CMDT_TP_CD
          ,BKG_CMDT_DEF_CD
          ,BKG_POR_TP_CD
          ,BKG_POR_DEF_CD
          ,BKG_POL_TP_CD
          ,BKG_POL_DEF_CD
          ,BKG_POD_TP_CD
          ,BKG_POD_DEF_CD
          ,BKG_DEL_TP_CD
          ,BKG_DEL_DEF_CD
          ,BKG_SLAN_CD
          ,BKG_VSL_CD
          ,BKG_SKD_VOY_NO
          ,BKG_SKD_DIR_CD
          ,BKG_MIN_CGO_WGT
          ,BKG_MAX_CGO_WGT
          ,BKG_SOC_FLG
          ,BKG_TS_PORT_TP_CD
          ,BKG_TS_PORT_DEF_CD
          ,BKG_DIR_CALL_FLG
          ,BKG_HNGR_BAR_TP_CD
          ,CRE_USR_ID
          ,CRE_DT
          ,UPD_USR_ID
          ,UPD_DT
          ,BKG_YD_CD
          ,BKG_ESVC_TP_CD
          ,PRC_PROG_STS_CD
          ,MST_RFA_ROUT_ID
          ,SRC_INFO_CD
          ,N1ST_CMNC_AMDT_SEQ
          ,LEAD(N1ST_CMNC_AMDT_SEQ) OVER(PARTITION BY PROP_NO, SVC_SCP_CD, NOTE_CONV_SEQ ORDER BY AMDT_SEQ) AS NEXT_N1ST_CMNC_AMDT_SEQ
      FROM PRI_RFA_NOTE_CONV CONV
     WHERE NOTE_CONV_TP_CD = 'R'
       AND EXISTS (SELECT 'OK'
              FROM PRI_RP_SCP_RT_CMDT_RNOTE
             WHERE PROP_NO = @[prop_no]
               AND (AMDT_SEQ = @[amdt_seq] OR
                   (AMDT_SEQ = @[amdt_seq] - 1 AND
                   SRC_INFO_CD IN ('NW', 'PC', 'PM', 'GC', 'GM', 'AM')))
               AND SVC_SCP_CD = @[svc_scp_cd]
               AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
               AND ROUT_SEQ = @[rout_seq]
               AND NOTE_CONV_MAPG_ID = CONV.NOTE_CONV_MAPG_ID))A
 WHERE NEXT_N1ST_CMNC_AMDT_SEQ IS NULL
    OR N1ST_CMNC_AMDT_SEQ <> NEXT_N1ST_CMNC_AMDT_SEQ
 ORDER BY CHG_RULE_DEF_CD, NOTE_CONV_SEQ, AMDT_SEQ			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="1" out="N"/>
				<param name="prop_no" type="12" value="SEL160530" out="N"/>
				<param name="svc_scp_cd" type="12" value="AEW" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="1" out="N"/>
				<param name="rout_seq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
