<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSASpecialManifestDBDAOaddPsaSendFlatFileLogCSQL">
			<desc><![CDATA[bkg_cstms_eur_snd_log 테이블에 FlatFile을 통으로 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_PSA_DG_SND_LOG (
	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	PSA_EDI_MSG_TP_ID
,	MSG_SND_NO
,	SND_LOG_SEQ
,	MSG_DESC
) VALUES( 
	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	@[eur_edi_msg_tp_id]
,	@[msg_snd_no]
,	(
		SELECT NVL(MAX(SND_LOG_SEQ), 0) + 1 AS CNTR_CGO_SEQ
		FROM BKG_CSTMS_PSA_DG_SND_LOG
		WHERE PSA_EDI_MSG_TP_ID = @[eur_edi_msg_tp_id]
		AND   MSG_SND_NO  		= @[msg_snd_no]
	)

,	@[msg_desc]
)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eur_edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="msg_snd_no" type="12" value="" out="N"/>
				<param name="msg_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
