<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOmodifyBkgTsCoffTmUSQL">
			<desc><![CDATA[TS 화물의 Cutt off Time 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_TS_COFF_TM SET 
	BKG_CLZ_STS_CD = @[bkg_clz_sts_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	POL_CD = @[pol_cd]
AND	NVL(CLPT_IND_SEQ, 1) = NVL(@[clpt_ind_seq], 1)			]]></sql>
			<params>
				<param name="bkg_clz_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
