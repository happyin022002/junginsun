<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltRtDtlListVORSQL">
			<desc><![CDATA[Rate - rate조회]]></desc>
			<sql><![CDATA[
SELECT PROP_NO
      ,AMDT_SEQ
      ,SVC_SCP_CD
      ,CMDT_HDR_SEQ
      ,ROUT_SEQ
      ,RT_SEQ
      ,RAT_UT_CD
      ,PRC_CGO_TP_CD
      ,CURR_CD
      ,PROP_FRT_RT_AMT
      ,COFFR_FRT_RT_AMT
      ,FNL_FRT_RT_AMT
      ,PRS_SCG_AMT
      ,PRS_RESPB_CM_UC_AMT
      ,PRS_RESPB_OPFIT_UC_AMT
      ,PRS_RESPB_CMPB_AMT
      ,PRS_GID_CMPB_AMT
      ,PRS_RESPB_OPB_AMT
      ,PRS_RESPB_CMPB_AMT - PRS_GID_CMPB_AMT AS DIFF
      ,PRS_PFIT_CM_UC_AMT
      ,PRS_PFIT_CMPB_AMT
      ,PRS_UPD_DT
      ,DECODE(GRI_APPL_TP_CD, 'N', '', GRI_APPL_TP_CD) AS GRI_APPL_TP_CD
      ,DECODE(GRI_APPL_TP_CD, 'N', '', GRI_APPL_AMT) AS GRI_APPL_AMT
      ,PRC_PROG_STS_CD
      ,(SELECT INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID = 'CD01719'
           AND INTG_CD_VAL_CTNT = PRC_PROG_STS_CD
           AND ROWNUM = 1) AS PRC_PROG_STS_NM
      ,SRC_INFO_CD
      ,(SELECT INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID = 'CD02198'
           AND INTG_CD_VAL_CTNT = SRC_INFO_CD
           AND ROWNUM = 1) AS SRC_INFO_NM
      ,N1ST_CMNC_AMDT_SEQ
      ,NEXT_N1ST_CMNC_AMDT_SEQ
      ,(SELECT TO_CHAR(EFF_DT, 'YYYYMMDD')
          FROM PRI_RP_SCP_MN
         WHERE PROP_NO = A.PROP_NO
           AND AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ
           AND SVC_SCP_CD = A.SVC_SCP_CD) AS EFF_DT
      ,(SELECT CASE
                 WHEN M.AMDT_SEQ = A.AMDT_SEQ THEN
                  TO_CHAR(M.EXP_DT, 'YYYYMMDD')
                 WHEN M.EFF_DT <= N.EXP_DT THEN
                  TO_CHAR(M.EFF_DT - 1, 'YYYYMMDD')
                 ELSE
                  TO_CHAR(N.EXP_DT, 'YYYYMMDD')
               END AS EXP_DT
          FROM PRI_RP_SCP_MN M, PRI_RP_SCP_MN N
         WHERE M.PROP_NO = A.PROP_NO
           AND N.PROP_NO = A.PROP_NO
           AND M.AMDT_SEQ = @[amdt_seq]
           AND N.AMDT_SEQ = M.AMDT_SEQ - DECODE(@[amdt_seq], A.AMDT_SEQ, 0, 0, 0, 1)
           AND M.SVC_SCP_CD = A.SVC_SCP_CD
           AND N.SVC_SCP_CD = A.SVC_SCP_CD) AS EXP_DT
      ,ACPT_USR_ID
      ,ACPT_OFC_CD
      ,(SELECT USR_NM FROM COM_USER WHERE USR_ID = A.ACPT_USR_ID AND ROWNUM = 1) || ' / ' || A.ACPT_OFC_CD AS ACPT_USR_NM
      ,TO_CHAR(A.ACPT_DT, 'YYYYMMDD') AS ACPT_DT
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,(SELECT COUNT(*) AS CNT
          FROM PRI_RP_SCP_RT_CGO_SPEC
         WHERE PROP_NO = A.PROP_NO
           AND AMDT_SEQ = A.AMDT_SEQ
           AND SVC_SCP_CD = A.SVC_SCP_CD
           AND CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
           AND ROUT_SEQ = A.ROUT_SEQ
           AND RT_SEQ = A.RT_SEQ) AS SPEC_CNT
      ,FIRST_VALUE(DECODE(PRC_CGO_TP_CD, 'DR', 1, 'RF', 2, 'DG', 3, 'AK', 4, 99)) OVER(PARTITION BY PROP_NO, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, RT_SEQ ORDER BY AMDT_SEQ) AS N1ST_ORD_REF
      ,FIRST_VALUE(RAT_UT_CD) OVER(PARTITION BY PROP_NO, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, RT_SEQ ORDER BY AMDT_SEQ) AS N2ND_ORD_REF
      ,FIC_PROP_RT_AMT
      ,FIC_COFFR_RT_AMT
      ,FIC_FNL_RT_AMT
      ,FIC_GLINE_RT_AMT
      ,FIC_GLINE_UPD_DT
      ,OPTM_TRSP_MOD_FLG
      ,FIC_RT_USE_STS_CD
      ,FIC_ORG_PROP_RT_AMT
      ,FIC_ORG_COFFR_RT_AMT
      ,FIC_ORG_FNL_RT_AMT
      ,FIC_ORG_GLINE_RT_AMT
      ,FIC_ORG_GLINE_UPD_DT
      ,ORG_OPTM_TRSP_MOD_FLG
      ,FIC_ORG_RT_USE_STS_CD
      ,FIC_DEST_PROP_RT_AMT
      ,FIC_DEST_COFFR_RT_AMT
      ,FIC_DEST_FNL_RT_AMT
      ,FIC_DEST_GLINE_RT_AMT
      ,FIC_DEST_GLINE_UPD_DT
      ,DEST_OPTM_TRSP_MOD_FLG
      ,FIC_DEST_RT_USE_STS_CD
	  ,MST_RFA_ROUT_ID    --------------RFA 효율화를 위한 요청 (1차) (CHM-201640671)
  FROM (SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,RT_SEQ
              ,RAT_UT_CD
              ,PRC_CGO_TP_CD
              ,CURR_CD
              ,PROP_FRT_RT_AMT
              ,COFFR_FRT_RT_AMT
              ,FNL_FRT_RT_AMT
              ,PRS_SCG_AMT
              ,PRS_RESPB_CM_UC_AMT
              ,PRS_RESPB_OPFIT_UC_AMT
              ,PRS_RESPB_CMPB_AMT
              ,PRS_GID_CMPB_AMT
              ,PRS_RESPB_OPB_AMT
              ,PRS_PFIT_CM_UC_AMT
              ,PRS_PFIT_CMPB_AMT
              ,PRS_UPD_DT
              ,GRI_APPL_TP_CD
              ,GRI_APPL_AMT
              ,PRC_PROG_STS_CD
              ,SRC_INFO_CD
              ,N1ST_CMNC_AMDT_SEQ
              ,LEAD(N1ST_CMNC_AMDT_SEQ) OVER(PARTITION BY PROP_NO, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, RT_SEQ ORDER BY AMDT_SEQ) AS NEXT_N1ST_CMNC_AMDT_SEQ
              ,ACPT_USR_ID
              ,ACPT_OFC_CD
              ,ACPT_DT
              ,CRE_USR_ID
              ,CRE_DT
              ,UPD_USR_ID
              ,UPD_DT
              ,FIC_PROP_RT_AMT
              ,FIC_COFFR_RT_AMT
              ,FIC_FNL_RT_AMT
              ,FIC_GLINE_RT_AMT
              ,FIC_GLINE_UPD_DT
              ,OPTM_TRSP_MOD_FLG
              ,FIC_RT_USE_STS_CD
      	      ,FIC_ORG_PROP_RT_AMT
      ,FIC_ORG_COFFR_RT_AMT
      ,FIC_ORG_FNL_RT_AMT
      ,FIC_ORG_GLINE_RT_AMT
      ,FIC_ORG_GLINE_UPD_DT
      ,ORG_OPTM_TRSP_MOD_FLG
      ,FIC_ORG_RT_USE_STS_CD
      ,FIC_DEST_PROP_RT_AMT
      ,FIC_DEST_COFFR_RT_AMT
      ,FIC_DEST_FNL_RT_AMT
      ,FIC_DEST_GLINE_RT_AMT
      ,FIC_DEST_GLINE_UPD_DT
      ,DEST_OPTM_TRSP_MOD_FLG
      ,FIC_DEST_RT_USE_STS_CD
      ,MST_RFA_ROUT_ID    --------------RFA 효율화를 위한 요청 (1차) (CHM-201640671)
          FROM PRI_RP_SCP_RT
         WHERE PROP_NO = @[prop_no]
           AND (AMDT_SEQ = @[amdt_seq] OR
               (AMDT_SEQ = @[amdt_seq] - 1 AND
               SRC_INFO_CD IN ('NW', 'PC', 'PM', 'GC', 'GM', 'AM')))
           AND SVC_SCP_CD = @[svc_scp_cd]
#if(${cmdt_hdr_seq} != 'null')
           AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#end
           AND ROUT_SEQ = @[rout_seq]) A
 WHERE NEXT_N1ST_CMNC_AMDT_SEQ IS NULL
    OR N1ST_CMNC_AMDT_SEQ <> NEXT_N1ST_CMNC_AMDT_SEQ
 ORDER BY N1ST_ORD_REF, N2ND_ORD_REF, RT_SEQ, AMDT_SEQ			]]></sql>
			<params>
				<param name="amdt_seq" type="2" value="2" out="N"/>
				<param name="prop_no" type="12" value="TES090001" out="N"/>
				<param name="svc_scp_cd" type="12" value="ACE" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="1" out="N"/>
				<param name="rout_seq" type="2" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
