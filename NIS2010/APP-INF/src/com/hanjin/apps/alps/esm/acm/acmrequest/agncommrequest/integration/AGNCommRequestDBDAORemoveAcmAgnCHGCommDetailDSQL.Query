<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAORemoveAcmAgnCHGCommDetailDSQL">
			<desc><![CDATA[Remove Acm Agn CHG Comm Detail]]></desc>
			<sql><![CDATA[
DELETE ACM_AGN_COMM_DTL_CHG
WHERE 1=1
AND BKG_NO = @[bkg_no]
AND (BKG_NO,AGN_CD,IO_BND_CD,AC_TP_CD,AC_SEQ) NOT IN 
    (
        SELECT BKG_NO,AGN_CD,IO_BND_CD,AC_TP_CD,AC_SEQ
        FROM   ACM_AGN_COMM
        WHERE  BKG_NO    = @[bkg_no]
    )
    			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
