<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDBDAOFmsEmailAddresssCSQL">
			<desc><![CDATA[E-mail List Registration Insert]]></desc>
			<sql><![CDATA[
INSERT INTO FMS_SCR_EML_ADDR (
	   EML_SEQ,
	   CNTC_PSON_NM,
	   CNTC_PSON_EML,
	   CRE_USR_ID,
	   CRE_DT,
	   UPD_USR_ID,
	   UPD_DT
) VALUES (
  	   FMS_EML_SEQ.nextval,
       @[cntc_pson_nm],
	   @[cntc_pson_eml],
	   @[cre_usr_id],
	   SYSDATE,
	   @[upd_usr_id],
	   SYSDATE
)			]]></sql>
			<params>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_pson_eml" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
