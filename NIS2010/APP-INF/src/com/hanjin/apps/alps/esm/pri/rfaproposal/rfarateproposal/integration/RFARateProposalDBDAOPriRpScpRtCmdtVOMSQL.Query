<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtCmdtVOMSQL">
			<desc><![CDATA[CMDT입력 및 수정]]></desc>
			<sql><![CDATA[
MERGE INTO PRI_RP_SCP_RT_CMDT
USING DUAL
ON (
	PROP_NO = @[prop_no]
	AND AMDT_SEQ = @[amdt_seq]
	AND SVC_SCP_CD = @[svc_scp_cd]
	AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
	AND CMDT_SEQ = @[cmdt_seq]
)
WHEN MATCHED THEN
UPDATE SET
	PRC_CMDT_TP_CD = @[prc_cmdt_tp_cd]
,	PRC_CMDT_DEF_CD = @[prc_cmdt_def_cd]
,	PRC_PROG_STS_CD = 'I'
,	SRC_INFO_CD = 
(CASE WHEN @[amdt_seq] = '0' THEN 'NW'
     WHEN @[amdt_seq] = N1ST_CMNC_AMDT_SEQ THEN 'NW'
     WHEN @[amdt_seq] != N1ST_CMNC_AMDT_SEQ THEN 'AM'
END)
,	N1ST_CMNC_AMDT_SEQ = @[amdt_seq] 
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHEN NOT MATCHED THEN
INSERT (
	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	CMDT_HDR_SEQ
,	CMDT_SEQ
,	PRC_CMDT_TP_CD
,	PRC_CMDT_DEF_CD
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	N1ST_CMNC_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[prop_no]
,	@[amdt_seq]
,	@[svc_scp_cd]
,	@[cmdt_hdr_seq]
,	@[cmdt_seq]
,	@[prc_cmdt_tp_cd]
,	@[prc_cmdt_def_cd]
,	'I'
,	'NW'
,	@[amdt_seq]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="cmdt_seq" type="2" value="" out="N"/>
				<param name="prc_cmdt_tp_cd" type="12" value="" out="N"/>
				<param name="prc_cmdt_def_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
