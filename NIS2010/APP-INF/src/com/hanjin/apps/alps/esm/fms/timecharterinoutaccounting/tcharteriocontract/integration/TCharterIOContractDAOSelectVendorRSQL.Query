<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOContractDAOSelectVendorRSQL">
			<desc><![CDATA[TCharterIOContractDAOSelectVendorRSQL]]></desc>
			<sql><![CDATA[
select 
	mv.vndr_lgl_eng_nm,
	fo.ownr_nm
  from mdm_vendor mv, fms_owner fo
 where mv.vndr_seq = @[vndr_seq]
   and mv.fms_ownr_seq = fo.ownr_seq			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
