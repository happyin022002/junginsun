<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOsearchExceptionMtyReturnCYRSQL">
			<desc><![CDATA[searchExceptionMtyReturnCY]]></desc>
			<sql><![CDATA[
SELECT MTY_PKUP_RTN_YD_CD
FROM PRD_PKUP_RTN_YD
WHERE 1=1
AND  POR_DEL_CD = @[loc_cd]
AND  POL_POD_CD = @[f_pol_pod_cd]
AND  IO_BND_CD = 'O'
AND  SPCL_CGO_CD IN (${f_spcl_cgo_cd})
AND  VSL_SLAN_CD = 'ALL'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="f_pol_pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
