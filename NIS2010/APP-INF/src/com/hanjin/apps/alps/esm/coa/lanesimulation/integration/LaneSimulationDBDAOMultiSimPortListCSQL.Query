<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOMultiSimPortListCSQL">
			<desc><![CDATA[터미널 insert]]></desc>
			<sql><![CDATA[
INSERT INTO COA_SIM_TML_INFO( 
	   SIM_DT 
	  ,SIM_NO 
      ,SKD_DIR_CD 
      ,TML_CD 
      ,VSL_DBL_CALL_SEQ
	  ,MNVR_IN_HRS 
	  ,MNVR_OUT_HRS
	  ,LNK_DIST
      ,PORT_BUF_HRS  
      ,CRN_KNT
      ,TML_PROD_QTY
      ,LNK_SPD
	  ,TZTM_HRS
      ,PORT_SEQ 
      ,PORT_DYS 
      ,SEA_DYS 
	  ,TURN_PORT_IND_CD
	  ,OB_OCN_CGO_QTY
      ,BZC_VSL_SPD
	  ,PORT_USD_AMT
      ,CRE_USR_ID 
      ,CRE_DT 
      ,UPD_USR_ID 
      ,UPD_DT
      ,ETB_DY_NO
      ,ETB_DY_CD
      ,ETB_TM_HRMNT
      ,ETD_DY_NO
      ,ETD_DY_CD
      ,ETD_TM_HRMNT
      ,TURN_PORT_FLG
      ,CALL_YD_IND_SEQ
      ,SEA_BUF_SPD
	  ,ACT_WRK_HRS
	  ,SEA_BUF_HRS
      ,IB_IPCGO_QTY
      ,IB_OCN_CGO_QTY
      ,OB_IPCGO_QTY) 
  VALUES(
	   @[sim_dt] 
	  ,@[sim_no] 
      ,@[skd_dir_cd] 
	  ,@[tml_cd] 
	  ,(SELECT TO_CHAR(COUNT(TML_CD) + 1) 
		FROM COA_SIM_TML_INFO
		WHERE SIM_DT = @[sim_dt]
		AND SIM_NO = @[sim_no]
		AND TML_CD = @[tml_cd]
		AND SKD_DIR_CD = @[skd_dir_cd])
	  ,@[mnvr_in_hrs]
      ,@[mnvr_out_hrs]
      ,@[lnk_dist]
      ,@[port_buf_hrs]
      ,@[crn_knt]
      ,@[tml_prod_qty]
      ,@[lnk_spd]
      ,@[tztm_hrs]
	  ,@[port_seq] 
	  ,@[port_dys] 
	  ,@[sea_dys] 
	  ,@[turn_port_ind_cd] 
      ,@[ob_ocn_cgo_qty]
	  ,@[bzc_vsl_spd]
	  ,@[port_usd_amt]
	  ,@[cre_usr_id] 
	  ,SYSDATE 
	  ,@[upd_usr_id] 
	  ,SYSDATE
	  ,@[etb_dy_no]
      ,@[etb_dy_cd]
      ,@[etb_tm_hrmnt]
      ,@[etd_dy_no]
      ,@[etd_dy_cd]
      ,@[etd_tm_hrmnt]
      ,@[turn_port_flg]
      ,@[call_yd_ind_seq]
      ,@[sea_buf_spd]
      ,@[act_wrk_hrs]
	  ,@[sea_buf_hrs]
      ,@[ib_ipcgo_qty]
      ,@[ib_ocn_cgo_qty]
      ,@[ob_ipcgo_qty])			]]></sql>
			<params>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="tml_cd" type="12" value="" out="N"/>
				<param name="mnvr_in_hrs" type="12" value="" out="N"/>
				<param name="mnvr_out_hrs" type="12" value="" out="N"/>
				<param name="lnk_dist" type="12" value="" out="N"/>
				<param name="port_buf_hrs" type="12" value="" out="N"/>
				<param name="crn_knt" type="12" value="" out="N"/>
				<param name="tml_prod_qty" type="12" value="" out="N"/>
				<param name="lnk_spd" type="12" value="" out="N"/>
				<param name="tztm_hrs" type="12" value="" out="N"/>
				<param name="port_seq" type="12" value="" out="N"/>
				<param name="port_dys" type="12" value="" out="N"/>
				<param name="sea_dys" type="12" value="" out="N"/>
				<param name="turn_port_ind_cd" type="12" value="" out="N"/>
				<param name="ob_ocn_cgo_qty" type="12" value="" out="N"/>
				<param name="bzc_vsl_spd" type="12" value="" out="N"/>
				<param name="port_usd_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="etb_dy_no" type="12" value="" out="N"/>
				<param name="etb_dy_cd" type="12" value="" out="N"/>
				<param name="etb_tm_hrmnt" type="12" value="" out="N"/>
				<param name="etd_dy_no" type="12" value="" out="N"/>
				<param name="etd_dy_cd" type="12" value="" out="N"/>
				<param name="etd_tm_hrmnt" type="12" value="" out="N"/>
				<param name="turn_port_flg" type="12" value="" out="N"/>
				<param name="call_yd_ind_seq" type="12" value="" out="N"/>
				<param name="sea_buf_spd" type="12" value="" out="N"/>
				<param name="act_wrk_hrs" type="12" value="" out="N"/>
				<param name="sea_buf_hrs" type="12" value="" out="N"/>
				<param name="ib_ipcgo_qty" type="12" value="" out="N"/>
				<param name="ib_ocn_cgo_qty" type="12" value="" out="N"/>
				<param name="ob_ipcgo_qty" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
