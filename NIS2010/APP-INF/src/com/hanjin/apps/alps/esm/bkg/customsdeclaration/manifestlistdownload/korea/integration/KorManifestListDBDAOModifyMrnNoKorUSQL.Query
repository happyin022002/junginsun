<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOModifyMrnNoKorUSQL">
			<desc><![CDATA[searchMrnNoInKorCstm 오퍼레이션에서 조회한 MRN No와 parameter값 MRN No가 틀릴 경우 MRN No를 update한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_KR_VVD_SMRY
   SET MRN_NO = @[new_mrn_no] 
     , MRN_CHK_NO = @[new_mrn_chk_no]
 WHERE MRN_NO = @[old_mrn_no]
   AND MRN_CHK_NO = @[old_mrn_chk_no]
   AND VSL_CD = SUBSTR(@[vvd_cd],1,4)
   AND SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
   AND SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
   AND PORT_CD = @[mrn_port]
   AND IO_BND_CD = @[bound]			]]></sql>
			<params>
				<param name="new_mrn_no" type="12" value="" out="N"/>
				<param name="new_mrn_chk_no" type="12" value="" out="N"/>
				<param name="old_mrn_no" type="12" value="" out="N"/>
				<param name="old_mrn_chk_no" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="mrn_port" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
