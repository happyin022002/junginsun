<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BrcsManifestDownloadDBDAOmodifyHSCodeUSQL">
			<desc><![CDATA[HS CODE 를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_CMDT
SET CMDT_DESC = @[cmdt_desc]
	, CMDT_CATE_CTNT = @[cmdt_cate_ctnt]
	, UPD_USR_ID = @[upd_usr_id]
	, UPD_DT = SYSDATE
WHERE CNT_CD = 'BR'
AND MF_CMDT_CD = @[brz_cmdt_cd]			]]></sql>
			<params>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="cmdt_cate_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="brz_cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
