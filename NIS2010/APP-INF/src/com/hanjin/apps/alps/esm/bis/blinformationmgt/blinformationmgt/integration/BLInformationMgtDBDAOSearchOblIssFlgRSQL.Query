<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLInformationMgtDBDAOSearchOblIssFlgRSQL">
			<desc><![CDATA[SearchOblIssFlg]]></desc>
			<sql><![CDATA[
SELECT obl_iss_flg
  FROM BIS_BL_ISS
 WHERE 1=1
   AND BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
