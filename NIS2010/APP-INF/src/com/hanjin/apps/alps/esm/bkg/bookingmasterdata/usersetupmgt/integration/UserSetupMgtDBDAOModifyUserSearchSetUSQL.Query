<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOModifyUserSearchSetUSQL">
			<desc><![CDATA[UserSetupMgtDBDAOModifyUserSearchSetUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_CUST_SRCH_SET
SET CUST_CNT_CD = @[cust_cnt_cd],
    CUST_SEQ = @[cust_seq],
    SET_SLCT_FLG = DECODE(@[set_slct_flg],0,'N','Y'),
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT = SYSDATE
WHERE USR_ID = @[usr_id]
  AND SET_SUB_SEQ = @[set_sub_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="set_slct_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="set_sub_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
