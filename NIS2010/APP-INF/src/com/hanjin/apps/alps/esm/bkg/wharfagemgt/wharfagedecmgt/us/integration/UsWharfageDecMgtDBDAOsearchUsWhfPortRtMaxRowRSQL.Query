<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOsearchUsWhfPortRtMaxRowRSQL">
			<desc><![CDATA[searchUsWhfPortRtMaxRow]]></desc>
			<sql><![CDATA[
SELECT TB.EFF_DT, TB.WHF_DC_RT
  FROM (
        SELECT NVL(TO_CHAR(EFF_DT,'YYYYMMDD'), TO_CHAR(SYSDATE, 'YYYYMMDD')) AS EFF_DT
              ,NVL(WHF_DC_RT, 0) AS WHF_DC_RT
              ,ROW_NUMBER() OVER(ORDER BY EFF_DT DESC) AS RNUM
          FROM BKG_USA_WHF_RT
         WHERE PORT_CD = @[port]
           AND IO_BND_CD = @[bound]
       ) TB
 WHERE TB.RNUM=1			]]></sql>
			<params>
				<param name="port" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
