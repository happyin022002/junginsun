<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchAlocStsCdByBkgNoRSQL">
			<desc><![CDATA[ALOC_STS_CD 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT ALOC_STS_CD 
     , ALOC_SVC_CD
     , BKG_ALOC_TP_CD
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
