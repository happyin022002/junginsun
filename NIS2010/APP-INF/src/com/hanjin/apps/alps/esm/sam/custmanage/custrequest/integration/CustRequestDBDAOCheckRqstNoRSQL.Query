<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustRequestDBDAOCheckRqstNoRSQL">
			<desc><![CDATA[Customer request no check]]></desc>
			<sql><![CDATA[
SELECT A.MDM_CUSTOMER_RQST_SEQ 
FROM MDM_CUSTOMER_RQST A 
WHERE 1=1 
AND A.MDM_CUSTOMER_RQST_SEQ = @[rqst_no]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
