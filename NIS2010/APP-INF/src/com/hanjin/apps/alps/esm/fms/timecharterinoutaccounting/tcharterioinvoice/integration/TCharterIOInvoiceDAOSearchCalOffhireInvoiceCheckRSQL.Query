<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOSearchCalOffhireInvoiceCheckRSQL">
			<desc><![CDATA[TCharterIOInvoiceDAOSearchCalOffhireInvoiceCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT *
FROM  FMS_INVOICE FI
WHERE  FI.FLET_CTRT_NO = @[flet_ctrt_no]
AND  FI.FLET_ISS_TP_CD = 'OFF'
AND (   TO_DATE(@[ori_eff_dt],'YYYYMMDDHH24MI') BETWEEN  FI.EFF_DT AND FI.EXP_DT - 0.00001
	OR  TO_DATE(@[ori_exp_dt],'YYYYMMDDHH24MI') BETWEEN  FI.EFF_DT AND FI.EXP_DT - 0.00001
)			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="ori_eff_dt" type="12" value="" out="N"/>
				<param name="ori_exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
