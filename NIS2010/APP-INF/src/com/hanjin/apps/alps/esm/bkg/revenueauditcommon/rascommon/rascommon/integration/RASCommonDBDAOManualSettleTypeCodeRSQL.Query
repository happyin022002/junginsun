<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RASCommonDBDAOManualSettleTypeCodeRSQL">
			<desc><![CDATA[Manual Settle Type Code를 조회한다]]></desc>
			<sql><![CDATA[
SELECT MNL_STL_TP_CD CD, 
       MNL_STL_TP_DESC NM, 
       MNL_STL_TP_CD||'\t'||MNL_STL_TP_DESC||'\t'||(SELECT INTG_CD_VAL_DP_DESC 
        FROM COM_INTG_CD_DTL 
        WHERE INTG_CD_ID = 'CD03476'
        AND INTG_CD_VAL_CTNT = MNL_STL_CATE_CD) ETC2, 
       (SELECT INTG_CD_VAL_DP_DESC 
        FROM COM_INTG_CD_DTL 
        WHERE INTG_CD_ID = 'CD03476'
        AND INTG_CD_VAL_CTNT = MNL_STL_CATE_CD) ETC1
FROM BKG_REV_UMCH_MNL_STL_TP
ORDER BY MNL_STL_TP_DP_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
