<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOHJSSalesUnitCostRSQL">
			<desc><![CDATA[HJSSalesUnitCost
2015.06.04 김시몬 단가생성시 BSA 의 FINAL SML CAPA로 생성하도록 변경]]></desc>
			<sql><![CDATA[
SELECT
       A.COST_YRMON                              AS COST_YRMON,
       A.COST_WK                                 AS COST_WK,
       A.TRD_CD                                  AS TRD_CD,
       A.RLANE_CD                                AS RLANE_CD,
       A.VSL_CD                                  AS VSL_CD,
       A.SKD_VOY_NO                              AS SKD_VOY_NO,
       A.DIR_CD                                  AS DIR_CD,
       (SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03217' AND INTG_CD_VAL_CTNT = D.HUL_BND_CD) HUL_BND_CD,
       B.VOP_CD                                  AS VOP_CD,
       NVL(V.VSL_OSHP_CD,'OTH')                  AS VSL_OSHP_CD,
       B.VSL_CAPA                                AS VSL_CAPA,
       B.BSA_CAPA                                AS BSA_CAPA,
       B.BSA_OP_CD                               AS BSA_OP_CD,
       DECODE(B.BSA_OP_CD,'J','J/O','S','S/C')   AS BSA_OP_NM,
              
       C.HJS_SLS_UC_QTY                          AS HJS_SLS_UC_QTY,
       C.FNL_HJS_BSA_CAPA                        AS FNL_HJS_BSA_CAPA,
       C.HJS_SLS_AMT                             AS HJS_SLS_AMT,
       
       C.AMT_1_01                                AS AMT_1_01, --Port Expense
       C.AMT_1_02                                AS AMT_1_02, --Canal Transit Fee
       C.AMT_1_03                                AS AMT_1_03, --Bunker
       C.AMT_1_04                                AS AMT_1_04, --Crew Expense
       C.AMT_1_05                                AS AMT_1_05, --Insurance
       C.AMT_1_06                                AS AMT_1_06, --Lubricant Expense
       C.AMT_1_07                                AS AMT_1_07, --Store Supply Expense
       C.AMT_1_08                                AS AMT_1_08, --Vessel M&R
       C.AMT_1_09                                AS AMT_1_09, --Depreciations
       C.AMT_1_10                                AS AMT_1_10, --Telecom Expense
       C.AMT_1_11                                AS AMT_1_11, --Other Operation Fixed Exp
       C.AMT_1_12                                AS AMT_1_12, --Time Charterage
       C.AMT_1_13                                AS AMT_1_13, --Space Charterage
       C.AMT_1_14                                AS AMT_1_14, --VSL Interest
    
       C.AMT_2_01                                AS AMT_2_01, 
       C.AMT_2_02                                AS AMT_2_02,
       C.AMT_2_03                                AS AMT_2_03,
       C.AMT_2_04                                AS AMT_2_04,
       C.AMT_2_05                                AS AMT_2_05,
       C.AMT_2_06                                AS AMT_2_06,
       C.AMT_2_07                                AS AMT_2_07,
       C.AMT_2_08                                AS AMT_2_08,
       C.AMT_2_09                                AS AMT_2_09,
       C.AMT_2_10                                AS AMT_2_10,
       C.AMT_2_11                                AS AMT_2_11,
       C.AMT_2_12                                AS AMT_2_12,
       C.AMT_2_13                                AS AMT_2_13,
       C.AMT_2_14                                AS AMT_2_14
    
  FROM MAS_MON_VVD A,
       BSA_VVD_MST B,
       (SELECT
               A1.TRD_CD     ,
               A1.RLANE_CD   ,
               A1.IOC_CD     ,
               A1.VSL_CD     ,
               A1.SKD_VOY_NO ,
               A1.DIR_CD     ,
			   NVL(MAX(HJS_SLS_UC_QTY),0)                                         AS HJS_SLS_UC_QTY,
			   NVL(MAX(FNL_HJS_BSA_CAPA),0)                                       AS FNL_HJS_BSA_CAPA,
			   NVL(SUM(HJS_SLS_AMT),0)                                            AS HJS_SLS_AMT,
 			 
               NVL(SUM(DECODE(A1.STND_COST_CD,'53101000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_01, --Port Expense
               NVL(SUM(DECODE(A1.STND_COST_CD,'53102000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_02, --Canal Transit Fee
               NVL(SUM(DECODE(A1.STND_COST_CD,'53200000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_03, --Bunker
               NVL(SUM(DECODE(A1.STND_COST_CD,'54100000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_04, --Crew Expense
               NVL(SUM(DECODE(A1.STND_COST_CD,'54250000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_05, --Insurance
               NVL(SUM(DECODE(A1.STND_COST_CD,'54300000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_06, --Lubricant Expense
               NVL(SUM(DECODE(A1.STND_COST_CD,'54200000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_07, --Store Supply Expense
               NVL(SUM(DECODE(A1.STND_COST_CD,'54150000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_08, --Vessel M&R
               NVL(SUM(DECODE(A1.STND_COST_CD,'54450000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_09, --Depreciations
               NVL(SUM(DECODE(A1.STND_COST_CD,'54180000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_10, --Telecom Expense
               NVL(SUM(DECODE(A1.STND_COST_CD,'54550000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_11, --Other Operation Fixed Exp
               NVL(SUM(DECODE(A1.STND_COST_CD,'54350000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_12, --Time Charterage
               NVL(SUM(DECODE(A1.STND_COST_CD,'54400000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_13, --Space Charterage
               NVL(SUM(DECODE(A1.STND_COST_CD,'72100000',A1.HJS_SLS_AMT,0)),0)    AS AMT_1_14, --Interest
             
               NVL(SUM(DECODE(A1.STND_COST_CD,'53101000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_01,
               NVL(SUM(DECODE(A1.STND_COST_CD,'53102000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_02,
               NVL(SUM(DECODE(A1.STND_COST_CD,'53200000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_03,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54100000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_04,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54250000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_05,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54300000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_06,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54200000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_07,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54150000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_08,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54450000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_09,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54180000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_10,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54550000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_11,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54350000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_12,
               NVL(SUM(DECODE(A1.STND_COST_CD,'54400000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_13,
               NVL(SUM(DECODE(A1.STND_COST_CD,'72100000',A1.HJS_SLS_UC_AMT,0)),0) AS AMT_2_14             
          FROM MAS_VVD_HIR A1
         GROUP BY A1.TRD_CD,
                  A1.RLANE_CD,
                  A1.IOC_CD,
                  A1.VSL_CD,
                  A1.SKD_VOY_NO,
                  A1.DIR_CD
       ) C,
       MAS_VSL_RGST V,
	   (SELECT TRD_CD, RLANE_CD, IOC_CD, DIR_CD, HUL_BND_CD FROM MAS_LANE_RGST) D
 WHERE A.TRD_CD          = B.TRD_CD
   AND A.RLANE_CD        = B.RLANE_CD
   AND A.IOC_CD          = B.IOC_CD
   AND A.VSL_CD          = B.VSL_CD
   AND A.SKD_VOY_NO      = B.SKD_VOY_NO
   AND A.DIR_CD          = B.SKD_DIR_CD
   AND A.TRD_CD          = C.TRD_CD
   AND A.RLANE_CD        = C.RLANE_CD
   AND A.IOC_CD          = C.IOC_CD
   AND A.VSL_CD          = C.VSL_CD
   AND A.SKD_VOY_NO      = C.SKD_VOY_NO
   AND A.DIR_CD          = C.DIR_CD
   AND NVL(A.DELT_FLG,'N') = 'N'
   AND A.VSL_CD          = V.VSL_CD(+)
   AND V.LST_FLG(+)      = 'Y'
   AND V.DELT_FLG(+)     = 'N'
   AND A.TRD_CD          = D.TRD_CD
   AND A.RLANE_CD        = D.RLANE_CD
   AND A.IOC_CD          = D.IOC_CD
   AND A.DIR_CD          = D.DIR_CD
   AND A.SLS_YRMON LIKE @[f_year]||'%'
   AND A.COST_WK BETWEEN @[f_fm_wk] AND @[f_to_wk]
#if (${f_seltrade} != '')
   AND A.TRD_CD = @[f_seltrade]
#end
#if (${f_selrlane} != '')
   AND A.RLANE_CD = @[f_selrlane]
#end
#if (${f_selioc} != '')
   AND A.IOC_CD = @[f_selioc]
#end
#if (${f_vsl_cd} != '')
   AND A.VSL_CD = @[f_vsl_cd]
#end
#if (${f_skd_voy_no} != '')
   AND A.SKD_VOY_NO = @[f_skd_voy_no]
#end
#if (${f_dir_cd} != '')
   AND A.DIR_CD = @[f_dir_cd]
#end			]]></sql>
			<params>
				<param name="f_year" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
				<param name="f_seltrade" type="12" value="" out="N"/>
				<param name="f_selrlane" type="12" value="" out="N"/>
				<param name="f_selioc" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
				<param name="f_skd_voy_no" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
