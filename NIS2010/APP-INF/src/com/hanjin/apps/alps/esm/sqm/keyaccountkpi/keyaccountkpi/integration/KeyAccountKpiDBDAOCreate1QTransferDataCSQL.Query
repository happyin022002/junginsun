<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KeyAccountKpiDBDAOCreate1QTransferDataCSQL">
			<desc><![CDATA[Create1QTransferData]]></desc>
			<sql><![CDATA[
INSERT INTO SQM_KEY_ACCT_CFM_QTA(
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,ACCT_TGT_CD
      ,TRD_CD
      ,SUB_TRD_CD
      ,DIR_CD
      ,CONV_DIR_CD
      ,BSE_MON
      ,CUST_GRP_ID
      ,LOD_QTY
      ,RA_CMPB_AMT
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      )
SELECT 'Q' AS BSE_TP_CD
      ,A1.BSE_YR
      ,'1Q' AS BSE_QTR_CD
      ,A1.ACCT_TGT_CD
      ,A1.TRD_CD
      ,A1.SUB_TRD_CD
      ,A1.DIR_CD
      ,A1.DIR_CD
      ,A1.BSE_MON
      ,A1.CUST_GRP_ID
      ,A1.LOD_QTY
      ,A1.RA_CMPB_AMT
      ,@[usr_id] AS CRE_USR_ID
      ,SYSDATE
      ,@[usr_id] AS UPD_USR_ID
      ,SYSDATE
  FROM SQM_KEY_ACCT_CFM_QTA A1
 WHERE 1=1
   AND A1.BSE_TP_CD = 'Y'
   AND A1.BSE_YR    = @[f_bse_yr]
   AND A1.BSE_MON   IN ('01', '02', '03')			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
