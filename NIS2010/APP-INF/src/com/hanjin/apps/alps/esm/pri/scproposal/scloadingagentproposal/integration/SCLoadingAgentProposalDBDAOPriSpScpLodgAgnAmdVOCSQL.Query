<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCLoadingAgentProposalDBDAOPriSpScpLodgAgnAmdVOCSQL">
			<desc><![CDATA[History---------------------------------------
2011.03.25 이행지 [CHM-201109659-01]  Loading Agent POA Attach(L/Agent section) 기능 개발요청
                         - PRI_SP_SCP_LODG_AGN.POA_ATCH_FILE_ID 컬럼 추가로 인한 쿼리 수정
2013.07.05 전윤주 [CHM-201325627] ALPS 데이터품질 - PRI validation 로직보완 - MNL_INP_FLG 입력 부분 보완]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_LODG_AGN(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    LODG_AGN_SEQ,
    CUST_CNT_CD,
    CUST_SEQ,
    MNL_INP_FLG,
    CUST_NM,
    CUST_ADDR,
    CUST_LOC_CD,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ,
	POA_ATCH_FILE_ID 
)     
SELECT
    PROP_NO         ,
    AMDT_SEQ+1      ,
    SVC_SCP_CD      ,
    LODG_AGN_SEQ    ,
    CUST_CNT_CD     ,
    CUST_SEQ        ,
    DECODE(MNL_INP_FLG, '1', 'Y', 'N'),
    CUST_NM         ,
    CUST_ADDR       ,
    CUST_LOC_CD     ,
    PRC_PROG_STS_CD ,
    SRC_INFO_CD     ,
    ACPT_USR_ID		,
    ACPT_OFC_CD		,
    ACPT_DT			,
    @[cre_usr_id]   ,
    SYSDATE         ,
    @[upd_usr_id]   ,
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ,
	POA_ATCH_FILE_ID
FROM 
    PRI_SP_SCP_LODG_AGN
WHERE
    PROP_NO     = @[prop_no]
AND AMDT_SEQ    = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
