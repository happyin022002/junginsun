<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpProgVOCSQL">
			<desc><![CDATA[PRI SP PROG 생성]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_PROG
SELECT
    @[prop_no]             		,
    @[amdt_seq]            		,
    NVL(MAX(PROP_PROG_SEQ)+1,1)	,
    @[prop_sts_cd]      		,
    @[prog_usr_id]      		,
    @[prog_ofc_cd]      		,
    SYSDATE             		,
    @[cre_usr_id]       		,
    SYSDATE             		,
    @[upd_usr_id]       		,
    SYSDATE
FROM 
	PRI_RP_PROG
WHERE
    PROP_NO    = @[prop_no]
AND AMDT_SEQ   = @[amdt_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="prop_sts_cd" type="12" value="" out="N"/>
				<param name="prog_usr_id" type="12" value="" out="N"/>
				<param name="prog_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
