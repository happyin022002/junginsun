<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOCheckFileCancelAuthRSQL">
			<desc><![CDATA[S/C File Cancel 권한 조회]]></desc>
			<sql><![CDATA[
SELECT 'Y' AS ETC1
  FROM PRI_AUTH_APRO_USR
 WHERE PRC_CTRT_TP_CD = 'S'
   AND PRC_USR_AUTH_TP_CD = 'C'
   AND AUTH_APRO_USE_FLG = 'Y'
   AND AUTH_APRO_USR_ID = @[etc2]			]]></sql>
			<params>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
