<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAOSearchVIPDeductAmountRSQL">
			<desc><![CDATA[VIP Agreement별 공제 금액 조회.]]></desc>
			<sql><![CDATA[
SELECT SUM(VIP_BKG_RT) AS VIP_BKG_RT
FROM ACM_VIP_AGMT AGMT,
  ACM_VIP_AGMT_CNTR CNTR,
  BKG_CONTAINER BKG
WHERE AGMT.CUST_GRP_ID = @[cust_grp_id]
  AND AGMT.AGMT_SEQ = @[agmt_seq]
  AND AGMT.CUST_GRP_ID = CNTR.CUST_GRP_ID
  AND AGMT.AGMT_SEQ = CNTR.AGMT_SEQ
  AND BKG.BKG_NO = @[bkg_no]
  AND BKG.CNTR_TPSZ_CD = CNTR.CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
