<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupLocationQuotationDBDAOPriSqGrpLocVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE PRI_SQ_GRP_LOC SET 
	PRC_GRP_LOC_CD = @[prc_grp_loc_cd]
,	PRC_GRP_LOC_DESC = @[prc_grp_loc_desc]
,	SRC_INFO_CD = @[src_info_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	QTTN_NO = @[qttn_no]
AND	QTTN_VER_NO = @[qttn_ver_no]
AND	GRP_LOC_SEQ = @[grp_loc_seq]
			]]></sql>
			<params>
				<param name="prc_grp_loc_cd" type="12" value="" out="N"/>
				<param name="prc_grp_loc_desc" type="12" value="" out="N"/>
				<param name="src_info_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="grp_loc_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
