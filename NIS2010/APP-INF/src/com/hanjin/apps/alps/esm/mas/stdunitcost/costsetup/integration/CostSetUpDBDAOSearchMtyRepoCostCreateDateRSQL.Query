<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostSetUpDBDAOSearchMtyRepoCostCreateDateRSQL">
			<desc><![CDATA[CostSetUpDBDAOSearchMtyRepoCostCreateDateRSQL]]></desc>
			<sql><![CDATA[
SELECT 'Creation Date : '|| TO_CHAR(UPD_DT,'YYYY/MM/DD HH24:MI:SS') mty_repo_cre_dt
FROM  COA_UT_COST_CRE_STS
WHERE COST_YRMON = @[f_cost_yrmon]
  AND COST_WK  = '00'
  AND CM_UC_CD = 'MRCD'			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
