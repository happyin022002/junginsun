<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOSearchBkgHblByRqstNoRSQL">
			<desc><![CDATA[적하신고 번호 조회]]></desc>
			<sql><![CDATA[
SELECT CNTR_MF_NO
FROM   BKG_HBL
WHERE  1 = 1
AND    BKG_NO = @[bkg_no]
AND    XTER_SI_NO = @[si_no]
AND    XTER_SI_SEQ = @[xter_rqst_seq]
AND    ROWNUM <= 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="si_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
