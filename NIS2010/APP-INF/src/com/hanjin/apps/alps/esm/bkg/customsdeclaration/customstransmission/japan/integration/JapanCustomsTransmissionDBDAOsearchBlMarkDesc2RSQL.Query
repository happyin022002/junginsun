<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOsearchBlMarkDesc2RSQL">
			<desc><![CDATA[searchBlMarkDesc2]]></desc>
			<sql><![CDATA[
SELECT 
	RPAD(SUBSTR(NVL(@[mark],'N/M'),1,140),140,' ') DIFF_RMK,
	RPAD(SUBSTR(NVL(@[desc],'N/M'),1,70),70,' ') BL_DESC
FROM 
	DUAL			]]></sql>
			<params>
				<param name="mark" type="12" value="" out="N"/>
				<param name="desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
