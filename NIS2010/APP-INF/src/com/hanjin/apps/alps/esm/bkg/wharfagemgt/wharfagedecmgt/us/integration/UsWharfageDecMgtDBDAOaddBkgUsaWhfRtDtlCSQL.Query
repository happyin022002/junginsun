<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOaddBkgUsaWhfRtDtlCSQL">
			<desc><![CDATA[addBkgUsaWhfRtDtl]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_USA_WHF_RT_DTL
USING DUAL 
        ON (
           PORT_CD = @[port_cd]
       AND IO_BND_CD = @[io_bnd_cd]
       AND TO_CHAR(EFF_DT, 'YYYYMMDD') = @[eff_dt]
       AND USA_WHF_RAT_UT_CD = @[usa_whf_rat_ut_cd]
       AND FULL_MTY_CD = @[full_mty_cd]
       AND USA_WHF_TRSP_TP_CD = @[usa_whf_trsp_tp_cd]
       AND USA_WHF_EXPT_FLG = @[usa_whf_expt_flg]
           )
WHEN NOT MATCHED THEN
	INSERT 
(
	PORT_CD
,	IO_BND_CD
,	EFF_DT
,   RT_SEQ
,   USA_WHF_RAT_UT_CD
,   FULL_MTY_CD
,   USA_WHF_TRSP_TP_CD
,   USA_WHF_EXPT_FLG
,   WHF_UT_PRC
,	CRE_USR_ID
,	UPD_USR_ID
) VALUES( 
	@[port_cd]
,	@[io_bnd_cd]
,	TO_DATE(@[eff_dt], 'YYYYMMDD')
,	(SELECT NVL(MAX(RT_SEQ), 0) + 1 
       FROM BKG_USA_WHF_RT_DTL 
      WHERE PORT_CD = @[port_cd] 
        AND IO_BND_CD = @[io_bnd_cd] 
        AND TO_CHAR(EFF_DT,'YYYYMMDD') = @[eff_dt]
    )
,	@[usa_whf_rat_ut_cd]
,	@[full_mty_cd]
,	@[usa_whf_trsp_tp_cd]
,	@[usa_whf_expt_flg]
,	@[whf_ut_prc]
,	@[upd_usr_id]
,	@[upd_usr_id]
)
WHEN MATCHED THEN
UPDATE
   SET UPD_DT = SYSDATE
      ,UPD_USR_ID = @[upd_usr_id]
      ,WHF_UT_PRC = @[whf_ut_prc]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="usa_whf_rat_ut_cd" type="12" value="" out="N"/>
				<param name="full_mty_cd" type="12" value="" out="N"/>
				<param name="usa_whf_trsp_tp_cd" type="12" value="" out="N"/>
				<param name="usa_whf_expt_flg" type="12" value="" out="N"/>
				<param name="whf_ut_prc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
