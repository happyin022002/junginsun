<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchJpcusSendLogRSQL">
			<desc><![CDATA[searchJpcusSendLog]]></desc>
			<sql><![CDATA[
#if (${jp_snd_log_id} == 'DOR' )
SELECT 
	EDI_SND_MSG_CTNT EDI_SND_MSG
FROM BKG_IB_EDI_SND_LOG DOR, BKG_BOOKING BKG
WHERE	FLT_FILE_REF_NO = 'DOR'
AND	EDI_EVNT_DT = TO_DATE(@[msg_snd_dt],'YYYYMMDDHH24MISS')
AND	EDI_EVNT_OFC_CD = @[ofc_cd]
AND	LOG_SEQ = @[log_seq]
AND BKG.BL_NO = @[bl_no]
AND DOR.BKG_NO = BKG.BKG_NO
AND	EDI_SND_SEQ = @[log_seq]
#else
SELECT *
FROM (
SELECT 
	SUB_SEQ+2 SUB_SEQ
	,	EDI_SND_MSG,
	ROW_NUMBER() OVER(ORDER BY SUB_SEQ) AS RN,
	COUNT(*) OVER() AS TOTAL
FROM BKG_CSTMS_JP_SND_LOG_DTL
WHERE	JP_SND_LOG_ID = @[jp_snd_log_id]
AND	MSG_SND_DT = TO_DATE(@[msg_snd_dt],'YYYYMMDDHH24MISS')
AND	OFC_CD = @[ofc_cd]
AND	LOG_SEQ = @[log_seq]
) 
WHERE RN BETWEEN @[startno] AND @[endno]
#end			]]></sql>
			<params>
				<param name="msg_snd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="log_seq" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="jp_snd_log_id" type="12" value="" out="N"/>
				<param name="startno" type="12" value="" out="N"/>
				<param name="endno" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
