<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndManifestListDownloadDBDAOsearchBkgBlMkDescRSQL">
			<desc><![CDATA[searchBkgBlMkDesc]]></desc>
			<sql><![CDATA[
SELECT  'CA' 		AS CNT_CD
       ,@[bl_no]    AS BL_NO
       ,MK_SEQ		AS MK_DESC_SEQ
       ,MK_DESC		AS BL_MK_DESC
       ,CMDT_DESC	AS BL_GDS_DESC
       ,@[upd_usr_id]     AS UPD_USR_ID
  FROM  BKG_BL_MK_DESC
 WHERE  BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
