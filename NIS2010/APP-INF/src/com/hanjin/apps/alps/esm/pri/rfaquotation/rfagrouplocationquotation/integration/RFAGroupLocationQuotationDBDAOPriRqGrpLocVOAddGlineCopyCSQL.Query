<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupLocationQuotationDBDAOPriRqGrpLocVOAddGlineCopyCSQL">
			<desc><![CDATA[Gline Copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RQ_GRP_LOC (
	QTTN_NO
,	QTTN_VER_NO
,	GRP_LOC_SEQ
,	PRC_GRP_LOC_CD
,	PRC_GRP_LOC_DESC
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	ORG_DEST_TP_CD
)
SELECT  @[qttn_no]
,       @[qttn_ver_no]
,       GRP_LOC_SEQ
,       PRC_GRP_LOC_CD
,       PRC_GRP_LOC_DESC
,       'GC'
,       @[cre_usr_id]
,       SYSDATE
,       @[upd_usr_id]
,       SYSDATE
,		ORG_DEST_TP_CD
FROM    PRI_RG_GRP_LOC
WHERE   1=1
AND    (SVC_SCP_CD, GLINE_SEQ) =  ( 
									SELECT  SVC_SCP_CD
                                    ,       GLINE_SEQ
                                      FROM (
                                            SELECT SVC_SCP_CD
                                                 , GLINE_SEQ
                                                 , ROW_NUMBER() OVER (ORDER BY EFF_DT DESC) AS SEQ
                                             FROM  PRI_RG_MN
                                             WHERE SVC_SCP_CD = @[svc_scp_cd]
                                             AND   CFM_FLG = 'Y'
                                             AND   TO_CHAR(EFF_DT,'YYYY-MM-DD') <= @[eff_dt]
                                             AND   TO_CHAR(EXP_DT,'YYYY-MM-DD') >= @[eff_dt]
                                            )
                                      WHERE SEQ = 1
                                   )			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
