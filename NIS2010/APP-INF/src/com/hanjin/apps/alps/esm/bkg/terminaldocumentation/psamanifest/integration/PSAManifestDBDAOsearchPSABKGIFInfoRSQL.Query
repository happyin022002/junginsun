<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPSABKGIFInfoRSQL">
			<desc><![CDATA[PSA Interface정보를 조회]]></desc>
			<sql><![CDATA[
SELECT BKG.BKG_NO BKG_NO
     , BKG.MTY_PKUP_YD_CD YD_CD
     , BQ.CNTR_TPSZ_CD||'-'||ltrim(TO_CHAR(NVL(BQ.OP_CNTR_QTY, 0),'990.99')) BKG_QTY
     , BQ.CNTR_TPSZ_CD CNTR_TPSZ_CD
	 , LTRIM(TO_CHAR(NVL(BQ.OP_CNTR_QTY, 0),'990.99')) CNTR_QTY
  FROM BKG_BOOKING BKG 
     , BKG_QUANTITY BQ
 WHERE BQ.BKG_NO       = BKG.BKG_NO
   AND BKG.BKG_NO      = @[bkg_no]
   AND BQ.CNTR_TPSZ_CD NOT LIKE 'Q%'
 ORDER BY BQ.CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
