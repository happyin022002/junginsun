<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiTtlPkgDescRSQL">
			<desc><![CDATA[searchDblEdiTtlPkgDesc]]></desc>
			<sql><![CDATA[
SELECT CASE
         WHEN    /*  A.TTL_PCK_DESC IS NOT NULL AND */
               (   A.PCK_CMDT_DESC IS NOT NULL 
                OR A.CNTR_CMDT_DESC IS NOT NULL 
                OR B.CMDT_DESC IS NOT NULL)
         THEN 'Y'
         ELSE 'N'
       END
FROM   BKG_BL_DOC A, BKG_BL_MK_DESC B
WHERE  A.BKG_NO = B.BKG_NO (+)
AND    A.BKG_NO = @[bkg_no]
AND    B.MK_SEQ (+) = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
