<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchRfaListRSQL">
			<desc><![CDATA[화주의 계약서상의 RFA를 조회한다.]]></desc>
			<sql><![CDATA[
with cust as (select 'S' cust_tp_cd, cust_cnt_cd, cust_seq, cust_lgl_eng_nm
         from mdm_customer
        where cust_cnt_cd     = nvl(@[s_cust_cnt_cd], 'xx')
#if (${s_cust_seq} == '' && ${s_cust_nm} == '') 
		  and cust_seq = 0 --seq, name이 둘다 없는 경우 조회에서 제외
#end  
#if (${s_cust_seq} != '')
		  and cust_seq 		  = @[s_cust_seq]
#end
#if (${s_cust_nm} != '') 
          and cust_lgl_eng_nm like decode(@[include_flag], 'Y', '%', '')||@[s_cust_nm]||'%'
#end
          and delt_flg        = 'N'
       union
       select 'C' cust_tp_cd, cust_cnt_cd, cust_seq, cust_lgl_eng_nm
         from mdm_customer
        where cust_cnt_cd     = nvl(@[c_cust_cnt_cd], 'xx')
#if (${c_cust_seq} == '' && ${c_cust_nm} == '') 
		  and cust_seq = 0 --seq, name이 둘다 없는 경우 조회에서 제외
#end  
#if (${c_cust_seq} != '')
		  and cust_seq 		  = @[c_cust_seq]
#end
#if (${c_cust_nm} != '') 
          and cust_lgl_eng_nm like decode(@[include_flag], 'Y', '%', '')||@[c_cust_nm]||'%'
#end        
          and delt_flg        = 'N'
#if (${bkg_no} != '') 
	   union 
       select cust.bkg_cust_tp_cd, cust.cust_cnt_cd, cust.cust_seq, cust.cust_nm
         from bkg_customer cust, bkg_booking bk
        where bk.bkg_no          = @[bkg_no]   
          and bk.bkg_no          = cust.bkg_no
          and bk.CUST_TO_ORD_FLG = 'Y'
          and cust.bkg_cust_tp_Cd    = 'N'
#end
          )
select distinct cust.cust_tp_cd
        , cust.cust_cnt_cd
        , cust.cust_seq
        , cust.cust_lgl_eng_nm
        , (select rfa_no from pri_rp_hdr where prop_no = ctrt_prop_no) rfa_no
        , respb_sls_ofc_cd sales_ofc
        , respb_srep_cd    sales_rep        
        , rp_scp.svc_scp_cd
        , (select svc_scp_nm from mdm_svc_scp where svc_scp_cd = rp_scp.svc_scp_cd) svc_scp_nm
  from 
    (select appl_dt
         from 
            (
#if (${bkg_no} != '') 
            select 1 rank, RT_APLY_DT appl_dt 
              from bkg_rt_his r
             where bkg_no = @[bkg_no]
			   and corr_no = 'TMP0000001'
               and rt_aply_dt is not null
			union all
			select 2 rank, RT_APLY_DT appl_dt --rate applicable
              from bkg_rate r
             where bkg_no = @[bkg_no]
               and rt_aply_dt is not null
            union all
            select 3 rank, skd.vps_etd_dt appl_dt --onboard date
              from bkg_vvd_his vvd, vsk_vsl_port_skd skd, bkg_bkg_his bk
             where bk.bkg_no          = @[bkg_no] 
			   and bk.corr_no 		  = 'TMP0000001'
			   and vvd.corr_no 		  = 'TMP0000001'
               and bk.bkg_no          = vvd.bkg_no
               and vvd.vsl_pre_pst_cd in ('S', 'T')
               and vvd.pol_cd         = bk.pol_cd
               and vvd.vsl_cd         = skd.vsl_cd
               and vvd.skd_voy_no     = skd.skd_voy_no
               and vvd.skd_dir_cd     = skd.skd_dir_cd
               and vvd.pol_cd         = skd.vps_port_cd
               and vvd.pol_CLPT_IND_SEQ = skd.CLPT_IND_SEQ
            union all
            select 4 rank, skd.vps_etd_dt appl_dt --onboard date
              from bkg_vvd vvd, vsk_vsl_port_skd skd, bkg_booking bk
             where bk.bkg_no          = @[bkg_no] 
               and bk.bkg_no          = vvd.bkg_no
               and vvd.vsl_pre_pst_cd in ('S', 'T')
               and vvd.pol_cd         = bk.pol_cd
               and vvd.vsl_cd         = skd.vsl_cd
               and vvd.skd_voy_no     = skd.skd_voy_no
               and vvd.skd_dir_cd     = skd.skd_dir_cd
               and vvd.pol_cd         = skd.vps_port_cd
               and vvd.pol_CLPT_IND_SEQ = skd.CLPT_IND_SEQ
            union all 
#end
            select 5 rank, sysdate appl_dt
              from dual)   
        where rownum = 1) appl
    , cust
#if (${por_cd} != '' && ${del_cd} != '') 
    , (SELECT DISTINCT LOC_SCOPE.SVC_SCP_CD
		 FROM (SELECT ORG.SVC_SCP_CD SVC_SCP_CD, COUNT(1) CNT
        		 FROM MDM_SVC_SCP_LMT ORG
		            , MDM_SVC_SCP_LMT DEST
			    WHERE ORG.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[por_cd])
			      AND ORG.ORG_DEST_CD 		= 'O'
			      AND ORG.SVC_SCP_IND_FLG 	= 'Y'
			      AND ORG.DELT_FLG			= 'N'
			      AND DEST.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[del_cd])
			      AND DEST.ORG_DEST_CD 		= 'D'
			      AND DEST.SVC_SCP_IND_FLG 	= 'Y'
			      AND DEST.DELT_FLG 		= 'N'
        		  AND ORG.SVC_SCP_CD = DEST.SVC_SCP_CD
		        GROUP BY ORG.SVC_SCP_CD) LOC_SCOPE,
			  (SELECT ORG.SVC_SCP_CD SVC_SCP_CD, 1 CNT
			     FROM MDM_SVC_SCP_LANE LANE 
			        , MDM_SVC_SCP_LMT ORG
			        , MDM_SVC_SCP_LMT DEST
			        , VSK_VSL_SKD SKD
			    WHERE ORG.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[por_cd])
			      AND ORG.ORG_DEST_CD 		= 'O'
			      AND ORG.SVC_SCP_IND_FLG 	= 'Y'
			      AND ORG.DELT_FLG			= 'N'
			      AND DEST.RGN_CD 			= (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[del_cd])
			      AND DEST.ORG_DEST_CD 		= 'D'
			      AND DEST.SVC_SCP_IND_FLG 	= 'Y'
			      AND DEST.DELT_FLG 		= 'N'
			      AND ORG.SVC_SCP_CD   		= DEST.SVC_SCP_CD
			      AND LANE.VSL_SLAN_CD 		= SKD.VSL_SLAN_CD
			      AND SKD.VSL_CD    		= SUBSTR(@[bkg_vvd], 1, 4)
			      AND SKD.SKD_VOY_NO 		= SUBSTR(@[bkg_vvd], 5, 4)
			      AND SKD.SKD_DIR_CD 		= SUBSTR(@[bkg_vvd], 9, 1)
			    GROUP BY ORG.SVC_SCP_CD
			    UNION 
			    SELECT '   ' SVC_SCP_CD , 1 CNT FROM DUAL) VSL_SCOPE
		WHERE ((LOC_SCOPE.CNT = 1 AND LOC_SCOPE.CNT = VSL_SCOPE.CNT)
		       OR 
		       (LOC_SCOPE.CNT > 1 AND LOC_SCOPE.SVC_SCP_CD = VSL_SCOPE.SVC_SCP_CD)
		      )
         ) scope
#end 
    , (select ctrt.ctrt_cust_cnt_cd ctrt_cust_cnt
            , ctrt.ctrt_cust_seq    ctrt_cust_seq
            , ctrt.prop_no          ctrt_prop_no
            , ctrt.amdt_seq         ctrt_amdt_seq
         from pri_rp_mn ctrt
		where (ctrt_cust_cnt_cd, ctrt_cust_seq) in (select cust_cnt_cd, cust_seq from cust)
       union
       select afil.cust_cnt_cd  ctrt_cust_cnt
            , afil.cust_seq     ctrt_cust_seq
            , afil.prop_no      ctrt_prop_no
            , afil.amdt_seq     ctrt_amdt_seq
         from pri_rp_afil afil
		where (cust_cnt_cd, cust_seq) in (select cust_cnt_cd, cust_seq from cust)
	   ) ctrt
    , pri_rp_mn main, pri_rp_scp_mn rp_scp
  where cust.cust_cnt_cd    = ctrt.ctrt_cust_cnt 
    and cust.cust_seq       = ctrt.ctrt_cust_Seq
    and main.prop_no        = ctrt.ctrt_prop_no
    and main.amdt_seq       = ctrt.ctrt_amdt_Seq
    and main.prop_no        = rp_scp.prop_no(+)
    and main.amdt_seq       = rp_scp.amdt_seq(+)
#if (${por_cd} != '' && ${del_cd} != '') 
    and rp_scp.svc_scp_cd   =  scope.SVC_SCP_CD
#end
#if (${svc_scp_cd} != '' && ${svc_scp_cd} != '*') 
	and rp_scp.svc_scp_cd   = @[svc_scp_cd]
#end
    and MAIN.eff_dt - 0.0001 < APPL.APPL_DT
    AND MAIN.exp_dt + 0.9999 > APPL.APPL_DT
    AND main.prop_sts_cd    = 'A'			]]></sql>
			<params>
				<param name="s_cust_cnt_cd" type="12" value="KR" out="N"/>
				<param name="s_cust_seq" type="2" value="" out="N"/>
				<param name="include_flag" type="12" value="" out="N"/>
				<param name="s_cust_nm" type="12" value="" out="N"/>
				<param name="c_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="c_cust_seq" type="2" value="" out="N"/>
				<param name="c_cust_nm" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="KRPUS" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="bkg_vvd" type="12" value="HJXX0001E" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
