<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchDiffBkgForYRSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
SELECT BKG.BKG_NO
  FROM BKG_BOOKING BKG
       , BKG_VVD VVD
       , BKG_BL_DOC DOC
 WHERE VVD.SLAN_CD = @[lane_cd]
   AND VVD.VSL_CD = @[vsl_cd]
   AND VVD.SKD_VOY_NO = @[skd_voy_no]
   AND VVD.SKD_DIR_CD = @[skd_dir_cd]
   AND VVD.BKG_NO = BKG.BKG_NO
   AND VVD.POL_CD = @[pol_cd]
   AND VVD.POD_CD = 'NLRTM'
   AND VVD.POD_CLPT_IND_SEQ = @[pod_clpt_ind_seq]
   AND BKG.BKG_NO =   DOC.BKG_NO
   AND BDR_FLG = DECODE(BKG_CGO_TP_CD,'F','Y','R','Y','B','Y',BDR_FLG)
   AND BKG_STS_CD <> 'X'
   AND BKG_CGO_TP_CD in ('F','P','R','B')
   AND BKG.BL_NO is not null
MINUS
SELECT RBL.BKG_NO
  FROM BKG_CSTMS_RTM_BL RBL
 WHERE RBL.VSL_CALL_REF_NO = @[crn_number]
   AND RBL.VSL_CD = @[vsl_cd]
   AND RBL.SKD_VOY_NO = @[skd_voy_no]
   AND RBL.SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="crn_number" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
