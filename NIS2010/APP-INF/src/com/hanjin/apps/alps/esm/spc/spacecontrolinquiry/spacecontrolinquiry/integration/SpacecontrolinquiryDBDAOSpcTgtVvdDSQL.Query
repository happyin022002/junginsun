<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAOSpcTgtVvdDSQL">
			<desc><![CDATA[기존에 입력된 VVD List 삭제]]></desc>
			<sql><![CDATA[
DELETE
  FROM SPC_TGT_VVD
 WHERE 1=1
#if(${trd_cd} != '')
   AND TRD_CD = @[trd_cd]
#end			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
