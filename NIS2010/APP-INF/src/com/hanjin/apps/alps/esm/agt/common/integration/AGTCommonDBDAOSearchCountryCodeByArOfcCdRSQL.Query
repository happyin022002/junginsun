<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchCountryCodeByArOfcCdRSQL">
			<desc><![CDATA[searchCountryCodeByArOfcCd]]></desc>
			<sql><![CDATA[
SELECT 
	VNDR_CNT_CD AS CODE
FROM MDM_ORGANIZATION
WHERE OFC_CD = @[code]			]]></sql>
			<params>
				<param name="code" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
