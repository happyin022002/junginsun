<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAORemoveInterfaceChargeDSQL">
			<desc><![CDATA[Exempt Request 승인받은 Interface된 Charge를 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CHG_RT
WHERE (BKG_NO, RT_SEQ) IN (SELECT BKG_NO, RT_SEQ
                           FROM BKG_CHG_RT R
                           WHERE BKG_NO = @[bkg_no]
                           AND CHG_CD = @[chg_cd]
                           AND AUTO_RAT_CD = 'I'
                           AND CRE_DT = (SELECT MAX(CRE_DT)
                                         FROM BKG_CHG_RT T
                                         WHERE T.BKG_NO = R.BKG_NO
                                         AND T.CHG_CD = R.CHG_CD)
                           AND ROWNUM = 1)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
