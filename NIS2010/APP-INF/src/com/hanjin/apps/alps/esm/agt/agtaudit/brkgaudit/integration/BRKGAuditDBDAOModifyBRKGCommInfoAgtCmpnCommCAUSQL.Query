<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BRKGAuditDBDAOModifyBRKGCommInfoAgtCmpnCommCAUSQL">
			<desc><![CDATA[ModifyBRKGCommInfoAgtCmpnCommCA]]></desc>
			<sql><![CDATA[
update agt_cmpn_comm
set COMM_PROC_STS_CD = 'CA'
   ,COMM_PROC_RSLT_RSN = 'Payment Cancel!' 
   ,upd_dt = sysdate
   ,upd_usr_id = @[upd_usr_id]
where bkg_no = @[bkg_no]
  AND COMM_PROC_STS_CD = 'AS'
  AND CMPN_SEQ = @[cmpn_seq]
  AND AR_OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cmpn_seq" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
