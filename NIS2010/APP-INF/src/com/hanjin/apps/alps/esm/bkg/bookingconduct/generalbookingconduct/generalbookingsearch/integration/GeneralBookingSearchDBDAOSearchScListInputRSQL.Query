<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchScListInputRSQL">
			<desc><![CDATA[SearchScListInput 파라메터 조회]]></desc>
			<sql><![CDATA[
SELECT BK.BKG_NO
     , BK.VSL_CD||BK.SKD_VOY_NO||BK.SKD_DIR_CD BKG_VVD
     , BK.POR_CD
     , BK.DEL_CD
     , SHPR.CUST_CNT_CD  S_CUST_CNT_CD
     , SHPR.CUST_SEQ     S_CUST_SEQ
     , CNEE.CUST_CNT_CD  C_CUST_CNT_CD
     , CNEE.CUST_SEQ     C_CUST_SEQ
#if (${ca_flg}== 'Y')
  FROM BKG_BKG_HIS BK
#else
  FROM BKG_BOOKING BK
#end
     , BKG_CUSTOMER SHPR
     , BKG_CUSTOMER CNEE
 WHERE 1=1
   AND BK.BKG_NO = SHPR.BKG_NO
   AND BK.BKG_NO = CNEE.BKG_NO
   AND SHPR.BKG_CUST_TP_CD = 'S'
   AND CNEE.BKG_CUST_TP_CD = 'C'
   AND BK.BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y')
   AND BK.CORR_NO = 'TMP0000001'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
