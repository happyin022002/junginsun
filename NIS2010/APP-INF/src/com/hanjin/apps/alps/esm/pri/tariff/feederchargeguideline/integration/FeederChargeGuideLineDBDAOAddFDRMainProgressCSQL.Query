<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAOAddFDRMainProgressCSQL">
			<desc><![CDATA[Add FDR Main Progress]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_FDR_PROG (
            SVC_SCP_CD
          , ORG_DEST_TP_CD  
          , FDR_TRF_NO
          , AMDT_SEQ
          , PROG_SEQ
          , FIC_PROP_STS_CD
          , PROG_OFC_CD
          , PROG_USR_ID
          , PROG_DT
          , CRE_USR_ID
          , CRE_DT
          , UPD_USR_ID
          , UPD_DT
          )
     VALUES (
            @[svc_scp_cd]
          , @[org_dest_tp_cd]  
          , @[fdr_trf_no]
          , @[amdt_seq]
          , ( SELECT NVL(MAX(PROG_SEQ)+1, 1)
                FROM PRI_TRF_FDR_PROG
               WHERE 1 = 1
                 AND SVC_SCP_CD = @[svc_scp_cd] 
                 AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
                 AND FDR_TRF_NO = @[fdr_trf_no] 
                 AND AMDT_SEQ   = @[amdt_seq]
            )
          , @[fic_prop_sts_cd]
          , @[prog_ofc_cd]
          , @[prog_usr_id]
          , SYSDATE
          , @[cre_usr_id]
          , SYSDATE
          , @[upd_usr_id]
          , SYSDATE
          )			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="fic_prop_sts_cd" type="12" value="" out="N"/>
				<param name="prog_ofc_cd" type="12" value="" out="N"/>
				<param name="prog_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
