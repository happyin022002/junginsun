<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchCustomsResultRSQL">
			<desc><![CDATA[UsaBlCustomsResultVO]]></desc>
			<sql><![CDATA[
SELECT 0 SEQ 
      ,BL_NO
      ,DSPO_CD
      ,SCAC_CD
      ,IBD_REF_NO
      ,CNTR_QTY
      ,ENTR_TP_NO
      ,ENTR_NO
      ,RCV_LOC_CD
      ,VVD
      ,CSTMS_BAT_NO
      ,CASE WHEN LENGTH(TRIM(CSTMS_RMK)) > 0 THEN CSTMS_RMK ELSE (SELECT TRIM(SUBSTR(X.MSG_DESC, 24))
          FROM BKG_CSTMS_ADV_RCV_LOG_DTL X , BKG_CSTMS_ADV_BL BL
          WHERE X.CNT_CD = @[cnt_cd]
             AND X.IO_BND_CD = RSLT.IO_BND_CD 
             AND X.RCV_DT = RSLT.RCV_DT 
             AND X.RCV_SEQ = RSLT.RCV_SEQ 
             AND RSLT.DSPO_CD = '2Z'      
             AND X.CNT_CD = BL.CNT_CD
             AND RSLT.BL_NO = BL.BL_NO
             AND BL.CSTMS_FILE_TP_CD = '2'
             AND SUBSTR(X.MSG_DESC, 0, 3) = 'R02' 
             AND ( SUBSTR(X.MSG_DESC, 11, 1) = ' ' OR SUBSTR(X.MSG_DESC, 11, 4) = '0000' )
            AND ROWNUM = 1
       )  END CSTMS_RMK
      ,CSTMS_CLR_CD
      ,RCV_MSG_TP_ID
      ,IO_BND_CD
      ,RCV_SEQ
      ,POL_CD
      ,POD_CD
      ,RCV_DATE
      ,RJCT_FLG
	  ,CSTMS_LOC_DIFF_FLG
  FROM (
        SELECT RS.BL_NO
              ,RS.CSTMS_SEQ
              ,RS.DSPO_CD
              ,RS.IBD_REF_NO
              ,RS.CNTR_QTY
              ,RS.ENTR_TP_NO
              ,RS.ENTR_NO
              ,RS.RCV_LOC_CD
              ,RS.VSL_CD||RS.SKD_VOY_NO||RS.SKD_DIR_CD AS VVD
              ,RS.CSTMS_BAT_NO
              ,TRIM(RS.CSTMS_RMK1) || ' ' || TRIM(RS.CSTMS_RMK2) || DECODE(RS.DSPO_CD, 'SN', ' ' || TRIM(RS.CSTMS_RMK3)) AS CSTMS_RMK
              ,RS.CSTMS_CLR_CD
              ,(CASE WHEN RS.DSPO_CD != 'RC' THEN NVL(RM.SCAC_CD, 'SMLM')
                     ELSE RM.SCAC_CD
                 END ) SCAC_CD
              ,RM.RCV_MSG_TP_ID
              ,RM.IO_BND_CD
              ,RM.RCV_SEQ
              ,RM.POL_CD
              ,RM.POD_CD
              ,TO_CHAR(RS.ARR_DT, 'YYYYMMDDHH24MISS') AS RCV_DATE
              ,DECODE(SUBSTR(RD.MSG_DESC, 1, 3), 'W01', 'Y', 'N') AS RJCT_FLG
			  ,NVL(RS.CSTMS_LOC_DIFF_FLG , 'N') AS CSTMS_LOC_DIFF_FLG
              ,RD.RCV_DT
              ,RD.RCV_MSG_DTL_SEQ
          FROM BKG_CSTMS_ADV_RSLT  RS
              ,BKG_CSTMS_ADV_RCV_LOG RM
              ,BKG_CSTMS_ADV_RCV_LOG_DTL RD
         WHERE RS.BL_NO = @[bl_no]
           AND RS.CNT_CD= @[cnt_cd]
           AND RS.CNT_CD = RM.CNT_CD(+)
           AND RM.IO_BND_CD(+) = 'I'
           AND RS.VSL_CD = RM.VSL_CD(+)
           AND RS.SKD_VOY_NO   = RM.SKD_VOY_NO(+)
           AND RS.SKD_DIR_CD   = RM.SKD_DIR_CD(+)
           AND RS.ARR_DT       = RM.RCV_DT(+)
           AND RS.CSTMS_BAT_NO = RM.CSTMS_BAT_NO(+)
           AND RM.CNT_CD       = RD.CNT_CD(+)
           AND RM.IO_BND_CD    = RD.IO_BND_CD(+)
           AND RM.RCV_DT       = RD.RCV_DT(+)
           AND RM.RCV_SEQ      = RD.RCV_SEQ(+)
           AND SUBSTR(RD.MSG_DESC(+), 1, 3) = 'ACR'
           AND RD.RCV_MSG_DTL_SEQ(+)= 1
        UNION ALL
        SELECT LOG.BL_NO
              ,0 AS CSTMS_SEQ
              ,'AR' AS DSPO_CD
              ,'' AS IBD_REF_NO
              ,0 AS CNTR_QTY
              ,'' AS ENTR_TP_NO
              ,'' AS ENTR_NO
              ,LOG.POD_CD AS RCV_LOC_CD
              ,LOG.VSL_CD || LOG.SKD_VOY_NO || LOG.SKD_DIR_CD AS VVD
              ,DTL.CSTMS_BAT_NO
              ,'' AS CSTMS_RMK
              ,'N' AS CSTMS_CLR_CD
              ,LOG.SCAC_CD
              ,LOG.RCV_MSG_TP_ID
              ,LOG.IO_BND_CD
              ,LOG.RCV_SEQ
              ,'' AS POL_CD
              ,'' AS POD_CD
              ,TO_CHAR(LOG.RCV_DT, 'YYYYMMDDHH24MISS') AS RCV_DATE
              ,DECODE(SUBSTR(DTL.MSG_DESC, 1, 3), 'W01', 'Y', 'N') AS RJCT_FLG
			  ,'N' AS CSTMS_LOC_DIFF_FLG
              ,MAX(DTL.RCV_DT) RCV_DT
              ,MAX(DTL.RCV_MSG_DTL_SEQ) RCV_MSG_DTL_SEQ
          FROM BKG_CSTMS_ADV_EDI_BL_RSPN RSPN
              ,BKG_CSTMS_ADV_RCV_LOG LOG
              ,BKG_CSTMS_ADV_RCV_LOG_DTL DTL
         WHERE 1=1
           AND RSPN.CNT_CD = LOG.CNT_CD
           AND RSPN.CRR_BAT_NO = LOG.CRR_BAT_NO
           AND LOG.RCV_MSG_TP_ID = 'AR'
           AND LOG.CNT_CD    = DTL.CNT_CD
           AND LOG.IO_BND_CD = DTL.IO_BND_CD
           AND LOG.RCV_DT    = DTL.RCV_DT
           AND LOG.RCV_SEQ   = DTL.RCV_SEQ
           AND RSPN.CNT_CD    = @[cnt_cd]
           AND RSPN.BL_NO     = @[bl_no]
           AND DTL.MSG_DESC LIKE 'W01' || @[bl_no] || '%'
        GROUP BY
               LOG.BL_NO
              ,LOG.POD_CD
              ,LOG.VSL_CD || LOG.SKD_VOY_NO || LOG.SKD_DIR_CD
              ,DTL.CSTMS_BAT_NO
              ,LOG.SCAC_CD
              ,LOG.RCV_MSG_TP_ID
              ,LOG.IO_BND_CD
              ,LOG.RCV_SEQ
              ,TO_CHAR(LOG.RCV_DT, 'YYYYMMDDHH24MISS')
              ,DECODE(SUBSTR(DTL.MSG_DESC, 1, 3), 'W01', 'Y', 'N')
         UNION ALL
        SELECT LOG.BL_NO
              ,0 AS CSTMS_SEQ
              ,'MR' AS DSPO_CD
              ,'' AS IBD_REF_NO
              ,0 AS CNTR_QTY
              ,'' AS ENTR_TP_NO
              ,'' AS ENTR_NO
              ,LOG.POD_CD AS RCV_LOC_CD
              ,LOG.VSL_CD || LOG.SKD_VOY_NO || LOG.SKD_DIR_CD AS VVD
              ,DTL.CSTMS_BAT_NO
              ,'' AS CSTMS_RMK
              ,'N' AS CSTMS_CLR_CD
              ,LOG.SCAC_CD
              ,LOG.RCV_MSG_TP_ID
              ,LOG.IO_BND_CD
              ,LOG.RCV_SEQ
              ,'' AS POL_CD
              ,'' AS POD_CD
              ,TO_CHAR(LOG.RCV_DT, 'YYYYMMDDHH24MISS') AS RCV_DATE
              ,DECODE(SUBSTR(DTL.MSG_DESC, 1, 3), 'W01', 'Y', 'N') AS RJCT_FLG
			  ,'N' AS CSTMS_LOC_DIFF_FLG
              ,MAX(DTL.RCV_DT) RCV_DT
              ,MAX(DTL.RCV_MSG_DTL_SEQ) RCV_MSG_DTL_SEQ
          FROM BKG_CSTMS_ADV_EDI_BL_RSPN RSPN
              ,BKG_CSTMS_ADV_RCV_LOG LOG
              ,BKG_CSTMS_ADV_RCV_LOG_DTL DTL
         WHERE 1=1
           AND RSPN.CNT_CD = LOG.CNT_CD
           AND RSPN.CRR_BAT_NO = LOG.CRR_BAT_NO
           AND LOG.RCV_MSG_TP_ID = 'MR'
           AND LOG.CNT_CD    = DTL.CNT_CD
           AND LOG.IO_BND_CD = DTL.IO_BND_CD
           AND LOG.RCV_DT    = DTL.RCV_DT
           AND LOG.RCV_SEQ   = DTL.RCV_SEQ
           AND RSPN.CNT_CD    = @[cnt_cd]
           AND RSPN.BL_NO     = @[bl_no]
           AND DTL.MSG_DESC LIKE 'W01%'
        GROUP BY
               LOG.BL_NO
               ,LOG.POD_CD
               ,LOG.VSL_CD || LOG.SKD_VOY_NO || LOG.SKD_DIR_CD
               ,DTL.CSTMS_BAT_NO
               ,LOG.SCAC_CD
               ,LOG.RCV_MSG_TP_ID
               ,LOG.IO_BND_CD
               ,LOG.RCV_SEQ
               ,TO_CHAR(LOG.RCV_DT, 'YYYYMMDDHH24MISS')
               ,DECODE(SUBSTR(DTL.MSG_DESC, 1, 3), 'W01', 'Y', 'N')
     )RSLT
ORDER BY RSLT.RCV_DATE, RSLT.CSTMS_SEQ, RSLT.ENTR_TP_NO, RSLT.DSPO_CD			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
