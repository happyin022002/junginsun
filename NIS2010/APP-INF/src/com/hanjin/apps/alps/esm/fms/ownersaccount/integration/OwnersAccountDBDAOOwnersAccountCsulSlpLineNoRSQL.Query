<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOOwnersAccountCsulSlpLineNoRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT FCS.SLP_SEQ_NO LINE_NO
  FROM FMS_CSUL_SLP FCS
 WHERE 1=1
   AND FCS.SLP_TP_CD||FCS.SLP_FUNC_CD||FCS.SLP_OFC_CD||FCS.SLP_ISS_DT||FCS.SLP_SER_NO||FCS.SLP_SEQ_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
