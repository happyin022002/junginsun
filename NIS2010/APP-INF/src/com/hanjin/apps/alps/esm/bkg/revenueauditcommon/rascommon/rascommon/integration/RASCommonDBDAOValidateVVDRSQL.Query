<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RASCommonDBDAOValidateVVDRSQL">
			<desc><![CDATA[VVD 존재여부 확인]]></desc>
			<sql><![CDATA[
SELECT 'Y'
FROM VSK_VSL_SKD
WHERE VSL_CD = SUBSTR(@[input_text],1,4)
AND SKD_VOY_NO = SUBSTR(@[input_text],5,4)
AND SKD_DIR_CD = SUBSTR(@[input_text],9,1)			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
