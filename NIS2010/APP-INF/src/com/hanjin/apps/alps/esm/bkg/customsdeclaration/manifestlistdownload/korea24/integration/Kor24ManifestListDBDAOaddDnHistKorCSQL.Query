<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Kor24ManifestListDBDAOaddDnHistKorCSQL">
			<desc><![CDATA[Download History 추가]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_KR_DL_HIS (
    MRN_NO, MRN_CHK_NO, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, DL_SEQ,
    POL_CD,  POD_CD, OFC_CD, UPD_USR_ID, CRE_USR_ID, BL_KNT, KR_CSTMS_DL_ACT_CD, DELT_DT)
VALUES (
    SUBSTR(@[mrn_no], 1, 10), SUBSTR(@[mrn_no], 11, 1), SUBSTR(@[vvd], 1, 4), SUBSTR(@[vvd], 5, 4), SUBSTR(@[vvd], 9, 1), @[dl_seq],
    @[pol_cd], @[pod_cd], @[ofc_cd], @[user_id], @[user_id], @[del_cnt], 'D', SYSDATE)			]]></sql>
			<params>
				<param name="mrn_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="dl_seq" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="del_cnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
