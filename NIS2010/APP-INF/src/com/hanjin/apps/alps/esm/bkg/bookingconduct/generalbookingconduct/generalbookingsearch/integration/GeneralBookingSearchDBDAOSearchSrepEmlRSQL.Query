<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchSrepEmlRSQL">
			<desc><![CDATA[MDM_SLS_REP 에서 SREP_EML 을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SREP_EML 
  FROM MDM_SLS_REP
 WHERE SREP_CD = @[srep_cd]			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
