<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterCntrRSQL">
			<desc><![CDATA[searchXterCntr]]></desc>
			<sql><![CDATA[
SELECT UPPER(CNTR.CNTR_NO) CNTR_NO
        , CNTR.CNTR_NO CNTR_NO_OLD
        , CNTR.CNTR_TPSZ_CD 
        , UPPER(SEAL.XTER_CNTR_SEAL_NO) CNTR_SEAL_NO
        , CNTR.CNTR_SEQ
        , CNTR.PCK_QTY
        , CNTR.PCK_TP_CD
        , CNTR.CNTR_WGT
        , CNTR.WGT_UT_CD
        , CNTR.MEAS_QTY
        , CNTR.MEAS_UT_CD
        , CNTR.PO_NO
		, CNTR.PRT_FLG
        , CNTR.VGM_WGT
        , CNTR.VGM_WGT_UT_CD
        ,CNTR.VGM_DTMN_DT
		,CNTR.VGM_MZD_TP_CD
		,CNTR.VGM_VRFY_DT
		,CNTR.VGM_VRFY_SIG_CTNT
  FROM BKG_XTER_CNTR CNTR, BKG_XTER_CNTR_SEAL_NO SEAL
 WHERE CNTR.XTER_SNDR_ID = SEAL.XTER_SNDR_ID(+)
   AND CNTR.XTER_RQST_NO = SEAL.XTER_RQST_NO(+)
   AND CNTR.XTER_RQST_SEQ= SEAL.XTER_RQST_SEQ(+)
   AND CNTR.CNTR_NO      = SEAL.CNTR_NO      (+)
   AND 1                 = SEAL.CNTR_SEAL_SEQ(+)
   AND CNTR.CNTR_SEQ     = SEAL.CNTR_SEQ(+)
   AND CNTR.XTER_SNDR_ID = @[sender_id]
   AND CNTR.XTER_RQST_NO = @[rqst_no]
   AND CNTR.XTER_RQST_SEQ= @[rqst_seq]
ORDER BY CNTR.CNTR_SEQ			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
