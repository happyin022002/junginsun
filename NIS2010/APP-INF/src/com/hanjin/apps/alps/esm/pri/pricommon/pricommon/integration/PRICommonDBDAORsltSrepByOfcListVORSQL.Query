<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltSrepByOfcListVORSQL">
			<desc><![CDATA[PRICommonDBDAORsltSrepByOfcListVORSQL]]></desc>
			<sql><![CDATA[
SELECT
    SREP_CD CD,
    SREP_NM NM,
	SREP_EML ETC2
FROM
    MDM_SLS_REP
WHERE
    OFC_CD = @[etc1]
AND SREP_STS_CD = 'N'
ORDER BY SREP_CD			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
