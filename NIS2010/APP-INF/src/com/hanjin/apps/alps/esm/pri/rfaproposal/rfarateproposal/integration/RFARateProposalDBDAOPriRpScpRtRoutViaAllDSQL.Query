<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtRoutViaAllDSQL">
			<desc><![CDATA[Rate Route Via 를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_RP_SCP_RT_ROUT_VIA
WHERE 1=1
AND PROP_NO = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND SVC_SCP_CD = @[svc_scp_cd]
AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
AND ROUT_SEQ = @[rout_seq]
AND ORG_DEST_TP_CD = @[org_dest_tp_cd]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
