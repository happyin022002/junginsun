<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCntrRtBkgChgRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
#if (${ca_flag} == 'Y') 
SELECT T1.BKG_NO
,      T1.RT_SEQ
,      T1.DP_SEQ
,      T1.CHG_CD
,      T1.TRF_ITM_NO
,      T1.CURR_CD
,      T1.CHG_UT_AMT
,      T1.RAT_AS_QTY
,      T1.RAT_UT_CD
,      T1.CHG_AMT
,      T1.FRT_INCL_XCLD_DIV_CD
,      T1.FRT_TERM_CD
,      T1.N3PTY_RCV_OFC_CD
,      T1.N3PTY_CUST_CNT_CD
,      DECODE (T1.N3PTY_CUST_SEQ, 0, '', T1.N3PTY_CUST_SEQ) AS N3PTY_CUST_SEQ
,      T1.CGO_CATE_CD
,      T1.RCV_TERM_CD
,      T1.DE_TERM_CD
,      T1.IMDG_CLSS_CD
,      T1.INV_STS_CD
,      T1.PRN_HDN_FLG
,      T1.AUTO_RAT_CD
,      T1.APLY_XCH_RTO
,      T1.AGMT_RAT_UT_CD
,      T1.CRE_USR_ID
,      T1.UPD_USR_ID
FROM   BKG_CHG_RT_HIS T1
#if (${mst_flag} == 'Y') 
WHERE  t1.bkg_no in (SELECT a.bkg_no
FROM   bkg_booking a, bkg_bl_doc b
,      (SELECT NVL (b.mst_cvrd_bl_no, a.bl_no) mst_bl_no
        FROM   bkg_booking a, bkg_bl_doc b
        WHERE  a.bkg_no = b.bkg_no
        AND    a.bkg_no = @[bkg_no]) c
WHERE  a.bkg_no = b.bkg_no
AND    (a.bl_no = c.mst_bl_no OR b.mst_cvrd_bl_no = c.mst_bl_no))
#else 
WHERE  t1.bkg_no = @[bkg_no]
#end
AND    t1.CORR_NO = 'TMP0000001'
AND    t1.frt_incl_xcld_div_cd = 'N'
#else 
SELECT T1.BKG_NO
,      T1.RT_SEQ
,      T1.DP_SEQ
,      T1.CHG_CD
,      T1.TRF_ITM_NO
,      T1.CURR_CD
,      T1.CHG_UT_AMT
,      T1.RAT_AS_QTY
,      T1.RAT_UT_CD
,      T1.CHG_AMT
,      T1.FRT_INCL_XCLD_DIV_CD
,      T1.FRT_TERM_CD
,      T1.N3PTY_RCV_OFC_CD
,      T1.N3PTY_CUST_CNT_CD
,      DECODE (T1.N3PTY_CUST_SEQ, 0, '', T1.N3PTY_CUST_SEQ) AS N3PTY_CUST_SEQ
,      T1.CGO_CATE_CD
,      T1.RCV_TERM_CD
,      T1.DE_TERM_CD
,      T1.IMDG_CLSS_CD
,      T1.INV_STS_CD
,      T1.PRN_HDN_FLG
,      T1.AUTO_RAT_CD
,      T1.APLY_XCH_RTO
,      T1.AGMT_RAT_UT_CD
,      T1.CRE_USR_ID
,      T1.UPD_USR_ID
FROM   BKG_CHG_RT T1
#if (${mst_flag} == 'Y') 
WHERE  t1.bkg_no in (SELECT a.bkg_no
FROM   bkg_booking a, bkg_bl_doc b
,      (SELECT NVL (b.mst_cvrd_bl_no, a.bl_no) mst_bl_no
        FROM   bkg_booking a, bkg_bl_doc b
        WHERE  a.bkg_no = b.bkg_no
        AND    a.bkg_no = @[bkg_no]) c
WHERE  a.bkg_no = b.bkg_no
AND    (a.bl_no = c.mst_bl_no OR b.mst_cvrd_bl_no = c.mst_bl_no))
#else 
WHERE  t1.bkg_no = @[bkg_no]
#end
AND    t1.frt_incl_xcld_div_cd = 'N'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="SHAZSM02770" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
