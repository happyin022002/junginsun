<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CRMKeymanDBDAOPriCrmCustKmanCSQL">
			<desc><![CDATA[CRM Keyman 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_CRM_CUST_KMAN (
	CUST_CNT_CD
,	CUST_SEQ
,	CUST_KMAN_SEQ
,	N1ST_NM
,	LST_NM
,	JB_TIT_NM
,	INTL_PHN_NO
,	PHN_NO
,	FAX_NO
,	KMAN_EML
,	PRMRY_KMAN_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[cust_cnt_cd]
,	@[cust_seq]
,   NVL((SELECT /*+ INDEX_DESC(PRI_CRM_CUST_KMAN XPKPRI_CRM_CUST_KMAN) */
                CUST_KMAN_SEQ
         FROM PRI_CRM_CUST_KMAN
         WHERE CUST_CNT_CD = @[cust_cnt_cd]
         AND   CUST_SEQ = @[cust_seq]
         AND   ROWNUM = 1
        ),0)+1
,	@[n1st_nm]
,	@[lst_nm]
,	@[jb_tit_nm]
,	@[intl_phn_no]
,	@[phn_no]
,	@[fax_no]
,	@[kman_eml]
,	@[prmry_kman_flg]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="n1st_nm" type="12" value="" out="N"/>
				<param name="lst_nm" type="12" value="" out="N"/>
				<param name="jb_tit_nm" type="12" value="" out="N"/>
				<param name="intl_phn_no" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="kman_eml" type="12" value="" out="N"/>
				<param name="prmry_kman_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
