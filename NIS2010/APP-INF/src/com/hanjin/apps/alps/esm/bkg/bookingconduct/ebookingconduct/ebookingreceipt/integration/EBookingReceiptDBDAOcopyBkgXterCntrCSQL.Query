<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOcopyBkgXterCntrCSQL">
			<desc><![CDATA[copyBkgXterCntr]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_XTER_CNTR
(xter_sndr_id,  xter_rqst_no,  xter_rqst_seq
  , CNTR_NO, cNTR_TpSz_CD, 
  , pck_qty, Pck_tp_cd
  , meas_qty, meas_ut_cd
  , meas_qty, meas_ut_cd
  , soc_flg, prt_flg)
SELECT
  xter_sndr_id,  xter_rqst_no,  xter_rqst_seq+1
  , CNTR_NO, cNTR_TpSz_CD, 
  , pck_qty, Pck_tp_cd
  , meas_qty, meas_ut_cd
  , meas_qty, meas_ut_cd
  , soc_flg, prt_flg
FROM	bkg_xter_cntr
WHERE	xter_rqst_no = @[rqst_no]
AND	xter_rqst_seq    = @[rqst_seq]
AND     xter_sndr_id = @[sender_id]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
				<param name="sender_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
