<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VIPDeductAgreementDBDAOSearchMaxAgmtSeqRSQL">
			<desc><![CDATA[VIP Agreement sequance 조회.]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(AGMT_SEQ), 1) + 1
FROM ACM_VIP_AGMT
WHERE CUST_GRP_ID = @[cust_grp_id]			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
