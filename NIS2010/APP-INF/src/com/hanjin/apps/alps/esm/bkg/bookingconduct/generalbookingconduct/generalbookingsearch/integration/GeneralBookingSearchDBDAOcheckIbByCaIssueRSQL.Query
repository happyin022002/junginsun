<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOcheckIbByCaIssueRSQL">
			<desc><![CDATA[GeneralBookingSearchDBDAOcheckIbByCaIssueRSQL]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(1), 0, 'N', 'Y') AS INBOUNTNOTI
  FROM BKG_BOOKING A
      ,BKG_VVD B
WHERE A.BKG_NO = B.BKG_NO
   AND (   SUBSTR(A.DEL_CD, 1, 2) IN (SELECT ATTR_CTNT1 FROM BKG_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INBOUNTNOTI')
        OR SUBSTR(B.POD_CD, 1, 2) IN (SELECT ATTR_CTNT1 FROM BKG_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INBOUNTNOTI'))
   AND A.BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
