<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOmodifySnaccsTmlEdiStsCngFlgUSQL">
			<desc><![CDATA[SNACCS_TML_EDI_STS_CNG_FLG를 N으로 업데이트 한다]]></desc>
			<sql><![CDATA[
UPDATE BKG_TML_EDI_JP_BL
 SET SNACCS_TML_EDI_STS_CNG_FLG='N'
 WHERE BKG_NO=@[bkg_no]
 AND BKG_SKD_SEQ=@[bkg_skd_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_skd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
