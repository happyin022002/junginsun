<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOSearchJmsReportRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD AS VVD,
       BL_NO,
       POL_CD,
       POD_CD,
       DEL_CD,
       JP_SND_LOG_ID,
       'OK' AS SNT_TP,
       'Accepted' AS ACK_STS,
       TO_CHAR(SND_DT, 'YYYY-MM-DD HH24:MI') AS SND_DT,
       TO_CHAR(SYSDATE, 'YYYY-MM-DD HH24:MI') AS RCV_DT,
       CRE_USR_ID
  FROM BKG_CSTMS_ADV_JP_SND_LOG
 WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND POL_CD = @[pol_cd]
			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
