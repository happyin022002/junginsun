<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOmodifyPSAImpStsInfoUSQL">
			<desc><![CDATA[PSA Import Status를 update한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_PSA_IMP_STS
   SET POL_CD = @[pol_cd]
     , POD_CD = @[pod_cd]
     , TS_TP_CD = @[ts_tp_cd]
     , FULL_MTY_CD = @[fm_cd]
     , CNTR_WGT = @[cntr_wgt]
     , CNTR_OPR_CD = @[cop]
     , IB_SLT_OPR_CD = @[iop]
     , DCGO_FLG = @[dcgo_flg]
     , RC_FLG = @[rc_flg]
     , AWK_CGO_FLG = @[awk_cgo_flg]
     , BB_CGO_FLG = @[bb_cgo_flg]
     , RD_CGO_FLG = @[rd_cgo_flg]
     , LODG_VSL_CD = @[next_vsl_cd]
     , LODG_SKD_VOY_NO = @[next_skd_voy_no]
     , LODG_VSL_DIR_CD = @[next_skd_dir_cd]
     , OB_SLT_OPR_CD = @[oop]
     , PSA_BAT_NO = @[batch_no]
     , N1ST_POD_CD = @[port_cd]
     , UPD_DT = SYSDATE
     , UPD_USR_ID = @[user_id]
     , CNTR_SEAL_NO = SUBSTR(@[seal_no],1,20)
     , CNTR_TPSZ_CD = @[cntr_tpsz_cd]
     , VGM_WGT = @[vgm_wgt]
     , VGM_MZD_TP_CD = @[vgm_mzd_tp_cd]
     , VGM_VRFY_SIG_CTNT = REPLACE(REPLACE(REPLACE(UPPER(@[vgm_vrfy_sig_ctnt]),CHR(13)||CHR(10),' '),CHR(13),' '),CHR(10),' ') 
     , VGM_REF_NO = @[vgm_ref_no]
     , VGM_VRFY_DT = @[vgm_vrfy_dt]
 WHERE VSL_CD        = @[vsl_cd]
   AND SKD_VOY_NO    = @[skd_voy_no]
   AND SKD_DIR_CD    = @[skd_dir_cd]
   AND CNTR_NO       = @[cntr_no]
   AND BKG_NO 		 = @[bkg_no]			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="ts_tp_cd" type="12" value="" out="N"/>
				<param name="fm_cd" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="cop" type="12" value="" out="N"/>
				<param name="iop" type="12" value="" out="N"/>
				<param name="dcgo_flg" type="12" value="" out="N"/>
				<param name="rc_flg" type="12" value="" out="N"/>
				<param name="awk_cgo_flg" type="12" value="" out="N"/>
				<param name="bb_cgo_flg" type="12" value="" out="N"/>
				<param name="rd_cgo_flg" type="12" value="" out="N"/>
				<param name="next_vsl_cd" type="12" value="" out="N"/>
				<param name="next_skd_voy_no" type="12" value="" out="N"/>
				<param name="next_skd_dir_cd" type="12" value="" out="N"/>
				<param name="oop" type="12" value="" out="N"/>
				<param name="batch_no" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="seal_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="vgm_wgt" type="12" value="" out="N"/>
				<param name="vgm_mzd_tp_cd" type="12" value="" out="N"/>
				<param name="vgm_vrfy_sig_ctnt" type="12" value="" out="N"/>
				<param name="vgm_ref_no" type="12" value="" out="N"/>
				<param name="vgm_vrfy_dt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
