<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOmodifyConfirmIndUSQL">
			<desc><![CDATA[ROCS(ROTTERDAM) 세관에 신고할 대상 Manifest List를 전송하기 전에 데이터를 확인했다는 의미로 Confirm Indicator를 업데이트 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_RTM_BL SET    
CSTMS_DAT_CFM_CD = 'Y', 
IB_FILE_SEQ = @[seq],
CSTMS_DECL_USR_ID = @[user_id], 
BL_DAT_CFM_DT = TO_DATE(@[bl_dat_cfm_dt],'YYYY-MM-DD HH24:MI:SS')
WHERE  VSL_CALL_REF_NO = @[frm_crn_number]
AND    BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="seq" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="bl_dat_cfm_dt" type="12" value="" out="N"/>
				<param name="frm_crn_number" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
