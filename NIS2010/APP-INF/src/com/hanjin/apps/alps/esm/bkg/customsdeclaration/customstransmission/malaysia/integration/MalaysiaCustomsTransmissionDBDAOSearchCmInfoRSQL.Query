<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MalaysiaCustomsTransmissionDBDAOSearchCmInfoRSQL">
			<desc><![CDATA[CM Info 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
 CNTR_NO
,CNTR_MF_SEQ CM_SEQ
,PCK_QTY CM_PKG
,PCK_TP_CD CM_PKG_UNIT
,CNTR_MF_WGT CM_WGT
,WGT_UT_CD CM_WGT_UNIT
,MEAS_QTY CM_MEA
,MEAS_UT_CD CM_MEA_UNIT
,HAMO_TRF_CD HS_CODE

FROM BKG_CNTR_MF_DESC
WHERE BKG_NO = @[bkg_no]
--AND   CNTR_NO = [cntr_no]     --CNTR와 별개로 가기로 했음. SBE/임경하 수석님이 MIG확인 사항임.			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
