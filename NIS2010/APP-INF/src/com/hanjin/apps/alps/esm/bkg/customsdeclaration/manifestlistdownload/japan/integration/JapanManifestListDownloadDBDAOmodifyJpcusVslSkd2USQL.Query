<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOmodifyJpcusVslSkd2USQL">
			<desc><![CDATA[modifyJpcusVslSkd2]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_JP_VSL_SKD SET 
	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,	ETA_DT = TO_DATE(DECODE(@[eta_dt],null,null,@[eta_dt]),'YYYY-MM-DDHH24MISS')
,	ETB_DT = TO_DATE(DECODE(@[etb_dt],null,null,@[etb_dt]),'YYYY-MM-DDHH24MISS')
,	ARR_YD_ID  = @[arr_yd_cd]
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	POD_CD = @[pod_cd]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eta_dt" type="12" value="" out="N"/>
				<param name="etb_dt" type="12" value="" out="N"/>
				<param name="arr_yd_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
