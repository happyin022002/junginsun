<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOsearchPkupMtRtnCyRSQL">
			<desc><![CDATA[Pick-up Notice에 기입될 Port(POD), Rail Destination Location, DEL기준으로 Office별로 Empty Return CY코드를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
	RTN_YD_SEQ
,	POD_CD
--,	RAIL_DEST_CD
,	FNL_DEST_CD
,	MCNTR_RTN_YD_CD
,	RTN_YD_SAV_OFC_CD
,	RTN_YD_SAV_USR_ID
,	RTN_YD_SAV_DT
,	DELT_FLG
,	DELT_USR_ID
,	DELT_DT
,	PKUP_YD_ID
,	PKUP_CNTR_TP_ID
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_PKUP_CNTR_RTN_YD
WHERE	DELT_FLG = @[delt_flg]
#if (${pod_cd} != '')
AND		POD_CD = @[pod_cd]
#end

#if (${rtn_yd_sav_dt_s} != '' && ${rtn_yd_sav_dt_e} != '')
AND	RTN_YD_SAV_DT >= TO_DATE(@[rtn_yd_sav_dt_s],'YYYY-MM-DD') 
AND	RTN_YD_SAV_DT < TO_DATE(@[rtn_yd_sav_dt_e],'YYYY-MM-DD')+1
#end

#if(${pkup_yd_id} != '')
AND	PKUP_YD_ID = @[pkup_yd_id]
#end

#if(${fnl_dest_cd} != '')
AND	FNL_DEST_CD = @[fnl_dest_cd]
#end

#if(${mcntr_rtn_yd_cd} != '')
AND	MCNTR_RTN_YD_CD LIKE @[mcntr_rtn_yd_cd] || '%'
#end

#if(${rtn_yd_sav_ofc_cd} != '')
AND	RTN_YD_SAV_OFC_CD = @[rtn_yd_sav_ofc_cd]
#end			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="rtn_yd_sav_dt_s" type="12" value="" out="N"/>
				<param name="rtn_yd_sav_dt_e" type="12" value="" out="N"/>
				<param name="pkup_yd_id" type="12" value="" out="N"/>
				<param name="fnl_dest_cd" type="12" value="" out="N"/>
				<param name="mcntr_rtn_yd_cd" type="12" value="" out="N"/>
				<param name="rtn_yd_sav_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
