<sqls>
	<querys>
		<query name="TCharterIOContractDAOSearchContractTypeListByContractRSQL">
			<desc><![CDATA[TCharterIOContractDAOSearchContractTypeListByContractRSQL]]></desc>
			<sql><![CDATA[
SELECT
		INTG_CD_VAL_CTNT	CODE,
		INTG_CD_VAL_DP_DESC NAME
  FROM  COM_INTG_CD_DTL
 WHERE INTG_CD_ID = 'CD01513'
 #if(${type_flag} != '')
   AND INTG_CD_VAL_CTNT IN (#foreach($TYPE_FLAG IN ${lstTYPE_FLAG})
								#if($lstTYPE_FLAG.hasNext()) '$TYPE_FLAG', #else '$TYPE_FLAG' #end
							#end
						   )
 #end
 ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
