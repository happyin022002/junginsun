<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOmodifyReceiveLogUSQL">
			<desc><![CDATA[modifyReceiveLog]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_JP_RCV_LOG SET 
	VSL_CD = SUBSTR(@[in_vvd_cd],1,4)
,	SKD_VOY_NO = SUBSTR(@[in_vvd_cd],5,4)
,	SKD_DIR_CD = SUBSTR(@[in_vvd_cd],9,1)
,	POD_CD = @[in_pod_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	JP_MSG_TP_ID = 'MFR'
AND	SUBSTR(RCV_KEY_DAT_CTNT,21,12) IN 	(
										SELECT 
											DISTINCT BL_NO
										FROM
											BKG_CSTMS_JP_SND_LOG
										WHERE	VSL_CD = SUBSTR(@[in_vvd_cd],1,4)
										AND	SKD_VOY_NO = SUBSTR(@[in_vvd_cd],5,4)  
										AND SKD_DIR_CD = SUBSTR(@[in_vvd_cd],9,4)
										AND POD_CD = @[in_pod_cd]
										)			]]></sql>
			<params>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="in_pod_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
