<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchCaTmpRSQL">
			<desc><![CDATA[c/a tmp를 지워야 하는지 조회]]></desc>
			<sql><![CDATA[
select CORR_USR_ID
  from bkg_correction
 where corr_no = 'TMP0000001'
   and corr_gdt < GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', sysdate, 'GMT') - 1/24 --30분보다 이전 -> 1시간
   and bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
