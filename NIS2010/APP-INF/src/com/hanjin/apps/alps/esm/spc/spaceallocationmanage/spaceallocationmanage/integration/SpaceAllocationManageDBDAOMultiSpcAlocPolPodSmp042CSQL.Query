<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOMultiSpcAlocPolPodSmp042CSQL">
			<desc><![CDATA[SPC_ALOC_CUST_POL_POD 의 지정된 ibflag 값에 따라 SPC_ALOC_POL_POD DB에 반영한다.(입력)
[CHM-201322502-01] SPC 프로젝트 - 성수기 선복운영 개선을 위한 T/F추진
[CHM-201324211-01] SLS_OFC_CD 관련 로직 수정
2013.08.13 진마리아 [trouble shooting] 성수기 Alloc 저장시 비수기로 말아올리는 과정에 mnl_aloc_rmk 가 3으로 하드코딩된 것 수정]]></desc>
			<sql><![CDATA[
MERGE INTO SPC_ALOC_POL_POD T
USING (
         SELECT RLANE_CD      ,
                DIR_CD        ,
                VSL_CD        ,
                SKD_VOY_NO    ,
                SKD_DIR_CD    ,
                SLS_OFC_CD    ,
                POL_YD_CD     ,
                POD_YD_CD     ,
                TS_FLG        ,
                MNL_FLG       ,
                -- 2014.08.06 컬럼 추가
                CUST_CNT_CD   ,
                CUST_SEQ      ,
                CTRT_NO       ,
                USA_BKG_MOD_CD,
                DEST_LOC_CD   ,
                --
                REP_TRD_CD    ,
                REP_SUB_TRD_CD,
                TRD_CD        ,
                SUB_TRD_CD    ,
                IOC_CD        ,
                SLS_RHQ_CD    ,
                SLS_AQ_CD     ,
                SLS_RGN_OFC_CD,
                @[mnl_aloc_rmk] AS MNL_ALOC_RMK,
                @[upd_usr_id]  AS ALOC_USR_ID ,
                CAST(SYS_EXTRACT_UTC(TO_TIMESTAMP(@[aloc_gdt], 'YYYY/MM/DD HH24:MI:SS')) AS DATE) AS ALOC_GDT,
                SUM(ASGN_TTL_QTY)         AS ASGN_TTL_QTY         ,
                SUM(ASGN_20FT_QTY)        AS ASGN_20FT_QTY        ,
                SUM(ASGN_40FT_QTY)        AS ASGN_40FT_QTY        ,
                SUM(ASGN_40FT_HC_QTY)     AS ASGN_40FT_HC_QTY     ,
                SUM(ASGN_45FT_HC_QTY)     AS ASGN_45FT_HC_QTY     ,
                SUM(ASGN_53FT_QTY)        AS ASGN_53FT_QTY        ,
                SUM(ASGN_RF_QTY)          AS ASGN_RF_QTY          ,
                SUM(ASGN_TTL_WGT)         AS ASGN_TTL_WGT         ,
                SUM(BKG_AVAL_TTL_QTY)     AS BKG_AVAL_TTL_QTY     ,
                SUM(BKG_AVAL_20FT_QTY)    AS BKG_AVAL_20FT_QTY    ,
                SUM(BKG_AVAL_40FT_QTY)    AS BKG_AVAL_40FT_QTY    ,
                SUM(BKG_AVAL_40FT_HC_QTY) AS BKG_AVAL_40FT_HC_QTY ,
                SUM(BKG_AVAL_45FT_HC_QTY) AS BKG_AVAL_45FT_HC_QTY ,
                SUM(BKG_AVAL_53FT_QTY)    AS BKG_AVAL_53FT_QTY    ,
                SUM(BKG_AVAL_RF_QTY)      AS BKG_AVAL_RF_QTY      ,
                SUM(BKG_AVAL_TTL_WGT)     AS BKG_AVAL_TTL_WGT     ,
                SUM(FCAST_TTL_QTY)        AS FCAST_TTL_QTY        ,
                SUM(FCAST_40FT_HC_QTY)    AS FCAST_40FT_HC_QTY    ,
                SUM(FCAST_45FT_HC_QTY)    AS FCAST_45FT_HC_QTY    ,
                SUM(FCAST_53FT_QTY)       AS FCAST_53FT_QTY       ,
                SUM(FCAST_RF_QTY)         AS FCAST_RF_QTY         ,
                SUM(FCAST_TTL_WGT)        AS FCAST_TTL_WGT        ,
                SUM(USD_BKG_TTL_QTY)      AS USD_BKG_TTL_QTY      ,
                SUM(USD_BKG_20FT_QTY)     AS USD_BKG_20FT_QTY     ,
                SUM(USD_BKG_40FT_QTY)     AS USD_BKG_40FT_QTY     ,
                SUM(USD_BKG_40FT_HC_QTY)  AS USD_BKG_40FT_HC_QTY  ,
                SUM(USD_BKG_45FT_HC_QTY)  AS USD_BKG_45FT_HC_QTY  ,
                SUM(USD_BKG_53FT_QTY)     AS USD_BKG_53FT_QTY     ,
                SUM(USD_BKG_RF_QTY)       AS USD_BKG_RF_QTY       ,
                SUM(USD_BKG_TTL_WGT)      AS USD_BKG_TTL_WGT      ,
                @[spc_ctrl_aloc_rmk]      AS SPC_CTRL_ALOC_RMK    ,
                @[spc_ctrl_aloc_pol_rmk]  AS SPC_CTRL_ALOC_POL_RMK,
                @[spc_ctrl_aloc_pod_rmk]  AS SPC_CTRL_ALOC_POD_RMK,
                @[upd_usr_id]             AS CRE_USR_ID           ,
                SYSDATE                   AS CRE_DT               ,
                @[upd_usr_id]             AS UPD_USR_ID           ,
                SYSDATE                   AS UPD_DT               ,
                --2014.08.06 컬럼추가
                SUM(ASGN_20FT_DRY_QTY)    AS ASGN_20FT_DRY_QTY    ,
                SUM(ASGN_40FT_DRY_QTY)    AS ASGN_40FT_DRY_QTY    ,
                SUM(ASGN_RD_QTY)          AS ASGN_RD_QTY          ,
                SUM(BKG_AVAL_20FT_DRY_QTY) AS BKG_AVAL_20FT_DRY_QTY,
                SUM(BKG_AVAL_40FT_DRY_QTY) AS BKG_AVAL_40FT_DRY_QTY,
                SUM(BKG_AVAL_RD_QTY)       AS BKG_AVAL_RD_QTY      ,
                SUM(FCAST_20FT_DRY_QTY)   AS FCAST_20FT_DRY_QTY    ,
                SUM(FCAST_40FT_DRY_QTY)   AS FCAST_40FT_DRY_QTY    ,
                SUM(FCAST_RD_QTY)         AS FCAST_RD_QTY          ,
                SUM(USD_BKG_20FT_DRY_QTY) AS USD_BKG_20FT_DRY_QTY  ,
                SUM(USD_BKG_40FT_DRY_QTY) AS USD_BKG_40FT_DRY_QTY  ,
                SUM(USD_BKG_RD_QTY)       AS USD_BKG_RD_QTY        
           FROM SPC_ALOC_CUST_POL_POD
          WHERE RLANE_CD   = @[rlane_cd]
            AND DIR_CD     = @[dir_cd]
            AND VSL_CD     = @[vsl_cd]
            AND SKD_VOY_NO = @[skd_voy_no]
            AND SKD_DIR_CD = @[skd_dir_cd]
            AND SLS_OFC_CD = DECODE(TS_FLG, 'Y', NVL(@[sls_ofc_cd], @[sls_rhq_cd]), @[sls_ofc_cd])
            AND POL_YD_CD  = NVL(@[pol_cd], '0000000')
            AND POD_YD_CD  = NVL(@[pod_cd], '0000000')
            AND TS_FLG     = @[ts_flg]
            AND MNL_FLG    = @[mnl_flg]
            AND IOC_CD     = DECODE(@[ioc_cd], 'OCN', 'O', 'T-OCN', 'O', 'I')
            --2014.08.06 조건추가
            AND DEST_LOC_CD  = DECODE(NVL(@[del_cd], '00000'), 'OTHERS', 'XXXXX', NVL(@[del_cd], '00000')) 
            AND USA_BKG_MOD_CD = DECODE(NVL(@[us_mod], '00000'), 'OTHERS', 'OTH', NVL(@[us_mod], '00000'))
            AND CUST_CNT_CD||LPAD(CUST_SEQ, 6, '0') = DECODE(NVL(@[account_cd], '00000000'), 'OTHERS', 'XX999999', NVL(@[account_cd], '00000000'))
            AND CTRT_NO  = NVL(@[ctrt_no], '0')
       GROUP BY RLANE_CD      ,
                DIR_CD        ,
                VSL_CD        ,
                SKD_VOY_NO    ,
                SKD_DIR_CD    ,
                SLS_OFC_CD    ,
                POL_YD_CD     ,
                POD_YD_CD     ,
                TS_FLG        ,
                MNL_FLG       ,
                -- 2014.08.06 컬럼 추가
                CUST_CNT_CD   ,
                CUST_SEQ      ,
                CTRT_NO	      ,
                USA_BKG_MOD_CD,
                DEST_LOC_CD   ,
                REP_TRD_CD    ,
                REP_SUB_TRD_CD,
                TRD_CD        ,
                SUB_TRD_CD    ,
                IOC_CD        ,
                SLS_RHQ_CD    ,
                SLS_AQ_CD     ,
                SLS_RGN_OFC_CD
      ) C
   ON (
             T.RLANE_CD     = C.RLANE_CD
         AND T.DIR_CD       = C.DIR_CD
         AND T.VSL_CD       = C.VSL_CD
         AND T.SKD_VOY_NO   = C.SKD_VOY_NO
         AND T.SKD_DIR_CD   = C.SKD_DIR_CD
         AND T.SLS_OFC_CD   = C.SLS_OFC_CD
         AND T.POL_YD_CD    = C.POL_YD_CD
         AND T.POD_YD_CD    = C.POD_YD_CD
         AND T.TS_FLG       = C.TS_FLG
         AND T.MNL_FLG      = C.MNL_FLG
         AND T.IOC_CD       = C.IOC_CD
         -- 2014.08.06 컬럼 추가
         AND T.DEST_LOC_CD  = C.DEST_LOC_CD
         AND T.USA_BKG_MOD_CD  = C.USA_BKG_MOD_CD
         AND T.CUST_CNT_CD  = C.CUST_CNT_CD
         AND T.CUST_SEQ     = C.CUST_SEQ
         AND T.CTRT_NO      = C.CTRT_NO
      )
WHEN MATCHED THEN
    UPDATE 
       SET T.ASGN_TTL_QTY          = C.ASGN_TTL_QTY         ,
           T.ASGN_20FT_QTY         = C.ASGN_20FT_QTY        ,
           T.ASGN_40FT_QTY         = C.ASGN_40FT_QTY        ,
           T.ASGN_40FT_HC_QTY      = C.ASGN_40FT_HC_QTY     ,
           T.ASGN_45FT_HC_QTY      = C.ASGN_45FT_HC_QTY     ,
           T.ASGN_53FT_QTY         = C.ASGN_53FT_QTY        ,
           T.ASGN_RF_QTY           = C.ASGN_RF_QTY          ,
           T.ASGN_TTL_WGT          = C.ASGN_TTL_WGT         ,
           T.BKG_AVAL_TTL_QTY      = C.BKG_AVAL_TTL_QTY     ,
           T.BKG_AVAL_20FT_QTY     = C.BKG_AVAL_20FT_QTY    ,
           T.BKG_AVAL_40FT_QTY     = C.BKG_AVAL_40FT_QTY    ,
           T.BKG_AVAL_40FT_HC_QTY  = C.BKG_AVAL_40FT_HC_QTY ,
           T.BKG_AVAL_45FT_HC_QTY  = C.BKG_AVAL_45FT_HC_QTY ,
           T.BKG_AVAL_53FT_QTY     = C.BKG_AVAL_53FT_QTY    ,
           T.BKG_AVAL_RF_QTY       = C.BKG_AVAL_RF_QTY      ,
           T.BKG_AVAL_TTL_WGT      = C.BKG_AVAL_TTL_WGT     ,
           T.FCAST_TTL_QTY         = C.FCAST_TTL_QTY        ,
           T.FCAST_40FT_HC_QTY     = C.FCAST_40FT_HC_QTY    ,
           T.FCAST_45FT_HC_QTY     = C.FCAST_45FT_HC_QTY    ,
           T.FCAST_53FT_QTY        = C.FCAST_53FT_QTY       ,
           T.FCAST_RF_QTY          = C.FCAST_RF_QTY         ,
           T.FCAST_TTL_WGT         = C.FCAST_TTL_WGT        ,
           T.USD_BKG_TTL_QTY       = C.USD_BKG_TTL_QTY      ,
           T.USD_BKG_20FT_QTY      = C.USD_BKG_20FT_QTY     ,
           T.USD_BKG_40FT_QTY      = C.USD_BKG_40FT_QTY     ,
           T.USD_BKG_40FT_HC_QTY   = C.USD_BKG_40FT_HC_QTY  ,
           T.USD_BKG_45FT_HC_QTY   = C.USD_BKG_45FT_HC_QTY  ,
           T.USD_BKG_53FT_QTY      = C.USD_BKG_53FT_QTY     ,
           T.USD_BKG_RF_QTY        = C.USD_BKG_RF_QTY       ,
           T.USD_BKG_TTL_WGT       = C.USD_BKG_TTL_WGT      ,
           T.SPC_CTRL_ALOC_RMK     = C.SPC_CTRL_ALOC_RMK    ,
           T.SPC_CTRL_ALOC_POL_RMK = C.SPC_CTRL_ALOC_POL_RMK,
           T.SPC_CTRL_ALOC_POD_RMK = C.SPC_CTRL_ALOC_POD_RMK,
           T.ALOC_USR_ID           = C.ALOC_USR_ID          ,
           T.MNL_ALOC_RMK          = C.MNL_ALOC_RMK         ,
           T.ALOC_GDT              = C.ALOC_GDT             ,
           T.UPD_USR_ID            = C.UPD_USR_ID           ,
           T.UPD_DT                = C.UPD_DT		    ,
           --2014.08.06 컬럼추가
           T.ASGN_20FT_DRY_QTY     = C.ASGN_20FT_DRY_QTY    ,
           T.ASGN_40FT_DRY_QTY     = C.ASGN_40FT_DRY_QTY    ,
           T.ASGN_RD_QTY     	   = C.ASGN_RD_QTY          ,
           T.BKG_AVAL_20FT_DRY_QTY = C.BKG_AVAL_20FT_DRY_QTY,
           T.BKG_AVAL_40FT_DRY_QTY = C.BKG_AVAL_40FT_DRY_QTY,
           T.BKG_AVAL_RD_QTY       = C.BKG_AVAL_RD_QTY      ,
           T.FCAST_20FT_DRY_QTY    = C.FCAST_20FT_DRY_QTY   ,
           T.FCAST_40FT_DRY_QTY    = C.FCAST_40FT_DRY_QTY   ,
           T.FCAST_RD_QTY          = C.FCAST_RD_QTY         ,
           T.USD_BKG_20FT_DRY_QTY  = C.USD_BKG_20FT_DRY_QTY ,
           T.USD_BKG_40FT_DRY_QTY  = C.USD_BKG_40FT_DRY_QTY ,
           T.USD_BKG_RD_QTY        = C.USD_BKG_RD_QTY           
WHEN NOT MATCHED THEN
    INSERT (
              RLANE_CD             ,
              DIR_CD               ,
              VSL_CD               ,
              SKD_VOY_NO           ,
              SKD_DIR_CD           ,
              SLS_OFC_CD           ,
              POL_YD_CD            ,
              POD_YD_CD            ,
              TS_FLG               ,
              MNL_FLG              ,
              -- 2014.08.06 컬럼 추가
              CUST_CNT_CD          ,
              CUST_SEQ             ,
              CTRT_NO              ,
              USA_BKG_MOD_CD       ,
              DEST_LOC_CD   	   ,
              --
              REP_TRD_CD           ,
              REP_SUB_TRD_CD       ,
              TRD_CD               ,
              SUB_TRD_CD           ,
              IOC_CD               ,
              SLS_RHQ_CD           ,
              SLS_AQ_CD            ,
              SLS_RGN_OFC_CD       ,
              ASGN_TTL_QTY         ,
              ASGN_20FT_QTY        ,
              ASGN_40FT_QTY        ,
              ASGN_40FT_HC_QTY     ,
              ASGN_45FT_HC_QTY     ,
              ASGN_53FT_QTY        ,
              ASGN_RF_QTY          ,
              ASGN_TTL_WGT         ,
              BKG_AVAL_TTL_QTY     ,
              BKG_AVAL_20FT_QTY    ,
              BKG_AVAL_40FT_QTY    ,
              BKG_AVAL_40FT_HC_QTY ,
              BKG_AVAL_45FT_HC_QTY ,
              BKG_AVAL_53FT_QTY    ,
              BKG_AVAL_RF_QTY      ,
              BKG_AVAL_TTL_WGT     ,
              FCAST_TTL_QTY        ,
              FCAST_40FT_HC_QTY    ,
              FCAST_45FT_HC_QTY    ,
              FCAST_53FT_QTY       ,
              FCAST_RF_QTY         ,
              FCAST_TTL_WGT        ,
              USD_BKG_TTL_QTY      ,
              USD_BKG_20FT_QTY     ,
              USD_BKG_40FT_QTY     ,
              USD_BKG_40FT_HC_QTY  ,
              USD_BKG_45FT_HC_QTY  ,
              USD_BKG_53FT_QTY     ,
              USD_BKG_RF_QTY       ,
              USD_BKG_TTL_WGT      ,
              SPC_CTRL_ALOC_RMK    ,
              SPC_CTRL_ALOC_POL_RMK,
              SPC_CTRL_ALOC_POD_RMK,
              MNL_ALOC_RMK         ,
              ALOC_USR_ID          ,
              ALOC_GDT             ,
              CRE_USR_ID           ,
              CRE_DT               ,
              UPD_USR_ID           ,
              UPD_DT               ,
              --2014.08.06 컬럼추가
              ASGN_20FT_DRY_QTY    ,
              ASGN_40FT_DRY_QTY    ,
              ASGN_RD_QTY          ,
              BKG_AVAL_20FT_DRY_QTY,
              BKG_AVAL_40FT_DRY_QTY,
              BKG_AVAL_RD_QTY      ,
              FCAST_20FT_DRY_QTY   ,
              FCAST_40FT_DRY_QTY   ,
              FCAST_RD_QTY         ,
              USD_BKG_20FT_DRY_QTY ,
              USD_BKG_40FT_DRY_QTY ,
              USD_BKG_RD_QTY        
           )
    VALUES (
              C.RLANE_CD             ,
              C.DIR_CD               ,
              C.VSL_CD               ,
              C.SKD_VOY_NO           ,
              C.SKD_DIR_CD           ,
              C.SLS_OFC_CD           ,
              C.POL_YD_CD            ,
              C.POD_YD_CD            ,
              C.TS_FLG               ,
              C.MNL_FLG              ,
              -- 2014.08.06 컬럼 추가
              C.CUST_CNT_CD          ,
              C.CUST_SEQ             ,
              C.CTRT_NO              ,
              C.USA_BKG_MOD_CD       ,
              C.DEST_LOC_CD   	     ,
              --
              C.REP_TRD_CD           ,
              C.REP_SUB_TRD_CD       ,
              C.TRD_CD               ,
              C.SUB_TRD_CD           ,
              C.IOC_CD               ,
              C.SLS_RHQ_CD           ,
              C.SLS_AQ_CD            ,
              C.SLS_RGN_OFC_CD       ,
              C.ASGN_TTL_QTY         ,
              C.ASGN_20FT_QTY        ,
              C.ASGN_40FT_QTY        ,
              C.ASGN_40FT_HC_QTY     ,
              C.ASGN_45FT_HC_QTY     ,
              C.ASGN_53FT_QTY        ,
              C.ASGN_RF_QTY          ,
              C.ASGN_TTL_WGT         ,
              C.BKG_AVAL_TTL_QTY     ,
              C.BKG_AVAL_20FT_QTY    ,
              C.BKG_AVAL_40FT_QTY    ,
              C.BKG_AVAL_40FT_HC_QTY ,
              C.BKG_AVAL_45FT_HC_QTY ,
              C.BKG_AVAL_53FT_QTY    ,
              C.BKG_AVAL_RF_QTY      ,
              C.BKG_AVAL_TTL_WGT     ,
              C.FCAST_TTL_QTY        ,
              C.FCAST_40FT_HC_QTY    ,
              C.FCAST_45FT_HC_QTY    ,
              C.FCAST_53FT_QTY       ,
              C.FCAST_RF_QTY         ,
              C.FCAST_TTL_WGT        ,
              C.USD_BKG_TTL_QTY      ,
              C.USD_BKG_20FT_QTY     ,
              C.USD_BKG_40FT_QTY     ,
              C.USD_BKG_40FT_HC_QTY  ,
              C.USD_BKG_45FT_HC_QTY  ,
              C.USD_BKG_53FT_QTY     ,
              C.USD_BKG_RF_QTY       ,
              C.USD_BKG_TTL_WGT      ,
              C.SPC_CTRL_ALOC_RMK    ,
              C.SPC_CTRL_ALOC_POL_RMK,
              C.SPC_CTRL_ALOC_POD_RMK,
              C.MNL_ALOC_RMK         ,
              C.ALOC_USR_ID          ,
              C.ALOC_GDT             ,
              C.CRE_USR_ID           ,
              C.CRE_DT               ,
              C.UPD_USR_ID           ,
              C.UPD_DT               ,
              --2014.08.06 컬럼추가
              C.ASGN_20FT_DRY_QTY    ,
              C.ASGN_40FT_DRY_QTY    ,
              C.ASGN_RD_QTY          ,
              C.BKG_AVAL_20FT_DRY_QTY,
              C.BKG_AVAL_40FT_DRY_QTY,
              C.BKG_AVAL_RD_QTY      ,
              C.FCAST_20FT_DRY_QTY   ,
              C.FCAST_40FT_DRY_QTY   ,
              C.FCAST_RD_QTY         ,
              C.USD_BKG_20FT_DRY_QTY ,
              C.USD_BKG_40FT_DRY_QTY ,
              C.USD_BKG_RD_QTY      
           )			]]></sql>
			<params>
				<param name="mnl_aloc_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="aloc_gdt" type="12" value="" out="N"/>
				<param name="spc_ctrl_aloc_rmk" type="12" value="" out="N"/>
				<param name="spc_ctrl_aloc_pol_rmk" type="12" value="" out="N"/>
				<param name="spc_ctrl_aloc_pod_rmk" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="sls_ofc_cd" type="12" value="" out="N"/>
				<param name="sls_rhq_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="ts_flg" type="12" value="" out="N"/>
				<param name="mnl_flg" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="us_mod" type="12" value="" out="N"/>
				<param name="account_cd" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
