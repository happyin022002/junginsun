<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOFrustrateEurTroUSQL">
			<desc><![CDATA[TransferOrderIssueDBDAOFrustrateEurTroUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_EUR_TRO
   SET EUR_TRNS_TP_CD       = 'FR'
        , CFM_USR_ID        = @[upd_usr_id]
        , CFM_OFC_CD        = @[ofc_cd]
--        , TRO_PROC_CD       = NULL --20100305 장인호 수석님 null로 update하면 안됨(as-is)
        , CFM_DT            = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', SYSDATE, GLOBALDATE_PKG.GET_LOCCD_FNC(@[ofc_cd]))
        , CORR_FLG          = 'N'
        , CORR_NO           = @[corr_no]
        , CFM_HLG_TP_CD     = HLG_TP_CD
        , CFM_ALL_IN_RT_CD  = ALL_IN_RT_CD
        , CFM_CURR_CD       = CURR_CD
        , CFM_REV_AMT       = NMF_TRNS_REV_AMT
        , CFM_VAT_FLG       = VAT_FLG
        , UPD_USR_ID        = @[upd_usr_id]
        , UPD_DT            = SYSDATE
 WHERE BKG_NO    = @[bkg_no]
   AND IO_BND_CD = @[io_bnd_cd]
   AND TRO_SEQ   = @[tro_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="corr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
