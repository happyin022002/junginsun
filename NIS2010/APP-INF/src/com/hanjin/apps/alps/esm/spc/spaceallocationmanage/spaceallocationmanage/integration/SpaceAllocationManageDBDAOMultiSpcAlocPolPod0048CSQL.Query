<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOMultiSpcAlocPolPod0048CSQL">
			<desc><![CDATA[History----------------------------------
20110.08.16 [CHM-201112741-01] Control by HO/RHQ 화면 보완 2차 - SLS_OFC_CD가 HAMRU,NYCRA로 Allocation 될때 삽입하는 쿼리
2013.11.26 진마리아 [CHM-201326854] SAQ project로 인한 SPC 변경건_FNC 우선제거]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_ALOC_POL_POD (
	RLANE_CD
,	DIR_CD
,	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	SLS_OFC_CD
,	POL_YD_CD
,	POD_YD_CD
,	TS_FLG
,	MNL_FLG
,	REP_TRD_CD
,	REP_SUB_TRD_CD
,	TRD_CD
,	SUB_TRD_CD
,	IOC_CD
,	SLS_RHQ_CD
,	SLS_RGN_OFC_CD
,	ASGN_TTL_QTY
,	ASGN_20FT_QTY
,	ASGN_40FT_QTY
,	ASGN_40FT_HC_QTY
,	ASGN_45FT_HC_QTY
,	ASGN_RF_QTY
,	ASGN_TTL_WGT
,	MNL_ALOC_RMK
,	ALOC_USR_ID
,	ALOC_GDT
,	ASGN_53FT_QTY
, 	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	ASGN_20FT_DRY_QTY
,	ASGN_40FT_DRY_QTY
,	ASGN_RD_QTY
,   CUST_CNT_CD
,   CUST_SEQ
,   CTRT_NO 
,   USA_BKG_MOD_CD
,   DEST_LOC_CD
) VALUES( 
	@[rlane_cd]
,	@[dir_cd]
,	@[vsl_cd]
,	@[skd_voy_no]
,	@[skd_dir_cd]
,	@[sls_ofc_cd]
,	@[pol_cd]
,	@[pod_cd]
,	DECODE(@[ioc_cd],'T','Y','N')  -- ts_flg
,	'N' --[mnl_flg]
,	SPC_GET_REP_TRD_FNC(@[rlane_cd])
,	SPC_GET_REP_SUB_TRD_FNC(@[rlane_cd])
,	@[trd_cd]
,	@[sub_trd_cd]
,	DECODE(SUBSTR(@[trd_cd],1,1),'I','I','O')
,	@[sls_ofc_cd] --sls_rhq_cd
,	NULL --sls_rgn_ofc_cd
,	NVL(@[asgn_ttl_qty],0)
,	NVL(@[asgn_20ft_qty],0)
,	NVL(@[asgn_40ft_qty],0)
,	NVL(@[asgn_40ft_hc_qty],0)
,	NVL(@[asgn_45ft_hc_qty],0)
,	NVL(@[asgn_rf_qty],0)
,	NVL(@[asgn_ttl_wgt],0)
,	'3' -- mnl_aloc_rmk
,	@[upd_usr_id]
,	CAST(SYS_EXTRACT_UTC(SYSTIMESTAMP) AS DATE)
,	@[asgn_53ft_qty]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	NVL(@[asgn_20ft_dry_qty],0)
,	NVL(@[asgn_40ft_dry_qty],0)
,	NVL(@[asgn_rd_qty],0)
,   NVL(SUBSTR(@[account_cd], 1, 2), 'XX')
,   NVL(SUBSTR(@[account_cd], 3, 6), '999999')
,   NVL(@[ctrt_no], 'X')
,   NVL(@[us_mod], 'OTH')
,   NVL(@[del_cd], 'XXXXX')
)			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="sls_ofc_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="asgn_ttl_qty" type="2" value="" out="N"/>
				<param name="asgn_20ft_qty" type="2" value="" out="N"/>
				<param name="asgn_40ft_qty" type="2" value="" out="N"/>
				<param name="asgn_40ft_hc_qty" type="2" value="" out="N"/>
				<param name="asgn_45ft_hc_qty" type="2" value="" out="N"/>
				<param name="asgn_rf_qty" type="2" value="" out="N"/>
				<param name="asgn_ttl_wgt" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="asgn_53ft_qty" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="asgn_20ft_dry_qty" type="12" value="" out="N"/>
				<param name="asgn_40ft_dry_qty" type="12" value="" out="N"/>
				<param name="asgn_rd_qty" type="12" value="" out="N"/>
				<param name="account_cd" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
				<param name="us_mod" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
