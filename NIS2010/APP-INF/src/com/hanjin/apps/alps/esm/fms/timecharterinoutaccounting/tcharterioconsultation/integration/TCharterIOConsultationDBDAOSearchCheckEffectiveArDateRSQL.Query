<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOSearchCheckEffectiveArDateRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 	EFF_YRMON
FROM   	AP_PERIOD P
WHERE 	P.SYS_DIV_CD = '18'
AND   	P.AR_AP_DIV_CD = 'R'
AND   	P.EFF_YRMON = @[eff_yrmon]
AND   	OFC_CD =  (SELECT S.AR_HD_QTR_OFC_CD
FROM   MDM_ORGANIZATION S
WHERE  S.OFC_CD = 'SELADG')
AND   	P.CLZ_STS_CD = 'O'			]]></sql>
			<params>
				<param name="eff_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
