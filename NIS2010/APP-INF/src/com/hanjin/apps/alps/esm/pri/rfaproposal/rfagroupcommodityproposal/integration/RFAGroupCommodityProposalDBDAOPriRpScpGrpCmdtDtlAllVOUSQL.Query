<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupCommodityProposalDBDAOPriRpScpGrpCmdtDtlAllVOUSQL">
			<desc><![CDATA[RFAGroupCommodityProposalDBDAOPriRpScpGrpCmdtDtlAllVOUSQL]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_GRP_CMDT_DTL  dtl SET 
	   PRC_PROG_STS_CD = @[prc_prog_sts_cd],
	   ACPT_USR_ID     = @[acpt_usr_id],
	   ACPT_OFC_CD     = @[acpt_ofc_cd],
       ACPT_DT         = DECODE(@[acpt_usr_id],'',ACPT_DT,SYSDATE),
       UPD_USR_ID      = @[upd_usr_id],
       UPD_DT          = SYSDATE
WHERE 
	PROP_NO         = @[prop_no]
AND AMDT_SEQ 	   	= @[amdt_seq]
AND SVC_SCP_CD 	   	= @[svc_scp_cd]
AND N1ST_CMNC_AMDT_SEQ    = @[amdt_seq]
AND PRC_PROG_STS_CD 	<> 	@[prc_prog_sts_cd]			]]></sql>
			<params>
				<param name="prc_prog_sts_cd" type="12" value="" out="N"/>
				<param name="acpt_usr_id" type="12" value="" out="N"/>
				<param name="acpt_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
