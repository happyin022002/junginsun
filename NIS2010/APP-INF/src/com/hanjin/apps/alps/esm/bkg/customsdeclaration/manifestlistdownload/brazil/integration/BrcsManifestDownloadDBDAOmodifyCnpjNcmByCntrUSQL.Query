<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BrcsManifestDownloadDBDAOmodifyCnpjNcmByCntrUSQL">
			<desc><![CDATA[컨테이너 정보를 업데이트한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_BRZ_CNTR_MF SET 
	BRZ_CMDT_CD = @[ncm_no]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE BL_NO = @[bl_no]
AND	CNTR_MF_SEQ = @[cntr_mf_seq]
AND	CNTR_NO 	= @[cntr_no]			]]></sql>
			<params>
				<param name="ncm_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_mf_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
