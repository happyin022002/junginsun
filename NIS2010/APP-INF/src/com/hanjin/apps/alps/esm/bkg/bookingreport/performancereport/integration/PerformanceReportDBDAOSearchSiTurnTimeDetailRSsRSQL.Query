<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchSiTurnTimeDetailRSsRSQL">
			<desc><![CDATA[2012.04.12 변종건 [CHM-201217122-01] SI Turn Time Report 보완2]]></desc>
			<sql><![CDATA[
SELECT  *
FROM    (
          SELECT  DISTINCT R.SR_NO
                , R.BKG_NO
                , R.SR_AMD_TP_CD
                , S.BKG_OFC_CD
                , B.VSL_CD||B.SKD_VOY_NO||B.SKD_DIR_CD TVVD
                , B.POL_CD
                , B.POD_CD
                , R.SR_KND_CD
                , (SELECT COUNT(*) FROM BKG_CNTR_MF_DESC WHERE BKG_NO = B.BKG_NO) CM_CNT
                , (SELECT COUNT(*) FROM BKG_HBL WHERE BKG_NO = B.BKG_NO) HBL_CNT
                , TO_CHAR(R.FNT_OFC_TRNS_DT,'YYYY-MM-DD HH24:MI:SS') FNT_OFC_TRNS_DT
                , TO_CHAR(R.SR_DUE_DT,'YYYY-MM-DD HH24:MI:SS') SR_DUE_DT
                , (SELECT C.INTG_CD_VAL_DESC
                   FROM COM_INTG_CD_DTL C
                   WHERE C.INTG_CD_ID = 'CD02405'
                   AND C.INTG_CD_VAL_CTNT = S.RGN_OFC_CD
                   AND ROWNUM = 1) RGN_OFC_CD 
                
                , TO_CHAR(ID.SR_PROC_UPD_DT,'YYYY-MM-DD HH24:MI:SS')         ID_ST_DT
                , TO_CHAR(ID.ST_DT,'YYYY-MM-DD HH24:MI:SS')                  ID_END_DT
                , BKG_GET_CONV_INTVAL_TIME_FNC(ID.BL_DOC_WRK_HRS,'SEC')      ID_BIZ_TM
                , BKG_GET_CONV_INTVAL_TIME_FNC(ID.SR_PROC_HRS,'SEC')         ID_ACT_TM
                , BKG_GET_CONV_INTVAL_TIME_FNC(ID.BL_DOC_OVT_HRS,'SEC')      ID_OVT
                , BKG_GET_CONV_INTVAL_TIME_FNC(ID.SR_WRK_TM_IDLE_HRS, 'SEC') ID_SR_WRK_TM_IDLE_HRS
                , BKG_GET_CONV_INTVAL_TIME_FNC(ID.SR_OVT_IDLE_HRS, 'SEC')    ID_SR_OVT_IDLE_HRS
                , BKG_GET_CONV_INTVAL_TIME_FNC(ID.SR_IDLE_HRS, 'SEC') 	     ID_SR_IDLE_HRS
                
                , TO_CHAR(RD.SR_PROC_UPD_DT,'YYYY-MM-DD HH24:MI:SS')         RD_ST_DT
                , TO_CHAR(RD.ST_DT,'YYYY-MM-DD HH24:MI:SS')                  RD_END_DT
                , BKG_GET_CONV_INTVAL_TIME_FNC(RD.BL_DOC_WRK_HRS,'SEC')      RD_BIZ_TM
                , BKG_GET_CONV_INTVAL_TIME_FNC(RD.SR_PROC_HRS,'SEC')         RD_ACT_TM
                , BKG_GET_CONV_INTVAL_TIME_FNC(RD.BL_DOC_OVT_HRS,'SEC')      RD_OVT
                , BKG_GET_CONV_INTVAL_TIME_FNC(RD.SR_WRK_TM_IDLE_HRS, 'SEC') RD_SR_WRK_TM_IDLE_HRS
                , BKG_GET_CONV_INTVAL_TIME_FNC(RD.SR_OVT_IDLE_HRS, 'SEC')    RD_SR_OVT_IDLE_HRS
                , BKG_GET_CONV_INTVAL_TIME_FNC(RD.SR_IDLE_HRS, 'SEC') 	     RD_SR_IDLE_HRS
                
                , TO_CHAR(AD.SR_PROC_UPD_DT,'YYYY-MM-DD HH24:MI:SS')         AD_ST_DT
                , TO_CHAR(AD.ST_DT,'YYYY-MM-DD HH24:MI:SS')                  AD_END_DT
                , BKG_GET_CONV_INTVAL_TIME_FNC(AD.BL_DOC_WRK_HRS,'SEC')      AD_BIZ_TM
                , BKG_GET_CONV_INTVAL_TIME_FNC(AD.SR_PROC_HRS,'SEC')         AD_ACT_TM
                , BKG_GET_CONV_INTVAL_TIME_FNC(AD.BL_DOC_OVT_HRS,'SEC')      AD_OVT
                , BKG_GET_CONV_INTVAL_TIME_FNC(AD.SR_WRK_TM_IDLE_HRS, 'SEC') AD_SR_WRK_TM_IDLE_HRS
                , BKG_GET_CONV_INTVAL_TIME_FNC(AD.SR_OVT_IDLE_HRS, 'SEC')    AD_SR_OVT_IDLE_HRS
                , BKG_GET_CONV_INTVAL_TIME_FNC(AD.SR_IDLE_HRS, 'SEC') 	     AD_SR_IDLE_HRS
                
                , TO_CHAR(PN.SR_PROC_UPD_DT,'YYYY-MM-DD HH24:MI:SS')         PN_ST_DT
                , TO_CHAR(PN.ST_DT,'YYYY-MM-DD HH24:MI:SS')                  PN_END_DT
                , BKG_GET_CONV_INTVAL_TIME_FNC(PN.BL_DOC_WRK_HRS,'SEC')      PN_BIZ_TM
                , BKG_GET_CONV_INTVAL_TIME_FNC(PN.SR_PROC_HRS,'SEC')         PN_ACT_TM
                , TO_CHAR(R.RTN_DT,'YYYY-MM-DD HH24:MI:SS')                  RT_ST_DT
                , TO_CHAR(R.RTN_TO_RTN_END_DT,'YYYY-MM-DD HH24:MI:SS')       RT_END_DT
                , BKG_GET_CONV_INTVAL_TIME_FNC(R.RTN_HRS + R.RTN_TO_RTN_HRS,'SEC') RT_ACT_TM
                , (
                    SELECT  COUNT(DISTINCT HOL_DT)
                    FROM    MDM_ORGANIZATION O
                          , MDM_LOCATION L
                          , DMT_HOLIDAY H
                    WHERE   O.OFC_CD = B.BKG_OFC_CD
                    AND     O.LOC_CD = L.LOC_CD
                    AND     HOL_YR = TO_CHAR(ID.SR_PROC_UPD_DT,'YYYY')
                    AND     (H.CNT_CD = L.CNT_CD OR H.CNT_CD = ' ')
                    AND     (H.RGN_CD = L.RGN_CD OR H.RGN_CD = ' ')
                    AND     (H.STE_CD = L.STE_CD OR H.STE_CD = ' ')
                    AND     (H.LOC_CD = L.LOC_CD OR H.LOC_CD = ' ')
                    AND     (     (HOL_DT BETWEEN TRUNC(ID.SR_PROC_UPD_DT) AND TRUNC(ID.ST_DT))
                              OR  (HOL_DT BETWEEN TRUNC(RD.SR_PROC_UPD_DT) AND TRUNC(RD.ST_DT))
                              OR  (HOL_DT BETWEEN TRUNC(AD.SR_PROC_UPD_DT) AND TRUNC(AD.ST_DT))
                              OR  (HOL_DT BETWEEN TRUNC(R.RTN_DT) AND TRUNC(R.RTN_TO_RTN_END_DT))
                              OR  (HOL_DT BETWEEN TRUNC(PN.SR_PROC_UPD_DT) AND TRUNC(PN.ST_DT))
                            )
                  ) HOL_CNT
                , R.PND_FLG
                , R.SR_CRNT_INFO_CD
                , R.SR_CRNT_STS_CD
                , NVL(R.BL_DOC_INP_FLG,'N') BL_DOC_INP_FLG
                , NVL(R.BL_RT_FLG,'N') BL_RT_FLG
                , NVL(R.BL_AUD_FLG,'N') BL_AUD_FLG
                , NVL(R.BL_DRFT_FAX_OUT_FLG,'N') BL_DRFT_FAX_OUT_FLG
                , DECODE(R.SR_WRK_STS_CD,'T',NULL,SR_WRK_STS_CD) SR_WRK_STS_CD
          FROM    BKG_SR_CRNT_RQST R
                , BKG_BOOKING B
                , BKG_SR_HIS ID
                , BKG_SR_HIS RD
                , BKG_SR_HIS AD
                , BKG_SR_HIS PN
                , (SELECT BKG_OFC_CD, RGN_OFC_CD
                   FROM (SELECT DISTINCT BKG_OFC_CD, RGN_OFC_CD, delt_flg,
                                RANK() OVER (PARTITION BY BKG_OFC_CD ORDER BY DELT_FLG, RGN_OFC_CD) RNK
                                FROM BKG_EML_ACCT_STUP)
                   WHERE RNK = 1) S
#if (${sr_no} != '')
          WHERE   R.SR_NO = @[sr_no]
#else
#if (${tvvd} != '')
          WHERE R.BKG_NO IN (SELECT BKG_NO
                               FROM BKG_BOOKING
                              WHERE VSL_CD = SUBSTR(@[tvvd],1,4)
                                AND SKD_VOY_NO = SUBSTR(@[tvvd],5,4)
                                AND SKD_DIR_CD = SUBSTR(@[tvvd],9,1))
#else
          WHERE R.FNT_OFC_TRNS_DT BETWEEN TO_DATE(@[fm_dt],'YYYY-MM-DD') AND TO_DATE(@[to_dt],'YYYY-MM-DD') + 0.999999
#end
    #if (${si_sts} == 'C')		
          AND     (NVL(R.BL_DOC_INP_FLG,'N') = 'Y' AND NVL(R.BL_RT_FLG,'N') = 'Y' AND NVL(R.BL_AUD_FLG,'N') = 'Y')
    #end
    #if (${si_sts} == 'P')
          AND     (NVL(R.BL_DOC_INP_FLG,'N') <> 'Y' OR NVL(R.BL_RT_FLG,'N') <> 'Y' OR NVL(R.BL_AUD_FLG,'N') <> 'Y')
    #end
          AND     R.DPCS_OFC_CD = NVL(@[dpcs_ofc_cd],R.DPCS_OFC_CD)
          AND     R.SR_AMD_TP_CD = DECODE(@[sr_amd_tp_cd], 'L', R.SR_AMD_TP_CD, @[sr_amd_tp_cd])
          AND     B.BKG_OFC_CD = NVL(@[bkg_ofc_cd], B.BKG_OFC_CD)
          AND     S.RGN_OFC_CD = DECODE(@[rgn_ofc_cd], 'A',S.RGN_OFC_CD, @[rgn_ofc_cd])
          AND     (     ID.ATND_USR_ID = NVL(@[atnd_usr_id], ID.ATND_USR_ID)
                    OR  RD.ATND_USR_ID = NVL(@[atnd_usr_id], RD.ATND_USR_ID)
                    OR AD.ATND_USR_ID = NVL(@[atnd_usr_id], AD.ATND_USR_ID)
                    OR PN.ATND_USR_ID = NVL(@[atnd_usr_id], PN.ATND_USR_ID)
                  )
#end
          AND     R.BKG_NO = B.BKG_NO
          AND     B.BKG_OFC_CD = S.BKG_OFC_CD
          AND     R.SR_KND_CD = ID.SR_KND_CD(+)
          AND     R.SR_NO = ID.SR_NO(+)
          AND     R.BKG_NO = ID.BKG_NO(+)
          AND     ID.SR_STS_CD(+) = 'ID'
          AND     R.SR_KND_CD = RD.SR_KND_CD(+)
          AND     R.SR_NO = RD.SR_NO(+)
          AND     R.BKG_NO = RD.BKG_NO(+)
          AND     RD.SR_STS_CD(+) = 'RD'
          AND     R.SR_KND_CD = AD.SR_KND_CD(+)
          AND     R.SR_NO = AD.SR_NO(+)
          AND     R.BKG_NO = AD.BKG_NO(+)
          AND     AD.SR_STS_CD(+) = 'AD'
          AND     R.SR_KND_CD = PN.SR_KND_CD(+)
          AND     R.SR_NO = PN.SR_NO(+)
          AND     R.BKG_NO = PN.BKG_NO(+)
          AND     PN.SR_STS_CD(+) = 'PN')
ORDER BY RGN_OFC_CD, DECODE(@[ord_tp], 'O', BKG_OFC_CD, 'V', TVVD, RGN_OFC_CD), BKG_NO, SR_NO			]]></sql>
			<params>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="tvvd" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="dpcs_ofc_cd" type="12" value="" out="N"/>
				<param name="sr_amd_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="atnd_usr_id" type="12" value="" out="N"/>
				<param name="ord_tp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
