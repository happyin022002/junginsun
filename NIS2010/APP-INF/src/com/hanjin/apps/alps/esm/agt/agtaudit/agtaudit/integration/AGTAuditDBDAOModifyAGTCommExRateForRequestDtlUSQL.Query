<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOModifyAGTCommExRateForRequestDtlUSQL">
			<desc><![CDATA[ModifyAGTCommExRateForRequestDtl]]></desc>
			<sql><![CDATA[
UPDATE AGT_AGN_COMM_DTL U
       SET
         ( U.ACT_USD_COMM_AMT,
           U.ACT_LOCL_COMM_AMT, 
           U.UPD_USR_ID,
           U.UPD_DT
         )
         =
         (     
     SELECT
                    CASE B.CNTR_TPSZ_CD
                    WHEN 'D2'
                    THEN                  
                        ROUND
                        ( B.BKG_VOL_QTY * 0.5 / 
                            (
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D4'
                                ),0)
                                +
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D5'
                                ),0)
                                 + B.BKG_VOL_QTY * 0.5 
                            )
                            * A.ACT_IF_COMM_AMT, 
                            2
                         )
                    WHEN 'D4'
                    THEN 
                        ROUND
                        ( B.BKG_VOL_QTY  / 
                            (
                                B.BKG_VOL_QTY
                                +
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D5'
                                ),0)
                                +                                 
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D2'
                                ),0) * 0.5 
                            )
                            * A.ACT_IF_COMM_AMT, 
                            2
                         )                         
                    ELSE
                        ROUND
                        ( B.BKG_VOL_QTY  / 
                            (
                                B.BKG_VOL_QTY
                                +
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D4'
                                ),0)                                
                                + 
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D2'
                                ),0) * 0.5 
                            )
                            * A.ACT_IF_COMM_AMT, 
                            2
                         )
                    END   
                                                                AS ACT_USD_COMM_AMT,
                 CASE A.CURR_CD
                 WHEN 'USD'
                 THEN B.ACT_USD_COMM_AMT
                 ELSE 
                    CASE B.CNTR_TPSZ_CD
                    WHEN 'D2'
                    THEN                  
                        ROUND
                        ( B.BKG_VOL_QTY * 0.5 / 
                            (
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D4'
                                ),0) 
                                +
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D5'
                                ),0)                                
                                + B.BKG_VOL_QTY * 0.5 
                            )
                            * A.ACT_IF_LOCL_COMM_AMT, 
                            2
                         )
                    WHEN 'D4'
                    THEN
                        ROUND
                        ( B.BKG_VOL_QTY  / 
                            (
                                B.BKG_VOL_QTY
                                +
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D5'
                                ),0)
                                +                                 
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D2'
                                ),0) * 0.5 
                            )
                            * A.ACT_IF_LOCL_COMM_AMT, 
                            2
                         )                         
                    ELSE
                        ROUND
                        ( B.BKG_VOL_QTY  / 
                            (
                                B.BKG_VOL_QTY
                                +
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D4'
                                ),0)
                                +                                 
                                NVL((
                                SELECT BKG_VOL_QTY
                                  FROM AGT_AGN_COMM_DTL X
                                 WHERE 1=1
                                   AND X.BKG_NO         = B.BKG_NO
                                   AND X.AGN_CD         = B.AGN_CD
                                   AND X.AC_TP_CD       = B.AC_TP_CD
                                   AND X.IO_BND_CD      = B.IO_BND_CD
                                   AND X.AC_SEQ         = B.AC_SEQ
                                   AND X.CNTR_TPSZ_CD   = 'D2'
                                ),0) * 0.5 
                            )
                            * A.ACT_IF_LOCL_COMM_AMT, 
                            2
                         )
                    END   
                  END                                             AS ACT_IF_LOCL_COMM_AMT,

                      @[upd_usr_id]                               AS UPD_USR_ID,
                      SYSDATE                                     AS UPD_DT
                 FROM AGT_AGN_COMM     A,
                      AGT_AGN_COMM_DTL B
                     
                WHERE A.BKG_NO         = U.BKG_NO
                  AND A.AGN_CD         = U.AGN_CD
                  AND A.AC_TP_CD      <> 'T'
                  AND A.IO_BND_CD      = U.IO_BND_CD
                  AND A.AC_SEQ         = U.AC_SEQ
--                  AND A.CRE_USR_ID    != 'COST'
                  AND A.AC_TP_CD       = U.AC_TP_CD
                  AND B.CNTR_TPSZ_CD   = U.CNTR_TPSZ_CD
                  AND A.BKG_NO         = B.BKG_NO
                  AND A.AGN_CD         = B.AGN_CD
                  AND A.AC_TP_CD       = B.AC_TP_CD
                  AND A.IO_BND_CD      = B.IO_BND_CD
                  AND A.AC_SEQ         = B.AC_SEQ
         )
     WHERE U.BKG_NO         = @[bkg_no]
       AND U.AGN_CD         = @[agn_cd]
       AND U.AC_TP_CD      <> 'T'
       AND U.IO_BND_CD      = @[io_bnd_cd]
       AND U.AC_SEQ         = @[ac_seq]
--       AND U.CRE_USR_ID    != 'COST'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="Y"/>
				<param name="agn_cd" type="12" value="" out="Y"/>
				<param name="io_bnd_cd" type="12" value="" out="Y"/>
				<param name="ac_seq" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
