<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOremoveDoCntrDSQL">
			<desc><![CDATA[Do Container를 삭제한다.]]></desc>
			<sql><![CDATA[
  delete from bkg_do_cntr
   where bkg_no   = @[bkg_no]
     and rlse_seq = @[rlse_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rlse_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
