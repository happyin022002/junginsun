<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchSREmlCtntVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	C.SR_NO
,	C.FAX_LOG_REF_NO
,	C.SR_KND_CD
,	C.EML_MN_CTNT
,	C.CRE_USR_ID
,	C.CRE_DT
,	C.UPD_USR_ID
,	C.UPD_DT
,	EML.EML_SUBJ_CTNT
,	EML.EML_ORG_SUBJ_CTNT
,   (SELECT /*+ INDEX_DESC(MAS_OFC_LVL XPKMAS_OFC_LVL) */ H.ATTR_CTNT2
     FROM COM_USER U, MAS_OFC_LVL O, BKG_HRD_CDG_CTNT H
     WHERE U.USR_ID = @[cre_usr_id]
     AND U.OFC_CD = O.OFC_CD
     AND O.OFC_N2ND_LVL_CD = H.ATTR_CTNT1 --RHQ
     AND H.HRD_CDG_ID = 'SI_TRANS_MAIL'
     AND ROWNUM = 1) RECIPIENT
,   '<ROTATION_SRNO:'||C.SR_NO||'>
<ROTATION_SYS:ALPS>' TEXT_CONTENT
FROM BKG_SR_EML_CTNT C
,	BKG_SR_FAX EML
WHERE 1=1
AND	C.SR_NO(+)	= EML.SR_NO
AND	C.FAX_LOG_REF_NO(+) = EML.FAX_LOG_REF_NO
AND	C.SR_KND_CD(+) = EML.SR_KND_CD
AND	EML.SR_NO = @[sr_no]
AND	EML.FAX_LOG_REF_NO = @[fax_log_ref_no]
AND	EML.SR_KND_CD = @[sr_knd_cd]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="fax_log_ref_no" type="12" value="" out="N"/>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
