<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiFncCodeRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
FROM   BKG_NTC_HIS
WHERE  BKG_NO = @[bkg_no]
AND    NTC_VIA_CD = 'E'
AND    NTC_KND_CD = 'BL'
AND    EDI_ID = @[edi_receive_id]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="edi_receive_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
