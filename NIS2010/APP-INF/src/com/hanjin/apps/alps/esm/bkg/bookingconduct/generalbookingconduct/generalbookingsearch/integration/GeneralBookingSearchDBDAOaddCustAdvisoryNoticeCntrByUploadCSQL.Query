<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOaddCustAdvisoryNoticeCntrByUploadCSQL">
			<desc><![CDATA[VVD별 Customer Advisory Notice 대상 Container 정보를 신규 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CUST_AVC_NTC_CNTR
( SELECT BK.BL_NO         AS BL_NO
       , CNTR.CNTR_NO       AS CNTR_NO              
       , @[cre_usr_id]   AS CRE_USR_ID
       , SYSDATE         AS CRE_DT
       , @[upd_usr_id]   AS UPD_USR_ID
       , SYSDATE         AS UPD_DT
	   , 'E'			 AS SRC_DAT_TP_CD
    FROM BKG_CONTAINER CNTR, BKG_BOOKING BK, BKG_VVD BV
   WHERE 1=1
     AND BK.BKG_NO = CNTR.BKG_NO
     AND BK.BKG_NO = BV.BKG_NO
     AND BV.VSL_CD = SUBSTR(@[vvd], 1,4)
     AND BV.SKD_VOY_NO = SUBSTR(@[vvd], 5,4)
     AND BV.SKD_DIR_CD IN (${dir_sts_cd})
     AND BK.BKG_STS_CD <>'X'
     AND CNTR_NO IN (${cntr_no})

	 AND NOT EXISTS (SELECT '*' FROM BKG_CUST_AVC_NTC_CNTR A 
						WHERE A.SRC_DAT_TP_CD = 'E'
                		AND A.BL_NO = CNTR.BKG_NO
						AND A.CNTR_NO = CNTR.CNTR_NO
                	)
)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
