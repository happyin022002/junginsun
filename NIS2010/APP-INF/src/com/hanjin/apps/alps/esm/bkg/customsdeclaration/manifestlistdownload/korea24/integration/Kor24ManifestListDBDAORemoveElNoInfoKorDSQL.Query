<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Kor24ManifestListDBDAORemoveElNoInfoKorDSQL">
			<desc><![CDATA[한국세관 Export Lic. No 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CSTMS_ADV_KR_XPT_LIC
WHERE  BKG_NO = @[bkg_no]
AND    CSTMS_DECL_TP_CD = @[kcd_tp]
AND    DMST_PORT_CD = @[kt_port]
AND    TRNS_SEQ = @[kt_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="kcd_tp" type="12" value="" out="N"/>
				<param name="kt_port" type="12" value="" out="N"/>
				<param name="kt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
