<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOSearchAllocationResultInterRSQL">
			<desc><![CDATA[History ----------------------------
2010.10.22 이행지 [CHM-201006375-01][MAS] Trunk IPC와 Ocean간 내부거래 신규 추가 
                                                       - Allocation Result( Internal Pricing) 화면 Retrieve버튼적용 쿼리]]></desc>
			<sql><![CDATA[
SELECT	FX.FM_TRD_CD
	,   FX.FM_IOC_CD
	,   FX.FM_RLANE_CD
	,   FX.FM_VSL_CD
	,	FX.FM_SKD_VOY_NO
	,   FX.FM_SKD_DIR_CD
	,   FX.FX_COST_DTRB_AMT
	,   FX.TO_TRD_CD
	,   FX.TO_IOC_CD
	,   FX.TO_RLANE_CD
	,   FX.TO_VSL_CD
	,   FX.TO_SKD_VOY_NO
	,   FX.TO_SKD_DIR_CD
	,   FX.FX_COST_DTRB_AMT
  FROM	MAS_MON_VVD        HD
    ,   MAS_FX_AMT_DTRB    FX
 WHERE  HD.TRD_CD          = FX.FM_TRD_CD
   AND  HD.RLANE_CD        = FX.FM_RLANE_CD
   AND  HD.IOC_CD          = FX.FM_IOC_CD
   AND  HD.VSL_CD          = FX.FM_VSL_CD
   AND  HD.SKD_VOY_NO      = FX.FM_SKD_VOY_NO
   AND  HD.DIR_CD          = FX.FM_SKD_DIR_CD
   AND  FX.STND_COST_CD    = '54600000'
   AND  NVL(FX.DTRB_STEP_CD,'BZC') = 'BZC'
   AND  HD.DELT_FLG		   = 'N'
#if (${f_chkprd} == 'M')
   AND HD.COST_YRMON BETWEEN @[f_year]||@[f_fm_mon] AND @[f_year]||@[f_to_mon]
#elseif (${f_chkprd} == 'W')
   AND HD.SLS_YRMON LIKE @[f_year]||'%'
   AND HD.COST_WK BETWEEN @[f_fm_wk] AND @[f_to_wk]
#end
#if (${f_seltrade} != '')
   AND	HD.TRD_CD	= @[f_seltrade]
#end
#if (${f_selrlane} != '')
   AND	HD.RLANE_CD	= @[f_selrlane]
#end
#if (${f_vsl_cd} != '')
   AND HD.VSL_CD = @[f_vsl_cd]
#end
#if (${f_skd_voy_no} != '')
   AND HD.SKD_VOY_NO = @[f_skd_voy_no]
#end
#if (${f_dir_cd} != '')
   AND HD.DIR_CD = @[f_dir_cd]
#end			]]></sql>
			<params>
				<param name="f_year" type="12" value="" out="N"/>
				<param name="f_fm_mon" type="12" value="" out="N"/>
				<param name="f_to_mon" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
				<param name="f_seltrade" type="12" value="" out="N"/>
				<param name="f_selrlane" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
				<param name="f_skd_voy_no" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
