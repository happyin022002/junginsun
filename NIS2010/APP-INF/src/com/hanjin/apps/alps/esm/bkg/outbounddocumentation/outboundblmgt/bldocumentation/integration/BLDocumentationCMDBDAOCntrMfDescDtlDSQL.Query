<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCntrMfDescDtlDSQL">
			<desc><![CDATA[BKG_CNTR_MF_DESC_DTL을 삭제한다.]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
DELETE FROM BKG_CNTR_MF_DESC_DTL_HIS
WHERE  BKG_NO = @[bkg_no]
AND    CORR_NO = 'TMP0000001'
AND    CNTR_NO = @[cntr_no]
#else 
DELETE FROM BKG_CNTR_MF_DESC_DTL
WHERE  BKG_NO = @[bkg_no]
AND    CNTR_NO = @[cntr_no]
#end
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
