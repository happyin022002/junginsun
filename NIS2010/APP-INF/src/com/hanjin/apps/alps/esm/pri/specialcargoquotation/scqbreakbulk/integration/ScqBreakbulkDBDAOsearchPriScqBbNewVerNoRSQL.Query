<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqBreakbulkDBDAOsearchPriScqBbNewVerNoRSQL">
			<desc><![CDATA[searchPriScqBbNewVerNo]]></desc>
			<sql><![CDATA[
SELECT (NVL((SELECT /*+INDEX_DESC(A XPKPRI_SCQ_BB_MN )*/ SCQ_VER_NO
               FROM PRI_SCQ_BB_MN A
              WHERE A.SCQ_RQST_NO = @[scq_rqst_no]
                AND ROWNUM <= 1), 000 ) + 1) AS SCQ_VER_NO 
FROM DUAL			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
