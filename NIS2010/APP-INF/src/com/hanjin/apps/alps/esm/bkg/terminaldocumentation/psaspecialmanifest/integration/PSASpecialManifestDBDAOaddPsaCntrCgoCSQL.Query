<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSASpecialManifestDBDAOaddPsaCntrCgoCSQL">
			<desc><![CDATA[BKG_CSTMS_PSA_DG_CGO Insert]]></desc>
			<sql><![CDATA[
INSERT
INTO BKG_CSTMS_PSA_DG_CGO( MSG_SND_NO, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, BL_NO, CSTMS_PORT_CD, CNTR_NO, DCGO_SEQ, IMDG_UN_NO, IMDG_UN_NO_SEQ, GRS_WGT, IMDG_CLSS_CD, PCK_QTY, PCK_TP_CD, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT, WGT_UT_CD, MEAS_QTY, MEAS_UT_CD )
VALUES(@[msg_snd_no], 
	  SUBSTR(@[vvd_cd], 1, 4),
      SUBSTR(@[vvd_cd], 5, 4),
      SUBSTR(@[vvd_cd], 9, 1),
      @[bl_no],
      'D', --cstms_port_cd
      @[cntrnbr],
      (SELECT NVL(MAX(TO_NUMBER(DCGO_SEQ)), 0) + 1
        FROM BKG_CSTMS_PSA_DG_CGO
        WHERE VSL_CD||SKd_VOY_NO||SKD_DIR_CD = @[vvd_cd]
          AND BL_NO = @[bl_no]
          AND CSTMS_PORT_CD = 'D' --cstms_port_cd
          AND CNTR_NO = @[cntrnbr]),
      @[un_nbr],
      @[un_nbr_seq],
      @[grosswgt],
      @[imo_clss],
      @[pck_qty],
      '',
      @[usr_id],
      sysdate,
      @[usr_id],
      sysdate,
      '',
      '',
      '' )			]]></sql>
			<params>
				<param name="msg_snd_no" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntrnbr" type="12" value="" out="N"/>
				<param name="un_nbr" type="12" value="" out="N"/>
				<param name="un_nbr_seq" type="12" value="" out="N"/>
				<param name="grosswgt" type="12" value="" out="N"/>
				<param name="imo_clss" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
