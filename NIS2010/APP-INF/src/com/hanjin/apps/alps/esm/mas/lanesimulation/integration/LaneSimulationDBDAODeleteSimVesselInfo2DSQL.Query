<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAODeleteSimVesselInfo2DSQL">
			<desc><![CDATA[Vessel info 삭제]]></desc>
			<sql><![CDATA[
DELETE 
  FROM MAS_SIM_VSL_SET_INFO 
 WHERE SIM_DT  =  @[sim_dt] 
   AND SIM_NO  =  @[sim_no] 
   AND SECT_NO <> @[sect_no] 
   AND SIM_DIV_CD  <> '2' 
   AND VSL_CD NOT IN ( 
                  SELECT VSL_CD 
                    FROM MAS_SIM_VSL_SET_INFO 
                   WHERE SIM_DT  = @[sim_dt] 
                     AND SIM_NO  = @[sim_no] 
                     AND SECT_NO = @[sect_no] 
                     AND VSL_CD  = @[vsl_cd] 
                     AND SIM_DIV_CD  <> '2' 
                 )			]]></sql>
			<params>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="sect_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
