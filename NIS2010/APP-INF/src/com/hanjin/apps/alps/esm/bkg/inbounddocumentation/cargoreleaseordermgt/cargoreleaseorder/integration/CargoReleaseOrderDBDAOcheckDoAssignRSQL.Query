<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOcheckDoAssignRSQL">
			<desc><![CDATA[D/O Assign 수행 여부를 점검한다]]></desc>
			<sql><![CDATA[
SELECT DECODE( COUNT(*), 0, 'N', 'Y')
FROM BKG_DO_DTL
WHERE BKG_NO = @[bkg_no]
  AND RLSE_STS_CD IN ('A')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
