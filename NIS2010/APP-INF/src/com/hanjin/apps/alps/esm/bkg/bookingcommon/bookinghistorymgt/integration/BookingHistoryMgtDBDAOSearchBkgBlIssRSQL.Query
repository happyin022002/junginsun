, OBL_RDEM_UPD_USR_ID
, TO_CHAR(OBL_RDEM_DT, 'YYYY-MM-DD HH24:MI:SS') OBL_RDEM_DT
, OBL_RDEM_KNT
, OTR_DOC_CGOR_FLG
, BL_OTR_DOC_RCV_CD
