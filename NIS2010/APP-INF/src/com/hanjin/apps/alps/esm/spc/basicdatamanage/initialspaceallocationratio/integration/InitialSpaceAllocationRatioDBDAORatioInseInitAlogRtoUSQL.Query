<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InitialSpaceAllocationRatioDBDAORatioInseInitAlogRtoUSQL">
			<desc><![CDATA[SPC_INIT_ALOC_RTO 정보 UPDATE]]></desc>
			<sql><![CDATA[
UPDATE SPC_INIT_ALOC_RTO
   SET INIT_ALOC_RTO = @[init_aloc_rto],
       UPD_USR_ID    = @[upd_usr_id]   ,
       UPD_DT        = TO_DATE(SYSDATE,'YYYY-MM-DD')
 WHERE REP_TRD_CD = @[rep_trd_cd]
   AND DIR_CD     = @[dir_cd]
   AND BSE_MON    = @[bse_mon]			]]></sql>
			<params>
				<param name="init_aloc_rto" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="bse_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
