<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MyanmarCustomsTransmissionDBDAOsearchBkgCgoTpCdRSQL">
			<desc><![CDATA[BkgCgoTpCd 조회해서 empty cargo 인지 아닌지 확인]]></desc>
			<sql><![CDATA[
SELECT BKG_CGO_TP_CD CGO_CD 
FROM BKG_BOOKING 
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
