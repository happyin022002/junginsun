<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MalaysiaCustomsTransmissionDBDAOsearchRfGoodsInfoRSQL">
			<desc><![CDATA[BKG REEFER CARGO 정보]]></desc>
			<sql><![CDATA[
SELECT CDO_TEMP AS CNTR_TMP
     , 'SEL'    AS CNTR_TMP_UNIT
  FROM BKG_RF_CGO RF
 WHERE RF.BKG_NO  = @[bl_no]
#if (${cntr_no} != '') 
   AND RF.CNTR_NO = @[cntr_no]
#end 			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
