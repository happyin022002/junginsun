<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQHoldingDBDAOSearchEQHoldingCostRSQL">
			<desc><![CDATA[_HLD_COST, _COST_SRC_ACCT, _STND_ACCT, COM_INTG_CD_DTL 테이블의 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT   A.EQ_TPSZ_CD
         , A.STND_COST_CD
         , C.STND_COST_NM
         , B.MAS_COST_SRC_CD
         , B.MAS_COST_SRC_CD_NM
         , DECODE(A.COST_ASS_BSE_CD,'F',A.CHSS_HLD_UC_AMT
                                   , DECODE(A.CNTR_CHSS_DIV_CD,'CNTR',A.TTL_HLD_AMT / (EQ_BX_KNT * 30)
                                                              , A.TTL_HLD_AMT / A.CHSS_USA_QTY)) CHSS_HLD_UC_AMT
         , A.TTL_HLD_AMT
         , A.EQ_BX_KNT
         , DECODE(A.COST_ASS_BSE_CD,'F',A.EQ_HLD_DYS
                                   , NVL(A.EQ_HLD_DYS,30)) EQ_HLD_DYS
         , A.CHSS_USA_QTY
         , A.COST_ASS_BSE_CD
         , D.INTG_CD_VAL_DP_DESC                                                                 COST_ASS_BSE_NM
FROM     MAS_HLD_COST A
         , MAS_COST_SRC_ACCT B
         , MAS_STND_ACCT C
         , COM_INTG_CD_DTL D
WHERE    A.STND_COST_CD = C.STND_COST_CD
AND A.STND_COST_CD = B.STND_COST_CD
AND A.COST_ASS_BSE_CD = D.INTG_CD_VAL_CTNT
AND D.INTG_CD_ID = 'CD00201'
AND A.COST_YRMON = @[f_cost_yrmon]
AND A.CNTR_CHSS_DIV_CD = @[f_calc_term_cd]
#if (${f_cntr_tpsz_cd} != '')
AND A.EQ_TPSZ_CD = @[f_cntr_tpsz_cd]
#end
ORDER BY A.EQ_TPSZ_CD
			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
				<param name="f_calc_term_cd" type="12" value="" out="N"/>
				<param name="f_cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
