<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOaddBkgUsaWhfRtCSQL">
			<desc><![CDATA[addBkgUsaWhfRt]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_USA_WHF_RT
USING DUAL 
        ON (
           PORT_CD = @[port_cd]
       AND IO_BND_CD = @[io_bnd_cd]
       AND TO_CHAR(EFF_DT, 'YYYYMMDD') = @[eff_dt]
           )
WHEN NOT MATCHED THEN
	INSERT 
(
	PORT_CD
,	IO_BND_CD
,	EFF_DT
,   WHF_DC_RT
,	CRE_USR_ID
,	UPD_USR_ID
) VALUES( 
	@[port_cd]
,	@[io_bnd_cd]
,	TO_DATE(@[eff_dt], 'YYYYMMDD')
,   @[whf_dc_rt]
,	@[upd_usr_id]
,	@[upd_usr_id]
)
WHEN MATCHED THEN
UPDATE
   SET WHF_DC_RT = @[whf_dc_rt]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="whf_dc_rt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
