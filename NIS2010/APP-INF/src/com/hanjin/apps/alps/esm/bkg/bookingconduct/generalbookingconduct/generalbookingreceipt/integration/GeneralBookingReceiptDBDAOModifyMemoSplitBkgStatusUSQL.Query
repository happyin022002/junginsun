<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModifyMemoSplitBkgStatusUSQL">
			<desc><![CDATA[Memo split 된 전체 BKG이 Cancel 된 경우 Memo master의 상태를 변경한다.]]></desc>
			<sql><![CDATA[
update bkg_booking
   set SPLIT_RSN_CD = NULL 
		, BKG_STS_CD= 'F'
        , UPD_USR_ID= @[usr_id]
        , UPD_DT    = sysdate
 where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
