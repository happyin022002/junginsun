<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOSearchAtchFileCntRSQL">
			<desc><![CDATA[첨부파일 개수 조회]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS ATCH_FILE_CNT 
FROM FMS_OWNR_ACCT_ATCH_FILE F
WHERE 1=1
AND F.ATCH_FILE_OA_LNK_ID LIKE @[csr_no] || '%'			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
