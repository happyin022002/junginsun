<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Kor24ManifestListDBDAOSearchCntCdTpCRSQL">
			<desc><![CDATA["C"]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD CNT_CD
     , CUST_SEQ CUST_CD
	 , CUST_NM
  FROM BKG_CUSTOMER
 WHERE BKG_NO = @[a_bkg_no]
   AND BKG_CUST_TP_CD = 'C'			]]></sql>
			<params>
				<param name="a_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
