<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqAwkwardDBDAOcalcPriScqAwkYdSmryTmpCSQL">
			<desc><![CDATA[PRI_SCQ_AWK_YD_SMRY_TMP 테이블 생성
기 계산 처리되어 생성된 PRI_SCQ_AWK_YD_DTL_TMP 데이터를 기반하여 Summary Data 를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SCQ_AWK_YD_SMRY_TMP
(       SCQ_RQST_NO
    ,   SCQ_VER_NO
    ,   ROUT_SEQ
    ,   YD_COST_SEQ
    ,   IO_BND_CD
    ,   YD_CD
    ,   COST_TP_CD
    ,   COST_AMT
    ,   CRE_USR_ID
    ,   CRE_DT
    ,   UPD_USR_ID
    ,   UPD_DT
)
SELECT  @[scq_rqst_no], @[scq_ver_no_tmp], A.ROUT_SEQ, A.YD_COST_SEQ, A.IO_BND_CD, A.YD_CD, A.COST_TP_CD, SUM ( A.CNTR_QTY * A.USD_AMT )
    ,   @[cre_usr_id], SYSDATE, @[cre_usr_id], SYSDATE
FROM    PRI_SCQ_AWK_YD_DTL_TMP A
WHERE   SCQ_RQST_NO = @[scq_rqst_no] 
AND     SCQ_VER_NO  = @[scq_ver_no_tmp] 
GROUP   BY A.ROUT_SEQ, A.YD_COST_SEQ, A.IO_BND_CD, A.YD_CD, A.COST_TP_CD
			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no_tmp" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
