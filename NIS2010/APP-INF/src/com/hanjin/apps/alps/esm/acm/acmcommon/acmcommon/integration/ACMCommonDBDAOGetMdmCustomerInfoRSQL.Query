<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCommonDBDAOGetMdmCustomerInfoRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT CUST_SEQ AS VALUE0,
       CUST_CNT_CD AS VALUE1,
       CUST_LGL_ENG_NM AS VALUE2
  FROM MDM_CUSTOMER
 WHERE NVL(DELT_FLG, 'N') = 'N'
   AND CUST_CNT_CD = SUBSTR(@[value0], 1, 2)
   AND CUST_SEQ = TO_NUMBER(SUBSTR(@[value0], 3))
   AND CNTR_DIV_FLG = 'Y'			]]></sql>
			<params>
				<param name="value0" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
