<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchBkgQtyRSQL">
			<desc><![CDATA[qty 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  CNTR_TPSZ_CD HANTYPE
        ,NVL(OP_CNTR_QTY,0) COUNT
  FROM  BKG_QUANTITY
 WHERE  BKG_NO  = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
