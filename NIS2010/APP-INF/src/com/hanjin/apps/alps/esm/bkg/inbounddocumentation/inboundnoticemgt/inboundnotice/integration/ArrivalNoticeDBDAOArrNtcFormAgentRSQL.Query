<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOArrNtcFormAgentRSQL">
			<desc><![CDATA[...]]></desc>
			<sql><![CDATA[
SELECT chn_agn_cd,an_seq

from (SELECT '' as chn_agn_cd
       ,'ALL'  as an_seq
FROM DUAL
UNION ALL
SELECT DISTINCT chn_agn_cd as chn_agn_cd,
                chn_agn_cd as an_seq
  FROM BKG_ARR_NTC_WD
 WHERE AN_TP_CD = 'ARN'
   AND OFC_CD = @[ofc_cd]
   AND pod_cd = DECODE(@[pod_cd], NULL, '*', @[pod_cd])
   AND CHN_AGN_CD <> '*'
)

ORDER BY decode(an_seq,'ALL','1',an_seq) asc			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
