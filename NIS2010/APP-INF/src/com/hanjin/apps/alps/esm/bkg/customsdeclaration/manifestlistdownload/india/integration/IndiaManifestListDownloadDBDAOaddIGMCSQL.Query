<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaManifestListDownloadDBDAOaddIGMCSQL">
			<desc><![CDATA[인도세관 테이블에 BL정보를 추가한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_IDA_BL (
	IDA_CSTMS_BL_TP_CD
,	IDA_HBL_SEQ
,	BL_CGO_DESC
,	IDA_CSTMS_ITM_TP_CD
,	IDA_LINE_NO
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	POD_CD
,	BL_NO
,	NVOCC_REF_NO
,	POL_CD
,	DEL_CD
,	BL_DECL_TP_CD
,	IDA_DEST_CD
,	IDA_CFS_ID
,	IAL_RGN_CD
,	IDA_TRSP_CD
,	CNEE_NM
,   BD_AREA_CD
,   TRNS_OPR_ID
) VALUES( 
	@[bl_cust_tp]
,	@[hbl_ind]
,	@[bcd_desc]
,	@[item_tp]
,	@[ida_line_no]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	SUBSTR(@[vvd_cd], 1, 4)
,	SUBSTR(@[vvd_cd], 5, 4)
,	SUBSTR(@[vvd_cd], 9, 1)
,	@[pod_cd]
,	@[bl_no]
,	@[hbl_no]
,	@[pol_cd]
,	@[del_cd]
,	@[entry_tp]
,	@[dest_cd]
,	@[cfs_cd]
,	@[ial]
,	@[mode_trans]
,	@[customer_name]
,   @[bd_area_cd]
,   @[trns_opr_id]
)			]]></sql>
			<params>
				<param name="bl_cust_tp" type="12" value="" out="N"/>
				<param name="hbl_ind" type="12" value="" out="N"/>
				<param name="bcd_desc" type="12" value="" out="N"/>
				<param name="item_tp" type="12" value="" out="N"/>
				<param name="ida_line_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="hbl_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="entry_tp" type="12" value="" out="N"/>
				<param name="dest_cd" type="12" value="" out="N"/>
				<param name="cfs_cd" type="12" value="" out="N"/>
				<param name="ial" type="12" value="" out="N"/>
				<param name="mode_trans" type="12" value="" out="N"/>
				<param name="customer_name" type="12" value="" out="N"/>
				<param name="bd_area_cd" type="12" value="" out="N"/>
				<param name="trns_opr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
