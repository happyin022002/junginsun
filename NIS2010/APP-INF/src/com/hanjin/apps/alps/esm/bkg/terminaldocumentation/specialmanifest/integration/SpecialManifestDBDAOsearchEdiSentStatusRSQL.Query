<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchEdiSentStatusRSQL">
			<desc><![CDATA[EDI전송 결과를 조회 해 온다.]]></desc>
			<sql><![CDATA[
SELECT
    CASE
        WHEN 'ESM_BKG_0965' = @[ui_type] THEN /* ANRBS */
            (

                SELECT
                    CASE
                        WHEN TOT.TOT_CNT > 0 THEN
                            CASE
                                WHEN TOT.TOT_CNT = RCV.RCV_ACP_CNT THEN 'SUCCESS'
                                ELSE 'FAIL'
                            END
                        
                        ELSE ''
                        
                    END  ACK_RCV_STS_CD
                FROM (
	                    SELECT
                            COUNT(*) TOT_CNT
                        FROM  BKG_CSTMS_EUR_DG_SND  A
                              , BKG_CSTMS_EUR_DG_EDI_RSPN B
                        WHERE A.EUR_EDI_MSG_TP_ID = 'IFD'
						AND   A.MSG_SND_NO > ' '
						AND   A.EUR_DG_DECL_TP_CD = @[d_type]
                        AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                        AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                        AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                        AND   A.PORT_CD     = @[port_cd]
                        AND   A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                        AND   A.MSG_SND_NO = B.MSG_SND_NO
                        AND   A.UPD_DT = (
                                            SELECT MAX(A.UPD_DT)
                                            FROM BKG_CSTMS_EUR_DG_SND A
                                            WHERE A.EUR_EDI_MSG_TP_ID = 'IFD'
											AND   A.MSG_SND_NO > ' '
											AND   A.EUR_DG_DECL_TP_CD = @[d_type]
                                            AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                                            AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                                            AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                                            AND   A.PORT_CD     = @[port_cd]
                                        )

                        
                    ) TOT
                    ,(
                
                        SELECT
                            COUNT(*) RCV_ACP_CNT
                        FROM  BKG_CSTMS_EUR_DG_EDI_RSPN B
                             , BKG_CSTMS_EUR_DG_RCV C
                        WHERE 1=1
                        AND   B.MSG_SND_NO = ( 
                                        SELECT MIN(MSG_SND_NO)
                                        FROM BKG_CSTMS_EUR_DG_SND A
                                        WHERE A.EUR_EDI_MSG_TP_ID = 'IFD'
                                        AND   A.EUR_DG_DECL_TP_CD = @[d_type]
                       					AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                        				AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                        				AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                        				AND   A.PORT_CD     = @[port_cd]
                                        AND   A.UPD_DT = (
                                        
                                                            SELECT MAX(UPD_DT)
                                                            FROM BKG_CSTMS_EUR_DG_SND A
                                                            WHERE A.EUR_EDI_MSG_TP_ID = 'IFD'
                                        					AND   A.EUR_DG_DECL_TP_CD = @[d_type]
                       										AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                        									AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                        									AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                        									AND   A.PORT_CD     = @[port_cd]
                                        
                                                        )
                                        )
                        AND  B.EUR_EDI_MSG_TP_ID = C.EUR_EDI_MSG_TP_ID
                        AND  B.MSG_SND_NO = C.MSG_RCV_NO
                        AND C.ACK_RCV_STS_CD = 'A'

                    ) RCV                                


            )
        WHEN 'ESM_BKG_0966' = @[ui_type] THEN /* ETC */
            (
            	SELECT /*+ INDEX_DESC(BKG_CSTMS_EUR_DG_RCV XPKBKG_CSTMS_EUR_DG_RCV)*/ 
                        CASE
                			WHEN MSG_RCV_NO IS NULL AND ACK_RCV_STS_CD IS NULL THEN ''
                			WHEN MSG_RCV_NO IS NOT NULL AND ACK_RCV_STS_CD IS NULL THEN 'P'
                			ELSE ACK_RCV_STS_CD
            			END ACK_RCV_STS_CD
 
                FROM BKG_CSTMS_EUR_DG_RCV
                WHERE EUR_EDI_MSG_TP_ID = 'IFD'
                AND MSG_RCV_NO = (
                                    SELECT MAX(A.MSG_SND_NO)
                                    FROM BKG_CSTMS_EUR_DG_SND A
                                    WHERE A.EUR_DG_DECL_TP_CD = @[d_type]
                                    AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
                                    AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
                                    AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
                                    AND   A.PORT_CD     = @[port_cd]
                                    AND   A.EUR_EDI_MSG_TP_ID ='IFD'

                                )
                AND ROWNUM = 1
            )            
    END ACK_RCV_STS_CD
FROM DUAL			]]></sql>
			<params>
				<param name="ui_type" type="12" value="" out="N"/>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
