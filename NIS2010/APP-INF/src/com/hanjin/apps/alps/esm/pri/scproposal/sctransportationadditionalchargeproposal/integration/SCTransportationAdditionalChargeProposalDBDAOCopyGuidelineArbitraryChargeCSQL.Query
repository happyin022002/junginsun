<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCTransportationAdditionalChargeProposalDBDAOCopyGuidelineArbitraryChargeCSQL">
			<desc><![CDATA[guideline copy
2014.06.12 전윤주 [CHM-201430680] Guideline Copy 시 Arbitrary Copy 가 되지 않는 현상 수정
(MAX Seq. 찾아오는 쿼리 로직 수정)]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_TRSP_ADD_CHG (
   	   PROP_NO
     , AMDT_SEQ
     , SVC_SCP_CD
     , ADD_CHG_TP_CD
     , ORG_DEST_TP_CD
     , ADD_CHG_SEQ
     , ROUT_PNT_LOC_TP_CD
     , ROUT_PNT_LOC_DEF_CD
     , BSE_PORT_TP_CD
     , BSE_PORT_DEF_CD
     , VIA_PORT_TP_CD
     , VIA_PORT_DEF_CD
     , DIR_CALL_FLG
     , RAT_UT_CD
     , PRC_CGO_TP_CD
     , PRC_TRSP_MOD_CD
     , RCV_DE_TERM_CD
     , CURR_CD
     , PROP_FRT_RT_AMT
	 , GRI_APPL_TP_CD
	 , GRI_APPL_AMT
     , PRC_PROG_STS_CD
     , SRC_INFO_CD
     , N1ST_CMNC_AMDT_SEQ
     , CRE_USR_ID
     , UPD_USR_ID
)
SELECT @[prop_no] AS PROP_NO 
     , @[amdt_seq] AS AMDT_SEQ
     , SVC_SCP_CD
	 , 'A' AS ADD_CHG_TP_CD
	 , ORG_DEST_TP_CD
	 , DENSE_RANK() OVER (PARTITION BY SVC_SCP_CD, GLINE_SEQ, ORG_DEST_TP_CD
     	ORDER BY SVC_SCP_CD, GLINE_SEQ, ORG_DEST_TP_CD, ARB_SEQ) AS ADD_CHG_SEQ
	 , ROUT_PNT_LOC_TP_CD
	 , ROUT_PNT_LOC_DEF_CD
	 , BSE_PORT_TP_CD
	 , BSE_PORT_DEF_CD
	 , VIA_PORT_TP_CD
	 , VIA_PORT_DEF_CD
	 , DIR_CALL_FLG
	 , RAT_UT_CD
	 , PRC_CGO_TP_CD
	 , PRC_TRSP_MOD_CD
	 , RCV_DE_TERM_CD
	 , CURR_CD
	 , FRT_RT_AMT
	 , 'N'
	 , 0
	 , 'I' AS SRC_INFO_CD
	 , 'GC' AS SRC_INFO_CD
     , @[amdt_seq] AS N1ST_CMNC_AMDT_SEQ
     , @[cre_usr_id] AS CRE_USR_ID
     , @[upd_usr_id] AS UPD_USR_ID
  FROM PRI_SG_ARB
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND GLINE_SEQ = (SELECT MAX(GLINE_SEQ) AS GLINE_SEQ
                      FROM PRI_SG_MN 
                     WHERE SVC_SCP_CD = @[svc_scp_cd] 
                       AND TO_DATE(REPLACE(@[eff_dt],'-',''),'YYYYMMDD') BETWEEN EFF_DT AND EXP_DT
                       AND CFM_FLG = 'Y')			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
