<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOSearchBLStsReportList1RSQL">
			<desc><![CDATA[StatusReportDBDAOSearchBLStsReportList1RSQL
* History
* 2012.07.11 김보배 [CHM-201218861] [BKG] B/L Status Report 상 Web Original B/L 추가 요청
* 2012.08.08 김기택 [CHM-201219181] [BKG] BL Status Report에 Issue & Release 로직 수정]]></desc>
			<sql><![CDATA[
SELECT *
    FROM (
	   SELECT 
            ROWNUM RNUM            
             /* 2009.12.28 수정 */
            , NVL(BKG_GET_TOKEN_FNC(PPD_ORG0,1),'N') AS PPD_ORG
            , BKG_GET_TOKEN_FNC(PPD_ORG0,2) AS PPD_ORG2
            , NVL(BKG_GET_TOKEN_FNC(PPD_3RD0,1),'N') AS PPD_3RD
            , BKG_GET_TOKEN_FNC(PPD_3RD0,2) AS PPD_3RD2
            , NVL(BKG_GET_TOKEN_FNC(CCT_DEST0,1),'N') AS CCT_DEST
            , BKG_GET_TOKEN_FNC(CCT_DEST0,2) AS CCT_DEST2
            , NVL(BKG_GET_TOKEN_FNC(CCT_3RD0,1),'N') AS CCT_3RD
            , BKG_GET_TOKEN_FNC(CCT_3RD0,2) AS CCT_3RD2
            ,ZZ.*

		FROM (
		  SELECT 
		    	COUNT(BKG_NO) OVER() TOTAL_CNT, 		    	
			Z.*
		  FROM ( SELECT
		     ( SELECT REPLACE(CUST_NM,chr(13)||chr(10),' ')
		       FROM BKG_CUSTOMER 
		       WHERE BKG_NO = Y.BKG_NO
		       AND   BKG_CUST_TP_CD ='S'
		     ) AS SHIPPER
            
			/* 2009.12.28 수정 */
			,( SELECT REPLACE(CUST_NM,chr(13)||chr(10),' ')
			FROM BKG_CUSTOMER
			WHERE BKG_NO = Y.BKG_NO
			AND   BKG_CUST_TP_CD ='F'
			) AS FOWARDER

		    ,( SELECT REPLACE(CUST_NM,chr(13)||chr(10),' ')
		       FROM BKG_CUSTOMER 
		       WHERE BKG_NO = Y.BKG_NO
		       AND   BKG_CUST_TP_CD ='C'
		     ) AS CONSIGNEE

            /* 2009.12.28 수정 */
            , (SELECT ORG_PPD_RCV_CD||','||ORG_PPD_RCV_UPD_OFC_CD  FROM BKG_BL_ISS         
            WHERE BKG_NO = Y.BKG_NO) PPD_ORG0
            , (SELECT ORG_N3PTY_PPD_CD||','||ORG_N3PTY_PPD_UPD_OFC_CD  FROM BKG_BL_ISS
            WHERE BKG_NO = Y.BKG_NO) PPD_3RD0
            , (SELECT DEST_CLT_RCV_CD||','||(SELECT N3PTY_RCV_OFC_CD 
                                    					FROM BKG_CHG_RT RT
					                                    WHERE RT.BKG_NO =ISS.BKG_NO
					                                    AND FRT_TERM_CD ='P'
					                                    AND N3PTY_RCV_OFC_CD IS NOT NULL
					                                    AND ROWNUM =1 )  FROM BKG_BL_ISS ISS
            WHERE BKG_NO = Y.BKG_NO) CCT_DEST0
            , (SELECT ISS.DEST_N3PTY_CLT_CD||','||NVL((SELECT N3PTY_RCV_OFC_CD 
                                    					FROM BKG_CHG_RT RT
					                                    WHERE RT.BKG_NO =ISS.BKG_NO
					                                    AND FRT_TERM_CD ='P'
					                                    AND N3PTY_RCV_OFC_CD IS NOT NULL
					                                    AND ROWNUM =1 ),ISS.DEST_N3PTY_CLT_UPD_OFC_CD)  FROM BKG_BL_ISS ISS
            WHERE ISS.BKG_NO = Y.BKG_NO) CCT_3RD0

		    , Y.*
		    FROM ( SELECT   
							BK.BKG_NO AS BKG_NO ,
							BK.BL_NO||BK.BL_TP_CD AS BL_NO,
							BK.POR_CD AS POR_CD,
							BK.POL_CD AS POL_CD,
							BK.POD_CD AS POD_CD,
							BK.DEL_CD AS DEL_CD,
							BK.BKG_OFC_CD       AS BKG_OFC,
							BK.IB_SLS_OFC_CD    AS DEL_OFC,
							DOC.BL_OBRD_TP_CD   AS OB_TYPE,
							TO_CHAR(DOC.BL_OBRD_DT, 'YYYY-MM-DD')      AS OB_DATE,
                            --NVL(BK.BL_TP_CD,'B')         AS IR_BL_TYPE,
                            CASE WHEN BK.BL_TP_CD = 'W' THEN 'SWB'
                                -- WHEN (SELECT MAX(N1ST_PRN_DT||N2ND_PRN_DT) 
                                --         FROM BKG_INET_BL_PRN_AUTH 
                                --        WHERE BKG_NO = BK.BKG_NO
                                --          AND DELT_FLG = 'N') is not null THEN 'Web OB/L'
								WHEN ISS.OBL_INET_FLG = 'Y' THEN 'Web OB/L'
                                WHEN ISS.OBL_RLSE_FLG = 'Y' THEN 'OB/L'
                                ELSE 'B'
                            END AS IR_BL_TYPE,

                            ISS.OBL_ISS_OFC_CD  AS IR_OFFICE,
                            TO_CHAR(ISS.OBL_ISS_DT, 'YYYY-MM-DD')      AS IR_DATE,
                            ISS.OBL_ISS_USR_ID  AS IR_BY,
                            ISS.OBL_RDEM_OFC_CD AS ORS_OFFICE,
                            TO_CHAR(ISS.OBL_RDEM_DT, 'YYYY-MM-DD')     AS ORS_DATE,
                            /* 2009 10 06 수정*/
                            NVL(ISS.OBL_ISS_FLG,'N') BL_ISSUED,
                            /* 2012.07.24수정 */
                            CASE
                            	WHEN ISS.OBL_PRN_FLG = 'Y' THEN 'Y'
                            	WHEN (SELECT MAX(N1ST_PRN_DT||N2ND_PRN_DT)
                                      FROM BKG_INET_BL_PRN_AUTH
                                      WHERE BKG_NO = BK.BKG_NO
                                      AND DELT_FLG ='N') IS NOT NULL THEN 'Y'
                                ELSE 'N'
                            END BL_PRINT,
                            NVL(ISS.OBL_RLSE_FLG,'N') BL_RELEASED,
							NVL(ISS.OBL_RDEM_KNT,0)    AS ORS_NO,
							NVL(ISS.OBL_SRND_FLG,'N')    AS ORS_SURRENDER,
							(SELECT DECODE(COUNT(*),1,'Y','N')
							 FROM BKG_DO A
							 WHERE A.BKG_NO = BK.BKG_NO
								 AND ROWNUM = 1
							 ) AS ORS_DO,
							 BK.SI_FLG         AS BDI_SR,
							 NVL(ISS.BL_RDY_FLG,'N')  AS BDI_COMPLETE,
							 ISS.BL_RDY_TP_CD AS BDI_TYPE,
							 ISS.BL_RDY_OFC_CD AS BDI_OFFICE,
							 ISS.BL_RDY_USR_ID AS BDI_BY,
							 VVD.VSL_CD||VVD.SKD_VOY_NO||VVD.SKD_DIR_CD  AS VVD_CD,
							 (SELECT TO_CHAR(VPS_ETD_DT,'YYYY-MM-DD') 
                                                FROM VSK_VSL_PORT_SKD SKD 
                                                WHERE VVD.VSL_CD = SKD.VSL_CD 
                                                AND VVD.SKD_VOY_NO = SKD.SKD_VOY_NO 
                                                AND VVD.SKD_DIR_CD = SKD.SKD_DIR_CD 
                                                AND VVD.POL_CD = SKD.VPS_PORT_CD
                                                AND VVD.POL_CLPT_IND_SEQ = SKD.CLPT_IND_SEQ
                                                AND ROWNUM = 1) VVD_ETD,
							 (SELECT TO_CHAR(VPS_ETA_DT,'YYYY-MM-DD') 
                                                FROM VSK_VSL_PORT_SKD SKD 
                                                WHERE VVD.VSL_CD = SKD.VSL_CD 
                                                AND VVD.SKD_VOY_NO = SKD.SKD_VOY_NO 
                                                AND VVD.SKD_DIR_CD = SKD.SKD_DIR_CD 
                                                AND VVD.POD_CD = SKD.VPS_PORT_CD
                                                AND VVD.POD_CLPT_IND_SEQ = SKD.CLPT_IND_SEQ
                                                AND ROWNUM = 1) POD_ETA,
							 NVL((SELECT 'Y' from BKG_DOC_PROC_SKD WHERE BKG_NO = BK.BKG_NO AND BKG_DOC_PROC_TP_CD = 'CNTCFM' AND DOC_PERF_DELT_FLG = 'N'),'N') AS CNTR_CONFIRM,
							 BK.OB_SLS_OFC_CD  AS SALES_OFFICE,
							 BK.OB_SREP_CD     AS SALES_REP,
							
							/* 2009.12.28 수정 */
            			    DECODE(@[sc_rfa_cd],'SC',BK.SC_NO,BK.RFA_NO) AS SC_RFA_NO, 
			                (SELECT FRT_TERM_CD FROM BKG_RATE WHERE BKG_NO = VVD.BKG_NO ) AS PAY_TERM_CD,
							ISS.OBL_ISS_RMK,
                            ISS.BL_PRF_SHPR_FLG BDI_CONFIRM
					
						/* Duration Option */
						/* 2012.07.24수정 */
						#if(${dura_opt} == 'OS')
							/* OB/L Surrender*/
							FROM   BKG_VVD VVD, BKG_BOOKING BK, BKG_BL_DOC DOC, BKG_BL_ISS ISS,
								 BKG_DOC_PROC_TP DOC_TP, BKG_DOC_PROC_SKD DOC_SKD 
							WHERE 1=1
								AND BK.BKG_NO = DOC.BKG_NO
								AND BK.BKG_NO = ISS.BKG_NO(+)
								AND BK.BKG_NO = VVD.BKG_NO
								AND BK.BKG_STS_CD !='X'
								AND BK.BKG_CGO_TP_CD !='P'
								AND BK.BKG_NO = DOC_SKD.BKG_NO(+)
								AND DOC_SKD.BKG_DOC_PROC_TP_CD = DOC_TP.BKG_DOC_PROC_TP_CD(+)
						#elseif(${dura_opt} == 'BP')
							FROM   BKG_VVD VVD, BKG_BOOKING BK, BKG_BL_DOC DOC, BKG_BL_ISS ISS,
								 BKG_INET_BL_PRN_AUTH WEB 
							WHERE 1=1 
								AND BK.BKG_NO = DOC.BKG_NO
								AND BK.BKG_NO = ISS.BKG_NO(+)
								AND BK.BKG_NO = VVD.BKG_NO
								AND BK.BKG_STS_CD !='X'
								AND BK.BKG_CGO_TP_CD !='P'
								AND BK.BKG_NO = WEB.BKG_NO(+)
                                AND WEB.DELT_FLG(+) ='N'
						#elseif(${dura_opt} == 'VE' || ${dura_opt} == 'VA')
							FROM   BKG_VVD VVD, BKG_BOOKING BK, BKG_BL_DOC DOC, BKG_BL_ISS ISS,
								 VSK_VSL_PORT_SKD SKD
							WHERE 1=1
								AND BK.BKG_NO = DOC.BKG_NO
								AND BK.BKG_NO = ISS.BKG_NO(+)
								AND BK.BKG_NO = VVD.BKG_NO
								AND BK.BKG_STS_CD !='X'
								AND BK.BKG_CGO_TP_CD !='P'
								AND VVD.VSL_CD = SKD.VSL_CD
								AND VVD.SKD_VOY_NO = SKD.SKD_VOY_NO
								AND VVD.SKD_DIR_CD = SKD.SKD_DIR_CD

						#else
							FROM   BKG_VVD VVD, BKG_BOOKING BK, BKG_BL_DOC DOC, BKG_BL_ISS ISS
							WHERE 1=1
								AND BK.BKG_NO = DOC.BKG_NO
								AND BK.BKG_NO = ISS.BKG_NO(+)
								AND BK.BKG_NO = VVD.BKG_NO
								AND BK.BKG_STS_CD !='X'
								AND BK.BKG_CGO_TP_CD !='P'
						#end
						
						/* On Board */
						#if(${dura_opt} == 'OB')
								#if(${dura_from_dt} != '')
									AND DOC.BL_OBRD_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
								#end
								#if(${dura_to_dt} != '')
									AND DOC.BL_OBRD_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
								#end
						#end
						
						/* BKG Create */
						#if(${dura_opt} == 'BC')
								#if(${dura_from_dt} != '')
									AND BK.BKG_CRE_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
								#end
								#if(${dura_to_dt} != '')
									AND BK.BKG_CRE_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
								#end
						#end

						/* B/L Issue */
						#if(${dura_opt} == 'BI')
								#if(${dura_from_dt} != '')
									AND ISS.OBL_ISS_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
								#end
								#if(${dura_to_dt} != '')
									AND ISS.OBL_ISS_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
								#end
						#end

						/* OB/L Surrender */
						#if(${dura_opt} == 'OS')
								AND DOC_TP.BKG_DOC_PROC_TP_CD  = 'OBLSRD'
								
								#if(${dura_from_dt} != '')
									AND DOC_SKD.EVNT_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
								#end
								#if(${dura_to_dt} != '')
									AND DOC_SKD.EVNT_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
								#end
						#end
                        
                        /* web OB/L print */
						#if(${dura_opt} == 'BP')								
								#if(${dura_from_dt} != '')
									AND WEB.N1ST_PRN_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
								#end
								#if(${dura_to_dt} != '')
									AND WEB.N1ST_PRN_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
								#end
						#end
						
						/* OB/L Receive */
						#if(${dura_opt} == 'OR')
								#if(${dura_from_dt} != '')
									AND ISS.OBL_RDEM_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
								#end
								#if(${dura_to_dt} != '')
									AND ISS.OBL_RDEM_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
								#end	
						#end
						
						/* OB/L Receive */
						#if(${dura_opt} == 'VE')
								#if(${dura_from_dt} != '')
									AND SKD.VPS_ETD_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
                                    AND VVD.POL_CD = SKD.VPS_PORT_CD
								#end
								#if(${dura_to_dt} != '')
									AND SKD.VPS_ETD_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
                                    AND VVD.POL_CD = SKD.VPS_PORT_CD
								#end	
						#end
                        
                        /*  VVD ETA */
						#if(${dura_opt} == 'VA')
								#if(${dura_from_dt} != '')
									AND SKD.VPS_ETA_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
                                    AND VVD.POD_CD = SKD.VPS_PORT_CD
								#end
								#if(${dura_to_dt} != '')
									AND SKD.VPS_ETA_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
                                    AND VVD.POD_CD = SKD.VPS_PORT_CD
								#end	
						#end
                        
                        /* B/L confirm*/
						#if(${dura_opt} == 'CF')
								#if(${dura_from_dt} != '')
									AND ISS.BL_PRF_SHPR_DT >= TO_DATE(@[dura_from_dt],'YYYY-MM-DD')
								#end
								#if(${dura_to_dt} != '')
									AND ISS.BL_PRF_SHPR_DT <= TO_DATE(@[dura_to_dt],'YYYY-MM-DD') + 0.99999
								#end
						#end

						/* BL_TYPE Radio 처리*/
						/* Orignal */
						#if(${bl_type_ori} != '')
						/*ORIGINAL BL 이 데이터 이행시 NULL 로 들어가있음 */
						--AND NVL(BK.BL_TP_CD,'N') ='N'
						AND NVL(BK.BL_TP_CD,DECODE((SELECT MAX(N1ST_PRN_DT||N2ND_PRN_DT) 
                                                      FROM BKG_INET_BL_PRN_AUTH 
                                                     WHERE BKG_NO = BK.BKG_NO
                                                       AND DELT_FLG = 'N'),NULL,DECODE(ISS.OBL_RLSE_FLG,'Y','N','B'),'W/B')) = 'N' 
						#end
						/* Waybill */
						#if(${bl_type_way} != '')
						/*ORIGINAL BL 이 데이터 이행시 NULL 로 들어가있음 */
						AND NVL(BK.BL_TP_CD,'N') ='W' 
						#end
						/* Web O/BL */
						#if(${bl_type_web} != '')
						/*ORIGINAL BL 이 데이터 이행시 NULL 로 들어가있음 */
						AND NVL(BK.BL_TP_CD,DECODE((SELECT MAX(N1ST_PRN_DT||N2ND_PRN_DT) 
                                                      FROM BKG_INET_BL_PRN_AUTH 
                                                     WHERE BKG_NO = BK.BKG_NO 
                                                       AND DELT_FLG = 'N'),NULL,'N','W/B')) = 'W/B' 
						#end

						/* VVD */
						#if (${vvd_cd} != '') 
						 AND VVD.VSL_CD     = SUBSTR(@[vvd_cd], 1, 4)
						 AND VVD.SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
						 AND VVD.SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
						#end
						/* VVD 로 조회 하지 않을 경우 DATA 가 중복 되므로 TRUNK 만 잡아준다 */
						#if (${vvd_cd} == '') 
							AND VVD.VSL_PRE_PST_CD ='T'
						#end
						/* POL */
						#if(${pol_cd} != '')
						AND VVD.POL_CD = @[pol_cd]
						#end
						 /* POD */
						#if(${pod_cd} != '')
						AND VVD.POD_CD = @[pod_cd]
						#end   
						 /* POR */ 
						#if(${por_cd} != '')
						AND BK.POR_CD = @[por_cd]
						#end   

							/* DEL */
						#if(${del_cd} != '')
						AND BK.DEL_CD = @[del_cd]
						#end      

							/* DEL Control Office */
						#if(${del_ofc_cd} != '')
						AND BK.IB_SLS_OFC_CD = @[del_ofc_cd]
						#end      
							 
							/* OB/L Surrender Office */
						#if(${obl_ofc_cd} != '')
						AND ISS.OBL_RDEM_OFC_CD = @[obl_ofc_cd]
						#end      

							/* Booking Office*/
						#if(${bkg_ofc_cd} != '')
						AND BKG_OFC_CD in (${bkg_ofc_cd})
						#end      

							/* Sales Office */
						#if(${sal_ofc_cd} != '')
						AND BK.OB_SLS_OFC_CD = @[sal_ofc_cd]
						#end

							/* B/L Issue Office */
						#if(${bl_ofc_cd} != '')
						AND ISS.OBL_ISS_OFC_CD = @[bl_ofc_cd]
						#end

							/* OB/L Receive Office */
						#if(${obl_rcv_ofc_cd} != '')
						AND ISS.OBL_RDEM_OFC_CD = @[obl_rcv_ofc_cd]
						#end
		 

                            /*By*/
                        #if(${by_cd} == 'BS' && ${staff_id} != '')
                            /* Booking Staff*/
                            AND UPPER(BK.DOC_USR_ID) = UPPER(@[staff_id])
                        #end
                        #if(${by_cd} == 'SR' && ${staff_id} != '')
                            /* SALES REP*/              
                            AND UPPER(BK.OB_SREP_CD) = UPPER(@[staff_id])
                        #end
                        #if(${by_cd} == 'BR' && ${staff_id} != '')
                            /* BL READY */
                            AND UPPER(ISS.BL_RDY_USR_ID) = UPPER(@[staff_id])
                        #end
                        #if(${by_cd} == 'BI' && ${staff_id} != '')
                            /* B/L Issue */
                            AND UPPER(ISS.OBL_ISS_USR_ID) = UPPER(@[staff_id])
                        #end
                        #if(${by_cd} == 'OR' && ${staff_id} != '')
                            /*OBL Receive */
                            AND UPPER(ISS.OBL_RDEM_USR_ID) = UPPER(@[staff_id])
                        #end 

						#if( ${bkg_bl_cd} =='BKG' && ${bkg_bl_no} != '')
						 /* bkg_bl_no BKG_NO.... */
                          AND BK.BKG_NO = @[bkg_bl_no]
						  #if (${vvd_cd} == '' && ${pol_cd} =='' && ${pod_cd} =='' )
						  	AND VVD.VSL_PRE_PST_CD ='T'
						  #end	
						#end    
					    #if( ${bkg_bl_cd} =='BL' && ${bkg_bl_no} != '')
						 /* bkg_bl_no BL_NO.... */
						 AND BK.BL_NO = @[bkg_bl_no]
						  #if (${vvd_cd} == '' && ${pol_cd} =='' && ${pod_cd} =='' )
							 AND VVD.VSL_PRE_PST_CD ='T'
						  #end	
						#end 
                        #if(${dura_opt} == 'BP')
                                #if(${cust_cnt_cd} != '')
                                AND WEB.PRN_CUST_CNT_CD = @[cust_cnt_cd]
                                #end
                                #if(${cust_seq} != '')
                                AND WEB.PRN_CUST_SEQ =@[cust_seq]
                                #end
                        
                        #else
                            /* Customer */
                             #if(${cust_tp_cd} != '' || ${cust_cnt_cd} != '' || ${cust_seq} != '' || ${cust_nm} != '')
                                AND EXISTS (SELECT 'Y'
                                                        FROM BKG_CUSTOMER C
                                                        WHERE C.BKG_NO = BK.BKG_NO
                                                            #if(${cust_tp_cd} != '')
                                                            AND C.BKG_CUST_TP_CD = @[cust_tp_cd]
                                                            #end
                                                            #if(${cust_cnt_cd} != '')
                                                            AND C.CUST_CNT_CD =@[cust_cnt_cd]
                                                            #end
                                                            #if(${cust_seq} != '')
                                                            AND C.CUST_SEQ =@[cust_seq]
                                                            #end
                                                            #if(${cust_nm} != '')
                                                            AND C.CUST_NM LIKE '%'||@[cust_nm]||'%'
                                                            #end
                                                        ) 
                            #end
                        #end 


						/* 2009.12.28 수정 */
                        /* S/C RFA NO */
						#if(${sc_rfa_no} != '')
                        /* B/L Surrendered */
                         AND DECODE(@[sc_rfa_cd],'SC',BK.SC_NO,BK.RFA_NO) LIKE @[sc_rfa_no]||'%'
                        #end

                          /* B/L Status */
                        #if(${bl_sts_cd} == 'ISS')
                             /*B/L Issued */
                            AND NVL(ISS.OBL_ISS_FLG,'N') = 'Y'
                        #end
                        #if(${bl_sts_cd} == 'NIS')
                            /*B/L Non-Issued add-wonjoocho*/
                            AND NVL(ISS.OBL_ISS_FLG,'N') = 'N'
                        #end
						/* B/L Released  */
						#if(${bl_sts_cd} =='REL')
						AND ISS.OBL_RLSE_FLG ='Y'/* 아래의 조건이 맞으나 우선 수정 */
						/*AND EXISTS (SELECT 'Y'*/
								    /*FROM BKG_DOC_PROC_TP TP, BKG_DOC_PROC_SKD SKD1*/
									/*WHERE TP.BKG_DOC_PROC_TP_CD = SKD1.BKG_DOC_PROC_TP_CD*/
									/* AND SKD1.BKG_DOC_PROC_TP_CD ='OBLREL'*/
				   				    /*AND SKD1.BKG_NO = BK.BKG_NO)*/
						#end
						#if(${bl_sts_cd} =='NRL')
						AND ISS.OBL_RLSE_FLG ='N'
						#end
                        #if(${bl_sts_cd} == 'SUR')
                        /* B/L Surrendered */
                         AND ISS.OBL_SRND_FLG ='Y'
                        #end

                        #if(${bl_sts_cd} == 'REC')
                        /* B/L Received*/
                         AND ISS.OBL_RDEM_KNT > 0
                        #end 

						/* 2009.12.28 수정 */
						#if(${bl_sts_cd} == 'PRI')
                        /* B/L Printed */
                         AND ISS.OBL_PRN_FLG ='Y'
                        #end
						#if(${bl_sts_cd} == 'NPR')
                        /* B/L Non-Printed add-wonjoocho*/
                         AND ISS.OBL_PRN_FLG ='N'
                        #end
						#if(${bl_sts_cd} == 'DCP')
						/* B/L Completed */
						AND ISS.BL_RDY_FLG ='Y'
  						#end
                        #if(${bl_sts_cd} == 'NCP')
						/* B/L Non-Completed add-wonjoocho*/
						AND ISS.BL_RDY_FLG ='N'
  						#end
                        #if(${bl_sts_cd} == 'CFM')
						/* B/L Confirmed */
						AND ISS.BL_PRF_SHPR_FLG ='Y'
  						#end
                        #if(${bl_sts_cd} == 'NCF')
						/* B/L Non-confirmed */
						AND ISS.BL_PRF_SHPR_FLG ='N'
  						#end
                        
                        #if(${bl_sts_cd} == 'HLD')
						AND ISS.BL_HLD_FLG ='Y'
						#end

                        #if(${n3pty_ofc_cd} != '')
						AND EXISTS ( SELECT 'Y' FROM BKG_N3RD_PTY_BL_BIL_RQST WHERE BK.BKG_NO = BKG_NO AND N3PTY_OFC_CD = @[n3pty_ofc_cd])
						#end
			  ) Y
	       ) Z
        GROUP BY POD_ETA, CNTR_CONFIRM, VVD_ETD, SHIPPER,FOWARDER, CONSIGNEE,PPD_ORG0,PPD_3RD0,CCT_DEST0,CCT_3RD0, BKG_NO, BL_NO, POR_CD, POL_CD, POD_CD, DEL_CD, BKG_OFC, DEL_OFC, OB_TYPE, OB_DATE, IR_BL_TYPE, IR_OFFICE, IR_DATE, IR_BY, ORS_OFFICE, ORS_DATE, ORS_NO, ORS_SURRENDER, ORS_DO, BDI_SR, BDI_COMPLETE, BDI_OFFICE, BDI_TYPE, BDI_BY, VVD_CD, SALES_OFFICE, SALES_REP,SC_RFA_NO,BL_ISSUED,BL_PRINT,BL_RELEASED,PAY_TERM_CD,OBL_ISS_RMK,BDI_CONFIRM
        ORDER BY BKG_NO
        ) ZZ
    ) 
 WHERE RNUM BETWEEN NVL(@[rows_per_page],50) * (NVL(@[curr_page],1) - 1) + 1
           AND     NVL(@[rows_per_page],50) *  NVL(@[curr_page],1)			]]></sql>
