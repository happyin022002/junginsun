<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOsearchCustCmdtCntcInfoRSQL">
			<desc><![CDATA[searchCustCmdtCntcInfo]]></desc>
			<sql><![CDATA[
SELECT BKG.OFC_CD                              OFC_CD
     , BKG.FAX_NO                              FAX_NO
     , BKG.CNTC_EML                            CNTC_EML
     , BKG.CNTC_RMK                            CNTC_RMK
     , TO_CHAR(BKG.UPD_DT, 'YYYY-MM-DD HH:MI') UPD_DT
     , BKG.CUST_CNT_CD                         CUST_CNT_CD
     , BKG.CUST_SEQ                            CUST_SEQ    
     , BKG.CMDT_CNTC_SEQ                       CMDT_CNTC_SEQ
     , BKG.UPD_USR_ID                          UPD_USR_ID
     , USR.USR_NM                              USR_NM
  FROM BKG_IB_CMDT_CNTC     BKG
     , COM_USER             USR
 WHERE BKG.CUST_CNT_CD = @[cust_cnt_cd]
   AND BKG.CUST_SEQ = @[cust_seq]
   AND BKG.OFC_CD = @[ofc_cd]
   AND BKG.DELT_FLG = 'N'
   AND USR.USR_ID = BKG.UPD_USR_ID			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
