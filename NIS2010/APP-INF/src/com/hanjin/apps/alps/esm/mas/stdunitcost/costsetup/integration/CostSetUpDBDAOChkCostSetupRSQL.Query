<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostSetUpDBDAOChkCostSetupRSQL">
			<desc><![CDATA[MAS_MNL_COST_STUP에 해당 데이터 존재여부 확인]]></desc>
			<sql><![CDATA[
SELECT COUNT(COST_YRMON) AS CNT FROM MAS_MNL_COST_STUP
WHERE COST_YRMON = (SELECT COST_YR||EQ_WK FROM MAS_WK_PRD WHERE COST_YR = SUBSTR(REPLACE(@[cost_yrmon],'-',''),1,4) AND COST_WK = SUBSTR(REPLACE(@[cost_yrmon],'-',''),5,2))
    AND COST_WK = SUBSTR(REPLACE(@[cost_yrmon],'-',''),5,2)
	AND RLANE_CD = @[rlane_cd]			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
