<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchBkgChgCdRtCntRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchBkgChgCdRtCntRSQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(DISTINCT DECODE(CHG_CD,'SMC','OFT',
                                       'TSC','OFT',
                                       'DTS','OFT',
                                       'TXS','OFT',
                                       'WHC','OFT',
                                       'CPU','OFT',
                                       'MSC','OFT',
                                       'OCR','OFT',
                                       'LWS','OFT',
                                       'HWS','OFT',
                                       'CFC','OFT',
                                       'EMC','OFT',
                                       CHG_CD)  
            ) CNT
   FROM BKG_CHG_RT
  WHERE BKG_NO = @[bkg_no]
    AND FRT_INCL_XCLD_DIV_CD = 'N'
    AND CHG_CD IN('OFT','SMC','TSC','DTS','TXS','WHC','CPU','MSC','OCR','LWS','HWS','CFC','EMC')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
