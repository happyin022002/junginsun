<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQBalanceDBDAOMasCntrRepoShtgInfoVODSQL">
			<desc><![CDATA[* 2012.03.22 김종준 [CHM-201217091-01] EMU 관련 로직 보완/변경 검토 요청의 건 -MAS_CNTR_REPO_SHTG_INFO 테이블의 삭제]]></desc>
			<sql><![CDATA[
DELETE MAS_CNTR_REPO_SHTG_INFO
 WHERE COST_YRMON    = @[cost_yrmon]
   AND CNTR_TPSZ_CD   = REPLACE(@[cntr_tpsz_cd], 'RD', 'R')
   AND ECC_CD = @[ecc_cd]
   AND CNTR_ORG_DEST_CD = @[cntr_org_dest_cd]			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_org_dest_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
