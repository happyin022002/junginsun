<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchBlIssRqstInquiryRSQL">
			<desc><![CDATA[B/L 발급 신청 명세 화면 설정을 위한 Data를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_NO                                                                                                    
     , BII_VSL_NM                                                                     
     , BII_VVD                     
     , BL_ISS_RQST_DT
     , RQST_RCT_LOC_CD
     , SUB.LOC_NM
     , BL_NO
     , XTER_RQST_NO
     , XTER_RQST_SEQ
     , SUB.CRE_USR_ID
     , BL_ISS_STS_CD
     , BL_ISS_DT
     , BL_ISS_RJCT_DT
     , BL_ISS_REQ_DT
     , BL_ISS_RMK
     , RQST_CO_NM 
     , RQST_USR_EML
     , RQST_ATND_NM
     , RQST_PHN_NO
     , BL_RQST_RMK
     , ACT_SHPR_NM
     , ACT_SHPR_RGST_NO
     , TAX_INV_RCVR_CO_NM
     , TAX_INV_RCVR_RGST_NO
     , TAX_INV_RCVR_ATND_NM
     , TAX_INV_RCVR_PHN_NO
     , REMIT_CO_NM
     , TO_CHAR(CRR_REMIT_AMT,'FM999,999,999,999') CRR_REMIT_AMT
     , CRR_ACCT_CURR_CD
     , RQST_BL_TP_CD
     , CRR_BANK_NM
     , CRR_BANK_ACCT_NO
     , CRR_USA_BANK_NM
     , CRR_USA_BANK_ACCT_NO
     , TO_CHAR(CRR_USA_REMIT_AMT,'FM999,999,999,990.00') CRR_USA_REMIT_AMT
     , TO_CHAR(CRR_USA_REMIT_DT,'YYYY-MM-DD') CRR_USA_REMIT_DT
     , CRR_USA_ACCT_CURR_CD
	 ,(SELECT INTG_CD_VAL_DP_DESC
       FROM   COM_INTG_CD_DTL
       WHERE  INTG_CD_ID = 'CD02806'
       AND INTG_CD_VAL_CTNT = REMIT_KND_CD) REMIT_KND_CD
     , TO_CHAR(CRR_REMIT_DT,'YYYY-MM-DD') CRR_REMIT_DT
     , SUB.VSL_CD
     , SUB.SKD_VOY_NO
     , SUB.SKD_DIR_CD
     , VVD
     , POL_CD
     , RN 
     , TO_CHAR(SKD.VPS_ETD_DT, 'YYYY-MM-DD HH24:MI') VPS_ETD_DT
	 , DECODE(NVL(BL_ISS_RQST_CD,'W'),'W','Web','M','Mobile','Web') BL_ISS_RQST_CD
     , BL_RCV_TP_CD
     , MNL_BL_OBRD_DT
     , MNL_BL_ISS_DT
	 , CERTI_EXIST_FLG
	 , FRT_DP_FLG
	 , BL_OBRD_DT
FROM
(
SELECT SUB2.*, BNK.INTG_CD_VAL_DP_DESC CRR_USA_BANK_NM
FROM(
      SELECT SUB1.*
                  , BNK.INTG_CD_VAL_DP_DESC CRR_BANK_NM
      FROM
      (
          SELECT  B.BKG_NO                                                                                                    
                 , MDM.VSL_ENG_NM BII_VSL_NM                                                                     
                 , V.SKD_VOY_NO||V.SKD_DIR_CD BII_VVD                     
                 , TO_CHAR(EBL.BL_ISS_RQST_DT, 'YYYY-MM-DD HH24:MI') BL_ISS_RQST_DT
                 , EBL.RQST_RCT_LOC_CD
                 , ORG.OFC_KRN_NM LOC_NM
                 , B.BL_NO
                 , EBL.XTER_RQST_NO
                 , EBL.XTER_RQST_SEQ
                 , EBL.CRE_USR_ID
                 , DECODE(B.BL_TP_CD,'W','A',EBL.BL_ISS_STS_CD) BL_ISS_STS_CD
                 , TO_CHAR(EBL.BL_ISS_ACT_DT, 'YYYY-MM-DD HH24:MI') BL_ISS_DT
                 , TO_CHAR(EBL.BL_ISS_RJCT_DT, 'YYYY-MM-DD HH24:MI') BL_ISS_RJCT_DT
                 , TO_CHAR(EBL.BL_ISS_REQ_DT, 'YYYY-MM-DD HH24:MI') BL_ISS_REQ_DT
                 , EBL.BL_ISS_RMK
                 , EBL.RQST_CO_NM 
                 , EBL.RQST_USR_EML
                 , EBL.RQST_ATND_NM
                 , EBL.RQST_PHN_NO
                 , EBL.BL_RQST_RMK
                 , EBL.ACT_SHPR_NM
                 , EBL.ACT_SHPR_RGST_NO
                 , EBL.TAX_INV_RCVR_CO_NM
                 , EBL.TAX_INV_RCVR_RGST_NO
                 , EBL.TAX_INV_RCVR_ATND_NM
                 , EBL.TAX_INV_RCVR_PHN_NO
                 , EBL.REMIT_CO_NM
                 , EBL.CRR_REMIT_AMT
                 , EBL.CRR_ACCT_CURR_CD
                 , EBL.CRR_BANK_CD
                 , EBL.CRR_BANK_ACCT_NO
                 , EBL.CRR_USA_BANK_CD
                 , EBL.CRR_USA_BANK_ACCT_NO
                 , EBL.CRR_USA_REMIT_AMT
                 , EBL.CRR_USA_REMIT_DT
                 , EBL.CRR_USA_ACCT_CURR_CD 
                 , V.VSL_CD
                 , V.SKD_VOY_NO
                 , V.SKD_DIR_CD
                 , V.VSL_CD||V.SKD_VOY_NO||V.SKD_DIR_CD VVD
                 , V.POL_CD
				 , EBL.REMIT_KND_CD
                 , EBL.CRR_REMIT_DT
                 , EBL.RQST_BL_TP_CD
                 , DENSE_RANK() OVER (PARTITION BY V.BKG_NO ORDER BY V.VSL_PRE_PST_CD||V.VSL_SEQ) RN
                 , EBL.BL_ISS_RQST_CD
                 , EBL.BL_RCV_TP_CD
     		     , TO_CHAR(EBL.MNL_BL_OBRD_DT,'YYYY-MM-DD') MNL_BL_OBRD_DT
     			 , TO_CHAR(EBL.MNL_BL_ISS_DT,'YYYY-MM-DD') MNL_BL_ISS_DT
	 			 , EBL.CERTI_EXIST_FLG
	 			 , EBL.FRT_DP_FLG
				 , TO_CHAR(BBD.BL_OBRD_DT,'YYYY-MM-DD') BL_OBRD_DT
            FROM  BKG_BOOKING B                                                                                
                 , BKG_CUSTOMER C 
                 , BKG_BL_DOC BBD 
                 , BKG_VVD V 
                 , MDM_VSL_CNTR MDM 
                 , BKG_BL_ISS ISS
                 , MDM_ORGANIZATION ORG
                 , BKG_BL_ISS_RQST EBL
            WHERE B.BKG_NO = C.BKG_NO
            AND B.BKG_NO = BBD.BKG_NO
            AND B.BKG_NO = V.BKG_NO
            AND B.VSL_CD = MDM.VSL_CD
            AND EBL.RQST_RCT_LOC_CD = ORG.OFC_CD
            AND B.BKG_NO = ISS.BKG_NO(+)
            AND B.BL_NO = EBL.BL_NO(+)
            AND B.BKG_STS_CD <> 'X'
            AND C.BKG_CUST_TP_CD = 'S'
            AND EBL.XTER_RQST_NO = @[xter_rqst_no]
            AND EBL.XTER_RQST_SEQ = @[xter_rqst_seq]
              ) SUB1
              , COM_INTG_CD_DTL BNK
            WHERE INTG_CD_ID(+) = 'CD02933'
            AND SUB1.CRR_BANK_CD = BNK.INTG_CD_VAL_CTNT(+)
            ) SUB2
            , COM_INTG_CD_DTL BNK
            WHERE INTG_CD_ID(+) = 'CD02933'
            AND SUB2.CRR_USA_BANK_CD = BNK.INTG_CD_VAL_CTNT(+)
        ) SUB
      , VSK_VSL_PORT_SKD SKD
  WHERE SUB.VSL_CD = SKD.VSL_CD
  AND SUB.SKD_VOY_NO = SKD.SKD_VOY_NO
  AND SUB.SKD_DIR_CD = SKD.SKD_DIR_CD
  AND SUB.POL_CD = SKD.VPS_PORT_CD
  AND SKD.CLPT_IND_SEQ = 1
  AND SUB.RN = 1			]]></sql>
			<params>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
