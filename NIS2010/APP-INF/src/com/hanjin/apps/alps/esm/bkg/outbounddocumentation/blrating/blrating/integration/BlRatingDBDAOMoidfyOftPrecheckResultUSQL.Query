<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOMoidfyOftPrecheckResultUSQL">
			<desc><![CDATA[OFT Rating 가능여부, 사용자, 날짜를 BKG RATE에 저장한다.]]></desc>
			<sql><![CDATA[
#if(${ca_flg} == 'Y') 

    UPDATE BKG_RT_HIS
       SET OFT_MSS_FLG = @[oft_mss_flg],
           OFT_CHK_USR_ID = @[usr_id],
           OFT_CHK_DT = sysdate
    WHERE BKG_NO = @[bkg_no]
      AND CORR_NO = 'TMP0000001'
      
#else

    UPDATE BKG_RATE
       SET OFT_MSS_FLG = @[oft_mss_flg],
           OFT_CHK_USR_ID = @[usr_id],
           OFT_CHK_DT = sysdate
    WHERE BKG_NO = @[bkg_no]
    
#end
			]]></sql>
			<params>
				<param name="oft_mss_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
