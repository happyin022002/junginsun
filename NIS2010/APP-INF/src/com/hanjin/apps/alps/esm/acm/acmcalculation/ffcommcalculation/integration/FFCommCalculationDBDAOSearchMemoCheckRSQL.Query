<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCommCalculationDBDAOSearchMemoCheckRSQL">
			<desc><![CDATA[FFCommCalculationDBDAOSearchMemoCheckRSQL]]></desc>
			<sql><![CDATA[
SELECT 
  CASE 
  WHEN BKG.BL_NO_TP = '9' 
  THEN 1 
  ELSE 0 
   END AS MEMO_CHECK 
  FROM BKG_BOOKING BKG 
 WHERE BKG.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
