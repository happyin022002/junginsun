<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOaddCNTRInqInfoCSQL">
			<desc><![CDATA[컨테이너 정보 INSERT]]></desc>
			<sql><![CDATA[
INSERT 
  INTO BKG_CSTMS_KR_CNTR 
     ( BKG_NO
     , CSTMS_DECL_TP_CD
     , TRNS_SEQ
     , CNTR_NO
     , FULL_MTY_CD
     , CNTR_SEAL_NO1
     , CNTR_SEAL_NO2
     , CNTR_TPSZ_CD
     , PCK_QTY
     , PCK_TP_CD
     , CNTR_WGT
     , WGT_UT_CD
     , MEAS_QTY
     , MEAS_UT_CD
     , DMST_PORT_CD
	 , CRE_USR_ID
     , UPD_USR_ID
     , CSTMS_BL_NO
     )
VALUES 
     ( @[kc_bkg_no]
     , @[kc_cstms_decl_tp_cd]
     , @[trns_seq]
     , @[kc_cntr_no]
     , @[kc_full_mty_cd]
     , @[kc_cntr_seal_no1]
     , @[kc_cntr_seal_no2]
     , @[kc_cntr_tpsz_cd]
     , @[kc_pck_qty]
     , @[kc_pck_tp_cd]
     , @[kc_cntr_wgt]
     , @[kc_wgt_ut_cd]
  	 , CASE WHEN @[kc_meas_ut_cd] = 'CBM' THEN  
         CASE WHEN SUBSTR(@[kc_cntr_tpsz_cd], 2,1) = '2' THEN 
           CASE WHEN TO_NUMBER(@[kc_meas_qty])*0.883 > 70 THEN 70 ELSE TO_NUMBER(@[kc_meas_qty]) END
  	   	 ELSE 
           CASE WHEN TO_NUMBER(@[kc_meas_qty])*0.883 > 140 THEN 140 ELSE TO_NUMBER(@[kc_meas_qty]) END
       	 END
       ELSE
    	TO_NUMBER(@[kc_meas_qty])
       END
     , @[kc_meas_ut_cd] 
     , @[port_cd]
	 , @[user_id]  
     , @[user_id]
     , @[bl_no]
     )			]]></sql>
			<params>
				<param name="kc_bkg_no" type="12" value="" out="N"/>
				<param name="kc_cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="trns_seq" type="12" value="" out="N"/>
				<param name="kc_cntr_no" type="12" value="" out="N"/>
				<param name="kc_full_mty_cd" type="12" value="" out="N"/>
				<param name="kc_cntr_seal_no1" type="12" value="" out="N"/>
				<param name="kc_cntr_seal_no2" type="12" value="" out="N"/>
				<param name="kc_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="kc_pck_qty" type="12" value="" out="N"/>
				<param name="kc_pck_tp_cd" type="12" value="" out="N"/>
				<param name="kc_cntr_wgt" type="12" value="" out="N"/>
				<param name="kc_wgt_ut_cd" type="12" value="" out="N"/>
				<param name="kc_meas_ut_cd" type="12" value="" out="N"/>
				<param name="kc_meas_qty" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
