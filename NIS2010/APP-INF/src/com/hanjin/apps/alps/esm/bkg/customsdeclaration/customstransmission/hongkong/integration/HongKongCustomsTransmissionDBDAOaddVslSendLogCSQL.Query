<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HongKongCustomsTransmissionDBDAOaddVslSendLogCSQL">
			<desc><![CDATA[홍콩세관에 Manifest 신고한 VSL 전송 History를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_HKG_SND_LOG
         (MF_SND_DT, CRE_DT, UPD_DT,
          VSL_CD,  SKD_VOY_NO, SKD_DIR_CD,
          MF_SND_SEQ, POL_CD, POD_CD, 
          CRE_USR_ID, UPD_USR_ID, SNDR_OFC_CD )
   VALUES(TO_DATE(@[mf_snd_dt],'YYYY-MM-DD HH24:MI:SS'), 
   	      sysdate, 
   	      sysdate, 
		 @[vsl_cd], @[skd_voy_no], @[skd_dir_cd], 
		 (SELECT	NVL(MAX(MF_SND_SEQ),0)
			FROM	BKG_CSTMS_HKG_SND_LOG
			WHERE	VSL_CD        = @[vsl_cd]
			AND	SKD_VOY_NO = @[skd_voy_no]
			AND	SKD_DIR_CD    = @[skd_dir_cd]) +1, @[pol_cd], @[pod_cd], 
		 @[user_id], @[user_id],@[ofc_cd] )			]]></sql>
			<params>
				<param name="mf_snd_dt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
