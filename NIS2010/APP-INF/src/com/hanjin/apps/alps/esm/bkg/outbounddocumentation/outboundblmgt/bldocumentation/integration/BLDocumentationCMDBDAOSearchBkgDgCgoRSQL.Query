<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOSearchBkgDgCgoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 'Seq'||DG_CNTR_SEQ||'-'||'Un No: '||IMDG_UN_NO||',Dcgo Seq : '||DCGO_SEQ|| ',Class : '||IMDG_CLSS_CD DIFF_RMK
      ,DCGO_SEQ 
      ,CNTR_NO
  FROM BKG_DG_CGO
 WHERE BKG_NO = @[bkg_no]
 ORDER BY CNTR_NO,DCGO_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
