<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQBalanceDBDAOSearchEQBalance0019ListVORSQL">
			<desc><![CDATA[MT 회송비 조회   ]]></desc>
			<sql><![CDATA[
SELECT  A.ECC_CD ECC_CD  
       ,A.CNTR_ORG_DEST_CD 
       ,A.CNTR_TPSZ_CD 	    
       ,A.MTY_STVG_UC_AMT 
       ,A.MTY_TRSP_UC_AMT 
       ,A.MTY_TZ_DYS 
  FROM MAS_MTY_ECC_CNTR_SMRY A, MAS_SPCL_REPO_CNTR_RGST C  
 WHERE A.COST_YRMON = @[f_cost_yrmon]  
   AND NVL(C.REPO_FLG,'N') = 'Y'  
   AND NVL(C.DELT_FLG,'N') = 'N'  
   AND A.CNTR_TPSZ_CD = C.CNTR_TPSZ_CD(+)  
   AND A.MTY_REPO_SIM_CD = 'S'  
   
   #if (${f_cntr_tpsz_cd} != '') 
   AND A.CNTR_TPSZ_CD = @[f_cntr_tpsz_cd]
   #end
   
   #if (${f_cost_loc_grp_cd} == 'E') 
     #if (${f_mty_ecc_cd} != '') 
     AND A.ECC_CD = @[f_mty_ecc_cd]
     #end
   #elseif (${f_cost_loc_grp_cd} == 'L') 
     #if (${f_mty_lcc_cd} != '') 
     AND A.ECC_CD = @[f_mty_lcc_cd]
     #end
   #elseif (${f_cost_loc_grp_cd} == 'R')   
     #if (${f_mty_rcc_cd} != '') 
     AND A.ECC_CD = @[f_mty_rcc_cd]
     #end
   #end
   
   #if (${f_cost_loc_grp_cd} != '') 
   AND A.COST_LOC_GRP_CD = @[f_cost_loc_grp_cd]
   #end
  
ORDER BY A.ECC_CD, A.CNTR_ORG_DEST_CD, A.CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
				<param name="f_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="f_mty_ecc_cd" type="12" value="" out="N"/>
				<param name="f_mty_lcc_cd" type="12" value="" out="N"/>
				<param name="f_mty_rcc_cd" type="12" value="" out="N"/>
				<param name="f_cost_loc_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
