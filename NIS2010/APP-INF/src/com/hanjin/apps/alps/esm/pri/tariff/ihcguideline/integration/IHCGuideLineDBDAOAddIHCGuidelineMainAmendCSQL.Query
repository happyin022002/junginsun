<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOAddIHCGuidelineMainAmendCSQL">
			<desc><![CDATA[Add IHC Guideline Main Amend]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_IHC_MN (
            SVC_SCP_CD
          , ORG_DEST_TP_CD  
          , IHC_TRF_NO
          , AMDT_SEQ
          , EFF_DT
          , EXP_DT
          , FIC_PROP_STS_CD
          , TRF_CURR_CD
          , LOCL_CURR_CD   
          , CRE_OFC_CD
          , CRE_USR_ID
          , CRE_DT
          , UPD_USR_ID
          , UPD_DT
          , USA_SCP_BND_FLG )
     SELECT SVC_SCP_CD
          , ORG_DEST_TP_CD
          , IHC_TRF_NO
          , @[amdt_seq]+1
          , TO_DATE(@[amdt_eff], 'YYYY-MM-DD')
          , TO_DATE('99991231', 'YYYYMMDD')
          , 'I' AS FIC_PROP_STS_CD
          , TRF_CURR_CD
          , LOCL_CURR_CD    
          , @[cre_ofc_cd]
          , @[cre_usr_id]
          , SYSDATE
          , @[upd_usr_id]
          , SYSDATE
          , USA_SCP_BND_FLG
       FROM PRI_TRF_IHC_MN   
      WHERE 1=1
        AND SVC_SCP_CD      = @[svc_scp_cd] 
        AND ORG_DEST_TP_CD  = @[org_dest_tp_cd]
        AND IHC_TRF_NO      = @[ihc_trf_no] 
        AND AMDT_SEQ        = @[amdt_seq]			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="amdt_eff" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
