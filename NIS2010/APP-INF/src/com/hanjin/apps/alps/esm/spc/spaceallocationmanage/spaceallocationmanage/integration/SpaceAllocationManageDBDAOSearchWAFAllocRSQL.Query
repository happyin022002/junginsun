<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOSearchWAFAllocRSQL">
			<desc><![CDATA[WAFIE노선 Allocation 을 조회한다.
History-----------------------------------
20110.08.12 [CHM-201112741-01] Control by HO/RHQ 화면 보완 2차 - SLS_OFC_CD가 HAMRU,NYCRA로 Allocation된 내용을 조회]]></desc>
			<sql><![CDATA[
SELECT  RLANE_CD
      , DIR_CD
      , VSL_CD
      , SKD_VOY_NO
      , SKD_DIR_CD
      , SLS_OFC_CD
      , POL_YD_CD AS POL_CD
      , POD_YD_CD AS POD_CD
      , TS_FLG
      , MNL_FLG
      , TRD_CD
      , SUB_TRD_CD
      , REP_TRD_CD
      , REP_SUB_TRD_CD
      , DECODE(TS_FLG,'Y','T',IOC_CD) IOC_CD
      , ASGN_TTL_QTY
      , ASGN_TTL_WGT
      , ASGN_40FT_HC_QTY
      , ASGN_45FT_HC_QTY
      , ASGN_53FT_QTY
      , ASGN_RF_QTY
      , DEST_LOC_CD AS DEL_CD
      , ASGN_20FT_DRY_QTY
      , ASGN_40FT_DRY_QTY
      , ASGN_RD_QTY
      , CUST_CNT_CD
      , CUST_SEQ
	  , CTRT_NO 
      , USA_BKG_MOD_CD
  FROM  SPC_ALOC_POL_POD
 WHERE  1=1
   AND  RLANE_CD   = @[lane]
   AND  DIR_CD     = @[bound]
   AND  VSL_CD     = @[vsl_cd]
   AND  SKD_VOY_NO = @[skd_voy_no]
   AND  SKD_DIR_CD = @[skd_dir_cd]
   AND  SLS_OFC_CD IN ('HAMRU', 'NYCRA')
   -- AND	POL_YD_CD  = 'XXXXXXX'
   AND	DEST_LOC_CD  = 'XXXXX'			]]></sql>
			<params>
				<param name="lane" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
