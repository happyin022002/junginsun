<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostAssignDBDAOSearchListAssignRSQL">
			<desc><![CDATA[BATCH Code별 개수 구하기]]></desc>
			<sql><![CDATA[
/*
---------------------------------------------------------------------------
--//searchListAssign ENIS=SCE_COP_HDR에서 ALPS=MAS_BKG_COST_CALC로 테이블 변경
---------------------------------------------------------------------------
*/
SELECT 
       MAS_BAT_CD 
       ,COUNT(*) CNT 
FROM MAS_BKG_COST_CALC
#if (${f_mas_bat_cd} != '') 
WHERE MAS_BAT_CD IN ('${f_mas_bat_cd}')
#end
GROUP BY MAS_BAT_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
