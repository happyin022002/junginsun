<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchBlCustomerInfoRSQL">
			<desc><![CDATA[B/L정보를 조회한다.
1. 2011.01.10 이수진 [CHM-201007774] EUR 24HR 관련 구주 세관 시스템 MRN 정보 추가 요청
   : IND_AGREE, VALUE_AGREE 정보 추가 (RFA/TAA No. 정보)]]></desc>
			<sql><![CDATA[
SELECT 
BKG_GET_TOKEN_FNC(SHPR_CNEE_NTFY_TXID,1) SHPRTXID,
BKG_GET_TOKEN_FNC(SHPR_CNEE_NTFY_TXID,2) CNEETXID,
BKG_GET_TOKEN_FNC(SHPR_CNEE_NTFY_TXID,3) NTFYTXID,
X.*
FROM (
SELECT   
       ( SELECT NVL(MAX(I_S),MAX(O_S))||','||NVL(MAX(I_C),MAX(O_C))||','||NVL(MAX(I_N),MAX(O_N))
           FROM ( SELECT DECODE(IO_BND_CD ,'I', DECODE(MAX(CNT_CD), 'TR', MAX(TR_SHPR_TAX_ID), 'IL',MAX(IL_SHPR_TAX_ID), 'LB',MAX(LB_SHPR_TAX_ID), ''), '') I_S
                       , DECODE(IO_BND_CD ,'O', DECODE(MAX(CNT_CD), 'TR', MAX(TR_SHPR_TAX_ID), 'IL',MAX(IL_SHPR_TAX_ID), 'LB',MAX(LB_SHPR_TAX_ID), ''), '') O_S
                         
                       , DECODE(IO_BND_CD ,'I', DECODE(MAX(CNT_CD), 'TR', MAX(TR_CNEE_TAX_ID), 'IL',MAX(IL_CNEE_TAX_ID), 'LB',MAX(LB_CNEE_TAX_ID), ''), '') I_C
                       , DECODE(IO_BND_CD ,'O', DECODE(MAX(CNT_CD), 'TR', MAX(TR_CNEE_TAX_ID), 'IL',MAX(IL_CNEE_TAX_ID), 'LB',MAX(LB_CNEE_TAX_ID), ''), '') O_C
                        
                       , DECODE(IO_BND_CD ,'I', DECODE(MAX(CNT_CD), 'TR', MAX(TR_NTFY_TAX_ID), 'IL',MAX(IL_NTFY_TAX_ID), 'LB',MAX(LB_NTFY_TAX_ID), ''), '') I_N
                       , DECODE(IO_BND_CD ,'O', DECODE(MAX(CNT_CD), 'TR', MAX(TR_NTFY_TAX_ID), 'IL',MAX(IL_NTFY_TAX_ID), 'LB',MAX(LB_NTFY_TAX_ID), ''), '') O_N
                    FROM BKG_XPT_IMP_LIC L
                   WHERE BKG_NO = @[bkg_no]
                     AND IO_BND_CD IN ('I','O')
                     AND CNT_CD IN (SUBSTR(@[pol_cd], 1, 2), SUBSTR(@[pod_cd], 1, 2))
                     AND  XPT_IMP_SEQ = ( SELECT MAX(XPT_IMP_SEQ)
                                            FROM BKG_XPT_IMP_LIC
                                           WHERE BKG_NO    = L.BKG_NO
                                             AND IO_BND_CD = L.IO_BND_CD
                                             AND CNT_CD IN (SUBSTR(@[pol_cd], 1, 2), SUBSTR(@[pod_cd], 1, 2))
                                         )
                GROUP BY IO_BND_CD        
               )
       ) AS SHPR_CNEE_NTFY_TXID

         ,BK.BL_NO || DECODE(BK.BL_TP_CD, 'W', BK.BL_TP_CD)			   BLNBR                                               	
         ,BK.POL_CD       					   BLPOL                                                	
         ,LOC1.LOC_AMS_PORT_CD                 POL_AMS                                            		
         ,LOC1.LOC_NM                          POL_FULLNAME                                       		
         ,BK.POD_CD       					   BLPOD                                                	
         ,LOC2.LOC_AMS_PORT_CD                 POD_AMS                                           			
         ,LOC2.LOC_NM                          POD_FULLNAME                                       		
         ,BK.POR_CD       					   BLPOR                                                	
         ,LOC3.LOC_AMS_PORT_CD                 POR_AMS                                             		
         ,LOC3.LOC_NM                          POR_FULLNAME                                       		
         ,BK.DEL_CD       					   BLDEL                                                	
         ,LOC4.LOC_AMS_PORT_CD                 DEL_AMS                                         		
         ,LOC4.LOC_NM                          DEL_FULLNAME                                       		
         ,DECODE(NVL(@[pol_cd],''), '', NVL(LOC5.UN_LOC_CD, BK.PST_RLY_PORT_CD), NVL(LOC5.UN_LOC_CD, BK.PRE_RLY_PORT_CD)) BLRLY		
         ,LOC5.LOC_AMS_PORT_CD RLY_AMS                                               							  
         ,LOC5.LOC_NM        RLY_FULLNAME                                                          		            
         ,OFC.LOC_CD         BLPLACE                                                                 	
		 ,NVL(TO_CHAR(BBI.OBL_ISS_DT,'RRMMDD'),' ') BLDATE                                                               	
         ,BCS.CUST_CNT_CD    SHPRCN                                                                  	
         ,BCS.CUST_SEQ       SHPRCD                                                                  	
         ,SCE_TOKEN_NL_FNC(BCS.CUST_NM, 1)        SHPR1                                                                   	
         ,SCE_TOKEN_NL_FNC(BCS.CUST_NM, 2)        SHPR2                                                                   	
         ,SCE_TOKEN_NL_FNC(BCS.CUST_ADDR, 1)      SHPR3                                                                   	
         ,SCE_TOKEN_NL_FNC(BCS.CUST_ADDR, 2)      SHPR4                                                                   	
         ,SCE_TOKEN_NL_FNC(BCS.CUST_ADDR, 3)      SHPR5                                                                   	
         ,BCC.CUST_CNT_CD    CNEECN                                                                  	
         ,BCC.CUST_SEQ       CNEECD                                                                  	
         ,SCE_TOKEN_NL_FNC(BCC.CUST_NM, 1)        CNEE1                                                                   	
         ,SCE_TOKEN_NL_FNC(BCC.CUST_NM, 2)        CNEE2                                                                   	
         ,SCE_TOKEN_NL_FNC(BCC.CUST_ADDR, 1)      CNEE3                                                                   	
         ,SCE_TOKEN_NL_FNC(BCC.CUST_ADDR, 2)      CNEE4                                                                   	
         ,SCE_TOKEN_NL_FNC(BCC.CUST_ADDR, 3)      CNEE5                                                                   	
         ,BCN.CUST_CNT_CD    NTFYCN   
         ,BCN.CUST_SEQ       NTFYCD                                                                  	
         ,SCE_TOKEN_NL_FNC(BCN.CUST_NM, 1)        NTFY1                                                                   	
         ,SCE_TOKEN_NL_FNC(BCN.CUST_NM, 2)        NTFY2                                                                   	
         ,SCE_TOKEN_NL_FNC(BCN.CUST_ADDR, 1)      NTFY3                                                                   	
         ,SCE_TOKEN_NL_FNC(BCN.CUST_ADDR, 2)      NTFY4                                                                   	
         ,SCE_TOKEN_NL_FNC(BCN.CUST_ADDR, 3)      NTFY5                                                                   	
         ,BCA.CUST_CNT_CD    NTFY2CN                                                                 	
         ,BCA.CUST_SEQ      NTFY2CD                                                                  	
         ,SCE_TOKEN_NL_FNC(BCA.CUST_NM, 1)       NTFY21                                                                   	
         ,SCE_TOKEN_NL_FNC(BCA.CUST_NM, 2)       NTFY22                                                                   	
         ,SCE_TOKEN_NL_FNC(BCA.CUST_NM, 3)       NTFY23                                                                   	
         ,SCE_TOKEN_NL_FNC(BCA.CUST_NM, 4)       NTFY24                                                                   	
         ,SCE_TOKEN_NL_FNC(BCA.CUST_NM, 5)       NTFY25                                                                   	
         ,BCF.CUST_CNT_CD   FFWDCN                                                                   	
         ,BCF.CUST_SEQ      FFWDCD                                                                   	
         ,SCE_TOKEN_NL_FNC(BCF.CUST_NM, 1)       FFWD1                                                                   	
         ,SCE_TOKEN_NL_FNC(BCF.CUST_NM, 2)       FFWD2                                                                    	
         ,SCE_TOKEN_NL_FNC(BCF.CUST_NM, 3)     	 FFWD3                                                                    	
         ,SCE_TOKEN_NL_FNC(BCF.CUST_NM, 4)       FFWD4                                                                    	
         ,SCE_TOKEN_NL_FNC(BCF.CUST_NM, 5)       FFWD5                                                                    	
         ,''             EXPOCN                          
         ,''             EXPOCD
         
         ,SCE_TOKEN_NL_FNC(BCE.CUST_NM, 1)       EXPO1
         ,SCE_TOKEN_NL_FNC(BCE.CUST_NM, 2)       EXPO2
         ,SCE_TOKEN_NL_FNC(BCE.CUST_NM, 3)       EXPO3
         ,SCE_TOKEN_NL_FNC(BCE.CUST_NM, 4)       EXPO4
         ,SCE_TOKEN_NL_FNC(BCE.CUST_NM, 5)       EXPO5
                                                                         		
         ,BBI.BL_CPY_KNT            BLCOPY  
         ,'1'                       BLORG                                                                 	
         ,BBD.PCK_QTY               BLPKG                                                                    	
         ,BBD.PCK_TP_CD             BLPKGU  
         
        ,NVL(BBD.ACT_WGT, 0)        BLWGT                                                                    	
        ,NVL(BBD.MEAS_QTY, 0)       BLMEA                                                                    	

         ,BBD.WGT_UT_CD             BL_WGT_UNIT                                                              	
         ,BBD.MEAS_UT_CD            BL_MEA_UNIT                                                              	

         ,NVL(BK.RCV_TERM_CD, '') || NVL(BK.DE_TERM_CD, '') RDTYPE   
                                                                         	
         ,BK.BKG_CGO_TP_CD          CARGOTYPE                                                                 
         ,NVL(BK.CMDT_CD, '')       COMMODITY
         ,@[cmdt_nm]       BLCMD
         
         ,BK.REP_CMDT_CD    BLREPCMDCD
         ,@[rep_cmdt_nm]    BLREPCMD
         ,TRANSLATE(NVL(bk.XTER_RMK,' '),CHR(13)||CHR(10),' ') REMARK
         ,'' AUS_QUAR -- B.bkg_aus_no 
         ,'' SRNBR
         ,NVL(BK.BKG_NO, '') BKGNBR
         ,'' RGN_BKGNBR -- bkg_psa_no 

         
         
         ,NVL(BR.PPD_RCV_OFC_CD, '')    PPDOFC                                                                   	
         ,NVL(BR.CLT_OFC_CD, '')        CCTOFC                                                                   	
         ,''                            THDOFC                                              							
         ,NVL(BK.SC_NO, '')             SCNO                                                                     	
         ,NVL(BK.RFA_NO, '')            RFANO        
         
         ,''                            WAYBILL_IND
         ,''                            CUSTREF_NUM
         ,''                            FINAL_ETA
         ,''                            FUNC_CODE
         ,NVL(TO_CHAR(BBD.BL_OBRD_DT,'YYYYMMDDHH24MI'),'') ONBOARD
         ,''                            INV_NO
         
		 ,NVL(BK.BL_NO,' ') IN_BL_NO
		 ,NVL(BK.BKG_NO,' ') IN_BKG_NO                                            
		 ,NVL(BK.BKG_CGO_TP_CD,' ') IN_BKG_CGO_TP_CD                              
		 ,NVL(BK.DCGO_FLG,' ') IN_DCGO_FLG                                        
		 ,NVL(BK.RC_FLG,' ') IN_RC_FLG                                            
		 ,NVL(BK.AWK_CGO_FLG,' ') IN_AWK_CGO_FLG                                  
		 ,NVL(BK.BB_CGO_FLG,' ') IN_BB_CGO_FLG                                    
		 ,NVL(BK.RD_CGO_FLG,' ') IN_RD_CGO_FLG                                    
		 ,NVL(BK.CMDT_CD,' ') IN_CMDT_CD                                         
		 ,@[cmdt_nm] IN_CMDT_DESC   
         ,DECODE(RFA_NO, NULL, DECODE(TAA_NO, NULL, NULL, 'TAA'), 'RFA') IND_AGREE
         ,DECODE(RFA_NO, NULL, TAA_NO, RFA_NO) VALUE_AGREE                                                       
  FROM   
        BKG_VVD BV
        ,BKG_BOOKING BK                                                                               
        ,BKG_BL_ISS  BBI                                                                              
        ,BKG_BL_DOC  BBD                                                                              
        ,BKG_RATE    BR                                                                               
        ,BKG_CUSTOMER BCS                                                                             
        ,BKG_CUSTOMER BCC                                                                             
        ,BKG_CUSTOMER BCN                                                                             
        ,BKG_CUSTOMER BCF                                                                             
        ,BKG_CUSTOMER BCA                                                                             
        ,BKG_CUSTOMER BCE                                                                             
        ,BKG_CSTMS_CMDT   CMD                                                                         
        ,MDM_LOCATION     LOC0                                                                        
        ,MDM_LOCATION     LOC1                                                                        
        ,MDM_LOCATION     LOC2                                                                        
        ,MDM_LOCATION     LOC3                                                                        
        ,MDM_LOCATION     LOC4                                                                        
        ,MDM_LOCATION     LOC5                                                                        
        ,MDM_ORGANIZATION OFC                                                                         
        ,MDM_COMMODITY    COM                                                                         
 WHERE   BK.BKG_NO          =   @[bkg_no]
   AND   BK.BKG_NO          =   BV.BKG_NO
   AND   BK.BKG_NO          =   BBI.BKG_NO      (+)
   AND   BK.BKG_NO          =   BBD.BKG_NO      (+)
   AND   BK.BKG_NO          =   BR.BKG_NO       (+)
   AND   BK.BKG_NO          =   BCS.BKG_NO      (+)
   AND   BCS.BKG_CUST_TP_CD =   'S'
   AND   BK.BKG_NO          =   BCC.BKG_NO      (+)
   AND   BCC.BKG_CUST_TP_CD(+) =   'C'
   AND   BK.BKG_NO          =   BCN.BKG_NO      (+)
   AND   BCN.BKG_CUST_TP_CD(+) =   'N'
   AND   BK.BKG_NO          =   BCF.BKG_NO      (+)
   AND   BCF.BKG_CUST_TP_CD(+) =   'F'
   AND   BK.BKG_NO          =   BCA.BKG_NO      (+)
   AND   BCA.BKG_CUST_TP_CD(+) =   'A'
   AND   BK.BKG_NO          =   BCE.BKG_NO      (+)
   AND   BCE.BKG_CUST_TP_CD(+) =   'E'
   
   AND   LOC0.LOC_CD(+)     =   DECODE(NVL(@[pol_cd],''), '', BV.POL_CD, BV.POD_CD)
   AND   BK.POR_CD         =   LOC3.LOC_CD  (+)
   AND   BK.POL_CD         =   LOC1.LOC_CD  (+)
   AND   BK.POD_CD         =   LOC2.LOC_CD  (+)
   AND   BK.DEL_CD         =   LOC4.LOC_CD  (+)
   AND   LOC5.LOC_CD(+)    = DECODE(NVL(@[pol_cd],''), '', NVL(BK.PST_RLY_PORT_CD,' '),NVL(BK.PRE_RLY_PORT_CD,' '))
   AND   BBI.OBL_ISS_OFC_CD      = OFC.OFC_CD(+)
   AND   BK.CMDT_CD         = COM.CMDT_CD(+)
   AND   BK.CMDT_CD         = CMD.MF_CMDT_CD(+)
   AND   ROWNUM = 1
) X			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="cmdt_nm" type="12" value="" out="N"/>
				<param name="rep_cmdt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
