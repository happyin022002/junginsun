<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchOfcFullCntrRlseOrdBracUpdFlgRSQL">
			<desc><![CDATA[SearchOfcFullCntrRlseOrdBracUpdFlg]]></desc>
			<sql><![CDATA[
SELECT 'Y' 
  FROM BKG_HRD_CDG_CTNT 
  WHERE 1=1 
     AND HRD_CDG_ID= 'BKG_EUR_RLS_UPD_FLG' 
     AND ATTR_CTNT1 = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
