<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USDomesticDBDAOModifyEmptyRepoCreditUSQL">
			<desc><![CDATA[변경된 Empty Repo Credit Value 를 Update 한다.
2013.05.31 김수정 [CHM-201324859] MAS Domestic Saving Credit 보완]]></desc>
			<sql><![CDATA[
UPDATE MAS_USA_DMST_UT_COST
   SET SIM_MTY_UC_AMT      = REPLACE(@[sim_mty_uc_amt], ',', '')
     , USA_DMST_SAV_UT_AMT = REPLACE(@[usa_dmst_sav_ut_amt], ',', '')
     , EQ_OFFH_SAV_AMT     = REPLACE(@[eq_offh_sav_amt], ',', '')
     , EQ_OFFH_SAV_UC_AMT  = REPLACE(@[eq_offh_sav_uc_amt], ',', '')
     , EQ_OFFH_FNL_UC_AMT  = REPLACE(@[eq_offh_fnl_uc_amt], ',', '')
     , TRP_SAV_AMT         = REPLACE(@[trp_sav_amt], ',', '')
     , TRP_CR_UC_AMT       = REPLACE(@[trp_cr_uc_amt], ',', '')
	 , UPD_USR_ID          = @[upd_usr_id]
	 , UPD_DT              = SYSDATE
 WHERE 1=1
   AND COST_YRMON       = REPLACE(@[cost_yrmon], '-', '')
   AND ORG_RAIL_LOC_CD  = @[org_rail_loc_cd]
   AND COST_LOC_GRP_CD  = 'E'
   AND CNTR_TPSZ_CD       = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="sim_mty_uc_amt" type="12" value="" out="N"/>
				<param name="usa_dmst_sav_ut_amt" type="12" value="" out="N"/>
				<param name="eq_offh_sav_amt" type="12" value="" out="N"/>
				<param name="eq_offh_sav_uc_amt" type="12" value="" out="N"/>
				<param name="eq_offh_fnl_uc_amt" type="12" value="" out="N"/>
				<param name="trp_sav_amt" type="12" value="" out="N"/>
				<param name="trp_cr_uc_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="org_rail_loc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
