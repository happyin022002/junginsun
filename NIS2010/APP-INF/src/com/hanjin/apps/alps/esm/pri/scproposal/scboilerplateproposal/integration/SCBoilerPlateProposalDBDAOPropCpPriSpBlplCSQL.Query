<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateProposalDBDAOPropCpPriSpBlplCSQL">
			<desc><![CDATA[Proposal Copy PRI_SP_BLPL Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_BLPL (
      PROP_NO
    , AMDT_SEQ
    , BLPL_SEQ
    , PRC_PROG_STS_CD
    , SRC_INFO_CD
    , BLPL_TIT_NM
    , DP_SEQ
    --, N1ST_CMNC_DT
    , N1ST_CMNC_AMDT_SEQ
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
    )
SELECT PROP_NO
     , AMDT_SEQ
     , BLPL_SEQ
     , PRC_PROG_STS_CD
     , SRC_INFO_CD
     , BLPL_TIT_NM
     , DP_SEQ
     --, N1ST_CMNC_DT
     , 0 AS N1ST_CMNC_AMDT_SEQ
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
FROM (
    SELECT @[new_prop_no] AS PROP_NO
         , 0 AS AMDT_SEQ
         , DENSE_RANK() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ
                              ORDER BY A.PROP_NO, A.AMDT_SEQ, A.DP_SEQ, A.BLPL_SEQ) AS BLPL_SEQ
         , DENSE_RANK() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.BLPL_SEQ
                              ORDER BY A.PROP_NO, A.AMDT_SEQ, A.BLPL_SEQ, B.BLPL_CTNT_SEQ) AS SEQ
         , 'I' AS PRC_PROG_STS_CD
         , 'PC' AS SRC_INFO_CD
         , A.BLPL_TIT_NM
         , A.DP_SEQ
         --, TO_DATE(TO_CHAR(SYSDATE, 'YYYYMMDD'), 'YYYYMMDD') AS N1ST_CMNC_DT
         --, 0 AS N1ST_CMNC_AMDT_SEQ
         , @[cre_usr_id] AS CRE_USR_ID
         , SYSDATE AS CRE_DT
         , @[upd_usr_id] AS UPD_USR_ID
         , SYSDATE AS UPD_DT
    FROM PRI_SP_BLPL A
        ,PRI_SP_BLPL_CTNT B
    WHERE A.PROP_NO = @[prop_no]
    AND   A.AMDT_SEQ = @[amdt_seq]
    AND   A.SRC_INFO_CD <> 'AD'
    AND   B.PROP_NO = A.PROP_NO
    AND   B.AMDT_SEQ = A.AMDT_SEQ
    AND   B.BLPL_SEQ = A.BLPL_SEQ
    AND   B.SRC_INFO_CD <> 'AD'
)
WHERE SEQ = 1			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
