		<query name="CostStructureDBDAOSearchCostStructure0137ListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT
       'R' IBFLAG
      ,COLUMN_NAME COLNAME
      ,'=' INEQUALITY
      ,NULL VALUE
  FROM ALL_TAB_COLUMNS
 WHERE
#if (${f_table_name} != 'MAS_LNK_AVG_STND_COST')
   TABLE_NAME = 'MAS_NOD_AVG_STND_COST'
ORDER BY DECODE(COLUMN_NAME,'COST_ACT_GRP_CD', 5, COLUMN_ID)
#else 
    TABLE_NAME = 'MAS_LNK_AVG_STND_COST'
   AND COLUMN_NAME <> 'CO_CD'
ORDER BY DECODE(COLUMN_NAME,'VNDR_SEQ', 6.1,'COST_ACT_GRP_CD', 6.2, COLUMN_ID)
#end			]]></sql>
