<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingsplitCombineDBDAOSearchCodPctlNoRSQL">
			<desc><![CDATA[cod의 변경된 route의 pctl_no를 조회한다.]]></desc>
			<sql><![CDATA[
select pctl_no
  from bkg_cod
 where bkg_no = @[bkg_no]
   and cod_rqst_seq = @[cod_rqst_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
