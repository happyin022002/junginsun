<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpAmdtSmryAmdVOCSQL">
			<desc><![CDATA[RFAProposalMainDBDAOPriRpScpAmdtSmryVOCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO pri_Rp_amdt_smry(
    PROP_NO,
    AMDT_SEQ,
    PROP_TERM_TP_CD,
    AMDT_FLG,
    ACPT_FLG,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
SELECT
    prop_no	            ,
    amdt_seq+1          ,
    prop_term_tp_cd     ,
    'N'                 ,
    'N'                 ,
    @[cre_usr_id]       ,
    sysdate             ,
    @[upd_usr_id]       ,
    sysdate             
FROM 
    pri_Rp_amdt_smry
WHERE     
    prop_no  = @[prop_no]
AND amdt_seq = @[amdt_seq]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
