<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCTransportationAdditionalChargeProposalDBDAOExclPriSpScpTrspAddChgRSQL">
			<desc><![CDATA[excel upload시 PRI_SP_SCP_TRSP_ADD_CHG 테이블 조회]]></desc>
			<sql><![CDATA[
SELECT 
	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	ADD_CHG_TP_CD
,	ORG_DEST_TP_CD
,	ADD_CHG_SEQ
,	ROUT_PNT_LOC_TP_CD
,	ROUT_PNT_LOC_DEF_CD
,	LOC_GRD_CNT_CD
,	LOC_GRD_CD
,	BSE_PORT_TP_CD
,	BSE_PORT_DEF_CD
,	VIA_PORT_TP_CD
,	VIA_PORT_DEF_CD
,	DECODE(DIR_CALL_FLG, 'Y', '1', '0') AS DIR_CALL_FLG
,	RAT_UT_CD
,	PRC_CGO_TP_CD
,	PRC_TRSP_MOD_CD
,	RCV_DE_TERM_CD
,	PRC_CMDT_TP_CD
,	PRC_CMDT_DEF_CD
,	CURR_CD
FROM PRI_SP_SCP_TRSP_ADD_CHG
WHERE	PROP_NO = @[prop_no]
AND	AMDT_SEQ = @[amdt_seq]
AND	SVC_SCP_CD = @[svc_scp_cd]
AND	ADD_CHG_TP_CD = @[add_chg_tp_cd]
AND	ORG_DEST_TP_CD = @[org_dest_tp_cd]
ORDER BY ADD_CHG_SEQ			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="add_chg_tp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
