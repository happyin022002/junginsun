<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltRpScpServiceScopeListVORSQL">
			<desc><![CDATA[service scope]]></desc>
			<sql><![CDATA[
SELECT ' ' CD
      ,' ' NM
FROM   DUAL
UNION ALL
SELECT CD
      ,NM
FROM   (SELECT   A.SVC_SCP_CD CD
                ,B.SVC_SCP_NM NM
                ,A.CRE_DT
        FROM     PRI_RP_SCP_MN A
                ,MDM_SVC_SCP B
        WHERE    A.PROP_NO = @[etc1]
        AND      A.AMDT_SEQ = @[etc2]
        AND      A.SVC_SCP_CD = B.SVC_SCP_CD
        ORDER BY A.CRE_DT)			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
