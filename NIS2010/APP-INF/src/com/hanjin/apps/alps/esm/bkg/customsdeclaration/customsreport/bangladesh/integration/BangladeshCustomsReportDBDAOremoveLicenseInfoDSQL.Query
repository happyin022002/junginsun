<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshCustomsReportDBDAOremoveLicenseInfoDSQL">
			<desc><![CDATA[Bangladesh Customer License 정보 삭제 쿼리]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CSTMS_BD_FRT_FWRD_LIC
 WHERE CNT_CD = SUBSTR(@[cust_cd],1,2)
   AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3)) 			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
