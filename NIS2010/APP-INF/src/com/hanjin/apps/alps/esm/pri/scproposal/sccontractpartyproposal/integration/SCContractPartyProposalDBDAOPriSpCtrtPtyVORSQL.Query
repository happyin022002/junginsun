<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCContractPartyProposalDBDAOPriSpCtrtPtyVORSQL">
			<desc><![CDATA[History--------------------------------------
2011.03.30 이행지 [CHM-201109659-01] OTI Bond / Tariff Title page / OTI License Attach / Signing POA 기능 개발(Customer section)
                         - PRI_SP_CTRT_PTY ( OTI_LIC_ATCH_FILE_ID, OTI_BD_ATCH_FILE_ID, TRF_TIT_ATCH_FILE_ID, POA_ATCH_FILE_ID) 컬럼추가에 따른 쿼리 수정]]></desc>
			<sql><![CDATA[
SELECT 
    A.PROP_NO                       ,
    A.AMDT_SEQ                      ,
    A.PRC_CTRT_PTY_TP_CD            ,
    A.CUST_CNT_CD           ,
	A.CUST_SEQ ,
	A.CTRT_CUST_VAL_SGM_CD,
	A.CTRT_CUST_SREP_CD,
	A.CTRT_CUST_SLS_OFC_CD, 
	A.CTRT_PTY_NM, 
	A.CTRT_PTY_ADDR, 
	A.CTRT_PTY_SGN_NM, 
	A.CTRT_PTY_SGN_TIT_NM, 
    A.N1ST_CMNC_AMDT_SEQ ,
    (SELECT TO_CHAR(EFF_DT, 'YYYYMMDD') FROM PRI_SP_MN WHERE PROP_NO = A.PROP_NO 
										AND AMDT_SEQ = A.N1ST_CMNC_AMDT_SEQ  ) EFF_DT,
    CASE
        WHEN A.AMDT_SEQ = @[amdt_seq] THEN TO_CHAR(M.EXP_DT,'YYYYMMDD')
        ELSE
        (    
        SELECT CASE WHEN M.EFF_DT <= N.EXP_DT THEN TO_CHAR(M.EFF_DT - 1,'YYYYMMDD')
               ELSE TO_CHAR(N.EXP_DT,'YYYYMMDD')
               END AS EXP_DT
        FROM PRI_SP_MN N
        WHERE PROP_NO = M.PROP_NO AND AMDT_SEQ = M.AMDT_SEQ-1    
        )    
    END  EXP_DT,      
    A.SRC_INFO_CD                   ,
    SRC.INTG_CD_VAL_DESC SRC_INFO_NM  ,       
    A.PRC_PROG_STS_CD ,
    STS.INTG_CD_VAL_DESC PRC_PROG_STS_NM ,
    A.ACPT_USR_ID,
	A.ACPT_OFC_CD,
	TO_CHAR(A.ACPT_DT,'YYYYMMDD') ACPT_DT,
	NVOCC_BD_NO,
	NVOCC_LIC_NO,
	A.OTI_BD_ATCH_FILE_ID,
    CASE WHEN LENGTH(F1.FILE_UPLD_NM) > 10 
        THEN SUBSTR(F1.FILE_UPLD_NM,1,10)||'...'
        ELSE F1.FILE_UPLD_NM
    END OTI_BD_ATCH_FILE_NM,
	F1.FILE_UPLD_NM AS OTI_BD_ATCH_FILE_NM_ORG,
	A.OTI_LIC_ATCH_FILE_ID,
    CASE WHEN LENGTH(F2.FILE_UPLD_NM) > 10 
        THEN SUBSTR(F2.FILE_UPLD_NM,1,10)||'...'
        ELSE F2.FILE_UPLD_NM
    END OTI_LIC_ATCH_FILE_NM,
	F2.FILE_UPLD_NM AS OTI_LIC_ATCH_FILE_NM_ORG,
	A.TRF_TIT_ATCH_FILE_ID,
    CASE WHEN LENGTH(F3.FILE_UPLD_NM) > 10 
        THEN SUBSTR(F3.FILE_UPLD_NM,1,10)||'...'
        ELSE F3.FILE_UPLD_NM
    END TRF_TIT_ATCH_FILE_NM,
	F3.FILE_UPLD_NM AS TRF_TIT_ATCH_FILE_NM_ORG,
	A.POA_ATCH_FILE_ID,
    CASE WHEN LENGTH(F4.FILE_UPLD_NM) > 10 
        THEN SUBSTR(F4.FILE_UPLD_NM,1,10)||'...'
        ELSE F4.FILE_UPLD_NM
    END POA_ATCH_FILE_NM,
	F4.FILE_UPLD_NM AS POA_ATCH_FILE_NM_ORG,
	A.MOC_LIC_NO
FROM   
    PRI_SP_CTRT_PTY     A  ,
    PRI_SP_MN           M  ,
    COM_INTG_CD_DTL     SRC,
    COM_INTG_CD_DTL     STS,
	MDM_CUSTOMER		MDM,
	COM_UPLD_FILE		F1,
	COM_UPLD_FILE		F2,
	COM_UPLD_FILE		F3,
	COM_UPLD_FILE		F4
WHERE  
    M.PROP_NO                = A.PROP_NO
AND A.CUST_CNT_CD            = MDM.CUST_CNT_CD(+)
AND A.CUST_SEQ               = MDM.CUST_SEQ(+)
AND A.OTI_BD_ATCH_FILE_ID    = F1.FILE_SAV_ID(+)
AND A.OTI_LIC_ATCH_FILE_ID   = F2.FILE_SAV_ID(+)
AND A.TRF_TIT_ATCH_FILE_ID   = F3.FILE_SAV_ID(+)
AND A.POA_ATCH_FILE_ID       = F4.FILE_SAV_ID(+) 
AND M.AMDT_SEQ          = @[amdt_seq]
AND A.PROP_NO = @[prop_no]
AND A.PRC_CTRT_PTY_TP_CD = @[prc_ctrt_pty_tp_cd]
AND A.AMDT_SEQ IN ( @[amdt_seq], DECODE((SELECT LGCY_IF_FLG FROM PRI_SP_MN TMP WHERE TMP.PROP_NO = @[prop_no] AND TMP.AMDT_SEQ = @[amdt_seq]), 'Y', @[amdt_seq], @[amdt_seq]-1))
AND    (( A.AMDT_SEQ = @[amdt_seq] AND A.SRC_INFO_CD <> DECODE((SELECT LGCY_IF_FLG FROM PRI_SP_MN TMP WHERE TMP.PROP_NO = @[prop_no] AND TMP.AMDT_SEQ = @[amdt_seq]),'Y','AD','ZZ'))
       OR ( A.AMDT_SEQ = @[amdt_seq]-1
            AND  A.SRC_INFO_CD <> 'AD'
            AND  NOT EXISTS ( SELECT 'X' FROM PRI_SP_CTRT_PTY B
                               WHERE 
                                   B.PROP_NO            = A.PROP_NO 
                               AND B.AMDT_SEQ           = @[amdt_seq]
                               AND B.PRC_CTRT_PTY_TP_CD = A.PRC_CTRT_PTY_TP_CD 
                               AND B.N1ST_CMNC_AMDT_SEQ    = A.N1ST_CMNC_AMDT_SEQ           
                             )
           )
       )
AND SRC.INTG_CD_VAL_CTNT = A.SRC_INFO_CD
AND SRC.INTG_CD_ID       = 'CD02064'
AND STS.INTG_CD_VAL_CTNT = A.PRC_PROG_STS_CD
AND STS.INTG_CD_ID       = 'CD01719'
ORDER BY A.AMDT_SEQ			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="prc_ctrt_pty_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
