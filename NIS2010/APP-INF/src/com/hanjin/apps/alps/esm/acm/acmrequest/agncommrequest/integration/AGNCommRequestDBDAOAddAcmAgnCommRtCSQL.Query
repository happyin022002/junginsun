<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAOAddAcmAgnCommRtCSQL">
			<desc><![CDATA[AddAcmAgnCommRt]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_AGN_COMM(BKG_NO,AGN_CD,IO_BND_CD,AC_TP_CD,AC_SEQ,BDR_FLG,AC_STS_CD,PPD_AMT,CRNT_AMT,IF_AMT,CRNT_REV_AMT,DDCT_CHG_AMT,DDCT_TRSP_AMT,DDCT_SPCL_CMPN_AMT,DDCT_VIP_AMT,COMM_FX_AMT,REV_DIV_CD,COMM_RT,AGN_AGMT_NO,AGN_AGMT_SEQ,LOC_DIV_CD,LOC_CD,AR_OFC_CD,AP_OFC_CD,AP_CTR_CD,COMM_STND_COST_CD,SAIL_ARR_DT,AC_OCCR_INFO_CD,AC_SLAN_CD,AC_RLANE_CD,AC_VSL_CD,AC_SKD_VOY_NO,AC_SKD_DIR_CD,AC_REV_DIR_CD,CURR_CD,XCH_RT_APLY_LVL,PAY_XCH_RT,PAY_PPD_AMT,PAY_CRNT_AMT,PAY_IF_AMT,OFC_CHR_CD,VNDR_CNT_CD,VNDR_SEQ,ACCL_FLG,RQST_USR_ID,RQST_DT,RQST_GDT,AUD_NO,AUD_USR_ID,AUD_DT,AUD_GDT,CSR_NO,APRO_USR_ID,APRO_DT,APRO_GDT,GL_DT,ASA_NO,INV_TAX_RT,IF_USR_ID,IF_DT,IF_GDT,AC_PROC_DESC,PPD_OFRT_AMT,PPD_CHG_AMT,CLT_OFRT_AMT,CLT_CHG_AMT,AGN_INFO_SEQ,ORG_COMM_AMT,MIN_COMM_AMT,MIN_COMM_FLG,CRE_USR_ID,CRE_DT,UPD_USR_ID,UPD_DT)
SELECT 
  @[bkg_no]    AS BKG_NO
, @[agn_cd]    AS AGN_CD 
, @[io_bnd_cd] AS IO_BND_CD
, @[ac_tp_cd]  AS AC_TP_CD
, @[max_ac_seq] AS AC_SEQ
, @[bdr_flg]   AS BDR_FLG
, (CASE WHEN ROUND( @[comm_amt],2) - @[ppd_crnt_amt] < 0 AND @[max_ac_seq] = 1 THEN 'CN'  
        WHEN ROUND( @[comm_amt],2) - @[ppd_crnt_amt] = 0 THEN 'CZ'
        WHEN ROUND( @[comm_amt],2) <= 0 AND @[ppd_crnt_amt] = 0 AND @[max_ac_seq] > 2  THEN 'CZ' -- SEQ 2 로 이미 상계 처리 된 경우 최종 커미션 CRNT 금액이 0 이하이고, IF 금액이 0 일 때 CZ 로 만들어 주기 위한 로직 
        ELSE 'CS'
   END) AS AC_STS_CD
, NVL(@[ppd_crnt_amt],0) AS PPD_AMT
, CASE WHEN @[max_ac_seq] > 1  AND ROUND( @[comm_amt],2) < 0 THEN 0
       ELSE ROUND( @[comm_amt],2) 
  END                                        AS CRNT_AMT
, CASE WHEN @[max_ac_seq] > 1  AND ROUND( @[comm_amt],2) < 0 THEN 0 - @[ppd_crnt_amt]
       ELSE ROUND( @[comm_amt],2) - @[ppd_crnt_amt]    
  END                                        AS IF_AMT
, ROUND( @[ttl_rev_amt] ,2)                  AS CRNT_REV_AMT
, ROUND( @[ddct_chg],2)                      AS DDCT_CHG_AMT
, ROUND( @[ddct_trs],2)                      AS DDCT_TRSP_AMT
, ROUND( @[ddct_spcl],2)                     AS DDCT_SPCL_CMPN_AMT
, ROUND( @[ddct_vip_amt], 2)                 AS DDCT_VIP_AMT
, @[comm_fx_amt]  AS COMM_FX_AMT
, @[rev_div_cd]   AS REV_DIV_CD
, @[comm_rt]      AS COMM_RT
, @[agn_agmt_no]  AS AGN_AGMT_NO
, @[agn_agmt_seq] AS AGN_AGMT_SEQ
, NULL AS LOC_DIV_CD
, NULL AS LOC_CD
, @[ar_ofc_cd] AS AR_OFC_CD
, @[ap_ofc_cd] AS AP_OFC_CD
, @[ap_ctr_cd] AS AP_CTR_CD
, (SELECT COMM_STND_COST_CD FROM ACM_COMM_TP_CD_MAPG WHERE COMM_TP_CD = 'C' AND IO_BND_CD = @[io_bnd_cd] AND AC_TP_CD = @[ac_tp_cd])  AS COMM_STND_COST_CD
, @[sa_dt]      AS SAIL_ARR_DT
, @[port]       AS AC_OCCR_INFO_CD
, @[slan_cd]    AS AC_SLAN_CD
, @[rlane_cd]   AS AC_RLANE_CD
, @[vsl_cd]     AS AC_VSL_CD
, @[skd_voy_no] AS AC_SKD_VOY_NO
, @[skd_dir_cd] AS AC_SKD_DIR_CD
, @[rev_dir_cd] AS AC_REV_DIR_CD
, @[ofc_curr_cd]
, @[xch_rt_div_lvl] AS XCH_RT_APLY_LVL
, @[pay_xch_rt]     AS PAY_XCH_RT
, CASE WHEN (SELECT COUNT(DISTINCT CURR_CD) FROM ACM_AGN_COMM WHERE BKG_NO = @[bkg_no] AND AGN_CD = @[agn_cd] AND IO_BND_CD = @[io_bnd_cd] AND AC_TP_CD = @[ac_tp_cd] AND CURR_CD <> @[ofc_curr_cd]) > 0  
       THEN ROUND( TO_NUMBER(@[pay_xch_rt]) * NVL(@[ppd_crnt_amt],0),2)
       ELSE TO_NUMBER(@[ppd_pay_crnt_amt]) 
  END                                                     AS PAY_PPD_AMT
, CASE WHEN  @[pay_xch_rt] = 0 THEN 0
       WHEN (SELECT COUNT(DISTINCT CURR_CD) FROM ACM_AGN_COMM WHERE BKG_NO = @[bkg_no] AND AGN_CD = @[agn_cd] AND IO_BND_CD = @[io_bnd_cd] AND AC_TP_CD = @[ac_tp_cd] AND CURR_CD <> @[ofc_curr_cd]) > 0  
       THEN  ROUND( @[pay_xch_rt] * ROUND( @[comm_amt],2) ,2)
       WHEN  @[max_ac_seq] > 1  AND ROUND(@[comm_amt],2) < 0 THEN 0
       ELSE  ROUND( @[pay_xch_rt] * ROUND(@[comm_amt],2),2) 
  END                                                                            AS PAY_CRNT_AMT                   
, CASE WHEN @[pay_xch_rt] = 0 THEN 0
       WHEN (SELECT COUNT(DISTINCT CURR_CD) FROM ACM_AGN_COMM WHERE BKG_NO = @[bkg_no] AND AGN_CD = @[agn_cd] AND IO_BND_CD = @[io_bnd_cd] AND AC_TP_CD = @[ac_tp_cd] AND CURR_CD <> @[ofc_curr_cd]) > 0  
       THEN  ROUND( @[pay_xch_rt] * (ROUND( @[comm_amt],2) - @[ppd_crnt_amt]) ,2)
       WHEN @[max_ac_seq] > 1  AND ROUND( @[comm_amt],2) < 0 THEN 0 - @[ppd_pay_crnt_amt]
       ELSE  ROUND( @[pay_xch_rt] * ROUND(@[comm_amt],2),2) - @[ppd_pay_crnt_amt]
  END                                                                            AS PAY_IF_AMT
, @[ofc_chr_cd]  AS OFC_CHR_CD
, @[vndr_cnt_cd] AS VNDR_CNT_CD
, @[vndr_seq]    AS VNDR_SEQ
, 'N'  AS ACCL_FLG
, NULL AS RQST_USR_ID
, NULL AS RQST_DT
, NULL AS RQST_GDT
, NULL AS AUD_NO
, NULL AS AUD_USR_ID
, NULL AS AUD_DT
, NULL AS AUD_GDT
, NULL AS CSR_NO
, NULL AS APRO_USR_ID
, NULL AS APRO_DT
, NULL AS APRO_GDT
, NULL AS GL_DT
, NULL AS ASA_NO
, NULL AS INV_TAX_RT
, NULL AS IF_USR_ID
, NULL AS IF_DT
, NULL AS IF_GDT
, (CASE WHEN @[comm_amt] > 0 THEN 'Calculation-Success'  
        WHEN @[comm_amt] = 0 THEN 'Calculation-ZeroAMT'  
        ELSE 'Calculation-NegativeAMT' END) AS AC_PROC_DESC
, @[ppd_ofrt_amt] AS PPD_OFRT_AMT
, @[ppd_chg_amt]  AS PPD_CHG_AMT
, @[clt_ofrt_amt] AS CLT_OFRT_AMT
, @[clt_chg_amt]  AS CLT_CHG_AMT
, @[agn_info_seq] AS AGN_INFO_SEQ
, CASE WHEN @[max_ac_seq] > 1  AND ROUND( @[comm_amt],2) < 0 THEN 0
       ELSE ROUND( @[org_comm_amt],2) 
  END                                        AS ORG_COMM_AMT
, CASE WHEN @[max_ac_seq] > 1  AND ROUND( @[comm_amt],2) < 0 THEN 0
       ELSE ROUND( @[min_comm_amt],2) 
  END                                        AS MIN_COMM_AMT
, @[min_comm_flg] AS MIN_COMM_FLG
, @[usr_id]
, SYSDATE
, @[usr_id]
, SYSDATE
FROM DUAL D			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ac_tp_cd" type="12" value="" out="N"/>
				<param name="max_ac_seq" type="12" value="" out="N"/>
				<param name="bdr_flg" type="12" value="" out="N"/>
				<param name="comm_amt" type="12" value="" out="N"/>
				<param name="ppd_crnt_amt" type="12" value="" out="N"/>
				<param name="ttl_rev_amt" type="12" value="" out="N"/>
				<param name="ddct_chg" type="12" value="" out="N"/>
				<param name="ddct_trs" type="12" value="" out="N"/>
				<param name="ddct_spcl" type="12" value="" out="N"/>
				<param name="ddct_vip_amt" type="12" value="" out="N"/>
				<param name="comm_fx_amt" type="12" value="" out="N"/>
				<param name="rev_div_cd" type="12" value="" out="N"/>
				<param name="comm_rt" type="12" value="" out="N"/>
				<param name="agn_agmt_no" type="12" value="" out="N"/>
				<param name="agn_agmt_seq" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="ap_ofc_cd" type="12" value="" out="N"/>
				<param name="ap_ctr_cd" type="12" value="" out="N"/>
				<param name="sa_dt" type="12" value="" out="N"/>
				<param name="port" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="ofc_curr_cd" type="12" value="" out="N"/>
				<param name="xch_rt_div_lvl" type="12" value="" out="N"/>
				<param name="pay_xch_rt" type="12" value="" out="N"/>
				<param name="ppd_pay_crnt_amt" type="12" value="" out="N"/>
				<param name="ofc_chr_cd" type="12" value="" out="N"/>
				<param name="vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="ppd_ofrt_amt" type="12" value="" out="N"/>
				<param name="ppd_chg_amt" type="12" value="" out="N"/>
				<param name="clt_ofrt_amt" type="12" value="" out="N"/>
				<param name="clt_chg_amt" type="12" value="" out="N"/>
				<param name="agn_info_seq" type="12" value="" out="N"/>
				<param name="org_comm_amt" type="12" value="" out="N"/>
				<param name="min_comm_amt" type="12" value="" out="N"/>
				<param name="min_comm_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
