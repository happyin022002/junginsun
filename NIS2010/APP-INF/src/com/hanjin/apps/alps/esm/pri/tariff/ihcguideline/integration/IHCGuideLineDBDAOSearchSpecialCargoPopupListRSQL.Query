<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOSearchSpecialCargoPopupListRSQL">
			<desc><![CDATA[IHC Special Cargo List
2013.08.06 전윤주 [CHM-201326196] Overweight Fixed AMT 를 Local curr. 금액 그대로 IF 하는 컬럼 추가]]></desc>
			<sql><![CDATA[
SELECT A.SVC_SCP_CD,
       A.IHC_TRF_NO,
       A.PRC_TRSP_MOD_CD,
       B.INTG_CD_VAL_DESC PRC_TRSP_MOD_CD_NM,
       A.PRC_INLND_TRF_CNTR_TPSZ_CD,
       A.DG_FLT_PCT_TP_CD,
       A.DG_RT_AMT,
       A.DG_RT_RTO,
       A.MIN_CGO_WGT,
       A.MAX_CGO_WGT,
       A.OVR_WGT_FLT_PCT_TP_CD,
       A.OVR_WGT_RT_AMT,
       A.OVR_WGT_RT_RTO,
       A.DCGO_SVC_FLG,
       A.OVR_WGT_CGO_SVC_FLG,
       A.LOCL_CURR_CD,
       A.LOCL_OVR_WGT_RT_AMT
  FROM PRI_TRF_IHC_SPCL_CGO_RT A,
       COM_INTG_CD_DTL         B
 WHERE A.SVC_SCP_CD = @[svc_scp_cd]
   AND A.IHC_TRF_NO = @[ihc_trf_no]
   AND A.ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND B.INTG_CD_ID(+) = 'CD01720'
   AND A.PRC_TRSP_MOD_CD = B.INTG_CD_VAL_CTNT(+)
 ORDER BY A.PRC_TRSP_MOD_CD,
          A.PRC_INLND_TRF_CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
