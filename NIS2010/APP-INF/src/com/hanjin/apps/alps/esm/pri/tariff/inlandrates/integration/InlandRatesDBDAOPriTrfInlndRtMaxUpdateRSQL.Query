<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRatesDBDAOPriTrfInlndRtMaxUpdateRSQL">
			<desc><![CDATA[최근 업데이트 날짜를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(NVL(MAX(UPD_DT), SYSDATE), 'YYYYMMDD-HH24MISS') UPD_DT
  FROM PRI_TRF_INLND_RT
 WHERE TRF_PFX_CD 		= @[trf_pfx_cd]
   AND TRF_NO 			= @[trf_no]
   AND TRF_INLND_SEQ 	= @[trf_inlnd_seq]
   AND AMDT_SEQ 		= @[amdt_seq]			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_inlnd_seq" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
