<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQBalanceDBDAOMasCntrRepoRuleVO2USQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE  MAS_CNTR_REPO_RULE
   SET  IMBAL_ALL_APLY_FLG = DECODE(@[imbal_all_aply_flg],'1','Y','0','N')
	   ,OPB_ALL_APLY_FLG   = DECODE(@[opb_all_aply_flg],'1','Y','0','N')
	   ,MB_ALL_APLY_FLG    = DECODE(@[mb_all_aply_flg],'1','Y','0','N')
WHERE  COST_YRMON = @[cost_yrmon]
  AND  CNTR_TPSZ_CD = REPLACE(@[cntr_tpsz_cd], 'RD', 'R')			]]></sql>
			<params>
				<param name="imbal_all_aply_flg" type="12" value="" out="N"/>
				<param name="opb_all_aply_flg" type="12" value="" out="N"/>
				<param name="mb_all_aply_flg" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
