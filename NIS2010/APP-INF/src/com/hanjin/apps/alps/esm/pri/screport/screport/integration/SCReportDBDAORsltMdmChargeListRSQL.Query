<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltMdmChargeListRSQL">
			<desc><![CDATA[charge 리스트 조회 - 콤보용]]></desc>
			<sql><![CDATA[
SELECT	CHG_CD
       ,CHG_NM
       ,REP_CHG_CD
FROM	MDM_CHARGE
WHERE 	DELT_FLG = 'N'
#if (${rep_chg_cd} != '') 
AND     REP_CHG_CD = @[rep_chg_cd]
ORDER BY REP_CHG_CD, CHG_CD 
#else
ORDER BY CHG_CD
#end			]]></sql>
			<params>
				<param name="rep_chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
