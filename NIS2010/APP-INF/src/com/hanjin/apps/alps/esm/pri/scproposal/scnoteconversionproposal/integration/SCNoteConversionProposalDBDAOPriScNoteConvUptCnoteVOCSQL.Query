<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteConversionProposalDBDAOPriScNoteConvUptCnoteVOCSQL">
			<desc><![CDATA[cnote
2012.08.21 전윤주 [CHM-201219729] S/C Amendment History Conversion Update 체크시 속도 개선
기존 쿼리에서 index를 사용하지 못하던 부분을 상수로 바인딩하여 인덱스로 처리할 수 있도록 변경]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SC_NOTE_CONV
(
       NOTE_CONV_MAPG_ID
     , NOTE_CONV_SEQ
     , NOTE_CONV_TP_CD
     , SVC_SCP_CD
     , PROP_NO
     , AMDT_SEQ
     , CHG_RULE_TP_CD
     , NOTE_CONV_CHG_CD
     , NOTE_CONV_RULE_CD
     , EFF_DT
     , EXP_DT
     , RT_APPL_TP_CD
     , RT_OP_CD
     , CURR_CD
     , FRT_RT_AMT
     , PAY_TERM_CD
     , GEN_SPCL_RT_TP_CD
     , BKG_RAT_UT_CD
     , BKG_PRC_CGO_TP_CD
     , BKG_IMDG_CLSS_CD
     , BKG_CMDT_TP_CD
     , BKG_CMDT_DEF_CD
     , BKG_SCG_GRP_CMDT_CD
     , BKG_USA_SVC_MOD_CD
     , BKG_POR_TP_CD
     , BKG_POR_DEF_CD
     , BKG_POL_TP_CD
     , BKG_POL_DEF_CD
     , BKG_POD_TP_CD
     , BKG_POD_DEF_CD
     , BKG_DEL_TP_CD
     , BKG_DEL_DEF_CD
     , BKG_ORG_TRSP_MOD_CD
     , BKG_DEST_TRSP_MOD_CD
     , BKG_RCV_TERM_CD
     , BKG_DE_TERM_CD
     , BKG_SLAN_CD
     , BKG_VSL_CD
     , BKG_SKD_VOY_NO
     , BKG_SKD_DIR_CD
     , BKG_SOC_FLG
     , BKG_ACT_CUST_CNT_CD
     , BKG_ACT_CUST_SEQ
     , BKG_TS_PORT_TP_CD
     , BKG_TS_PORT_DEF_CD
     , BKG_DIR_CALL_FLG
     , BKG_MST_HBL_TP_CD
     , CONV_RAT_UT_CD
     , CONV_PRC_CGO_TP_CD
     , CONV_CMDT_TP_CD
     , CONV_CMDT_DEF_CD
     , CONV_ORG_LOC_TP_CD
     , CONV_ORG_LOC_DEF_CD
     , CONV_ORG_VIA_LOC_TP_CD
     , CONV_ORG_VIA_LOC_DEF_CD
     , CONV_DEST_VIA_LOC_TP_CD
     , CONV_DEST_VIA_LOC_DEF_CD
     , CONV_DEST_LOC_TP_CD
     , CONV_DEST_LOC_DEF_CD
     , CONV_PRC_RCV_TERM_CD
     , CONV_PRC_DE_TERM_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , NOTE_HDR_SEQ
     , RULE_APPL_CHG_TP_CD
     , BKG_IO_GA_CD
     , BKG_CNL_TZ_CD
     , BKG_ESVC_TP_CD
     , RULE_APPL_CHG_CD
     , RT_PATT_TP_CD
     , IGN_TRF_FLG
	 , BKG_NO
)
SELECT NOTE_CONV_MAPG_ID
      ,NOTE_CONV_SEQ
      ,NOTE_CONV_TP_CD
      ,SVC_SCP_CD
      ,PROP_NO
      ,AMDT_SEQ 
      ,CHG_RULE_TP_CD
     , NOTE_CONV_CHG_CD
     , NOTE_CONV_RULE_CD
     , EFF_DT
     , EXP_DT
     , RT_APPL_TP_CD
     , RT_OP_CD
     , CURR_CD
     , FRT_RT_AMT
     , PAY_TERM_CD
     , GEN_SPCL_RT_TP_CD
     , BKG_RAT_UT_CD
     , BKG_PRC_CGO_TP_CD
     , BKG_IMDG_CLSS_CD
     , BKG_CMDT_TP_CD
     , BKG_CMDT_DEF_CD
     , BKG_SCG_GRP_CMDT_CD
     , BKG_USA_SVC_MOD_CD
     , BKG_POR_TP_CD
     , BKG_POR_DEF_CD
     , BKG_POL_TP_CD
     , BKG_POL_DEF_CD
     , BKG_POD_TP_CD
     , BKG_POD_DEF_CD
     , BKG_DEL_TP_CD
     , BKG_DEL_DEF_CD
     , BKG_ORG_TRSP_MOD_CD
     , BKG_DEST_TRSP_MOD_CD
     , BKG_RCV_TERM_CD
     , BKG_DE_TERM_CD
     , BKG_SLAN_CD
     , BKG_VSL_CD
     , BKG_SKD_VOY_NO
     , BKG_SKD_DIR_CD
     , BKG_SOC_FLG
     , BKG_ACT_CUST_CNT_CD
     , BKG_ACT_CUST_SEQ
     , BKG_TS_PORT_TP_CD
     , BKG_TS_PORT_DEF_CD
     , BKG_DIR_CALL_FLG
     , BKG_MST_HBL_TP_CD
     , CONV_RAT_UT_CD
     , CONV_PRC_CGO_TP_CD
     , CONV_CMDT_TP_CD
     , CONV_CMDT_DEF_CD
     , CONV_ORG_LOC_TP_CD
     , CONV_ORG_LOC_DEF_CD
     , CONV_ORG_VIA_LOC_TP_CD
     , CONV_ORG_VIA_LOC_DEF_CD
     , CONV_DEST_VIA_LOC_TP_CD
     , CONV_DEST_VIA_LOC_DEF_CD
     , CONV_DEST_LOC_TP_CD
     , CONV_DEST_LOC_DEF_CD
     , CONV_PRC_RCV_TERM_CD
     , CONV_PRC_DE_TERM_CD
     , CRE_USR_ID
     , CRE_DT     
     , UPD_USR_ID
     , UPD_DT
     , NOTE_HDR_SEQ
     , RULE_APPL_CHG_TP_CD    
     , BKG_IO_GA_CD
     , BKG_CNL_TZ_CD
     , BKG_ESVC_TP_CD
     , RULE_APPL_CHG_CD
     , RT_PATT_TP_CD
     , IGN_TRF_FLG
	 , BKG_NO
FROM(
    SELECT
           MN.N_MAP_ID NOTE_CONV_MAPG_ID
          ,CONV.NOTE_CONV_SEQ
          ,CONV.NOTE_CONV_TP_CD
          ,CONV.SVC_SCP_CD
          ,CONV.PROP_NO
          ,CONV.AMDT_SEQ + 1 AMDT_SEQ
          ,CONV.CHG_RULE_TP_CD
         , CONV.NOTE_CONV_CHG_CD
         , CONV.NOTE_CONV_RULE_CD
          ,CASE AMD_FLG WHEN 'N' THEN CONV.EFF_DT --어멘드 이전
           ELSE DECODE(SIGN(CONV.EFF_DT - MN.N_SCP_EFF_DT), -1, MN.N_SCP_EFF_DT,CONV.EFF_DT)
           END EFF_DT
          ,CASE WHEN MN.ADD_EXP_DT_FLG = 'Y' THEN --EXP_DT가 증가했다
              CASE WHEN CONV.EXP_DT = MN.EXP_DT THEN MN.N_EXP_DT 
              ELSE
                  DECODE(SIGN(CONV.EXP_DT - MN.N_EXP_DT), -1,  CONV.EXP_DT, MN.N_EXP_DT)
              END                    
           ELSE
              DECODE(SIGN(MN.N_EXP_DT - CONV.EXP_DT), -1, MN.N_EXP_DT, CONV.EXP_DT)
           END EXP_DT
         , CONV.RT_APPL_TP_CD
         , CONV.RT_OP_CD
         , CONV.CURR_CD
         , CONV.FRT_RT_AMT
         , CONV.PAY_TERM_CD
         , CONV.GEN_SPCL_RT_TP_CD
         , CONV.BKG_RAT_UT_CD
         , CONV.BKG_PRC_CGO_TP_CD
         , CONV.BKG_IMDG_CLSS_CD
         , CONV.BKG_CMDT_TP_CD
         , CONV.BKG_CMDT_DEF_CD
         , CONV.BKG_SCG_GRP_CMDT_CD
         , CONV.BKG_USA_SVC_MOD_CD
         , CONV.BKG_POR_TP_CD
         , CONV.BKG_POR_DEF_CD
         , CONV.BKG_POL_TP_CD
         , CONV.BKG_POL_DEF_CD
         , CONV.BKG_POD_TP_CD
         , CONV.BKG_POD_DEF_CD
         , CONV.BKG_DEL_TP_CD
         , CONV.BKG_DEL_DEF_CD
         , CONV.BKG_ORG_TRSP_MOD_CD
         , CONV.BKG_DEST_TRSP_MOD_CD
         , CONV.BKG_RCV_TERM_CD
         , CONV.BKG_DE_TERM_CD
         , CONV.BKG_SLAN_CD
         , CONV.BKG_VSL_CD
         , CONV.BKG_SKD_VOY_NO
         , CONV.BKG_SKD_DIR_CD
         , CONV.BKG_SOC_FLG
         , CONV.BKG_ACT_CUST_CNT_CD
         , CONV.BKG_ACT_CUST_SEQ
         , CONV.BKG_TS_PORT_TP_CD
         , CONV.BKG_TS_PORT_DEF_CD
         , CONV.BKG_DIR_CALL_FLG
         , CONV.BKG_MST_HBL_TP_CD
         , CONV.CONV_RAT_UT_CD
         , CONV.CONV_PRC_CGO_TP_CD
         , CONV.CONV_CMDT_TP_CD
         , CONV.CONV_CMDT_DEF_CD
         , CONV.CONV_ORG_LOC_TP_CD
         , CONV.CONV_ORG_LOC_DEF_CD
         , CONV.CONV_ORG_VIA_LOC_TP_CD
         , CONV.CONV_ORG_VIA_LOC_DEF_CD
         , CONV.CONV_DEST_VIA_LOC_TP_CD
         , CONV.CONV_DEST_VIA_LOC_DEF_CD
         , CONV.CONV_DEST_LOC_TP_CD
         , CONV.CONV_DEST_LOC_DEF_CD
         , CONV.CONV_PRC_RCV_TERM_CD
         , CONV.CONV_PRC_DE_TERM_CD
         , CONV.CRE_USR_ID
         , CONV.CRE_DT
         , CONV.UPD_USR_ID
         , CONV.UPD_DT
         , CONV.NOTE_HDR_SEQ
         , CONV.RULE_APPL_CHG_TP_CD
         , CONV.BKG_IO_GA_CD
         , CONV.BKG_CNL_TZ_CD
         , CONV.BKG_ESVC_TP_CD
         , CONV.RULE_APPL_CHG_CD
         , CONV.RT_PATT_TP_CD
         , CONV.IGN_TRF_FLG
		 , CONV.BKG_NO
    FROM PRI_SC_NOTE_CONV CONV
       ,(
        SELECT N_NOTE.NOTE_CONV_MAPG_ID N_MAP_ID
              ,DECODE(N_NOTE.N1ST_CMNC_AMDT_SEQ,@[amdt_seq] + 1, 'Y', 'N') AMD_FLG
              ,N_NOTE.N1ST_CMNC_AMDT_SEQ N_N1ST_SEQ
              ,NOTE.NOTE_CONV_MAPG_ID MAP_ID
              ,N_MN.CTRT_EFF_DT N_EFF_DT
              ,N_MN.CTRT_EXP_DT N_EXP_DT
              ,MN.CTRT_EFF_DT EFF_DT
              ,MN.CTRT_EXP_DT EXP_DT
              ,DECODE(SIGN(MN.CTRT_EXP_DT - N_MN.CTRT_EXP_DT), -1,'Y','N') ADD_EXP_DT_FLG
              ,N_SCP.EFF_DT N_SCP_EFF_DT
        FROM PRI_SP_SCP_RT_CNOTE NOTE
            ,PRI_SP_SCP_RT_CNOTE N_NOTE
            ,PRI_SP_SCP_DUR N_MN
            ,PRI_SP_SCP_DUR MN    
            ,PRI_SP_SCP_MN  N_SCP
        WHERE N_MN.PROP_NO           = MN.PROP_NO
--        AND N_MN.AMDT_SEQ            = MN.AMDT_SEQ + 1
        AND MN.AMDT_SEQ              = @[amdt_seq]
        AND N_MN.SVC_SCP_CD          = MN.SVC_SCP_CD 
        AND N_MN.PROP_NO             = N_NOTE.PROP_NO
        AND N_MN.AMDT_SEQ            = N_NOTE.AMDT_SEQ
        AND N_MN.SVC_SCP_CD          = N_NOTE.SVC_SCP_CD
        AND N_NOTE.PROP_NO           = NOTE.PROP_NO
--        AND N_NOTE.AMDT_SEQ          = NOTE.AMDT_SEQ + 1
        AND NOTE.AMDT_SEQ            = @[amdt_seq]
        AND N_NOTE.SVC_SCP_CD        = NOTE.SVC_SCP_CD
        AND N_NOTE.GEN_SPCL_RT_TP_CD = NOTE.GEN_SPCL_RT_TP_CD
        AND N_NOTE.CMDT_HDR_SEQ      = NOTE.CMDT_HDR_SEQ
        AND N_NOTE.CMDT_NOTE_SEQ     = NOTE.CMDT_NOTE_SEQ
        AND N_MN.PROP_NO         = N_SCP.PROP_NO
        AND N_MN.AMDT_SEQ        = N_SCP.AMDT_SEQ
        AND N_MN.SVC_SCP_CD      = N_SCP.SVC_SCP_CD
        AND N_MN.PROP_NO             = @[prop_no]
        AND N_MN.AMDT_SEQ            = @[amdt_seq] + 1
    )MN
    WHERE MN.MAP_ID = CONV.NOTE_CONV_MAPG_ID
)
WHERE EFF_DT <= EXP_DT 
OR NOTE_CONV_RULE_CD = 'APP'			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
