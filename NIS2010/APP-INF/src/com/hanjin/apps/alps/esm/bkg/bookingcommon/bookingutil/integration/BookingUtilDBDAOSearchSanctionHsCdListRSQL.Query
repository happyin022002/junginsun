<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchSanctionHsCdListRSQL">
			<desc><![CDATA[Iran Sanction 관련 CM 특정 HS 코드]]></desc>
			<sql><![CDATA[
SELECT CTNT.ATTR_CTNT5, CTNT.ATTR_CTNT6, CTNT.HRD_CDG_ID, CTNT.HRD_CDG_ID_SEQ
FROM BKG_BOOKING BK, BKG_HRD_CDG_CTNT CTNT
WHERE 1=1

#if (${bkg_no} != '') 
  AND BK.BKG_NO = @[bkg_no]
#else 
  AND BK.BL_NO = SUBSTR(@[bl_no], 1, 12)
#end

  AND CTNT.HRD_CDG_ID = 'SANCTION_HS_CD_LIST'
  AND (
     (CTNT.ATTR_CTNT1 > ' ' AND BK.POR_CD LIKE CTNT.ATTR_CTNT1||'%') OR 
     (CTNT.ATTR_CTNT2 > ' ' AND BK.POL_CD LIKE CTNT.ATTR_CTNT2||'%') OR
     (CTNT.ATTR_CTNT3 > ' ' AND BK.POD_CD LIKE CTNT.ATTR_CTNT3||'%') OR 
     (CTNT.ATTR_CTNT4 > ' ' AND BK.DEL_CD LIKE CTNT.ATTR_CTNT4||'%')
      )

ORDER BY CTNT.ATTR_CTNT5			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
