<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchBkgCntrRSQL">
			<desc><![CDATA[searchBkgCntr]]></desc>
			<sql><![CDATA[
SELECT 
	A.BL_NO, 
	'  ' BL_SPLIT_NO,
	B.CNTR_NO, 
	B.CNTR_TPSZ_CD,
	(  SELECT NVL(MAX(DECODE(ROWNUM, 1, CNTR_SEAL_NO, '')),'999') 
	   FROM (
          SELECT *
          FROM BKG_CNTR_SEAL_NO
          ORDER BY CNTR_SEAL_SEQ ASC
         ) X
	   WHERE X.BKG_NO = B.BKG_NO
	   AND  X.CNTR_NO = B.CNTR_NO
      ) CNTR_SEAL_NO,

	(  SELECT NVL(MAX(DECODE(ROWNUM, 2, CNTR_SEAL_NO, '')),' ') 
	   FROM (
          SELECT *
          FROM BKG_CNTR_SEAL_NO
          ORDER BY CNTR_SEAL_SEQ ASC
         ) X
	   WHERE X.BKG_NO = B.BKG_NO
	   AND  X.CNTR_NO = B.CNTR_NO
      ) CNTR_SEAL_NO2,

	(  SELECT NVL(MAX(DECODE(ROWNUM, 3, CNTR_SEAL_NO, '')),' ') 
	   FROM (
          SELECT *
          FROM BKG_CNTR_SEAL_NO
          ORDER BY CNTR_SEAL_SEQ ASC
         ) X
	   WHERE X.BKG_NO = B.BKG_NO
	   AND  X.CNTR_NO = B.CNTR_NO
      ) CNTR_SEAL_NO3,

	(  SELECT NVL(MAX(DECODE(ROWNUM, 4, CNTR_SEAL_NO, '')),' ') 
	   FROM (
          SELECT *
          FROM BKG_CNTR_SEAL_NO
          ORDER BY CNTR_SEAL_SEQ ASC
         ) X
	   WHERE X.BKG_NO = B.BKG_NO
	   AND  X.CNTR_NO = B.CNTR_NO
      ) CNTR_SEAL_NO4,

	(  SELECT NVL(MAX(DECODE(ROWNUM, 5, CNTR_SEAL_NO, '')),' ') 
	   FROM (
          SELECT *
          FROM BKG_CNTR_SEAL_NO
          ORDER BY CNTR_SEAL_SEQ ASC
         ) X
	   WHERE X.BKG_NO = B.BKG_NO
	   AND  X.CNTR_NO = B.CNTR_NO
      ) CNTR_SEAL_NO5,

	(  SELECT NVL(MAX(DECODE(ROWNUM, 6, CNTR_SEAL_NO, '')),' ') 
	   FROM (
          SELECT *
          FROM BKG_CNTR_SEAL_NO
          ORDER BY CNTR_SEAL_SEQ ASC
         ) X
	   WHERE X.BKG_NO = B.BKG_NO
	   AND  X.CNTR_NO = B.CNTR_NO
      ) CNTR_SEAL_NO6,
	B.CNTR_PRT_FLG,
	B.RCV_TERM_CD,    
	B.DE_TERM_CD,
	@[bkg_cgo_tp_cd] BKG_CGO_TP_CD,
	DECODE(D.LSTM_CD,'SH','1','2') LSTM_CD
FROM 
	BKG_BOOKING A, 
	BKG_CONTAINER B, 
	MST_CONTAINER D
WHERE  A.BKG_NO       = @[bkg_no]
AND  A.BKG_NO       = B.BKG_NO
AND	 B.CNTR_NO = D.CNTR_NO			]]></sql>
			<params>
				<param name="bkg_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
