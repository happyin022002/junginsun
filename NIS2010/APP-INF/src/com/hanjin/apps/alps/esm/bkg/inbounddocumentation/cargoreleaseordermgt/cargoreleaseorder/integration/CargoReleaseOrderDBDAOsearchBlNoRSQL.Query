<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchBlNoRSQL">
			<desc><![CDATA[BkgNo로 BlNo를 조회한다.]]></desc>
			<sql><![CDATA[
select bl_no
  from bkg_booking
 where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
