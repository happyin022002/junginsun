<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOmodifyIbDtlBlIssUSQL">
			<desc><![CDATA[Local ISS Update]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_ISS
   SET CSTMS_ENTR_CD = @[cstms_entr_cd]
     , CSTMS_CLR_LOC_CD = @[cstms_clr_loc_cd]
     , CSTMS_CLR_WH_NM = @[cstms_clr_wh_nm]
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="cstms_entr_cd" type="12" value="" out="N"/>
				<param name="cstms_clr_loc_cd" type="12" value="" out="N"/>
				<param name="cstms_clr_wh_nm" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
