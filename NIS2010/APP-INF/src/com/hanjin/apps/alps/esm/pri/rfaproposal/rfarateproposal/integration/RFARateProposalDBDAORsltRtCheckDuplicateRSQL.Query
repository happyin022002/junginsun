<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltRtCheckDuplicateRSQL">
			<desc><![CDATA[RFA Proposal Rate 중복 확인]]></desc>
			<sql><![CDATA[
WITH T AS (
  SELECT A.PROP_NO
        ,A.AMDT_SEQ
        ,A.SVC_SCP_CD
        ,A.CMDT_HDR_SEQ
        ,A.ROUT_SEQ
        ,A.RT_SEQ
        ,B.PRC_CMDT_DEF_CD
        ,REPLACE(B.PRC_CMDT_DEF_NM, '|||', ' / ' || CHR(13)) AS PRC_CMDT_DEF_NM
        ,NVL(C.CUST_LGL_ENG_CD, '||') AS CUST_LGL_ENG_CD
        ,REPLACE(C.CUST_LGL_ENG_NM, '|||', ' / ' || CHR(13)) AS CUST_LGL_ENG_NM
        ,D.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD
        ,REPLACE(D.ROUT_PNT_LOC_DEF_NM, '|||', CHR(13)) AS ORG_ROUT_PNT_LOC_DEF_NM
        ,NVL(E.ROUT_VIA_PORT_DEF_CD, '||') AS ORG_ROUT_VIA_PORT_DEF_CD
        ,REPLACE(E.ROUT_VIA_PORT_DEF_NM, '|||', CHR(13)) AS ORG_ROUT_VIA_PORT_DEF_NM
        ,NVL(F.ROUT_VIA_PORT_DEF_CD, '||') AS DEST_ROUT_VIA_PORT_DEF_CD
        ,REPLACE(F.ROUT_VIA_PORT_DEF_NM, '|||', CHR(13)) AS DEST_ROUT_VIA_PORT_DEF_NM
        ,G.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD
        ,REPLACE(G.ROUT_PNT_LOC_DEF_NM, '|||', CHR(13)) AS DEST_ROUT_PNT_LOC_DEF_NM
        ,A.RAT_UT_CD
        ,A.PRC_CGO_TP_CD
        ,A.CURR_CD
        ,A.PROP_FRT_RT_AMT
    FROM (SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, RT_SEQ, RAT_UT_CD, PRC_CGO_TP_CD, CURR_CD, PROP_FRT_RT_AMT
            FROM PRI_RP_SCP_RT A
           WHERE PROP_NO = @[prop_no]
             AND AMDT_SEQ = @[amdt_seq]
             AND SVC_SCP_CD = @[svc_scp_cd]
             AND SRC_INFO_CD <> 'AD') A
        ,(SELECT PROP_NO
                ,AMDT_SEQ
                ,SVC_SCP_CD
                ,CMDT_HDR_SEQ
                ,MAX(SYS_CONNECT_BY_PATH(PRC_CMDT_DEF_CD, '||')) AS PRC_CMDT_DEF_CD
                ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(PRC_CMDT_DEF_NM, '|||')), 4) AS PRC_CMDT_DEF_NM
            FROM (SELECT PROP_NO
                        ,AMDT_SEQ
                        ,SVC_SCP_CD
                        ,CMDT_HDR_SEQ
                        ,PRC_CMDT_DEF_CD
                        ,DECODE(PRC_CMDT_TP_CD
                               ,'G'
                               ,(SELECT PRC_GRP_CMDT_DESC
                                  FROM PRI_RP_SCP_GRP_CMDT
                                 WHERE PROP_NO = A.PROP_NO
                                   AND AMDT_SEQ = A.AMDT_SEQ
                                   AND SVC_SCP_CD = A.SVC_SCP_CD
                                   AND PRC_GRP_CMDT_CD = A.PRC_CMDT_DEF_CD
                                   AND ROWNUM = 1)
                               ,'R'
                               ,(SELECT REP_CMDT_NM
                                  FROM MDM_REP_CMDT
                                 WHERE REP_CMDT_CD = A.PRC_CMDT_DEF_CD
                                   AND ROWNUM = 1)
                               ,'C'
                               ,(SELECT CMDT_NM
                                  FROM MDM_COMMODITY
                                 WHERE CMDT_CD = A.PRC_CMDT_DEF_CD
                                   AND ROWNUM = 1)) AS PRC_CMDT_DEF_NM
                        ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, DECODE(PRC_CMDT_TP_CD, 'G', 1, 'R', 2, 'C', 3, 99), PRC_CMDT_DEF_CD) AS RN
                    FROM PRI_RP_SCP_RT_CMDT A
                   WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]
                     AND SVC_SCP_CD = @[svc_scp_cd]
                     AND SRC_INFO_CD <> 'AD')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                 AND PRIOR RN = RN - 1
           GROUP BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ) B
        ,(SELECT PROP_NO
                ,AMDT_SEQ
                ,SVC_SCP_CD
                ,CMDT_HDR_SEQ
                ,MAX(SYS_CONNECT_BY_PATH(CUST_CNT_CD || '|' || CUST_SEQ, '||')) AS CUST_LGL_ENG_CD
                ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(CUST_LGL_ENG_NM, '|||')), 4) AS CUST_LGL_ENG_NM
            FROM (SELECT PROP_NO
                        ,AMDT_SEQ
                        ,SVC_SCP_CD
                        ,CMDT_HDR_SEQ
                        ,CUST_CNT_CD
                        ,CUST_SEQ
                        ,(SELECT CUST_LGL_ENG_NM
                            FROM MDM_CUSTOMER
                           WHERE CUST_CNT_CD = A.CUST_CNT_CD
                             AND CUST_SEQ = A.CUST_SEQ
                             AND ROWNUM = 1) AS CUST_LGL_ENG_NM
                        ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ACT_CUST_SEQ) AS RN
                    FROM PRI_RP_SCP_RT_ACT_CUST A
                   WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]
                     AND SVC_SCP_CD = @[svc_scp_cd]
                     AND SRC_INFO_CD <> 'AD')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                 AND PRIOR RN = RN - 1
           GROUP BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ) C
        ,(SELECT PROP_NO
                ,AMDT_SEQ
                ,SVC_SCP_CD
                ,CMDT_HDR_SEQ
                ,ROUT_SEQ
                ,MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD || '|' || RCV_DE_TERM_CD, '||')) AS ROUT_PNT_LOC_DEF_CD
                ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_NM || NVL2(RCV_DE_TERM_NM, '(' || RCV_DE_TERM_NM || ')', '')
                                               ,'|||'))
                       ,4) AS ROUT_PNT_LOC_DEF_NM
            FROM (SELECT PROP_NO
                        ,AMDT_SEQ
                        ,SVC_SCP_CD
                        ,CMDT_HDR_SEQ
                        ,ROUT_SEQ
                        ,ORG_DEST_TP_CD
                        ,ROUT_PNT_LOC_DEF_CD
                        ,RCV_DE_TERM_CD
                        ,DECODE(ROUT_PNT_LOC_TP_CD
                               ,'G'
                               ,(SELECT PRC_GRP_LOC_DESC
                                  FROM PRI_RP_SCP_GRP_LOC
                                 WHERE PROP_NO = A.PROP_NO
                                   AND AMDT_SEQ = A.AMDT_SEQ
                                   AND SVC_SCP_CD = A.SVC_SCP_CD
                                   AND PRC_GRP_LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                   AND ROWNUM = 1)
                               ,'L'
                               ,(SELECT LOC_NM
                                  FROM MDM_LOCATION
                                 WHERE LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                   AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM
                        ,(SELECT INTG_CD_VAL_DP_DESC
                            FROM COM_INTG_CD_DTL
                           WHERE INTG_CD_ID = 'CD02138'
                             AND INTG_CD_VAL_CTNT = A.RCV_DE_TERM_CD
                             AND ROWNUM = 1) AS RCV_DE_TERM_NM
                        ,PRC_PROG_STS_CD
                        ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                    FROM PRI_RP_SCP_RT_ROUT_PNT A
                   WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]
                     AND SVC_SCP_CD = @[svc_scp_cd]
                     AND ORG_DEST_TP_CD = 'O'
                     AND SRC_INFO_CD <> 'AD')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                 AND PRIOR ROUT_SEQ = ROUT_SEQ
                 AND PRIOR RN = RN - 1
           GROUP BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) D
        ,(SELECT PROP_NO
                ,AMDT_SEQ
                ,SVC_SCP_CD
                ,CMDT_HDR_SEQ
                ,ROUT_SEQ
                ,MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '||')) AS ROUT_VIA_PORT_DEF_CD
                ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '|||')), 4) AS ROUT_VIA_PORT_DEF_NM
            FROM (SELECT PROP_NO
                        ,AMDT_SEQ
                        ,SVC_SCP_CD
                        ,CMDT_HDR_SEQ
                        ,ROUT_SEQ
                        ,ORG_DEST_TP_CD
                        ,ROUT_VIA_PORT_DEF_CD
                        ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                    FROM PRI_RP_SCP_RT_ROUT_VIA A
                   WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]
                     AND SVC_SCP_CD = @[svc_scp_cd]
                     AND ORG_DEST_TP_CD = 'O'
                     AND SRC_INFO_CD <> 'AD')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                 AND PRIOR ROUT_SEQ = ROUT_SEQ
                 AND PRIOR RN = RN - 1
           GROUP BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) E
        ,(SELECT PROP_NO
                ,AMDT_SEQ
                ,SVC_SCP_CD
                ,CMDT_HDR_SEQ
                ,ROUT_SEQ
                ,MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '||')) AS ROUT_VIA_PORT_DEF_CD
                ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '|||')), 4) AS ROUT_VIA_PORT_DEF_NM
            FROM (SELECT PROP_NO
                        ,AMDT_SEQ
                        ,SVC_SCP_CD
                        ,CMDT_HDR_SEQ
                        ,ROUT_SEQ
                        ,ORG_DEST_TP_CD
                        ,ROUT_VIA_PORT_DEF_CD
                        ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                    FROM PRI_RP_SCP_RT_ROUT_VIA A
                   WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]
                     AND SVC_SCP_CD = @[svc_scp_cd]
                     AND ORG_DEST_TP_CD = 'D'
                     AND SRC_INFO_CD <> 'AD')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                 AND PRIOR ROUT_SEQ = ROUT_SEQ
                 AND PRIOR RN = RN - 1
           GROUP BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) F
        ,(SELECT PROP_NO
                ,AMDT_SEQ
                ,SVC_SCP_CD
                ,CMDT_HDR_SEQ
                ,ROUT_SEQ
                ,MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD || '|' || RCV_DE_TERM_CD, '||')) AS ROUT_PNT_LOC_DEF_CD
                ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_NM || NVL2(RCV_DE_TERM_NM, '(' || RCV_DE_TERM_NM || ')', '')
                                               ,'|||'))
                       ,4) AS ROUT_PNT_LOC_DEF_NM
            FROM (SELECT PROP_NO
                        ,AMDT_SEQ
                        ,SVC_SCP_CD
                        ,CMDT_HDR_SEQ
                        ,ROUT_SEQ
                        ,ORG_DEST_TP_CD
                        ,ROUT_PNT_LOC_DEF_CD
                        ,RCV_DE_TERM_CD
                        ,DECODE(ROUT_PNT_LOC_TP_CD
                               ,'G'
                               ,(SELECT PRC_GRP_LOC_DESC
                                  FROM PRI_RP_SCP_GRP_LOC
                                 WHERE PROP_NO = A.PROP_NO
                                   AND AMDT_SEQ = A.AMDT_SEQ
                                   AND SVC_SCP_CD = A.SVC_SCP_CD
                                   AND PRC_GRP_LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                   AND ROWNUM = 1)
                               ,'L'
                               ,(SELECT LOC_NM
                                  FROM MDM_LOCATION
                                 WHERE LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                   AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM
                        ,(SELECT INTG_CD_VAL_DP_DESC
                            FROM COM_INTG_CD_DTL
                           WHERE INTG_CD_ID = 'CD02139'
                             AND INTG_CD_VAL_CTNT = A.RCV_DE_TERM_CD
                             AND ROWNUM = 1) AS RCV_DE_TERM_NM
                        ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                    FROM PRI_RP_SCP_RT_ROUT_PNT A
                   WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]
                     AND SVC_SCP_CD = @[svc_scp_cd]
                     AND ORG_DEST_TP_CD = 'D'
                     AND SRC_INFO_CD <> 'AD')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                 AND PRIOR ROUT_SEQ = ROUT_SEQ
                 AND PRIOR RN = RN - 1
           GROUP BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) G
   WHERE A.PROP_NO = B.PROP_NO(+)
     AND A.AMDT_SEQ = B.AMDT_SEQ(+)
     AND A.SVC_SCP_CD = B.SVC_SCP_CD(+)
     AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ(+)
     AND A.PROP_NO = C.PROP_NO(+)
     AND A.AMDT_SEQ = C.AMDT_SEQ(+)
     AND A.SVC_SCP_CD = C.SVC_SCP_CD(+)
     AND A.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ(+)
     AND A.PROP_NO = D.PROP_NO(+)
     AND A.AMDT_SEQ = D.AMDT_SEQ(+)
     AND A.SVC_SCP_CD = D.SVC_SCP_CD(+)
     AND A.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
     AND A.ROUT_SEQ = D.ROUT_SEQ(+)
     AND A.PROP_NO = E.PROP_NO(+)
     AND A.AMDT_SEQ = E.AMDT_SEQ(+)
     AND A.SVC_SCP_CD = E.SVC_SCP_CD(+)
     AND A.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ(+)
     AND A.ROUT_SEQ = E.ROUT_SEQ(+)
     AND A.PROP_NO = F.PROP_NO(+)
     AND A.AMDT_SEQ = F.AMDT_SEQ(+)
     AND A.SVC_SCP_CD = F.SVC_SCP_CD(+)
     AND A.CMDT_HDR_SEQ = F.CMDT_HDR_SEQ(+)
     AND A.ROUT_SEQ = F.ROUT_SEQ(+)
     AND A.PROP_NO = G.PROP_NO(+)
     AND A.AMDT_SEQ = G.AMDT_SEQ(+)
     AND A.SVC_SCP_CD = G.SVC_SCP_CD(+)
     AND A.CMDT_HDR_SEQ = G.CMDT_HDR_SEQ(+)
     AND A.ROUT_SEQ = G.ROUT_SEQ(+)
)
SELECT A.PROP_NO
      ,A.AMDT_SEQ
      ,A.SVC_SCP_CD
      ,A.CMDT_HDR_SEQ
      ,A.ROUT_SEQ
      ,A.RT_SEQ
      ,A.PRC_CMDT_DEF_NM
      ,A.CUST_LGL_ENG_NM
      ,A.ORG_ROUT_PNT_LOC_DEF_NM
      ,A.ORG_ROUT_VIA_PORT_DEF_NM
      ,A.DEST_ROUT_VIA_PORT_DEF_NM
      ,A.DEST_ROUT_PNT_LOC_DEF_NM
      ,A.RAT_UT_CD
      ,A.PRC_CGO_TP_CD
      ,A.CURR_CD
      ,A.PROP_FRT_RT_AMT
      ,DENSE_RANK() OVER(PARTITION BY NULL ORDER BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.PRC_CMDT_DEF_CD, A.CUST_LGL_ENG_CD, A.ORG_ROUT_PNT_LOC_DEF_CD, A.ORG_ROUT_VIA_PORT_DEF_CD, A.DEST_ROUT_VIA_PORT_DEF_CD, A.DEST_ROUT_PNT_LOC_DEF_CD, A.RAT_UT_CD, A.PRC_CGO_TP_CD) AS SEQ
      ,'Show ' || CHR(64 + ROW_NUMBER() OVER(PARTITION BY A.PROP_NO
                          ,A.AMDT_SEQ
                          ,A.SVC_SCP_CD
                          ,A.PRC_CMDT_DEF_CD
                          ,A.CUST_LGL_ENG_CD
                          ,A.ORG_ROUT_PNT_LOC_DEF_CD
                          ,A.ORG_ROUT_VIA_PORT_DEF_CD
                          ,A.DEST_ROUT_VIA_PORT_DEF_CD
                          ,A.DEST_ROUT_PNT_LOC_DEF_CD
                          ,A.RAT_UT_CD
                          ,A.PRC_CGO_TP_CD ORDER BY A.RT_SEQ)) AS VIEW_TEXT
  FROM T A
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,PRC_CMDT_DEF_CD
              ,CUST_LGL_ENG_CD
              ,ORG_ROUT_PNT_LOC_DEF_CD
              ,ORG_ROUT_VIA_PORT_DEF_CD
              ,DEST_ROUT_VIA_PORT_DEF_CD
              ,DEST_ROUT_PNT_LOC_DEF_CD
              ,RAT_UT_CD
              ,PRC_CGO_TP_CD
          FROM T
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,PRC_CMDT_DEF_CD
                 ,CUST_LGL_ENG_CD
                 ,ORG_ROUT_PNT_LOC_DEF_CD
                 ,ORG_ROUT_VIA_PORT_DEF_CD
                 ,DEST_ROUT_VIA_PORT_DEF_CD
                 ,DEST_ROUT_PNT_LOC_DEF_CD
                 ,RAT_UT_CD
                 ,PRC_CGO_TP_CD
        HAVING COUNT(*) > 1) B
 WHERE A.PROP_NO = B.PROP_NO
   AND A.AMDT_SEQ = B.AMDT_SEQ
   AND A.SVC_SCP_CD = B.SVC_SCP_CD
   AND A.PRC_CMDT_DEF_CD = B.PRC_CMDT_DEF_CD
   AND A.CUST_LGL_ENG_CD = B.CUST_LGL_ENG_CD
   AND A.ORG_ROUT_PNT_LOC_DEF_CD = B.ORG_ROUT_PNT_LOC_DEF_CD
   AND A.ORG_ROUT_VIA_PORT_DEF_CD = B.ORG_ROUT_VIA_PORT_DEF_CD
   AND A.DEST_ROUT_VIA_PORT_DEF_CD = B.DEST_ROUT_VIA_PORT_DEF_CD
   AND A.DEST_ROUT_PNT_LOC_DEF_CD = B.DEST_ROUT_PNT_LOC_DEF_CD
   AND A.RAT_UT_CD = B.RAT_UT_CD
   AND A.PRC_CGO_TP_CD = B.PRC_CGO_TP_CD
			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
