<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DubaiManifestListDownloadDBDAOremoveBkgCstmsDuVvdDSQL">
			<desc><![CDATA[removeBkgCstmsDuVvd]]></desc>
			<sql><![CDATA[
DELETE 
  FROM BKG_CSTMS_DU_VVD V
 WHERE 1 = 1
#if (${vvd} != '' && ${bl_no} == '')
   AND V.VSL_CD = SUBSTR(@[vvd],1,4)
   AND V.SKD_VOY_NO = SUBSTR(@[vvd],5,4)
   AND V.SKD_DIR_CD = SUBSTR(@[vvd],9,1)
   AND V.POD_CD = @[pod_cd]                               
#else
   AND VSL_CD || SKD_VOY_NO || SKD_DIR_CD || POD_CD IN 
       (
         SELECT DISTINCT 
                V.VSL_CD || V.SKD_VOY_NO || V.SKD_dIR_CD || V.POD_CD
           FROM BKG_BOOKING B
               ,BKG_VVD V
          WHERE B.BKG_NO = V.BKG_NO
        #if (${bl_no} != '') 
            AND BL_NO = @[bl_no]
        #end
        #if (${vvd} != '') 
            AND V.VSL_CD = SUBSTR(@[vvd],1,4)
            AND V.SKD_VOY_NO = SUBSTR(@[vvd],5,4)
            AND V.SKD_DIR_CD = SUBSTR(@[vvd],9,1)
        #end
        #if (${pod_cd} != '')
            AND V.POD_CD = @[pod_cd]                               
        #end
        #if (${pol_cd} != '')      
            AND V.POL_CD = @[pol_cd]
        #end
        #if (${cgo_type} == 'F') 
            AND B.BKG_CGO_TP_CD IN ('F')
        #elseif (${cgo_type} == 'M') 
            AND B.BKG_CGO_TP_CD IN ('R', 'P')
        #end
       )
#end 			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
