<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOModifyUCbyCustomerListUSQL">
			<desc><![CDATA[Unit Cost by Customer (Door. CY Exception) - Update]]></desc>
			<sql><![CDATA[
UPDATE MAS_CHSS_EXPT_CUST_LIST
   SET USA_IO_BND_CD      = TRIM(@[usa_io_bnd_cd])
     , BKG_RCV_DE_TERM_CD = TRIM(@[bkg_rcv_de_term_cd])
     , STND_UC_AMT        = @[stnd_uc_amt]
     , UPD_USR_ID         = @[upd_usr_id]
     , UPD_DT             = SYSDATE
 WHERE COST_YRMON = @[cost_yrmon]
   AND CTRT_NO    = @[ctrt_no]
   AND SCC_CD     = @[scc_cd]
   AND CTRT_SEQ   = @[ctrt_seq]			]]></sql>
			<params>
				<param name="usa_io_bnd_cd" type="12" value="" out="N"/>
				<param name="bkg_rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="stnd_uc_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="ctrt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
