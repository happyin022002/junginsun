<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOsearchContactInfoRSQL">
			<desc><![CDATA[Contact Info 조회]]></desc>
			<sql><![CDATA[
    SELECT USR_NM
         , XTN_PHN_NO
         , FAX_NO
         , USR_EML
    FROM COM_USER
    WHERE USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
