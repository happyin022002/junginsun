<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchCntcPsonEmlRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT CNTC_PSON_EML, CNTC_PSON_PHN_NO
  FROM (SELECT CNTC_PSON_EML, CNTC_PSON_PHN_NO
          FROM BKG_CNTC_PSON
         WHERE BKG_NO = @[bkg_no]
           AND CNTC_PSON_EML IS NOT NULL
           --AND BKG_CNTC_PSON_TP_CD = 'BK'
         ORDER BY BKG_CNTC_PSON_TP_CD DESC)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
