<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateQuotationDBDAORsltPriSurchargeLastAccessDateVORSQL">
			<desc><![CDATA[   S/C Quotation Surcharge View All]]></desc>
			<sql><![CDATA[
SELECT GEN_SPCL_RT_TP_CD ,TO_CHAR(MAX(CRE_DT),'YYYY-MM-DD') CRE_YMD 
FROM PRI_SQ_RT_SCG_ROUT
WHERE 
	QTTN_NO = @[qttn_no]
	AND QTTN_VER_NO = @[qttn_ver_no]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
    AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end 
GROUP BY  GEN_SPCL_RT_TP_CD			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
