<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOCheckAddRFIHCGuidelineDetailRSQL">
			<desc><![CDATA[Check Add RF IHC Guideline Detail]]></desc>
			<sql><![CDATA[
SELECT PNT_LOC_CD
     , BSE_PORT_LOC_CD
     , RCV_DE_TERM_CD
  FROM PRI_TRF_IHC_RT
WHERE 1=1
  AND SVC_SCP_CD = @[svc_scp_cd]
  AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
  AND IHC_TRF_NO = @[ihc_trf_no]
  AND AMDT_SEQ = @[amdt_seq]
  AND IHC_CGO_TP_CD = 'DR'
  AND PNT_LOC_CD = @[pnt_loc_cd]
  AND RCV_DE_TERM_CD = @[rcv_de_term_cd]
  AND BSE_PORT_LOC_CD = @[bse_port_loc_cd]
  AND OPTM_TRSP_MOD_FLG = 'Y'
  AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="pnt_loc_cd" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="bse_port_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
