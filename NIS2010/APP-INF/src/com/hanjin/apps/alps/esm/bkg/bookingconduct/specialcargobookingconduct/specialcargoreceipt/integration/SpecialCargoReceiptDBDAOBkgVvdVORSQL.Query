<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOBkgVvdVORSQL">
			<desc><![CDATA[BkgVvdVO]]></desc>
			<sql><![CDATA[
SELECT A1.BKG_NO
      ,A1.VSL_PRE_PST_CD
      ,A1.VSL_SEQ
      ,A1.VSL_CD
      ,A1.SKD_VOY_NO
      ,A1.SKD_DIR_CD
      ,A1.POL_CLPT_IND_SEQ
      ,A1.POL_CD
      ,A1.POD_CD
      ,A1.CRE_USR_ID
      ,A1.CRE_DT
      ,A1.UPD_USR_ID
      ,A1.UPD_DT
      ,A1.POD_CLPT_IND_SEQ
      ,A1.POL_YD_CD
      ,A1.POD_YD_CD
      ,A1.SLAN_CD
FROM BKG_VVD A1
,BKG_BL_DOC A2
,MDM_VSL_SVC_LANE A3
WHERE A1.BKG_NO = @[bkg_no]
AND A1.BKG_NO = A2.BKG_NO
AND A1.VSL_PRE_PST_CD = DECODE(A2.BDR_FLG,'N',A1.VSL_PRE_PST_CD, 'U')
AND A1.SLAN_CD = A3.VSL_SLAN_CD
AND A3.SPCL_CGO_RQST_TGT_LANE_FLG = 'Y'
AND A3.VSL_SVC_TP_CD <> 'O' 
AND A3.DELT_FLG = 'N'
AND A1.VSL_CD IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
