<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAOAddTsPlanGuideDetailListCSQL">
			<desc><![CDATA[[T/S Plan & guide detail]을 [insert]합니다.]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_TS_PLN_GID_DTL 
          ( 
            PLN_SEQ
          , REP_TRD_CD
          , REP_SUB_TRD_CD
          , RLANE_CD
          , VVD_CD
          , IRR_PORT_CD
          , IRR_YD_CD
          , DIR_CD
          , IOC_CD
          , CNTR_FULL_FLG
          , CNTR_MTY_FLG
          , MLT_CRR_LIST_CTNT
          , MLT_POL_LIST_CTNT
          , N1ST_PORT_ETD_DT
          , N1ST_RLANE_CD
          , N1ST_VVD_CD
          , N1ST_PORT_ETB_DT
          , N1ST_POD_CD
          , N1ST_POD_YD_CD
          , N2ND_POL_CD
          , N2ND_POL_YD_CD
          , N2ND_PORT_ETD_DT
          , N2ND_RLANE_CD
          , N2ND_VVD_CD
          , N2ND_PORT_ETB_DT
          , N2ND_POD_CD
          , N2ND_POD_YD_CD
          , N3RD_POL_CD
          , N3RD_POL_YD_CD
          , N3RD_PORT_ETD_DT
          , N3RD_RLANE_CD
          , N3RD_VVD_CD
          , N3RD_PORT_ETB_DT
          , N3RD_POD_CD
          , N3RD_POD_YD_CD
          , N4TH_POL_CD
          , N4TH_POL_YD_CD
          , N4TH_PORT_ETD_DT
          , N4TH_RLANE_CD
          , N4TH_VVD_CD
          , N4TH_PORT_ETB_DT
          , N4TH_POD_CD
          , N4TH_POD_YD_CD
          , N5TH_POL_CD
          , N5TH_POL_YD_CD
          , N5TH_PORT_ETD_DT
          , N5TH_RLANE_CD
          , N5TH_VVD_CD
          , N5TH_PORT_ETB_DT
          , MLT_POD_LIST_CTNT
          , TS_PLN_CFM_STS_CD
          , TS_RMK
          , CRE_USR_ID
          , CRE_DT
          , UPD_USR_ID
          , UPD_DT 
          )
VALUES ( 
           (
           SELECT NVL(MAX(PLN_SEQ),0)+1
            FROM  SPC_TS_PLN_GID_DTL
            WHERE REP_TRD_CD     = @[rep_trd_cd]
            AND   REP_SUB_TRD_CD = @[rep_sub_trd_cd]
            AND   RLANE_CD       = @[rlane_cd]
            AND   VVD_CD         = @[vvd_cd]
            AND   IRR_PORT_CD    = @[irr_port_cd]
            AND   IRR_YD_CD      = @[irr_yd_cd]
           ) 
          ,@[rep_trd_cd]
          ,@[rep_sub_trd_cd]
          ,@[rlane_cd]
          ,@[vvd_cd]
          ,@[irr_port_cd]
          ,@[irr_yd_cd]
          ,@[dir_cd]
          ,@[ioc_cd]
          ,DECODE(@[cntr_full_flg], 1, 'Y', 'N')
          ,DECODE(@[cntr_mty_flg], 1, 'Y', 'N')
          ,@[mlt_crr_list_ctnt]
          ,@[mlt_pol_list_ctnt]
          ,TO_DATE(@[n1st_port_etd_dt],'YYYYMMDD')
          ,@[n1st_rlane_cd]
          ,@[n1st_vvd_cd]
          ,TO_DATE(@[n1st_port_etb_dt],'YYYYMMDD')
          ,@[n1st_pod_cd]
          ,@[n1st_pod_yd_cd]
          ,@[n2nd_pol_cd]
          ,@[n2nd_pol_yd_cd]
          ,TO_DATE(@[n2nd_port_etd_dt],'YYYYMMDD')
          ,@[n2nd_rlane_cd]
          ,@[n2nd_vvd_cd]
          ,TO_DATE(@[n2nd_port_etb_dt],'YYYYMMDD')
          ,@[n2nd_pod_cd]
          ,@[n2nd_pod_yd_cd]
          ,@[n3rd_pol_cd]
          ,@[n3rd_pol_yd_cd]
          ,TO_DATE(@[n3rd_port_etd_dt],'YYYYMMDD')
          ,@[n3rd_rlane_cd]
          ,@[n3rd_vvd_cd]
          ,TO_DATE(@[n3rd_port_etb_dt],'YYYYMMDD')
          ,@[n3rd_pod_cd]
          ,@[n3rd_pod_yd_cd]
          ,@[n4th_pol_cd]
          ,@[n4th_pol_yd_cd]
          ,TO_DATE(@[n4th_port_etd_dt],'YYYYMMDD')
          ,@[n4th_rlane_cd]
          ,@[n4th_vvd_cd]
          ,TO_DATE(@[n4th_port_etb_dt],'YYYYMMDD')
          ,@[n4th_pod_cd]
          ,@[n4th_pod_yd_cd]
          ,@[n5th_pol_cd]
          ,@[n5th_pol_yd_cd]
          ,TO_DATE(@[n5th_port_etd_dt],'YYYYMMDD')
          ,@[n5th_rlane_cd]
          ,@[n5th_vvd_cd]
          ,TO_DATE(@[n5th_port_etb_dt],'YYYYMMDD')
          ,@[mlt_pod_list_ctnt]
          ,@[ts_pln_cfm_sts_cd]
          ,@[ts_rmk]
          ,@[cre_usr_id]
          ,sysdate
          ,@[upd_usr_id]
          ,sysdate 
        )			]]></sql>
			<params>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="rep_sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
