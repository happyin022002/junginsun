<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOSearchOwnersAccountCancellationSlipRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.SLP_TP_CD
       , A.SLP_FUNC_CD
       , A.SLP_OFC_CD
       , A.SLP_ISS_DT
       , A.SLP_SER_NO
       , A.SLP_SEQ_NO
       , '962111' ACCT_CD
       , A.CTR_CD
       , B.SLP_LOC_CD
       , A.N2ND_CURR_CD CSR_CURR_CD
       , A.N2ND_AMT CSR_AMT
       , A.AP_DESC CSR_DESC
       , A.VNDR_SEQ
       , A.N1ST_CURR_CD TRNS_CURR_CD
       , A.N1ST_AMT TRNS_AMT
       , B.VAT_FLG
       , B.STL_FLG
       , B.INV_SEQ
       , B.FLET_SRC_TP_CD
       , A.VSL_CD
       , A.SKD_VOY_NO
       , A.SKD_DIR_CD
       , A.REV_DIR_CD
	     , A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD||A.REV_DIR_CD VVD_CD
       , A.SLP_TP_CD ORG_SLP_TP_CD
       , A.SLP_FUNC_CD ORG_SLP_FUNC_CD
       , A.SLP_OFC_CD ORG_SLP_OFC_CD
       , A.SLP_ISS_DT ORG_ISS_DT
       , A.SLP_SER_NO ORG_SLP_SER_NO
       , A.SLP_SEQ_NO ORG_SLP_SEQ_NO
       , B.TO_INV_NO
       , A.CRE_USR_ID
       , A.CRE_DT
       , A.UPD_USR_ID
       , A.UPD_DT
       , A.ACCT_ITM_SEQ
       , B.OA_INV_DT
       , B.ATCH_FILE_OA_LNK_ID 
       , B.OA_LOC_CD
  FROM FMS_OWNR_ACCT_SLP A,
       FMS_CSUL_SLP B
 WHERE 1 = 1
   AND A.SLP_TP_CD||A.SLP_FUNC_CD||A.SLP_OFC_CD||A.SLP_ISS_DT||A.SLP_SER_NO||A.SLP_SEQ_NO =
        B.SLP_TP_CD(+)||B.SLP_FUNC_CD(+)||B.SLP_OFC_CD(+)||B.SLP_ISS_DT(+)||B.SLP_SER_NO(+)||B.SLP_SEQ_NO(+) 
#if(${s_csr_no} != '')
   AND A.SLP_TP_CD||A.SLP_FUNC_CD||A.SLP_OFC_CD||A.SLP_ISS_DT||A.SLP_SER_NO||A.SLP_SEQ_NO IN ( 
	#foreach ($user_csr_no IN ${csrNos})
		#if($velocityCount < $csrNos.size())
			'$user_csr_no',
		#else
			'$user_csr_no'
		#end
	#end              
	)
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
