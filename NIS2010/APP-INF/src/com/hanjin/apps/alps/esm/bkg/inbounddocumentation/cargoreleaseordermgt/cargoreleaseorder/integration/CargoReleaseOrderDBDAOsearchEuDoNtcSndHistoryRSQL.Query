<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchEuDoNtcSndHistoryRSQL">
			<desc><![CDATA[searchEuDoNtcSndHistory]]></desc>
			<sql><![CDATA[
SELECT BKGM.BKG_NO                                                  AS BKG_NO
      ,NULL                                                         AS HIS_SEQ
      ,@[ntc_via_cd]                                                AS NTC_VIA_CD
      ,'DO'                                                         AS NTC_KND_CD
      ,NULL                                                         AS CNTR_NO
      ,NULL                                                         AS AGN_CD
      ,NULL                                                         AS NTC_FOM_CD
      ,NULL                                                         AS NTC_SEQ
      ,BCST.BKG_CUST_TP_CD                                          AS BKG_CUST_TP_CD
      ,NULL                                                         AS CUST_CNTC_TP_CD
      ,decode(@[ntc_via_cd],'F',@[ntc_fax_no],NULL)                 AS NTC_FAX_NO
      ,decode(@[ntc_via_cd],'E',@[ntc_eml],NULL)                    AS NTC_EML
      ,@[snd_id]                                                    AS SND_ID
      ,NULL                                                         AS EDI_ID   -- 수신 ID
      ,NULL                                                         AS DO_EDI_TP_CD   -- 수신 ID
      ,NULL                                                         AS ESVC_GRP_CD
      ,NULL                                                         AS BKG_NTC_SND_RSLT_CD
      ,NULL                                                         AS TML_NTC_SND_STS_CD
      ,NULL                                                         AS CGOR_RCVR_TP_CD
      ,NULL                                                         AS CGOR_STS_CD
      ,'N'                                                          AS FRT_HDN_FLG
      ,'N'                                                          AS FRT_ALL_FLG
      ,'N'                                                          AS FRT_CLT_FLG
      ,'N'                                                          AS FRT_PPD_FLG
      ,'N'                                                          AS FRT_CHG_FLG
      ,'N'                                                          AS FRT_ARR_FLG
      ,@[ofc_cd]                                                    AS SND_OFC_CD
      ,@[usr_id]                                                    AS SND_USR_ID
      ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,GLOBALDATE_PKG.GET_LOCCD_FNC(@[ofc_cd]) ), 'YYYYMMDD HH24MISS')      AS SND_RQST_DT
      ,NULL                                                        AS SND_RTY_KNT
      ,NULL                                                        AS SND_DT 
      ,NULL                                                        AS SND_GDT 
      ,NULL                                                        AS DIFF_RMK
      ,NULL                                                        AS CUST_CNTC_AMD_FLG
      ,NULL                                                        AS DP_HDN_FLG
      ,@[cre_usr_id]                                               AS CRE_USR_ID
      ,SYSDATE                                                     AS CRE_DT
      ,@[upd_usr_id]                                               AS UPD_USR_ID
      ,SYSDATE                                                     AS UPD_DT   
FROM BKG_DO       BDO,
     BKG_BOOKING  BKGM,
     BKG_CUSTOMER BCST
WHERE DO_NO               = @[do_no]
  AND DO_NO_SPLIT         = @[do_no_split]
  AND BKGM.BKG_NO         = BDO.BKG_NO  
  AND BCST.BKG_NO         = BKGM.BKG_NO
  AND BCST.BKG_CUST_TP_CD = DECODE(BKGM.CUST_TO_ORD_FLG,'Y','N','C')			]]></sql>
			<params>
				<param name="ntc_via_cd" type="12" value="" out="N"/>
				<param name="ntc_fax_no" type="12" value="" out="N"/>
				<param name="ntc_eml" type="12" value="" out="N"/>
				<param name="snd_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="do_no" type="12" value="" out="N"/>
				<param name="do_no_split" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
