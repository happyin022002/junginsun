<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RASCommonDBDAOBkgRevUmchSubTpRSQL">
			<desc><![CDATA[BkgRevUmchSubTp]]></desc>
			<sql><![CDATA[
SELECT UMCH_SUB_TP_CD AS CD
,      UMCH_SUB_TP_DESC AS NM
FROM BKG_REV_UMCH_SUB_TP
WHERE UMCH_TP_CD = @[etc1]
UNION ALL
SELECT SUB.UMCH_SUB_TP_CD AS CD
,      SUB.UMCH_SUB_TP_DESC AS NM
FROM BKG_REV_UMCH_SUB_TP SUB, BKG_REV_UMCH_TP MN
WHERE SUB.UMCH_TP_CD = MN.UMCH_TP_CD
AND MN.UMCH_TP_DESC = @[etc1]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
