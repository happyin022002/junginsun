<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOBkgdocClzSetVODSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE BKG_DOC_CLZ_SET SET 
	DELT_FLG = 'Y'
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	VSL_SLAN_CD = @[vsl_slan_cd]
AND	YD_CD = @[yd_cd]
AND	DEST_CNT_CD = @[dest_cnt_cd]
AND CONTI_CD = @[conti_cd]
AND VVD_CD = DECODE(@[vvd_cd],'ALL','*',@[vvd_cd])			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="dest_cnt_cd" type="12" value="" out="N"/>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
