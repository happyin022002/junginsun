<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAYearlyPlanDBDAOInsertBsaBudSltChtrCrrCapaCSQL">
			<desc><![CDATA[BSA_BUD_SLT_CHTR_CRR_CAPA 데이타를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BSA_BUD_SLT_CHTR_CRR_CAPA (
    TRD_CD      ,
    RLANE_CD    ,
    DIR_CD      ,
    BSA_SEQ     ,
    VSL_SEQ     ,
    BSA_OP_CD   ,
    BSA_OP_JB_CD,
    CRR_CD      ,
    CRR_BSA_CAPA,
    CRE_USR_ID  ,
    CRE_DT      ,
    UPD_USR_ID  ,
    UPD_DT
)
  SELECT B.TRD_CD       ,
         B.RLANE_CD     ,
         B.DIR_CD       ,
         B.BSA_SEQ      ,
         B.VSL_SEQ      ,
         B.BSA_OP_CD    ,
         B.BSA_OP_JB_CD ,
         B.CRR_CD       ,
         B.CRR_BSA_CAPA ,
         @[cre_usr_id]  ,
         SYSDATE        ,
         @[upd_usr_id]  ,
         SYSDATE
    FROM BSA_BUD_SLT_CHTR_BZC  A,
         BSA_SLT_CHTR_CRR_CAPA B
   WHERE 1=1
     AND A.TRD_CD   = B.TRD_CD
     AND A.RLANE_CD = B.RLANE_CD
     AND A.DIR_CD   = B.DIR_CD
     AND A.VSL_SEQ  = B.VSL_SEQ
     AND A.BSA_SEQ  = B.BSA_SEQ
     
#if (${trd_cd} != 'All')
     AND A.TRD_CD   = @[trd_cd]
#end

#if (${rlane_cd} != 'All')
     AND A.RLANE_CD = @[rlane_cd]
#end			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
