<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOMultiSimMInfoListCSQL">
			<desc><![CDATA[sim info 등록 및 수정]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_SIM_INFO A
 USING (SELECT @[sim_dt] AS SIM_DT,
               @[sim_no] AS SIM_NO
        FROM DUAL
       ) B
 ON ( A.SIM_DT = B.SIM_DT AND
      A.SIM_NO = B.SIM_NO
    )
 WHEN MATCHED THEN
      UPDATE
      SET    SIM_RMK = @[sim_rmk],
             EXTD_LANE_FLG = @[extd_lane_flg],
             UPD_USR_ID = @[upd_usr_id],
             UPD_DT = SYSDATE
 WHEN NOT MATCHED THEN
      INSERT (SLAN_CD, 
              SIM_DT, 
              SIM_NO, 
			  SIM_DEPT_CD, 
			  EXTD_LANE_FLG, 
			  SIM_RMK, 
			  CRE_USR_ID, 
			  CRE_DT, 
			  UPD_USR_ID, 
			  UPD_DT)
      VALUES (@[slan_cd], @[sim_dt], @[sim_no], @[dept_cd], @[extd_lane_flg],
              @[sim_rmk], @[cre_usr_id], SYSDATE, @[upd_usr_id],SYSDATE)			]]></sql>
			<params>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="sim_rmk" type="12" value="" out="N"/>
				<param name="extd_lane_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="dept_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
