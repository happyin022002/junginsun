<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOValidateLocByOrzRSQL">
			<desc><![CDATA[Location Validation]]></desc>
			<sql><![CDATA[
SELECT 'Y' 
FROM MDM_ORGANIZATION
WHERE DELT_FLG ='N'
AND AR_HD_QTR_OFC_CD = @[rgn_ofc_cd]
AND SUBSTR(LOC_CD,1,2) = @[cnt_cd]
AND PRNT_OFC_CD IS NOT NULL
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
