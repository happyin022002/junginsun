<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterBkgAckReceiverRSQL">
			<desc><![CDATA[searchXterBkgAckReceiver]]></desc>
			<sql><![CDATA[
select 'Y' ACK_RECEIVER
  from bkg_hrd_cdg_ctnt
 where HRD_CDG_ID = 'XTER_ACK_RECEIVER'
   and ATTR_CTNT1 = @[sender_id]
  and exists (select 'x'
                 from bkg_xter_rqst_mst
                where xter_sndr_id = @[sender_id]
                  and xter_rqst_no = @[rqst_no]
                  and xter_rqst_seq = @[rqst_seq] 
                  and xter_rqst_via_cd = 'EDI' )			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
