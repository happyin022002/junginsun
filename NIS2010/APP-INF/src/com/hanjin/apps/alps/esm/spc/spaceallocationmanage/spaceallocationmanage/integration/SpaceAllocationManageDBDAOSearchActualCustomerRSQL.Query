<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOSearchActualCustomerRSQL">
			<desc><![CDATA[* 2014.12.08 신자영 [CHM-201433097] Control Option - A/C 및 CDMT추가 건 상세
* 2014.12.12 신자영 [CHM-201433097] Actual Account 조회 시 SC/RFA상의 contractor조회 추가]]></desc>
			<sql><![CDATA[
#if (${type} == 'B')
SELECT DISTINCT CODE,
                CUST_LGL_ENG_NM,
	   			SC_NO,
	   			RFA_NO
FROM   (
        SELECT  HD.SC_NO AS SC_NO,
                '*' AS RFA_NO,
                AC.CUST_CNT_CD || LPAD(AC.CUST_SEQ, 6, 0) AS CODE     ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')          AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')          AS TO_DT    ,
                AC.CUST_CNT_CD  ,
                AC.CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = AC.CUST_CNT_CD AND A.CUST_SEQ = AC.CUST_SEQ )  CUST_LGL_ENG_NM
                --,RANK() OVER (PARTITION BY SM.SVC_SCP_CD ORDER BY DECODE(BK.SVC_SCP_CD, SM.SVC_SCP_CD, 1, 2)) AS RN
        FROM    PRI_SP_HDR    HD  ,
                PRI_SP_MN     MN  ,
                PRI_SP_SCP_MN SM  ,
                PRI_SP_SCP_RT_ACT_CUST  AC  
        WHERE   1 = 1
        AND     HD.SC_NO        = @[sc_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='F'        /*상수*/ 
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        --AND     SM.SVC_SCP_CD   IN ( BK.SVC_SCP_CD, DECODE(BK.SVC_SCP_CD, 'ACE', 'TPE', 'MXE', 'TPE') )
        AND     AC.PROP_NO      = SM.PROP_NO
        AND     AC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     AC.SVC_SCP_CD   = SM.SVC_SCP_CD
#if (${app_dt} != '')     
	AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT 
#end   
        UNION
        SELECT  HD.SC_NO AS SC_NO,
                '*' AS RFA_NO,
                NC.BKG_ACT_CUST_CNT_CD || LPAD(NC.BKG_ACT_CUST_SEQ, 6, 0) AS CODE ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')  AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')  AS TO_DT    ,
                NC.BKG_ACT_CUST_CNT_CD            AS CUST_CNT_CD  ,
                NC.BKG_ACT_CUST_SEQ               AS CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = NC.BKG_ACT_CUST_CNT_CD AND A.CUST_SEQ = NC.BKG_ACT_CUST_SEQ )  CUST_LGL_ENG_NM
                --,RANK() OVER (PARTITION BY SM.SVC_SCP_CD ORDER BY DECODE(BK.SVC_SCP_CD, SM.SVC_SCP_CD, 1, 2)) AS RN
        FROM    PRI_SP_HDR        HD  ,
                PRI_SP_MN         MN  ,
                PRI_SP_SCP_MN     SM  ,
                PRI_SC_NOTE_CONV  NC  
        WHERE   1 = 1
        AND     HD.SC_NO        = @[sc_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='F'        /*상수*/
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
       -- AND     SM.SVC_SCP_CD   IN ( BK.SVC_SCP_CD, DECODE(BK.SVC_SCP_CD, 'ACE', 'TPE', 'MXE', 'TPE') )
        AND     NC.PROP_NO      = SM.PROP_NO
        AND     NC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     NC.SVC_SCP_CD   = SM.SVC_SCP_CD
        AND     NC.BKG_ACT_CUST_CNT_CD IS NOT NULL
#if (${app_dt} != '')     
	AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT
	AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN NC.EFF_DT AND NC.EXP_DT
#end   
       )
--WHERE RN = 1  
WHERE 1 = 1 
/*CUSTOMER Name*/
#if (${cust_lgl_eng_nm} != '') 
AND UPPER(CUST_LGL_ENG_NM) LIKE '%'||UPPER(@[cust_lgl_eng_nm])||'%'
#end
 
#if (${sc_no} == '') 
AND 'Y' = 'N'/*SWITCH*/
#end
UNION ALL
SELECT DISTINCT CODE,
                CUST_LGL_ENG_NM,
	   			SC_NO,
	   			RFA_NO
FROM   (
        SELECT  '*' AS SC_NO,
                HD.RFA_NO AS RFA_NO,
				AC.CUST_CNT_CD || LPAD(AC.CUST_SEQ, 6, 0) AS CODE     ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')            AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')            AS TO_DT    ,
                AC.CUST_CNT_CD  ,
                AC.CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = AC.CUST_CNT_CD AND A.CUST_SEQ = AC.CUST_SEQ )  CUST_LGL_ENG_NM
        FROM    PRI_RP_HDR    HD  ,
                PRI_RP_MN     MN  ,
                PRI_RP_SCP_MN SM  ,
                PRI_RP_SCP_RT_ACT_CUST  AC  
        WHERE   1 = 1
        AND     HD.RFA_NO        = @[rfa_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='A'        /*상수*/
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        --AND     SM.SVC_SCP_CD   = BK.SVC_SCP_CD
        AND     AC.PROP_NO      = SM.PROP_NO
        AND     AC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     AC.SVC_SCP_CD   = SM.SVC_SCP_CD
#if (${app_dt} != '')     
	AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT
#end   
        UNION
        SELECT  '*' AS SC_NO,
                HD.RFA_NO AS RFA_NO,
				NC.BKG_ACT_CUST_CNT_CD || LPAD(NC.BKG_ACT_CUST_SEQ, 6, 0) AS CODE ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')  AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')  AS TO_DT    ,
                NC.BKG_ACT_CUST_CNT_CD            AS CUST_CNT_CD  ,
                NC.BKG_ACT_CUST_SEQ               AS CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = NC.BKG_ACT_CUST_CNT_CD AND A.CUST_SEQ = NC.BKG_ACT_CUST_SEQ )  CUST_LGL_ENG_NM
        FROM    PRI_RP_HDR        HD  ,
                PRI_RP_MN         MN  ,
                PRI_RP_SCP_MN     SM  ,
                PRI_SC_NOTE_CONV  NC  
        WHERE   1 = 1
        AND     HD.RFA_NO        = @[rfa_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='A'        /*상수*/
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
       -- AND     SM.SVC_SCP_CD   = BK.SVC_SCP_CD
        AND     NC.PROP_NO      = SM.PROP_NO
        AND     NC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     NC.SVC_SCP_CD   = SM.SVC_SCP_CD
#if (${app_dt} != '')     
	AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT
	AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN NC.EFF_DT AND NC.EXP_DT
#end   
) A
WHERE  1=1
/*CUSTOMER Name*/
#if (${cust_lgl_eng_nm} != '') 
AND UPPER(CUST_LGL_ENG_NM) LIKE '%'||UPPER(@[cust_lgl_eng_nm])||'%'
#end

#if (${rfa_no} == '') 
AND 'Y' = 'N'/*SWITCH*/
#end
#elseif (${type} == 'C')
SELECT A.CMDT_CD AS CODE, 
       A.CMDT_NM AS CUST_LGL_ENG_NM,
	   B.SC_NO,
	   B.RFA_NO
FROM MDM_COMMODITY A, 
    (SELECT DISTINCT CMDT.PRC_CMDT_DEF_CD,
			H.SC_NO AS SC_NO,
            '*' AS RFA_NO
       FROM PRI_SP_HDR H
            ,PRI_SP_MN  M
            ,PRI_SP_SCP_MN SCP_M
            ,PRI_SP_SCP_RT_CMDT_HDR C_HDR
            ,PRI_SP_SCP_RT_CMDT  CMDT
      WHERE H.SC_NO             =  @[sc_no]
        AND H.PROP_NO           = M.PROP_NO
#if (${app_dt} != '')   
        AND M.EFF_DT            <=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
        AND M.EXP_DT            >=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
#end
        AND M.PROP_STS_CD       = 'F'
        AND M.PROP_NO           = SCP_M.PROP_NO
        AND M.AMDT_SEQ          = SCP_M.AMDT_SEQ
--AND     SCP_M.SVC_SCP_CD      = 'TPE'
        AND SCP_M.PROP_NO       = C_HDR.PROP_NO
        AND SCP_M.AMDT_SEQ      = C_HDR.AMDT_SEQ
        AND SCP_M.SVC_SCP_CD    = C_HDR.SVC_SCP_CD
        AND C_HDR.PROP_NO       = CMDT.PROP_NO
        AND C_HDR.AMDT_SEQ      = CMDT.AMDT_SEQ
        AND C_HDR.SVC_SCP_CD    = CMDT.SVC_SCP_CD
        AND C_HDR.GEN_SPCL_RT_TP_CD = CMDT.GEN_SPCL_RT_TP_CD
        AND C_HDR.CMDT_HDR_SEQ  = CMDT.CMDT_HDR_SEQ
        AND CMDT.PRC_CMDT_TP_CD = 'C' -- Commodity
        AND CMDT.SRC_INFO_CD    <> 'AD'
     UNION ALL
     SELECT DISTINCT DTL.PRC_CMDT_DEF_CD,
			H.SC_NO AS SC_NO,
            '*' AS RFA_NO
       FROM PRI_SP_HDR H
            ,PRI_SP_MN  M
            ,PRI_SP_SCP_MN SCP_M
            ,PRI_SP_SCP_RT_CMDT_HDR C_HDR
            ,PRI_SP_SCP_RT_CMDT  CMDT
            ,PRI_SP_SCP_GRP_CMDT GRP
            ,PRI_SP_SCP_GRP_CMDT_DTL DTL
      WHERE H.SC_NO             =  @[sc_no]
        AND H.PROP_NO           = M.PROP_NO
#if (${app_dt} != '')  
        AND M.EFF_DT            <=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
        AND M.EXP_DT            >=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
#end
        AND M.PROP_STS_CD       = 'F'
        AND M.PROP_NO           = SCP_M.PROP_NO
        AND M.AMDT_SEQ          = SCP_M.AMDT_SEQ
--        AND SCP_M.SVC_SCP_CD    = 'TPE'
        AND SCP_M.PROP_NO       = C_HDR.PROP_NO
        AND SCP_M.AMDT_SEQ      = C_HDR.AMDT_SEQ
        AND SCP_M.SVC_SCP_CD    = C_HDR.SVC_SCP_CD
        AND C_HDR.PROP_NO       = CMDT.PROP_NO
        AND C_HDR.AMDT_SEQ      = CMDT.AMDT_SEQ
        AND C_HDR.SVC_SCP_CD    = CMDT.SVC_SCP_CD
        AND C_HDR.GEN_SPCL_RT_TP_CD = CMDT.GEN_SPCL_RT_TP_CD
        AND C_HDR.CMDT_HDR_SEQ  = CMDT.CMDT_HDR_SEQ
        AND CMDT.PRC_CMDT_TP_CD = 'G' -- Group Commodity
        AND CMDT.SRC_INFO_CD    <> 'AD'
        AND C_HDR.PROP_NO       = GRP.PROP_NO
        AND C_HDR.AMDT_SEQ      = GRP.AMDT_SEQ
        AND C_HDR.SVC_SCP_CD    = GRP.SVC_SCP_CD
        AND CMDT.PRC_CMDT_DEF_CD  = GRP.PRC_GRP_CMDT_CD
        AND GRP.PROP_NO         = DTL.PROP_NO
        AND GRP.AMDT_SEQ        = DTL.AMDT_SEQ
        AND GRP.SVC_SCP_CD      = DTL.SVC_SCP_CD
        AND GRP.GRP_CMDT_SEQ    = DTL.GRP_CMDT_SEQ) B
WHERE   A.CMDT_CD = B.PRC_CMDT_DEF_CD
/*CUSTOMER Name*/
#if (${cust_lgl_eng_nm} != '') 
  AND UPPER(CMDT_NM) LIKE '%'||UPPER(@[cust_lgl_eng_nm])||'%'
#end
UNION ALL 
SELECT A.CMDT_CD, 
       A.CMDT_NM,
	   B.SC_NO,
	   B.RFA_NO
FROM MDM_COMMODITY A, 
    (SELECT DISTINCT CMDT.PRC_CMDT_DEF_CD,
			'*' AS SC_NO,
            H.RFA_NO AS RFA_NO
--   H.RFA_NO
--  ,M.PROP_NO
--  ,M.AMDT_SEQ
--  ,SCP_M.SVC_SCP_CD
----  ,C_HDR.GEN_SPCL_RT_TP_CD
--  ,CMDT.CMDT_HDR_SEQ
--  ,CMDT.CMDT_SEQ
--  ,CMDT.PRC_CMDT_TP_CD
--  ,CMDT.PRC_CMDT_DEF_CD
       FROM PRI_RP_HDR H
            ,PRI_RP_MN  M
            ,PRI_RP_SCP_MN SCP_M
            ,PRI_RP_SCP_RT_CMDT_HDR C_HDR
            ,PRI_RP_SCP_RT_CMDT  CMDT
-- where   H.RFA_NO     = 'AAR7A100153'
      WHERE H.RFA_NO            = @[rfa_no]
        AND H.PROP_NO           = M.PROP_NO
#if (${app_dt} != '')  
        AND M.EFF_DT            <=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
        AND M.EXP_DT            >=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
#end
        AND M.PROP_STS_CD       = 'A'
        AND M.PROP_NO           = SCP_M.PROP_NO
        AND M.AMDT_SEQ          = SCP_M.AMDT_SEQ
--and     SCP_M.SVC_SCP_CD  = 'TPE'
        AND SCP_M.PROP_NO       = C_HDR.PROP_NO
        AND SCP_M.AMDT_SEQ      = C_HDR.AMDT_SEQ
        AND SCP_M.SVC_SCP_CD    = C_HDR.SVC_SCP_CD
        AND C_HDR.PROP_NO       = CMDT.PROP_NO
        AND C_HDR.AMDT_SEQ      = CMDT.AMDT_SEQ
        AND C_HDR.SVC_SCP_CD    = CMDT.SVC_SCP_CD
        AND C_HDR.CMDT_HDR_SEQ  = CMDT.CMDT_HDR_SEQ
        AND CMDT.PRC_CMDT_TP_CD = 'C' 
        AND CMDT.SRC_INFO_CD    <> 'AD'
     UNION ALL
     SELECT DISTINCT DTL.PRC_CMDT_DEF_CD,
			'*' AS SC_NO,
            H.RFA_NO AS RFA_NO
       FROM PRI_RP_HDR H
            ,PRI_RP_MN  M
            ,PRI_RP_SCP_MN SCP_M
            ,PRI_RP_SCP_RT_CMDT_HDR C_HDR
            ,PRI_RP_SCP_RT_CMDT  CMDT
            ,PRI_RP_SCP_GRP_CMDT GRP
            ,PRI_RP_SCP_GRP_CMDT_DTL DTL
      WHERE H.RFA_NO            = @[rfa_no]
        AND H.PROP_NO           = M.PROP_NO
#if (${app_dt} != '')  
        AND M.EFF_DT            <=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
        AND M.EXP_DT            >=  TO_DATE(@[app_dt], 'YYYY-MM-DD')
#end
        AND M.PROP_STS_CD       = 'A'
        AND M.PROP_NO           = SCP_M.PROP_NO
        AND M.AMDT_SEQ          = SCP_M.AMDT_SEQ
--        AND SCP_M.SVC_SCP_CD    = 'TPE'
        AND SCP_M.PROP_NO       = C_HDR.PROP_NO
        AND SCP_M.AMDT_SEQ      = C_HDR.AMDT_SEQ
        AND SCP_M.SVC_SCP_CD    = C_HDR.SVC_SCP_CD
        AND C_HDR.PROP_NO       = CMDT.PROP_NO
        AND C_HDR.AMDT_SEQ      = CMDT.AMDT_SEQ
        AND C_HDR.SVC_SCP_CD    = CMDT.SVC_SCP_CD
--        AND C_HDR.GEN_SPCL_RT_TP_CD = CMDT.GEN_SPCL_RT_TP_CD
        AND C_HDR.CMDT_HDR_SEQ  = CMDT.CMDT_HDR_SEQ
        AND CMDT.PRC_CMDT_TP_CD = 'G' -- Group Commodity
        AND CMDT.SRC_INFO_CD    <> 'AD'
        AND C_HDR.PROP_NO       = GRP.PROP_NO
        AND C_HDR.AMDT_SEQ      = GRP.AMDT_SEQ
        AND C_HDR.SVC_SCP_CD    = GRP.SVC_SCP_CD
        AND CMDT.PRC_CMDT_DEF_CD  = GRP.PRC_GRP_CMDT_CD
        AND GRP.PROP_NO         = DTL.PROP_NO
        AND GRP.AMDT_SEQ        = DTL.AMDT_SEQ
        AND GRP.SVC_SCP_CD      = DTL.SVC_SCP_CD
        AND GRP.GRP_CMDT_SEQ    = DTL.GRP_CMDT_SEQ) B
WHERE A.CMDT_CD = B.PRC_CMDT_DEF_CD
/*CUSTOMER Name*/
#if (${cust_lgl_eng_nm} != '') 
AND UPPER(CMDT_NM) LIKE '%'||UPPER(@[cust_lgl_eng_nm])||'%'
#end
#elseif (${type} == 'A')

SELECT DISTINCT CODE,
                CUST_LGL_ENG_NM,
	   			SC_NO,
	   			RFA_NO
  FROM (
		SELECT  HD.SC_NO AS SC_NO,
                '*' AS RFA_NO,
                AC.CUST_CNT_CD || LPAD(AC.CUST_SEQ, 6, 0) AS CODE     ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')          AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')          AS TO_DT    ,
                AC.CUST_CNT_CD  ,
                AC.CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = AC.CUST_CNT_CD AND A.CUST_SEQ = AC.CUST_SEQ )  CUST_LGL_ENG_NM
                --,RANK() OVER (PARTITION BY SM.SVC_SCP_CD ORDER BY DECODE(BK.SVC_SCP_CD, SM.SVC_SCP_CD, 1, 2)) AS RN
        FROM    PRI_SP_HDR    HD  ,
                PRI_SP_MN     MN  ,
                PRI_SP_SCP_MN SM  ,
                PRI_SP_CTRT_PTY  AC  
        WHERE   1 = 1
        AND     HD.SC_NO        = @[sc_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='F'        /*상수*/ 
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        --AND     SM.SVC_SCP_CD   IN ( BK.SVC_SCP_CD, DECODE(BK.SVC_SCP_CD, 'ACE', 'TPE', 'MXE', 'TPE') )
        AND     AC.PROP_NO      = SM.PROP_NO
        AND     AC.AMDT_SEQ     = SM.AMDT_SEQ
--        AND     AC.SVC_SCP_CD   = SM.SVC_SCP_CD
        AND     AC.PRC_CTRT_PTY_TP_CD = 'C'
        UNION ALL
		SELECT  

             '*' AS SC_NO,
                HD.RFA_NO AS RFA_NO,
				MN.CTRT_CUST_CNT_CD || LPAD(MN.CTRT_CUST_SEQ, 6, 0) AS CODE     ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')            AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')            AS TO_DT    ,
                MN.CTRT_CUST_CNT_CD  ,
                MN.CTRT_CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = MN.CTRT_CUST_CNT_CD AND A.CUST_SEQ = MN.CTRT_CUST_SEQ )  CUST_LGL_ENG_NM
        FROM    PRI_RP_HDR    HD  ,
                PRI_RP_MN     MN  ,
                PRI_RP_SCP_MN SM   
        WHERE   1 = 1
        AND     HD.RFA_NO        = @[rfa_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='A'        /*상수*/
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        )
/*CUSTOMER Name*/
WHERE  1=1
#if (${cust_lgl_eng_nm} != '') 
AND UPPER(CMDT_NM) LIKE '%'||UPPER(@[cust_lgl_eng_nm])||'%'
#end
#end			]]></sql>
			<params>
				<param name="sc_no" type="12" value="AEN337337" out="N"/>
				<param name="app_dt" type="12" value="" out="N"/>
				<param name="cust_lgl_eng_nm" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
