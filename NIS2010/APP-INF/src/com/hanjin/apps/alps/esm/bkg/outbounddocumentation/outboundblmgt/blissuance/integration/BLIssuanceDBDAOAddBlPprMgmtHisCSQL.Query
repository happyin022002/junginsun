<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOAddBlPprMgmtHisCSQL">
			<desc><![CDATA[Web OBL수정이력을 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CUST_BL_PPR_MGMT_HIS 
(
   DTRB_YR
  ,RHQ_CD
  ,OFC_CD
  ,CUST_CNT_CD
  ,CUST_SEQ
  ,HIS_SEQ
  ,PRE_CTNT
  ,CRNT_CTNT
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
) 
VALUES 
(
  @[dtrb_yr]
 ,@[rhq_cd]
 ,@[ofc_cd]
 ,@[cust_cnt_cd]
 ,@[cust_seq]
,(SELECT NVL(MAX(HIS_SEQ)+1, 1) AS HIS_SEQ
    FROM BKG_CUST_BL_PPR_MGMT_HIS
   WHERE DTRB_YR = @[dtrb_yr]
     AND RHQ_CD= @[rhq_cd]
     AND OFC_CD= @[ofc_cd]
     AND CUST_CNT_CD= @[cust_cnt_cd]
     AND CUST_SEQ= @[cust_seq]
     )
 ,@[pre_ctnt]
 ,@[crnt_ctnt]
 ,@[usr_id]
 ,sysdate
 ,@[usr_id]
 ,sysdate
)			]]></sql>
			<params>
				<param name="dtrb_yr" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="pre_ctnt" type="12" value="" out="N"/>
				<param name="crnt_ctnt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
