<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriUsdRoutCsAmdtCalculateVODSQL">
			<desc><![CDATA[Calculate logic.]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_TRI_RT_USD_ROUT_CS A
 WHERE TRI_PROP_NO = @[tri_prop_no]
   AND AMDT_SEQ = (SELECT DISTINCT AMDT_SEQ 
					 FROM PRI_TRI_RT B
                    WHERE B.TRI_PROP_NO = @[tri_prop_no]
                      AND A.TRI_PROP_NO = B.TRI_PROP_NO
                      AND A.AMDT_SEQ = B.AMDT_SEQ
                      AND B.PROP_STS_CD IN ( 'I', 'R')
                   )			]]></sql>
			<params>
				<param name="tri_prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
