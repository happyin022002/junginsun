<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOsearchHldNtcObStaffSetupInfoRSQL">
			<desc><![CDATA[(HldNtcObStaffSetupInfoVO 생성)]]></desc>
			<sql><![CDATA[
-- HldNtcObStaffSetupInfoVO
SELECT DISTINCT
	   SND_USR_ID
      ,CUSER.USR_EML
      ,CUSER.USR_NM
FROM   BKG_CSTMS_ADV_BL      ABL
      ,BKG_CSTMS_ADV_EDI_BL_RSPN B_R
      ,BKG_CSTMS_ADV_SND_LOG SLOG
      ,COM_USER              CUSER
WHERE ABL.CNT_CD             = @[cnt_cd]
  AND ABL.BL_NO              = @[bl_no] -- param

  AND ABL.CNT_CD             = B_R.CNT_CD
  AND ABL.BL_NO              = B_R.BL_NO
  AND SLOG.CRR_BAT_NO        = B_R.CRR_BAT_NO

  AND SLOG.VSL_CD(+)         = ABL.VSL_CD
  AND SLOG.SKD_VOY_NO(+)     = ABL.SKD_VOY_NO
  AND SLOG.SKD_DIR_CD(+)     = ABL.SKD_DIR_CD
  AND SLOG.POL_CD(+)         = ABL.CSTMS_POL_CD
  AND SLOG.POD_CD(+)         = ABL.CSTMS_POD_CD
  AND SLOG.CNT_CD(+)         = @[cnt_cd]
  AND SLOG.IO_BND_CD(+)      ='I'
  AND SLOG.TRSM_MSG_TP_ID(+) = @[msg_tp_id]
  AND CUSER.USR_ID(+)        = SLOG.SND_USR_ID			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="msg_tp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
