<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchUsWestRSQL">
			<desc><![CDATA[EBookingReceiptDBDAOsearchUsWestRSQL

추가 지역 : Canada(CAVAN, CAPRR), 미동안(USBOS, USNYC, USORF, USILM, USCHS, USSAV, USMIA, USHOU) 추가 

]]></desc>
			<sql><![CDATA[
SELECT 
  CASE WHEN  RGN_CD IN ('UPS','UPN') AND CALL_PORT_FLG= 'Y' THEN 'Y'
       WHEN LOC_CD IN ('CAVAN', 'CAPRR','USBOS', 'USNYC', 'USORF', 'USILM', 'USCHS', 'USSAV', 'USMIA', 'USHOU') THEN 'Y'
       ELSE 'N'
  END US_WEST_FLAG   
FROM MDM_LOCATION 
WHERE LOC_CD =@[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
