<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMPBGuidelineDBDAOPriOrgCmpbGlineRoutPntVODSQL">
			<desc><![CDATA[base 별 org 별 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_CMPB_GLINE_ROUT_PNT
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	CRE_OFC_CD = @[cre_ofc_cd]
AND	GLINE_SEQ = @[gline_seq]
AND	PRS_CUST_TP_CD = @[prs_cust_tp_cd]
AND	BSE_SEQ = @[bse_seq]
AND	ORG_DEST_TP_CD = 'O'			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="prs_cust_tp_cd" type="12" value="" out="N"/>
				<param name="bse_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
