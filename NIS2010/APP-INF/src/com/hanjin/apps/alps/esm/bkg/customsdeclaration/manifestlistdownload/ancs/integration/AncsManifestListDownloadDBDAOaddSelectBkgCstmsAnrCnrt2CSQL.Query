<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsManifestListDownloadDBDAOaddSelectBkgCstmsAnrCnrt2CSQL">
			<desc><![CDATA[i]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ANR_CNTR (
VSL_CD, SKD_VOY_NO, SKD_DIR_CD, BKG_NO, CNTR_NO,
CNTR_TPSZ_CD, ISO_CNTR_TPSZ_CD, PCK_QTY, PCK_TP_CD, CNTR_WGT, WGT_UT_CD,
ORG_RCV_TERM_CD, DEST_DE_TERM_CD, DECL_FLG,
CRE_USR_ID, CRE_OFC_CD, CRE_DT, UPD_USR_ID, UPD_OFC_CD, UPD_DT)
SELECT B.VSL_CD, B.SKD_VOY_NO, B.SKD_DIR_CD, C.BKG_NO, C.CNTR_NO,
C.CNTR_TPSZ_CD, D.CNTR_TPSZ_ISO_CD, NVL(C.PCK_QTY,0), C.PCK_TP_CD, NVL(C.CNTR_WGT,0), C.WGT_UT_CD,
DECODE(C.RCV_TERM_CD, 'S', 'LCL', 'FCL'), DECODE(C.DE_TERM_CD, 'S', 'LCL', 'FCL'), 'Y',
@[cre_usr_id], @[cre_usr_cd], SYSDATE, @[upd_usr_id], @[upd_ofc_cd], SYSDATE
FROM BKG_CSTMS_ANR_BL B, BKG_CONTAINER C, MDM_CNTR_TP_SZ D
WHERE 1=1
AND B.BKG_NO = @[bkg_no]
AND C.BKG_NO = B.BKG_NO
AND D.CNTR_TPSZ_CD(+) = C.CNTR_TPSZ_CD
AND NOT EXISTS (SELECT '*' FROM BKG_CSTMS_ANR_CNTR E 
                WHERE 1=1
				AND E.BKG_NO = @[bkg_no]
                AND E.CNTR_NO = C.CNTR_NO)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
