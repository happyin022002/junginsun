<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RASCommonDBDAOSearchCodRequestResonCodeListRSQL">
			<desc><![CDATA[COD Request Resaon Code 조회]]></desc>
			<sql><![CDATA[
SELECT   INTG_CD_VAL_CTNT CD
,        INTG_CD_VAL_DESC NM
FROM     COM_INTG_CD_DTL
WHERE    INTG_CD_ID = 'CD02153'
AND      (APLY_ST_DT <= TO_CHAR(SYSDATE, 'YYYYMMDD') AND APLY_END_DT >= TO_CHAR(SYSDATE, 'YYYYMMDD'))
ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
