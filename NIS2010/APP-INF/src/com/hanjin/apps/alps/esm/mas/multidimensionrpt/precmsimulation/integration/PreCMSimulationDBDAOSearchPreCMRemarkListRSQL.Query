                           AND D1.SUB_TRD_CD   = D3.SUB_TRD_CD
                           AND D1.STND_COST_CD = D2.STND_COST_CD
                       )
                 GROUP BY NOD_CD, COST_ACT_GRP_SEQ, STND_COST_CD, STND_COST_NM, SGRP_COST_CD_DESC
