                                                                       DECODE(SUBSTR(A2.CNTR_TPSZ_CD,-1),'2',A2.CNTR_QTY, '3',A2.CNTR_QTY , A2.CNTR_QTY * 2) AS CNTR_TEU
                                                                  FROM MAS_COM_PARA A1,
