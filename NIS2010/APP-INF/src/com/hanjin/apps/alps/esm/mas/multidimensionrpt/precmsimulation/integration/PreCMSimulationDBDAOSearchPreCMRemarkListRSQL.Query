<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PreCMSimulationDBDAOSearchPreCMRemarkListRSQL">
			<desc><![CDATA[   MAS_MON_MISC_REV_PRE_TEU 테이블의 데이터 조회
2010.12.21 이윤정 [CHM-201007742-01] 미주 철도 냉동화물비용 반영요청
2011.01.11 이윤정 [CHM-201108216-01] NIBC, NOBC 요율 반영 로직 수정.TMFDFL 계정에 대해서 TMFDMT으로 변환하여 보여주는 부분을 제거.
2011.12.21 최윤성 [CHM-201115260-01] [MAS] Pre CM/OP Simulation화면 U.I 변경요청 - LOC, NOD Chekc 로직 추가
2011.12.26 최윤성 [CSR전환중] [MAS] Pre CM/OP Simulation화면 U.I변경건 Inquiry by BKG / Product Catalog Inquiry 동일 적용요청 - Full Transport Expense 조건 추가
2012.02.06 이석준 [CHM-201215969-01] CM2 적용
2012.07.30 최윤성 [CHM-201219111-01] AWK 에 대한 Remark 처리 조건 추가
2014.12.30 김시몬 op 추가, ORDER BY 순서 변경
2015.01.08 김시몬 OP(Lane) 추가
2015.01.20 김시몬 OP(분기로 조회하도록 변경)
2015.02.13 김시몬 OP 제어 OFC 추가
2015.03.26 컬럼 속성명 변경으로 수정()]]></desc>
			<sql><![CDATA[
SELECT	NOD_CD
		,GRP
		,STND_COST_CD
		,DECODE(STND_COST_CD,'51701011','CM2 COST',STND_COST_NM) STND_COST_NM
		,MAS_COST_SRC_CD
		,DECODE(STND_COST_CD,'51701011','CM2 COST('||MAS_COST_SRC_NM||')',MAS_COST_SRC_NM) MAS_COST_SRC_NM
		,AMT
		,CTRT_RTN_FLG
		,COST_CALC_RMK
		,AVG_LVL_CHK
		,VNDR 
FROM 
	(	SELECT	'DEM/DET' NOD_CD
				,' '|| 2 COST_ACT_GRP_SEQ 
				,'' GRP
				,'' STND_COST_CD
				,'' STND_COST_NM
				,'' MAS_COST_SRC_CD
				,'' MAS_COST_SRC_NM
				,ESTM_USD_TTL_AMT AMT
				,DECODE(CTRT_RTN_FLG, 'Y', 'Contract', 'Average') CTRT_RTN_FLG
				,REPLACE(COST_CALC_RMK, '>', '') COST_CALC_RMK
				,'00' ACCT_DP_SEQ
				,'N' AVG_LVL_CHK
				,'' VNDR
                ,'Y' AS OP_CHK
		FROM	MAS_COM_COST_PARA
		WHERE	PCTL_NO = @[f_pctl_no]
		AND		STND_COST_CD = '43201011'
		
		UNION ALL
		
		SELECT	'Misc Rev' NOD_CD
				,' '|| 4 COST_ACT_GRP_SEQ
				,'' GRP
				,'' STND_COST_CD
				,'' STND_COST_NM
				,'' MAS_COST_SRC_CD
				,'' MAS_COST_SRC_NM
				,DECODE(IS_USE_TRD_UC, 'Y', TRD_UC_AMT * CNTR_TEU, RLANE_UC_AMT * CNTR_TEU) AMT
				,'Average' CTRT_RTN_FLG
				,CASE	WHEN IS_USE_TRD_UC = 'Y'
						THEN 'MRI Trade ' || '('||TRD_CD||', '||DIR_CD||')' || ROUND(TRD_UC_AMT, 2) || '*' || CNTR_TEU || '(TEU)'
						ELSE 'MRI Lane ' || '('||RLANE_CD||', '||DIR_CD||')' || ROUND(RLANE_UC_AMT, 2) || '*' || CNTR_TEU || '(TEU)'
						END COST_CALC_RMK
				,'00' ACCT_DP_SEQ
				,'N' AVG_LVL_CHK
				, VNDR
				, 'Y' AS OP_CHK
		FROM 
			(	SELECT	A2.PCTL_NO
						,A2.COST_ACT_GRP_SEQ
						,A2.CNTR_TPSZ_CD
						,A2.CNTR_QTY
						,DECODE(SUBSTR(A2.CNTR_TPSZ_CD, -1), '2', A2.CNTR_QTY, '3', A2.CNTR_QTY, A2.CNTR_QTY * 2) CNTR_TEU
						,A3.REV_YRMON
						,DECODE(NVL(A1.COST_ROUT_NO, 1), 1, A1.N1ST_TRD_CD, 2, A1.N2ND_TRD_CD, 3, A1.N3RD_TRD_CD, A1.N4TH_TRD_CD) TRD_CD --A3.TRD_CD
						,DECODE(NVL(A1.COST_ROUT_NO, 1), 1, A1.N1ST_RLANE_CD, 2, A1.N2ND_RLANE_CD, 3, A1.N3RD_RLANE_CD, A1.N4TH_RLANE_CD) RLANE_CD --A3.RLANE_CD
						,SUBSTR(DECODE(NVL(COST_ROUT_NO, 1), 1, A1.N1ST_FINC_VVD_CD, 2, A1.N2ND_FINC_VVD_CD, 3, A1.N3RD_FINC_VVD_CD, A1.N4TH_FINC_VVD_CD), -1) DIR_CD--A3.DIR_CD
						,NVL(A3.TRD_UC_AMT, 0) TRD_UC_AMT
						,NVL(A3.RLANE_UC_AMT, 0) RLANE_UC_AMT
						,A3.IS_USE_TRD_UC
						,'' VNDR
				FROM	MAS_COM_PARA A1
						,MAS_COM_COST_PARA A2
					,(	SELECT	REV_YRMON
								,TRD_CD
								,RLANE_CD
								,DIR_CD
								,MAX(TRD_UC_AMT) TRD_UC_AMT
								,MAX(RLANE_UC_AMT) RLANE_UC_AMT
								,MAX(IS_USE_TRD_UC) IS_USE_TRD_UC
						FROM 
							(	SELECT	REV_YRMON
										,TRD_CD
										,DIR_CD
										,DECODE(RLANE_CD, 'XXXXX', NULL, RLANE_CD) RLANE_CD
										,DECODE(RLANE_CD, 'XXXXX', ETC_UT_REV_AMT, 0) TRD_UC_AMT
										,DECODE(RLANE_CD, 'XXXXX', 0, ETC_UT_REV_AMT) RLANE_UC_AMT
										,CASE -- 단가가 100 이상이고 물량이 100 이하이면 TRD 단가 사용
												WHEN ETC_UT_REV_AMT > 100 AND TRD_TTL_QTY < 100
												THEN 'Y'
												ELSE 'N'
												END AS IS_USE_TRD_UC
								FROM	MAS_MON_MISC_REV_PRE_TEU
								WHERE	REV_YRMON = MAS_BZC_COST_YRMON_FNC('')
							)
						GROUP	BY REV_YRMON, TRD_CD, RLANE_CD, DIR_CD
					) A3
				WHERE	A1.PCTL_NO = A2.PCTL_NO
				AND		A2.PCTL_NO = @[f_pctl_no]
				AND		a2.MAS_COST_SRC_CD = '43201011'   --DEM/DET계정과 동일한 설정으로
				AND		A3.REV_YRMON(+) = MAS_BZC_COST_YRMON_FNC('')
				AND		A3.TRD_CD(+) = DECODE(NVL(A1.COST_ROUT_NO, 1), 1, A1.N1ST_TRD_CD, 2, A1.N2ND_TRD_CD, 3, A1.N3RD_TRD_CD, A1.N4TH_TRD_CD )
				AND		A3.RLANE_CD(+) = DECODE(NVL(A1.COST_ROUT_NO, 1)
													,1, A1.N1ST_RLANE_CD
													,2, A1.N2ND_RLANE_CD
													,3, A1.N3RD_RLANE_CD
													,A1.N4TH_RLANE_CD
													)
				AND		A3.DIR_CD(+) = SUBSTR(DECODE(NVL(COST_ROUT_NO, 1)
													,1, A1.N1ST_FINC_VVD_CD
													,2, A1.N2ND_FINC_VVD_CD
													,3, A1.N3RD_FINC_VVD_CD
													,A1.N4TH_FINC_VVD_CD ),-1) 
			)
			
		UNION ALL
		
		SELECT	A4.NOD
				,''||A1.COST_ACT_GRP_SEQ
				,MAS_GET_COM_NM_FNC('cost_act_grp_cd', A1.COST_ACT_GRP_CD) GRP
				,A1.STND_COST_CD
				,A2.STND_COST_NM
				,DECODE(A1.MAS_COST_SRC_CD
						,'92404011', A3.SLS_ACT_CD
						,'65000000', A3.SLS_ACT_CD
						,'65901011', A3.SLS_ACT_CD
						,'65901021', A3.SLS_ACT_CD
						,A1.MAS_COST_SRC_CD ) MAS_COST_SRC_CD -- 2011.01.11 이윤정 [CHM-201108216-01] TMFDFL 계정에 대해서 TMFDMT으로 변환하여 보여주는 부분을 제거.
				,DECODE(A1.MAS_COST_SRC_CD
						,'92404011', A3.SLS_ACT_CD_NM
						,'65000000', A3.SLS_ACT_CD_NM
						,'65901011', A3.SLS_ACT_CD_NM
						,'65901021', A3.SLS_ACT_CD_NM
						,'51102000', A2.STND_COST_NM   --MT의 경우 Standard name 으로
						,'51302000', A2.STND_COST_NM   --MT의 경우 Standard name 으로				
						,MAS_GET_COM_NM_FNC('mas_cost_src_cd', A1.MAS_COST_SRC_CD) ) MAS_COST_SRC_NM -- 2011.01.11 이윤정 [CHM-201108216-01] TMFDFL 계정에 대해서 TMFDMT으로 변환하여 보여주는 부분을 제거.
				,DECODE(A1.MAS_COST_SRC_CD
						,'92404011', A3.SVC_TRNS_PRC_AMT
						,'65000000', A3.SVC_TRNS_PRC_AMT
						,'65901011', A3.SVC_TRNS_PRC_AMT
						,'65901021', A3.SVC_TRNS_PRC_AMT
						,DECODE(@[f_cob_profit_vw], 'P', A1.ESTM_USD_TTL_AMT, A1.RESPB_USD_TTL_AMT) ) AMT
				,DECODE(A1.CTRT_RTN_FLG, 'Y', 'Contract', 'Average') CTRT_RTN_FLG
				,CASE	WHEN REGEXP_LIKE(REPLACE(A1.COST_CALC_RMK,'(',''), '^>T[RE]S AVG-LOCSCOTAL') 
						THEN SUBSTR(A1.COST_CALC_RMK, 2, 20) || ' -> ' || ROUND(ESTM_USD_UC_AMT, 2) -- [CHM-201007742-01] 미주 철도 냉동화물비용 반영요청
						WHEN REGEXP_LIKE(A1.COST_CALC_RMK, '^>T[RE]S AVG-')
						THEN SUBSTR(A1.COST_CALC_RMK, 2, 12) || ' -> ' || ROUND(ESTM_USD_UC_AMT, 2)
						WHEN REGEXP_LIKE(A1.COST_CALC_RMK, '^>AWK OOG-Qty')    -- 2012.07.30 최윤성 [CHM-201219111-01] AWK 에 대한 Remark 처리 조건 추가
						THEN SUBSTR(A1.COST_CALC_RMK, 2, INSTR(A1.COST_CALC_RMK, '>T') + 10 ) || ' -> ' || ROUND(ESTM_USD_UC_AMT, 2)						
						ELSE DECODE(A1.MAS_COST_SRC_CD
									,'51601011', SUBSTR(A1.COST_CALC_RMK, 2)   -- Volume Discount
									,'92404011', A3.COST_CALC_RMK   --ABC/STP
									,'65000000', A3.COST_CALC_RMK   --ABC/STP
									,'65901011', A3.COST_CALC_RMK   --ABC/STP
									,'65901021', A3.COST_CALC_RMK   --ABC/STP
									,'92202011', REPLACE(REPLACE(A1.COST_CALC_RMK, ', RepoPOR', ', RepoPOR'), ', CNTREQ', ', CNTREQ')   --EMU EQ
									,'92202012', REPLACE(A1.COST_CALC_RMK, ', Direction=', ', Direction=')   --EMU SIM
									,REPLACE(A1.COST_CALC_RMK, '>TP', 'TP') )
						END AS COST_CALC_RMK
				,ACCT_DP_SEQ
				,CASE WHEN REGEXP_LIKE(A1.COST_CALC_RMK, 'AVG-SCC|AVG-ECC|AVG-LCC|AVG-RCC') AND A2.SGRP_COST_CD = 'CVTR'
									THEN 'Y'
									ELSE 'N'
				  END AS AVG_LVL_CHK
				, DECODE(A1.CTRT_RTN_FLG,'Y', VNDR) VNDR
				, 'Y' AS OP_CHK
		FROM	MAS_COM_COST_PARA A1
				,MAS_STND_ACCT_V A2
			,(	SELECT	B1.PCTL_NO
						,DECODE(B1.RA_ACCT_CD
								,'91401011', '92404011'
								,'65901011', DECODE(@[f_cob_profit_vw], 'P', '65000000', '65901011')
								,'65901021', DECODE(@[f_cob_profit_vw], 'P', '65000000', '65901021')
								,B1.RA_ACCT_CD ) MAS_COST_SRC_CD   --STP Income을 STP Cost로
						,B1.COND_OFC_CD
						,B1.SLS_ACT_CD
						,B2.SLS_ACT_DESC SLS_ACT_CD_NM
						,B1.SVC_TRNS_PRC_AMT
						,B1.COST_CALC_RMK
				FROM	MAS_COM_SVC_TRNS_PRC_PARA B1, MAS_OFC_ROUT_MAPG B2
				WHERE	B1.SLS_ACT_CD = B2.SLS_ACT_CD
				AND		B1.OFC_CLSS_CD = B2.OFC_CLSS_CD
				AND		B2.COST_YRMON = MAS_BZC_COST_YRMON_FNC('')
				AND		PCTL_NO = @[f_pctl_no]
			) A3
			,(	SELECT	DISTINCT COST_ACT_GRP_SEQ
						,COST_ACT_GRP_CD
						,N1ST_NOD_CD ORG_NOD_CD
						,COALESCE(N4TH_NOD_CD, N3RD_NOD_CD, N2ND_NOD_CD) DEST_NOD_CD
						,DECODE(N1ST_NOD_CD
								,N2ND_NOD_CD, N1ST_NOD_CD
								, DECODE(N1ST_NOD_CD, NULL, ' ', N1ST_NOD_CD)
								|| DECODE(N2ND_NOD_CD, NULL, ' ', ' -> ' || N2ND_NOD_CD)
								|| DECODE(N3RD_NOD_CD, NULL, ' ', ' -> ' || N3RD_NOD_CD)
								|| DECODE(N4TH_NOD_CD, NULL, ' ', ' -> ' || N4TH_NOD_CD) ) NOD
				FROM	MAS_COM_COST_PARA
				WHERE	PCTL_NO = @[f_pctl_no]
			) A4

              , (SELECT COST_ACT_GRP_SEQ
                      , COST_ACT_GRP_CD
                      , ORG_NOD_CD
                      , DEST_NOD_CD
                      , NOD
                      , WM_CONCAT(DISTINCT  VNDR_SEQ) VNDR
                   FROM ( SELECT DISTINCT COST_ACT_GRP_SEQ
                              , COST_ACT_GRP_CD
                              , N1ST_NOD_CD ORG_NOD_CD
                              , COALESCE(N4TH_NOD_CD, N3RD_NOD_CD, N2ND_NOD_CD) DEST_NOD_CD
                              , DECODE(N1ST_NOD_CD
                                       , N2ND_NOD_CD
                                       , N1ST_NOD_CD
                                       , DECODE(N1ST_NOD_CD, NULL, ' ', N1ST_NOD_CD) 
                                         || DECODE(N2ND_NOD_CD, NULL, ' ', ' -> ' || N2ND_NOD_CD) 
                                         || DECODE(N3RD_NOD_CD, NULL, ' ', ' -> ' || N3RD_NOD_CD) 
                                         || DECODE(N4TH_NOD_CD, NULL, ' ', ' -> ' || N4TH_NOD_CD) ) NOD
                              , DECODE(B.NO,1, N1ST_VNDR_SEQ,2,N2ND_VNDR_SEQ,3,N3RD_VNDR_SEQ,4,N4TH_VNDR_SEQ,5,N5TH_VNDR_SEQ) VNDR_SEQ
                           FROM MAS_COM_COST_PARA A
                              , (SELECT LEVEL NO FROM dual CONNECT BY LEVEL <= 5) B
                          WHERE PCTL_NO = @[f_pctl_no]
                       )
                   GROUP BY COST_ACT_GRP_SEQ
                      , COST_ACT_GRP_CD
                      , ORG_NOD_CD
                      , DEST_NOD_CD
                      , NOD
                ) A5

		WHERE	A1.PCTL_NO = @[f_pctl_no]
		AND		MAS_COST_SRC_PRT_CD IN(DECODE(@[f_cob_profit_lv], 'C', 'CO', 'CO'), DECODE(@[f_cob_profit_vw], 'P', 'PA', 'RA'))   --MAS_COST_SRC_PRT_CD:R,P
		AND		STND_COST_TP_CD IN('C', DECODE(@[f_cob_profit_lv], 'C', 'C', 'M', 'C','O'))   --STND_COST_TP_CD:C,O
		AND		A1.STND_COST_CD = A2.STND_COST_CD
		AND		A2.PA_VW = 'BKG'
#if (${f_cob_profit_lv} != 'M') 
		AND 	A1.STND_COST_CD <> '51701011'
#end
		AND		A1.MAS_COST_SRC_CD = A3.MAS_COST_SRC_CD(+)
		AND		A1.COST_ACT_GRP_SEQ = A4.COST_ACT_GRP_SEQ
		AND     A1.COST_ACT_GRP_SEQ = A5.COST_ACT_GRP_SEQ
		AND	(	A1.ESTM_USD_TTL_AMT <> 0 OR A1.RESPB_USD_TTL_AMT <> 0 OR A3.SVC_TRNS_PRC_AMT <> 0)
		
        UNION ALL
        
        SELECT NOD_CD
               , NOD_CD||COST_ACT_GRP_SEQ AS COST_ACT_GRP_SEQ
               , GRP
               , STND_COST_CD
               , NVL(STND_COST_NM,SGRP_COST_CD_DESC) AS STND_COST_NM
               , ' '           AS SGRP_COST_CD
               , STND_COST_NM AS SGRP_COST_CD_DESC               
               , AMT
               , DECODE(NVL(STND_COST_CD,'X'),'X',' ','Average') AS CTRT_RTN_FLG
               , DECODE(NVL(STND_COST_CD,'X'),'54400000','Slot Price','53101000','Port Tariff/TEU','53102000','Canal Tariff/TEU','X',' ','TTL Cost/TEU')  AS COST_CALC_RMK
               , STND_COST_CD  AS ACCT_DP_SEQ
			   ,'Y'       AS AVG_LVL_CHK
			   , '' AS VNDR
			   , NVL((SELECT 'Y'
                       FROM MAS_OP_EXPT_OFC
                      WHERE OFC_CD = @[f_ofc_cd]
                    ),'N') AS OP_CHK
          FROM (  
                SELECT   NOD_CD
                       , COST_ACT_GRP_SEQ
                       , 'Common'              AS GRP
                       , STND_COST_CD
                       , STND_COST_NM
                       , SGRP_COST_CD_DESC                       
                       , SUM(TEU_UC_AMT * (CNTR_TEU + @[f_void_qty]))  AS AMT
                  FROM (     
                        SELECT   D1.TRD_CD||' / '||D1.RLANE_CD as NOD_CD
                               , D2.ACCT_DP_SEQ        AS COST_ACT_GRP_SEQ
                               , D1.STND_COST_CD       AS STND_COST_CD
                               , D2.STND_COST_NM       AS STND_COST_NM
                               , D2.MGRP_COST_CD_DESC  AS SGRP_COST_CD_DESC  
                               , D1.TEU_UC_AMT 
                               , D3.CNTR_TEU
                          FROM MAS_STND_UT_COST D1,
                               (
                                SELECT A.MGRP_COST_CD,A.MGRP_COST_CD_DESC,
                                       B.SGRP_COST_CD,B.SGRP_COST_CD_DESC,
                                       C.STND_COST_CD,C.STND_COST_NM,C.PERF_VW_CD,
                                       DECODE(A.MGRP_COST_CD,'OV',1001,'VF',1004,'OF',1007,'GE',1009) AS ACCT_DP_SEQ
                                  FROM MAS_MN_GRP_COST  A,
                                       MAS_SUB_GRP_COST B,
                                       MAS_STND_ACCT    C
                                 WHERE A.MGRP_COST_CD = B.MGRP_COST_CD
                                   AND A.MGRP_COST_CD = C.MGRP_COST_CD
                                   AND B.SGRP_COST_CD = C.SGRP_COST_CD
                                   AND A.MGRP_COST_CD IN ('OF','OV','GE','VF') 
                               ) D2,
                               (SELECT SUBSTR(COST_YRQ,1,4) AS COST_YR,
                                       SUBSTR(COST_YRQ,5,2) AS BSE_QTR_CD,
                                       TRD_CD,
                                       SUB_TRD_CD,
                                       RLANE_CD,
                                       DIR_CD,
                                       STND_COST_CD,
                                       CNTR_TEU,
                                       GEN_FLG
                                  FROM (
                                        SELECT COST_YRQ,
                                               TRD_CD,
                                               SUB_TRD_CD,
                                               RLANE_CD,
                                               DIR_CD,
                                               STND_COST_CD,
                                               CNTR_TEU,
                                               GEN_FLG,
                                               R_CNT,
                                               MIN(R_CNT) OVER() AS MIN_R_CNT
                                          FROM (
                                                SELECT MAX(D3.COST_YR||D3.BSE_QTR_CD) AS COST_YRQ,D3.TRD_CD,D3.SUB_TRD_CD,D3.RLANE_CD,D3.DIR_CD,D3.STND_COST_CD, D2.CNTR_TEU,D2.GEN_FLG, 1 AS R_CNT
                                                  FROM MAS_STND_UT_COST D3,
                                                       (
                                                        SELECT DISTINCT TRD_CD,
                                                               RLANE_CD,
                                                               DIR_CD,
                                                               CNTR_TEU,
                                                               DECODE(FLG,COST_ROUT_NO,'Y','N') AS GEN_FLG
                                                          FROM (
                                                                SELECT 1 AS FLG,
                                                                       A1.PCTL_NO,
                                                                       A1.N1ST_TRD_CD    AS TRD_CD,
                                                                       A1.N1ST_RLANE_CD  AS RLANE_CD,
                                                                       SUBSTR(A1.N1ST_FINC_VVD_CD,-1) AS DIR_CD,
                                                                       NVL(A1.COST_ROUT_NO,1) AS COST_ROUT_NO,
                                                                       DECODE(SUBSTR(A2.CNTR_TPSZ_CD,-1),'2',A2.CNTR_QTY, '3',A2.CNTR_QTY , A2.CNTR_QTY * 2) AS CNTR_TEU
                                                                  FROM MAS_COM_PARA A1,
                                                                       MAS_COM_COST_PARA A2
                                                                 WHERE A1.PCTL_NO = A2.PCTL_NO
                                                                   AND A2.PCTL_NO = @[f_pctl_no] --'T1412240554189010006' 
                                                                   AND A2.MAS_COST_SRC_CD = '43201011'
                                                            
                                                                UNION ALL
                                                            
                                                                SELECT 2 AS FLG,
                                                                       A1.PCTL_NO,
                                                                       A1.N2ND_TRD_CD    AS TRD_CD,
                                                                       A1.N2ND_RLANE_CD  AS RLANE_CD,
                                                                       SUBSTR(A1.N2ND_FINC_VVD_CD,-1) AS DIR_CD,
                                                                       NVL(A1.COST_ROUT_NO,1) AS COST_ROUT_NO,
                                                                       DECODE(SUBSTR(A2.CNTR_TPSZ_CD,-1),'2',A2.CNTR_QTY, '3',A2.CNTR_QTY , A2.CNTR_QTY * 2) AS CNTR_TEU
                                                                  FROM MAS_COM_PARA A1,
                                                                       MAS_COM_COST_PARA A2
                                                                 WHERE A1.PCTL_NO = A2.PCTL_NO
