<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOPrsBatchLogUSQL">
			<desc><![CDATA[Approval Office list]]></desc>
			<sql><![CDATA[
UPDATE	PRI_PRS_BAT
   SET	PRS_BAT_ERR_VAL = @[prs_bat_err_val],
   		PRS_BAT_ERR_DESC = @[prs_bat_err_desc],
   		UPD_DT = SYSDATE
 WHERE	PGM_NO = @[pgm_no]
   AND	PARA_INFO_CTNT = @[para_info_ctnt]
   AND	PRS_BAT_SEQ	= ( SELECT MAX( PRS_BAT_SEQ ) FROM PRI_PRS_BAT WHERE  PGM_NO =  @[pgm_no] AND PARA_INFO_CTNT = @[para_info_ctnt] )
    				]]></sql>
			<params>
				<param name="prs_bat_err_val" type="12" value="" out="N"/>
				<param name="prs_bat_err_desc" type="12" value="" out="N"/>
				<param name="pgm_no" type="12" value="" out="N"/>
				<param name="para_info_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
