<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CsScreenDBDAOsearchUsCstmsRstInfoRSQL">
			<desc><![CDATA[searchUsCstmsRstInfo]]></desc>
			<sql><![CDATA[
SELECT  DSPO_CD
     , CSTMS_CLR_CD
     , CNTR_QTY
     , ENTR_TP_NO
     , ENTR_NO
     , TO_CHAR(ARR_DT,'yyyymmdd hh24MI') AS ARR_DT
     , RCV_LOC_CD
     , CSTMS_RMK1 || CSTMS_RMK2 || CSTMS_RMK3 AS CSTMS_RMK1 
     , RM.SCAC_CD AS SCAC_CD
  FROM BKG_BOOKING BKGM
     , BKG_CSTMS_ADV_RSLT CRST
     , BKG_CSTMS_ADV_RCV_LOG RM
 WHERE BKGM.BKG_NO       = @[bkg_no]
   AND CRST.CNT_CD       = @[cnt_cd]
   AND CRST.BL_NO        = BKGM.BL_NO
   AND CRST.CNT_CD       = RM.CNT_CD(+)
   AND RM.IO_BND_CD(+)   = 'I'
   AND CRST.VSL_CD       = RM.VSL_CD(+)
   AND CRST.SKD_VOY_NO   = RM.SKD_VOY_NO(+)
   AND CRST.SKD_DIR_CD   = RM.SKD_DIR_CD(+)
   AND CRST.ARR_DT       = RM.RCV_DT(+)
   AND CRST.CSTMS_BAT_NO = RM.CSTMS_BAT_NO(+)
ORDER BY ARR_DT,CSTMS_SEQ, ENTR_TP_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="KORZ1220008A1" out="N"/>
				<param name="cnt_cd" type="12" value="US" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
