<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOMstRsltAcceptListRSQL">
			<desc><![CDATA[Master RFA에서 accept되지 않은 data 조회]]></desc>
			<sql><![CDATA[
SELECT PROP_NO
FROM   (SELECT 'MN' PROP_NO
        FROM   PRI_RP_AMDT_SMRY
        WHERE  PROP_NO  = @[prop_no]
        AND    AMDT_SEQ = @[amdt_seq]
        AND    ACPT_FLG = 'N'
        AND    AMDT_FLG = 'Y'
        AND    ROWNUM = 1
        UNION ALL
        SELECT 'SCP' PROP_NO
        FROM   PRI_RP_SCP_AMDT_SMRY
        WHERE  PROP_NO  = @[prop_no]
        AND    AMDT_SEQ = @[amdt_seq]
        AND    ACPT_FLG = 'N'
        AND    AMDT_FLG = 'Y'
        AND    ROWNUM = 1
        -- Note Conversion을 별도로 체크
        UNION ALL
        SELECT 'NOTE' PROP_NO
        FROM   PRI_RFA_NOTE_CONV
        WHERE  PROP_NO  = @[prop_no]
        AND    AMDT_SEQ = @[amdt_seq]
        AND    PRC_PROG_STS_CD <> 'A'
        AND    ROWNUM = 1)
WHERE  ROWNUM = 1			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
