<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAORsltTriPropInquiryListVORSQL">
			<desc><![CDATA[Tri Proposal Inquiry List]]></desc>
			<sql><![CDATA[
SELECT A.TRI_PROP_NO
     , A.TRF_PFX_CD
     , A.TRF_NO
     , NVL2(A.TRI_NO, SUBSTR(A.TRI_NO, 1, 6) || '-' || SUBSTR(A.TRI_NO, 7, 4) || '-' || SUBSTR(A.TRI_NO, 11), NULL) AS TRI_NO
     , F.AMDT_SEQ
     , A.CMDT_CD
     , (SELECT CMDT_NM
          FROM MDM_COMMODITY
         WHERE CMDT_CD = A.CMDT_CD
           AND ROWNUM = 1) AS CMDT_NM
     , B.ROUT_PNT_LOC_NM AS ORG_ROUT_PNT_LOC_NM
     , C.ROUT_VIA_PORT_NM AS ORG_ROUT_VIA_PORT_NM
     , D.ROUT_VIA_PORT_NM AS DEST_ROUT_VIA_PORT_NM
     , E.ROUT_PNT_LOC_NM AS DEST_ROUT_PNT_LOC_NM
     , F.RAT_UT_CD
     , F.CURR_CD
     , F.FNL_FRT_RT_AMT
     , F.NOTE_CTNT || CHR(13) NOTE_CTNT
     , TO_CHAR(F.EFF_DT, 'YYYY-MM-DD') AS EFF_DT
     , TO_CHAR(F.EXP_DT, 'YYYY-MM-DD') AS EXP_DT
     , G.TAA_NO
  FROM PRI_TRI_MN A
     , (SELECT TRI_PROP_NO, REPLACE(ROUT_PNT_LOC_NM, '|||', CHR(13)) AS ROUT_PNT_LOC_NM
          FROM (
                SELECT TRI_PROP_NO
                     , SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_CD || NVL2(RCV_DE_TERM_NM, '(' || RCV_DE_TERM_NM || ')', '') ||
                                                      NVL2(PRC_TRSP_MOD_NM, '(' || PRC_TRSP_MOD_NM || ')', '')
                                                     ,'|||'))
                             ,4) AS ROUT_PNT_LOC_NM
                  FROM (SELECT TRI_PROP_NO
                             , ORG_DEST_TP_CD
                             , ROUT_PNT_LOC_CD
                             , RCV_DE_TERM_CD
                             , (SELECT INTG_CD_VAL_DP_DESC
                                  FROM COM_INTG_CD_DTL
                                 WHERE INTG_CD_ID = 'CD02070'
                                   AND INTG_CD_VAL_CTNT = RCV_DE_TERM_CD
                                   AND ROWNUM = 1) AS RCV_DE_TERM_NM
                             , PRC_TRSP_MOD_CD
                             , (SELECT INTG_CD_VAL_DP_DESC
                                  FROM COM_INTG_CD_DTL
                                 WHERE INTG_CD_ID = 'CD01720'
                                   AND INTG_CD_VAL_CTNT = PRC_TRSP_MOD_CD
                                   AND ROWNUM = 1) AS PRC_TRSP_MOD_NM
                             , ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_PNT_LOC_CD) AS RN
                        FROM PRI_TRI_RT_ROUT_PNT
                       WHERE ORG_DEST_TP_CD = 'O')
                 START WITH RN = 1
                CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
                   AND PRIOR RN = RN - 1
                GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD)
       ) B
     , (SELECT TRI_PROP_NO, REPLACE(ROUT_VIA_PORT_NM, '|||', CHR(13)) AS ROUT_VIA_PORT_NM
          FROM (SELECT TRI_PROP_NO, SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_CD, '|||')), 4) AS ROUT_VIA_PORT_NM
		          FROM (SELECT TRI_PROP_NO
		                     , ORG_DEST_TP_CD
		                     , ROUT_VIA_PORT_CD
		                     , ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_VIA_PORT_CD) AS RN
		                  FROM PRI_TRI_RT_ROUT_VIA
		                 WHERE ORG_DEST_TP_CD = 'O')
		         START WITH RN = 1
		        CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
		               AND PRIOR RN = RN - 1
		         GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD)
       ) C
     , (SELECT TRI_PROP_NO, REPLACE(ROUT_VIA_PORT_CD, '|||', CHR(13)) AS ROUT_VIA_PORT_NM
          FROM (SELECT TRI_PROP_NO, SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_CD, '|||')), 4) AS ROUT_VIA_PORT_CD
                  FROM (SELECT TRI_PROP_NO
                             , ORG_DEST_TP_CD
                             , ROUT_VIA_PORT_CD
                             , ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_VIA_PORT_CD) AS RN
                          FROM PRI_TRI_RT_ROUT_VIA
                         WHERE ORG_DEST_TP_CD = 'D')
                 START WITH RN = 1
                CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
                       AND PRIOR RN = RN - 1
                 GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD)
       ) D
     , (SELECT TRI_PROP_NO, REPLACE(ROUT_PNT_LOC_NM, '|||', CHR(13)) AS ROUT_PNT_LOC_NM
          FROM (SELECT TRI_PROP_NO
                     , SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_CD || NVL2(RCV_DE_TERM_NM, '(' || RCV_DE_TERM_NM || ')', '') ||
                                                      NVL2(PRC_TRSP_MOD_NM, '(' || PRC_TRSP_MOD_NM || ')', '')
                                                     ,'|||'))
                             ,4) AS ROUT_PNT_LOC_NM
                  FROM (SELECT TRI_PROP_NO
                             , ORG_DEST_TP_CD
                             , ROUT_PNT_LOC_CD
                             , RCV_DE_TERM_CD
                             , (SELECT INTG_CD_VAL_DP_DESC
                                  FROM COM_INTG_CD_DTL
                                 WHERE INTG_CD_ID = 'CD02071'
                                   AND INTG_CD_VAL_CTNT = RCV_DE_TERM_CD
                                   AND ROWNUM = 1) AS RCV_DE_TERM_NM
                             , PRC_TRSP_MOD_CD
                             , (SELECT INTG_CD_VAL_DP_DESC
                                  FROM COM_INTG_CD_DTL
                                 WHERE INTG_CD_ID = 'CD01720'
                                   AND INTG_CD_VAL_CTNT = PRC_TRSP_MOD_CD
                                   AND ROWNUM = 1) AS PRC_TRSP_MOD_NM
                             , ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO, ORG_DEST_TP_CD ORDER BY TRI_PROP_NO, ORG_DEST_TP_CD, ROUT_PNT_LOC_CD) AS RN
                          FROM PRI_TRI_RT_ROUT_PNT
                         WHERE ORG_DEST_TP_CD = 'D')
                 START WITH RN = 1
                CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
                       AND PRIOR RN = RN - 1
                 GROUP BY TRI_PROP_NO, ORG_DEST_TP_CD)
       ) E
     , (SELECT T.TRI_PROP_NO
             , T.AMDT_SEQ
             , T.EFF_DT
             , T.EXP_DT
             , T.PUB_DT
             , T.TRI_RQST_OFC_CD
             , T.TRI_RQST_USR_ID
             , T.TRI_APRO_OFC_CD
             , T.TRI_APRO_USR_ID
             , T.PROP_STS_CD
             , T.RAT_UT_CD
             , T.PRC_CGO_TP_CD
             , T.CURR_CD
             , T.PROP_FRT_RT_AMT
             , T.COFFR_FRT_RT_AMT
             , T.FNL_FRT_RT_AMT
             , T.NOTE_CTNT
             , T.NOTE_CONV_MAPG_ID
             , T.GRI_APPL_TP_CD
             , T.GRI_APPL_AMT
          FROM PRI_TRI_RT T, (SELECT TRI_PROP_NO, AMDT_SEQ FROM PRI_TRI_RT GROUP BY TRI_PROP_NO, AMDT_SEQ) S
         WHERE T.TRI_PROP_NO = S.TRI_PROP_NO
           AND T.AMDT_SEQ = S.AMDT_SEQ
           AND T.PROP_STS_CD = 'F') F
     , (SELECT TRI_PROP_NO, REPLACE(TAA_NO, '|||', ' '||CHR(13)) || CHR(13) AS TAA_NO
         FROM (SELECT TRI_PROP_NO
                    , SUBSTR(MAX(SYS_CONNECT_BY_PATH(TAA_NO, '|||')), 4) AS TAA_NO
                 FROM (SELECT D.TRI_PROP_NO
                            , A.TAA_NO
                            , ROW_NUMBER() OVER(PARTITION BY TRI_PROP_NO ORDER BY TRI_PROP_NO, TAA_NO) AS RN
                         FROM PRI_TAA_HDR A
                            , PRI_TAA_MN B
                            , (SELECT TAA_PROP_NO
                                    , MAX(AMDT_SEQ) AS AMDT_SEQ
                                 FROM PRI_TAA_MN
                                WHERE CFM_FLG = 'Y'
                               GROUP BY TAA_PROP_NO) C
                            , PRI_TAA_TRI_LIST D
                        WHERE (@[srch_taa_no] IS NULL OR A.TAA_NO = @[srch_taa_no])
                          AND A.TAA_PROP_NO = B.TAA_PROP_NO
                          AND B.TAA_PROP_NO = C.TAA_PROP_NO
                          AND B.AMDT_SEQ = C.AMDT_SEQ
                          AND B.TAA_PROP_NO = D.TAA_PROP_NO
                          AND B.AMDT_SEQ = D.AMDT_SEQ
						  AND exists (select 'f' from PRI_TRI_MN G where G.TRF_PFX_CD = @[srch_trf_pfx_cd] AND G.TRF_NO = @[srch_trf_no] and D.TRI_PROP_NO = G.TRI_PROP_NO)
                       ORDER BY TRI_PROP_NO, A.TAA_NO) 
                START WITH RN = 1
               CONNECT BY PRIOR TRI_PROP_NO = TRI_PROP_NO
                  AND PRIOR RN = RN - 1
               GROUP BY TRI_PROP_NO)
       ) G 
 WHERE A.TRI_PROP_NO = B.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = C.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = D.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = E.TRI_PROP_NO(+)
   AND A.TRI_PROP_NO = F.TRI_PROP_NO
#if (${srch_taa_no} == '') 
   AND A.TRI_PROP_NO = G.TRI_PROP_NO(+)
#else
   AND A.TRI_PROP_NO = G.TRI_PROP_NO
#end
   AND A.TRF_PFX_CD = @[srch_trf_pfx_cd]
   AND A.TRF_NO = @[srch_trf_no]
   AND A.TRI_NO IS NOT NULL
   AND (@[srch_cmdt_cd] IS NULL OR A.CMDT_CD = @[srch_cmdt_cd])
   AND (@[srch_org_rout_pnt_loc_nm] IS NULL OR B.ROUT_PNT_LOC_NM LIKE '%' || @[srch_org_rout_pnt_loc_nm] || '%')
   AND (@[srch_org_rout_via_port_nm] IS NULL OR C.ROUT_VIA_PORT_NM LIKE '%' || @[srch_org_rout_via_port_nm] || '%')
   AND (@[srch_dest_rout_via_port_nm] IS NULL OR D.ROUT_VIA_PORT_NM LIKE '%' || @[srch_dest_rout_via_port_nm] || '%')
   AND (@[srch_dest_rout_pnt_loc_nm] IS NULL OR E.ROUT_PNT_LOC_NM LIKE '%' || @[srch_dest_rout_pnt_loc_nm] || '%')
   AND (@[srch_tri_no] IS NULL OR A.TRI_NO = REPLACE(@[srch_tri_no], '-'))
   AND (@[srch_rat_ut_cd] IS NULL OR F.RAT_UT_CD = @[srch_rat_ut_cd])
   AND REPLACE(@[srch_acs_dt],'-','') BETWEEN TO_CHAR(F.EFF_DT, 'YYYYMMDD') AND TO_CHAR(F.EXP_DT, 'YYYYMMDD')
 ORDER BY A.TRI_NO, A.CMDT_CD			]]></sql>
			<params>
				<param name="srch_taa_no" type="12" value="" out="N"/>
				<param name="srch_trf_pfx_cd" type="12" value="" out="N"/>
				<param name="srch_trf_no" type="12" value="" out="N"/>
				<param name="srch_cmdt_cd" type="12" value="" out="N"/>
				<param name="srch_org_rout_pnt_loc_nm" type="12" value="" out="N"/>
				<param name="srch_org_rout_via_port_nm" type="12" value="" out="N"/>
				<param name="srch_dest_rout_via_port_nm" type="12" value="" out="N"/>
				<param name="srch_dest_rout_pnt_loc_nm" type="12" value="" out="N"/>
				<param name="srch_tri_no" type="12" value="" out="N"/>
				<param name="srch_rat_ut_cd" type="12" value="" out="N"/>
				<param name="srch_acs_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
