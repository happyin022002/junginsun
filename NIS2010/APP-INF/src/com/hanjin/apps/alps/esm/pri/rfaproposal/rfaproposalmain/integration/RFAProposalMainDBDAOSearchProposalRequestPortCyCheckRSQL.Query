<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOSearchProposalRequestPortCyCheckRSQL">
			<desc><![CDATA[2013.07.08 전윤주 [CHM-201324601] RFA Request 시 Port 운임에 속한 Route (Origin, Dest) 를 체크하여 call_port_flag가 'N' 인 경우 validation 처리 
                                                     Port CY 운임에 Port 가 아닌 Route가 존재하는지 체크 - MDM 에 Call_port_flg 컬럼을 확인한다.
2013.09.13 전윤주 [CHM-201326757] Group Location이 Both로 등록된 경우도 체크하도록 수정
2013.09.16 전윤주 [CHM-201326776] RFA Request 체크 시 예외 location 테이블에 IHC Only flag도 체크하여 에러로 리턴해주는 로직 추가]]></desc>
			<sql><![CDATA[
SELECT A1.SVC_SCP_CD ||'-'|| 
     'Bullet No.'||A2.BLET_DP_SEQ || '-'||
      DECODE(A1.ORG_DEST_TP_CD, 'O', 'Origin', 'Dest.')|| '-' ||
      A1.ROUT_PNT_LOC_DEF_CD AS ETC1
FROM PRI_RP_SCP_RT_ROUT_PNT A1
    ,PRI_RP_SCP_RT_CMDT_HDR A2
WHERE A1.PROP_NO = @[prop_no]
AND A1.AMDT_SEQ = @[amdt_seq]
AND A1.SVC_SCP_CD NOT IN ('TPE','TPW','ACE','ACW','TAE','TAW','ASE','ASW','MMW','MME','SAN','SAS','CAN','CAS','CSE','CME','MWS','MWN','CCS','CCN','CLN','CLS') -- PORT CY, IHC RATE 구분하지 않는 예외 SCOPE
AND A1.ROUT_PNT_LOC_DEF_CD IN (
                               SELECT  GC.PRC_GRP_LOC_CD
                                 FROM  PRI_RP_SCP_GRP_LOC GC  
                                      ,PRI_RP_SCP_GRP_LOC_DTL GD
                                      ,MDM_LOCATION           MD
                                WHERE GD.PROP_NO          = GC.PROP_NO
                                  AND GD.AMDT_SEQ         = GC.AMDT_SEQ
                                  AND GD.SVC_SCP_CD       = GC.SVC_SCP_CD
                                  AND GD.GRP_LOC_SEQ      = GC.GRP_LOC_SEQ
                                  AND GD.SRC_INFO_CD      <> 'AD'
                                  AND GC.PROP_NO          = A1.PROP_NO
                                  AND GC.AMDT_SEQ         = A1.AMDT_SEQ
                                  AND GC.SVC_SCP_CD       = A1.SVC_SCP_CD
                                  AND DECODE(GC.ORG_DEST_TP_CD, 'B', A1.ORG_DEST_TP_CD,  GC.ORG_DEST_TP_CD)  = A1.ORG_DEST_TP_CD
                                  AND GD.LOC_CD           = MD.LOC_CD
                                  AND MD.CALL_PORT_FLG = 'N'
                               )
AND A1.ROUT_PNT_LOC_TP_CD = 'G' -- Group location 일 경우 
AND A1.PROP_NO = A2.PROP_NO
AND A1.AMDT_SEQ = A2.AMDT_SEQ
AND A1.SVC_SCP_CD = A2.SVC_SCP_CD
AND A1.CMDT_HDR_SEQ = A2.CMDT_HDR_SEQ
AND A2.FIC_RT_TP_CD = 'G' -- Port CY 운임인 경우
UNION 
SELECT A1.SVC_SCP_CD || '-' ||
      'Bullet No.'||A3.BLET_DP_SEQ || '-' ||
       DECODE(A1.ORG_DEST_TP_CD, 'O', 'Origin', 'Dest.')|| '-' ||
       A1.ROUT_PNT_LOC_DEF_CD AS ETC1
FROM PRI_RP_SCP_RT_ROUT_PNT A1
    ,MDM_LOCATION A2
    ,PRI_RP_SCP_RT_CMDT_HDR A3
WHERE A1.PROP_NO = @[prop_no]
AND A1.AMDT_SEQ = @[amdt_seq]
AND A1.SVC_SCP_CD NOT IN ('TPE','TPW','ACE','ACW','TAE','TAW','ASE','ASW','MMW','MME','SAN','SAS','CAN','CAS','CSE','CME','MWS','MWN','CCS','CCN','CLN','CLS') -- PORT CY, IHC RATE 구분하지 않는 예외 SCOPE
AND A1.ROUT_PNT_LOC_DEF_CD = A2.LOC_CD
AND A2.CALL_PORT_FLG = 'N'
AND A1.SRC_INFO_CD      <> 'AD'
AND A1.ROUT_PNT_LOC_TP_CD = 'L' -- Location으로 입력된 경우 
AND A1.PROP_NO = A3.PROP_NO
AND A1.AMDT_SEQ = A3.AMDT_SEQ
AND A1.SVC_SCP_CD = A3.SVC_SCP_CD
AND A1.CMDT_HDR_SEQ = A3.CMDT_HDR_SEQ
AND A3.FIC_RT_TP_CD = 'G' -- Port CY 운임인 경우
UNION
SELECT A1.SVC_SCP_CD ||'-'|| 
     'Bullet No.'||A2.BLET_DP_SEQ || '-'||
      DECODE(A1.ORG_DEST_TP_CD, 'O', 'Origin', 'Dest.')|| '-' ||
      A1.ROUT_PNT_LOC_DEF_CD AS ETC1
FROM PRI_RP_SCP_RT_ROUT_PNT A1
    ,PRI_RP_SCP_RT_CMDT_HDR A2
WHERE A1.PROP_NO = @[prop_no]
AND A1.AMDT_SEQ = @[amdt_seq]
AND A1.SVC_SCP_CD NOT IN ('TPE','TPW','ACE','ACW','TAE','TAW','ASE','ASW','MMW','MME','SAN','SAS','CAN','CAS','CSE','CME','MWS','MWN','CCS','CCN','CLN','CLS') -- PORT CY, IHC RATE 구분하지 않는 예외 SCOPE
AND A1.ROUT_PNT_LOC_DEF_CD IN (
                               SELECT  GC.PRC_GRP_LOC_CD
                                 FROM  PRI_RP_SCP_GRP_LOC GC  
                                      ,PRI_RP_SCP_GRP_LOC_DTL GD
                                      ,PRI_TRF_IHC_EXPT_CY_LOC   EX
                                WHERE GD.PROP_NO          = GC.PROP_NO
                                  AND GD.AMDT_SEQ         = GC.AMDT_SEQ
                                  AND GD.SVC_SCP_CD       = GC.SVC_SCP_CD
                                  AND GD.GRP_LOC_SEQ      = GC.GRP_LOC_SEQ
                                  AND GD.SRC_INFO_CD      <> 'AD'
                                  AND GC.PROP_NO          = A1.PROP_NO
                                  AND GC.AMDT_SEQ         = A1.AMDT_SEQ
                                  AND GC.SVC_SCP_CD       = A1.SVC_SCP_CD
                                  AND DECODE(GC.ORG_DEST_TP_CD, 'B', A1.ORG_DEST_TP_CD,  GC.ORG_DEST_TP_CD)  = A1.ORG_DEST_TP_CD
                                  AND GD.SVC_SCP_CD       = EX.SVC_SCP_CD
                                  AND DECODE(GC.ORG_DEST_TP_CD, 'B', EX.ORG_DEST_TP_CD,  GC.ORG_DEST_TP_CD)  = EX.ORG_DEST_TP_CD
                                  AND GD.LOC_CD           = EX.EXPT_LOC_CD
                                  AND EX.PRC_IHC_ONY_FLG = 'Y'  --PORT Flag가 Y 이나 IHC에만 넣어야 하는 예외 Location 에 있는 경우 걸러냄
                               )
AND A1.ROUT_PNT_LOC_TP_CD = 'G' -- Group location 일 경우 
AND A1.PROP_NO = A2.PROP_NO
AND A1.AMDT_SEQ = A2.AMDT_SEQ
AND A1.SVC_SCP_CD = A2.SVC_SCP_CD
AND A1.CMDT_HDR_SEQ = A2.CMDT_HDR_SEQ
AND A2.FIC_RT_TP_CD = 'G' -- Port CY 운임인 경우
UNION 
SELECT A1.SVC_SCP_CD || '-' ||
      'Bullet No.'||A3.BLET_DP_SEQ || '-' ||
       DECODE(A1.ORG_DEST_TP_CD, 'O', 'Origin', 'Dest.')|| '-' ||
       A1.ROUT_PNT_LOC_DEF_CD AS ETC1
FROM PRI_RP_SCP_RT_ROUT_PNT A1
    ,PRI_TRF_IHC_EXPT_CY_LOC A2
    ,PRI_RP_SCP_RT_CMDT_HDR A3
WHERE A1.PROP_NO = @[prop_no]
AND A1.AMDT_SEQ = @[amdt_seq]
AND A1.SVC_SCP_CD NOT IN ('TPE','TPW','ACE','ACW','TAE','TAW','ASE','ASW','MMW','MME','SAN','SAS','CAN','CAS','CSE','CME','MWS','MWN','CCS','CCN','CLN','CLS') -- PORT CY, IHC RATE 구분하지 않는 예외 SCOPE
AND A1.SVC_SCP_CD      = A2.SVC_SCP_CD
AND A1.ORG_DEST_TP_CD  = A2.ORG_DEST_TP_CD
AND A1.ROUT_PNT_LOC_DEF_CD  = A2.EXPT_LOC_CD
AND A2.PRC_IHC_ONY_FLG = 'Y' --PORT Flag가 Y 이나 IHC에만 넣어야 하는 예외 Location 에 있는 경우 걸러냄
AND A1.SRC_INFO_CD      <> 'AD'
AND A1.ROUT_PNT_LOC_TP_CD = 'L' -- Location으로 입력된 경우 
AND A1.PROP_NO = A3.PROP_NO
AND A1.AMDT_SEQ = A3.AMDT_SEQ
AND A1.SVC_SCP_CD = A3.SVC_SCP_CD
AND A1.CMDT_HDR_SEQ = A3.CMDT_HDR_SEQ
AND A3.FIC_RT_TP_CD = 'G' -- Port CY 운임인 경우			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
