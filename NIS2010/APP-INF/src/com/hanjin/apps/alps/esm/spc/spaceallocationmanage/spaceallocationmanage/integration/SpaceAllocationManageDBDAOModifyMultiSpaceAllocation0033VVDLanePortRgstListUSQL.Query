<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOModifyMultiSpaceAllocation0033VVDLanePortRgstListUSQL">
			<desc><![CDATA[ModifyMultiSpaceAllocation0033VVDLanePortRgstList]]></desc>
			<sql><![CDATA[
UPDATE SPC_IRR_LANE_PORT_RGST
SET ALOC_HC_CALC_QTY = @[aloc_hc_calc_qty]
  , ALOC_HC_BZC_BX_QTY = @[aloc_hc_bzc_bx_qty]
  , ALOC_HC_APLY_FLG = @[aloc_hc_aply_flg]
  , ALOC_HC_OVR_CALC_QTY = @[aloc_hc_ovr_calc_qty]
  , ALOC_45FT_CALC_QTY = @[aloc_45ft_calc_qty]
  , ALOC_45FT_BZC_BX_QTY = @[aloc_45ft_bzc_bx_qty]
  , ALOC_45FT_APLY_FLG = @[aloc_45ft_aply_flg]
  , ALOC_45FT_OVR_CALC_QTY = @[aloc_45ft_ovr_calc_qty]
  , EFF_TO_DT = REPLACE(@[eff_to_dt], '-', '')
  , UPD_USR_ID = @[usr_id]
  , UPD_DT = SYSDATE
WHERE 1=1
AND REP_TRD_CD = @[rep_trd_cd]
AND SUB_TRD_CD = @[sub_trd_cd]
AND RLANE_CD = @[rlane_cd]
AND DIR_CD = @[dir_cd]
AND EFF_FM_DT = REPLACE(@[eff_fm_dt], '-', '')
AND BSA_CAPA = @[bsa_capa]
AND VSL_CD = SUBSTR(@[vvd],1,4)
AND SKD_VOY_NO = SUBSTR(@[vvd],5,4)
AND SKD_DIR_CD = SUBSTR(@[vvd],9,1)
AND POL_CD = @[pol_cd]			]]></sql>
			<params>
				<param name="aloc_hc_calc_qty" type="12" value="" out="N"/>
				<param name="aloc_hc_bzc_bx_qty" type="12" value="" out="N"/>
				<param name="aloc_hc_aply_flg" type="12" value="" out="N"/>
				<param name="aloc_hc_ovr_calc_qty" type="12" value="" out="N"/>
				<param name="aloc_45ft_calc_qty" type="12" value="" out="N"/>
				<param name="aloc_45ft_bzc_bx_qty" type="12" value="" out="N"/>
				<param name="aloc_45ft_aply_flg" type="12" value="" out="N"/>
				<param name="aloc_45ft_ovr_calc_qty" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="bsa_capa" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
