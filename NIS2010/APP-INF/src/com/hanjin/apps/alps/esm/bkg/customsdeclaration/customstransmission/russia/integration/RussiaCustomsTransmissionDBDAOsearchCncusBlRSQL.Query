<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RussiaCustomsTransmissionDBDAOsearchCncusBlRSQL">
			<desc><![CDATA[searchCncusBl]]></desc>
			<sql><![CDATA[
SELECT	/*+ ORDERED */                                                                                                                                               
	     ST.BKG_NO                                                                                                                                              
                ,TO_CHAR((SELECT MAX(SKD.VPS_ETB_DT)
                                   FROM     BKG_VVD VVD, VSK_VSL_PORT_SKD SKD
                                   WHERE    VVD.BKG_NO 		= ST.BKG_NO
                                   AND      VVD.VSL_CD 		= SKD.VSL_CD
                                   AND      VVD.SKD_VOY_NO 	= SKD.SKD_VOY_NO
                                   AND      VVD.SKD_DIR_CD 	= SKD.SKD_DIR_CD
                                   AND      VVD.POL_CD 		= SKD.VPS_PORT_CD
                                   AND      VVD.POL_CLPT_IND_SEQ = SKD.CLPT_IND_SEQ), 'YYYYMMDD') 	VPS_ETB_DT                                                                                                 
                ,TO_CHAR(ST.POD_ETA_DT, 'YYYYMMDD')   			BL_POD_ETA_DT                                                                                                  
                ,TO_CHAR(ISS.OBL_ISS_DT, 'YYYYMMDD')		   	BL_ISS_DT                                                                                              
                ,MO.LOC_CD	            						BL_ISS_LOC_CD                                                                                                             
                ,LOC5.LOC_NM                					BL_ISS_LOC_NM                                                                                                              
                ,ST.POR_CD		 								POR_CD                                                                                                                        
                ,LOC1.LOC_NM	    							POR_NM                                                                                                                         
                ,ST.POL_CD		 								BKG_POL_CD 
				,ST.POD_CD		 								BKG_POD_CD 
				,ST.DEL_CD		 								DEL_CD                                                                                                                  
                ,LOC2.LOC_NM	          						POL_NM                                                                                                                      
                ,LOC3.LOC_NM	         						POD_NM         
                ,LOC4.LOC_NM	        						DEL_NM                                                                                                                              
                ,ST.RCV_TERM_CD||ST.DE_TERM_CD              	RD_TERM                   	                                                                                                                         
                ,BKG_SPCLCHAR_CONV_FNC(TRIM(S.CUST_NM), 'C')   S_CUST_NM                                                                                                                         
                ,BKG_SPCLCHAR_CONV_FNC(TRIM(S.CUST_ADDR), 'C') S_CUST_ADDR                                                                                             
                ,BKG_SPCLCHAR_CONV_FNC(TRIM(C.CUST_NM), 'C')   C_CUST_NM            	                                                                                                         
                ,BKG_SPCLCHAR_CONV_FNC(TRIM(C.CUST_ADDR), 'C') C_CUST_ADDR                                                                                                 
                ,BKG_SPCLCHAR_CONV_FNC(TRIM(N.CUST_NM), 'C')   N_CUST_NM                                                                                                                          
                ,BKG_SPCLCHAR_CONV_FNC(TRIM(N.CUST_ADDR), 'C') N_CUST_ADDR                                                                                                                      
                ,NVL(TRIM(DOC.CSTMS_DESC),'N/M') 				CSTMS_DESC
                ,SUBSTR(TRIM(SM.BL_MK_DESC),1,500) 				BL_MK_DESC
                ,DOC.ACT_WGT									ACT_WGT	                                                                                                             
                ,DOC.PCK_QTY	            					PCK_QTY	                                                                                                                 
                ,DOC.PCK_TP_CD			    					PCK_TP_CD
				,MPT.PCK_NM                                     BLPKGU_NM                                                                                                
                ,DOC.MEAS_QTY	
			    ,DECODE(BR.FRT_TERM_CD,'P','PP','C','CC')  		PRFLAG                                                
FROM	BKG_VVD VVD,
        BKG_BOOKING ST,   
        BKG_BL_DOC DOC,
    	BKG_CUSTOMER C,                                                                                                                                        
    	BKG_CUSTOMER S,                                                                                                                                        
    	BKG_CUSTOMER N,                                                                                                                                        
    	MDM_LOCATION LOC1,                                                                                                                                           
    	MDM_LOCATION LOC2,                                                                                                                                           
    	MDM_LOCATION LOC3,                                                                                                                                           
    	MDM_LOCATION LOC4,
 		MDM_LOCATION LOC5,                                                                                                                                           
    	MDM_ORGANIZATION MO,
		MDM_PCK_TP  MPT,
        BKG_BL_ISS ISS,
		BKG_RATE   BR,
        ( SELECT  BKG_NO, DBMS_LOB.SUBSTR( BKG_SPCLCHAR_CONV_CLOB_FNC(MK_DESC,'C'), DBMS_LOB.GetLength(BKG_SPCLCHAR_CONV_CLOB_FNC(MK_DESC,'C')) ) BL_MK_DESC                                                                    
          FROM    BKG_BL_MK_DESC                                                                                                                                   
          WHERE	  1=1                                                                                                                                                  
		  AND   ( #foreach($field_id in ${field_list})                                                                                                                           
      	#if($velocityCount > 1)                                                                                                                                      
      	OR #end      BKG_NO IN ( $field_id )                                                                                                                       
      #end                                                                                                                                                           
)     
		) SM                                                                                                                               
WHERE	1=1                                                                                                                                                          
AND ( #foreach($field_id in ${field_list})                                                                                                                           
      	#if($velocityCount > 1)                                                                                                                                      
      	OR #end     VVD.BKG_NO IN ( $field_id )                                                                                                                       
      #end                                                                                                                                                           
)   
#if ( ${mode_type} == 'I' ) 
AND VVD.POD_CD = @[pod_cd]
#else 
AND VVD.POL_CD = @[pol_cd]
#end

AND VVD.BKG_NO = ST.BKG_NO
AND ST.BKG_NO = DOC.BKG_NO
AND	    ST.BKG_NO		      =	C.BKG_NO(+)
AND	    C.BKG_CUST_TP_CD(+)	  =	'C'
AND	    ST.BKG_NO		      =	S.BKG_NO(+)
AND	    S.BKG_CUST_TP_CD(+)	  =	'S'
AND	    ST.BKG_NO		      =	N.BKG_NO(+)                                                                                                                        
AND	    N.BKG_CUST_TP_CD(+)	  =	'N'                                                                                                                                  
AND	    ST.POR_CD		      =	LOC1.LOC_CD                                                                                                                          
ANd	    ST.POL_CD	      	  =	LOC2.LOC_CD                                                                                                                          
AND	    ST.POD_CD	      	  =	LOC3.LOC_CD                                                                                                                          
AND	    ST.DEL_CD	      	  =	LOC4.LOC_CD  
AND     ST.BKG_NO = ISS.BKG_NO
AND	    ISS.OBL_ISS_OFC_CD    =	MO.OFC_CD(+)
AND     MO.LOC_CD             =  LOC5.LOC_CD(+)                                                                                                                       
AND	    ST.BKG_NO		      =	SM.BKG_NO(+)
AND		ST.BKG_NO = BR.BKG_NO(+)
AND     DOC.PCK_TP_CD    =  MPT.PCK_CD(+)			]]></sql>
			<params>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
