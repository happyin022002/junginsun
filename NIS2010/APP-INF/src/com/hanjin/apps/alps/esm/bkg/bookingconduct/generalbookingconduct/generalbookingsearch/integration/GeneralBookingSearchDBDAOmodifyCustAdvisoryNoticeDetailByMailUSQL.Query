<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOmodifyCustAdvisoryNoticeDetailByMailUSQL">
			<desc><![CDATA[메일 전송 후 Advisory Notice 대상 B/L의 연락처 정보를 갱신한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CUST_AVC_NTC_DTL
SET  NTC_EML         = @[ntc_eml]
   , EML_SND_DT     = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[snd_ofc_cd])
   , EML_SND_GDT    = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', SYSDATE, 'GMT' )
   , EML_SND_USR_ID = @[eml_snd_usr_id]
   , NTC_EML_SND_ID = @[ntc_eml_snd_id]
   , UPD_USR_ID     = @[upd_usr_id]
   , UPD_DT         = sysdate
WHERE BL_NO          = @[bl_no]
AND   BKG_CUST_TP_CD = @[bkg_cust_tp_cd]
AND   SRC_DAT_TP_CD  = @[src_dat_tp_cd]			]]></sql>
			<params>
				<param name="ntc_eml" type="12" value="" out="N"/>
				<param name="snd_ofc_cd" type="12" value="" out="N"/>
				<param name="eml_snd_usr_id" type="12" value="" out="N"/>
				<param name="ntc_eml_snd_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="src_dat_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
