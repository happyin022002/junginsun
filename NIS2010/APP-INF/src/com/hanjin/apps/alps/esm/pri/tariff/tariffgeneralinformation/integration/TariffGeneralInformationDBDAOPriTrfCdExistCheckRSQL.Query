<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffGeneralInformationDBDAOPriTrfCdExistCheckRSQL">
			<desc><![CDATA[Tariff Code의 존재 유무를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT  COUNT(1) CNT
  FROM  PRI_TARIFF
 WHERE  1 = 1
   AND  TRF_PFX_CD = @[trf_pfx_cd]
   AND  TRF_NO     = @[trf_no]			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
