<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDAOFmsAccountItemListRSQL">
			<desc><![CDATA[TCharterIOBasicRegisterDAOFmsAccountItemListRSQL]]></desc>
			<sql><![CDATA[
select 
	   mi.acct_itm_nm,
	   mi.acct_cd, 
	   mi.acct_itm_seq
  from fms_acct_cate mc, fms_acct_itm mi
 where mc.flet_acct_cate_cd = @[flet_acct_cate_cd]
   and mc.acct_cd = mi.acct_cd
   and mc.acct_itm_seq = mi.acct_itm_seq
#if (${acct_itm_nm} != '')    
   and Upper(mi.acct_itm_nm) like '%' || Upper(@[acct_itm_nm]) || '%'
#end
 order by mi.acct_itm_nm 			]]></sql>
			<params>
				<param name="flet_acct_cate_cd" type="12" value="" out="N"/>
				<param name="acct_itm_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
