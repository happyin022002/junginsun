<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCdlMkDescRSQL">
			<desc><![CDATA[searchCdlMkDesc]]></desc>
			<sql><![CDATA[
SELECT	REPLACE(DOC.PCK_CMDT_DESC,'*','-')||CHR(10)||
        REPLACE(DOC.CNTR_CMDT_DESC,'*','-')||CHR(10)||
        REPLACE(BMD.CMDT_DESC,'*','-') CMDT_DESC,
	    REPLACE(BMD.MK_DESC,'*','-') MK_DESC
FROM BKG_BL_MK_DESC BMD, BKG_BL_DOC DOC
WHERE	DOC.BKG_NO = @[in_bkg_no]
AND     DOC.BKG_NO = BMD.BKG_NO			]]></sql>
			<params>
				<param name="in_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
