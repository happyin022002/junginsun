<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOSelectLocCdExistYnRSQL">
			<desc><![CDATA[ESM_BKG_0905]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(LOC_CD), 0, 'N', 'Y') EXIST_YN 
  FROM MDM_LOCATION 
 WHERE LOC_CD = @[loc_cd] 
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
