   AND (A2.NMD_CUST_FLG IS NULL OR A2.NMD_CUST_FLG = 'N')
   AND A1.CUST_GRP_ID = A2.CUST_GRP_ID
#if(${ofc_team_cd} != '' && ${ofc_team_cd} != 'All')
   AND A1.OFC_TEAM_CD = @[ofc_team_cd]
#end
#if(${cust_rhq_cd} != '' && ${cust_rhq_cd} != 'All')
   AND A1.RHQ_CD = @[cust_rhq_cd]
#end
 GROUP BY A1.CUST_GRP_ID, A1.CUST_GRP_NM
 ORDER BY A1.CUST_GRP_NM

#elseif (${methodname} == 'searchMtyReturnCY')
/* 입력된 DEL에 해당하는 MT Return CY 조회 */
#if(${de_term} == 'Y')
 SELECT EQ_RTN_YD_CD
   FROM MDM_LOCATION
  WHERE LOC_CD = @[loc_cd]
    AND DELT_FLG = 'N'
#else
 SELECT REP_YD_CD
   FROM MDM_ZONE
  WHERE 1=1
#if(${tml_cd} != '')
    AND ZN_CD = @[loc_cd]||@[tml_cd]
#else
    AND ZN_CD = (SELECT REP_ZN_CD
                   FROM MDM_LOCATION
                  WHERE LOC_CD = @[loc_cd]
                    AND DELT_FLG = 'N')
#end
    AND DELT_FLG = 'N'
#end

#end			]]></sql>
