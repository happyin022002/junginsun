<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOGetCodeSelectRSQL">
			<desc><![CDATA[COA에서 사용하는 목록성 코드조회
2011.01.26 최윤성 [CHM-201108537-01] searchVslOwnerList 기능 추가
2011.06.22 김민아 [CHM-201111640-01] searchMdmReeferCoreAcctList 기능 추가
2012.03.22 김종준 [CHM-201217091-01] searchCntrOrgDestCdList 기능 추가
2012.08.29 이석준[CHM-201219872]   Inquiry by customized condition_MT Pick up Location 등 메뉴 추가
2012.10.25 김보배 [CHM-201220395] [COA] Add-on Tariff Management 개선 프로젝트
2012.12.24 최윤성 [CHM-201222072] RF화물의 EMU Credit 적용로직 변경 - TPSZ combo 쿼리 수정
2013.12.13 김수정 [CHM-201328111] [COA] EMU COST 변경 로직 Pre CM 반영 요청 - DEL Code, DEL Term 에 따른 MT Return CY 조회 추가
2014.08.13 박은주 [CHM-201431516]  Logistics PFMC Report - KPI 3 추가 및 화면변경 요청사항(KPI1,3 추가)
2015.08.31 손진환 [CHM-201536992] Split14-[그룹사 표준 코드 시행] HJS 프로그램 대응 및 데이타 마이그레이션 작업 요청]]></desc>
			<sql><![CDATA[
#if (${methodname} == '')
	SELECT	'' AS CODE
	,		'' AS NAME
	
	,		'' AS KEY
	,		'' AS RTN_DATE
	,		'' AS ETD_DT 
	,		'' AS OFC_CD
	,		'' AS PRNT_OFC_CD
	,		'' AS SIM_NO
	,		'' AS OFC_LEVEL
	,		'' AS COST_YR
	,		'' AS COST_WK 
	,		'' AS TEAM_CODE
	,		'' AS RT
	,		'' AS SORT_KEY
	FROM	DUAL
#elseif (${methodname} == 'searchRevLaneList')

/*revenue lane의 정보를 가져온다*/

	SELECT DISTINCT RLANE_CD NAME
          ,RLANE_CD CODE 
	  FROM COA_LANE_RGST 
	 WHERE TRD_CD = CASE  
	               	WHEN @[str_trd_cd] IS NULL AND @[str_sub_trd_cd] IS NULL 
    	             THEN '' 
  	                WHEN @[str_trd_cd] IS NULL AND @[str_sub_trd_cd] IS NOT NULL 
                     THEN TRD_CD 
  	                ELSE @[str_trd_cd]
                   END
       AND SUB_TRD_CD = NVL(@[str_sub_trd_cd],SUB_TRD_CD) 
       AND NVL(DELT_FLG,'N') = 'N' 
	 ORDER BY RLANE_CD

#elseif (${methodname} == 'searchRevLaneList2')

/*revenue lane의 정보를 가져온다*/

	SELECT DISTINCT RLANE_CD NAME
          ,RLANE_CD CODE 
	  FROM COA_LANE_RGST 
     WHERE NVL(DELT_FLG,'N') = 'N' 
	 ORDER BY RLANE_CD 

#elseif (${methodname} == 'searchRLaneList')

/*revenue lane의 정보를 가져온다*/

	SELECT DISTINCT TRD_CD KEY
          ,RLANE_CD NAME
		  ,RLANE_CD CODE 
	  FROM COA_LANE_RGST 
	 WHERE NVL(DELT_FLG,'N') = 'N' 
 	   AND TRD_CD = NVL(@[trd_cd], TRD_CD) 
	 ORDER BY KEY
     		,NAME 

#elseif (${methodname} == 'searchRLaneList4')

/*IP를 제외한 revenue lane의 정보를 가져온다*/

	SELECT DISTINCT TRD_CD KEY
          ,RLANE_CD NAME
 	      ,RLANE_CD CODE 
	  FROM COA_LANE_RGST 
	 WHERE NVL(DELT_FLG,'N') = 'N' 
  	   AND TRD_CD = NVL(@[trd_cd], TRD_CD) 
  	   AND SUB_TRD_CD <> 'IP' 
	 ORDER BY KEY
     		 ,NAME 

#elseif (${methodname} == 'searchSMUSubTradeList')

/*SMU Sub Trade 정보를 가져온다*/

	SELECT DISTINCT  SUB_TRD_CD CODE
          ,SUB_TRD_CD NAME 
	  FROM MDM_DTL_REV_LANE                           
	 WHERE SUB_TRD_CD IS NOT NULL                     
	   AND TRD_CD = NVL(@[trd_cd], TRD_CD)                
	   AND SUB_TRD_CD NOT IN ('IP', 'OT')                      
	 ORDER BY SUB_TRD_CD 

#elseif (${methodname} == 'searchSLaneList')

/*service lane의 정보를 가져온다*/

	SELECT DISTINCT TRD_CD KEY
          ,SLAN_CD NAME
          ,SLAN_CD CODE 
	  FROM COA_LANE_RGST 
	 WHERE TRD_CD = NVL(@[trd_cd], TRD_CD) 
  	   AND NVL(DELT_FLG,'N') = 'N' 
	 ORDER BY TRD_CD
        	 ,SLAN_CD  

#elseif (${methodname} == 'searchLccList')

/*LCC의 목록을 가져온다*/

	#if(${code} == '') 
   		SELECT DISTINCT LCC_CD NAME
	          ,LCC_CD CODE 
    	  FROM COA_LOCATION_V 
    	 ORDER BY LCC_CD 			
	#else
    	SELECT DISTINCT RCC_CD KEY
			  ,LCC_CD NAME
			  ,LCC_CD CODE 
    	  FROM COA_LOCATION_V 
    	 WHERE RCC_CD = @[code] 
   	     ORDER BY RCC_CD, LCC_CD  
	#end 

#elseif (${methodname} == 'searchEccList')

/*ECC의 목록을 가져온다*/

	#if (${code} == '') 
		SELECT DISTINCT ECC_CD NAME
			  ,ECC_CD CODE 
		FROM COA_LOCATION_V 
		ORDER BY ECC_CD 
  #else 
		SELECT DISTINCT RCC_CD || LCC_CD KEY
			  ,ECC_CD NAME
			  ,ECC_CD CODE 
		  FROM COA_LOCATION_V 
         WHERE RCC_CD = @[rcc_cd] 
		   AND LCC_CD = @[lcc_cd]
		 ORDER BY RCC_CD
				 ,LCC_CD  
   #end 

#elseif (${methodname} == 'searchSimList')

/*Simulation number의 설명을 조회한다*/

	SELECT TO_CHAR(TO_DATE(SIM_DT,'yyyymmdd'),'yyyy-mm-dd')  || chr(14849154)|| SIM_NO  || chr(14849154) ||  CRE_USR_ID  NAME   
      	  ,SIM_DEPT_CD || '|' || SIM_DT  || '|' || SIM_NO  CODE  
	  FROM COA_SIM_INFO 
     WHERE SLAN_CD = @[slan_cd] 
  	   AND SIM_DEPT_CD = NVL(@[sim_dept_cd],SIM_DEPT_CD) 
     ORDER BY  SIM_DT DESC
              ,SIM_NO DESC 

#elseif (${methodname} == 'searchActivityByYrmon')

/*목록을 조회한다*/

	SELECT OFC_ACT_CD CODE
      	  ,MAX(OFC_ACT_NM) NAME 
	  FROM COA_SVC_TRNS_PRC 

	   #if (${code} != '')
  	   	  WHERE COST_YRMON  = @[code] 
      #end

	 GROUP BY OFC_ACT_CD 

#elseif (${methodname} == 'searchSimVesselList')

/*Vessel콤보의 목록을 가져온다*/

	SELECT '1' SEQ
		  ,VSL_CD CODE
		  ,VSL_CD NAME 
	  FROM COA_VSL_RGST 
	 WHERE VSL_TP_CD = 'C' 
  	   AND DELT_FLG  = 'N' 
  	   AND LST_FLG   = 'Y'
    UNION ALL 
    SELECT '0' SEQ
		  ,VSL_CD CODE
		  ,VSL_CD NAME 
      FROM COA_SIM_VSL_RGST 
     ORDER BY SEQ
			,CODE 

#elseif (${methodname} == 'searchSubOFCList')

/*선택한 office Level에 해당하는 office code목록을 조회한다*/

	SELECT DISTINCT DECODE(@[sel_ofc_lvl]
                      	,'1', OFC_N1ST_LVL_CD
                      	,'2', CASE
                          	WHEN @[cost_yr]||@[cost_mon] < '200901' AND OFC_N3RD_LVL_CD = 'SINRS' AND OFC_N3RD_LVL_TP_CD = 'SQ'
                             	THEN OFC_N3RD_LVL_CD
                          	ELSE OFC_N2ND_LVL_CD
                       	END
                      	,'3', OFC_N3RD_LVL_CD
                      	,'4', OFC_N4TH_LVL_CD
                      	,'5', OFC_N5TH_LVL_CD
                      	,'6', OFC_N6TH_LVL_CD
                      	,'7', OFC_N7TH_LVL_CD
                      	) CODE
               	,DECODE(@[sel_ofc_lvl]
                      	,'1', OFC_N1ST_LVL_CD
                      	,'2', CASE
                          	WHEN @[cost_yr]||@[cost_mon] < '200901' AND OFC_N3RD_LVL_CD = 'SINRS' AND OFC_N3RD_LVL_TP_CD = 'SQ'
                             	THEN OFC_N3RD_LVL_CD
                          	ELSE OFC_N2ND_LVL_CD
                       	END
                      	,'3', OFC_N3RD_LVL_CD
                      	,'4', OFC_N4TH_LVL_CD
                      	,'5', OFC_N5TH_LVL_CD
                      	,'6', OFC_N6TH_LVL_CD
                      	,'7', OFC_N7TH_LVL_CD
                      	) NAME
    FROM COA_OFC_LVL
   WHERE 1 = 1
		
			/*COST_MON이 들어온 경우, 2009년 이상 데이터는 월별관리*/
			#if(${cost_yr} >= 2009 &&  ${cost_mon}!= 'XX') 
		  		AND @[cost_yr]||@[cost_mon] BETWEEN OFC_APLY_FM_YRMON AND OFC_APLY_TO_YRMON /*월별관리*/
		#elseif(${cost_yr} < 2009)
		  		AND '200812' BETWEEN OFC_APLY_FM_YRMON AND OFC_APLY_TO_YRMON
		  #else
		  		AND '200901' BETWEEN OFC_APLY_FM_YRMON AND OFC_APLY_TO_YRMON
		  #end
		
     			AND DECODE(@[usr_ofc_lvl]
               		,'1', OFC_N1ST_LVL_CD
               		,'2', CASE
                   WHEN @[cost_yr]||@[cost_mon] < '200901' AND OFC_N3RD_LVL_CD = 'SINRS' AND OFC_N3RD_LVL_TP_CD = 'SQ'
                      THEN OFC_N3RD_LVL_CD
                   ELSE OFC_N2ND_LVL_CD
                END
               ,'3', OFC_N3RD_LVL_CD
               ,'4', OFC_N4TH_LVL_CD
               ,'5', OFC_N5TH_LVL_CD
               ,'6', OFC_N6TH_LVL_CD
               ,'7', OFC_N7TH_LVL_CD
               ) #if(${usr_ofc_cd} == 'SHADSA' && ${usr_ofc_lvl} == '4')
                  IN (@[usr_ofc_cd], 'SHADPI', '', '')

                 #elseif(${usr_ofc_cd}  == 'HKGSC' && ${usr_ofc_lvl} == '5')
                  IN (@[usr_ofc_cd], 'SZPSC', 'TPESC', 'XMNSC')

				 #elseif(${usr_ofc_cd}  == 'TORBS' && ${usr_ofc_lvl} == '6')
                  IN (@[usr_ofc_cd], 'VANSO', 'MTRBS', '')

				 #else 
				  IN (@[usr_ofc_cd], '', '', '')
				 #end

     AND DECODE(@[sel_ofc_lvl]
               ,'1', OFC_N1ST_LVL_CD
               ,'2', OFC_N2ND_LVL_TP_CD
               ,'3', OFC_N3RD_LVL_TP_CD
               ,'4', CASE
                   WHEN @[cost_yr]||@[cost_mon] < '200901'
                      THEN CASE
                             WHEN OFC_N4TH_LVL_CD = 'NYCRA'
                                THEN OFC_N4TH_LVL_CD
                             ELSE OFC_N4TH_LVL_TP_CD
                          END
                   ELSE CASE
                   WHEN OFC_N4TH_LVL_CD IN('SHARC', 'SINRS', 'HAMRU', 'NYCRA','SELIB','TYOIB')
                      THEN OFC_N4TH_LVL_CD
                   ELSE OFC_N4TH_LVL_TP_CD
                END
                END
               ,'5', OFC_N5TH_LVL_TP_CD
               ,'6', OFC_N6TH_LVL_TP_CD
               ,'7', OFC_N7TH_LVL_TP_CD
               ,' '
               ) IS NOT NULL
  ORDER BY CODE	

#elseif (${methodname} == 'searchReportItem')

/*Report Item Infomation 목록을 조회한다*/

	SELECT SLCT_ITM_FOM_SEQ CODE
	      ,SLCT_ITM_FOM_DESC NAME
  	  FROM COA_RPT_ITM_INFO_MST
 	 WHERE CRE_USR_ID = @[code]

#elseif (${methodname} == 'checkBKGNo')

/*Booking Number 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
     	 		  SELECT '1' 
     	 			FROM COA_RGST_BKG 
      			   WHERE BKG_NO = @[bkgno] 
				  )

#elseif (${methodname} == 'checkLocationCode')

/*Location Code 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
      			   SELECT '1' 
      			     FROM MDM_LOCATION 
      			    WHERE LOC_CD = @[loccd]  
				   ) 

#elseif (${methodname} == 'checkVesselCode')

/*Vessel Code 유무를 확인*/

	SELECT 'X' 
 	  FROM DUAL 
 	 WHERE EXISTS ( 
               	   SELECT '1' 
                     FROM MDM_VSL_CNTR 
                    WHERE VSL_CD = @[vslcd]  
                  )   

#elseif (${methodname} == 'checkVVDCode')

/*VVD Code 유무를 확인*/

	SELECT 'X' 
      FROM DUAL 
     WHERE EXISTS ( 
      			   SELECT '1' 
      			     FROM VSK_VSL_SKD 
     			    WHERE VSL_CD = @[vslcd] 
        		      AND SKD_VOY_NO = @[skdvoyno]
                      AND SKD_DIR_CD = @[skddircd] 
    			   ) 

#elseif (${methodname} == 'checkOfficeCode')

/*Office Code 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
      			   SELECT '1' 
      				 FROM MDM_ORGANIZATION 
      				WHERE OFC_CD = @[ofccd] 
     			   ) 

#elseif (${methodname} == 'checkRevLaneCode')

/*Revenue Lane Code 유무를 확인*/

	SELECT 'X' 
	  FROM DUAL 
	 WHERE EXISTS ( 
      			   SELECT '1' 
      				 FROM MDM_REV_LANE 
      			    WHERE RLANE_CD = @[rlanecd] 
      				  AND NVL(DELT_FLG,'*') = 'N' 
     			   ) 

#elseif (${methodname} == 'checkNodeCode')

/*Port or Node Code 유무를 확인*/

	SELECT 'X' 
      FROM DUAL 
     WHERE EXISTS ( 
                   SELECT '1' 
                     FROM MDM_YARD 
                    WHERE YD_CD = @[tmnlcd] 
	               ) 

#elseif (${methodname} == 'checkSLaneCode')

/*Service Lane Code 유무를 확인*/

	SELECT 'X' 
  	  FROM DUAL 
 	 WHERE EXISTS ( 
               	   SELECT '1' 
                 	 FROM MDM_VSL_SVC_LANE 
                	WHERE DELT_FLG      = 'N'
                  	  AND VSL_SLAN_CD   = @[slancd] 
              	   ) 

#elseif (${methodname} == 'searchActivityByYrmon')

/*목록을 조회한다*/

	SELECT OFC_ACT_CD CODE, MAX(OFC_ACT_NM) NAME 
  	  FROM COA_SVC_TRNS_PRC 
   	   	#if(${code} != '')  
	    	WHERE COST_YRMON = @[code] 
      	#end

#elseif (${methodname} == 'searchOFCLevelList')

/*office Level List 목록을 조회한다*/

	SELECT DISTINCT OFC_LVL CODE
		  ,OFC_LVL_DESC NAME 
  	  FROM   COA_RPT_AUTH_MGMT 
 	 WHERE OFC_LVL >= @[code] 
 	 ORDER BY OFC_LVL 

#elseif (${methodname} == 'searchDatePeriod')

/*검색 조건에 맞는 DatePriod을 가져온다. - 입력파라메터의 경우의 수 1. Year, Month 2. Year, Week 3. Year, Month, Week : Week가 우선권을 가진다. 
															 4. Year, FromMonth, ToMonth 5. Year, FromWeek, ToWeek 6. Year, FromMonth, ToMonth, FromWeek, ToWeek*/
	#if(${gubun}=='1')

	/*searchDatePeriod : YEAR, MONTH, WEEK인경우*/
		SELECT TO_CHAR(TO_DATE(DECODE(FM_CHK,'Y',SLS_FM_DT, @[year]||@[month]||'01'),'YYYYMMDD'),'YYYY-MM-DD') || ' ~ '|| 
    	   					   DECODE(TO_CHK,'Y',TO_CHAR(TO_DATE(SLS_TO_DT,'YYYYMMDD'),'YYYY-MM-DD')
			  ,TO_CHAR(LAST_DAY(TO_DATE(SLS_FM_DT,'YYYYMMDD')),'YYYY-MM-DD')) RTN_DATE
		  FROM ( 
				SELECT SLS_FM_DT,SLS_TO_DT 
    	   			  ,DECODE(SUBSTR(SLS_FM_DT,1,6), @[year]||@[month],'Y','N') FM_CHK 
       	   			  ,DECODE(SUBSTR(SLS_TO_DT,1,6), @[year]||@[month],'Y','N') TO_CHK 
	  			  FROM  COA_WK_PRD 
			     WHERE COST_YR = @[year] 
  				   AND COST_WK = @[week] 
      	) 
	#elseif(${gubun}=='2')

	/*log.info('searchDatePeriod : YEAR, MONTH인경우')*/
		SELECT TO_CHAR(TO_DATE(@[year]||@[month],'YYYYMM'),'YYYY-MM-DD') || ' ~ ' || 
       	   	   TO_CHAR(LAST_DAY(TO_DATE(@[year]||@[month],'YYYYMM')),'YYYY-MM-DD') RTN_DATE
	 	  FROM DUAL 

	
	#elseif(${gubun}=='3')			

	/*log.info('searchDatePeriod : YEAR, WEEK인경우')*/
		SELECT TO_CHAR(TO_DATE(SLS_FM_DT, 'YYYYMMDD'),'YYYY-MM-DD') || ' ~ '|| 
       	       TO_CHAR(TO_DATE(SLS_TO_DT, 'YYYYMMDD'),'YYYY-MM-DD') RTN_DATE 
	 	  FROM  COA_WK_PRD 
	     WHERE COST_YR = @[year] 
  	       AND COST_WK = @[week]

	#elseif(${gubun}=='4')	

/*log.info('searchDatePeriod : year,from month,to_month,from_week,to_week인경우')*/
		SELECT TO_CHAR(TO_DATE(DECODE(SIGN(TO_NUMBER(MN_FM_DT) - TO_NUMBER(WK_TO_DT)),  1, '' 
                              ,DECODE(SIGN(TO_NUMBER(MN_TO_DT) - TO_NUMBER(WK_FM_DT)),  -1, '' 
                              ,DECODE(SIGN(TO_NUMBER(MN_FM_DT)-TO_NUMBER(WK_FM_DT)), 1, MN_FM_DT, WK_FM_DT) )), 'YYYYMMDD'),'YYYY-MM-DD')  || '~' || 
      		   TO_CHAR(TO_DATE(DECODE(SIGN(TO_NUMBER(MN_TO_DT) - TO_NUMBER(WK_FM_DT)), -1, '', 
                               DECODE(SIGN(TO_NUMBER(MN_FM_DT) - TO_NUMBER(WK_TO_DT)),  1, '', 
                               DECODE(SIGN(TO_NUMBER(MN_TO_DT)-TO_NUMBER(WK_TO_DT)), -1, MN_TO_DT, WK_TO_DT) )), 'YYYYMMDD'),'YYYY-MM-DD') RTN_DATE  
		 FROM ( 
     		   SELECT MAX(WK_FM_DT) WK_FM_DT
					 ,MAX(WK_TO_DT) WK_TO_DT
					 ,MAX(MN_FM_DT) MN_FM_DT
					 ,MAX(MN_TO_DT) MN_TO_DT 
     			 FROM ( 
          			   SELECT MIN(SLS_FM_DT) WK_FM_DT
							 ,MAX(SLS_TO_DT) WK_TO_DT
							 ,'' MN_FM_DT
							 ,'' MN_TO_DT 
          FROM  COA_WK_PRD 
          WHERE COST_YR = @[year] 
            AND COST_WK BETWEEN @[frmweek] AND @[toweek] 
          UNION ALL 
          SELECT '','',TO_CHAR(TO_DATE(@[year]||@[frmmonth],'YYYYMM'),'YYYYMMDD'), 
                  TO_CHAR(LAST_DAY(TO_DATE(@[year]||@[tomonth],'YYYYMM')),'YYYYMMDD') 
           FROM DUAL 
      ) 
 ) 

#elseif(${gubun}=='5')

/*log.debug('searchDatePeriod : year,from month,to month인경우')*/
	SELECT TO_CHAR(TO_DATE(@[year]||@[frmmonth],'YYYYMM'),'YYYY-MM-DD') || ' ~ ' ||        
       	   TO_CHAR(LAST_DAY(TO_DATE(@[year]||@[tomonth],'YYYYMM')),'YYYY-MM-DD') RTN_DATE 
	  FROM DUAL                                                           

#elseif(${gubun}=='6')

/*log.debug('searchDatePeriod : year,from week,to week인경우')*/
	SELECT MIN(TO_CHAR(TO_DATE(SLS_FM_DT, 'YYYYMMDD'),'YYYY-MM-DD')) ||' ~ ' || 
       	   MAX(TO_CHAR(TO_DATE(SLS_TO_DT, 'YYYYMMDD'),'YYYY-MM-DD')) RTN_DATE 
	  FROM COA_WK_PRD 
	 WHERE COST_YR = @[year] 
  	   AND COST_WK BETWEEN @[frmweek] AND @[toweek] 
#end




#elseif (${methodname} == 'searchFirstEtd')

/*VVD에 해당하는 etd date를 리턴한다*/

	SELECT TO_CHAR(MIN(VPS_ETD_DT),'YYYYMMDD') ETD_DT 
	  FROM VSK_VSL_PORT_SKD 
	 WHERE VSL_CD      = @[vsl_cd] 
  	   AND SKD_VOY_NO  = @[skd_voy_no] 
  	   AND SKD_DIR_CD  = @[skd_dir_cd]
  	   /*AND NVL(CNG_STS_CD,'*') <> 'S'*/ 

#elseif (${methodname} == 'searchKeyAcctInDvlList')	

/*Group ID = code인 Key Account List*/

	SELECT CUST_CNT_CD||CUST_SEQ CODE 
       	  ,CUST_LGL_ENG_NM NAME 
  	  FROM MDM_CUSTOMER 
 	 WHERE DELT_FLG     = 'N'  
   	   AND NEW_KEY_ACCT_FLG = 'Y' 
   	   AND (NMD_CUST_FLG IS NULL OR NMD_CUST_FLG = 'N') 
   	   	#if(${keyacctgrp} != '' && ${keyacctgrp} != 'All')
   	    	AND CUST_GRP_ID = @[keyacctgrp]
   	    #end 
 	 ORDER BY CUST_LGL_ENG_NM

#elseif (${methodname} == 'searchMltTrdInDvlList')	

/*Group ID = code인 M/A List*/

	SELECT CUST_CNT_CD||CUST_SEQ CODE 
       	  ,CUST_LGL_ENG_NM NAME 
  	  FROM MDM_CUSTOMER 
 	 WHERE DELT_FLG     = 'N'  
   	   AND GLO_ACCT_FLG = 'Y' 
   	   AND (NMD_CUST_FLG IS NULL OR NMD_CUST_FLG = 'N') 
   	   	#if(${mlttrdgrp} != '' && ${mlttrdgrp} != 'All')
   	    	AND CUST_GRP_ID = @[mlttrdgrp]
   	    #end 
 	 ORDER BY CUST_LGL_ENG_NM
#elseif (${methodname} == 'searchTotCoaCodeList')

/*COA CODE ComboList항목을 가져온다*/

	SELECT STND_COST_CD CODE
		  ,STND_COST_NM NAME 
	  FROM COA_STND_ACCT 
	 WHERE (MGRP_COST_CD in ('OV','OF') AND STND_COST_CD != '54600000' )
 	    OR (STND_COST_CD = '43102011' )
	 ORDER BY STND_COST_CD 

#elseif (${methodname} == 'searchChgOffice')

/*Parent가 HQ Level이고 해당 office가 HQ 직속이라면(ofc kind : 2) HQ의 level 부여됨  <br>
 특정조건에 만족하는 경우 office code에 변경하여 반환한다.
(HQ 산하의 조직이면서 Area가 아닌경우 HQ레벨의 office code를 리턴한다.)*/

	SELECT OFC_CD
		  ,PRNT_OFC_CD                               
      FROM MDM_ORGANIZATION     
     WHERE OFC_CD     = @[ofc_cd]      		
   	   AND OFC_KND_CD = '2'    
   	   AND OFC_TP_CD  <> 'AQ'
