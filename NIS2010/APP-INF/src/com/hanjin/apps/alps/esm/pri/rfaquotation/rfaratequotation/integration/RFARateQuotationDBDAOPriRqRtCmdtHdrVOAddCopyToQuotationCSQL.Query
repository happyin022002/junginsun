<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAOPriRqRtCmdtHdrVOAddCopyToQuotationCSQL">
			<desc><![CDATA[AddCopyToQuotation]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RQ_RT_CMDT_HDR (
	QTTN_NO
,	QTTN_VER_NO
,	CMDT_HDR_SEQ
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	FIC_RT_TP_CD
) 
SELECT  @[qttn_no]
,	@[qttn_ver_no]
,	CMDT_HDR_SEQ
,	SRC_INFO_CD
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	FIC_RT_TP_CD
FROM    PRI_RQ_RT_CMDT_HDR
WHERE   1=1
AND     QTTN_NO = @[qttn_no_from]
AND     QTTN_VER_NO = @[qttn_ver_no_from]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="qttn_no_from" type="12" value="" out="N"/>
				<param name="qttn_ver_no_from" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
