<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQBalanceDBDAOSearchEQBalance0016ListRSQL">
			<desc><![CDATA[ ]]></desc>
			<sql><![CDATA[
SELECT COST_YRMON
      ,CNTR_TPSZ_CD
      ,'' EQ_REPO_CR_LVL
      ,IMBAL_ALL_APLY_FLG
      ,0 IMBAL_FM_RTO
      ,0 IMBAL_TO_RTO
      ,OPB_ALL_APLY_FLG
      ,0 OPB_FM_AMT
      ,0 OPB_TO_AMT
      ,MB_ALL_APLY_FLG
      ,0 MB_FM_RTO
      ,0 MB_TO_RTO
      ,1 NUM
  FROM MAS_CNTR_REPO_RULE
WHERE COST_YRMON = @[f_cost_yrmon]
    #if (${f_cntr_tpsz_cd} != '') 
    AND CNTR_TPSZ_CD = REPLACE(@[f_cntr_tpsz_cd], 'RD', 'R')
    #end
    AND ROWNUM<2
UNION ALL
SELECT COST_YRMON
      ,CNTR_TPSZ_CD
	  ,EQ_REPO_CR_LVL
      ,EQ_REPO_CR_LVL
      ,DECODE(EQ_REPO_CR_LVL, 'X', IMBAL_FM_RTO, IMBAL_FM_RTO*100) IMBAL_FM_RTO
      ,DECODE(EQ_REPO_CR_LVL, 'X', IMBAL_TO_RTO, IMBAL_TO_RTO*100) IMBAL_TO_RTO
      ,EQ_REPO_CR_LVL
      ,OPB_FM_AMT
      ,OPB_TO_AMT
      ,EQ_REPO_CR_LVL
      ,(MB_FM_RTO*100) MB_FM_RTO
 	  ,(MB_TO_RTO*100) MB_TO_RTO
      ,ROWNUM+1 NUM
  FROM MAS_CNTR_REPO_RULE
WHERE COST_YRMON = @[f_cost_yrmon]            	
#if (${f_cntr_tpsz_cd} != '') 
AND CNTR_TPSZ_CD = REPLACE(@[f_cntr_tpsz_cd], 'RD', 'R')
#end			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
				<param name="f_cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
