<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOSearchAddOnCostTraiffListRSQL">
			<desc><![CDATA[Cost Table Interface - Add on (Ocean feeder)
2013.02.27 전윤주 creation date 컬럼에 upd_dt를 보여주고 있어 에러 수정
2013.03.04 [CHM-201323352] 전윤주 AOC 테이블의 delt_flg 확인 로직 추가
2014.12.09 [CHM-201433135] [Add-on/IHC Tariff > SIN] 국가코드 ZA 추가]]></desc>
			<sql><![CDATA[
WITH T AS
 (SELECT COST_TRF_NO,
         RHQ_CD,
         EFF_FM_DT,
         SVC_SCP_CD,
         ORG_DEST_CD,
         ROW_NO
    FROM (SELECT RANK() OVER(PARTITION BY A.RHQ_CD ORDER BY A.COST_TRF_NO DESC) RNK,
                 A.COST_TRF_NO,
                 A.RHQ_CD,
                 A.EFF_FM_DT,
                 B.SVC_SCP_CD,
                 B.ORG_DEST_CD,
                 (SELECT COUNT(X.COST_TRF_NO)
                    FROM #if(${rhq_cd} == 'HAMRU')    
                             AOC_EUR_FDR_TRF_DTL X 
                         #end 
                         #if(${rhq_cd} == 'SINRS')
                             AOC_CHN_FDR_TRF_DTL X
                         #end 
                         #if(${rhq_cd} == 'SHARC')
                             AOC_CHN_FDR_TRF_DTL X
                         #end 
                   WHERE X.COST_TRF_NO = A.COST_TRF_NO
                     AND X.PCTL_IO_BND_CD IN (DECODE(B.ORG_DEST_CD,'D','I','O','O'), 'B')
                     AND NVL(X.DELT_FLG, 'N') <> 'Y') ROW_NO                     
            FROM #if(${rhq_cd} == 'HAMRU')    
                     AOC_EUR_FDR_TRF_HDR  A, 
                 #end 
                 #if(${rhq_cd} == 'SINRS')
                     AOC_CHN_FDR_TRF_HDR  A,
                 #end 
                 #if(${rhq_cd} == 'SHARC')
                     AOC_CHN_FDR_TRF_HDR  A,
                 #end
                 (
                   SELECT DISTINCT SCP.SVC_SCP_CD
                                  , SCP.ORG_DEST_CD
                                  , RHQ.RHQ_CD
                        FROM MDM_SVC_SCP_LMT SCP, MDM_REGION RGN, MDM_SVC_SCP SVC, AOC_TRF_CURR AOC_CURR,
                            (
                        #if(${rhq_cd} == 'SHARC')
          /* SHARC */
                            SELECT 'SHARC' AS RHQ_CD, A.CONTI_CD , B.SCONTI_CD  , C.CNT_CD  , C.CNT_NM  
                            FROM   MDM_CONTINENT A, MDM_SUBCONTINENT B, MDM_COUNTRY C
                            WHERE  A.CONTI_CD = 'A'
                            AND    A.DELT_FLG = 'N'
                            AND    A.CONTI_CD = B.CONTI_CD
                            AND    B.SCONTI_CD = 'AF'
                            AND    B.DELT_FLG = 'N'
                            AND    B.SCONTI_CD = C.SCONTI_CD
                            AND    C.DELT_FLG = 'N'
                        #end 
                        #if(${rhq_cd} == 'SINRS')
          /* SINRS */
                            SELECT 'SINRS' AS RHQ_CD, A.CONTI_CD, B.SCONTI_CD, C.CNT_CD, C.CNT_NM
                            FROM   MDM_CONTINENT A, MDM_SUBCONTINENT B, MDM_COUNTRY C
                            WHERE  A.CONTI_CD IN ('A', 'F')
                            AND    A.DELT_FLG = 'N'
                            AND    A.CONTI_CD = B.CONTI_CD
                            AND    B.SCONTI_CD <> 'AF'
                            AND    B.DELT_FLG = 'N'
                            AND    B.SCONTI_CD = C.SCONTI_CD
                            AND    C.DELT_FLG = 'N'
                        #end
                        #if(${rhq_cd} == 'NYCRA')
          /* NYCRA */
                            SELECT 'NYCRA' AS RHQ_CD, A.CONTI_CD, B.SCONTI_CD, C.CNT_CD, C.CNT_NM
                            FROM   MDM_CONTINENT A, MDM_SUBCONTINENT B, MDM_COUNTRY C
                            WHERE  A.CONTI_CD = 'M'
                            AND    A.DELT_FLG = 'N'
                            AND    A.CONTI_CD = B.CONTI_CD
                            AND    B.DELT_FLG = 'N'
                            AND    B.SCONTI_CD = C.SCONTI_CD
                            AND    C.DELT_FLG = 'N'
                        #end
                        #if(${rhq_cd} == 'HAMRU')
          /* HAMRU */
                            SELECT 'HAMRU' AS RHQ_CD, A.CONTI_CD, B.SCONTI_CD, C.CNT_CD, C.CNT_NM
                            FROM   MDM_CONTINENT A, MDM_SUBCONTINENT B, MDM_COUNTRY C
                            WHERE  A.CONTI_CD IN ('E', 'F')
                            AND    A.DELT_FLG = 'N'
                            AND    A.CONTI_CD = B.CONTI_CD
                            AND    B.DELT_FLG = 'N'
                            AND    B.SCONTI_CD = C.SCONTI_CD
                            AND    C.DELT_FLG = 'N'
                        #end    
                            ) RHQ                                  
                        WHERE  SCP.DELT_FLG = 'N'
                        AND    SCP.RGN_CD   = RGN.RGN_CD
                        AND    RGN.DELT_FLG = 'N'
                        AND    RGN.CNT_CD   = RHQ.CNT_CD
                        AND    SCP.SVC_SCP_CD = SVC.SVC_SCP_CD
                        AND    RGN.CNT_CD = AOC_CURR.CNT_CD
                        AND    RHQ.RHQ_CD = AOC_CURR.RHQ_CD
                        AND    SCP.ORG_DEST_CD = @[org_dest_tp_cd]     
						AND NOT ( 
							(RHQ.RHQ_CD = 'HAMRU' AND SCP.ORG_DEST_CD = 'D' and  SVC.SVC_SCP_CD = 'AEE' and RHQ.CNT_CD = 'EG')
							OR (RHQ.RHQ_CD = 'HAMRU' AND SCP.ORG_DEST_CD = 'D' and  SVC.SVC_SCP_CD = 'AEF' and RHQ.CNT_CD = 'EG')
							OR (RHQ.RHQ_CD = 'HAMRU' AND SCP.ORG_DEST_CD = 'D' and  SVC.SVC_SCP_CD = 'AEE' and RHQ.CNT_CD = 'EG')
							OR (RHQ.RHQ_CD = 'HAMRU' AND SCP.ORG_DEST_CD = 'D' and  SVC.SVC_SCP_CD = 'IAA' and RHQ.CNT_CD = 'EG')
							OR (RHQ.RHQ_CD = 'HAMRU' AND SCP.ORG_DEST_CD = 'O' and  SVC.SVC_SCP_CD = 'IAA' and RHQ.CNT_CD = 'EG')
							OR (RHQ.RHQ_CD = 'HAMRU' AND SCP.ORG_DEST_CD = 'D' and  SVC.SVC_SCP_CD = 'FES' and RHQ.CNT_CD = 'EG')
							OR (RHQ.RHQ_CD = 'HAMRU' AND SCP.ORG_DEST_CD = 'O' and  SVC.SVC_SCP_CD = 'FEN' and RHQ.CNT_CD = 'EG')
						)
                 ) B
           WHERE A.RHQ_CD = B.RHQ_CD
                 AND A.COST_TRF_STS_CD = 'C'

#if(${svc_scp_cd} != '')
             	 AND B.SVC_SCP_CD = @[svc_scp_cd]
#end  
			)
   WHERE RNK = 1)
SELECT T.COST_TRF_NO,
       T.RHQ_CD,
       TO_CHAR(T.EFF_FM_DT, 'YYYY-MM-DD') EFF_FM_DT,
       T.SVC_SCP_CD,
       T.ROW_NO,
       T1.FDR_TRF_NO,
       T1.AMDT_SEQ,
       T1.EFF_DT,
       TO_CHAR(T1.CRE_DT, 'YYYY-MM-DD') CRE_DT,
       T1.FIC_PROP_STS_CD,
       T1.FIC_PROP_STS_CD_NM,
       T1.FDR_COST_TRF_NO ADD_FDR_COST_TRF_NO,
       T.ORG_DEST_CD AS ORG_DEST_TP_CD
  FROM T,
       (SELECT A1.SVC_SCP_CD,
               A1.ORG_DEST_TP_CD,
               A1.FDR_TRF_NO,
               A1.AMDT_SEQ,
               B1.EFF_DT,
               B1.CRE_DT,
               B1.FIC_PROP_STS_CD,
               D1.INTG_CD_VAL_DP_DESC FIC_PROP_STS_CD_NM,
               C1.RHQ_CD,
               C1.FDR_COST_TRF_NO
          FROM (SELECT T2.SVC_SCP_CD,
                       T2.FDR_TRF_NO,
                       T2.ORG_DEST_TP_CD,
                       MAX(T2.AMDT_SEQ) AMDT_SEQ
                  FROM (SELECT SVC_SCP_CD,
                               ORG_DEST_TP_CD,
                               RHQ_CD,
                               MAX(FDR_TRF_NO) FDR_TRF_NO
                          FROM PRI_TRF_FDR_COST_VER_MAPG
                         GROUP BY SVC_SCP_CD,
                                  ORG_DEST_TP_CD,
                                  RHQ_CD) T1,
                       PRI_TRF_FDR_MN T2
                 WHERE T1.SVC_SCP_CD = T2.SVC_SCP_CD
                   AND T1.FDR_TRF_NO = T2.FDR_TRF_NO
                   AND T1.ORG_DEST_TP_CD = T2.ORG_DEST_TP_CD
                 GROUP BY T2.SVC_SCP_CD,
                          T2.FDR_TRF_NO,
                          T2.ORG_DEST_TP_CD) A1,
               PRI_TRF_FDR_MN B1,
               (SELECT SVC_SCP_CD,
                       ORG_DEST_TP_CD,
                       FDR_TRF_NO,
                       RHQ_CD,
                       FDR_COST_TRF_NO
                  FROM PRI_TRF_FDR_COST_VER_MAPG
                 GROUP BY SVC_SCP_CD,
                          ORG_DEST_TP_CD,
                          FDR_TRF_NO,
                          RHQ_CD,
                          FDR_COST_TRF_NO) C1,  
               COM_INTG_CD_DTL D1
         WHERE A1.SVC_SCP_CD = B1.SVC_SCP_CD
               AND A1.FDR_TRF_NO = B1.FDR_TRF_NO
               AND A1.AMDT_SEQ = B1.AMDT_SEQ
               AND A1.ORG_DEST_TP_CD = B1.ORG_DEST_TP_CD
               AND A1.SVC_SCP_CD = C1.SVC_SCP_CD
               AND A1.FDR_TRF_NO = C1.FDR_TRF_NO
               AND A1.ORG_DEST_TP_CD = C1.ORG_DEST_TP_CD
               AND D1.INTG_CD_ID(+) = 'CD03045'
               AND B1.FIC_PROP_STS_CD = D1.INTG_CD_VAL_CTNT(+)
               AND A1.ORG_DEST_TP_CD = @[org_dest_tp_cd]  
               ) T1
 WHERE T.SVC_SCP_CD = T1.SVC_SCP_CD(+)
       AND T.RHQ_CD = T1.RHQ_CD(+)
	   AND T.ORG_DEST_CD = T1.ORG_DEST_TP_CD(+)
 ORDER BY T.SVC_SCP_CD,
          T.COST_TRF_NO			]]></sql>
			<params>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
