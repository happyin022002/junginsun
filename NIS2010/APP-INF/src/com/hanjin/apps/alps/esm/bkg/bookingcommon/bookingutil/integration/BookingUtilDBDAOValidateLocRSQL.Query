<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOValidateLocRSQL">
			<desc><![CDATA[Location Validation]]></desc>
			<sql><![CDATA[
SELECT loc_cd
FROM   mdm_location
WHERE  delt_flg = 'N'
AND    loc_cd = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
