<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDAOSearchChinaAgentCodeRSQL">
			<desc><![CDATA[중국 Booking Agent 정보 등록 화면]]></desc>
			<sql><![CDATA[
SELECT 
	CHN_AGN_CD
,	AGN_NM
,	DIFF_RMK
,	VNDR_CNT_CD
,	VNDR_SEQ
,   VNDR_CNT_CD || VNDR_SEQ AS ORIGIN_VNDR
,	CUST_CNT_CD
,	CUST_SEQ
,   CUST_CNT_CD || CUST_SEQ AS ORIGIN_CUST
,	AUTO_DP_CHK_FLG
,	AGN_EML
,	OFC_CD
,	FINC_OFC_CD
,	AR_OFC_CD
,	DIR_PAY_OFC_CD
,	CRE_USR_ID
,	TO_CHAR(CRE_DT,'YYYY-MM-DD') CRE_DT
,	UPD_USR_ID
,	TO_CHAR(UPD_DT,'YYYY-MM-DD') UPD_DT
,	(SELECT C.OFC_CD  FROM   COM_USER C WHERE  C.USR_ID = A.UPD_USR_ID AND ROWNUM=1) UPD_OFC_CD
,   BKG_BLCK_FLG
FROM BKG_CHN_AGN A
WHERE	1 =1
AND  DELT_FLG  = 'N'
#if (${chn_agn_cd} != '') 
AND	CHN_AGN_CD = @[chn_agn_cd]
#end
#if (${ofc_cd} != '') 
AND	FINC_OFC_CD = @[ofc_cd]
#end
#if (${cust_cnt_cd} != '') 
AND	CUST_CNT_CD = @[cust_cnt_cd]
#end
#if (${cust_seq} != '') 
AND	CUST_SEQ = @[cust_seq]
#end
#if (${agn_nm} != '') 
AND	AGN_NM LIKE '%' ||  @[agn_nm] || '%'
#end
ORDER BY OFC_CD,CHN_AGN_CD
,	CUST_CNT_CD
,	CUST_SEQ			]]></sql>
			<params>
				<param name="chn_agn_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="agn_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
