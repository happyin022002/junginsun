<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOremoveDiscCYCodeListDSQL">
			<desc><![CDATA[Discharging Code List 삭제]]></desc>
			<sql><![CDATA[
DELETE 
  FROM BKG_DCHG_LOC
 WHERE OTR_DCHG_CD = @[otr_dchg_cd]
   AND PORT_CD = @[port_cd]
   AND SLAN_CD = @[slan_cd]
   AND SKD_DIR_CD = @[skd_dir_cd]
   AND LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="otr_dchg_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
