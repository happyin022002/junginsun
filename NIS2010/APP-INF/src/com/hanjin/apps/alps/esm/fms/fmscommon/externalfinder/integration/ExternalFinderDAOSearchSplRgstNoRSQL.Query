		<query name="ExternalFinderDAOSearchSplRgstNoRSQL">
			<desc><![CDATA[ExternalFinderDAOSearchSplRgstNoRSQL]]></desc>
			<sql><![CDATA[
SELECT VNDR_LOCL_LANG_NM, 
       CEO_NM, 
       BZCT_NM, 
       BZTP_NM,
       LOCL_LANG_ADDR,
       VNDR_SEQ
  FROM MDM_VENDOR 
 WHERE RGST_NO = @[rgst_no]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="rgst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
