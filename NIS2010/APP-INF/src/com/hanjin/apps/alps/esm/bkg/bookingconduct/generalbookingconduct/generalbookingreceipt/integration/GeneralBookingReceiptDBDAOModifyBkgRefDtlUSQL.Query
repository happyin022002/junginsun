<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModifyBkgRefDtlUSQL">
			<desc><![CDATA[bkg_ref_dtl을 update한다.]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y') 
UPDATE BKG_REF_DTL_HIS
#else
UPDATE BKG_REF_DTL
#end
SET    
       DE_NO          = @[de_no],
       SHP_REF_NO     = @[shp_ref_no],
       PRT_NO         = @[prt_no],
       CNTR_NO        = @[cntr_no],
       ITM_NO         = @[itm_no],
       ITM_DESC       = @[itm_desc],
       PCK_QTY        = @[pck_qty],
       PCK_TP_CD      = @[pck_tp_cd],
       CNTR_WGT       = @[cntr_wgt],
       WGT_UT_CD      = @[wgt_ut_cd],
       MEAS_QTY       = @[meas_qty],
       MEAS_UT_CD     = @[meas_ut_cd],
       PO_NO          = @[po_no],
       UPD_USR_ID     = @[upd_usr_id],
       UPD_DT         = sysdate
WHERE  BKG_NO         = @[bkg_no]
AND    REF_SEQ        = @[ref_seq]
#if (${ca_flg}== 'Y') 
AND	   CORR_NO		  = 'TMP0000001'
#end			]]></sql>
			<params>
				<param name="de_no" type="12" value="" out="N"/>
				<param name="shp_ref_no" type="12" value="" out="N"/>
				<param name="prt_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="itm_no" type="12" value="" out="N"/>
				<param name="itm_desc" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="meas_ut_cd" type="12" value="" out="N"/>
				<param name="po_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ref_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
