<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtScgCpGRICancelCSQL">
			<desc><![CDATA[GRI Cancel시 PriSpScpRtScg 테이블 데이터 복구]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_RT_SCG
  (PROP_NO
  ,AMDT_SEQ
  ,SVC_SCP_CD
  ,CMDT_HDR_SEQ
  ,ROUT_SEQ
  ,RT_SEQ
  ,CHG_CD
  ,BKG_RAT_UT_CD
  ,CURR_CD
  ,TRF_SCG_AMT
  ,ADJ_SCG_AMT
  ,ADJ_SCG_USD_AMT
  ,TRF_SCG_RMK
  ,TRF_ADJ_TP_CD
  ,ADJ_FLG
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT)
  SELECT PROP_NO
        ,AMDT_SEQ + 1
        ,SVC_SCP_CD
        ,CMDT_HDR_SEQ
        ,ROUT_SEQ
        ,RT_SEQ
        ,CHG_CD
        ,BKG_RAT_UT_CD
        ,CURR_CD
        ,TRF_SCG_AMT
        ,ADJ_SCG_AMT
        ,ADJ_SCG_USD_AMT
        ,TRF_SCG_RMK
        ,TRF_ADJ_TP_CD
        ,ADJ_FLG
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[cre_usr_id]
        ,SYSDATE
    FROM PRI_RP_SCP_RT_SCG T
   WHERE PROP_NO = @[prop_no]
     AND AMDT_SEQ = @[amdt_seq] - 1
     AND SVC_SCP_CD = @[svc_scp_cd]
     AND EXISTS (SELECT 'OK'
                   FROM PRI_RP_SCP_RT A
                  WHERE A.PROP_NO = @[prop_no]
                    AND A.AMDT_SEQ = @[amdt_seq]
                    AND A.SVC_SCP_CD = @[svc_scp_cd]
                    AND A.SRC_INFO_CD <> 'AD'
                    AND A.GRI_APPL_TP_CD = 'A'
                    AND A.CMDT_HDR_SEQ = T.CMDT_HDR_SEQ
                    AND A.ROUT_SEQ = T.ROUT_SEQ
                    AND A.RT_SEQ = T.RT_SEQ)			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
