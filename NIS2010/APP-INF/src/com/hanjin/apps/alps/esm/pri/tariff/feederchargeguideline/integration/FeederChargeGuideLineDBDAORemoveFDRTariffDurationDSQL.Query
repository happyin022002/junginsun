<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAORemoveFDRTariffDurationDSQL">
			<desc><![CDATA[Remove FDR Tariff Duration]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_TRF_FDR_DUR
 WHERE SVC_SCP_CD = @[svc_scp_cd] 
   AND FDR_TRF_NO = @[fdr_trf_no] 
   AND AMDT_SEQ   = @[amdt_seq]
   AND ORG_DEST_TP_CD = @[org_dest_tp_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
