<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOReactBkgStatusUSQL">
			<desc><![CDATA[Booking Status 를 'F'로 변경한다]]></desc>
			<sql><![CDATA[
UPDATE BKG_BOOKING
   SET BKG_STS_CD = 'F'
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
