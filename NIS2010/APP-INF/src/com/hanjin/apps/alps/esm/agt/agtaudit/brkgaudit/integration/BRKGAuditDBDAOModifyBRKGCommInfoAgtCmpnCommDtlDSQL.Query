<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BRKGAuditDBDAOModifyBRKGCommInfoAgtCmpnCommDtlDSQL">
			<desc><![CDATA[ModifyBRKGCommInfoAgtCmpnCommDtl]]></desc>
			<sql><![CDATA[
