<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterStandardPrimeCostDAOFmsMktRtUSQL">
			<desc><![CDATA[Hire Base Input Update]]></desc>
			<sql><![CDATA[
update fms_mkt_rt set 
	rng_fm_qty = @[rng_fm_qty],
	rng_to_qty = @[rng_to_qty],
	bld_tp_nm = @[bld_tp_nm],
	mkt_rt_amt = @[mkt_rt_amt],
	upd_usr_id = @[upd_usr_id]
where	mkt_rt_yrmon = @[mkt_rt_yrmon]
and	mkt_rt_seq = @[mkt_rt_seq]			]]></sql>
			<params>
				<param name="rng_fm_qty" type="2" value="" out="N"/>
				<param name="rng_to_qty" type="2" value="" out="N"/>
				<param name="bld_tp_nm" type="12" value="" out="N"/>
				<param name="mkt_rt_amt" type="3" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mkt_rt_yrmon" type="12" value="" out="N"/>
				<param name="mkt_rt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
