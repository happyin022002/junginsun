<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOmodifyKcdTpInCntrUSQL">
			<desc><![CDATA[T/S EMPTY 로 인하여 BKG NO단위로 BKG_CSTMS_KR_CNTR의 CSTMS_DECL_TP_CD를 맞춰주기 위함, 그러나 해당 BL_NO의 CNTR수정은 가능하게..]]></desc>
			<sql><![CDATA[
UPDATE  BKG_CSTMS_KR_CNTR
SET CSTMS_DECL_TP_CD  = @[cstms_decl_tp_cd]
WHERE   BKG_NO        = @[bkg_no]
AND CSTMS_DECL_TP_CD  = @[old_cstms_decl_tp_cd]
AND DMST_PORT_CD      = @[port_cd]
AND TRNS_SEQ      	  = @[trns_seq]
AND CSTMS_BL_NO		<>  @[bl_no]
AND CSTMS_BL_NO IN (
    SELECT CSTMS_BL_NO
    FROM BKG_CSTMS_KR_BL
    WHERE BKG_NO          =   @[bkg_no]
    AND CSTMS_DECL_TP_CD  =   @[cstms_decl_tp_cd]
    AND DMST_PORT_CD      =   @[port_cd]
    AND TRNS_SEQ          =   @[trns_seq]
    AND NVL(IB_VSL_CD||IB_SKD_VOY_NO||IB_SKD_DIR_CD, ' ')    =   (   SELECT  NVL(IB_VSL_CD||IB_SKD_VOY_NO||IB_SKD_DIR_CD, ' ')
    FROM    BKG_CSTMS_KR_BL
    WHERE   BKG_NO          =   @[bkg_no]
    AND CSTMS_DECL_TP_CD    =   @[cstms_decl_tp_cd]
    AND DMST_PORT_CD        =   @[port_cd]
    AND TRNS_SEQ            =   @[trns_seq]
    AND CSTMS_BL_NO         =   @[bl_no])
)			]]></sql>
			<params>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="old_cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="trns_seq" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
