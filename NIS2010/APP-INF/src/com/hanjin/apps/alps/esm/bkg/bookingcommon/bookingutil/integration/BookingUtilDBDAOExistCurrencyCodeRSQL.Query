<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOExistCurrencyCodeRSQL">
			<desc><![CDATA[ExistCurrencyCode 쿼리]]></desc>
			<sql><![CDATA[
SELECT 
    CASE 
        WHEN COUNT(*)>0 THEN 'Y'
        ELSE 'N'
    END OUTPUT_TEXT
FROM MDM_CURRENCY
WHERE CURR_CD = @[input_text]
 AND DELT_FLG='N'			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
