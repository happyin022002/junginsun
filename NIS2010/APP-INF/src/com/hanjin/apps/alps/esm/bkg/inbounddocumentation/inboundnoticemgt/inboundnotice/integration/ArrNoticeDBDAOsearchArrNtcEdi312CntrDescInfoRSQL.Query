<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrNoticeDBDAOsearchArrNtcEdi312CntrDescInfoRSQL">
			<desc><![CDATA[searchArrNtcEdi312CntrDescInfo]]></desc>
			<sql><![CDATA[
SELECT '{CNTR_DESC' || CHR(10) 
       || 'D_CMDT:' || CHR(10) 
       || 'D_PUNIT:' || PCK_TP_CD || CHR(10) 
       || 'D_PKG:' || PCK_QTY || CHR(10) 
       || 'D_WGT:' || CNTR_MF_WGT || CHR(10) 
       || 'D_MEAS:' || MEAS_QTY || CHR(10) 
       || 'D_DESC:' || REPLACE(TO_CLOB(CNTR_MF_GDS_DESC), CHR(13)||CHR(10), ' ') || CHR(10)
       || '}CNTR_DESC' || CHR(10) cntr_desc_info
  FROM BKG_CNTR_MF_DESC
WHERE BKG_NO = @[bkg_no]
AND   CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
