<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOAddQueueDetailReturnRsnCSQL">
			<desc><![CDATA[2011.03 김기종 [CHM-201109394-01] DPCS고도화]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_SR_RTN_RSN (
	SR_KND_CD
,	SR_NO
,	BKG_NO
,	SR_HIS_SEQ
,	SR_RTN_RSN_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[sr_knd_cd]
,	@[sr_no]
,	@[bkg_no]
,	(SELECT	MAX(SR_HIS_SEQ) FROM BKG_SR_HIS
	WHERE SR_KND_CD = @[sr_knd_cd]
	AND  SR_NO    = @[sr_no]
	AND  BKG_NO   = @[bkg_no]   
     )
,	@[code]  --SR_RTN_RSN_CD
,	@[upd_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="code" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
