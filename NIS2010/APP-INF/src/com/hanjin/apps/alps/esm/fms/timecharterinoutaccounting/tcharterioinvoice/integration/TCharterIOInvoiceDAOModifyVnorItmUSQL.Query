<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOModifyVnorItmUSQL">
			<desc><![CDATA[FMS VNOR ITEM 수정]]></desc>
			<sql><![CDATA[
UPDATE FMS_VNOR_ITM
SET VNOR_ITM_PROC_CD = @[vnor_itm_proc_cd]
    ,VNOR_ITM_FLET_RMK = @[vnor_itm_flet_rmk]
    ,UPD_USR_ID = @[cre_usr_id]
    ,UPD_DT = SYSDATE
WHERE 1=1
AND VSL_CD = @[vsl_cd]
AND VNOR_SEQ = @[vnor_seq] 
AND VNOR_ITM_SEQ = @[vnor_itm_seq] 			]]></sql>
			<params>
				<param name="vnor_itm_proc_cd" type="12" value="" out="N"/>
				<param name="vnor_itm_flet_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vnor_seq" type="12" value="" out="N"/>
				<param name="vnor_itm_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
