<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchImpCgoManiPrtCstmDescRSQL">
			<desc><![CDATA[Import Cargo Manifest Print를 위한 Description of Goods Information 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(CSTMS_DESC,' ') CSTMS_DESC
  FROM BKG_BL_DOC
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
