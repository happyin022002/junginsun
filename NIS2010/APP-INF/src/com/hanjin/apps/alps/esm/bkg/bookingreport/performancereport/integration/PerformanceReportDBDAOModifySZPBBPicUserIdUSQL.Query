<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOModifySZPBBPicUserIdUSQL">
			<desc><![CDATA[Queue List 에 Input/Rate PIC 정보를 수정한다]]></desc>
			<sql><![CDATA[
UPDATE BKG_SR_CRNT_RQST
SET INP_PIC_USR_ID = @[inp_pic_usr_id],
    RT_PIC_USR_ID = @[rt_pic_usr_id],
    UPD_USR_ID = @[usr_id],
    UPD_DT = SYSDATE
WHERE SR_KND_CD = @[sr_knd_cd]
AND SR_NO = @[sr_no]
AND BKG_NO = @[bkg_no]
AND SR_AMD_TP_CD = @[sr_amd_tp_cd]
AND SR_AMD_SEQ = @[act_sr_amd_seq]			]]></sql>
			<params>
				<param name="inp_pic_usr_id" type="12" value="" out="N"/>
				<param name="rt_pic_usr_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sr_amd_tp_cd" type="12" value="" out="N"/>
				<param name="act_sr_amd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
