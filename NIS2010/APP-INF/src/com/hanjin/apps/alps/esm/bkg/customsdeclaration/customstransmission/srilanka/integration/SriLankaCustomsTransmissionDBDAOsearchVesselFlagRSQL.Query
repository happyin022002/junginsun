<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SriLankaCustomsTransmissionDBDAOsearchVesselFlagRSQL">
			<desc><![CDATA[Vessel 등록 포트의 국가 설명]]></desc>
			<sql><![CDATA[
SELECT VSL_RGST_CNT_CD AS VSL_CNT_CD
  FROM MDM_VSL_CNTR
 WHERE VSL_CD =  @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
