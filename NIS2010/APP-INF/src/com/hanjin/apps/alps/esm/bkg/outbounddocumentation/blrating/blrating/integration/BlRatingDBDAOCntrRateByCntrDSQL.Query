<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCntrRateByCntrDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CNTR_RT
WHERE	BKG_NO = @[bkg_no]
AND	CNTR_NO = @[cntr_no]
#if (${cntr_rt_seq} != '')
AND	CNTR_RT_SEQ = @[cntr_rt_seq]
#end

			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_rt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
