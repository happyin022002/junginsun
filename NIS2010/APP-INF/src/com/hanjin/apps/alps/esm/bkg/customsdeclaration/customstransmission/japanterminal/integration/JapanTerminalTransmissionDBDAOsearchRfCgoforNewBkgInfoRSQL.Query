<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchRfCgoforNewBkgInfoRSQL">
			<desc><![CDATA[searchRfCgoforNewBkgInfo]]></desc>
			<sql><![CDATA[
SELECT RC.BKG_NO ,
  @[bkg_skd_seq] BKG_SKD_SEQ ,
  RC.RC_SEQ ,
  RC.CNTR_TPSZ_CD ,
  RC.CNTR_VOL_QTY ,
  RC.CNTR_NO ,
  RC.CLNG_TP_CD ,
  RC.FDO_TEMP ,
  RC.CDO_TEMP ,
  RC.CNTR_VENT_TP_CD ,
  RC.VENT_RTO ,
  RC.HUMID_NO ,
  (
    SELECT COM.INTG_CD_VAL_DP_DESC
    FROM COM_INTG_CD_DTL COM
    WHERE COM.INTG_CD_ID = 'CD03004'
      AND COM.INTG_CD_VAL_CTNT=RC.VENT_RTO) SNACCS_TML_EDI_VENT_RTO_CD
FROM BKG_RF_CGO RC
WHERE RC.BKG_NO LIKE SUBSTR(@[bkg_no], 1, 10)||'%'			]]></sql>
			<params>
				<param name="bkg_skd_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
