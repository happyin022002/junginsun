<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltFicRateByRouteRSQL">
			<desc><![CDATA[ FIC Rate값을 Route 정보를 이용해  조회한다.]]></desc>
			<sql><![CDATA[
SELECT  REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 1)  AS FIC_RT_USE_STS_CD
	,REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 2) AS OPTM_TRSP_MOD_FLG
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 3)) AS DR_20FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 4)) AS RF_20FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 5)) AS DG_20FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 6)) AS DR_40FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 7)) AS RF_40FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 8)) AS DG_40FT_AMT
	,REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 9) AS LOCL_CURR_CD
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 10)) AS DR_LOCL_20FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 11)) AS RF_LOCL_20FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 12)) AS DG_LOCL_20FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 13)) AS DR_LOCL_40FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 14)) AS RF_LOCL_40FT_AMT
	,TRIM(REGEXP_SUBSTR( FIC_RATE_RESULT, '[^|]+', 1, 15)) AS DG_LOCL_40FT_AMT
FROM (
    SELECT ((
				SELECT 
#if (${add_on_flag} == 'Y' )
	PRI_ADDON_RATE_CALCULATE_PKG.PRI_getFICRateByRoute_FNC(@[prop_no],@[amdt_seq], @[svc_scp_cd], @[in_out_bound],@[cmdt_hdr_seq],@[rout_seq]) AS  FIC_RATE_RESULT
#else
	PRI_FIC_RATE_CALCULATE_PKG.PRI_getFICRateByRoute_FNC(@[prop_no],@[amdt_seq], @[svc_scp_cd], @[cmdt_hdr_seq],@[rout_seq]) AS  FIC_RATE_RESULT
#end
				FROM DUAL
            )) AS FIC_RATE_RESULT
    FROM DUAL
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="in_out_bound" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
