<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchPortListRSQL">
			<desc><![CDATA[ROCS(ROTTERDAM) 세관에 신고할 대상 POL 별 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT SKD.VPS_PORT_CD
       , TO_CHAR(SKD.VPS_ETD_DT,'YYYY-MM-DD hh24:mm') VPS_ETD_DT
       , 'NLRTM' POD
	   , BDR.POD_CLPT_IND_SEQ
       , TRNK_BDR_FLG
       , TO_CHAR(NVL(BDR.TRNK_AUTO_BDR_DT, BDR.TRNK_MNL_BDR_DT),'YYYY-MM-DD hh24:mm') TRNK_AUTO_BDR_DT
       , SUM(DECODE(VVD.BKG_NO,null,0,1)) INCL_COUNT
       , SUM(DECODE(DECODE(VVD.BDR_FLG,'Y',1,0),0,0,1)) EXCL_COUNT
  FROM VSK_VSL_PORT_SKD SKD
       , BKG_VVD_BDR_LOG BDR
       , (SELECT VVD.VSL_CD, VVD.SKD_VOY_NO, VVD.SKD_DIR_CD, VVD.POL_CD, BKG.BKG_NO,DOC.BDR_FLG
            FROM BKG_BOOKING BKG, BKG_VVD VVD, BKG_BL_DOC DOC
           WHERE VVD.VSL_CD = @[vsl_cd]
             AND VVD.SKD_VOY_NO = @[skd_voy_no]
             AND VVD.SKD_DIR_CD = @[skd_dir_cd]
			 #if(${pod_clpt_ind_seq}!='')
             AND VVD.POD_CLPT_IND_SEQ = NVL(@[pod_clpt_ind_seq], '1')
			 #elseif(${frm_pod_clpt_ind_seq} !='')
			 AND VVD.POD_CLPT_IND_SEQ = NVL(@[frm_pod_clpt_ind_seq], '1')
			 #end
             --AND (bkg.POD_CD = 'NLRTM' or bkg.PST_RLY_PORT_CD = 'NLRTM')
             AND BKG.BKG_STS_CD <> 'X'
             AND BKG.BKG_CGO_TP_CD IN ('F','B','R','P')
             #if (${mt_flag}!= '' && ${mt_flag} == '') 
             AND DECODE(@[mt_flag],'P',BKG_CGO_TP_CD,'A') = DECODE(@[mt_flag],'P','P','A')
             AND DECODE(@[mt_flag],'F',BKG_CGO_TP_CD,'A') <> 'P'
             #end
             AND BKG.BKG_NO = DOC.BKG_NO
             AND DOC.BDR_FLG = DECODE(BKG_CGO_TP_CD,'F','Y','R','Y','B','Y',BDR_FLG)
             AND BKG.BKG_NO = VVD.BKG_NO
             AND VVD.POD_CD = 'NLRTM') VVD
 WHERE SKD.VSL_CD = @[vsl_cd]
	 AND SKD.SKD_VOY_NO = @[skd_voy_no]
	 AND SKD.SKD_DIR_CD = @[skd_dir_cd]
	 AND SKD.CLPT_IND_SEQ = '1'
	 AND NVL(SKD.SKD_CNG_STS_CD, 'XX') != 'S'
	 AND SKD.CLPT_SEQ < (SELECT MAX(CLPT_SEQ)
	                       FROM VSK_VSL_PORT_SKD
                        WHERE VSL_CD = @[vsl_cd]
                          AND SKD_VOY_NO = @[skd_voy_no]
                          AND SKD_DIR_CD = @[skd_dir_cd]
                          AND VPS_PORT_CD = 'NLRTM'
                          #if(${pod_clpt_ind_seq} != '')
						  AND CLPT_IND_SEQ =  NVL(@[pod_clpt_ind_seq],'1')
						  #elseif(${frm_pod_clpt_ind_seq} !='')
						  AND CLPT_IND_SEQ =  NVL(@[frm_pod_clpt_ind_seq],'1')
						  #end
                          AND NVL(SKD_CNG_STS_CD, 'XX') != 'S'
                       )
   AND SKD.VSL_CD = BDR.VSL_CD
   AND SKD.SKD_VOY_NO = BDR.SKD_VOY_NO
   AND SKD.SKD_DIR_CD = BDR.SKD_DIR_CD
   AND SKD.VPS_PORT_CD = BDR.POL_CD
   AND BDR.POD_CD = 'NLRTM'
   #if(${pod_clpt_ind_seq} != '')
   AND BDR.POD_CLPT_IND_SEQ = NVL(@[pod_clpt_ind_seq],'1')
   #elseif(${frm_pod_clpt_ind_seq} !='')
   AND BDR.POD_CLPT_IND_SEQ = NVL(@[frm_pod_clpt_ind_seq],'1')
   #end					  
   AND SKD.VSL_CD = VVD.VSL_CD(+)
   AND SKD.SKD_VOY_NO = VVD.SKD_VOY_NO(+)
   AND SKD.SKD_DIR_CD = VVD.SKD_DIR_CD(+)
   AND SKD.VPS_PORT_CD = VVD.POL_CD(+)
GROUP BY VPS_PORT_CD, TO_CHAR(VPS_ETD_DT,'YYYY-MM-DD hh24:mm'), BDR.POD_CLPT_IND_SEQ, TRNK_BDR_FLG,
					 TO_CHAR(NVL(bdr.TRNK_AUTO_BDR_DT, bdr.TRNK_MNL_BDR_DT),'YYYY-MM-DD hh24:mm')
ORDER BY VPS_ETD_DT			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value=" " out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="frm_pod_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="mt_flag" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
