<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAORsltHisScpVORSQL">
			<desc><![CDATA[RFAProposalMainDBDAO]]></desc>
			<sql><![CDATA[
SELECT SCPMN.SVC_SCP_CD CD
      ,MDM.SVC_SCP_NM NM
FROM   (SELECT DISTINCT SVC_SCP_CD
        FROM            PRI_RP_SCP_MN
        WHERE           PROP_NO = @[prop_no]) SCPMN
      ,MDM_SVC_SCP MDM
WHERE  SCPMN.SVC_SCP_CD = MDM.SVC_SCP_CD
AND    MDM.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
