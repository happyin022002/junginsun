				<param name="usrId" type="12" value="" out="N"/>
				<param name="hamo_tp_cd" type="12" value="" out="N"/>
				<param name="hamo_trf_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
