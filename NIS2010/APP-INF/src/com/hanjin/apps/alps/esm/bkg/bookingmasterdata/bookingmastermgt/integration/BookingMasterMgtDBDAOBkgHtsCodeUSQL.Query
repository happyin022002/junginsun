		<query name="BookingMasterMgtDBDAOBkgHtsCodeUSQL">
			<desc><![CDATA[code 업데이트 쿼리]]></desc>
			<sql><![CDATA[
UPDATE bkg_hamo_trf 
SET
    delt_flg = 'Y'
,	upd_usr_id = @[usrId] 
,	upd_dt = sysdate

WHERE hamo_tp_cd = @[hamo_tp_cd]
AND hamo_trf_cd = @[hamo_trf_cd]			]]></sql>
