<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOAverageUCVesselDetailAvgUSQL">
			<desc><![CDATA[AverageUCVesselDetailAvg]]></desc>
			<sql><![CDATA[
UPDATE MAS_POOL_VSL_DLY_HIR A 
   SET A.DHIR_AMT = (
                     SELECT AVG(DHIR_AMT)
                       FROM MAS_POOL_VSL_DLY_HIR
                      WHERE DHIR_AMT != 0
                        AND VSL_CD       != A.VSL_CD
                        AND COST_YRMON    = A.COST_YRMON
                        AND STND_COST_CD  = A.STND_COST_CD
                        AND VSL_CLSS_CAPA = A.VSL_CLSS_CAPA
                    )
     , A.UPD_USR_ID = @[upd_usr_id]
     , A.UPD_DT     = SYSDATE
 WHERE A.COST_YRMON    = @[cost_yrmon]
   AND A.STND_COST_CD  = @[stnd_cost_cd] 
   AND A.VSL_CLSS_CAPA = @[vsl_clss_capa]
   AND A.VSL_CD        = 'XXXX'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_capa" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
