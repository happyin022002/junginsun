<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchBkgTermRSQL">
			<desc><![CDATA[BLIssuanceDBDAOsearchBkgTerm]]></desc>
			<sql><![CDATA[
SELECT
	BK.BKG_NO,
	BK.RCV_TERM_CD,
	BK.DE_TERM_CD
FROM BKG_BOOKING BK
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
