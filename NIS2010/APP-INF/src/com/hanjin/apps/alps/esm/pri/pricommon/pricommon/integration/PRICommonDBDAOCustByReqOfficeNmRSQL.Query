<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOCustByReqOfficeNmRSQL">
			<desc><![CDATA[Office code select]]></desc>
			<sql><![CDATA[
SELECT OFC_CD CD
