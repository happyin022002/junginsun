<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchEdiHldFlgRSQL">
			<desc><![CDATA[EDI_HLD_FLG 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT EDI_HLD_FLG 
FROM   BKG_BOOKING
WHERE  BKG_NO = @[bkg_no]   			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
