<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOAutoEmlCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_AUTO_EML (BKG_NO
,                         ATD_DT
,                         SND_FLG
,                         CRE_USR_ID
,                         CRE_DT
,                         UPD_USR_ID
,                         UPD_DT)
VALUES (@[bkg_no]
,       TO_DATE(@[atd_dt], 'RRRR-MM-DD HH24:MI')
,       @[snd_flg]
,       @[cre_usr_id]
,       SYSDATE
,       @[upd_usr_id]
,       SYSDATE)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="atd_dt" type="12" value="" out="N"/>
				<param name="snd_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
