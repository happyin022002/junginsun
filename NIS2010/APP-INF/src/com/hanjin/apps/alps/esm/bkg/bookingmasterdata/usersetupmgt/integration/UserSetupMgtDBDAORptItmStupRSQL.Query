<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAORptItmStupRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT ITM_SEQ
,      BKG_OFC_CD
,      CUST_CNT_CD||CUST_SEQ CUST_CD
,      CUST_CNT_CD
,      CUST_SEQ
,      MTY_PKUP_YD_FLG
,      FULL_RTN_YD_FLG
,      PORT_COFF_FLG
,      RAIL_RCV_FLG
,      DOC_COFF_FLG
,      CUST_COFF_FLG
,      CALL_SGN_FLG
,      MRN_FLG
,      CRN_FLG
,      XCH_RT_FLG
,      DIFF_RMK
,      RCT_NTC_RMK
,      LODG_CFM_RMK
,      CRE_USR_ID
,      UPD_USR_ID
,      EML_CTNT
FROM   BKG_RPT_ITM_STUP
WHERE  1 = 1
#if (${bkg_ofc_cd} != '' && ${bkg_ofc_cd} != '  ') 
AND    BKG_OFC_CD = @[bkg_ofc_cd]
#end
#if (${cust_cd} != '') 
AND    CUST_CNT_CD||CUST_SEQ = @[cust_cd]
#end
ORDER BY 1			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
