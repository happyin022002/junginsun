<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusManageDBDAOCreateTransferSqmQtaRbcCSQL">
			<desc><![CDATA[Qta Transfer 시 연간 정보로 1Q SQM_QTA_RBC 생성]]></desc>
			<sql><![CDATA[
INSERT INTO SQM_QTA_RBC (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,TRD_CD
      ,RLANE_CD
      ,DIR_CD
      ,RGN_OFC_CD
      ,RHQ_CD
      ,SUB_TRD_CD
      ,GID_LOD_QTY
      ,GID_GRS_REV
      ,GID_PA_CM_AMT
      ,GID_RA_CM_AMT
      ,LOD_QTY
      ,GRS_REV
      ,PA_CM_AMT
      ,RA_CM_AMT
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT 'Q'    AS BSE_TP_CD
      ,BSE_YR AS BSE_YR
      ,'1Q'   AS BSE_QTR_CD
      ,OFC_VW_CD
      ,TRD_CD
      ,RLANE_CD
      ,DIR_CD
      ,RGN_OFC_CD
      ,RHQ_CD
      ,SUB_TRD_CD
      ,NVL(GID_LOD_QTY, 0) AS GID_LOD_QTY
      ,NVL(GID_GRS_REV, 0) AS GID_GRS_REV
      ,NVL(GID_PA_CM_AMT, 0) AS GID_PA_CM_AMT
      ,NVL(GID_RA_CM_AMT, 0) AS GID_RA_CM_AMT
      ,NVL(LOD_QTY, 0) AS LOD_QTY
      ,NVL(GRS_REV, 0) AS GRS_REV
      ,NVL(PA_CM_AMT, 0) AS PA_CM_AMT
      ,NVL(RA_CM_AMT, 0) AS RA_CM_AMT
      ,@[f_usr_id] AS CRE_USR_ID
      ,SYSDATE     AS CRE_DT
      ,@[f_usr_id] AS UPD_USR_ID
      ,SYSDATE     AS UPD_DT
  FROM SQM_QTA_RBC
 WHERE BSE_TP_CD  = 'Y'
   AND BSE_YR     = @[f_bse_yr]
   AND BSE_QTR_CD = '00'			]]></sql>
			<params>
				<param name="f_usr_id" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
