<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MASDBDAOMasSvcTrnsPrcVOUSQL">
			<desc><![CDATA[ABC STP 단가 수정   ]]></desc>
			<sql><![CDATA[
UPDATE MAS_SVC_TRNS_PRC
SET ACT_COST_UT_AMT  = TO_NUMBER(@[act_cost_ut_amt])
   ,STP_MGN_RTO  = TO_NUMBER(@[stp_mgn_rto])
   ,UPD_USR_ID  = @[upd_usr_id]
   ,UPD_DT = SYSDATE
WHERE COST_YRMON  = @[cost_yrmon]
  AND OFC_CD  = @[ofc_cd]
  AND OFC_ACT_CD = @[ofc_act_cd]			]]></sql>
			<params>
				<param name="act_cost_ut_amt" type="12" value="" out="N"/>
				<param name="stp_mgn_rto" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="ofc_act_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
