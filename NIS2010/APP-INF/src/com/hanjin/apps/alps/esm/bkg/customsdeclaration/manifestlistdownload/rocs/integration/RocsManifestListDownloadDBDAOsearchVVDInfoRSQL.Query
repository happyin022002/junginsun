<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchVVDInfoRSQL">
			<desc><![CDATA[ROCS VVD 정보 조회]]></desc>
			<sql><![CDATA[
SELECT SKD.VSL_CD||SKD.SKD_VOY_NO||SKD.SKD_DIR_CD VVD_NUMBER
      , SKD.CLPT_IND_SEQ AS POD_CLPT_IND_SEQ
      , TO_CHAR(VPS_ETA_DT,'YYYY-MM-DD') VPS_ETA_DT
      , VSL.VSL_ENG_NM
 FROM VSK_VSL_PORT_SKD SKD
     , MDM_VSL_CNTR VSL
 WHERE SKD.VSL_CD      = @[vsl_cd]
   AND SKD.SKD_VOY_NO  = @[skd_voy_no]
   AND SKD.SKD_DIR_CD  = @[skd_dir_cd]
   #if (${frm_pod_clpt_ind_seq} != '')
   AND SKD.CLPT_IND_SEQ = @[frm_pod_clpt_ind_seq]
   #end
   AND SKD.VPS_PORT_CD = 'NLRTM'
   AND SKD.VSL_CD = VSL.VSL_CD
   AND NVL(SKD.SKD_CNG_STS_CD, 'XX') !='S'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="frm_pod_clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
