<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOModifyEurTroDtlUSQL">
			<desc><![CDATA[TransferOrderIssueDBDAOModifyEurTroDtlUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_EUR_TRO_DTL SET 
       DOR_ADDR_TP_CD = @[dor_addr_tp_cd]     
     , LOC_CD         = @[dor_loc_cd]
#if (${zn_cd} != '') 
     , ZN_CD = @[dor_loc_cd]||@[zn_cd]
#else 
     , ZN_CD = @[zn_cd] 
#end      
     , LOD_REF_NO = @[lod_ref_no]
     , DOR_ZIP_ID = @[dor_pst_no]
     , DOR_ADDR   = RPAD(@[dor_addr_1], 50, ' ')||
                    RPAD(@[dor_addr_2], 50, ' ')||
                    RPAD(NVL(@[dor_addr_3],' '), 50, ' ')||
                    RPAD(@[dor_addr_4], 50, ' ')

#if (${arr_dt} != '') 
     ,	ARR_DT = TO_DATE(@[arr_dt]||' '||@[arr_dt_hhmi], 'YYYY-MM-DD HH24:MI') 
#else 
     ,	ARR_DT = @[arr_dt]
#end

     , CNTC_PSON_NM = @[cntc_pson_nm]
     , CNTC_PHN_NO  = @[cntc_phn_no]
     , CNTC_EML     = @[cntc_eml]
     ,	UPD_USR_ID  = @[upd_usr_id]
     ,	UPD_DT      = sysdate 
 WHERE BKG_NO      = @[bkg_no]
   AND IO_BND_CD   = @[io_bnd_cd]
   AND TRO_SEQ     = @[tro_seq]
   AND TRO_SUB_SEQ = @[tro_sub_seq]			]]></sql>
			<params>
				<param name="dor_addr_tp_cd" type="12" value="" out="N"/>
				<param name="dor_loc_cd" type="12" value="" out="N"/>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="lod_ref_no" type="12" value="" out="N"/>
				<param name="dor_pst_no" type="12" value="" out="N"/>
				<param name="dor_addr_1" type="12" value="" out="N"/>
				<param name="dor_addr_2" type="12" value="" out="N"/>
				<param name="dor_addr_3" type="12" value="" out="N"/>
				<param name="dor_addr_4" type="12" value="" out="N"/>
				<param name="arr_dt" type="12" value="" out="N"/>
				<param name="arr_dt_hhmi" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_phn_no" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
				<param name="tro_sub_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
