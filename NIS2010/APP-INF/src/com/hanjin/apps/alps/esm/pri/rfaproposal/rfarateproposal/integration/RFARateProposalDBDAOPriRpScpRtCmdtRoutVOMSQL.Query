<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtCmdtRoutVOMSQL">
			<desc><![CDATA[CMDT route 입력 및 수정]]></desc>
			<sql><![CDATA[
MERGE INTO PRI_RP_SCP_RT_CMDT_ROUT
USING DUAL
ON (
	PROP_NO = @[prop_no]
	AND AMDT_SEQ = @[amdt_seq]
	AND SVC_SCP_CD = @[svc_scp_cd]
	AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
	AND ROUT_SEQ = @[rout_seq]
)
WHEN MATCHED THEN
UPDATE SET
	N1ST_CMNC_AMDT_SEQ = @[amdt_seq]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHEN NOT MATCHED THEN
INSERT (
	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	CMDT_HDR_SEQ
,	ROUT_SEQ
,   N1ST_CMNC_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	ORG_CY_DOR_RT_TP_CD
,	DEST_CY_DOR_RT_TP_CD
,	MST_ROUT_ID
) VALUES( 
	@[prop_no]
,	@[amdt_seq]
,	@[svc_scp_cd]
,	@[cmdt_hdr_seq]
,	@[rout_seq]
,	@[amdt_seq]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	NVL(@[org_cy_dor_rt_tp_cd], 'C')
,	NVL(@[dest_cy_dor_rt_tp_cd], 'C')
,	(SELECT NVL(MAX(ROUT_SEQ), 0)+1
       FROM PRI_RP_SCP_RT_CMDT_ROUT
      WHERE PROP_NO = @[prop_no]
        AND AMDT_SEQ = @[amdt_seq]
        AND SVC_SCP_CD = @[svc_scp_cd]
        AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
    ) 
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="rout_seq" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="org_cy_dor_rt_tp_cd" type="12" value="" out="N"/>
				<param name="dest_cy_dor_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
