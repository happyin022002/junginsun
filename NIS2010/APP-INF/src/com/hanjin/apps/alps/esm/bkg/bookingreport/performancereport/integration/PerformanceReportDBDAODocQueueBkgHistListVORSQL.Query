<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAODocQueueBkgHistListVORSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
SELECT 
	SR_KND_CD
,	SR_NO
,	BKG_NO
,	SR_AMD_TP_CD
,	SR_AMD_SEQ
,	SR_WRK_STS_CD
,	SR_WRK_STS_USR_ID
,	SR_WRK_STS_DT
FROM BKG_SR_CRNT_RQST
WHERE	SR_KND_CD = @[sr_knd_cd]
AND	SR_NO = @[sr_no]
AND	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
