<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchIdDoNoRSQL">
			<desc><![CDATA[Do No search for Indonesia Container FlatFile]]></desc>
			<sql><![CDATA[
SELECT DO_NO||DO_NO_SPLIT AS DO_NO
  FROM BKG_DO
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
