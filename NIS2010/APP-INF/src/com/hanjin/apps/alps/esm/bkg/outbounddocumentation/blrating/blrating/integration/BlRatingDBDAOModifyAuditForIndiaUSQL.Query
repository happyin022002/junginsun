<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOModifyAuditForIndiaUSQL">
			<desc><![CDATA[India특정 surcharge항목에 대해서 별도 심사값을 저장한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_RATE
SET 
#if(${cnt_cd} == 'IN')
    IDA_IHC_AUD_STS_CD = NVL(@[ihc_flg],'Y'),
    IDA_THC_AUD_STS_CD = NVL(@[thc_flg],'Y'),
#elseif(${cnt_cd} == 'IR')
    IR_THC_AUD_STS_CD = NVL(@[thc_flg],'Y'),
#end
    IDA_AUD_BAT_DT = sysdate
WHERE BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="ihc_flg" type="12" value="" out="N"/>
				<param name="thc_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
