<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchYardNameRSQL">
			<desc><![CDATA[Berth Code로 YardName을 조회한다.]]></desc>
			<sql><![CDATA[

    SELECT YD_NM AS YD_NAME 
    FROM MDM_YARD
    WHERE YD_CD = @[frm_brth_yd_cd]
			]]></sql>
			<params>
				<param name="frm_brth_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
