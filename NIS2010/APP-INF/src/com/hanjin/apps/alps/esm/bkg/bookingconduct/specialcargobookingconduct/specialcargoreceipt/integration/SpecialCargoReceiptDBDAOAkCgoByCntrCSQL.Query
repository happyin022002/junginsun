<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOAkCgoByCntrCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_AWK_CGO
            (BKG_NO
,            AWK_CGO_SEQ
,            RCV_TERM_CD
,            DE_TERM_CD
,            CNTR_TPSZ_CD
,            CNTR_NO
,            PCK_TP_CD
,            PCK_QTY
,            GRS_WGT
,            NET_WGT
,            WGT_UT_CD
,            OVR_FWRD_LEN
,            OVR_BKWD_LEN
,            OVR_HGT
,            OVR_LF_LEN
,            OVR_RT_LEN
,            OVR_VOID_SLT_QTY
,            TTL_DIM_LEN
,            TTL_DIM_WDT
,            TTL_DIM_HGT
,            AWK_DCGO_SEQ
,            CMDT_CD
,            IN_GA_FLG
,            CRN_PST_STS_CD
,            XTD_OVR_QTY
,            PST_LCK_PIN_FLG
,            GRAV_CTR_DESC
,            STWG_RQST_DESC
,            DIFF_RMK
,            RQST_DT
,            RQST_USR_ID
,            SPCL_CGO_APRO_CD
,            CRE_USR_ID
,            CRE_DT
,            UPD_USR_ID
,            UPD_DT
,            CNTR_VOL_QTY)
    SELECT @[tgt_bkg_no]
,          (SELECT NVL(MAX(AWK_CGO_SEQ), 0)
              FROM BKG_AWK_CGO
             WHERE BKG_NO = @[tgt_bkg_no]) + 1
,          RCV_TERM_CD
,          DE_TERM_CD
,          CNTR_TPSZ_CD
,          CNTR_NO
,          PCK_TP_CD
,          PCK_QTY
,          GRS_WGT
,          NET_WGT
,          WGT_UT_CD
,          OVR_FWRD_LEN
,          OVR_BKWD_LEN
,          OVR_HGT
,          OVR_LF_LEN
,          OVR_RT_LEN
,          OVR_VOID_SLT_QTY
,          TTL_DIM_LEN
,          TTL_DIM_WDT
,          TTL_DIM_HGT
,          AWK_DCGO_SEQ
,          CMDT_CD
,          IN_GA_FLG
,          CRN_PST_STS_CD
,          XTD_OVR_QTY
,          PST_LCK_PIN_FLG
,          GRAV_CTR_DESC
,          STWG_RQST_DESC
,          DIFF_RMK
,          RQST_DT
,          RQST_USR_ID
,          SPCL_CGO_APRO_CD
,          @[cre_usr_id]
,          SYSDATE
,          @[cre_usr_id]
,          SYSDATE
,          @[tgt_cntr_vol]
    FROM   BKG_AWK_CGO
    WHERE  BKG_NO = @[src_bkg_no]
    AND    CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="tgt_bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="tgt_cntr_vol" type="12" value="" out="N"/>
				<param name="src_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
