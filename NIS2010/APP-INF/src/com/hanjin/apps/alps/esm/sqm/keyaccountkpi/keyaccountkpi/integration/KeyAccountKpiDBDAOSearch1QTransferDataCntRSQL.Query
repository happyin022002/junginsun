<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KeyAccountKpiDBDAOSearch1QTransferDataCntRSQL">
			<desc><![CDATA[Search1QTransferDataCnt]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS CNT 
  FROM SQM_KEY_ACCT_CFM_QTA
 WHERE 1=1
   AND BSE_TP_CD  = 'Q'  --FIX
   AND BSE_YR     = @[f_bse_yr]
   AND BSE_QTR_CD = '1Q' --FIX			]]></sql>
			<params>
				<param name="f_bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
