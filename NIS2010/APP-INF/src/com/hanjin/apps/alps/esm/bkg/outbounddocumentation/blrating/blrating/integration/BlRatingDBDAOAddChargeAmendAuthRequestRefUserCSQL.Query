<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOAddChargeAmendAuthRequestRefUserCSQL">
			<desc><![CDATA[PCT이후 운임 승인 권한 요청을 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CHG_AMD_AUTH_REF_USR
(
    BKG_NO,
    CHG_AMD_SEQ,
    APRO_RQST_REF_TP_CD,
    APRO_RQST_REF_USR_SEQ,
    APRO_RQST_REF_USR_ID,
    APRO_RQST_REF_USR_OFC_CD,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
VALUES
(
    @[bkg_no],
    (SELECT MAX(CHG_AMD_SEQ)
     FROM BKG_CHG_AMD_AUTH
     WHERE BKG_NO = @[bkg_no]),
    @[apro_rqst_ref_tp_cd],
    @[apro_rqst_ref_usr_seq],
    @[apro_rqst_ref_usr_id],
    @[apro_rqst_ref_usr_ofc_cd],
    @[cre_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="apro_rqst_ref_tp_cd" type="12" value="" out="N"/>
				<param name="apro_rqst_ref_usr_seq" type="12" value="" out="N"/>
				<param name="apro_rqst_ref_usr_id" type="12" value="" out="N"/>
				<param name="apro_rqst_ref_usr_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
