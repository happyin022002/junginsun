<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMSummaryDBDAORsltCoaWkPrdVORSQL">
			<desc><![CDATA[2015.06.26 CHM-201536492 Split05-주간 MAS Open에 따른 타모듈 프로그램 적용 요청 ]]></desc>
			<sql><![CDATA[
SELECT COST_YR,COST_WK,SLS_FM_DT,SLS_TO_DT ,
       CASE WHEN TO_DATE(SYSDATE) <= TO_DATE(SLS_TO_DT,'YYYYMMDD')  AND TO_DATE(SYSDATE) >= TO_DATE(SLS_FM_DT,'YYYYMMDD')
			THEN 0
			WHEN TO_DATE(SYSDATE) > TO_DATE(SLS_TO_DT,'YYYYMMDD')   
            THEN -1
            ELSE 1
       END AS WK_TP
FROM MAS_WK_PRD 
ORDER BY COST_YR,COST_WK			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
