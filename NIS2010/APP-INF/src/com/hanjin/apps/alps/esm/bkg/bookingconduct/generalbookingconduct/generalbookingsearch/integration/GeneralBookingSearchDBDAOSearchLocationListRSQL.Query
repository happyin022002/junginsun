<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchLocationListRSQL">
			<desc><![CDATA[LocationList를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
A.LOC_CD,
A.LOC_NM,
A.RGN_CD,
A.STE_CD LOC_STATE,
A.HUB_LOC_CD,
A.UN_LOC_IND_CD,
A.UN_LOC_CD,
A.LOC_CHR_CD,
A.SCC_CD,
C.LCC_CD,
C.ECC_CD,
C.RCC_CD,
A.CNT_CD,
A.SLS_OFC_CD,
A.EQ_CTRL_OFC_CD
FROM MDM_LOCATION A, MDM_COUNTRY B, MDM_EQ_ORZ_CHT C
WHERE 1 = 1 
AND A.CNT_CD = B.CNT_CD(+)
AND A.SCC_CD = C.SCC_CD
#if(${location_cd} != '')
	AND A.LOC_CD like @[location_cd] || '%'
#end
#if(${un_loc_ind_cd} != '')
	AND A.UN_LOC_IND_CD = @[un_loc_ind_cd]
#end 
#if(${loc_nm} != '')
	AND UPPER(A.LOC_NM) like '%' || UPPER(@[loc_nm])  || '%'
#end
#if(${cnt_cd} != '')
	AND A.CNT_CD like @[cnt_cd]  || '%'
#end
#if(${rcc_cd} != '')
	AND C.RCC_CD = @[rcc_cd]
#end
#if(${lcc_cd} != '')
	AND C.LCC_CD = @[lcc_cd]
#end
#if(${state} != '')
	AND A.STE_CD = @[state]
#end
#if(${select} == 'S')
	AND A.sls_ofc_cd like @[loc_eq_ofc]  || '%'
#elseif(${select} == 'F')
	AND A.finc_ctrl_ofc_cd like @[loc_eq_ofc]  || '%'
#elseif(${select} == 'E')
	AND A.eq_ctrl_ofc_cd like @[loc_eq_ofc]  || '%'
#end
	AND NVL(A.DELT_FLG, 'N') <> 'Y'
	AND NVL(B.DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="location_cd" type="12" value="" out="N"/>
				<param name="un_loc_ind_cd" type="12" value="" out="N"/>
				<param name="loc_nm" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="state" type="12" value="" out="N"/>
				<param name="loc_eq_ofc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
