<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAOPriSpScpScgAdjVODSQL">
			<desc><![CDATA[      CM/OP 산출 이후 Conversion 대상이 되는 예외적용 Surcharge 값을 반영하여 Simulation하기 위한 정보 delete]]></desc>
			<sql><![CDATA[
DELETE FROM pri_sp_scp_scg_adj
WHERE prop_no = @[prop_no]
    and amdt_seq = @[amdt_seq]
    and svc_scp_cd = @[svc_scp_cd]
    and scg_adj_seq = @[scg_adj_seq]
	and GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="scg_adj_seq" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
