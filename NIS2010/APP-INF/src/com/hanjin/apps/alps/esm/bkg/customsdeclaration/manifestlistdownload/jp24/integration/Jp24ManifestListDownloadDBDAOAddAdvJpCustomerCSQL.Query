<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOAddAdvJpCustomerCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_JP_CUST

       (BL_NO,
        BL_SPLIT_NO,
        BKG_CUST_TP_CD,
        BND_TP_CD,
        CUST_SEQ,
        CUST_CNT_CD,
        CUST_NM,
        CUST_ADDR,
        CUST_ZIP_ID,
        PHN_NO,
        FAX_NO,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (@[bl_no],
        NVL(@[bl_split_no], '  '),
        @[bkg_cust_tp_cd],
        'O',
        @[cust_seq],
        @[cust_cnt_cd],
        @[cust_nm],
        DECODE(@[cust_addr], '', @[cust_nm], @[cust_addr]),
        @[cust_zip_id],
        NVL(@[phn_no], (SELECT OFC_PHN_NO
						  FROM MDM_LOCATION ML, MDM_ORGANIZATION MO
 						 WHERE ML.EQ_CTRL_OFC_CD = MO.OFC_CD
   						   AND ML.LOC_CD         = @[pod_cd])
			),
        @[fax_no],
        @[usr_id],
        SYSDATE,
        @[usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="cust_addr" type="12" value="" out="N"/>
				<param name="cust_zip_id" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
