<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOSearchSegrGrpRSQL">
			<desc><![CDATA[유사한 화학적 특성을 갖는 위험물 격리군(Segregation Groups) 조회]]></desc>
			<sql><![CDATA[
SELECT   0 AS IMDG_SEGR_GRP_NO
       , 'NIL' AS IMDG_SEGR_GRP_NM
FROM     DUAL
UNION ALL
SELECT   IMDG_SEGR_GRP_NO
       , IMDG_SEGR_GRP_NM
FROM     SCG_IMDG_SEGR_GRP
ORDER BY IMDG_SEGR_GRP_NO			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
