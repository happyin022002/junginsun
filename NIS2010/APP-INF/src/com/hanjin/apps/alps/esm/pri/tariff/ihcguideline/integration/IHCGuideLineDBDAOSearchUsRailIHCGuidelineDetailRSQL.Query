<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOSearchUsRailIHCGuidelineDetailRSQL">
			<desc><![CDATA[Search US Rail IHC Guideline Detail]]></desc>
			<sql><![CDATA[
SELECT DISTINCT RT.HUB_LOC_CD
              , LOC.LOC_NM AS PNT_LOC_NM
              , RT.RCV_DE_TERM_CD
              , RT.BSE_PORT_LOC_CD
              , RT.COST_20FT_RAIL_FRT_RT_AMT
              , RT.COST_40FT_RAIL_FRT_RT_AMT
 FROM PRI_TRF_IHC_RT RT
    , MDM_LOCATION LOC
WHERE 1=1
  AND RT.SVC_SCP_CD = @[svc_scp_cd]
  AND RT.ORG_DEST_TP_CD = @[org_dest_tp_cd]
  AND RT.IHC_TRF_NO = @[ihc_trf_no]
  AND RT.AMDT_SEQ = @[amdt_seq]
  AND RT.IHC_CGO_TP_CD = @[ihc_cgo_tp_cd]
  AND RT.RCV_DE_TERM_CD = @[rcv_de_term_cd]
  AND RT.HUB_LOC_CD = LOC.LOC_CD
  AND RT.OPTM_TRSP_MOD_FLG = 'Y'
  AND RT.SRC_INFO_CD <> 'AD'
  AND RT.PRC_TRSP_MOD_CD IN ('R','B')
ORDER BY RT.HUB_LOC_CD, RT.BSE_PORT_LOC_CD			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="ihc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
