<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOremoveDischCoListDSQL">
			<desc><![CDATA[Discharging Company List 삭제]]></desc>
			<sql><![CDATA[
DELETE 
  FROM BKG_CSTMS_DCHG_CO
 WHERE PORT_CD = @[port_cd]
   AND CSTMS_DCHG_CO_ID = @[cstms_dchg_co_id]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="cstms_dchg_co_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
