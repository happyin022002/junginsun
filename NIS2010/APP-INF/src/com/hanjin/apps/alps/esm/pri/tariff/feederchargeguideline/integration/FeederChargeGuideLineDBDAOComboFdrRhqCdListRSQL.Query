<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAOComboFdrRhqCdListRSQL">
			<desc><![CDATA[Feeder의 서비스 코드에 따른 RHQ_CD 조회]]></desc>
			<sql><![CDATA[
SELECT RHQ_CD CD,
       RHQ_CD NM
  FROM PRI_TRF_FDR_HDR
 WHERE SVC_SCP_CD = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
