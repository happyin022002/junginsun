<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOaddPkupNtcPkupNoByPkupNtcByBkgNoCSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
/* 삭제됨!!!!
INSERT INTO BKG_PKUP_NTC_PKUP_NO
       (BKG_NO
       ,CNTR_NO
       ,OFC_CD
       ,BL_NO
       ,VSL_CD
       ,SKD_VOY_NO
       ,SKD_DIR_CD
       ,POD_CD
       ,DEL_CD
       ,DE_TERM_CD
       ,RAIL_ORG_LOC_CD
       ,RAIL_DEST_LOC_CD
       ,IBD_TRSP_HUB_CD
       ,PKUP_YD_CD
       ,PKUP_NO
       ,PKUP_AVAL_DT
       ,LST_FREE_DT
       ,RTN_YD_CD
       ,UNI_PCF_IND_FLG
       ,CAN_PCF_IND_FLG
       ,PKUP_NTC_IND_CD
       ,PKUP_NTC_SND_KNT
       ,STOP_USR_ID
       ,STOP_DT
       ,PKUP_CRE_DT
       ,PKUP_CRE_USR_ID
       ,PKUP_UPD_DT
       ,PKUP_UPD_USR_ID
       ,DELT_FLG
       ,PKUP_DELT_DT
       ,PKUP_DELT_USR_ID
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
       )
SELECT BPN.BKG_NO
      ,BPN.CNTR_NO
      ,LOC.EQ_CTRL_OFC_CD -- DEL의 EQ CONTROL OFFICE
      ,BKG.BL_NO
      ,BKG.VSL_CD
      ,BKG.SKD_VOY_NO
      ,BKG.SKD_DIR_CD
      ,BKG.POD_CD
      ,BKG.DEL_CD
      ,BKG.DE_TERM_CD
      ,SUBSTR(TRB.FM_NOD_CD, 1, 5) -- RAIL_ORG_LOC_CD,
      ,SUBSTR(TRB.TO_NOD_CD, 1, 5) -- RAIL_DEST_LOC_CD,
      ,BPN.IBD_TRSP_HUB_CD
      ,BPN.PKUP_YD_CD
      ,BPN.PKUP_NO
      ,BPN.PKUP_AVAL_DT
      ,BPN.LST_FREE_DT
      ,BPN.RTN_YD_CD
      ,'N' AS UNI_PCF_IND_FLG
      ,'N' AS CAN_PCF_IND_FLG
      ,NULL AS PKUP_NTC_IND_CD
      ,NULL AS PKUP_NTC_SND_KNT
      ,NULL AS STOP_USR_ID
      ,NULL AS STOP_DT
      ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(LOC.EQ_CTRL_OFC_CD) AS PKUP_CRE_DT
      ,@[usr_id] AS PKUP_CRE_USR_ID
      ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(LOC.EQ_CTRL_OFC_CD) AS PKUP_UPD_DT
      ,@[usr_id] AS PKUP_UPD_USR_ID
      ,'N' AS DELT_FLG
      ,NULL AS PKUP_DELT_DT
      ,NULL AS PKUP_DELT_USR_ID
      ,@[usr_id] AS CRE_USR_ID
      ,SYSDATE AS CRE_DT
      ,@[usr_id] AS UPD_USR_ID
      ,SYSDATE AS UPD_DT
  FROM BKG_PKUP_NTC          BPN
      ,TRS_TRSP_RAIL_BIL_ORD TRB
      ,BKG_BOOKING           BKG
      ,MDM_LOCATION          LOC
 WHERE 1 = 1
   AND BPN.BKG_NO  = @[bkg_no]
   AND BPN.NTC_SEQ = @[ntc_seq]
   AND TRB.TRSP_SO_OFC_CTY_CD(+) = BPN.TRSP_SO_OFC_CTY_CD
   AND TRB.TRSP_SO_SEQ(+)        = BPN.TRSP_SO_SEQ
   AND BKG.BKG_NO             = BPN.BKG_NO
   AND LOC.LOC_CD             = BKG.DEL_CD
*/			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ntc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
