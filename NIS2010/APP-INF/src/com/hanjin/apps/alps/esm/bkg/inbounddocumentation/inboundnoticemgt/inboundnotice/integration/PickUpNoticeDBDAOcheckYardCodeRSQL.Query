<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOcheckYardCodeRSQL">
			<desc><![CDATA[Yard 코드에 대한 유효성 여부를 체크한다]]></desc>
			<sql><![CDATA[
SELECT '1'
FROM	MDM_YARD
WHERE	YD_CD = @[loc_cd]
AND	DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
