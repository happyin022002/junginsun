<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOModifyAverageUCUSQL">
			<desc><![CDATA[Average UC단가를 생성한다]]></desc>
			<sql><![CDATA[
UPDATE COA_OP_AVG_UT_COST_IF T1
SET T1.BSA_CAPA = (
    			SELECT T2.FNL_HJS_BSA_CAPA
    			  FROM BSA_VVD_MST T2
    			 WHERE T1.TRD_CD=T2.TRD_CD
      			   AND T1.RLANE_CD=T2.RLANE_CD
     			   AND T1.VSL_CD=T2.VSL_CD
      			   AND T1.SKD_VOY_NO=T2.SKD_VOY_NO
      			   AND T1.DIR_CD=T2.SKD_DIR_CD)
  ,T1.UPD_USR_ID = @[upd_usr_id]
  ,T1.UPD_DT = SYSDATE
WHERE T1.COST_YRMON = @[cost_yrmon]
  AND T1.COST_USE_TP_CD = @[cost_use_tp_cd]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="cost_use_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
