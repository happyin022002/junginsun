<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommApprovalDBDAOSearchACMCSRInfoRSQL">
			<desc><![CDATA[SearchACMCSRInfo]]></desc>
			<sql><![CDATA[
SELECT CSR_NO,
           SRC_CTNT,
		   CSR_APRO_TP_CD 
      FROM AP_INV_HDR
     WHERE CSR_NO = @[csr_no] 			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
