<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqBreakbulkDBDAOaddPriScqBbMnCSQL">
			<desc><![CDATA[addPriScqBbMn
* 2015.04.23 송호진 [CHM-201533702] SCQ내에 Unit Dual (cm/Inch) System 개발 요청
]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SCQ_BB_MN
	( SCQ_RQST_NO                      
	, SCQ_VER_NO                       
	, PROG_STS_CD                      
	, RQST_OFC_CD                      
	, RQST_SREP_CD                                               
	, POL_CD                           
	, POL_YD_CD                        
	, POD_CD                           
	, POD_YD_CD                                                 
	, SVC_SCP_CD                       
	, CUST_CNT_CD                      
	, CUST_SEQ                         
	, RCV_TERM_CD                      
	, DE_TERM_CD
#if(${prop_eff_dt}!='')                       
	, PROP_EFF_DT
#end
#if(${prop_exp_dt}!='')                      
	, PROP_EXP_DT
#end
#if(${apro_eff_dt}!='')                      
	, APRO_EFF_DT
#end
#if(${apro_exp_dt}!='')                      
	, APRO_EXP_DT
#end                      
	, DELT_FLG                         
	, CRE_USR_ID                       
	, CRE_DT                           
	, UPD_USR_ID                       
	, UPD_DT
	, APRO_OFC_CD
    , TS_LOC_CD
    , TS_YD_CD
    , PROP_RT_AMT
    , APRO_RT_AMT
    , LANE_CD
    , VVD_CD
    , VPS_ETA_DT
    , ACT_CUST_CNT_CD
    , ACT_CUST_SEQ
    , ACT_CUST_NM
    , SCQ_BID_FLG
    , MEAS_SYS_CD
	)
	VALUES
	( @[scq_rqst_no]                
	, @[scq_ver_no]                     
	, @[prog_sts_cd]                     
	, @[rqst_ofc_cd]                     
	, @[rqst_srep_cd]                                        
	, @[pol_cd]                     
	, @[pol_yd_cd]                     
	, @[pod_cd]                     
	, @[pod_yd_cd]                                         
	, @[svc_scp_cd]                     
	, @[cust_cnt_cd]                     
	, @[cust_seq]                     
	, @[rcv_term_cd]                     
	, @[de_term_cd]                     
#if(${prop_eff_dt}!='')
	, TO_DATE(@[prop_eff_dt],'YYYY-MM-DD')
#end
#if(${prop_exp_dt}!='')                     
	, TO_DATE(@[prop_exp_dt],'YYYY-MM-DD')
#end
#if(${apro_eff_dt}!='')                     
	, TO_DATE(@[apro_eff_dt],'YYYY-MM-DD')
#end
#if(${apro_exp_dt}!='')                     
	, TO_DATE(@[apro_exp_dt],'YYYY-MM-DD')
#end                    
	, @[delt_flg]                     
	, @[cre_usr_id]                     
	, SYSDATE                     
	, @[cre_usr_id]                     
	, SYSDATE
	, @[apro_ofc_cd]
    , @[ts_loc_cd]
    , @[ts_yd_cd]
    , @[prop_rt_amt]
    , @[apro_rt_amt]
    , @[lane_cd]
    , @[vvd_cd]
    , TO_DATE ( @[vps_eta_dt], 'YYYY-MM-DD HH24:MI' )
    , @[act_cust_cnt_cd]
    , @[act_cust_seq]
    , @[act_cust_nm]
    , @[scq_bid_flg]
    , NVL ( @[meas_sys_cd], 'M' )
	)			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no" type="12" value="" out="N"/>
				<param name="prog_sts_cd" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="rqst_srep_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="rcv_term_cd" type="12" value="" out="N"/>
				<param name="de_term_cd" type="12" value="" out="N"/>
				<param name="prop_eff_dt" type="12" value="" out="N"/>
				<param name="prop_exp_dt" type="12" value="" out="N"/>
				<param name="apro_eff_dt" type="12" value="" out="N"/>
				<param name="apro_exp_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="ts_loc_cd" type="12" value="" out="N"/>
				<param name="ts_yd_cd" type="12" value="" out="N"/>
				<param name="prop_rt_amt" type="12" value="" out="N"/>
				<param name="apro_rt_amt" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="vps_eta_dt" type="12" value="" out="N"/>
				<param name="act_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="act_cust_seq" type="12" value="" out="N"/>
				<param name="act_cust_nm" type="12" value="" out="N"/>
				<param name="scq_bid_flg" type="12" value="" out="N"/>
				<param name="meas_sys_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
