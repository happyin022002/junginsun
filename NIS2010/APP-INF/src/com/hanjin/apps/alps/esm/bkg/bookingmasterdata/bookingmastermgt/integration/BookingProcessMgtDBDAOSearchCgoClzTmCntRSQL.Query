<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingProcessMgtDBDAOSearchCgoClzTmCntRSQL">
			<desc><![CDATA[cargo closing time setup Check]]></desc>
			<sql><![CDATA[
SELECT   
        (   SELECT  COUNT(*) 
            FROM    MDM_LOCATION
            WHERE   LOC_CD      = @[pol_cd]
            AND     DELT_FLG    = 'N'           ) POL_CNT,
        (   SELECT  COUNT(*)
            FROM    MDM_VSL_SVC_LANE_DIR
            WHERE   VSL_SLAN_CD = @[lane_cd]
            AND     DELT_FLG    = 'N'           ) LANE_CNT,
        (   SELECT  COUNT(*)
            FROM    MDM_VSL_SVC_LANE_DIR
            WHERE   VSL_SLAN_DIR_CD = @[dir_cd]
            AND     DELT_FLG    = 'N'           ) DIR_CNT
FROM    DUAL			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
