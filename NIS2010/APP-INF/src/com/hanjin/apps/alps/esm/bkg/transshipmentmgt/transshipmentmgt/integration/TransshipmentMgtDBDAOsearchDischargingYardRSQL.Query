<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransshipmentMgtDBDAOsearchDischargingYardRSQL">
			<desc><![CDATA[조회 조건에 맞는 yard list를 조회한다.]]></desc>
			<sql><![CDATA[
select yd_cd, yd_nm
from mdm_yard 
#if (${vps_port_cd} != '')
where loc_cd = @[vps_port_cd]
#end			]]></sql>
			<params>
				<param name="vps_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
