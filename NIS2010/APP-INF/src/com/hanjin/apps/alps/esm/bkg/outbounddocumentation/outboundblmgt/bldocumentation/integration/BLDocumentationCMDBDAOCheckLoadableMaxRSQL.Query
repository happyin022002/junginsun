<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCheckLoadableMaxRSQL">
			<desc><![CDATA[CNTR WGT가 Loadable MAX값을 넘는지 체크]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN TO_NUMBER(@[cntr_wgt]) < TO_NUMBER(ATTR_CTNT5) THEN 'Y' ELSE 'N' END FLG
FROM BKG_HRD_CDG_CTNT HRD
 WHERE HRD_CDG_ID ='VGM_MAX_MIN'
    AND HRD.ATTR_CTNT1 = @[cntr_tp_sz]
    AND HRD.ATTR_CTNT4 = DECODE(@[wgt_ut_cd],'KGM','KGS','LBR','LBS',@[wgt_ut_cd])
    AND ROWNUM = 1 			]]></sql>
			<params>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="cntr_tp_sz" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
