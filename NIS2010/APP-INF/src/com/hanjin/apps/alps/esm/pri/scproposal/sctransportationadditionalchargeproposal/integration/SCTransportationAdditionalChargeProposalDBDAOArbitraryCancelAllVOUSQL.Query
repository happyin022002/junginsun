<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCTransportationAdditionalChargeProposalDBDAOArbitraryCancelAllVOUSQL">
			<desc><![CDATA[Arbitrary Accept Cancel All]]></desc>
			<sql><![CDATA[
UPDATE PRI_SP_SCP_TRSP_ADD_CHG SET 	   
	   PRC_PROG_STS_CD =
					  CASE SIGN(COFFR_FRT_RT_AMT) WHEN 1 THEN 'R'
					  ELSE 'I'
	   				  END
	 , FNL_FRT_RT_AMT = NULL
     , ACPT_USR_ID = NULL
     , ACPT_OFC_CD = NULL
     , ACPT_DT     = NULL
     , UPD_USR_ID  = @[upd_usr_id]
     , UPD_DT = SYSDATE
 WHERE PROP_NO = @[prop_no]
   AND AMDT_SEQ = @[amdt_seq]
   AND SVC_SCP_CD = @[svc_scp_cd]
   AND ADD_CHG_TP_CD = 'A' 
   AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND PRC_PROG_STS_CD = 'A'
   AND N1ST_CMNC_AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
