<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAOSearchBatchStartDateRSQL">
			<desc><![CDATA[현재 유효한 batch data의 시작 시간을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
'Batch Executed Date : '||TO_CHAR(BAT_ST_DT, 'YYYY-MM-DD HH24:MI:SS') BAT_ST_DT
, DBD_CRE_DT
, DBD_CRE_SEQ
FROM BKG_DBD_BAT
WHERE 1=1
AND BAT_STS_CD = 'S'
AND ROWNUM = 1			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
