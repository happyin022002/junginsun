<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchAwkCgoRSQL">
			<desc><![CDATA[searchAwkCgo]]></desc>
			<sql><![CDATA[
SELECT 
    LTRIM(DECODE(OVR_VOID_SLT_QTY, 0 , DECODE(SUBSTR(CNTR_TPSZ_CD, 1, 1), 'A', 'INGU', 'F', 'INGU', 'S', 'INGU', 'O', 'INGU', ' '), 
    SUBSTR(RTRIM(REPLACE(FF, 'F:0')||REPLACE(BB, 'B:0')||REPLACE(HH, 'H:0')||REPLACE(LL, 'L:0')||REPLACE(RR, 'R:0')||REPLACE(PS, 'PS:0')||REPLACE(EH, 'EH:0')), 1, 50))) AWK_NO
FROM (
    SELECT 'F:'||TO_CHAR(ROUND(OVR_FWRD_LEN))||' ' FF,
      'B:'||TO_CHAR(ROUND(OVR_BKWD_LEN))||' ' BB,
      'H:'||TO_CHAR(ROUND(OVR_HGT))||' ' HH,
      'L:'||TO_CHAR(ROUND(OVR_LF_LEN))||' ' LL,
      'R:'||TO_CHAR(ROUND(OVR_RT_LEN))||' ' RR,
      'PS:'||TO_CHAR(DECODE(NVL(TRIM(CRN_PST_STS_CD), 0), 'F', 0, 'E', 0, NVL(TRIM(CRN_PST_STS_CD), 0)))||' ' PS,
      DECODE(NVL(XTD_OVR_QTY, 0), 0, '', DECODE(CNTR_TPSZ_CD, 'A4', 'EH:'||XTD_OVR_QTY||'  ', '')) EH,
      CNTR_TPSZ_CD,
      OVR_VOID_SLT_QTY
    FROM BKG_AWK_CGO
    WHERE BKG_NO = @[bkg_no]
      AND CNTR_NO = @[cntr_no] )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
