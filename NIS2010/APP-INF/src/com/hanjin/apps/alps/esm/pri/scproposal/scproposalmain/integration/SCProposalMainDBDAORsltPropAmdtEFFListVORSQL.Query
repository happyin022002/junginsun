<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAORsltPropAmdtEFFListVORSQL">
			<desc><![CDATA[Amend List 조회]]></desc>
			<sql><![CDATA[
select TO_CHAR(MN.EFF_DT, 'YYYYMMDD') EFF_DT
    ,TO_CHAR(MN.EXP_DT, 'YYYYMMDD')  EXP_DT
    ,TO_CHAR(DUR.CTRT_EFF_DT, 'YYYYMMDD') CTRT_EFF_DT 
    , TO_CHAR(DUR.CTRT_EXP_DT, 'YYYYMMDD') CTRT_EXP_DT
     ,( SELECT TO_CHAR(N.EFF_DT ,'YYYYMMDD')
          FROM PRI_SP_MN N
         WHERE N.PROP_NO = MN.PROP_NO AND N.AMDT_SEQ = MN.AMDT_SEQ-1 
       ) PRE_EFF_DT     
	,mn.prop_no,mn.amdt_seq
	,hdr.sc_no
from pri_sp_mn mn, PRI_SP_DUR DUR, pri_sp_hdr hdr
where mn.prop_no = @[prop_no]
    and mn.amdt_seq = @[amdt_seq]
    and mn.prop_no = dur.prop_no 
    and mn.amdt_seq = dur.amdt_seq  
	and mn.prop_no = hdr.prop_no			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
