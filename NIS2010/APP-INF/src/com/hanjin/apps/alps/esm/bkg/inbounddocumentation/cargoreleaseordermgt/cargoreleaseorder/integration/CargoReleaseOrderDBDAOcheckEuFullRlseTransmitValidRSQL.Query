<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOcheckEuFullRlseTransmitValidRSQL">
			<desc><![CDATA[Pickup Date 및 Release Eapire Date 에 해당하는 부킹데이타의 유무 체크]]></desc>
			<sql><![CDATA[
SELECT DECODE(count(*),0,'N','Y')
FROM BKG_BOOKING A
    ,BKG_HRD_CDG_CTNT B
WHERE A.BKG_NO = @[bkg_no]
AND   B.HRD_CDG_ID = 'EU_FULL_CHK_ITM'
AND   SUBSTR(A.POD_CD,1,2) = B.ATTR_CTNT1
#if (${chk_itm} == 'P')
AND   B.ATTR_CTNT2 ='Y'   -- Pick Date Check
#else
AND   B.ATTR_CTNT3 ='Y'   -- Rlse Expire Date Check
AND   B.ATTR_CTNT10 = 'N'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
