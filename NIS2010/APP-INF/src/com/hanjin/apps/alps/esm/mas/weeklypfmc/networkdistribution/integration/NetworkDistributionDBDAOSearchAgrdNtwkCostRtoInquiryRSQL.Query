<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOSearchAgrdNtwkCostRtoInquiryRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT A.COST_YRMON
     , A.GRP_SEQ, A.COST_YRMON_SEQ
     , DECODE(A.LOCL_TS_STS_CD, 'LO', A.TRD_CD, '')      AS FM_TRD_CD
     , DECODE(A.LOCL_TS_STS_CD, 'LO', B.SLAN_CD, '')     AS FM_LANE_CD
     , DECODE(A.LOCL_TS_STS_CD, 'LO', A.IOC_CD, '')      AS FM_IOC_CD
     , DECODE(A.LOCL_TS_STS_CD, 'LO', B.HUL_BND_CD, '')  AS FM_HUL_BND_CD
     , DECODE(A.LOCL_TS_STS_CD, 'LO', A.DIR_CD, '')      AS FM_DIR_CD
     , A.TRD_CD                                          AS TO_TRD_CD
     , B.SLAN_CD                                         AS TO_LANE_CD
     , A.IOC_CD                                          AS TO_IOC_CD
     , B.HUL_BND_CD                                      AS TO_HUL_BND_CD
     , A.DIR_CD                                          AS TO_DIR_CD
     , A.LOCL_TS_STS_CD
     , A.BZC_ALOC_TP_CD, A.BZC_ALOC_RTO, A.BZC_ALOC_FX_AMT
     , A.OVR_USD_ALOC_CHG_FLG, A.OVR_USD_ALOC_CHG_RTO
  FROM MAS_AGRD_NTWK_COST_RTO A, MAS_LANE_RGST B
 WHERE A.TRD_CD = B.TRD_CD
   AND A.RLANE_CD = B.RLANE_CD
   AND A.IOC_CD = B.IOC_CD
   AND A.DIR_CD = B.DIR_CD
   AND A.DELT_FLG <> 'Y'
   AND B.DELT_FLG <> 'Y'
   AND A.cost_yrmon = replace(@[f_cost_yrmon] ,'-','')
 
 ORDER BY A.COST_YRMON, A.GRP_SEQ, A.LOCL_TS_STS_CD, A.COST_YRMON_SEQ
			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
