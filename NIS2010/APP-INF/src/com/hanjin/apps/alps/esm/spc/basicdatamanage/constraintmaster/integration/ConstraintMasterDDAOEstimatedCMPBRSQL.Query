<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDDAOEstimatedCMPBRSQL">
			<desc><![CDATA[2015.07.06 [CHM-201536749]Mastertable Import기능 오류 수정 CR에 반영(Revenue Management System 추가 보완 개발 요청 선반영 포함 - CMPB 팝업 연결 추가)]]></desc>
			<sql><![CDATA[
WITH REV AS (
     SELECT BKG_NO
          , REV_COST_SEQ
          , REV_AMT
          , ESTM_COST_AMT
          , BKG_TEU_QTY
          , CMPB_AMT
          , EQ_MGMT_UC_AMT
          , OFT_AMT 
       FROM BKG_REV_COST REV
      WHERE BKG_NO     = @[bkg_no] 
        AND REV_COST_SEQ = (SELECT MAX(REV_COST_SEQ) 
                              FROM BKG_REV_COST 
                             WHERE BKG_NO = REV.BKG_NO )
)

SELECT '1' SEQ
     , 'Revenue Total' COST_NM
     , REV_AMT  AMT
     , '' RMK
FROM REV
UNION ALL
SELECT '' SEQ
     , '└ OFT' COST_NM
     , OFT_AMT AMT
     , '' RMK
FROM REV 
UNION ALL
SELECT '2' SEQ
     , 'Cost Total' COST_NM
     , ESTM_COST_AMT  AMT
     , '' RMK
FROM REV
UNION ALL 
SELECT '' SEQ
     , '└ EMU Cost' COST_NM
     , NVL(EQ_MGMT_UC_AMT,NULL) AMT 
     , '' RMK
FROM REV
UNION ALL  
SELECT '3' SEQ
     , 'Contribution Margin' COST_NM
     , (NVL(REV_AMT,0)-NVL(ESTM_COST_AMT,0)) AMT 
     , 'Per BL' RMK
FROM REV
UNION ALL  
SELECT '4' SEQ
     , 'CMPB (TEU)' COST_NM
     , CMPB_AMT  AMT 
     , 'Per Box' RMK
FROM REV
UNION ALL 
SELECT '5' SEQ
     , 'Guide CMPB' COST_NM
     , NULL  AMT -- 추후.. SPC 에서 조회해야 할 값
     , 'Per Box' RMK
FROM REV
UNION ALL
SELECT '6' SEQ
     , 'Difference' COST_NM
     , NULL  AMT -- 추후.. Guide CMPB - CMPB
     , '' RMK
FROM REV			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
