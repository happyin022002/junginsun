<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOMultiTsVolumeUSQL">
			<desc><![CDATA[Step1 T/S Volume팝업 수정]]></desc>
			<sql><![CDATA[
UPDATE MAS_SIM_INTR_TRNS_VOL
   SET VSL_TRNS_QTY = @[vsl_trns_qty]
      ,IOC_CD       = @[ioc_cd]
      ,UPD_USR_ID   = @[upd_usr_id]
      ,UPD_DT       = SYSDATE
 WHERE SIM_DT     = @[sim_dt]
   AND SIM_NO     = @[sim_no]
   AND VSL_CD     = @[vsl_cd]
   AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="vsl_trns_qty" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
