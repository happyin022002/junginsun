<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSetupDBDAOModifyFeederageInfoUSQL">
			<desc><![CDATA[ModifyFeederageInfo
2016.08.05 김상현 [CHM-201642499] ALPS > ACM VIP Agreement Creation 상 SC/RFA No. 추가 요청]]></desc>
			<sql><![CDATA[
UPDATE ACM_OTR_FDRG_DDCT
SET TO_EFF_DT = SYSDATE - 1,
  UPD_USR_ID = @[usr_id],
  UPD_DT = SYSDATE
WHERE POL_CD = @[pol_cd]
  AND POD_CD = @[pod_cd]
  AND CNTR_TPSZ_CD = @[cntr_tpsz_cd]
  AND OTR_FDRG_DDCT_SEQ = (SELECT NVL(MAX(OTR_FDRG_DDCT_SEQ), 0)
                           FROM ACM_OTR_FDRG_DDCT
                           WHERE POL_CD = @[pol_cd]
                             AND POD_CD = @[pod_cd]
                             AND CNTR_TPSZ_CD = @[cntr_tpsz_cd]
                          )			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
