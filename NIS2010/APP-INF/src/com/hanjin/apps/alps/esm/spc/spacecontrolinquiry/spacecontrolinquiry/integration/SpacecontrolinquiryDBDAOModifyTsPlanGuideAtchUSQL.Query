<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAOModifyTsPlanGuideAtchUSQL">
			<desc><![CDATA[[T/S Plan & guide Attach]을 [업데이트]합니다.]]></desc>
			<sql><![CDATA[
UPDATE (
	SELECT 
		 STO.FILE_NM AS IMG_FILE_NM
		,STO.FILE_PATH_RMK AS IMG_FILE_PATH
		,UPLD.FILE_UPLD_NM AS UPLD_FILE_NM
		,UPLD.FILE_PATH_URL AS UPLD_FILE_PATH
	FROM SPC_TS_PLN_GID_FILE_STO STO, COM_UPLD_FILE UPLD
	WHERE STO.FILE_SAV_ID     = UPLD.FILE_SAV_ID
	  AND STO.REP_TRD_CD      = @[rep_trd_cd]
      AND STO.REP_SUB_TRD_CD  = @[rep_sub_trd_cd]
      AND STO.RLANE_CD        = @[rlane_cd]
      AND STO.VVD_CD          = @[vvd_cd]
      AND STO.IRR_PORT_CD     = @[irr_port_cd]
      AND STO.IRR_YD_CD       = @[irr_yd_cd]
	  AND STO.PLN_SEQ         = @[pln_seq]
)
SET
IMG_FILE_NM = UPLD_FILE_NM,
IMG_FILE_PATH = UPLD_FILE_PATH			]]></sql>
			<params>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="rep_sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="irr_port_cd" type="12" value="" out="N"/>
				<param name="irr_yd_cd" type="12" value="" out="N"/>
				<param name="pln_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
