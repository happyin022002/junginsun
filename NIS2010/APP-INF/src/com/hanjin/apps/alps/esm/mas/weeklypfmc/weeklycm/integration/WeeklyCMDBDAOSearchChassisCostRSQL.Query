<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOSearchChassisCostRSQL">
			<desc><![CDATA[SearchChassisCost
2015.03.26 컬럼 속성명 변경으로 수정()]]></desc>
			<sql><![CDATA[
SELECT COST_YR,
       BSE_QTR_CD AS COST_QTR_CD,
	   COST_YR||'-'||BSE_QTR_CD AS COST_YR_QTR,
       EFF_FM_YRMON,
       EFF_TO_YRMON,
       ON_ST_AMT,
       ON_TML_AMT,
       MIG_AMT,
       REV_SHR_AMT,
       CR_MM_AMT,
       MISC_RBIL_AMT AS MISC_RE_BIL_AMT,
       ON_CHSS_AMT,
       CHSS_DRYG_AMT,
       ON_TML_AMT + MIG_AMT + REV_SHR_AMT + CR_MM_AMT + MISC_RBIL_AMT + ON_CHSS_AMT + CHSS_DRYG_AMT AS COM_SUB_TTL_AMT, -- 화면에서 EDIT시 자동계산
	   ON_ST_AMT + ON_TML_AMT + MIG_AMT + REV_SHR_AMT + CR_MM_AMT + MISC_RBIL_AMT + ON_CHSS_AMT + CHSS_DRYG_AMT AS COM_TTL_AMT,
       CHSS_RMK
FROM   MAS_CHSS_COST_UPLD
WHERE  COST_YR||BSE_QTR_CD  BETWEEN SUBSTR(@[fr_year]||@[fr_qtr]||'Q',1,6)
                             AND     SUBSTR(@[to_year]||@[to_qtr]||'Q',1,6)
ORDER BY EFF_FM_YRMON ASC			]]></sql>
			<params>
				<param name="fr_year" type="12" value="" out="N"/>
				<param name="fr_qtr" type="12" value="" out="N"/>
				<param name="to_year" type="12" value="" out="N"/>
				<param name="to_qtr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
