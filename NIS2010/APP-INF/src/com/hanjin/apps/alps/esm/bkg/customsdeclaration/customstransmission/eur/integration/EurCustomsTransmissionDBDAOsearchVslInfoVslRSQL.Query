<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchVslInfoVslRSQL">
			<desc><![CDATA[Vessel ETA 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
    VSL_CD || SKD_VOY_NO || SKD_DIR_CD AS BVVD
    ,POL_CD AS BPOL
    ,POD_CD AS BPOD
FROM BKG_VVD
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
