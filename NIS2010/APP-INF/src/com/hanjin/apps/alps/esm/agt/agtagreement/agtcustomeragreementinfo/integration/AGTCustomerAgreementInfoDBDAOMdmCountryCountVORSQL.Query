<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCustomerAgreementInfoDBDAOMdmCountryCountVORSQL">
			<desc><![CDATA[코드테이블에 등록되어 있는지 체크]]></desc>
			<sql><![CDATA[
SELECT 
	COUNT(*) AS ROW_COUNT
FROM   MDM_COUNTRY
WHERE  NVL(DELT_FLG, 'N') <> 'Y'
AND    CNT_CD = @[cnt_cd]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
