<sqls>
	<querys>
		<query name="SCBoilerPlateProposalDBDAOPriSpBlplCtntAmdVOCSQL">
			<desc><![CDATA[SCBoilerPlateProposalDB PriSpBlplCtntAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_BLPL_CTNT(
    PROP_NO,
    AMDT_SEQ,
    BLPL_SEQ,
    BLPL_CTNT_SEQ,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    BLPL_CTNT,
    DP_SEQ,
    N1ST_CMNC_AMDT_SEQ,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
SELECT
    PROP_NO         ,
    AMDT_SEQ+1      ,
    BLPL_SEQ        ,
    BLPL_CTNT_SEQ   ,
    PRC_PROG_STS_CD ,
    SRC_INFO_CD     ,
    BLPL_CTNT       ,
    DP_SEQ			,
    N1ST_CMNC_AMDT_SEQ    ,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    @[cre_usr_id]   ,
    SYSDATE         ,
    @[upd_usr_id]   ,
    SYSDATE			
    
FROM 
    PRI_SP_BLPL_CTNT
WHERE
    PROP_NO     = @[prop_no]
AND AMDT_SEQ    = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
