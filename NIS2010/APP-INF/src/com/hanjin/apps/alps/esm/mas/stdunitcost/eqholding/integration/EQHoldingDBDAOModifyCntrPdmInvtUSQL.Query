<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQHoldingDBDAOModifyCntrPdmInvtUSQL">
			<desc><![CDATA[Container PDM by Inventory Update 로직]]></desc>
			<sql><![CDATA[
UPDATE MAS_CNTR_EQ_COST
SET INVT_CNTR_QTY = @[invt_cntr_qty],
    COST_AMT = @[cost_amt],
    PD_COST_AMT = @[pd_cost_amt],
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT = SYSDATE
WHERE COST_YR = @[cost_yr]
AND BSE_QTR_CD = @[bse_qtr_cd]
AND TPSZ_CD = @[tpsz_cd]			]]></sql>
			<params>
				<param name="invt_cntr_qty" type="12" value="" out="N"/>
				<param name="cost_amt" type="12" value="" out="N"/>
				<param name="pd_cost_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
