<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VIPDeductAgreementDBDAOAddVIPAgmtCntrCSQL">
			<desc><![CDATA[VIP Agreement에 따른 Container type 추가]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_VIP_AGMT_CNTR(
  CUST_GRP_ID,
  AGMT_SEQ,
  AGMT_CNTR_SEQ,
  CNTR_TPSZ_CD,
  CRE_USR_ID,
  CRE_DT,
  UPD_USR_ID,
  UPD_DT
)
VALUES(
  @[cust_grp_id],
  @[agmt_seq],
  NVL((SELECT MAX(AGMT_CNTR_SEQ)
       FROM ACM_VIP_AGMT_CNTR
       WHERE CUST_GRP_ID = @[cust_grp_id]
         AND AGMT_SEQ = @[agmt_seq]
      ), 0
  ) + 1,
  @[cntr_tpsz_cd],
  @[cre_usr_id],
  SYSDATE,
  @[cre_usr_id],
  SYSDATE
)			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
