<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOCheckUsRailIHCGuidelineDetailRSQL">
			<desc><![CDATA[Check US Rail IHC Guideline Detail]]></desc>
			<sql><![CDATA[
SELECT RT.HUB_LOC_CD 
     , RT.BSE_PORT_LOC_CD 
 FROM PRI_TRF_IHC_RT RT
WHERE 1=1
  AND RT.SVC_SCP_CD = @[svc_scp_cd]
  AND RT.ORG_DEST_TP_CD = @[org_dest_tp_cd]
  AND RT.IHC_TRF_NO = @[ihc_trf_no]
  AND RT.AMDT_SEQ = @[amdt_seq]
  AND RT.IHC_CGO_TP_CD = @[ihc_cgo_tp_cd]
  AND RT.RCV_DE_TERM_CD = @[rcv_de_term_cd]
  AND RT.HUB_LOC_CD = @[hub_loc_cd]
  AND RT.BSE_PORT_LOC_CD = @[bse_port_loc_cd]
  AND RT.OPTM_TRSP_MOD_FLG = 'Y'
  AND RT.SRC_INFO_CD <> 'AD'
  AND RT.PRC_TRSP_MOD_CD IN ('A','R')			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="ihc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="hub_loc_cd" type="12" value="" out="N"/>
				<param name="bse_port_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
