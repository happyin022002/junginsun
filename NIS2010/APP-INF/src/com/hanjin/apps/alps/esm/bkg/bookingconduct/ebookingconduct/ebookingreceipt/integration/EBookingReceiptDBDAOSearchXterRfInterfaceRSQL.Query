<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchXterRfInterfaceRSQL">
			<desc><![CDATA[external request 처리를 위해 external rqst의 Reefer cgo 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT RF.CNTR_NO
        , RF.RC_SEQ
        , RF.CNTR_TPSZ_CD
        , MST.CMDT_CD
        , MST.CMDT_DESC
        , RF.PWR_SPL_CBL_FLG
        , RF.VENT_RTO
        , RF.CLNG_TP_CD
        , RF.HUMID_RTO HUMID_NO
        , RF.CNTR_VENT_TP_CD
        , RF.DIFF_RMK
        , 'I' AS IBFLAG
        , '' AS RF_DCGO_SEQ
        , '' AS PCK_TP_CD
        , ROUND(DECODE(MIN_TEMP_UT_CD, 'F', (5 * RF.MIN_TEMP - 160)/9, RF.MIN_TEMP), 1) AS CDO_TEMP
        , ROUND(DECODE(MIN_TEMP_UT_CD, 'C', 9 / 5 * RF.MIN_TEMP + 32 , RF.MIN_TEMP), 1) AS FDO_TEMP
  FROM BKG_XTER_RF_CGO RF, BKG_XTER_RQST_MST MST
 WHERE RF.XTER_SNDR_ID = MST.XTER_SNDR_ID
   AND RF.XTER_RQST_NO = MST.XTER_RQST_NO
   AND RF.XTER_RQST_SEQ= MST.XTER_RQST_SEQ
   AND RF.XTER_SNDR_ID = @[xter_sndr_id]
   AND RF.XTER_RQST_NO = @[xter_rqst_no]
   AND RF.XTER_RQST_SEQ= @[xter_rqst_seq]			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
