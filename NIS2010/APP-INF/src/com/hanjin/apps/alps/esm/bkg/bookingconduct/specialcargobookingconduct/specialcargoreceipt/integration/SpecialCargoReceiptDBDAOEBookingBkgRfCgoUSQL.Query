<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOEBookingBkgRfCgoUSQL">
			<desc><![CDATA[EBookingBkgRfCgo]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')

UPDATE BKG_RF_CGO_HIS SET 
	CNTR_NO 		= @[cntr_no]
,	CNTR_TPSZ_CD 	= @[cntr_tpsz_cd]
,	CMDT_CD         = @[cmdt_cd]
,	CMDT_DESC       = @[cmdt_desc]
,	FDO_TEMP        = @[fdo_temp]       
,	CDO_TEMP        = @[cdo_temp]       
,	PWR_SPL_CBL_FLG = @[pwr_spl_cbl_flg]
,	CNTR_VENT_TP_CD = NVL(@[cntr_vent_tp_cd], nvl(CNTR_VENT_TP_CD, 'P'))
,	VENT_RTO        = decode(NVL(@[cntr_vent_tp_cd], 'P'),'P',@[vent_rto],0)
,   CBM_PER_HR_QTY  = decode(NVL(@[cntr_vent_tp_cd], 'P'),'C',@[vent_rto],0)
,	CLNG_TP_CD      = @[clng_tp_cd]     
,	HUMID_NO 		= @[humid_no]       
,	UPD_USR_ID 		= @[upd_usr_id]
,	UPD_DT 			= SYSDATE
,   DIFF_RMK = @[diff_rmk]
WHERE BKG_NO = @[bkg_no]
AND	RC_SEQ = @[rc_seq]
AND NVL(SPCL_CGO_APRO_CD,' ') NOT IN ('R', 'Y', 'N')
AND CORR_NO = 'TMP0000001'

#else

UPDATE BKG_RF_CGO SET 
	CNTR_NO 		= @[cntr_no]
,	CNTR_TPSZ_CD 	= @[cntr_tpsz_cd]
,	CMDT_CD         = @[cmdt_cd]
,	CMDT_DESC       = @[cmdt_desc]
,	FDO_TEMP        = @[fdo_temp]       
,	CDO_TEMP        = @[cdo_temp]       
,	PWR_SPL_CBL_FLG = @[pwr_spl_cbl_flg]
,	CNTR_VENT_TP_CD = NVL(@[cntr_vent_tp_cd], nvl(CNTR_VENT_TP_CD, 'P'))
,	VENT_RTO        = decode(NVL(@[cntr_vent_tp_cd], 'P'),'P',@[vent_rto],0)
,   CBM_PER_HR_QTY  = decode(NVL(@[cntr_vent_tp_cd], 'P'),'C',@[vent_rto],0)     
,	CLNG_TP_CD      = @[clng_tp_cd]     
,	HUMID_NO 		= @[humid_no]       
,	UPD_USR_ID 		= @[upd_usr_id]
,	UPD_DT 			= SYSDATE
,   DIFF_RMK = @[diff_rmk]
WHERE BKG_NO = @[bkg_no]
AND	RC_SEQ = @[rc_seq]
AND NVL(SPCL_CGO_APRO_CD,' ') NOT IN ('R', 'Y', 'N')

#end			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="fdo_temp" type="12" value="" out="N"/>
				<param name="cdo_temp" type="12" value="" out="N"/>
				<param name="pwr_spl_cbl_flg" type="12" value="" out="N"/>
				<param name="cntr_vent_tp_cd" type="12" value="" out="N"/>
				<param name="vent_rto" type="12" value="" out="N"/>
				<param name="clng_tp_cd" type="12" value="" out="N"/>
				<param name="humid_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
