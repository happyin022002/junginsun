<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateGuidelineDBDAODestPriRgRtRoutPntVORSQL">
			<desc><![CDATA[Rate Origin Point조회]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
      ,GLINE_SEQ
      ,CMDT_HDR_SEQ
      ,ROUT_SEQ
      ,ORG_DEST_TP_CD
      ,ROUT_PNT_SEQ
      ,ROUT_PNT_LOC_TP_CD
      ,ROUT_PNT_LOC_DEF_CD
      ,DECODE(ROUT_PNT_LOC_TP_CD
             ,'G'
             ,(SELECT PRC_GRP_LOC_DESC
                FROM PRI_RG_GRP_LOC
               WHERE SVC_SCP_CD = A.SVC_SCP_CD
                 AND GLINE_SEQ = A.GLINE_SEQ
                 AND PRC_GRP_LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                 AND ROWNUM = 1)
             ,'L'
             ,(SELECT LOC_NM
                FROM MDM_LOCATION
               WHERE LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                 AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM
      ,PRC_TRSP_MOD_CD
      ,RCV_DE_TERM_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
  FROM PRI_RG_RT_ROUT_PNT A
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND GLINE_SEQ = @[gline_seq]
   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
   AND ROUT_SEQ = @[rout_seq]
   AND ORG_DEST_TP_CD = 'D'
 ORDER BY DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="2" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="rout_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
