<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPMasterDBDAOMultiHistVSLInfoChangeDateOfFormerSequenceUSQL">
			<desc><![CDATA[2016.03.03 Create Lane Table, Create Vessel Table history 자동 관리]]></desc>
			<sql><![CDATA[
update MAS_VSL_RGST
set VSL_APLY_TO_DT = SYSDATE - 1
where VSL_CD = @[vsl_cd]
#if (${update_flg} == 'Y')
and VSL_SEQ = (SELECT MAX(VSL_SEQ)
                FROM MAS_VSL_RGST
                WHERE VSL_CD = @[vsl_cd]) - 1
#else
and VSL_SEQ = (SELECT MAX(VSL_SEQ)
                FROM MAS_VSL_RGST
                WHERE VSL_CD = @[vsl_cd])
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
