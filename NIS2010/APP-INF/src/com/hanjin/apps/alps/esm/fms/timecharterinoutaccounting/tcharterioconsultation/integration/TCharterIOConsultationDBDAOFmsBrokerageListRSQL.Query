<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOFmsBrokerageListRSQL">
			<desc><![CDATA[Manual Slip â€“ Brokerage / Window Select]]></desc>
			<sql><![CDATA[
SELECT A.INV_DESC,
	   TO_CHAR(A.EFF_DT,'YYYYMMDD') EFF_DT,
	   TO_CHAR(A.EXP_DT,'YYYYMMDD') EXP_DT,
	   B.ACCT_ITM_NM,
	   A.ACCT_CD,
	   A.ACCT_ITM_SEQ,
	   A.CURR_CD,
	   A.INV_AMT,
	   '' VVD_CD,
	   A.FLET_CTRT_NO,
	   A.FLET_ISS_TP_CD,
       A.INV_SEQ,
       A.INV_DTL_SEQ
  FROM FMS_INV_DTL A,
  	   FMS_ACCT_ITM B
 WHERE A.ACCT_CD = B.ACCT_CD
   AND A.ACCT_ITM_SEQ = B.ACCT_ITM_SEQ
   AND A.BROG_ACCT_FLG = 'Y'
   AND A.SLP_TP_CD IS NULL
   AND CURR_CD = @[curr_cd]
 ORDER BY A.EFF_DT			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
