<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAODGCargoRSQL">
			<desc><![CDATA[Select]]></desc>
			<sql><![CDATA[
SELECT   OUT_IMDG_PCK_QTY1 PCK_QTY
,        OUT_IMDG_PCK_CD1 PCK_TP_CD
,        (SELECT   SUBSTR (IMDG_PCK_DESC
,                1
,                DECODE (INSTR (IMDG_PCK_DESC, ',', INSTR (IMDG_PCK_DESC, ',') + 1)
,                        0, LENGTH (IMDG_PCK_DESC)
,                        INSTR (IMDG_PCK_DESC, ',', INSTR (IMDG_PCK_DESC, ',') + 1) - 1
                 )
         )
FROM     SCG_IMDG_PCK_CD
WHERE    IMDG_PCK_CD = A.OUT_IMDG_PCK_CD1
) PCK_NM
,        IMDG_UN_NO
,        IMDG_CLSS_CD
,        PRP_SHP_NM
,        HZD_DESC
,        FLSH_PNT_CDO_TEMP
,        EMER_CNTC_PHN_NO_CTNT
FROM     BKG_DG_CGO A
WHERE    BKG_NO = @[bkg_no]
ORDER BY DCGO_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="BREZ2010010" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
