<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqListDBDAOsearchBbCntrListRSQL">
			<desc><![CDATA[PRI_SCQ_BB_CNTR 정보 조회]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD, CNTR_QTY
FROM PRI_SCQ_BB_CNTR
WHERE SCQ_RQST_NO = @[scq_rqst_no]
AND SCQ_VER_NO = @[scq_ver_no]
AND CNTR_GRP_VER_NO = ( SELECT  MAX ( CNTR_GRP_VER_NO ) 
                        FROM    PRI_SCQ_BB_CNTR
                        WHERE   SCQ_RQST_NO = @[scq_rqst_no]
                        AND     SCQ_VER_NO  = @[scq_ver_no] )			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
