      DECODE(CLL.STWG_CD, 'UT', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UT_40 ,
      DECODE(CLL.STWG_CD, 'UT', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UT_40H ,
      DECODE(CLL.STWG_CD, 'UT', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UT_45 ,
      
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UTAB_20 ,
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UTAB_40 ,
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UTAB_40H ,
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UTAB_45 ,
      
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UW_20 ,
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UW_40 ,
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UW_40H ,
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UW_45 ,
      VPS.CLPT_SEQ CLPT_SEQ
	FROM BKG_BOOKING BK
		, BKG_VVD BV
		, BKG_QUANTITY BQ
		, VSK_VSL_PORT_SKD VPS
		, MDM_COMMODITY CM
		, MDM_LOCATION LO
		, BKG_CSTMS_TML_KR_CLL CLL
	WHERE BK.BKG_NO = BV.BKG_NO
	  AND BK.BKG_NO = BQ.BKG_NO
	  AND BQ.CNTR_TPSZ_CD = CLL.CNTR_TPSZ_CD
	  AND BQ.CNTR_TPSZ_CD NOT IN ('Q2','Q4')
	  AND BV.VSL_CD = VPS.VSL_CD
	  AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
	  AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
	  AND BV.POD_CD = VPS.VPS_PORT_CD
	  AND BK.CMDT_CD = CM.CMDT_CD
	  AND VPS.VPS_PORT_CD = LO.LOC_CD
	  AND NVL(CM.DELT_FLG,'N')='N'
	  AND BV.VSL_CD = SUBSTR(@[in_vvd_cd],1,4)
  	  AND BV.SKD_VOY_NO = SUBSTR(@[in_vvd_cd],5,4)
  	  AND BV.SKD_DIR_CD = SUBSTR(@[in_vvd_cd],9,1)
  	  AND BV.POL_CD = @[in_pol_cd]
	  AND BK.BKG_STS_CD IN ( 'F','W')
	  AND VPS.CLPT_IND_SEQ = '1'
	  AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
	  AND CLL.CNTR_LIST_NO = BV.VSL_CD||SUBSTR(BV.SKD_VOY_NO,2,3)||BV.SKD_DIR_CD||SUBSTR(BV.POL_CD,3,3)
	  AND CLL.BKG_NO = BK.BKG_NO
	  AND BV.POD_CD = 'USLGB'    
	  AND CLL.BLCK_STWG_CD IN ('LOC','ONE','TWO','THR','FOR','FIV','HOT','TRS','SEV') 
 GROUP BY NVL(LO.UN_LOC_CD,BV.POD_CD), NVL(CLL.BLCK_STWG_CD,NVL(LO.UN_LOC_CD,BV.POD_CD)), CLL.BLCK_STWG_CD, CLL.STWG_CD, BQ.CNTR_TPSZ_CD, 1, CLL.KR_TML_PRCT_ID, BK.BKG_NO, VPS.CLPT_SEQ,CLL.CNTR_NO
  
  UNION ALL
  
	SELECT 
		NVL(LO.UN_LOC_CD,BV.POD_CD) GUBUN_CD3
		, NVL(CLL.BLCK_STWG_CD,NVL(LO.UN_LOC_CD,BV.POD_CD)) BLCK_STWG_CD
		, DECODE(NVL(LO.UN_LOC_CD,BV.POD_CD), 
											  'USOAK', '20', 
											  'USSEA', '30',
											  'CAVAN', '40',
											  '50') AA
		, DECODE(CLL.STWG_CD, 'PCOD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) PCOD_20,
      DECODE(CLL.STWG_CD, 'PCOD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) PCOD_40,
      DECODE(CLL.STWG_CD, 'PCOD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) PCOD_40H,
      DECODE(CLL.STWG_CD, 'PCOD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) PCOD_45,
      
      DECODE(CLL.STWG_CD, 'TS', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) TS_20 ,
      DECODE(CLL.STWG_CD, 'TS', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) TS_40 ,
      DECODE(CLL.STWG_CD, 'TS', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) TS_40H ,
       DECODE(CLL.STWG_CD, 'TS', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) TS_45 ,
       
      DECODE(CLL.STWG_CD, 'OLBP', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) OLBP_20 ,
      DECODE(CLL.STWG_CD, 'OLBP', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) OLBP_40 ,
      DECODE(CLL.STWG_CD, 'OLBP', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) OLBP_40H ,
      DECODE(CLL.STWG_CD, 'OLBP', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) OLBP_45 ,
      
      DECODE(CLL.STWG_CD, 'UD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UD_20 ,
      DECODE(CLL.STWG_CD, 'UD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UD_40 ,
      DECODE(CLL.STWG_CD, 'UD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UD_40H ,
      DECODE(CLL.STWG_CD, 'UD', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UD_45 ,
      
      DECODE(CLL.STWG_CD, 'UDAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UDAB_20 ,
      DECODE(CLL.STWG_CD, 'UDAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UDAB_40 ,
      DECODE(CLL.STWG_CD, 'UDAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UDAB_40H ,
      DECODE(CLL.STWG_CD, 'UDAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UDAB_45 ,

      DECODE(CLL.STWG_CD, 'UDMX', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 'UDAK', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UDMX_20 ,
      DECODE(CLL.STWG_CD, 'UDMX', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 'UDAK', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0),0) UDMX_40 ,
      DECODE(CLL.STWG_CD, 'UDMX', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 'UDAK', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1),0) UDMX_40H ,
      DECODE(CLL.STWG_CD, 'UDMX', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 'UDAK', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UDMX_45 ,

      DECODE(CLL.STWG_CD, 'UT', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UT_20 ,
      DECODE(CLL.STWG_CD, 'UT', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UT_40 ,
      DECODE(CLL.STWG_CD, 'UT', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UT_40H ,
      DECODE(CLL.STWG_CD, 'UT', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UT_45 ,
      
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UTAB_20 ,
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UTAB_40 ,
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UTAB_40H ,
      DECODE(CLL.STWG_CD, 'UTAB', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UTAB_45 ,
      
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) UW_20 ,
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) UW_40 ,
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0, 1), 0) UW_40H ,
      DECODE(CLL.STWG_CD, 'UW', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) UW_45 ,
      VPS.CLPT_SEQ CLPT_SEQ
	FROM BKG_BOOKING BK
		, BKG_VVD BV
		, BKG_QUANTITY BQ
		, VSK_VSL_PORT_SKD VPS
		, MDM_COMMODITY CM
		, MDM_LOCATION LO
		, BKG_CSTMS_TML_KR_CLL CLL
	WHERE BK.BKG_NO = BV.BKG_NO
	  AND BK.BKG_NO = BQ.BKG_NO
	  AND BQ.CNTR_TPSZ_CD = CLL.CNTR_TPSZ_CD
	  AND BQ.CNTR_TPSZ_CD NOT IN ('Q2','Q4')
	  AND BV.VSL_CD = VPS.VSL_CD
	  AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
	  AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
	  AND BV.POD_CD = VPS.VPS_PORT_CD
	  AND BK.CMDT_CD = CM.CMDT_CD
	  AND VPS.VPS_PORT_CD = LO.LOC_CD
	  AND NVL(CM.DELT_FLG,'N')='N'
	  AND BV.VSL_CD = SUBSTR(@[in_vvd_cd],1,4)
  	  AND BV.SKD_VOY_NO = SUBSTR(@[in_vvd_cd],5,4)
  	  AND BV.SKD_DIR_CD = SUBSTR(@[in_vvd_cd],9,1)
  	  AND BV.POL_CD = @[in_pol_cd]
	  AND SUBSTR(BV.POL_YD_CD,6,2) like @[in_pol_yd_cd]||'%'
	  AND BK.BKG_STS_CD IN ( 'F','W')
	  AND VPS.CLPT_IND_SEQ = '1'
	  AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
	  AND CLL.CNTR_LIST_NO = BV.VSL_CD||SUBSTR(BV.SKD_VOY_NO,2,3)||BV.SKD_DIR_CD||SUBSTR(BV.POL_CD,3,3)
	  AND CLL.BKG_NO = BK.BKG_NO
	  AND BV.POD_CD <> 'USLGB'        
 GROUP BY NVL(LO.UN_LOC_CD,BV.POD_CD), CLL.BLCK_STWG_CD, NVL(CLL.BLCK_STWG_CD,NVL(LO.UN_LOC_CD,BV.POD_CD)), CLL.STWG_CD, BQ.CNTR_TPSZ_CD, BQ.OP_CNTR_QTY, CLL.KR_TML_PRCT_ID, BK.BKG_NO, VPS.CLPT_SEQ,CLL.CNTR_NO
)
GROUP BY AA, GUBUN_CD3, BLCK_STWG_CD, CLPT_SEQ
ORDER BY CLPT_SEQ, AA, GUBUN_CD3
#end
)			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
