					DECODE(DECODE(BK.STWG_CD,'UW',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0, '7', 0,  BQ.OP_CNTR_QTY),0),0,'',DECODE(BK.STWG_CD,'UW',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '4', 0, '7', 0,  BQ.OP_CNTR_QTY),0)) UW_40H ,
                    DECODE(DECODE(BK.STWG_CD,'UW',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',BQ.OP_CNTR_QTY,0),0),0,'',DECODE(BK.STWG_CD,'UW',DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',BQ.OP_CNTR_QTY,0),0)) UW_45 ,
					VPS.CLPT_SEQ CLPT_SEQ
FROM BKG_BOOKING BK
, BKG_VVD BV
, BKG_QUANTITY BQ
, VSK_VSL_PORT_SKD VPS
, MDM_COMMODITY CM
, MDM_LOCATION LO
WHERE BK.BKG_NO = BV.BKG_NO
AND BK.BKG_NO = BQ.BKG_NO
AND BQ.CNTR_TPSZ_CD NOT IN ('Q2','Q4')
AND BV.VSL_CD = VPS.VSL_CD
AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
AND BV.POD_CD = VPS.VPS_PORT_CD
AND BK.CMDT_CD = CM.CMDT_CD
AND VPS.VPS_PORT_CD = LO.LOC_CD
AND NVL(CM.DELT_FLG,'N')='N'

	  AND BV.VSL_CD     = @[in_vsl_cd]
	  AND BV.SKD_VOY_NO = @[in_skd_voy_no]
	  AND BV.SKD_DIR_CD = @[in_skd_dir_cd]
	  AND BV.POL_CD     = @[in_pol_cd]

AND BK.BKG_STS_CD IN ( 'F','W')
AND VPS.CLPT_IND_SEQ = '1'
AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
AND BV.POD_CD = 'USLGB'
AND DECODE(NVL(BV.POD_CD,' '), 'USSEA', DECODE(BV.SLAN_CD,'PSX',DECODE(DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'ONE','SE1','SEA'),DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC')), 
    							'USOAK', DECODE(BK.BLCK_STWG_CD, 'OA1', 'OA1' , '', '', 'OAK'), 
    							'USLGB', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','7','SEV','T','TRS', 'LOC'), 
    							'CAVAN', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'') IN ('LOC','ONE','TWO','THR','FOR','FIV','HOT','TRS')
GROUP BY NVL(LO.UN_LOC_CD,BV.POD_CD), BK.STWG_CD, BQ.CNTR_TPSZ_CD, OP_CNTR_QTY, VPS.CLPT_SEQ, BK.BKG_NO

UNION ALL

SELECT
    NVL(LO.UN_LOC_CD,BV.POD_CD) GUBUN_CD3,
    MAX(DECODE(NVL(BV.POD_CD,' '), 'USSEA', DECODE(BV.SLAN_CD,'PSX',DECODE(DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'ONE','SE1','SEA'),DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC')), 
    							'USOAK', DECODE(BK.BLCK_STWG_CD, 'OA1', 'OA1' , '', '', 'OAK'), 
    							'USLGB', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','7','SEV','T','TRS', 'LOC'), 
