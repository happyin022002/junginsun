<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchItemInfoRSQL">
			<desc><![CDATA[호주 위험물 세관 플랫파일 전송시 ItemInfo 조회]]></desc>
			<sql><![CDATA[
SELECT 
     ''                         CONSOL_NO
    ,A.BL_NO                    SHIPPING_REF
    ,A.POL_CD                   POL_CD
    ,A.POD_CD                   POD_CD  
    ,''                         ITEM_NO    
    ,A.OUT_IMDG_PCK_QTY1        OUTPKG_QTY
    ,A.OUT_IMDG_PCK_CD1         OUTPKG_TP_ID
    ,SUBSTR(A.OUTR_PCK_DESC, 1 , 35 )  OUTPKG_TP    
    ,A.IN_IMDG_PCK_QTY1         INPKG_QTY
    ,SUBSTR(A.INR_PCK_DESC, 1, 35 )	   INPKG_TP
    ,'2'                        STOR_AGE
    ,SUBSTR(REPLACE(REPLACE(B.CUST_NM, CHR(13)||CHR(10),' '),CHR(10)||CHR(13),' '), 1 , 70)  PARTY1
   
    
    ,NVL(NVL(C.EMER_CNTC_PSON_NM,(SELECT EMER_CNTC_PSON_NM FROM BKG_DG_CGO WHERE BKG_NO = A.BL_NO AND CNTR_NO = A.CNTR_NO AND ROWNUM = 1)),'N/A')        PARTY2
    ,NVL(NVL(C.EMER_CNTC_PHN_NO_CTNT,(SELECT EMER_CNTC_PHN_NO_CTNT FROM BKG_DG_CGO WHERE BKG_NO = A.BL_NO AND CNTR_NO = A.CNTR_NO AND ROWNUM = 1)),'N/A')    PARTY3
    ,NVL(NVL(C.EMER_CNTC_PHN_NO_CTNT,(SELECT EMER_CNTC_PHN_NO_CTNT FROM BKG_DG_CGO WHERE BKG_NO = A.BL_NO AND CNTR_NO = A.CNTR_NO AND ROWNUM = 1)),'N/A')    PARTY4
    ,''                         PARTY5
    ,'IMD'                         DG_CD
    ,A.IMDG_CLSS_CD             IMDG_CLASS
    ,A.IMDG_UN_NO               UN_NO
    ,A.FLSH_PNT_CDO_TEMP        FLASH
    ,'CEL'                      FLASH_UNIT
    ,A.IMDG_PCK_GRP_CD          PKG_GROUP
    ,REPLACE(REPLACE(A.EMS_NO,',',''),' ','')  EMS_NO
    ,A.MFAG_NO                  MFAG
    ,A.PRP_SHP_NM               SHIPPING_NAME
    ,'KGM'                      NETWGT_UNIT
    ,A.NET_WGT                  NETWGT
    ,'KGM'                      GROSSWGT_UNIT
    ,A.GRS_WGT                  GROSSWGT
    ,'KGM'                      NEQWGT_UNIT
    ,A.NET_WGT                  NEQWGT
    ,A.CNTR_NO                  CNTR_NO

FROM BKG_CSTMS_DG A, BKG_CUSTOMER B, BKG_DG_CGO C

WHERE 1=1
AND A.BL_NO = C.BKG_NO(+)
AND A.CNTR_NO = C.CNTR_NO(+)
AND A.CNTR_CGO_SEQ = C.CNTR_CGO_SEQ(+)
AND A.BL_NO = B.BKG_NO
AND   A.DG_DECL_TP_CD = @[d_type]
AND   A.VSL_CD          = SUBSTR(@[vvd_cd], 1, 4)
AND   A.SKD_VOY_NO      = SUBSTR(@[vvd_cd], 5, 4)
AND   A.SKD_DIR_CD      = SUBSTR(@[vvd_cd], 9, 1)
AND   A.PORT_CD         = @[port_cd]
AND   A.BL_NO           = @[bl_no]
AND	  A.CNT_CD ='AU'
AND   B.BKG_CUST_TP_CD ='C'

ORDER BY A.CNTR_NO, A.CNTR_CGO_SEQ			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
