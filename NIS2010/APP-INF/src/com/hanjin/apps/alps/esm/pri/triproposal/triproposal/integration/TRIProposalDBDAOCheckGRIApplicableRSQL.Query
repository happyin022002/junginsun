<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOCheckGRIApplicableRSQL">
			<desc><![CDATA[GRI가 적용가능한지(진행중인게 있는지) 확인]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(A.TRI_NO, 1, 6) || '-' || SUBSTR(A.TRI_NO, 7, 4) || '-' || SUBSTR(A.TRI_NO, 11, 3) AS TRI_NO
  FROM PRI_TRI_MN A, PRI_TRI_RT B
 WHERE A.TRI_PROP_NO = B.TRI_PROP_NO
   AND A.TRF_PFX_CD = @[trf_pfx_cd]
   AND A.TRF_NO = @[trf_no]
   AND B.PROP_STS_CD = 'I'
   AND B.GRI_APPL_TP_CD = 'A'			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
