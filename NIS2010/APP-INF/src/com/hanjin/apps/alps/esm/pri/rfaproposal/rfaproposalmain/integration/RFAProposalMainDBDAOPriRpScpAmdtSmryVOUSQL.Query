<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpScpAmdtSmryVOUSQL">
			<desc><![CDATA[PriRpScpAmdtSmryVO]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_AMDT_SMRY UPD
SET 
    ( amdt_flg  ,acpt_flg ) = 
        (
            SELECT
				DECODE(count(src.amdt_seq),0,'N','','N','Y'), 
				DECODE(MAX(src.prc_prog_sts_cd),'A','Y','N')
            FROM
#if(${prop_scp_term_tp_cd} == "11") 
				pri_Rp_scp_dur src,
#elseif(${prop_scp_term_tp_cd} == "13") 
				pri_Rp_scp_grp_loc_dtl src,
#elseif(${prop_scp_term_tp_cd} == "14") 
				pri_Rp_scp_grp_cmdt_dtl src,
#elseif(${prop_scp_term_tp_cd} == "32") 
				PRI_RP_SCP_NOTE_CTNT src,
#elseif(${prop_scp_term_tp_cd} == "51" || ${prop_scp_term_tp_cd} == "52") 
				PRI_RP_SCP_TRSP_ADD_CHG src,
#elseif(${prop_scp_term_tp_cd} == "71" || ${prop_scp_term_tp_cd} == "73" ) 
                (SELECT PROP_NO,
			       AMDT_SEQ,
			       SVC_SCP_CD,
			       N1ST_CMNC_AMDT_SEQ,
			       PRC_PROG_STS_CD,
			       (SELECT X1.FIC_RT_TP_CD FROM PRI_RP_SCP_RT_CMDT_HDR X1 WHERE X1.PROP_NO = T1.PROP_NO AND X1.AMDT_SEQ = T1.AMDT_SEQ AND X1.SVC_SCP_CD = T1.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = T1.CMDT_HDR_SEQ) FIC_RT_TP_CD
			  FROM PRI_RP_SCP_RT_ACT_CUST T1
			UNION ALL
			SELECT PROP_NO,
			       AMDT_SEQ,
			       SVC_SCP_CD,
			       N1ST_CMNC_AMDT_SEQ,
			       PRC_PROG_STS_CD,
			(SELECT X1.FIC_RT_TP_CD FROM PRI_RP_SCP_RT_CMDT_HDR X1 WHERE X1.PROP_NO = T1.PROP_NO AND X1.AMDT_SEQ = T1.AMDT_SEQ AND X1.SVC_SCP_CD = T1.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = T1.CMDT_HDR_SEQ) FIC_RT_TP_CD
			  FROM PRI_RP_SCP_RT_CNOTE T1
			UNION ALL
			SELECT PROP_NO,
			       AMDT_SEQ,
			       SVC_SCP_CD,
			       N1ST_CMNC_AMDT_SEQ,
			       PRC_PROG_STS_CD,
			(SELECT X1.FIC_RT_TP_CD FROM PRI_RP_SCP_RT_CMDT_HDR X1 WHERE X1.PROP_NO = T1.PROP_NO AND X1.AMDT_SEQ = T1.AMDT_SEQ AND X1.SVC_SCP_CD = T1.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = T1.CMDT_HDR_SEQ) FIC_RT_TP_CD
			  FROM PRI_RP_SCP_RT_CMDT T1
			UNION ALL
			SELECT PROP_NO,
			       AMDT_SEQ,
			       SVC_SCP_CD,
			       N1ST_CMNC_AMDT_SEQ,
			       PRC_PROG_STS_CD,
			(SELECT X1.FIC_RT_TP_CD FROM PRI_RP_SCP_RT_CMDT_HDR X1 WHERE X1.PROP_NO = T1.PROP_NO AND X1.AMDT_SEQ = T1.AMDT_SEQ AND X1.SVC_SCP_CD = T1.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = T1.CMDT_HDR_SEQ) FIC_RT_TP_CD
			  FROM PRI_RP_SCP_RT T1
			UNION ALL
			SELECT PROP_NO,
			       AMDT_SEQ,
			       SVC_SCP_CD,
			       N1ST_CMNC_AMDT_SEQ,
			       PRC_PROG_STS_CD,
			(SELECT X1.FIC_RT_TP_CD FROM PRI_RP_SCP_RT_CMDT_HDR X1 WHERE X1.PROP_NO = T1.PROP_NO AND X1.AMDT_SEQ = T1.AMDT_SEQ AND X1.SVC_SCP_CD = T1.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = T1.CMDT_HDR_SEQ) FIC_RT_TP_CD
			  FROM PRI_RP_SCP_RT_CMDT_RNOTE T1
			UNION ALL
			SELECT PROP_NO,
			       AMDT_SEQ,
			       SVC_SCP_CD,
			       N1ST_CMNC_AMDT_SEQ,
			       PRC_PROG_STS_CD,
			(SELECT X1.FIC_RT_TP_CD FROM PRI_RP_SCP_RT_CMDT_HDR X1 WHERE X1.PROP_NO = T1.PROP_NO AND X1.AMDT_SEQ = T1.AMDT_SEQ AND X1.SVC_SCP_CD = T1.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = T1.CMDT_HDR_SEQ) FIC_RT_TP_CD
			  FROM PRI_RP_SCP_RT_ROUT_PNT T1
			UNION ALL
			SELECT PROP_NO,
			       AMDT_SEQ,
			       SVC_SCP_CD,
			       N1ST_CMNC_AMDT_SEQ,
			       PRC_PROG_STS_CD,
			(SELECT X1.FIC_RT_TP_CD FROM PRI_RP_SCP_RT_CMDT_HDR X1 WHERE X1.PROP_NO = T1.PROP_NO AND X1.AMDT_SEQ = T1.AMDT_SEQ AND X1.SVC_SCP_CD = T1.SVC_SCP_CD AND X1.CMDT_HDR_SEQ = T1.CMDT_HDR_SEQ) FIC_RT_TP_CD
			  FROM PRI_RP_SCP_RT_ROUT_VIA T1) SRC,
#end 
				pri_Rp_scp_mn mn
            where 
				src.prop_no 		= upd.prop_no
            and src.amdt_seq 		= upd.amdt_seq
            and src.svc_scp_cd 		= upd.svc_scp_cd
            and src.prop_no 		= mn.prop_no
            and src.amdt_seq 		= mn.amdt_seq
            and src.svc_scp_cd 		= upd.svc_scp_cd
			AND SRC.N1ST_CMNC_AMDT_SEQ 	= MN.AMDT_SEQ
#if(${prop_scp_term_tp_cd} == "32") 
            and src.note_tp_cd 	= 'P'
#elseif(${prop_scp_term_tp_cd} == "51") 
            and src.add_chg_tp_cd 	= 'A'
            and src.org_dest_tp_cd 	= 'O'
#elseif(${prop_scp_term_tp_cd} == "52") 
            and src.add_chg_tp_cd 	= 'A'
            and src.org_dest_tp_cd 	= 'D'
#elseif(${prop_scp_term_tp_cd} == "71") 
            and NVL(src.fic_rt_tp_cd, 'G') 	= 'G'
#elseif(${prop_scp_term_tp_cd} == "73") 
            and NVL(src.fic_rt_tp_cd, 'G') 	= 'A'
#end 
        )
WHERE
    prop_no    = @[prop_no]
AND amdt_seq   = @[amdt_seq]
AND svc_scp_cd = @[svc_scp_cd] 
AND prop_scp_term_tp_cd = @[prop_scp_term_tp_cd]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="prop_scp_term_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
