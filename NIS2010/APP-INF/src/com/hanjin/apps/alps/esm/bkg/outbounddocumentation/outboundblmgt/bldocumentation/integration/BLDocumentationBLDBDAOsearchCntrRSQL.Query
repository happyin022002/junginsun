<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOsearchCntrRSQL">
			<desc><![CDATA[컨테이너 콤보박스를 위한 쿼리]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO val     
      ,CNTR_TPSZ_CD
      ,CNTR_VOL_QTY
      ,RCV_TERM_CD
      ,DE_TERM_CD
  FROM BKG_CONTAINER
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
