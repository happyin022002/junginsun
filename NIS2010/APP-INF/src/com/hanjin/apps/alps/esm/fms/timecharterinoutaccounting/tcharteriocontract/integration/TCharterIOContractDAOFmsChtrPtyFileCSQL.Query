<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOContractDAOFmsChtrPtyFileCSQL">
			<desc><![CDATA[TCharterIOContractDAOFmsChtrPtyFileCSQL]]></desc>
			<sql><![CDATA[
insert into fms_chtr_pty_file (
	flet_ctrt_no,
	file_seq,
	flet_file_tp_cd,
	file_nm,
	file_desc,
	file_sav_id,
	eff_dt,
	exp_dt,
	cre_usr_id,
	cre_dt,
	upd_usr_id,
	upd_dt
) values( 
	@[flet_ctrt_no],
	FMS_CNTR_PTY_SEQ.NEXTVAL,
	@[flet_file_tp_cd],
	@[file_nm],
	@[file_desc],
	@[file_sav_id],
	DECODE(@[eff_dt],NULL,NULL,TO_DATE(@[eff_dt],'YYYYMMDD')),
	DECODE(@[exp_dt],NULL,NULL,TO_DATE(@[exp_dt],'YYYYMMDD')),
	@[cre_usr_id],
	sysdate,
	@[upd_usr_id],
	sysdate
)			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="flet_file_tp_cd" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_desc" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
