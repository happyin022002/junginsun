<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAORsltPriSurchargeViewAllVORSQL">
			<desc><![CDATA[   S/C Proposal/Amendment Surcharge View All]]></desc>
			<sql><![CDATA[

SELECT 
	A.PROP_NO
	,A.AMDT_SEQ
	,A.SVC_SCP_CD
	,A.GEN_SPCL_RT_TP_CD
	,A.CMDT_HDR_SEQ
	,A.ROUT_SEQ
	,RT.RT_SEQ
	,I.PRC_CMDT_DEF_NM
	,DECODE(A.GEN_SPCL_RT_TP_CD,'G','Y','N') AS G_GEN_SPCL_RT_TP_CD
	,DECODE(A.GEN_SPCL_RT_TP_CD,'S','Y','N') AS S_GEN_SPCL_RT_TP_CD
	,NVL(ACT_CUST.CUST_LGL_ENG_NM,' ') AS ACT_CUST_NM
	,NVL(B.ROUT_PNT_LOC_DEF_CD ,' ') AS ORG_ROUT_PNT_LOC_DEF_CD
	,NVL(C.ROUT_VIA_PORT_DEF_CD ,' ') AS ORG_ROUT_VIA_PORT_DEF_CD
	,NVL(D.ROUT_VIA_PORT_DEF_CD ,' ') AS DEST_ROUT_VIA_PORT_DEF_CD
	,NVL(E.ROUT_PNT_LOC_DEF_CD ,' ') AS DEST_ROUT_PNT_LOC_DEF_CD

	,NVL(REPLACE(F.GRI_NOTE_CTNT      , '^|^', '' ) ,' ')   AS GRI_RNOTE_CTNT       
	,NVL(REPLACE(F.SURCHARGE_NOTE_CTNT, '^|^', '' ) ,' ')   AS SURCHARGE_RNOTE_CTNT 
	,NVL(REPLACE(F.DEMDET_NOTE_CTNT   , '^|^', '' ) ,' ')   AS DEMDET_RNOTE_CTNT    
	,NVL(REPLACE(F.OTHERS_NOTE_CTNT   , '^|^', '' ) ,' ')   AS OTHERS_RNOTE_CTNT    

	,NVL(REPLACE(H.GRI_NOTE_CTNT      , '^|^', '' ) ,' ')   AS GRI_CNOTE_CTNT       
	,NVL(REPLACE(H.SURCHARGE_NOTE_CTNT, '^|^', '' ) ,' ')   AS SURCHARGE_CNOTE_CTNT 
	,NVL(REPLACE(H.DEMDET_NOTE_CTNT   , '^|^', '' ) ,' ')   AS DEMDET_CNOTE_CTNT    
	,NVL(REPLACE(H.OTHERS_NOTE_CTNT   , '^|^', '' ) ,' ')   AS OTHERS_CNOTE_CTNT    

	,DECODE(G.DIR_CALL_FLG,'Y','YES','NO') AS DIR_CALL_FLG
	,RT.RAT_UT_CD
	,RT.PRC_CGO_TP_CD
	,RT.CURR_CD AS RATE_CURR_CD
	
	,DECODE(RT.PRC_PROG_STS_CD, 'R', RT.COFFR_FRT_RT_AMT, RT.PROP_FRT_RT_AMT) AS  FRT_RT_AMT
	
	, NVL( SCG.POR_CD ,' ') AS POR_CD
	, NVL( SCG.POL_CD ,' ') AS POL_CD
	, NVL( SCG.POD_CD ,' ') AS POD_CD
	, NVL( SCG.DEL_CD ,' ') AS DEL_CD
	, SCG.TOT_SURCHARGE
	, SCG.CRE_YMD 
	, SCG.CHG_CD 
	, SCG.BKG_RAT_UT_CD
	, SCG.CURR_CD AS SURCHARGE_CURR_CD
	, SCG.TRF_SCG_AMT
	, SCG.ADJ_SCG_AMT
	, SCG.ADJ_SCG_USD_AMT
	,A.NOTE_DP_SEQ
	,A.N1ST_CMNC_AMDT_SEQ
	,A.N1ST_CMNC_AMDT_SEQ AS ORG_N1ST_CMNC_AMDT_SEQ
	,A.CRE_USR_ID
	,A.CRE_DT
	,A.UPD_USR_ID
	,A.UPD_DT
	, DENSE_RANK() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD ORDER BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ ) AS SEQ
  FROM PRI_SP_SCP_RT_CMDT_ROUT A
      ,(
		SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0), 0)) AS UP_AC_CNT
		      ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD ||  DECODE(RCV_DE_TERM_CD, NULL,'', '(' || TERM_NM || ')' ), ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
		      ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_PNT_LOC_DEF_CD 
			      ,ROUT_PNT_LOC_TP_CD
			      ,RCV_DE_TERM_CD
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
			      ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL CODE WHERE CODE.INTG_CD_ID = 'CD02070' AND INTG_CD_VAL_CTNT = RCV_DE_TERM_CD) as TERM_NM 
			  FROM PRI_SP_SCP_RT_ROUT_PNT
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND ORG_DEST_TP_CD = 'O'
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD
	) B
      ,(
		SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0), 0)) AS UP_AC_CNT
		      ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
		      ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_VIA_PORT_DEF_CD
			      ,ROUT_VIA_PORT_TP_CD
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
			  FROM PRI_SP_SCP_RT_ROUT_VIA
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND ORG_DEST_TP_CD = 'O'
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD	
	) C
      ,(
		SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0), 0)) AS UP_AC_CNT
		      ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD  , ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
		      ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_VIA_PORT_DEF_CD
			      ,ROUT_VIA_PORT_TP_CD
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
			  FROM PRI_SP_SCP_RT_ROUT_VIA
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND ORG_DEST_TP_CD = 'D'
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD
	) D
      ,(
	      SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', 0, 1)) AS NA_CNT
		      ,SUM(DECODE(PRC_PROG_STS_CD, 'A', DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0), 0)) AS UP_AC_CNT
		      ,SUM(DECODE(AMDT_SEQ, N1ST_CMNC_AMDT_SEQ, 1, 0)) AS UP_CNT
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD ||  DECODE(RCV_DE_TERM_CD, NULL,'', '(' || TERM_NM || ')' ) , ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
		      ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_PNT_LOC_DEF_CD
			      ,ROUT_PNT_LOC_TP_CD
			      ,RCV_DE_TERM_CD
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
			       ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL CODE WHERE CODE.INTG_CD_ID = 'CD02070' AND INTG_CD_VAL_CTNT = RCV_DE_TERM_CD) as TERM_NM 
			  FROM PRI_SP_SCP_RT_ROUT_PNT
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND ORG_DEST_TP_CD = 'D'
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD
	) E
      ,(
		SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'G',NOTE_CTNT,''), '^|^')), 4)   AS GRI_NOTE_CTNT
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'S',NOTE_CTNT,''), '^|^')), 4)   AS SURCHARGE_NOTE_CTNT
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'D',NOTE_CTNT,''), '^|^')), 4)   AS DEMDET_NOTE_CTNT
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'O',NOTE_CTNT,''), '^|^')), 4)   AS OTHERS_NOTE_CTNT
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      --,SUBSTRB(NOTE_CTNT,0,497)|| CHR(13) || CASE WHEN LENGTHB(NOTE_CTNT)<497 THEN '' ELSE '...' END AS NOTE_CTNT
			      ,NOTE_CTNT || CHR(13)  AS NOTE_CTNT
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      , NOTE_CLSS_CD
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, DECODE(NOTE_CLSS_CD, 'G', '1', 'S', '2', 'D', '3', 'O', '4')) AS RN
			  FROM PRI_SP_SCP_RT_CMDT_RNOTE
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
	) F
      ,(
		SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'G',NOTE_CTNT,''), '^|^')), 4)   AS GRI_NOTE_CTNT
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'S',NOTE_CTNT,''), '^|^')), 4)   AS SURCHARGE_NOTE_CTNT
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'D',NOTE_CTNT,''), '^|^')), 4)   AS DEMDET_NOTE_CTNT
		      , SUBSTR( MAX(SYS_CONNECT_BY_PATH(DECODE(NOTE_CLSS_CD,'O',NOTE_CTNT,''), '^|^')), 4)   AS OTHERS_NOTE_CTNT
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			       --,SUBSTRB(NOTE_CTNT,0,497)|| CHR(13) || CASE WHEN LENGTHB(NOTE_CTNT)<497 THEN '' ELSE '...' END AS NOTE_CTNT
			      ,NOTE_CTNT || CHR(13)  AS NOTE_CTNT
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      , NOTE_CLSS_CD
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, DECODE(NOTE_CLSS_CD, 'G', '1', 'S', '2', 'D', '3', 'O', '4')) AS RN
			  FROM PRI_SP_SCP_RT_CNOTE
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
	) H
      ,(
		SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(PRC_CMDT_DEF_NM, ' / ')), 4) AS PRC_CMDT_DEF_NM
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			      ,DECODE(PRC_CMDT_TP_CD
				     ,'G'
				     ,(SELECT PRC_GRP_CMDT_DESC
					FROM PRI_SP_SCP_GRP_CMDT
				       WHERE PROP_NO = A.PROP_NO
					 AND AMDT_SEQ = A.AMDT_SEQ
					 AND SVC_SCP_CD = A.SVC_SCP_CD
					 AND PRC_GRP_CMDT_CD = A.PRC_CMDT_DEF_CD
					 AND ROWNUM = 1)
				     ,'C'
				     ,(SELECT CMDT_NM
					FROM MDM_COMMODITY
				       WHERE CMDT_CD = A.PRC_CMDT_DEF_CD
					 AND ROWNUM = 1)) AS PRC_CMDT_DEF_NM
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, DECODE(PRC_CMDT_TP_CD, 'G', '1', 'C', '2'), PRC_CMDT_DEF_CD) AS RN
			  FROM PRI_SP_SCP_RT_CMDT A
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
      ) I
      , (
		SELECT 	PRI_ROUT.PROP_NO, PRI_ROUT.AMDT_SEQ, PRI_ROUT.SVC_SCP_CD, PRI_ROUT.GEN_SPCL_RT_TP_CD
			, PRI_ROUT.CMDT_HDR_SEQ, PRI_ROUT.ROUT_SEQ, PRI_ROUT.RT_SEQ
			, PRI_ROUT.POR_CD, PRI_ROUT.POL_CD, PRI_ROUT.POD_CD, PRI_ROUT.DEL_CD
			, TO_CHAR(PRI_ROUT.CRE_DT,'YYYY-MM-DD') CRE_YMD 
			, PRI_SCG.CHG_CD 
			, PRI_SCG.BKG_RAT_UT_CD
			, PRI_SCG.CURR_CD
			, PRI_SCG.TRF_SCG_AMT
			, PRI_SCG.ADJ_SCG_AMT
			, PRI_SCG.ADJ_SCG_USD_AMT
			, SUM( PRI_SCG.ADJ_SCG_USD_AMT ) OVER( PARTITION BY PRI_ROUT.PROP_NO, PRI_ROUT.AMDT_SEQ, PRI_ROUT.SVC_SCP_CD, PRI_ROUT.GEN_SPCL_RT_TP_CD, PRI_ROUT.CMDT_HDR_SEQ, PRI_ROUT.ROUT_SEQ, PRI_ROUT.RT_SEQ ) AS TOT_SURCHARGE
		FROM PRI_SP_SCP_RT_SCG_ROUT PRI_ROUT
			, PRI_SP_SCP_RT_SCG PRI_SCG
		WHERE
			PRI_ROUT.PROP_NO = PRI_SCG.PROP_NO
			AND PRI_ROUT.AMDT_SEQ = PRI_SCG.AMDT_SEQ
			AND PRI_ROUT.SVC_SCP_CD = PRI_SCG.SVC_SCP_CD
			AND PRI_ROUT.GEN_SPCL_RT_TP_CD = PRI_SCG.GEN_SPCL_RT_TP_CD
			AND PRI_ROUT.CMDT_HDR_SEQ = PRI_SCG.CMDT_HDR_SEQ
			AND PRI_ROUT.ROUT_SEQ = PRI_SCG.ROUT_SEQ
			AND PRI_ROUT.RT_SEQ = PRI_SCG.RT_SEQ
			AND PRI_ROUT.PROP_NO = @[prop_no]
			AND PRI_ROUT.AMDT_SEQ =@[amdt_seq]
			AND PRI_ROUT.SVC_SCP_CD = @[svc_scp_cd]
			AND PRI_ROUT.GEN_SPCL_RT_TP_CD = 'G'

      ) SCG
      , (      
		SELECT PROP_NO
		      ,AMDT_SEQ
		      ,SVC_SCP_CD
		      ,GEN_SPCL_RT_TP_CD
		      ,CMDT_HDR_SEQ
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(CUST_LGL_ENG_NM, '^|^')), 4) AS CUST_LGL_ENG_NM
		  FROM (SELECT PROP_NO
			      ,AMDT_SEQ
			      ,SVC_SCP_CD
			      ,GEN_SPCL_RT_TP_CD
			      ,CMDT_HDR_SEQ
			      ,(SELECT CUST_LGL_ENG_NM
				  FROM MDM_CUSTOMER
				 WHERE CUST_CNT_CD = A.CUST_CNT_CD
				   AND CUST_SEQ = A.CUST_SEQ
				   AND ROWNUM = 1) AS CUST_LGL_ENG_NM
			      ,PRC_PROG_STS_CD
			      ,SRC_INFO_CD
			      ,N1ST_CMNC_AMDT_SEQ
			      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ACT_CUST_SEQ) AS RN
			  FROM PRI_SP_SCP_RT_ACT_CUST A
			 WHERE PROP_NO = @[prop_no]
			   AND AMDT_SEQ = @[amdt_seq]
			   AND SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
			   AND SRC_INFO_CD <> 'AD' )
		 START WITH RN = 1
		CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY PROP_NO
			 ,AMDT_SEQ
			 ,SVC_SCP_CD
			 ,GEN_SPCL_RT_TP_CD
			 ,CMDT_HDR_SEQ
      ) ACT_CUST
      , PRI_SP_SCP_RT_ROUT_DIR G
      , PRI_SP_SCP_RT RT
 WHERE  I.PROP_NO = A.PROP_NO
	AND I.AMDT_SEQ = A.AMDT_SEQ
	AND I.SVC_SCP_CD = A.SVC_SCP_CD
	AND I.GEN_SPCL_RT_TP_CD = A.GEN_SPCL_RT_TP_CD
	AND I.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ

	AND A.PROP_NO = B.PROP_NO
	AND A.AMDT_SEQ = B.AMDT_SEQ
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.GEN_SPCL_RT_TP_CD = B.GEN_SPCL_RT_TP_CD
	AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
	AND A.ROUT_SEQ = B.ROUT_SEQ



	AND A.PROP_NO = RT.PROP_NO
	AND A.AMDT_SEQ = RT.AMDT_SEQ
	AND A.SVC_SCP_CD = RT.SVC_SCP_CD
	AND A.GEN_SPCL_RT_TP_CD = RT.GEN_SPCL_RT_TP_CD
	AND A.CMDT_HDR_SEQ = RT.CMDT_HDR_SEQ
	AND A.ROUT_SEQ = RT.ROUT_SEQ


	AND RT.PROP_NO = SCG.PROP_NO(+)
	AND RT.AMDT_SEQ = SCG.AMDT_SEQ(+)
	AND RT.SVC_SCP_CD = SCG.SVC_SCP_CD(+)
	AND RT.GEN_SPCL_RT_TP_CD = SCG.GEN_SPCL_RT_TP_CD(+)
	AND RT.CMDT_HDR_SEQ = SCG.CMDT_HDR_SEQ(+)
	AND RT.ROUT_SEQ = SCG.ROUT_SEQ(+)
	AND RT.RT_SEQ = SCG.RT_SEQ(+)

	AND I.PROP_NO = ACT_CUST.PROP_NO(+)
	AND I.AMDT_SEQ = ACT_CUST.AMDT_SEQ(+)
	AND I.SVC_SCP_CD = ACT_CUST.SVC_SCP_CD(+)
	AND I.GEN_SPCL_RT_TP_CD = ACT_CUST.GEN_SPCL_RT_TP_CD(+)
	AND I.CMDT_HDR_SEQ = ACT_CUST.CMDT_HDR_SEQ(+)



	AND A.PROP_NO = C.PROP_NO(+)
	AND A.AMDT_SEQ = C.AMDT_SEQ(+)
	AND A.SVC_SCP_CD = C.SVC_SCP_CD(+)
	AND A.GEN_SPCL_RT_TP_CD = C.GEN_SPCL_RT_TP_CD(+)
	AND A.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ(+)
	AND A.ROUT_SEQ = C.ROUT_SEQ(+)
	AND A.PROP_NO = D.PROP_NO(+)
	AND A.AMDT_SEQ = D.AMDT_SEQ(+)
	AND A.SVC_SCP_CD = D.SVC_SCP_CD(+)
	AND A.GEN_SPCL_RT_TP_CD = D.GEN_SPCL_RT_TP_CD(+)
	AND A.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
	AND A.ROUT_SEQ = D.ROUT_SEQ(+)
	AND A.PROP_NO = E.PROP_NO
	AND A.AMDT_SEQ = E.AMDT_SEQ
	AND A.SVC_SCP_CD = E.SVC_SCP_CD
	AND A.GEN_SPCL_RT_TP_CD = E.GEN_SPCL_RT_TP_CD
	AND A.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
	AND A.ROUT_SEQ = E.ROUT_SEQ
	AND A.PROP_NO = F.PROP_NO(+)
	AND A.AMDT_SEQ = F.AMDT_SEQ(+)
	AND A.SVC_SCP_CD = F.SVC_SCP_CD(+)
	AND A.GEN_SPCL_RT_TP_CD = F.GEN_SPCL_RT_TP_CD(+)
	AND A.CMDT_HDR_SEQ = F.CMDT_HDR_SEQ(+)
	AND A.ROUT_SEQ = F.ROUT_SEQ(+)
	AND A.PROP_NO = G.PROP_NO(+)
	AND A.AMDT_SEQ = G.AMDT_SEQ(+)
	AND A.SVC_SCP_CD = G.SVC_SCP_CD(+)
	AND A.GEN_SPCL_RT_TP_CD = G.GEN_SPCL_RT_TP_CD(+)
	AND A.CMDT_HDR_SEQ = G.CMDT_HDR_SEQ(+)
	AND A.ROUT_SEQ = G.ROUT_SEQ(+)
	AND A.PROP_NO = H.PROP_NO(+)
	AND A.AMDT_SEQ = H.AMDT_SEQ(+)
	AND A.SVC_SCP_CD = H.SVC_SCP_CD(+)
	AND A.GEN_SPCL_RT_TP_CD = H.GEN_SPCL_RT_TP_CD(+)
	AND A.CMDT_HDR_SEQ = H.CMDT_HDR_SEQ(+)
	AND A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.SVC_SCP_CD = @[svc_scp_cd]
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
			   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end
 ORDER BY  A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ
 ,A.GEN_SPCL_RT_TP_CD
	,I.PRC_CMDT_DEF_NM || A.CMDT_HDR_SEQ
	,B.ROUT_PNT_LOC_DEF_CD || A.ROUT_SEQ
	,C.ROUT_VIA_PORT_DEF_CD || A.ROUT_SEQ
	,D.ROUT_VIA_PORT_DEF_CD || A.ROUT_SEQ
	,E.ROUT_PNT_LOC_DEF_CD  || A.ROUT_SEQ
	,G.DIR_CALL_FLG  
	,RT.RAT_UT_CD    || RT.RT_SEQ
	,RT.PRC_CGO_TP_CD || RT.RT_SEQ
	,RT.CURR_CD       || RT.RT_SEQ
	, SCG.POR_CD
	, SCG.POL_CD
	, SCG.POD_CD
	, SCG.DEL_CD
	, SCG.CHG_CD			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
