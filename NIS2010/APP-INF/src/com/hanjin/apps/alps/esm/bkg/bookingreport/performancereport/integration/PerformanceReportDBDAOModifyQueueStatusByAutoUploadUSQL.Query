<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOModifyQueueStatusByAutoUploadUSQL">
			<desc><![CDATA[Web SI Auto Upload 처리된 Queue의 Input End Status를 업데이트]]></desc>
			<sql><![CDATA[
UPDATE BKG_SR_CRNT_RQST R 
SET SR_WRK_STS_USR_ID = 'HOMEPAGE'
   ,DPCS_DOC_FM_DT = GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,GLOBALDATE_PKG.GET_LOCCD_FNC('PKGSA'))
   ,BL_DOC_INP_FLG = 'Y'
   ,BL_DOC_INP_USR_ID = 'HOMEPAGE'
   ,BL_DOC_INP_ST_DT =	DPCS_DOC_FM_DT
   ,BL_DOC_INP_DT = GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,GLOBALDATE_PKG.GET_LOCCD_FNC('PKGSA'))
   ,BL_DOC_INP_HRS =  (SELECT SUM(SR_PROC_HRS) FROM BKG_SR_HIS H WHERE H.SR_KND_CD=R.SR_KND_CD AND H.SR_NO = R.SR_NO AND H.BKG_NO = R.BKG_NO AND H.SR_STS_CD = 'ID')
   ,CRNT_DT = GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,GLOBALDATE_PKG.GET_LOCCD_FNC('PKGSA'))
   ,UPD_USR_ID = 'HOMEPAGE'
   ,UPD_DT = SYSDATE	
WHERE BKG_NO = (SELECT BKG_NO 
                  FROM BKG_XTER_RQST_MST 
                  WHERE XTER_SNDR_ID = @[xter_sndr_id]
                  AND XTER_RQST_NO = @[xter_rqst_no]
                  AND XTER_RQST_SEQ = @[xter_rqst_seq])
AND R.XTER_SNDR_ID = @[xter_sndr_id]
AND R.XTER_RQST_NO = @[xter_rqst_no]
AND R.XTER_RQST_SEQ = @[xter_rqst_seq]			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
