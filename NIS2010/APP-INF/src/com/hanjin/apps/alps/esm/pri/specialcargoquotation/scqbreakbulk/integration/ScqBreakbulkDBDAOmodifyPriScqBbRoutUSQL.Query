<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqBreakbulkDBDAOmodifyPriScqBbRoutUSQL">
			<desc><![CDATA[modifyPriScqBbRout]]></desc>
			<sql><![CDATA[
UPDATE PRI_SCQ_BB_ROUT
   SET ROUT_TP_CD      = @[rout_tp_cd]   
     , ROUT_RMK        = @[rout_rmk] 
     , LOCL_CURR_CD    = @[locl_curr_cd]     
     , CFM_FLG         = @[cfm_flg]         
     , UPD_USR_ID      = @[upd_usr_id]    
     , UPD_DT          = SYSDATE        
 WHERE SCQ_RQST_NO     = @[scq_rqst_no]
   AND SCQ_VER_NO      = @[scq_ver_no]
   AND ROUT_SEQ        = @[rout_seq]
   AND ROUT_SEQ_VER_NO = @[rout_seq_ver_no]			]]></sql>
			<params>
				<param name="rout_tp_cd" type="12" value="" out="N"/>
				<param name="rout_rmk" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="rout_seq_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
