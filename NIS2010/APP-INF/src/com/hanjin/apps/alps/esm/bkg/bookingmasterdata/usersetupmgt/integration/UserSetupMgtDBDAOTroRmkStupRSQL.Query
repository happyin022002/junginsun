<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOTroRmkStupRSQL">
			<desc><![CDATA[TroRmkStup]]></desc>
			<sql><![CDATA[
SELECT 
      BKG_TRO_OFC_CD
,     DIFF_RMK
,     IO_BND_CD 
FROM   BKG_TRO_STUP
WHERE  1 = 1
#if (${bkg_tro_ofc_cd} != '') 
AND    BKG_TRO_OFC_CD = @[bkg_tro_ofc_cd]
#end			]]></sql>
			<params>
				<param name="bkg_tro_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
