<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOManageSPCHJSSalesAmountUSQL">
			<desc><![CDATA[SPC_INCOME 컬럼 수정]]></desc>
			<sql><![CDATA[
UPDATE	BSA_VVD_MST
SET		INCM_BZC_CHTR_AMT = @[spc_income]
							- (SELECT	INCM_SUB_CHTR_AMT
							   FROM		BSA_VVD_MST
							   WHERE	1 = 1
							   AND		TRD_CD		= @[trd_cd]
							   AND		RLANE_CD	= @[rlane_cd]
							   AND		VSL_CD		= @[vsl_cd]
							   AND		SKD_VOY_NO	= @[skd_voy_no]
							   AND		SKD_DIR_CD	= @[dir_cd])
							- (SELECT	INCM_CRS_CHTR_AMT
							   FROM		BSA_VVD_MST
							   WHERE	1 = 1
							   AND		TRD_CD		= @[trd_cd]
							   AND		RLANE_CD	= @[rlane_cd]
							   AND		VSL_CD		= @[vsl_cd]
							   AND		SKD_VOY_NO	= @[skd_voy_no]
							   AND		SKD_DIR_CD	= @[dir_cd])
WHERE	1 = 1
AND		TRD_CD		= @[trd_cd]
AND		RLANE_CD	= @[rlane_cd]
AND		VSL_CD		= @[vsl_cd]
AND		SKD_VOY_NO	= @[skd_voy_no]
AND		SKD_DIR_CD	= @[dir_cd]			]]></sql>
			<params>
				<param name="spc_income" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
