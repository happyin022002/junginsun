<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchRcvHistDtlRSQL">
			<desc><![CDATA[Korea에서 입/출항 선박 신고 적하목록 전송 문서의 상세내역 조회]]></desc>
			<sql><![CDATA[
SELECT RDTL.EDI_RCV_MSG AS EDI_RCV_MSG
  FROM BKG_CSTMS_KR_RCV_LOG RLOG, BKG_CSTMS_KR_RCV_LOG_DTL RDTL
 WHERE 1=1
   AND RLOG.FLT_FILE_REF_NO = RDTL.FLT_FILE_REF_NO
   AND RLOG.RCV_SEQ = RDTL.RCV_SEQ
   AND RLOG.RCV_DT = RDTL.RCV_DT
   AND RLOG.MSG_LOG_TP_CD = @[msg_log_tp_id]
   AND RLOG.RCV_DT = TO_DATE(@[rcv_dt], 'YYYY-MM-DD hh24:mi:ss')
   AND RLOG.RCV_SEQ = @[rcv_seq]
   AND RLOG.FLT_FILE_REF_NO = @[flt_file_ref_no]
#if(${msg_log_tp_id} == 'SCA' || ${msg_log_tp_id} == '5LI' || ${msg_log_tp_id} == '5LK')
   AND RLOG.SMT_AMD_NO = @[smt_amd_no]
#end
 ORDER BY RDTL.LOG_DTL_SEQ			]]></sql>
			<params>
				<param name="msg_log_tp_id" type="12" value="" out="N"/>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="rcv_seq" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="smt_amd_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
