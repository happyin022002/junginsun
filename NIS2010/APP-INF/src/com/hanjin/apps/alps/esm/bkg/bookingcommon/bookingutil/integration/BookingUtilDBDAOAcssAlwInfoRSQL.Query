<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOAcssAlwInfoRSQL">
			<desc><![CDATA[화면안에서 security control]]></desc>
			<sql><![CDATA[
SELECT  COUNT(1) CNT
FROM   BKG_HRD_CDG_CTNT
WHERE  HRD_CDG_ID = 'SCR_ROLE_DEF' 
AND    ATTR_CTNT1 = @[attr_ctnt1]
AND    ATTR_CTNT2 = @[attr_ctnt2]
AND    ATTR_CTNT3 = @[attr_ctnt3]
AND	   ATTR_CTNT4 = 'Y'			]]></sql>
			<params>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="attr_ctnt3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
