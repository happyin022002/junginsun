<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOsearchKrWhfLocCdListRSQL">
			<desc><![CDATA[S]]></desc>
			<sql><![CDATA[
SELECT LOC_CD, UN_LOC_ID
FROM BKG_CSTMS_LOC
WHERE 1=1

#if (${loc_cd} != '') 
AND LOC_CD    = @[loc_cd] 
#end

#if (${un_loc_id} != '') 
AND UN_LOC_ID = @[un_loc_id]
#end
			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="un_loc_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
