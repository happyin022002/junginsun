<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOPriRqGrpCmdtVORSQL">
			<desc><![CDATA[PRI_RQ_GRP_CMDT 테이블 조회]]></desc>
			<sql><![CDATA[
SELECT A.GRP_CMDT_SEQ AS SEQ
	 , A.PRC_GRP_CMDT_CD AS CD
     , A.PRC_GRP_CMDT_DESC AS NM
  FROM PRI_RQ_GRP_CMDT A
     , (SELECT QTTN_NO
             , QTTN_VER_NO
             , GRP_CMDT_SEQ
          FROM PRI_RQ_GRP_CMDT_DTL
         WHERE QTTN_NO = @[qttn_no]
           AND QTTN_VER_NO = @[qttn_ver_no]
           AND SRC_INFO_CD <> 'AD'
       GROUP BY QTTN_NO, QTTN_VER_NO, GRP_CMDT_SEQ) B
 WHERE A.QTTN_NO = B.QTTN_NO
   AND A.QTTN_VER_NO = B.QTTN_VER_NO
   AND A.GRP_CMDT_SEQ = B.GRP_CMDT_SEQ
ORDER BY A.PRC_GRP_CMDT_CD			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
