<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOaddDoRlseByCntrCSQL">
			<desc><![CDATA[addDoRlseByCntr]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_DO_CNTR
(
    BKG_NO
   ,RLSE_SEQ
   ,CNTR_NO
   ,TRKR_NM
   ,TRKR_PHN_NO
   ,TRKR_MVMT_REF_NO
   ,TRKR_MTY_RTN_YD_CD
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
) 
VALUES
(
       @[bkg_no]                --  AS BKG_NO
      ,@[rlse_seq]              --  AS RLSE_SEQ
      ,@[cntr_no]               --  AS CNTR_NO
      ,null                     --  AS TRKR_NM     
      ,null                     --  AS TRKR_PHN_NO       
      ,null                     --  AS TRKR_MVMT_REF_NO       
      ,null                     --  AS TRKR_MTY_RTN_YD_CD
      ,@[cre_usr_id]            --  AS CRE_USR_ID
      ,SYSDATE                  --  AS CRE_DT
      ,@[upd_usr_id]            --  AS UPD_USR_ID
      ,SYSDATE                  --  AS UPD_DT   
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rlse_seq" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
