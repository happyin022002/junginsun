<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CRMSalesLeadDBDAOPriCrmSlsLdCntRSQL">
			<desc><![CDATA[CRM Sales Lead 유무를 체크하기위해 Count를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(SLS_LD_NO) AS CNT
FROM PRI_CRM_SLS_LD
WHERE	SLS_LD_NO = @[sls_ld_no]			]]></sql>
			<params>
				<param name="sls_ld_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
