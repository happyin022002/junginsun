<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAORsltTriRoutDestViaVORSQL">
			<desc><![CDATA[TRI Destination Via. 조회]]></desc>
			<sql><![CDATA[
SELECT TRI_PROP_NO
      ,ORG_DEST_TP_CD
      ,ROUT_VIA_SEQ
      ,ROUT_VIA_PORT_CD
      ,(SELECT LOC_NM
          FROM MDM_LOCATION
         WHERE LOC_CD = A.ROUT_VIA_PORT_CD
           AND ROWNUM = 1) AS ROUT_VIA_PORT_NM
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
  FROM PRI_TRI_RT_ROUT_VIA A
 WHERE TRI_PROP_NO = @[tri_prop_no]
   AND ORG_DEST_TP_CD = 'D'
 ORDER BY ROUT_VIA_PORT_CD			]]></sql>
			<params>
				<param name="tri_prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
