<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchWhfWaveInfoRSQL">
			<desc><![CDATA[WHF Wave정보를 Manifest hard coding 테이블에서 찾아오기]]></desc>
			<sql><![CDATA[
SELECT CTNT.ATTR_CTNT1 AS WHF_WAVE_ENG
       , CTNT.ATTR_CTNT2 AS WHF_WAVE_KOR
FROM BKG_CSTMS_CD_CONV_CTNT CTNT
WHERE CTNT.CNT_CD ='KR'
AND CTNT.CSTMS_DIV_ID ='KR_WHF_EXEMPT_CD'
AND CTNT.ATTR_CTNT3 = @[whf_wave]			]]></sql>
			<params>
				<param name="whf_wave" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
