<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOsearchJpcusReceiveLogRSQL">
			<desc><![CDATA[searchJpcusReceiveLog]]></desc>
			<sql><![CDATA[
SELECT *
FROM (	
SELECT 
	RCV_DTL_SEQ,
	EDI_RCV_MSG,
	ROW_NUMBER() OVER(ORDER BY RCV_DTL_SEQ) AS RN,
	COUNT(*) OVER() AS TOTAL
FROM BKG_CSTMS_JP_RCV_LOG_DTL
WHERE	JP_MSG_TP_ID = @[jp_msg_tp_cd]
AND	RCV_DT = TO_DATE(@[rcv_dt],'YYYYMMDDHH24MISS')
AND	RCV_SEQ = @[rcv_seq]
) 
WHERE RN BETWEEN @[startno] AND @[endno]			]]></sql>
			<params>
				<param name="jp_msg_tp_cd" type="12" value="" out="N"/>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="rcv_seq" type="2" value="" out="N"/>
				<param name="startno" type="12" value="" out="N"/>
				<param name="endno" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
