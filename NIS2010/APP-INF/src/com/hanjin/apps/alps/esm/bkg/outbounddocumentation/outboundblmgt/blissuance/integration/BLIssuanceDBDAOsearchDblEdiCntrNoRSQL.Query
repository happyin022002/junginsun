<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiCntrNoRSQL">
			<desc><![CDATA[searchDblEdiCntrNo]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO
FROM   BKG_CONTAINER
WHERE  BKG_NO = @[bkg_no]
AND    ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
