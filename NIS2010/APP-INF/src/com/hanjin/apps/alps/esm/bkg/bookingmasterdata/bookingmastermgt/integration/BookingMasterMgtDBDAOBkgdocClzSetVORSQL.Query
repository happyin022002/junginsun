SELECT 
	VSL_SLAN_CD
,	YD_CD
,	DEST_CNT_CD
,	DOC_CLZ_TP_CD
,	ITVAL_HRS
,	XCLD_HOL_FLG
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
