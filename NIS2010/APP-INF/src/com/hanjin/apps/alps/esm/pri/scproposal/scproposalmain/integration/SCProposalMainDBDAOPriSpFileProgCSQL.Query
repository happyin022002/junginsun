<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOPriSpFileProgCSQL">
			<desc><![CDATA[PRI_SP_FILE_PROG 생성 SQL
* 2015.08.23 최성환 [CHM-201537109] Split19-[그룹사 표준 코드 시행] HJS 프로그램 대응 및 데이타 마이그레이션 작업 요청]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_FILE_PROG 
(
    PROP_NO,
    AMDT_SEQ,
    FILE_PROG_SEQ,
    FMC_FILE_TP_CD,
    FILE_EFF_DT,
    FMC_FILE_SND_DT,
    FILE_PROG_USR_ID,
    MNL_FILE_FLG,
    FILE_CORR_CMT_CTNT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
VALUES 
(
    @[prop_no],
    @[amdt_seq],
    @[file_prog_seq],
    @[fmc_file_tp_cd],
    TO_DATE ( @[eff_dt], 'YYYYMMDD' ),
    GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC('NYCSC'),
    @[cre_usr_id],
    'N',
	@[file_corr_cmt_ctnt],
    @[cre_usr_id],
    SYSDATE,
    @[cre_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="file_prog_seq" type="12" value="" out="N"/>
				<param name="fmc_file_tp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="file_corr_cmt_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
