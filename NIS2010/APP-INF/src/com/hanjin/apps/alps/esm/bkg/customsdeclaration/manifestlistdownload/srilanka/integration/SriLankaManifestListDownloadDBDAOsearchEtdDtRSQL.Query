<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SriLankaManifestListDownloadDBDAOsearchEtdDtRSQL">
			<desc><![CDATA[Estimated Vessel Departure 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(VPS_ETD_DT,'YYYY-MM-DD') vps_etd_dt
    FROM   VSK_VSL_PORT_SKD
    WHERE  VSL_CD        = @[vsl_cd]
    AND    SKD_VOY_NO = @[skd_voy_no]
    AND    SKD_DIR_CD    = @[skd_dir_cd]
#if(${pod_cd} != '')
    AND    VPS_PORT_CD    = @[call_port]  -- Last Call Port CD
#end
#if(${pol_cd} != '')
    AND    VPS_PORT_CD    = @[pol_cd]  
#end
    AND    NVL(SKD_CNG_STS_CD,' ') <> 'S'
    AND    CLPT_IND_SEQ  = (SELECT  MAX(CLPT_IND_SEQ)
                        	FROM    VSK_VSL_PORT_SKD
                        	WHERE   VSL_CD        = @[vsl_cd]
                            AND     SKD_VOY_NO = @[skd_voy_no]
                            AND     SKD_DIR_CD    = @[skd_dir_cd]
#if(${pod_cd} != '')
    AND    VPS_PORT_CD    = @[call_port]  -- Last Call Port CD
#end
#if(${pol_cd} != '')
    AND    VPS_PORT_CD    = @[pol_cd]  
#end
                            AND     NVL(SKD_CNG_STS_CD,' ') <> 'S')			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="call_port" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
