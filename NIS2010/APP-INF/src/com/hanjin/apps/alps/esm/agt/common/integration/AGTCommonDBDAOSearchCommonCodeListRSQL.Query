<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchCommonCodeListRSQL">
			<desc><![CDATA[searchCommonCodeList]]></desc>
			<sql><![CDATA[
SELECT 
		INTG_CD_VAL_CTNT AS CODE,
		INTG_CD_VAL_DP_DESC AS NAME
FROM COM_INTG_CD_DTL
WHERE INTG_CD_ID = @[code]
AND (TO_CHAR(SYSDATE,'YYYYMMDD') BETWEEN APLY_ST_DT AND APLY_END_DT)
ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="code" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
