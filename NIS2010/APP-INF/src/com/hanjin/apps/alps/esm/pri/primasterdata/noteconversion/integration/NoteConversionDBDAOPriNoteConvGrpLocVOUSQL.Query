		<query name="NoteConversionDBDAOPriNoteConvGrpLocVOUSQL">
			<desc><![CDATA[Group Location update]]></desc>
			<sql><![CDATA[
UPDATE PRI_NOTE_CONV_GRP_LOC SET 
	 NOTE_CONV_GRP_LOC_DESC = @[note_conv_grp_loc_desc]
    ,UPD_USR_ID = @[upd_usr_id]
	,UPD_DT = SYSDATE
WHERE PRC_CTRT_TP_CD = @[prc_ctrt_tp_cd]
AND	NOTE_CONV_GRP_LOC_CD = @[note_conv_grp_loc_cd]			]]></sql>
			<params>
				<param name="note_conv_grp_loc_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="note_conv_grp_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
