<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SriLankaCustomsTransmissionDBDAOsearchBkgNoRSQL">
			<desc><![CDATA[스리랑카 세관 신고용 B/L의 Booking Number 및 Cargo Type Code 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_NO, BKG_CGO_TP_CD        
        FROM  BKG_BOOKING
        WHERE BL_NO      = SUBSTR(TRIM(@[bl_no]),1,12)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
