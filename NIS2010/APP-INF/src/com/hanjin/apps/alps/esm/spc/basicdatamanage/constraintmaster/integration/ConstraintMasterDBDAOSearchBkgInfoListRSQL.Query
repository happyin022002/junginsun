<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOSearchBkgInfoListRSQL">
			<desc><![CDATA[ConstraintMaster 테이블에 Import할 대상 BKG의 정보를 조회한다
2015.07.06 [CHM-201536749]Mastertable Import기능 오류 수정]]></desc>
			<sql><![CDATA[
SELECT 
B.BKG_NO, B.BL_NO
     , (SELECT L.N2ND_PRNT_OFC_CD
        FROM SPC_OFC_LVL L
        WHERE L.OFC_CD = B.OB_SLS_OFC_CD
        AND (SELECT DISTINCT SUBSTR(SLS_YRMON, 0, 4)||COST_WK
              FROM MAS_BKG_EXPN_DTL D
             WHERE D.BKG_NO = B.BKG_NO) BETWEEN L.OFC_APLY_FM_YRWK AND L.OFC_APLY_TO_YRWK) SLS_RHQ_CD
     , '' AS 	bkg_aloc_tp_cd  
     , NULL AS 	sub_trd_yn  
     , NULL AS 	sub_trd_cd
     
     , NULL AS 	trnk_slan_yn  
     , V.SLAN_CD trnk_slan_cd    
     , NULL AS 	trnk_dir_yn  
     , V.SKD_DIR_CD	trnk_dir_cd      
     , NULL AS 	trnk_pol_yn  
     , V.POL_CD	trunk_pol_cd     
     , NULL AS 	trnk_pod_yn  
     , V.POD_CD	trunk_pod_cd     
, NULL AS 	bkg_por_cnt_yn  
, SUBSTR(B.POR_CD, 0, 2)	bkg_por_cnt_cd    
, NULL AS 	por_yn  
, B.POR_CD
, BL.POR_NM
, NULL AS 	por_nod_yn  
, B.POR_NOD_CD por_nod_cd      
, NULL AS 	bkg_por_scc_yn  
, (select sv.SCC_CD from MAS_LOCATION_V sv where sv.loc_cd = B.POR_CD)	bkg_por_scc_cd 
, NULL AS 	bkg_pol_cnt_yn  
, SUBSTR(B.POL_CD, 0, 2)	bkg_pol_cnt_cd    
, NULL AS 	pol_yn  
, B.POL_CD      
, BL.POL_NM
, NULL AS 	pol_nod_yn  
, B.POL_NOD_CD pol_nod_cd

, NULL AS 	n1st_ts_slan_yn  
, V2.SLAN_CD	n1st_ts_slan_cd   
, NULL AS 	n1st_ts_dir_yn  
, V2.SKD_DIR_CD	n1st_ts_dir_cd    
, NULL AS 	n1st_ts_pol_cnt_yn
, SUBSTR(V2.POL_CD, 0, 2)	n1st_ts_pol_cnt_cd
, NULL AS 	n1st_ts_pol_yn  
, V2.POL_CD	n1st_ts_pol_cd    
, NULL AS 	ts_pol_nod_yn 
, V2.POL_YD_CD	ts_pol_nod_cd    
, NULL AS 	n1st_ts_pod_cnt_yn
, SUBSTR(V2.POD_CD, 0, 2)	n1st_ts_pod_cnt_cd
, NULL AS 	n1st_ts_pod_yn  
, V2.POD_CD	n1st_ts_pod_cd  
, NULL AS 	ts_pod_nod_yn 
, V2.POD_YD_CD	ts_pod_nod_cd     

, NULL AS 	bkg_pod_cnt_yn  
, SUBSTR(B.POD_CD, 0, 2)	bkg_pod_cnt_cd    
, NULL AS 	pod_cd_yn  
, B.POD_CD	pod_cd  
, BL.POD_NM
, NULL AS 	pod_nod_yn  
, B.POD_NOD_CD pod_nod_cd      
, NULL AS 	bkg_del_cnt_yn  
, SUBSTR(B.DEL_CD, 0, 2)		bkg_del_cnt_cd    
, NULL AS 	del_yn  
, B.DEL_CD	del_cd  
, BL.DEL_NM
, NULL AS 	del_nod_yn  
, B.DEL_NOD_CD	del_nod_cd      
, NULL AS 	bkg_del_scc_yn  
, (select sv.SCC_CD from MAS_LOCATION_V sv where sv.loc_cd = B.DEL_CD)	bkg_del_scc_cd    

, NULL AS 	vvd_yn  
, V.VSL_CD||V.SKD_VOY_NO||V.SKD_DIR_CD vvd      

, NULL AS 	cntr_tpsz_yn  
, Q.cntr_tpsz_cd      

, NULL AS 	dg_rd_yn  
, B.rd_cgo_flg rd_flg
, B.dcgo_flg dg_flg   


, NULL AS 	ob_sls_ofc_yn  
, B.OB_SLS_OFC_CD ob_sls_ofc_cd     
, (select o.OFC_ENG_NM from mdm_organization o where o.ofc_cd = B.OB_SLS_OFC_CD) ob_sls_ofc_nm  

, NULL AS 	sc_yn  
, B.SC_NO      
, NULL AS 	rfa_yn  
, B.RFA_NO      

, NULL AS 	ctrt_yn  
,	B.CTRT_CUST_CNT_CD  
,	B.CTRT_CUST_SEQ  
,	B.CTRT_CUST_CNT_CD||LPAD(B.CTRT_CUST_SEQ, 6, '0') ctrt_cust_code  
,	CC2.CUST_LGL_ENG_NM ctrt_cust_lgl_eng_nm

, NULL AS 	agmt_yn
,	B.AGMT_ACT_CNT_CD  
,	B.AGMT_ACT_CUST_SEQ 
,	B.AGMT_ACT_CNT_CD||LPAD(B.AGMT_ACT_CUST_SEQ, 6, '0') agmt_act_cust_code
,	AC2.CUST_LGL_ENG_NM agmt_cust_lgl_eng_nm


, NULL AS 	shpr_yn
,	S.CUST_CNT_CD shpr_cust_cnt_cd  
,	S.CUST_SEQ shpr_cust_seq  
,	S.CUST_CNT_CD||LPAD(S.CUST_SEQ, 6, '0') shpr_cust_code
,	S2.CUST_LGL_ENG_NM shpr_cust_lgl_eng_nm

, NULL AS 	oft_chg_yn  
--OFT가 없는 경우 BKG ESTIMATED CMPB 테이블의 OFT값 가져오도록 수정
, NVL(R.CHG_UT_AMT, (select MAX(RV.oft_amt) KEEP (DENSE_RANK LAST ORDER BY RV.REV_COST_SEQ) FROM BKG_REV_COST RV WHERE RV.BKG_NO = B.BKG_NO)) AS oft_chg_amt    							
, NULL AS 	cmpb_yn  							
--, NULL AS 	cmpb_rule_cd      							
, (select MAX(RV.CMPB_AMT) KEEP (DENSE_RANK LAST ORDER BY RV.REV_COST_SEQ) AS CMPB FROM BKG_REV_COST RV WHERE RV.BKG_NO = B.BKG_NO) AS 	cmpb_amt  							 
, NULL AS 	aloc_lod_yn  
, NULL AS 	aloc_lod_qty      
, NULL AS 	aloc_rto_yn  
, NULL AS 	aloc_lod_qty_rto  

, NULL AS 	ASGN_TTL_WGT_YN  
, NULL AS 	ASGN_TTL_WGT      
, NULL AS 	ASGN_WGT_RTO_YN  
, NULL AS 	ASGN_WGT_RTO 

, NULL AS 	scg_grp_cmdt_yn  
, MC.GRP_CMDT_CD	scg_grp_cmdt_seq  
, (SELECT GP.SCG_GRP_CMDT_DESC
          FROM PRI_SCG_GRP_CMDT GP
         WHERE GP.SVC_SCP_CD = 'TPW'
           AND GP.CHG_CD = 'GRI'
           AND GP.SCG_GRP_CMDT_CD = MC.GRP_CMDT_CD
           AND GP.DELT_FLG = 'N') scg_grp_cmdt_desc 
, NULL AS 	cmdt_yn  
, B.CMDT_CD  
, MC.CMDT_NM    

, 'A' AS 	aloc_svc_cd      

, 'S' AS 	bkg_ctrl_tp_cd    
, NULL AS 	bkg_aloc_rmk      
--, NULL AS 	aloc_aply_fm_dt   
--, NULL AS 	aloc_aply_to_dt
, NULL AS 	aply_fm_yrwk   
, NULL AS 	aply_to_yrwk
, 'Y' AS 	aloc_use_flg      
     
, NULL AS 	upd_usr_id
, NULL AS 	upd_dt
, NULL AS 	bkg_aloc_seq
---------------------- report form all column
, B.RCV_TERM_CD ||'/'|| B.DE_TERM_CD AS RCV_DE_TERM_CD
, CASE WHEN F.CUST_CNT_CD IS NOT NULL THEN F.CUST_CNT_CD || LPAD(F.CUST_SEQ, 6, '0') || '(' || F2.CUST_LGL_ENG_NM || ')' ELSE NULL END AS FWDR_CUST_CNT_CD
, CASE WHEN C.CUST_CNT_CD IS NOT NULL THEN C.CUST_CNT_CD || LPAD(C.CUST_SEQ, 6, '0') || '(' ||C2.CUST_LGL_ENG_NM || ')' ELSE NULL END AS CNEE_CUST_CNT_CD
, B.TAA_NO
, B.REP_CMDT_CD || '(' || MR.REP_CMDT_NM|| ')'  AS REP_CMDT_CD
, DECODE(NVL(B.STWG_CD, 'NULL'), 'NULL', 'N', 'Y') AS STWG_FLG
, B.RC_FLG
, B.HNGR_FLG
, B.AWK_CGO_FLG
, B.CTRT_OFC_CD|| '/' || CTRT_SREP_CD AS CTRT_OFC_CD
, (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01720' AND INTG_CD_VAL_CTNT = B.ORG_TRNS_MOD_CD)
	|| '/' || (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01720' AND INTG_CD_VAL_CTNT = B.DEST_TRNS_MOD_CD) AS ORG_DEST_TRNS_MOD_CD
, Q.OP_CNTR_QTY
, DECODE(SPC_GET_CNTR_SZ_FNC(Q.CNTR_TPSZ_CD), '2', 1, 2) * Q.OP_CNTR_QTY AS BKG_TEU_QTY --TEU(Load)
, NULL AS 	us_yn 
, CASE WHEN (SUBSTR(B.POR_CD, 1, 2) IN ('CA', 'US') OR SUBSTR(B.DEL_CD, 1, 2) IN ('CA', 'US')) 
	   THEN SPC_USA_MODE_FNC(B.RCV_TERM_CD, B.DE_TERM_CD, B.POR_CD, B.POL_CD, B.POD_CD,B.DEL_CD) ELSE 'OTH' END usa_bkg_mod_cd 
---------------------- report form all column
--     , B.SLAN_CD
--     , NULL AS LANE_YN
--     , B.SC_NO
--     , B.TAA_NO
--     , B.RFA_NO
--     , B.CMDT_CD, MC.CMDT_NM 
--     , NULL AS CMDT_YN
--     , B.REP_CMDT_CD, MR.REP_CMDT_NM 
--     
--     , Q.CNTR_TPSZ_CD
--     , NULL AS TPSZ_FLG --EQ
--     , Q.OP_CNTR_QTY 
--     , DECODE(SPC_GET_CNTR_SZ_FNC(Q.CNTR_TPSZ_CD), '2', 1, 2) * Q.OP_CNTR_QTY BKG_TEU_QTY --TEU(Load)
--     , NULL AS QTY_YN     
--     , B.CTRT_OFC_CD, CTRT_SREP_CD
--     , B.OB_SLS_OFC_CD, OB_SREP_CD
--     , NULL AS LOFC_YN
--     , B.DCGO_FLG
--     , DECODE(NVL(B.STWG_CD, 'NULL'), 'NULL', 'N', 'Y') STWG_FLG
--     , B.RC_FLG
--     , B.HNGR_FLG
--     , B.AWK_CGO_FLG
--     
--     , (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01720' AND INTG_CD_VAL_CTNT = B.ORG_TRNS_MOD_CD) ORG_TRNS_MOD_CD
--     , (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01720' AND INTG_CD_VAL_CTNT = B.DEST_TRNS_MOD_CD) DEST_TRNS_MOD_CD
--
--     , S.CUST_CNT_CD AS SHPR_CUST_CNT_CD, S.CUST_SEQ AS SHPR_CUST_SEQ, S2.CUST_LGL_ENG_NM AS SHPR_CUST_LGL_ENG_NM
--     , NULL AS SHPR_YN
--     , C.CUST_CNT_CD AS CNEE_CUST_CNT_CD, C.CUST_SEQ AS CNEE_CUST_SEQ, C2.CUST_LGL_ENG_NM AS CNEE_CUST_LGL_ENG_NM
--     , NULL AS CNEE_YN
--     , F.CUST_CNT_CD AS FWDR_CUST_CNT_CD, F.CUST_SEQ AS FWDR_CUST_SEQ, F2.CUST_LGL_ENG_NM AS CUST_SEQ_CUST_LGL_ENG_NM
--     , NULL AS FWD_YN
--     , B.CTRT_CUST_CNT_CD, B.CTRT_CUST_SEQ, CC2.CUST_LGL_ENG_NM
--     , NULL AS CTRT_YN
--     , B.AGMT_ACT_CNT_CD, B.AGMT_ACT_CUST_SEQ, AC2.CUST_LGL_ENG_NM
--     , NULL AS AGMT_YN
--     , R.CURR_CD,  R.RAT_UT_CD, R.CHG_UT_AMT
--     , NULL AS CHG_YN
,'' f_header
,'' f_headernm
,'' pgm
,'' rpt_fom_nm
  FROM BKG_BOOKING B
     , BKG_BL_DOC BL
     , BKG_QUANTITY Q
     , BKG_VVD V
     , BKG_VVD V2 --TS
     , BKG_CHG_RT R
     , BKG_CUSTOMER S
     , BKG_CUSTOMER C
--     , BKG_CUSTOMER N
     , BKG_CUSTOMER F
     , MDM_CUSTOMER S2
     , MDM_CUSTOMER C2
----     , MDM_CUSTOMER N2
     , MDM_CUSTOMER F2
     , MDM_CUSTOMER CC2
     , MDM_CUSTOMER AC2
     , MDM_COMMODITY MC
     , MDM_REP_CMDT MR
 WHERE 1=1
--   AND B.BKG_NO = 'JKT410098106'
   AND 
(	B.BKG_NO IN (
#if($bkgList.size() != 0)
	#foreach(${bkg} in ${bkgList})
		#if($velocityCount < $bkgList.size())
	'${bkg}',
        #else
	'${bkg}'
        #end
    #end
#else
''
#end
   )
OR B.BL_NO IN (
#if($bkgList.size() != 0)
	#foreach(${bkg} in ${bkgList})
		#if($velocityCount < $bkgList.size())
	'${bkg}',
        #else
	'${bkg}'
        #end
    #end
#else
''
#end
   )
)
   AND B.BKG_NO = BL.BKG_NO
   AND B.BKG_NO = Q.BKG_NO
   AND B.BKG_NO = V.BKG_NO(+)
   AND V.VSL_PRE_PST_CD(+) = 'T'
   AND B.BKG_NO = V.BKG_NO(+)
   AND V2.VSL_PRE_PST_CD(+) <> 'T'
   AND B.BKG_NO = V2.BKG_NO(+)
   AND Q.BKG_NO = R.BKG_NO(+)
   AND R.CHG_CD(+) = 'OFT'
   AND (CASE
                         WHEN Q.CNTR_TPSZ_CD LIKE 'Q%' THEN 'BL'
                         ELSE Q.CNTR_TPSZ_CD
                       END = R.RAT_UT_CD(+))
   AND B.BKG_NO = S.BKG_NO
   AND S.BKG_CUST_TP_CD = 'S'
   AND S.CUST_CNT_CD = S2.CUST_CNT_CD(+)
   AND S.CUST_SEQ = S2.CUST_SEQ(+)
   AND B.BKG_NO = C.BKG_NO
   AND C.BKG_CUST_TP_CD = 'C'
   AND C.CUST_CNT_CD = C2.CUST_CNT_CD(+)
   AND C.CUST_SEQ = C2.CUST_SEQ(+)
--   AND B.BKG_NO = N.BKG_NO 
--   AND N.BKG_CUST_TP_CD = 'N'
   AND B.BKG_NO = F.BKG_NO
   AND F.BKG_CUST_TP_CD = 'F'
   AND F.CUST_CNT_CD = F2.CUST_CNT_CD(+)
   AND F.CUST_SEQ = F2.CUST_SEQ(+)
   AND B.ctrt_cust_cnt_cd = CC2.CUST_CNT_CD(+)
   AND B.ctrt_cust_seq = CC2.CUST_SEQ(+)
   AND B.agmt_act_cnt_cd = AC2.CUST_CNT_CD(+)
   AND B.agmt_act_cust_seq = AC2.CUST_SEQ(+)
   AND B.CMDT_CD = MC.CMDT_CD(+)
   AND B.REP_CMDT_CD = MR.REP_CMDT_CD(+)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
