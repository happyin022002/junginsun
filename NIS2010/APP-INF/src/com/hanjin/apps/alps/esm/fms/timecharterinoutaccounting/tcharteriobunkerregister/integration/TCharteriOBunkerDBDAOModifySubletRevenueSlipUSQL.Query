<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharteriOBunkerDBDAOModifySubletRevenueSlipUSQL">
			<desc><![CDATA[TCharteriOBunkerDBDAOModifySubletRevenueSlipUSQL]]></desc>
			<sql><![CDATA[
UPDATE FMS_BUNKER
   SET AR_SLP_TP_CD = @[slp_tp_cd],
   	   AR_SLP_FUNC_CD = @[slp_func_cd],
	   AR_SLP_OFC_CD = @[slp_ofc_cd],
	   AR_SLP_ISS_DT = SUBSTR(@[slp_iss_dt],3,6),
	   AR_SLP_SER_NO = @[slp_ser_no],
	   UPD_DT = SYSDATE,
	   UPD_USR_ID = @[upd_usr_id]
 WHERE FLET_CTRT_NO = @[flet_ctrt_no]
   AND BNK_SEQ = @[bnk_seq]			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="bnk_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
