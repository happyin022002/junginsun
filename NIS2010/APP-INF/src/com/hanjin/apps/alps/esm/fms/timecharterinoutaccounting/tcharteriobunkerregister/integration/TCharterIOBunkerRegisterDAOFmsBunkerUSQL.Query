<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBunkerRegisterDAOFmsBunkerUSQL">
			<desc><![CDATA[TCharterIOBunkerRegisterDAOFmsBunkerUSQL]]></desc>
			<sql><![CDATA[
update fms_bunker set 
	bnk_tp_cd = @[bnk_tp_cd],
	acct_cd = @[acct_cd],
	acct_itm_seq = @[acct_itm_seq],
	bnk_dt = TO_DATE(@[bnk_dt], 'YYYY-MM-DD HH24MI'),
	vsl_cd = @[vsl_cd],
	skd_voy_no = SUBSTR(@[bunker_vvd], 5, 4),
	skd_dir_cd = SUBSTR(@[bunker_vvd], 9, 1),
	rev_dir_cd = SUBSTR(@[bunker_vvd], 10, 1),
	port_cd = @[port_cd],
	flet_meas_ut_cd = @[flet_meas_ut_cd],
	bnk_qty = @[bnk_qty],
	bnk_prc_amt = @[bnk_prc_amt],
	bnk_amt = @[bnk_amt],
	upd_usr_id = @[upd_usr_id],
	upd_dt = SYSDATE
where flet_ctrt_no = @[flet_ctrt_no]
  and bnk_seq = @[bnk_seq]			]]></sql>
			<params>
				<param name="bnk_tp_cd" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="acct_itm_seq" type="12" value="" out="N"/>
				<param name="bnk_dt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="bunker_vvd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="flet_meas_ut_cd" type="12" value="" out="N"/>
				<param name="bnk_qty" type="12" value="" out="N"/>
				<param name="bnk_prc_amt" type="12" value="" out="N"/>
				<param name="bnk_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="bnk_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
