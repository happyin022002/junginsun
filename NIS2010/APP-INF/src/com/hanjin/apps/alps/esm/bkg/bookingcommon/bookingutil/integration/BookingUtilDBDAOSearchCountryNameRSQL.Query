<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchCountryNameRSQL">
			<desc><![CDATA[SearchCountryName]]></desc>
			<sql><![CDATA[
SELECT CNT_NM AS OUTPUT_TEXT 
FROM MDM_COUNTRY
WHERE CNT_CD = @[input_text]			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
