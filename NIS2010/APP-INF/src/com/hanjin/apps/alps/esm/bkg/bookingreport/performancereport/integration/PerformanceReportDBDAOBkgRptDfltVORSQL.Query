<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOBkgRptDfltVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	BKG_RPT_KND_CD
,	RPT_ID
,	RPT_NM
,	COM_FLG
,	BZC_COND_SQL_CTNT
,	BZC_ORD_CTNT
,	OWNR_USR_ID
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_RPT_DFLT
WHERE	OWNR_USR_ID = @[ownr_usr_id]
AND BKG_RPT_KND_CD = 'C'			]]></sql>
			<params>
				<param name="ownr_usr_id" type="12" value="NIS2010" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
