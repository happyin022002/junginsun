<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOSearchApInvCsrNoRSQL">
			<desc><![CDATA[OwnersAccount 전표 승인처리 후 자동생성된 AP_INV_HDR 데이터의 csr_no를 조회한다]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SLP_TP_CD||SLP_FUNC_CD||SLP_OFC_CD||SLP_ISS_DT||SLP_SER_NO CSR_NO
  FROM FMS_CSUL_SLP
 WHERE ORG_SLP_TP_CD||ORG_SLP_FUNC_CD||ORG_SLP_OFC_CD||ORG_ISS_DT||ORG_SLP_SER_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
