<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOEstimatedDBDAOSearchEstimatedRevenueCListVORSQL">
			<desc><![CDATA[SearchEstimatedRevenueCListVO]]></desc>
			<sql><![CDATA[
SELECT P.REV_YRMON,
  DECODE(P.ACCT_CD, '510911', 'TI', 'TO') FLET_CTRT_TP_CD,
  V.RLANE_CD,
  P.VSL_CD||P.SKD_VOY_NO||P.SKD_DIR_CD||P.REV_DIR_CD VVD_CD,
  P.HIR_DT_AMT AS HIRE_AMT,
  V.VST_DT,
  V.VED_DT,
  P.VVD_DUR_NO,
  P.ESTM_AMT AS EST_AMT,
  P.ACT_AMT AS ACT_AMT,
  P.ACCL_AMT AS ACC_AMT,
  P.ACCL_AMT AS ACC_OLD_AMT,
  P.FLET_ACCL_AMT AS FLET_ACCL_AMT
,P.FLET_ACCL_AMT as ACC_OLD_AMT
FROM GL_ESTM_IF_ERP P,
  FMS_VVD V
WHERE 1 = 1 
  AND P.EXE_YRMON = @[to_duration]
  AND DECODE(P.ACCT_CD, '510911', 'TI', 'TO') LIKE @[flet_ctrt_tp_cd]||'%'
  AND P.SYS_SRC_ID = 'CDA'
  AND P.REV_YRMON = V.REV_YRMON
  AND P.VSL_CD = V.VSL_CD
  AND P.SKD_VOY_NO = V.SKD_VOY_NO
  AND P.SKD_DIR_CD = V.SKD_DIR_CD
  AND P.REV_DIR_CD = V.REV_DIR_CD
  AND P.ESTM_VVD_TP_CD <> 'PV'			]]></sql>
			<params>
				<param name="to_duration" type="12" value="" out="N"/>
				<param name="flet_ctrt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
