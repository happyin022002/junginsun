<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOaddBkgUsaWhfBlInitCSQL">
			<desc><![CDATA[addBkgUsaWhfBlInit]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_USA_WHF_BL (
	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,   PORT_CD
,	IO_BND_CD
,	BL_NO
,   CSTMS_DESC
,   ORG_DEST_LOC_CD
,	CRE_USR_ID
,	UPD_USR_ID
)
(
	SELECT V.VSL_CD
	      ,V.SKD_VOY_NO
	      ,V.SKD_DIR_CD
	      ,DECODE(@[io_bnd_cd], 'I', V.POD_CD, V.POL_CD) AS PORT_CD
	      ,@[io_bnd_cd] AS IO_BND_CD
	      ,A.BL_NO
	      ,C.CSTMS_DESC
	      ,DECODE(@[io_bnd_cd], 'I', A.DEL_CD, A.POR_CD) AS ORG_DEST_LOC_CD
	      ,@[cre_usr_id] AS CRE_USR_ID
          ,@[cre_usr_id] AS UPD_USR_ID
	  FROM BKG_BOOKING A
	      ,BKG_BL_DOC C
          ,BKG_VVD V
	 WHERE A.BKG_NO = C.BKG_NO(+)
       AND A.BKG_NO = V.BKG_NO
	   AND V.VSL_CD = @[vsl_cd]
	   AND V.SKD_VOY_NO = @[skd_voy_no]
	   AND V.SKD_DIR_CD = @[skd_dir_cd]
	   AND A.BL_NO > ' '
	   AND A.BKG_STS_CD IN ('F','W')
	#if (${io_bnd_cd} == 'I') 
	   AND V.POD_CD = @[port_cd]
	#else 
	   AND V.POL_CD = @[port_cd]
	#end
)			]]></sql>
			<params>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
