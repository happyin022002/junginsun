<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOCheckActualCustomerRSQL">
			<desc><![CDATA[RFA Type이 Contract 일때, actual customer check]]></desc>
			<sql><![CDATA[
SELECT DISTINCT CUST_CNT_CD, LPAD(CUST_SEQ,6,0) AS CUST_SEQ , CMDT_HDR_SEQ
  FROM PRI_RP_SCP_RT_ACT_CUST 
 WHERE 1=1  
  AND PROP_NO = @[prop_no] AND AMDT_SEQ = @[amdt_seq] AND SRC_INFO_CD <> 'AD'
  AND CMDT_HDR_SEQ =  (SELECT MIN(CMDT_HDR_SEQ) FROM PRI_RP_SCP_RT_CMDT 
                        WHERE PROP_NO = @[prop_no] AND AMDT_SEQ = @[amdt_seq] AND SRC_INFO_CD <> 'AD')			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
