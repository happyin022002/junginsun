<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPropCpPriRpScpMnCSQL">
			<desc><![CDATA[Proposal Scope Copy PRI_SP_SCP_MN Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_MN (
      PROP_NO
    , AMDT_SEQ
    , SVC_SCP_CD
    , EFF_DT
    , EXP_DT
    , PROP_SCP_SREP_CD
    , PROP_SCP_OFC_CD
    , PROP_SCP_STS_CD
    , CNTR_LOD_UT_CD
    , TGT_MVC_QTY
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
)    
SELECT @[new_prop_no] AS PROP_NO
     , 0 AS AMDT_SEQ
     , SVC_SCP_CD
     , TO_DATE('99991231', 'YYYYMMDD') AS EFF_DT
     , TO_DATE('99991231', 'YYYYMMDD') AS EXP_DT
     , DECODE(@[srep_cd],NULL,PROP_SCP_SREP_CD,@[srep_cd]) AS PROP_SCP_SREP_CD
     , @[ofc_cd] AS PROP_SCP_OFC_CD
     , 'I' AS PROP_SCP_STS_CD
     , CNTR_LOD_UT_CD
     , TGT_MVC_QTY
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM PRI_RP_SCP_MN
WHERE PROP_NO = @[prop_no]
AND   AMDT_SEQ = @[amdt_seq]
AND   SVC_SCP_CD = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
