<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAOsearchBkgSubOfficeRSQL">
			<desc><![CDATA[BKG OFFICE의 SUB Office 조회]]></desc>
			<sql><![CDATA[
SELECT 
''''||BKG_JOIN_FNC( cursor(SELECT OFC_CD FROM   BKG_OFC_LVL_V
           WHERE @[bkg_ofc_cd] IN (OFC_CD ,OFC_N3RD_LVL_CD, OFC_N4TH_LVL_CD, OFC_N5TH_LVL_CD,OFC_N6TH_LVL_CD, OFC_N7TH_LVL_CD) )
        ,''',''')||'''' AS B_OFC_CD_SUB
FROM DUAL			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
