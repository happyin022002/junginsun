<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetWorkCostDBDAOCoaLaneTsBsaCmmtCopyCSQL">
			<desc><![CDATA[BSA Commitment 복사한다.]]></desc>
			<sql><![CDATA[
   INSERT
   INTO COA_LANE_TS_BSA_CMMT
        (
                COST_YRMON
              , FM_TRD_CD
              , FM_RLANE_CD
              , FM_IOC_CD
              , FM_DIR_CD
              , TO_TRD_CD
              , FM_HUL_BND_CD
              , TO_HUL_BND_CD
              , BSA_CMMT_AMT
              , BSA_CMMT_RTO
              , UPD_USR_ID
              , UPD_DT
              , CRE_USR_ID
              , CRE_DT
        )
 SELECT @[f_tar_mon] AS COST_YRMON
      , FM_TRD_CD
      , FM_RLANE_CD
      , FM_IOC_CD
      , FM_DIR_CD
      , TO_TRD_CD
      , FM_HUL_BND_CD
      , TO_HUL_BND_CD
      , BSA_CMMT_AMT
      , BSA_CMMT_RTO
      , @[user_id] AS UPD_USR_ID
      , SYSDATE AS UPD_DT
      , @[user_id] AS CRE_USR_ID
      , SYSDATE AS CRE_DT
   FROM COA_LANE_TS_BSA_CMMT
  WHERE COST_YRMON = @[f_src_mon] 

			]]></sql>
			<params>
				<param name="f_tar_mon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_src_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
