<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOCheckFirstVvdChgByCaRSQL">
			<desc><![CDATA[CA 를 통해 첫 VVD 가 바뀌는 지를 Check 함]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN     LENGTH(OLD_FIRST_VVD) = 9
                 AND LENGTH(NEW_FIRST_VVD) = 9
                 AND OLD_FIRST_VVD <> NEW_FIRST_VVD
            THEN 'Y'
            ELSE 'N'
       END FIRST_VVD_CHG   
  FROM (SELECT (SELECT NVL(FIRST_VVD,'') FIRST_VVD
                  FROM (SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD FIRST_VVD
                          FROM BKG_VVD
                         WHERE BKG_NO = @[bkg_no]
                         ORDER BY VSL_PRE_PST_CD,VSL_SEQ )
                 WHERE ROWNUM = 1) OLD_FIRST_VVD
             , (SELECT FIRST_VVD
                  FROM (SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD FIRST_VVD
                          FROM BKG_VVD_HIS
                         WHERE BKG_NO = @[bkg_no]
                           AND CORR_NO = 'TMP0000001'
                         ORDER BY VSL_PRE_PST_CD,VSL_SEQ )
                 WHERE ROWNUM = 1) NEW_FIRST_VVD
          FROM DUAL)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
