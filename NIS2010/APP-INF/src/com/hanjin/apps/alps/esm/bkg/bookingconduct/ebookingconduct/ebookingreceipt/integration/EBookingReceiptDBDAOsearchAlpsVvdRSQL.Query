<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchAlpsVvdRSQL">
			<desc><![CDATA[searchAlpsVvd]]></desc>
			<sql><![CDATA[
SELECT 
	BKG_NO
,	VSL_PRE_PST_CD
,	VSL_SEQ
,	SLAN_CD
,	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,   VSL_CD||SKD_VOY_NO||SKD_DIR_CD BKG_VVD_CD
,   POL_CLPT_IND_SEQ
,   POD_CLPT_IND_SEQ 
,	OP_CD
,	POL_CD
,	SUBSTR(POL_YD_CD,6,2) POL_YD_CD
,	POD_CD
,	SUBSTR(POD_YD_CD,6,2) POD_YD_CD
,	BKG_TRSP_MZD_CD
,	CNTR_LODG_FLG
,	REV_VVD_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
#if (${ca_flg}== 'Y')
FROM BKG_VVD_HIS
#else
FROM BKG_VVD
#end

WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
