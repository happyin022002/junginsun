<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgHistoryRSQL">
			<desc><![CDATA[BKG history이력을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL((SELECT 'Y'
              FROM BKG_HIS_MST
             WHERE BKG_NO = @[bkg_no]
               AND CORR_NO = @[ca_no]
               AND BKG_HIS_ISS_UI_ID = @[ui_id]
               AND ROWNUM = 1),'N') HIS_FLG
  FROM DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="ui_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
