<sqls>
	<querys>
		<query name="TCharteriOBunkerDBDAOModifyApSlipApprovalCancelBunkerUSQL">
			<desc><![CDATA[TCharteriOBunkerDBDAOModifyApSlipApprovalCancelBunkerUSQL]]></desc>
			<sql><![CDATA[
UPDATE FMS_BUNKER SET 
	SLP_TP_CD = NULL
,	SLP_FUNC_CD = NULL
,	SLP_OFC_CD = NULL
,	SLP_ISS_DT = NULL
,	SLP_SER_NO = NULL
,	APRO_FLG = 'N'
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	SLP_TP_CD 	= SUBSTR(@[csr_no],1,2)
AND		SLP_FUNC_CD = SUBSTR(@[csr_no],3,1)
AND		SLP_OFC_CD 	= SUBSTR(@[csr_no],4,6)
AND		SLP_ISS_DT 	= SUBSTR(@[csr_no],10,6)
AND		SLP_SER_NO 	= SUBSTR(@[csr_no],16,5)			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
