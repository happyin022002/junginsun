<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOOwnersAccountCsulSlpsUSQL">
			<desc><![CDATA[Owner's Account 상세 전표 수정   ]]></desc>
			<sql><![CDATA[
UPDATE FMS_CSUL_SLP
   SET ACCT_CD = @[acct_cd],
       CTR_CD = @[ctr_cd],
       CSR_CURR_CD	= @[csr_curr_cd],
       CSR_AMT = @[csr_amt],
       CSR_DESC = @[csr_desc],
       VNDR_SEQ = @[vndr_seq],
       TRNS_CURR_CD = 'USD',
       STL_FLG = 'N',
       FLET_SRC_TP_CD = '04',
       VSL_CD = SUBSTR(@[vvd_cd],1,4),
       SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4),
       SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1),
       REV_DIR_CD = SUBSTR(@[vvd_cd],10,1),
       LSG_GR_NO = @[lsg_gr_no],
       TO_INV_NO = @[to_inv_no],
       VVD_EFF_DT = TO_DATE(REPLACE(@[vvd_eff_dt],'-',''),'yyyymmdd'),
       VVD_EXP_DT = TO_DATE(REPLACE(@[vvd_exp_dt],'-',''),'yyyymmdd'),
       UPD_USR_ID = @[cre_usr_id],
       UPD_DT = SYSDATE,
       ACCT_ITM_SEQ = @[acct_itm_seq],
       OA_LOC_CD = @[oa_loc_cd],
       OA_INV_DT = @[oa_inv_dt],
       ATCH_FILE_OA_LNK_ID = @[atch_file_oa_lnk_id]
 WHERE SLP_TP_CD||SLP_FUNC_CD||SLP_OFC_CD||SLP_ISS_DT||SLP_SER_NO||SLP_SEQ_NO = @[slp_tp_cd]||@[slp_func_cd]||@[slp_ofc_cd]||@[slp_iss_dt]||@[slp_ser_no]||@[slp_seq_no]			]]></sql>
			<params>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="ctr_cd" type="12" value="" out="N"/>
				<param name="csr_curr_cd" type="12" value="" out="N"/>
				<param name="csr_amt" type="12" value="" out="N"/>
				<param name="csr_desc" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="lsg_gr_no" type="12" value="" out="N"/>
				<param name="to_inv_no" type="12" value="" out="N"/>
				<param name="vvd_eff_dt" type="12" value="" out="N"/>
				<param name="vvd_exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="acct_itm_seq" type="12" value="" out="N"/>
				<param name="oa_loc_cd" type="12" value="" out="N"/>
				<param name="oa_inv_dt" type="12" value="" out="N"/>
				<param name="atch_file_oa_lnk_id" type="12" value="" out="N"/>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="slp_seq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
