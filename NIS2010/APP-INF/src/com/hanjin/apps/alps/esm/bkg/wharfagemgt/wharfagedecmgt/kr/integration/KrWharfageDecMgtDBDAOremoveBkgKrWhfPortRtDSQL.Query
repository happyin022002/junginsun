<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOremoveBkgKrWhfPortRtDSQL">
			<desc><![CDATA[D]]></desc>
			<sql><![CDATA[
DELETE BKG_KR_WHF_PORT_RT
WHERE 1=1
AND CNTR_BLK_DIV_CD = @[cntr_blk_div_cd]
AND PORT_CD         = @[port_cd]
AND IO_BND_CD       = @[io_bnd_cd]  
AND DC_RTO_NO       = @[dc_rto_no]
AND PORT_SEQ		= @[port_seq] 			]]></sql>
			<params>
				<param name="cntr_blk_div_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dc_rto_no" type="12" value="" out="N"/>
				<param name="port_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
