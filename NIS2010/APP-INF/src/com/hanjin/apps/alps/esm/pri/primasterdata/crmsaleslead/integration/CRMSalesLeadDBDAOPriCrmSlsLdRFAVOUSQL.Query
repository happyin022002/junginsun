<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CRMSalesLeadDBDAOPriCrmSlsLdRFAVOUSQL">
			<desc><![CDATA[CRMSalesLeadDBDAOPriCrmSlsLdSCVOUSQL]]></desc>
			<sql><![CDATA[
UPDATE PRI_CRM_SLS_LD
SET SLS_LD_STS_CD = @[sls_ld_sts_cd]
#if (${sls_ld_no_up}=="Y")
WHERE SLS_LD_NO = @[sls_ld_no]
#else
WHERE SLS_LD_NO = (SELECT SLS_LD_NO FROM PRI_RP_MN 
				   WHERE PROP_NO = @[prop_no] AND AMDT_SEQ = @[amdt_seq])
#end			]]></sql>
			<params>
				<param name="sls_ld_sts_cd" type="12" value="" out="N"/>
				<param name="sls_ld_no" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
