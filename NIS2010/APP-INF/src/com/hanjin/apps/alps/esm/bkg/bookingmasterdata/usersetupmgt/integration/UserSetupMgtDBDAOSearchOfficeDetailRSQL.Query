<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOSearchOfficeDetailRSQL">
			<desc><![CDATA[UserSetupMgtDBDAOSearchOfficeDetailRSQL]]></desc>
			<sql><![CDATA[
SELECT 
    OFC_CD,
    OFC_ENG_NM,     
    OFC_ADDR,
    SUBSTR(LOC_CD,1,2) AS LOC_CD,
    OFC_PHN_NO,
    OFC_FAX_NO
 FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[ofc_cd] and DELT_FLG='N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
