<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentDBDAOSearchQtaAdjustmentListCntRSQL">
			<desc><![CDATA[[SKD Adjustment by VVD]의 [Data Count] 합니다.

* 2014.07.10 이혜민 trd_cd 조건 추가]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS CNT
  FROM SQM_SPCL_TGT_VVD
 WHERE BSE_TP_CD  = @[f_bse_tp_cd]
   AND BSE_YR     = @[f_bse_yr]
#if (${f_bse_qtr_cd} != '' && ${f_bse_qtr_cd} != 'All')
   AND BSE_QTR_CD = @[f_bse_qtr_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
   AND RLANE_CD   = @[f_rlane_cd]
#end
   AND TRD_CD     = @[f_trd_cd]			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
