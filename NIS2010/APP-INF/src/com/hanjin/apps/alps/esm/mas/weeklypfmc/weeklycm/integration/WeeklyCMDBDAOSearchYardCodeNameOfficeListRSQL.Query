<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOSearchYardCodeNameOfficeListRSQL">
			<desc><![CDATA[Yard Code 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT YD_CD AS NOD_CD
      ,YD_NM AS NOD_NM
      ,OFC_CD AS CTRL_OFC_CD
      ,CASE WHEN YD_FCTY_TP_MRN_TML_FLG = 'Y' THEN
               'M'
            WHEN YD_FCTY_TP_MRN_TML_FLG = 'N' AND YD_FCTY_TP_RAIL_RMP_FLG = 'Y' THEN
               'R'
            ELSE '*'
       END IB_NOD_TP
      ,(SELECT NOD_CD FROM MAS_STO_EXP_NOD WHERE NOD_CD = M.YD_CD) F_NOD_CD
FROM MDM_YARD M
WHERE M.YD_CD = REPLACE(@[nod_cd],'*','')
ORDER BY (SELECT NOD_CD FROM MAS_STO_EXP_NOD WHERE NOD_CD = M.YD_CD)ASC			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
