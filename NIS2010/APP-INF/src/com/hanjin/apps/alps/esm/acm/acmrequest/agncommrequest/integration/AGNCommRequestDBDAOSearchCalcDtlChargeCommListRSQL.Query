<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAOSearchCalcDtlChargeCommListRSQL">
			<desc><![CDATA[Search Calc Dtl Charge Comm List]]></desc>
			<sql><![CDATA[
SELECT CHG_CD
     , DECODE(IO_BND_CD, 'O', 'O/B', 'I/B') AS IO_BND_CD
     , CHG_UT_AMT
     , CURR_CD
     , RAT_UT_CD
     , CHG_AMT
     , DECODE(CHG_COMM_RT, '', CHG_COMM_OTR_AMT, CHG_COMM_RT) AS CHG_COMM_RT
     , DECODE(CHG_COMM_RT, '', CHG_COMM_CURR_CD, '%') AS CHG_COMM_CURR_CD
     , PAY_COMM_AMT
     , COMM_AMT
  FROM ACM_AGN_COMM_DTL_CHG
 WHERE BKG_NO = @[bkg_no]
   AND AGN_CD = @[agn_cd]
   AND IO_BND_CD = @[io_bnd_cd]
   AND AC_SEQ = @[ac_seq]
   AND AC_TP_CD = 'P'
 ORDER BY AC_CHG_SEQ
 
 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ac_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
