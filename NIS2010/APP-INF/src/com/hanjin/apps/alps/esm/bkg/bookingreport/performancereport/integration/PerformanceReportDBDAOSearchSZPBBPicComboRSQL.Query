<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchSZPBBPicComboRSQL">
			<desc><![CDATA[SZPBB의 Work Group별 PIC 콤보목록 조회]]></desc>
			<sql><![CDATA[
SELECT U.USR_ID VAL, U.USR_NM NAME
FROM BKG_DPCS_USR_GRP D, COM_USER U
WHERE D.USR_ID = U.USR_ID
AND D.DPCS_WRK_PRT_CD = 'CN'
AND D.DPCS_WRK_GRP_CD = @[wrk_grp_cd]			]]></sql>
			<params>
				<param name="wrk_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
