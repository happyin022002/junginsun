<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchPctlBLListRSQL">
			<desc><![CDATA[Port Closing Inquiry[ESM_BKG_0914]
PCT (Port Closing Time) 현황 조회 화면-2.B/L List]]></desc>
			<sql><![CDATA[
SELECT TB.*
  FROM (
#if (${dt_type} == 'PCT')
--------------------------------------------------------------------------------
    SELECT /*+ ALL_ROWS */
           BK.BKG_NO
          ,BK.BL_NO || BK.BL_NO_TP  BL_NO
          ,DECODE(NVL(SC.CUST_SEQ,'0'),'0','',TRIM(SC.CUST_CNT_CD) || TRIM(TO_CHAR(NVL(SC.CUST_SEQ, 0), '000000'))) SHPR_CD
          ,REPLACE(REPLACE(SUBSTR(SC.CUST_NM, 1, 50), CHR(13), ''), CHR(10), '') SHPR_NM
          ,BK.POL_CD
          ,DECODE(BMO.BL_CVRD_TP_CD, 'C', 'F', BKG_RATE_RESULT_FNC(BK.BKG_NO)) BKG_RT_STS_CD
          ,TO_CHAR(VPS.VPS_ETD_DT,'YYYY-MM-DD HH24:MI:SS') ATD
          ,TO_CHAR(VPS.VPS_ETD_DT,'YYYY-MM-DD HH24:MI:SS') ETD
          ,VPS.VSL_CD || VPS.SKD_VOY_NO || VPS.SKD_DIR_CD VVD_CD
          ,VPS.SLAN_CD
          ,NVL(REV.REV_STATUS, 'N') REV_STATUS
          ,BK.BKG_STS_CD
          ,BK.SI_FLG
          ,DECODE(BK.INDIV_PSON_FLG,'Y','Individual',DECODE(NVL(CC.CUST_SEQ,'0'),'0','',TRIM(CC.Cust_Cnt_Cd) || TRIM(TO_CHAR(NVL(CC.CUST_SEQ, 0), '000000')))) CNEE_CD
          ,REPLACE(REPLACE(SUBSTR(CC.CUST_NM, 1, 50), CHR(13), ''), CHR(10), '') CNEE_NM
          ,BK.Bkg_Ofc_Cd
          ,BK.DOC_USR_ID     BKG_STF
          ,DECODE(BK.CUST_TO_ORD_FLG, 'Y', 'O', 'S') BL_TYPE
          ,DECODE(NVL(NC.CUST_SEQ,'0'),'0','',TRIM(NC.Cust_Cnt_CD) || TRIM(TO_CHAR(NVL(NC.CUST_SEQ, 0), '000000'))) NTFY_CD
          ,REPLACE(REPLACE(SUBSTR(NC.CUST_NM, 1, 50), CHR(13), ''), CHR(10), '') NTFY_NM
          ,TO_CHAR(BK.PORT_CLZ_DT, 'YYYY-MM-DD') BKG_PCT_DT
          ,BK.SC_NO
          ,BK.RFA_NO
          ,'' BKG_STAFF
          ,'' FR_DT
          ,'' TO_DT
          ,'' GSO
          ,'' DT_TYPE  --PCT,ETD
          ,'' SEL_BKG_OFC_CD
          ,''   DOWN_TP_O
          ,''   DOWN_TP_B
          ,''   DOWN_TP_T
          ,(
           SELECT DECODE(COUNT(*), 0, 'N', 'Y')
	         FROM BKG_DOC_PROC_SKD
            WHERE BKG_DOC_PROC_TP_CD IN ('CNTCFM', 'CNTRLS')
              AND DOC_PERF_DELT_FLG = 'N'
              AND BKG_NO = BK.BKG_NO
              AND ROWNUM = 1
           ) AS CNTR_CFM
      FROM BKG_BOOKING BK
          ,BKG_BL_DOC  BMO
          ,BKG_VVD VVD
          ,BKG_CUSTOMER NC
          ,BKG_CUSTOMER CC
          ,BKG_CUSTOMER SC
          ,VSK_VSL_PORT_SKD VPS
          ,(
            SELECT S.*, DECODE(NVL(REV_LANE, 'N'), 'N', 'N', 'Y') REV_STATUS
              FROM MDM_VSL_SVC_LANE S
                  ,(
                    SELECT DISTINCT SUBSTR(RLANE_CD, 1, 3) REV_LANE
                      FROM MDM_DTL_REV_LANE
                     WHERE DELT_FLG = 'N'
                   ) R
             WHERE S.VSL_SLAN_CD = R.REV_LANE(+)
            ) REV
    #if (${down_tp_t} == 'TOT')
          ,(
            SELECT ROWNUM
                  ,REGION
                  ,GSO
                  ,OFC_CD
              FROM BKG_OFC_LVL_V
            ) OLA
     WHERE 1=1
       AND BK.Bkg_Ofc_Cd = OLA.OFC_CD
       AND OLA.REGION IS NOT NULL
       #if (${rhq_cd} != '')
       AND OLA.REGION = @[rhq_cd]
       #end
       #if (${gso} != '')
       AND OLA.GSO = @[gso]
       #end
       #if (${bkg_ofc_cd} != '')
       AND BK.Bkg_Ofc_Cd = @[bkg_ofc_cd]
       #end
    #else
     WHERE  1=1
       #if (${sel_bkg_ofc_cd} != '')
       AND BK.Bkg_Ofc_Cd = @[sel_bkg_ofc_cd]
       #end
    #end

     AND BK.BKG_STS_CD <> 'X'
     AND BK.BKG_NO = BMO.BKG_NO
     AND CC.BKG_NO = BK.BKG_NO
     AND CC.BKG_CUST_TP_CD = 'C'
     AND SC.BKG_NO = BK.BKG_NO
     AND SC.BKG_CUST_TP_CD = 'S'
     AND NC.BKG_NO = BK.BKG_NO
     AND NC.BKG_CUST_TP_CD = 'N'
     AND VVD.BKG_NO = BK.BKG_NO
     AND VPS.SLAN_CD = REV.VSL_SLAN_CD(+)
     AND VPS.VSL_CD = VVD.VSL_CD
     AND VPS.SKD_VOY_NO = VVD.SKD_VOY_NO
     AND VPS.SKD_DIR_CD = VVD.SKD_DIR_CD
     AND VPS.VPS_PORT_CD = VVD.POL_CD
     AND VPS.CLPT_IND_SEQ = 1
     AND BK.BKG_CRE_DT > TO_DATE('20060101', 'YYYYMMDD')
     AND VVD.VSL_PRE_PST_CD || VVD.VSL_SEQ = (SELECT /*+ INDEX(VVD XPKBKG_VVD) */
                                                     BV.VSL_PRE_PST_CD || BV.VSL_SEQ
                                                FROM BKG_VVD BV
                                               WHERE BV.BKG_NO = VVD.BKG_NO
                                                 AND ROWNUM =1)
    #if (${bkg_sts_cd} != '')
       AND BK.BKG_STS_CD = @[bkg_sts_cd]
    #end
    #if (${bkg_rt_sts_cd} != '' && ${bkg_rt_sts_cd} != 'A')
       AND BKG_RATE_RESULT_FNC(BK.BKG_NO) = @[bkg_rt_sts_cd]
    #end
    #if (${rev_status} != '')
       AND NVL(REV.REV_STATUS, 'N') = @[rev_status]
    #end
    #if (${bkg_staff} != '')
       AND BK.DOC_USR_ID LIKE @[bkg_staff] || '%%'
    #end
    #if (${fr_dt} != '')
       AND BK.PORT_CLZ_DT   >= TO_DATE(@[fr_dt], 'YYYY-MM-DD')
    #end
    #if (${to_dt} != '')
       AND BK.PORT_CLZ_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
    #end
    #if (${pol_cd} != '')
       AND BK.POL_CD = @[pol_cd]
    #end
    #if (${vvd_cd} != '')
       AND BK.VSL_CD =  SUBSTR(@[vvd_cd],1,4)
       AND BK.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
       AND BK.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
    #end
    #if (${slan_cd} != 'All')
       AND BK.SLAN_CD = @[slan_cd]
    #end

    -------------------------------------------
    #if (${down_tp_t} == 'TOT')
    ORDER BY OLA.REGION,OLA.GSO, BK.BKG_OFC_CD
    #end
    -------------------------------------------

#elseif (${dt_type} =='ETD')
--------------------------------------------------------------------------------
    SELECT /*+ ALL_ROWS */
           BK.BKG_NO
          ,BK.BL_NO || BK.BL_NO_TP BL_NO
          ,TRIM(SC.CUST_CNT_CD) || TRIM(TO_CHAR(NVL(SC.CUST_SEQ, 0), '000000')) SHPR_CD
          ,REPLACE(REPLACE(SUBSTR(SC.CUST_NM, 1, 50), CHR(13), ''), CHR(10), '') SHPR_NM
          ,BK.POL_CD POL_CD
          ,DECODE(BMO.BL_CVRD_TP_CD, 'C', 'F', BKG_RATE_RESULT_FNC(BK.BKG_NO)) BKG_RT_STS_CD
          ,VAS.ACT_DEP_DT   ATD
          ,TO_CHAR(VPS.VPS_ETD_DT,'YYYY-MM-DD HH24:MI:SS') ETD
          ,VPS.VSL_CD || VPS.SKD_VOY_NO || VPS.SKD_DIR_CD VVD_CD
          ,VPS.SLAN_CD
          ,NVL(REV.REV_STATUS, 'N') REV_STATUS
          ,BK.BKG_STS_CD
          ,BK.SI_FLG
          ,DECODE(BK.INDIV_PSON_FLG,'Y','Individual',TRIM(CC.CUST_CNT_CD) || TRIM(TO_CHAR(NVL(CC.CUST_SEQ, 0), '000000'))) CNEE_CD
          ,REPLACE(REPLACE(SUBSTR(CC.CUST_NM, 1, 50), CHR(13), ''), CHR(10), '') CNEE_NM
          ,BK.BKG_OFC_CD
          ,BK.DOC_USR_ID BKG_STF
          ,DECODE(BK.CUST_TO_ORD_FLG, 'Y', 'O', 'S') BL_TYPE
          ,TRIM(NC.CUST_CNT_CD) || TRIM(TO_CHAR(NVL(NC.CUST_SEQ, 0), '000000')) NTFY_CD
          ,REPLACE(REPLACE(SUBSTR(NC.CUST_NM, 1, 50), CHR(13), ''), CHR(10), '') NTFY_NM
          ,TO_CHAR(BK.PORT_CLZ_DT, 'YYYY-MM-DD') BKG_PCT_DT
          ,BK.SC_NO
          ,BK.RFA_NO
          ,(
           SELECT DECODE(COUNT(*), 0, 'N', 'Y')
	         FROM BKG_DOC_PROC_SKD
            WHERE BKG_DOC_PROC_TP_CD IN ('CNTCFM', 'CNTRLS')
              AND DOC_PERF_DELT_FLG = 'N'
              AND BKG_NO = BK.BKG_NO
              AND ROWNUM = 1
           ) AS CNTR_CFM
    FROM   BKG_BOOKING BK
          ,BKG_BL_DOC BMO
          ,BKG_CUSTOMER CC
          ,BKG_CUSTOMER NC
          ,BKG_CUSTOMER SC
          ,(
            SELECT S.*, DECODE(NVL(REV_LANE, 'N'), 'N', 'N', 'Y') REV_STATUS
              FROM MDM_VSL_SVC_LANE S
                  ,(SELECT DISTINCT SUBSTR(RLANE_CD, 1, 3) REV_LANE
                      FROM MDM_DTL_REV_LANE
                     WHERE DELT_FLG = 'N'
                    ) R
             WHERE S.VSL_SLAN_CD = R.REV_LANE(+)
           ) REV
          ,BKG_VVD         VVD
          ,VSK_VSL_PORT_SKD VPS
          ,(
            SELECT /*+ USE_NL(VPS VVD VAP) */
                   VPS.VSL_CD
                  ,VPS.SKD_VOY_NO
                  ,VPS.SKD_DIR_CD
                  ,VAP.ACT_DEP_DT
                  ,VPS.SLAN_CD
                  ,VPS.VPS_PORT_CD
              FROM VSK_VSL_PORT_SKD  VPS
                  ,BKG_VVD           VVD
                  ,VSK_ACT_PORT_SKD VAP
             WHERE 1=1
               AND VPS.VPS_PORT_CD   =   VVD.POL_CD
               AND VPS.VSL_CD       =   VVD.VSL_CD
               AND VPS.SKD_VOY_NO   =   VVD.SKD_VOY_NO
               AND VPS.SKD_DIR_CD   =   VVD.SKD_DIR_CD
               AND VPS.VSL_CD        =   VAP.VSL_CD
               AND VPS.SKD_VOY_NO   =   VAP.SKD_VOY_NO
               AND VPS.SKD_DIR_CD   =   VAP.SKD_DIR_CD
               AND VPS.VPS_PORT_CD  =   VAP.VPS_PORT_CD
               AND VPS.CLPT_IND_SEQ =   VAP.CLPT_IND_SEQ
               AND VPS.CLPT_IND_SEQ =    VVD.POL_CLPT_IND_SEQ
            #if (${fr_dt} != '')
               AND VPS.VPS_ETD_DT >= TO_DATE(@[fr_dt], 'YYYY-MM-DD')
            #end
            #if (${to_dt} != '')
               AND VPS.VPS_ETD_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
            #end
            GROUP BY VPS.VSL_CD,VPS.SKD_VOY_NO,VPS.SKD_DIR_CD,VAP.ACT_DEP_DT,VPS.SLAN_CD,VPS.VPS_PORT_CD
           ) VAS

    -------------------------------------------
    #if (${down_tp_t} == 'TOT')
           ,(
             SELECT ROWNUM
                   ,REGION
                   ,GSO
                   ,OFC_CD
               FROM BKG_OFC_LVL_V
            ) OLA
     WHERE 1=1
       AND BK.Bkg_Ofc_Cd = OLA.OFC_CD
       AND OLA.REGION IS NOT NULL
       #if (${rhq_cd} != '')
       AND OLA.REGION = @[rhq_cd]
       #end
       #if (${gso} != '')
       AND OLA.GSO = @[gso]
       #end
       #if (${bkg_ofc_cd} != '')
       AND BK.Bkg_Ofc_Cd = @[bkg_ofc_cd]
       #end
    #else
     WHERE 1=1
       #if (${sel_bkg_ofc_cd} != '')
       AND BK.Bkg_Ofc_Cd = @[sel_bkg_ofc_cd]
       #end
    #end
    -------------------------------------------

       AND BK.BKG_STS_CD <> 'X'
       AND BK.BKG_CGO_TP_CD <> 'P'
       AND VPS.SLAN_CD = REV.VSL_SLAN_CD
       AND BK.BKG_NO = BMO.BKG_NO
       AND CC.BKG_NO = BK.BKG_NO
       AND CC.BKG_CUST_TP_CD = 'C'
       AND SC.BKG_NO = BK.BKG_NO
       AND SC.BKG_CUST_TP_CD = 'S'
       AND NC.BKG_NO = BK.BKG_NO
       AND NC.BKG_CUST_TP_CD = 'N'
       AND BK.POL_CD           = VVD.POL_CD
       AND BK.BKG_NO           = VVD.BKG_NO
       AND VPS.VSL_CD          = VVD.VSL_CD
       AND VPS.SKD_VOY_NO      = VVD.SKD_VOY_NO
       AND VPS.SKD_DIR_CD      = VVD.SKD_DIR_CD
       AND VPS.CLPT_IND_SEQ    = VVD.POL_CLPT_IND_SEQ
       AND VPS.VPS_PORT_CD     = VVD.POL_CD
       AND VPS.VSL_CD          = VAS.VSL_CD(+)
       AND VPS.SKD_VOY_NO      = VAS.SKD_VOY_NO(+)
       AND VPS.SKD_DIR_CD      = VAS.SKD_DIR_CD(+)
       AND VPS.VPS_PORT_CD     = VAS.VPS_PORT_CD(+)

    #if (${bkg_sts_cd} != '')
       AND BK.BKG_STS_CD = @[bkg_sts_cd]
    #end
    #if (${bkg_rt_sts_cd} != '' && ${bkg_rt_sts_cd} != 'A')
       AND BKG_RATE_RESULT_FNC(BK.BKG_NO) = @[bkg_rt_sts_cd]
    #end
    #if (${rev_status} != '')
       AND NVL(REV.REV_STATUS, 'N') = @[rev_status]
    #end
    #if (${bkg_staff} != '')
       AND BK.DOC_USR_ID LIKE @[bkg_staff] || '%%'
    #end
    #if (${pol_cd} != '')
       AND BK.POL_CD = @[pol_cd]
    #end
    #if (${fr_dt} != '')
       AND VPS.VPS_ETD_DT >= TO_DATE(@[fr_dt], 'YYYY-MM-DD')
    #end
    #if (${to_dt} != '')
       AND VPS.VPS_ETD_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
    #end
    #if (${vvd_cd} != '')
       AND BK.VSL_CD =  SUBSTR(@[vvd_cd],1,4)
       AND BK.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
       AND BK.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
    #end
    #if (${slan_cd} != 'All')
       AND BK.SLAN_CD = @[slan_cd]
    #end
    -------------------------------------------
    #if (${down_tp_t} == 'TOT')
    ORDER  BY OLA.REGION,OLA.GSO, BK.BKG_OFC_CD
    #end
    -------------------------------------------
#end
       ) TB
#if (${cntr_cfm} != '')
 WHERE TB.CNTR_CFM = @[cntr_cfm]
#end			]]></sql>
			<params>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="gso" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="sel_bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_sts_cd" type="12" value="" out="N"/>
				<param name="bkg_rt_sts_cd" type="12" value="" out="N"/>
				<param name="rev_status" type="12" value="" out="N"/>
				<param name="bkg_staff" type="12" value="" out="N"/>
				<param name="fr_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="cntr_cfm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
