<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOUpdateTargerVvdFixUSQL">
			<desc><![CDATA[targer vvd list를 업데이트한다.
2015.12.02 김용습 [CHM-201538876] [SQM] 연간 판매목표 VBP SKD I/F 후 매뉴얼 수정관련 로직 수정 요청]]></desc>
			<sql><![CDATA[
UPDATE SQM_QTA_TGT_VVD 
   SET DELT_FLG    = @[delt_flg] 
      ,BSE_MON     = @[bse_mon]
      ,BSE_WK      = @[bse_wk] 
      ,FNL_BSA_CAPA = @[fnl_bsa_capa]
      ,UPD_USR_ID  = @[upd_usr_id]
      ,UPD_DT      = SYSDATE 
WHERE BSE_TP_CD    = @[bse_tp_cd]
  AND BSE_YR       = @[bse_yr]
  AND BSE_QTR_CD   = DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
  AND TRD_CD       = @[trd_cd]
  AND DIR_CD       = @[dir_cd]
  AND SUB_TRD_CD   = @[sub_trd_cd]
  AND RLANE_CD     = @[rlane_cd]
  AND VSL_CD       = @[vsl_cd]
  AND SKD_VOY_NO   = @[skd_voy_no]
  AND SKD_DIR_CD   = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="bse_mon" type="12" value="" out="N"/>
				<param name="bse_wk" type="12" value="" out="N"/>
				<param name="fnl_bsa_capa" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
