<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchSpclCgoDtlRSQL">
			<desc><![CDATA[searchSpclCgoDtl]]></desc>
			<sql><![CDATA[
SELECT *
FROM (

#if (${in_pgm_no} == 'ESM_BKG_0951') 
/**************************** ESM_BKG_0951에서 조회된 경우  *****************************/


SELECT ROWNUM SEQ,
  CGO_TYPE,
  BKG_NO,
  BKG_NO2,
  TS,
  A_POL,
  DECODE(A_POD, 'USSEA', DECODE(BLCK_STWG_CD, 'ONE', 'USSE1', 'USSEA'), 'CAVAN', DECODE(BLCK_STWG_CD, 'LOC', 'CAYVR', 'ONE', 'CAVA1', 'CAVAN'), A_POD) A_POD,
  POL,
  DECODE(POD, 'USSEA', DECODE(BLCK_STWG_CD, 'ONE', 'USSE1', 'USSEA'), 'CAVAN', DECODE(BLCK_STWG_CD, 'LOC', 'CAYVR', 'ONE', 'CAVA1', 'CAVAN'), POD) POD,
  CS,
  CS2,
  CNTR_NO,
  TP,
  ROUND(WGT, 1) WGT,
  WGT_UNIT,
  CLASS_CD,
  UNNO,
  TEMP,
  VENT,
  REMARK,
  MP,
  SG,
  LQ,
  VO_ID,
  STOW,
  MTY_BKG_CD,
  CLPT_SEQ,
  BLCK_STWG_CD
FROM (
    SELECT CGO_TYPE,
      BKG_NO,
      BKG_NO BKG_NO2,
      TS,
      A_POL,
      A_POD,
      POL,
      POD,
      DECODE(CS, 0, '', CS) CS,
      DECODE(CS2, 0, '', CS2) CS2,
      CNTR_NO,
      TP,
      WGT,
      WGT_UNIT,
      CLASS_CD,
      UNNO,
      TEMP,
      VENT,
      REMARK,
      MP,
      SG,
      LQ,
      VO_ID,
      STOW,
      MTY_BKG_CD,
      CLPT_SEQ,
      BLCK_STWG_CD
    FROM (
       SELECT 'AK' CGO_TYPE,
          AWK.BKG_NO BKG_NO,
          DECODE(BV.POL_CD,BKG.POL_CD, '','TS') TS,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POL_CD), BKG.POL_CD) A_POL,

--BKG.POL_CD A_POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POD_CD), BKG.POD_CD) A_POD,

--BKG.POD_CD A_POD,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BV.POL_CD), BV.POL_CD) POL,

--CLL.POL_CD POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BV.POD_CD), BV.POD_CD) POD,

--CLL.POD_CD POD,
          0 CS,
          0 CS2,
          AWK.CNTR_NO CNTR_NO,
          BC.CNTR_TPSZ_CD TP,
          BC.CNTR_WGT WGT,
          BC.WGT_UT_CD WGT_UNIT,
          ' ' CLASS_CD,
          ' ' UNNO,
          ' ' TEMP,
          ' ' VENT,
          DECODE(NVL(AWK.OVR_FWRD_LEN, 0), 0, '', 'O.FWRD:'||AWK.OVR_FWRD_LEN||'  ')|| DECODE(NVL(AWK.OVR_BKWD_LEN, 0), 0, '', 'O.BKWD:'||AWK.OVR_BKWD_LEN||'  ')|| DECODE(NVL(AWK.OVR_HGT, 0), 0, '', 'O.HGT:'||AWK.OVR_HGT||'  ')|| DECODE(NVL(AWK.OVR_LF_LEN, 0), 0, '', 'O.LF:'||AWK.OVR_LF_LEN||'  ')|| DECODE(NVL(AWK.OVR_RT_LEN, 0), 0, '', 'O.RT:'||AWK.OVR_RT_LEN||'  ')|| DECODE(NVL(AWK.CRN_PST_STS_CD, '0'), '0', '', DECODE(BC.CNTR_TPSZ_CD, 'A4', 'PS:'||AWK.CRN_PST_STS_CD||'  ', ''))|| DECODE(NVL(AWK.XTD_OVR_QTY, 0), 0, '', DECODE(BC.CNTR_TPSZ_CD, 'A4', 'EH:'||AWK.XTD_OVR_QTY||'  ', '')) REMARK,
          ' ' MP,
          ' ' SG,
          ' ' LQ,
          TO_CHAR(AWK.OVR_VOID_SLT_QTY) VO_ID,
          BKG.STWG_CD STOW,
          BKG.BKG_CGO_TP_CD MTY_BKG_CD,
          VPS.CLPT_SEQ CLPT_SEQ,
          DECODE(NVL(BV.POD_CD,' '), 'USSEA', DECODE(BV.SLAN_CD,'PSX',DECODE(DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'ONE','SE1','SEA'),DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC')), 
                							'USOAK', DECODE(BKG.BLCK_STWG_CD, 'OA1', 'OA1' ,'', '', 'OAK'), 
                							'USLGB', DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','7','SEV','T','TRS', 'LOC'), 
                							'CAVAN', DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'') BLCK_STWG_CD
        FROM BKG_AWK_CGO AWK,
          BKG_BOOKING BKG,
          BKG_QUANTITY BQ,
          BKG_CONTAINER BC,
          BKG_VVD BV,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE BKG.BKG_NO = AWK.BKG_NO
          AND BKG.BKG_NO = BQ.BKG_NO
          AND BKG.BKG_NO = BC.BKG_NO
          AND BKG.BKG_NO = BV.BKG_NO
          AND AWK.CNTR_NO = BC.CNTR_NO
          AND BKG.BKG_STS_CD IN ( 'F','W')

		  AND BV.VSL_CD     = SUBSTR(@[in_vvd_cd], 1, 4)
		  AND BV.SKD_VOY_NO = SUBSTR(@[in_vvd_cd], 5, 4)
		  AND BV.SKD_DIR_CD = SUBSTR(@[in_vvd_cd], 9, 1)
		  AND BV.POL_CD     = @[in_pol_cd]
		  AND SUBSTR(BV.POL_YD_CD, 6, 2) like @[in_pol_yd_cd]||'%'

          AND VPS.CLPT_IND_SEQ = '1'
          AND VPS.VSL_CD = BV.VSL_CD
          AND VPS.SKD_VOY_NO = BV.SKD_VOY_NO
          AND VPS.SKD_DIR_CD = BV.SKD_DIR_CD
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND BV.POD_CD = VPS.VPS_PORT_CD
        UNION ALL
        SELECT 'DG' CGO_TYPE,
          DG.BKG_NO BKG_NO,
          DECODE(BV.POL_CD,BKG.POL_CD, '','TS') TS,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POL_CD), BKG.POL_CD) A_POL,

--BKG.POL_CD A_POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POD_CD), BKG.POD_CD) A_POD,

--BKG.POD_CD A_POD,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BV.POL_CD), BV.POL_CD) POL,

--CLL.POL_CD POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BV.POD_CD), BV.POD_CD) POD,

--CLL.POD_CD POD,
          DG.DG_CNTR_SEQ CS,
          DG.CNTR_CGO_SEQ CS2,
          DG.CNTR_NO CNTR_NO,
          BC.CNTR_TPSZ_CD TP,
          BC.CNTR_WGT WGT,
          BC.WGT_UT_CD WGT_UNIT,
          DECODE(DG.IMDG_SUBS_RSK_LBL_CD1, NULL, DG.IMDG_CLSS_CD, DG.IMDG_CLSS_CD||'('||DG.IMDG_SUBS_RSK_LBL_CD1||')') CLASS_CD,
          DG.IMDG_UN_NO UNNO,
          ' ' TEMP,
          ' ' VENT,
          DG.DIFF_RMK REMARK,
          DG.MRN_POLUT_FLG MP,
          DG.HZD_CTNT SG,
          DG.IMDG_LMT_QTY_FLG LQ,
          ' ' VO_ID,
          BKG.STWG_CD STOW,
          BKG.BKG_CGO_TP_CD MTY_BKG_CD,
          VPS.CLPT_SEQ CLPT_SEQ ,
          DECODE(NVL(BV.POD_CD,' '), 'USSEA', DECODE(BV.SLAN_CD,'PSX',DECODE(DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'ONE','SE1','SEA'),DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC')), 
            							'USOAK', DECODE(BKG.BLCK_STWG_CD, 'OA1', 'OA1' ,'', '', 'OAK'), 
            							'USLGB', DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','7','SEV','T','TRS', 'LOC'), 
            							'CAVAN', DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'') BLCK_STWG_CD
        FROM BKG_DG_CGO DG,
          BKG_BOOKING BKG,
          BKG_QUANTITY BQ,
          BKG_CONTAINER BC,
          BKG_VVD BV,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE BKG.BKG_NO = DG.BKG_NO
          AND BKG.BKG_NO = BQ.BKG_NO
          AND BKG.BKG_NO = BC.BKG_NO
          AND BKG.BKG_NO = BV.BKG_NO
          AND DG.CNTR_NO = BC.CNTR_NO
          AND BKG.BKG_STS_CD IN ( 'F','W')

		  AND BV.VSL_CD     = SUBSTR(@[in_vvd_cd], 1, 4)
		  AND BV.SKD_VOY_NO = SUBSTR(@[in_vvd_cd], 5, 4)
		  AND BV.SKD_DIR_CD = SUBSTR(@[in_vvd_cd], 9, 1)
		  AND BV.POL_CD     = @[in_pol_cd]
		  AND SUBSTR(BV.POL_YD_CD, 6, 2) like @[in_pol_yd_cd]||'%'

          AND VPS.CLPT_IND_SEQ = '1'
          AND VPS.VSL_CD = BV.VSL_CD
          AND VPS.SKD_VOY_NO = BV.SKD_VOY_NO
          AND VPS.SKD_DIR_CD = BV.SKD_DIR_CD
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND BV.POD_CD = VPS.VPS_PORT_CD
        UNION ALL
        SELECT 'RF' CGO_TYPE,
          RF.BKG_NO BKG_NO,
          DECODE(BV.POL_CD,BKG.POL_CD, '','TS') TS,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POL_CD), BKG.POL_CD) A_POL,

--BKG.POL_CD A_POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POD_CD), BKG.POD_CD) A_POD,

--BKG.POD_CD A_POD,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BV.POL_CD), BV.POL_CD) POL,

--CLL.POL_CD POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BV.POD_CD), BV.POD_CD) POD,

--CLL.POD_CD POD,
          0 CS,
          0 CS2,
          RF.CNTR_NO CNTR_NO,
          BC.CNTR_TPSZ_CD TP,
          BC.CNTR_WGT WGT,
          BC.WGT_UT_CD WGT_UNIT,
          ' ' CLASS_CD,
          ' ' UNNO,
          TO_CHAR(RF.CDO_TEMP, '90.0') TEMP,
          DECODE(RF.CNTR_VENT_TP_CD, 'P', RF.VENT_RTO||' %', 'C', RF.CBM_PER_HR_QTY||' CMH', '') VENT,
          RF.DIFF_RMK REMARK,
          ' ' MP,
          ' ' SG,
          ' ' LQ,
          ' ' VO_ID,
          BKG.STWG_CD STOW,
          BKG.BKG_CGO_TP_CD MTY_BKG_CD,
          VPS.CLPT_SEQ CLPT_SEQ,
          DECODE(NVL(BV.POD_CD,' '), 'USSEA', DECODE(BV.SLAN_CD,'PSX',DECODE(DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'ONE','SE1','SEA'),DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC')), 
            							'USOAK', DECODE(BKG.BLCK_STWG_CD, 'OA1', 'OA1' , '', '', 'OAK'), 
            							'USLGB', DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','7','SEV','T','TRS', 'LOC'), 
            							'CAVAN', DECODE(SUBSTR(NVL(BKG.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'') BLCK_STWG_CD
        FROM BKG_RF_CGO RF,
          BKG_BOOKING BKG,
          BKG_QUANTITY BQ,
          BKG_CONTAINER BC,
          BKG_VVD BV,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE BKG.BKG_NO = RF.BKG_NO
          AND BKG.BKG_NO = BQ.BKG_NO
          AND BKG.BKG_NO = BC.BKG_NO
          AND BKG.BKG_NO = BV.BKG_NO
          AND RF.CNTR_NO = BC.CNTR_NO
          AND BKG.BKG_STS_CD IN ( 'F','W')

		  AND BV.VSL_CD     = SUBSTR(@[in_vvd_cd], 1, 4)
		  AND BV.SKD_VOY_NO = SUBSTR(@[in_vvd_cd], 5, 4)
		  AND BV.SKD_DIR_CD = SUBSTR(@[in_vvd_cd], 9, 1)
		  AND BV.POL_CD     = @[in_pol_cd]
		  AND SUBSTR(BV.POL_YD_CD, 6, 2) like @[in_pol_yd_cd]||'%'

          AND VPS.CLPT_IND_SEQ = '1'
          AND VPS.VSL_CD = BV.VSL_CD
          AND VPS.SKD_VOY_NO = BV.SKD_VOY_NO
          AND VPS.SKD_DIR_CD = BV.SKD_DIR_CD
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND BV.POD_CD = VPS.VPS_PORT_CD )
    ORDER BY POL, CLPT_SEQ, POD, DECODE(CGO_TYPE, 'DG', '1', 'RF', '2', 'AK', '3', '4'), BKG_NO, CS ) 

#else 
/***************************** 기본 조회 *****************************/

SELECT ROWNUM SEQ,
  CGO_TYPE,
  BKG_NO,
  BKG_NO2,
  TS,
  A_POL,
  DECODE(A_POD, 'USSEA', DECODE(SLAN_CD, 'PNH', DECODE(BKG_BS, 'SE8','USSE8', DECODE(BLCK_STWG_CD, 'ONE', 'USSE1', 'USSEA')), DECODE(BLCK_STWG_CD, 'ONE', 'USSE1', 'USSEA')), 'CAVAN', DECODE(BLCK_STWG_CD, 'LOC', 'CAYVR', 'ONE', 'CAVA1', 'CAVAN'), 'CAPRR', DECODE(BKG_BS, 'PR1', 'CAPR1', 'CAPRR'), A_POD) A_POD,
  POL,
  DECODE(POD, 'USSEA', DECODE(SLAN_CD, 'PNH', DECODE(BKG_BS, 'SE8','USSE8', DECODE(BLCK_STWG_CD, 'ONE', 'USSE1', 'USSEA')), DECODE(BLCK_STWG_CD, 'ONE', 'USSE1', 'USSEA')), 'CAVAN', DECODE(BLCK_STWG_CD, 'LOC', 'CAYVR', 'ONE', 'CAVA1', 'CAVAN'), 'CAPRR', DECODE(BKG_BS, 'PR1', 'CAPR1', 'CAPRR'), POD) POD,
  CS,
  CS2,
  CNTR_NO,
  TP,
  ROUND(WGT, 1) WGT,
  WGT_UNIT,
  CLASS_CD,
  UNNO,
  TEMP,
  VENT,
  REMARK,
  MP,
  SG,
  LQ,
  VO_ID,
  STOW,
  MTY_BKG_CD,
  CLPT_SEQ,
  BLCK_STWG_CD,
  BKG_BS
FROM (
    SELECT CGO_TYPE,
      BKG_NO,
      BKG_NO BKG_NO2,
      TS,
      A_POL,
      A_POD,
      POL,
      POD,
      DECODE(CS, 0, '', CS) CS,
      DECODE(CS2, 0, '', CS2) CS2,
      CNTR_NO,
      TP,
      WGT,
      WGT_UNIT,
      CLASS_CD,
      UNNO,
      TEMP,
      VENT,
      REMARK,
      MP,
      SG,
      LQ,
      VO_ID,
      STOW,
      MTY_BKG_CD,
      CLPT_SEQ,
      BLCK_STWG_CD,
      BKG_BS,
	  SLAN_CD
    FROM (
        SELECT 'AK' CGO_TYPE,
          AWK.BKG_NO BKG_NO,
          CLL.KR_CLL_TS_CD TS,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POL_CD), BKG.POL_CD) A_POL,

--BKG.POL_CD A_POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POD_CD), BKG.POD_CD) A_POD,

--BKG.POD_CD A_POD,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POL_CD), CLL.POL_CD) POL,

--CLL.POL_CD POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POD_CD), CLL.POD_CD) POD,

--CLL.POD_CD POD,
          0 CS,
          0 CS2,
          AWK.CNTR_NO CNTR_NO,
          CLL.CNTR_TPSZ_CD TP,
          CLL.BL_WGT WGT,
          CLL.WGT_UT_CD WGT_UNIT,
          ' ' CLASS_CD,
          ' ' UNNO,
          ' ' TEMP,
          ' ' VENT,
          DECODE(NVL(AWK.OVR_FWRD_LEN, 0), 0, '', 'O.FWRD:'||AWK.OVR_FWRD_LEN||'  ')|| DECODE(NVL(AWK.OVR_BKWD_LEN, 0), 0, '', 'O.BKWD:'||AWK.OVR_BKWD_LEN||'  ')|| DECODE(NVL(AWK.OVR_HGT, 0), 0, '', 'O.HGT:'||AWK.OVR_HGT||'  ')|| DECODE(NVL(AWK.OVR_LF_LEN, 0), 0, '', 'O.LF:'||AWK.OVR_LF_LEN||'  ')|| DECODE(NVL(AWK.OVR_RT_LEN, 0), 0, '', 'O.RT:'||AWK.OVR_RT_LEN||'  ')|| DECODE(NVL(AWK.CRN_PST_STS_CD, '0'), '0', '', DECODE(CLL.CNTR_TPSZ_CD, 'A4', 'PS:'||AWK.CRN_PST_STS_CD||'  ', ''))|| DECODE(NVL(AWK.XTD_OVR_QTY, 0), 0, '', DECODE(CLL.CNTR_TPSZ_CD, 'A4', 'EH:'||AWK.XTD_OVR_QTY||'  ', '')) REMARK,
          ' ' MP,
          ' ' SG,
          ' ' LQ,
          TO_CHAR(AWK.OVR_VOID_SLT_QTY) VO_ID,
          CLL.STWG_CD STOW,
          CLL.MTY_BKG_CD MTY_BKG_CD,
          VPS.CLPT_SEQ CLPT_SEQ,
          CLL.BLCK_STWG_CD,
          BKG.BLCK_STWG_CD AS BKG_BS,
		  VPS.SLAN_CD
        FROM BKG_AWK_CGO AWK,
          BKG_BOOKING BKG,
          BKG_CSTMS_TML_KR_CLL CLL,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE BKG.BKG_NO = AWK.BKG_NO
          AND BKG.BKG_NO = CLL.BKG_NO
          AND AWK.CNTR_NO = CLL.CNTR_NO
          AND CLL.POL_CD = @[in_pol_cd]
          AND CLL.POL_YD_CD like @[in_pol_yd_cd]||'%'
          AND CLL.VSL_CD = SUBSTR(@[in_vvd_cd], 1, 4)
          AND CLL.SKD_VOY_NO = SUBSTR(@[in_vvd_cd], 5, 4)
          AND CLL.SKD_DIR_CD = SUBSTR(@[in_vvd_cd], 9, 1)
          AND CLL.POD_CD IS NOT NULL
          AND VPS.CLPT_IND_SEQ = '1' #if (${in_by_type} == 'TS' )
          AND CLL.KR_CLL_TS_CD IN ('TS','TT') #end #if (${in_by_type} == 'LOCAL' )
          AND CLL.KR_CLL_TS_CD IS NULL #end
          AND VPS.VSL_CD = CLL.VSL_CD
          AND VPS.SKD_VOY_NO = CLL.SKD_VOY_NO
          AND VPS.SKD_DIR_CD = CLL.SKD_DIR_CD
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND (DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.UN_LOC_CD
              OR DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.LOC_CD)
        UNION ALL
        SELECT 'DG' CGO_TYPE,
          DG.BKG_NO BKG_NO,
          CLL.KR_CLL_TS_CD TS,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POL_CD), BKG.POL_CD) A_POL,

--BKG.POL_CD A_POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POD_CD), BKG.POD_CD) A_POD,

--BKG.POD_CD A_POD,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POL_CD), CLL.POL_CD) POL,

--CLL.POL_CD POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POD_CD), CLL.POD_CD) POD,

--CLL.POD_CD POD,
          DG.DG_CNTR_SEQ CS,
          DG.CNTR_CGO_SEQ CS2,
          DG.CNTR_NO CNTR_NO,
          CLL.CNTR_TPSZ_CD TP,
          CLL.BL_WGT WGT,
          CLL.WGT_UT_CD WGT_UNIT,
          DECODE(DG.IMDG_SUBS_RSK_LBL_CD1, NULL, DG.IMDG_CLSS_CD, DG.IMDG_CLSS_CD||'('||DG.IMDG_SUBS_RSK_LBL_CD1||')') CLASS_CD,
          DG.IMDG_UN_NO UNNO,
          ' ' TEMP,
          ' ' VENT,
          DG.DIFF_RMK REMARK,
          DG.MRN_POLUT_FLG    MP,
          DG.HZD_CTNT         SG,
          DG.IMDG_LMT_QTY_FLG LQ,
          ' ' VO_ID,
          CLL.STWG_CD STOW,
          CLL.MTY_BKG_CD MTY_BKG_CD,
          VPS.CLPT_SEQ CLPT_SEQ ,
          CLL.BLCK_STWG_CD,
          BKG.BLCK_STWG_CD AS BKG_BS,
		  VPS.SLAN_CD
        FROM BKG_DG_CGO DG,
          BKG_BOOKING BKG,
          BKG_CSTMS_TML_KR_CLL CLL,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE BKG.BKG_NO = DG.BKG_NO
          AND BKG.BKG_NO = CLL.BKG_NO
          AND DG.CNTR_NO = CLL.CNTR_NO
          AND CLL.POL_CD = @[in_pol_cd]
          AND CLL.POL_YD_CD like @[in_pol_yd_cd]||'%'
          AND CLL.VSL_CD = SUBSTR(@[in_vvd_cd], 1, 4)
          AND CLL.SKD_VOY_NO = SUBSTR(@[in_vvd_cd], 5, 4)
          AND CLL.SKD_DIR_CD = SUBSTR(@[in_vvd_cd], 9, 1)
          AND CLL.POD_CD IS NOT NULL
          AND VPS.CLPT_IND_SEQ = '1' #if (${in_by_type} == 'TS' )
          AND CLL.KR_CLL_TS_CD IN ('TS','TT') #end #if (${in_by_type} == 'LOCAL' )
          AND CLL.KR_CLL_TS_CD IS NULL #end
          AND VPS.VSL_CD = CLL.VSL_CD
          AND VPS.SKD_VOY_NO = CLL.SKD_VOY_NO
          AND VPS.SKD_DIR_CD = CLL.SKD_DIR_CD
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND (DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.UN_LOC_CD
              OR DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.LOC_CD)
        UNION ALL
        SELECT 'RF' CGO_TYPE,
          RF.BKG_NO BKG_NO,
          CLL.KR_CLL_TS_CD TS,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POL_CD), BKG.POL_CD) A_POL,

--BKG.POL_CD A_POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POD_CD), BKG.POD_CD) A_POD,

--BKG.POD_CD A_POD,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POL_CD), CLL.POL_CD) POL,

--CLL.POL_CD POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POD_CD), CLL.POD_CD) POD,

--CLL.POD_CD POD,
          0 CS,
          0 CS2,
          RF.CNTR_NO CNTR_NO,
          CLL.CNTR_TPSZ_CD TP,
          CLL.BL_WGT WGT,
          CLL.WGT_UT_CD WGT_UNIT,
          ' ' CLASS_CD,
          ' ' UNNO,
          TO_CHAR(RF.CDO_TEMP, '90.0') TEMP,
          DECODE(RF.CNTR_VENT_TP_CD, 'P', RF.VENT_RTO||' %', 'C', RF.CBM_PER_HR_QTY||' CMH', '') VENT,
          RF.DIFF_RMK REMARK,
          ' ' MP,
          ' ' SG,
          ' ' LQ,
          ' ' VO_ID,
          CLL.STWG_CD STOW,
          CLL.MTY_BKG_CD MTY_BKG_CD ,
          VPS.CLPT_SEQ CLPT_SEQ,
          CLL.BLCK_STWG_CD,
          BKG.BLCK_STWG_CD AS BKG_BS,
		  VPS.SLAN_CD
        FROM BKG_RF_CGO RF,
          BKG_BOOKING BKG,
          BKG_CSTMS_TML_KR_CLL CLL,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE BKG.BKG_NO = RF.BKG_NO
          AND BKG.BKG_NO = CLL.BKG_NO
          AND RF.CNTR_NO = CLL.CNTR_NO
          AND CLL.POL_CD = @[in_pol_cd]
          AND CLL.POL_YD_CD like @[in_pol_yd_cd]||'%'
          AND CLL.VSL_CD = SUBSTR(@[in_vvd_cd], 1, 4)
          AND CLL.SKD_VOY_NO = SUBSTR(@[in_vvd_cd], 5, 4)
          AND CLL.SKD_DIR_CD = SUBSTR(@[in_vvd_cd], 9, 1)
          AND CLL.POD_CD IS NOT NULL
          AND VPS.CLPT_IND_SEQ = '1' #if (${in_by_type} == 'TS' )
          AND CLL.KR_CLL_TS_CD IN ('TS','TT') #end #if (${in_by_type} == 'LOCAL' )
          AND CLL.KR_CLL_TS_CD IS NULL #end
          AND VPS.VSL_CD = CLL.VSL_CD
          AND VPS.SKD_VOY_NO = CLL.SKD_VOY_NO
          AND VPS.SKD_DIR_CD = CLL.SKD_DIR_CD
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND (DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.UN_LOC_CD
              OR DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.LOC_CD)
        UNION ALL
        SELECT 'BB' CGO_TYPE,
          CLL.BKG_NO BKG_NO,
          CLL.KR_CLL_TS_CD TS,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POL_CD), BKG.POL_CD) A_POL,

--BKG.POL_CD A_POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = BKG.POD_CD), BKG.POD_CD) A_POD,

--BKG.POD_CD A_POD,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POL_CD), CLL.POL_CD) POL,

--CLL.POL_CD POL,
          NVL((
                SELECT UN_LOC_CD
                FROM MDM_LOCATION
                WHERE LOC_CD = CLL.POD_CD), CLL.POD_CD) POD,

--CLL.POD_CD POD,
          0 CS,
          0 CS2,
          CLL.CNTR_NO CNTR_NO,
          CLL.CNTR_TPSZ_CD TP,
          CLL.BL_WGT WGT,
          CLL.WGT_UT_CD WGT_UNIT,
          ' ' CLASS_CD,
          ' ' UNNO,
          ' ' TEMP,
          ' ' VENT,
          ' ' REMARK,
          ' ' MP,
          ' ' SG,
          ' ' LQ,
          ' ' VO_ID,
          CLL.STWG_CD STOW,
          CLL.MTY_BKG_CD MTY_BKG_CD,
          VPS.CLPT_SEQ CLPT_SEQ,
          CLL.BLCK_STWG_CD,
          BKG.BLCK_STWG_CD AS BKG_BS,
		  VPS.SLAN_CD
        FROM BKG_BOOKING BKG,
          BKG_CSTMS_TML_KR_CLL CLL,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE BKG.BKG_NO = CLL.BKG_NO
          AND CLL.POL_CD = @[in_pol_cd]
          AND CLL.POL_YD_CD like @[in_pol_yd_cd]||'%'
          AND CLL.VSL_CD = SUBSTR(@[in_vvd_cd], 1, 4)
          AND CLL.SKD_VOY_NO = SUBSTR(@[in_vvd_cd], 5, 4)
          AND CLL.SKD_DIR_CD = SUBSTR(@[in_vvd_cd], 9, 1)
          AND CLL.POD_CD IS NOT NULL
          AND CLL.CLL_RMK1 like 'BB%'
          AND CLL.CNTR_TPSZ_CD NOT LIKE 'R%'
          AND VPS.CLPT_IND_SEQ = '1' #if (${in_by_type} == 'TS' )
          AND CLL.KR_CLL_TS_CD IN ('TS','TT') #end #if (${in_by_type} == 'LOCAL' )
          AND CLL.KR_CLL_TS_CD IS NULL #end
          AND VPS.VSL_CD = CLL.VSL_CD
          AND VPS.SKD_VOY_NO = CLL.SKD_VOY_NO
          AND VPS.SKD_DIR_CD = CLL.SKD_DIR_CD
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND (DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.UN_LOC_CD
              OR DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.LOC_CD) ) #if (${in_sort_type} == '1' )
    ORDER BY POL, CLPT_SEQ, POD, DECODE(CGO_TYPE, 'DG', '1', 'RF', '2', 'AK', '3', '4'), BKG_NO, CS #end #if (${in_sort_type} == '2' )
    ORDER BY CGO_TYPE, BKG_NO #end #if (${in_sort_type} == '3' )
    ORDER BY CNTR_NO #end #if (${in_sort_type} == '4' )
    ORDER BY POL, CLPT_SEQ, POD, DECODE(BLCK_STWG_CD, 'LOC', '11', 'ONE', '12', 'TWO', '13', 'THR', '14', 'FOR', '15', 'FIV', '15.5', 'SEV', '15.7', 'HOT', '16', 'TRS', '17', '18'), BKG_NO #end )


#end
)			]]></sql>
			<params>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
