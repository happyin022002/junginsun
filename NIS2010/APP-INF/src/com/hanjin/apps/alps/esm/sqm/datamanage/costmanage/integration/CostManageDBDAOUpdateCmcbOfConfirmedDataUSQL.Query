<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostManageDBDAOUpdateCmcbOfConfirmedDataUSQL">
			<desc><![CDATA[CHM-201639850 CMCB 수정 데이터 Creation 버튼 생성 CSR]]></desc>
			<sql><![CDATA[
MERGE INTO SQM_CFM_QTA QTA                                               
USING SQM_QTA_LANE_OFC_COST CST                                                                          
    ON (                                                                                   
                   QTA.BSE_TP_CD                  = CST.BSE_TP_CD
               AND QTA.BSE_YR                     = CST.BSE_YR
               AND QTA.BSE_QTR_CD                 = CST.BSE_QTR_CD
               AND QTA.OFC_VW_CD                  = CST.OFC_VW_CD
               AND QTA.TRD_CD                     = CST.TRD_CD
               AND QTA.RLANE_CD                   = CST.RLANE_CD
               AND QTA.DIR_CD                     = CST.DIR_CD
--               AND QTA.RHQ_CD                     = CST.RHQ_CD
               AND QTA.RGN_OFC_CD                 = CST.RGN_OFC_CD
               AND QTA.BSE_TP_CD                  = 'Q'
               AND QTA.BSE_YR                     = @[bse_yr] 
               AND QTA.BSE_QTR_CD                 = @[bse_qtr_cd]
               AND QTA.QTA_TGT_CD                 = 'D'
               AND SUBSTR(QTA.QTA_RLSE_VER_NO,-2) = '02'
               
               AND QTA.BSE_TP_CD                  = @[bse_tp_cd]
               AND QTA.BSE_YR                     = @[bse_yr]
               AND QTA.BSE_QTR_CD                 = @[bse_qtr_cd]
               AND QTA.OFC_VW_CD                  = @[ofc_vw_cd]
               AND QTA.TRD_CD                     = @[trd_cd]
               AND QTA.RLANE_CD                   = @[rlane_cd]
               AND QTA.DIR_CD                     = @[dir_cd]
               AND QTA.RHQ_CD                     = @[rhq_cd]
               AND QTA.RGN_OFC_CD                 = @[rgn_ofc_cd]
       )                                                                                   
WHEN MATCHED THEN                                                                        
                 UPDATE SET  QTA.PA_CM_UC_AMT = CST.PA_CM_UC_AMT                    
                            ,QTA.RA_CM_UC_AMT = CST.RA_CM_UC_AMT                
                            ,QTA.UPD_USR_ID   = @[upd_usr_id]                                  
                            ,QTA.UPD_DT       = SYSDATE			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_vw_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
