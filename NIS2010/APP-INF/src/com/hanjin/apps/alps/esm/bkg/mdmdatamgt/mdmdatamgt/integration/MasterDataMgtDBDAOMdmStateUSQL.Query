<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MasterDataMgtDBDAOMdmStateUSQL">
			<desc><![CDATA[중국 Booking Agent 정보 등록 화면]]></desc>
			<sql><![CDATA[
UPDATE MDM_STATE SET
IDA_STE_CD = @[ind_ste_cd]
, IDA_TERR_DIV_CD = @[ind_terr_div_cd]
, UPD_USR_ID = @[upd_usr_id]
WHERE STE_NM = @[ste_nm]
AND STE_CD = @[ste_cd]			]]></sql>
			<params>
				<param name="ind_ste_cd" type="12" value="" out="N"/>
				<param name="ind_terr_div_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ste_nm" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
