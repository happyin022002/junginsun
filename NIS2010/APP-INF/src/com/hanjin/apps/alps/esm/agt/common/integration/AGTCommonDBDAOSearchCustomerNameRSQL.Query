<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchCustomerNameRSQL">
			<desc><![CDATA[searchCustomerName]]></desc>
			<sql><![CDATA[
SELECT 
	CUST_CNT_CD || LTRIM (TO_CHAR (CUST_SEQ, '000000')) AS CUST_CD, 
	CUST_LGL_ENG_NM AS CUST_NM 
FROM MDM_CUSTOMER 
WHERE CUST_CNT_CD = @[cust_cnt_cd] 
AND CUST_SEQ = @[iCust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="Y"/>
				<param name="iCust_seq" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
