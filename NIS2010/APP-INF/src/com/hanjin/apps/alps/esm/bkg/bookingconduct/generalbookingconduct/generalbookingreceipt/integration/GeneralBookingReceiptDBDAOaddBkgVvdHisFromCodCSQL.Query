<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOaddBkgVvdHisFromCodCSQL">
			<desc><![CDATA[COD CONfirm시 변경된 값을 bkg_vvd_his에 넣는다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_VVD_HIS (
   BKG_NO 
   ,CORR_NO
   ,VSL_PRE_PST_CD
   ,VSL_SEQ
   ,SLAN_CD
   ,VSL_CD
   ,SKD_VOY_NO
   ,SKD_DIR_CD
   ,POL_CLPT_IND_SEQ
   ,POL_CD
   ,POL_YD_CD
   ,POD_CD
   ,POD_YD_CD
   ,POD_CLPT_IND_SEQ
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
) 
select BKG_NO
      ,@[corr_no]
	  ,VSL_PRE_PST_CD
	  ,VSL_SEQ
	  ,SLAN_CD
	  ,VSL_CD
	  ,SKD_VOY_NO
	  ,SKD_DIR_CD
	  ,POL_CLPT_IND_SEQ
	  ,substr(POL_YD_CD,0,5)
	  ,POL_YD_CD
	  ,substr(POD_YD_CD,0,5)
	  ,POD_YD_CD 
	  ,POD_CLPT_IND_SEQ
	  ,@[usr_id]
	  ,sysdate
	  ,@[usr_id]
	  ,sysdate
 from bkg_cod_vvd
 where bkg_no = @[bkg_no]
 and cod_rqst_seq = @[cod_rqst_seq]
 and vvd_op_cd='N'			]]></sql>
			<params>
				<param name="corr_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
