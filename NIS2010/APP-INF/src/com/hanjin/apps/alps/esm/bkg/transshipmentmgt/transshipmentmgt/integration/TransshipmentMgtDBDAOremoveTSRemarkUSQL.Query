<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransshipmentMgtDBDAOremoveTSRemarkUSQL">
			<desc><![CDATA[bkg_ts_rmk에 삭제 처리한다. ts_rmk_delt_flg = 'Y']]></desc>
			<sql><![CDATA[
UPDATE BKG_TS_RMK SET 
	TS_RMK_DELT_FLG = 'Y'
,	UPD_USR_ID = @[usr_id]
,	UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
