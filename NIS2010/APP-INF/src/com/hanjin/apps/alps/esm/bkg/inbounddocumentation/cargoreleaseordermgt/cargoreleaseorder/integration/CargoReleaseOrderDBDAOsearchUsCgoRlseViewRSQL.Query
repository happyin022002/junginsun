<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchUsCgoRlseViewRSQL">
			<desc><![CDATA[BKG_CGO_RLSE 단건 조회]]></desc>
			<sql><![CDATA[
SELECT A.BL_NO
, NVL(BK.POD_CD,'') BKG_POD_CD
, SUBSTR(BK.POD_NOD_CD, 6, 2) BKG_POD_YD_CD
FROM BKG_CGO_RLSE A, BKG_BOOKING BK
WHERE A.BL_NO = @[bl_no]
AND NVL(OBL_RDEM_FLG,'N') ='Y' 
AND NVL(FRT_CLT_FLG,'N') ='Y'
AND MRN_TML_EDI_LST_MSG_ID = 'CR'
AND A.BL_NO = BK.BL_NO			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
