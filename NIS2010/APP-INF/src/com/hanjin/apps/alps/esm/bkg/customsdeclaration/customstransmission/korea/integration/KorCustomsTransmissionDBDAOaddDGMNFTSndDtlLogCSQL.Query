<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOaddDGMNFTSndDtlLogCSQL">
			<desc><![CDATA[DGMNFT Send Log Detail]]></desc>
			<sql><![CDATA[
INSERT 
  INTO BKG_CSTMS_KR_SND_LOG_DTL
     ( MSG_LOG_TP_ID
     , SND_DT
     , OFC_CD
     , FLT_FILE_REF_NO
     , LOG_DTL_SEQ
     , EDI_SND_MSG
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , MF_SND_SEQ
     )
SELECT '6TC'
     , TO_DATE(@[snd_dt], 'YYYYMMDD HH24:MI:SS')
     , @[ofc_cd]
     , @[flt_file_ref_no]
     , NVL(MAX(LOG_DTL_SEQ), 0) + 1 
     , @[edi_snd_msg]
     , @[user_id]
     , SYSDATE
     , @[user_id]
     , SYSDATE 
	 , 1
  FROM BKG_CSTMS_KR_SND_LOG_DTL
 WHERE MSG_LOG_TP_ID =	'6TC'
   AND SND_DT		 =	TO_DATE(@[snd_dt], 'YYYYMMDD HH24:MI:SS')
   AND OFC_CD		 =	@[ofc_cd]			]]></sql>
			<params>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="edi_snd_msg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
