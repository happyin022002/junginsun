<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOArCsrNoRSQL">
			<desc><![CDATA[20T 전표 산출]]></desc>
			<sql><![CDATA[
SELECT B.SLP_TP_CD||B.SLP_FUNC_CD||B.SLP_OFC_CD||B.SLP_ISS_DT||B.SLP_SER_NO PCSR_NO
FROM FMS_CONSULTATION A, FMS_CONSULTATION B
WHERE 1 = 1
  AND A.FLET_CTRT_NO = B.FLET_CTRT_NO
  AND B.SLP_TP_CD = '20'
  AND B.APRO_FLG = 'Y'
  AND B.CSR_DESC LIKE '%OUTLAY COMM'
  AND TO_CHAR(A.CRE_DT, 'YYYYMMDD') = TO_CHAR(B.CRE_DT, 'YYYYMMDD')
  AND A.CRE_DT < B.CRE_DT
  AND A.SLP_TP_CD||A.SLP_FUNC_CD||A.SLP_OFC_CD||A.SLP_ISS_DT||A.SLP_SER_NO 
         = @[slp_tp_cd]||@[slp_func_cd]||@[slp_ofc_cd]||@[slp_iss_dt]||@[slp_ser_no]
  AND ROWNUM = 1			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
