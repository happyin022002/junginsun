<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqBreakbulkDBDAOsearchPriScqBbCntrRSQL">
			<desc><![CDATA[searchPriScqBbCntr]]></desc>
			<sql><![CDATA[
SELECT SCQ_RQST_NO
     , SCQ_VER_NO
     , CNTR_GRP_VER_NO
     , CNTR_SEQ
     , CNTR_TPSZ_CD
     , BKG_QTY
     , CNTR_QTY
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
  FROM PRI_SCQ_BB_CNTR
 WHERE SCQ_RQST_NO = @[scq_rqst_no]
   AND SCQ_VER_NO  = @[scq_ver_no]
   AND CNTR_GRP_VER_NO = ( SELECT  MAX ( CNTR_GRP_VER_NO ) 
                           FROM    PRI_SCQ_BB_CNTR
                           WHERE   SCQ_RQST_NO = @[scq_rqst_no]
                           AND     SCQ_VER_NO  = @[scq_ver_no] )			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
