<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerSpaceGuaranteeDBDAOMultiSaqCntrSzConvLaneUSQL">
			<desc><![CDATA[Lane,선사별, Type Size 예외사항에 대한 UPDATE]]></desc>
			<sql><![CDATA[
UPDATE SAQ_CNTR_SZ_CONV_LANE
   SET RLANE_CD                 = @[rlane_cd]                ,
       DIR_CD                   = @[dir_cd]                  ,
       CRR_CD                   = @[crr_cd]                  ,
       TEU_20FT_CONV_RTO        = @[teu_20ft_conv_rto]       ,
       TEU_40FT_CONV_RTO        = @[teu_40ft_conv_rto]       ,
       TEU_40FT_HC_CONV_RTO     = @[teu_40ft_hc_conv_rto]    ,
       TEU_45FT_HC_CONV_RTO     = @[teu_45ft_hc_conv_rto]    ,
       OVR_TEU_20FT_CONV_RTO    = @[ovr_teu_20ft_conv_rto]   ,
       OVR_TEU_40FT_CONV_RTO    = @[ovr_teu_40ft_conv_rto]   ,
       OVR_TEU_40FT_HC_CONV_RTO = @[ovr_teu_40ft_hc_conv_rto],
       OVR_TEU_45FT_HC_CONV_RTO = @[ovr_teu_45ft_hc_conv_rto],
       UPD_USR_ID               = @[upd_usr_id]              ,
       UPD_DT                   = SYSDATE
 WHERE RLANE_CD = @[rlane_cd]
   AND DIR_CD   = @[dir_cd]
   AND CRR_CD   = @[crr_cd]			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="teu_20ft_conv_rto" type="12" value="" out="N"/>
				<param name="teu_40ft_conv_rto" type="12" value="" out="N"/>
				<param name="teu_40ft_hc_conv_rto" type="12" value="" out="N"/>
				<param name="teu_45ft_hc_conv_rto" type="12" value="" out="N"/>
				<param name="ovr_teu_20ft_conv_rto" type="12" value="" out="N"/>
				<param name="ovr_teu_40ft_conv_rto" type="12" value="" out="N"/>
				<param name="ovr_teu_40ft_hc_conv_rto" type="12" value="" out="N"/>
				<param name="ovr_teu_45ft_hc_conv_rto" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
