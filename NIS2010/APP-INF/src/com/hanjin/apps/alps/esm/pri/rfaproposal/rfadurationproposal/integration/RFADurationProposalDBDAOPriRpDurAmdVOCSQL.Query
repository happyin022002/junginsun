<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFADurationProposalDBDAOPriRpDurAmdVOCSQL">
			<desc><![CDATA[RFADurationProposalDBPriRpDurAmdVO]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_DUR(
    PROP_NO,
    AMDT_SEQ,
    CTRT_EFF_DT,
    CTRT_EXP_DT,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,    
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    PROP_NO			,
    AMDT_SEQ+1		,
    CTRT_EFF_DT		,
    DECODE(@[exp_dt], NULL, CTRT_EXP_DT, TO_DATE(@[exp_dt],'YYYY-MM-DD'))	,
	DECODE(@[exp_dt], NULL, PRC_PROG_STS_CD	,'I') ,
    DECODE(@[exp_dt], NULL, SRC_INFO_CD, 'AM')		,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    @[cre_usr_id]	,
    SYSDATE			,
    @[upd_usr_id]	,
    SYSDATE			,
	DECODE(@[exp_dt], NULL, N1ST_CMNC_AMDT_SEQ, AMDT_SEQ + 1)
FROM
    PRI_RP_DUR
WHERE
    PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
