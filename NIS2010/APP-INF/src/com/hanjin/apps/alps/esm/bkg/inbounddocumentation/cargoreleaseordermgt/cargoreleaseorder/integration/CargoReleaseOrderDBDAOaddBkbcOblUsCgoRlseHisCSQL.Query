<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOaddBkbcOblUsCgoRlseHisCSQL">
			<desc><![CDATA[...]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CGO_RLSE_HIS (
            BL_NO              ,
            HIS_SEQ            ,
            FRT_CLT_FLG        ,
            OBL_RDEM_FLG       ,
            CSTMS_CLR_CD       ,
            CSTMS_DSPO_CD      ,
            CUST_EDI_SND_CD    ,
            MRN_TML_EDI_SND_CD ,
            INLND_YD_EDI_SND_CD,
            EVNT_OFC_CD        ,
            CGOR_EVNT_TP_CD    ,
            EVNT_USR_ID        ,
            EVNT_DT            ,
            CGOR_TEAM_CD       ,
            CGOR_EVNT_NM       ,
            EVNT_GDT           ,
            CRE_USR_ID         ,
            CRE_DT             ,
            UPD_USR_ID         ,
            UPD_DT             ,
            CSTMS_LOC_CD	   ,
			DO_HLD_FLG)
    (SELECT @[bl_no],
            NVL(MAX(HIS_SEQ),0) + 1,
            NVL(MAX(FRT_CLT_FLG),'N'),
            DECODE(@[cgor_team_cd],'S',@[obl_rdem_flg],NVL(MAX(OBL_RDEM_FLG),'N')),
            NVL(MAX(CSTMS_CLR_CD),'N'),
            MAX(CSTMS_DSPO_CD),
            NULL,
            NULL,
            NULL,
            @[evnt_ofc_cd],
            'O'  ,
            @[evnt_usr_id],
            NVL(MAX((SELECT OBL_RDEM_LST_DT FROM BKG_CGO_RLSE WHERE BL_NO = @[bl_no])),
                GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,'USNYC')),
            @[cgor_team_cd],
            @[cgo_evnt_nm],
            NVL(
        	    GLOBALDATE_PKG.TIME_CONV_FNC('USNYC',
                  MAX((SELECT OBL_RDEM_LST_DT FROM BKG_CGO_RLSE WHERE BL_NO = @[bl_no])),'GMT'),
                GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,'GMT')), 
            @[evnt_usr_id], 
            SYSDATE,
            @[evnt_usr_id], 
            SYSDATE,
            MAX(CSTMS_LOC_CD),
 	        NVL(MAX((
		   		SELECT A.DO_HLD_FLG DO_HLD_FLG
		   		FROM BKG_DO_REF A,
			  		 BKG_BOOKING B
		 		WHERE A.BKG_NO(+) = B.BKG_NO
		   		AND B.BL_NO = @[bl_no]
			)),'N') DO_HLD_FLG	
       FROM BKG_CGO_RLSE_HIS
      WHERE BL_NO = @[bl_no]  
      AND HIS_SEQ = (SELECT MAX(HIS_SEQ)      
                         FROM BKG_CGO_RLSE_HIS
                        WHERE BL_NO = @[bl_no]))			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cgor_team_cd" type="12" value="" out="N"/>
				<param name="obl_rdem_flg" type="12" value="" out="N"/>
				<param name="evnt_ofc_cd" type="12" value="" out="N"/>
				<param name="evnt_usr_id" type="12" value="" out="N"/>
				<param name="cgo_evnt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
