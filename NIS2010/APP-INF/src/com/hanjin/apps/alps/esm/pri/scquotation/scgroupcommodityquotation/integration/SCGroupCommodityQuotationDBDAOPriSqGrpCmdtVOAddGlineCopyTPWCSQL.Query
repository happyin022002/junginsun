<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupCommodityQuotationDBDAOPriSqGrpCmdtVOAddGlineCopyTPWCSQL">
			<desc><![CDATA[GLINE COPY TPW]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SQ_GRP_CMDT (
	QTTN_NO
,	QTTN_VER_NO
,	GRP_CMDT_SEQ
,	PRC_GRP_CMDT_CD
,	PRC_GRP_CMDT_DESC
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT @[qttn_no] AS PROP_NO
     , @[qttn_ver_no] AS AMDT_SEQ
     , @[new_grp_cmdt_seq] AS GRP_CMDT_SEQ
     , PRC_GRP_CMDT_CD
     , PRC_GRP_CMDT_DESC
	 , 'GC'
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM PRI_SG_GRP_CMDT
WHERE SVC_SCP_CD = @[svc_scp_cd]
AND   GLINE_SEQ = @[gline_seq]
AND   PRC_CUST_TP_CD = @[prc_cust_tp_cd]
AND   GRP_CMDT_SEQ = @[grp_cmdt_seq]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="new_grp_cmdt_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="prc_cust_tp_cd" type="12" value="" out="N"/>
				<param name="grp_cmdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
