<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltSearchMOFLaneHistoryListCSQL">
			<desc><![CDATA[Korea MOF Filing (Base Table) - Scope & Location History]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_MOF_LANE_MAPG_HIS
(
	ORG_DEST_TP_CD,
	MAPG_SEQ,
	HIS_SEQ,
	LOC_CD,
	MOF_LANE_CD,
	MOF_LOC_ID,
	MOF_LOC_NM,
	MAPG_RMK,
	FILE_USE_ONY_FLG,
	DELT_FLG,
	CRE_OFC_CD,
	CRE_USR_ID,
	CRE_DT,
	UPD_OFC_CD,
	UPD_USR_ID,
	UPD_DT
)
VALUES
(
	@[org_dest_tp_cd],
	@[mapg_seq],
	(SELECT NVL(MAX(HIS_SEQ),0)+1 FROM PRI_MOF_LANE_MAPG_HIS),
	@[loc_cd],
	@[mof_lane_cd],
	@[mof_loc_id],
	@[mof_loc_nm],
	@[mapg_rmk],
	DECODE(NVL(@[file_use_ony_flg],'0'),'0','N','1','Y'),	
	DECODE(NVL(@[delt_flg],'0'),'0','N','1','Y'),	
	@[cre_ofc_cd],
	@[cre_usr_id],
	SYSDATE,	
	@[upd_ofc_cd],
	@[upd_usr_id],
	SYSDATE
)			]]></sql>
			<params>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="mapg_seq" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="mof_lane_cd" type="12" value="" out="N"/>
				<param name="mof_loc_id" type="12" value="" out="N"/>
				<param name="mof_loc_nm" type="12" value="" out="N"/>
				<param name="mapg_rmk" type="12" value="" out="N"/>
				<param name="file_use_ony_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
