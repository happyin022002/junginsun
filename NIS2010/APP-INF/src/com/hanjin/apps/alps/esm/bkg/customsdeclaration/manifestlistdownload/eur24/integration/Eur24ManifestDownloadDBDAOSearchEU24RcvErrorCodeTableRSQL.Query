<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOSearchEU24RcvErrorCodeTableRSQL">
			<desc><![CDATA[Eur24ManifestDownloadDBDAOSearchEU24RcvErrorCodeTableRSQL
2012.01.12 김보배 [CHM-201215563] [BKG] [ENS] Italia Error Code description추가 및 로직보완]]></desc>
			<sql><![CDATA[
/* EU24RcvErrorCodeTable  VO */
SELECT 
	 CNT_CD
	,CSTMS_DIV_ID AS EU24_ERR_CD
	,ATTR_CTNT1   AS ERROR_CODE
	,ATTR_CTNT2   AS ERROR_DESCRIPTION
	,ATTR_CTNT3   AS REMARK
	,CSTMS_DIV_ID_SEQ
FROM BKG_CSTMS_CD_CONV_CTNT
WHERE 1=1
AND CSTMS_DIV_ID = 'EU24_ERR_CD'

#if (${cnt_cd} != '') 
	AND CNT_CD = @[cnt_cd]
#end

ORDER BY CNT_CD, length(ERROR_CODE),CONVERT(ERROR_CODE, 'US8ICL'), CSTMS_DIV_ID_SEQ			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
