<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOSearchSimReportMasterCountRSQL">
			<desc><![CDATA[리포트 마스터 카운트 조회]]></desc>
			<sql><![CDATA[
    SELECT COUNT(*) CNT
      FROM MAS_SIM_RPT_MST
     WHERE SIM_DT = @[f_sim_dt]
       AND SIM_NO = @[f_sim_no]
			]]></sql>
			<params>
				<param name="f_sim_dt" type="12" value="" out="N"/>
				<param name="f_sim_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
