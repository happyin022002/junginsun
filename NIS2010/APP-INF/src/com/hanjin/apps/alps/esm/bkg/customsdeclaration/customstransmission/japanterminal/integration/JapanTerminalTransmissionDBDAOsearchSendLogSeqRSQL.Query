<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchSendLogSeqRSQL">
			<desc><![CDATA[SendLogSeq 조회해 온다]]></desc>
			<sql><![CDATA[
SELECT
	NVL(MAX(LOG_SEQ)+1,0) LOG_SEQ
FROM 
	BKG_CSTMS_JP_SND_LOG
WHERE JP_SND_LOG_ID = 'BKRBKC'
AND SND_DT = TO_DATE(@[snd_dt],'YYYY-MM-DD HH24:MI:SS')			]]></sql>
			<params>
				<param name="snd_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
