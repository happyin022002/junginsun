<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAOAddTsPlanGuideAtchCSQL">
			<desc><![CDATA[[T/S Plan & guide Attach]을 [추가]합니다.]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_TS_PLN_GID_FILE_STO 
(
   REP_TRD_CD
  ,REP_SUB_TRD_CD
  ,RLANE_CD
  ,VVD_CD
  ,IRR_PORT_CD
  ,IRR_YD_CD
  ,FILE_SEQ
  ,FILE_NM
  ,FILE_PATH_RMK
  ,FILE_SAV_ID
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
  ,PLN_SEQ
) 
VALUES 
(
  @[rep_trd_cd]
 ,@[rep_sub_trd_cd]
 ,@[rlane_cd]
 ,@[vvd_cd]
 ,@[irr_port_cd]
 ,@[irr_yd_cd]
 ,(
	SELECT NVL(MAX(FILE_SEQ),0)+1
	FROM SPC_TS_PLN_GID_FILE_STO
	WHERE REP_TRD_CD        = @[rep_trd_cd]
      AND REP_SUB_TRD_CD    = @[rep_sub_trd_cd]
      AND RLANE_CD          = @[rlane_cd]
      AND VVD_CD            = @[vvd_cd]
      AND IRR_PORT_CD       = @[irr_port_cd]
      AND IRR_YD_CD         = @[irr_yd_cd]
	  AND PLN_SEQ           = @[pln_seq]
  )
  ,@[file_nm]
  ,@[file_path_rmk]
  ,@[file_sav_id]
  ,@[cre_usr_id]
  ,sysdate
  ,@[upd_usr_id]
  ,sysdate
  ,@[pln_seq]
)			]]></sql>
			<params>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="rep_sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="irr_port_cd" type="12" value="" out="N"/>
				<param name="irr_yd_cd" type="12" value="" out="N"/>
				<param name="pln_seq" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_path_rmk" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
