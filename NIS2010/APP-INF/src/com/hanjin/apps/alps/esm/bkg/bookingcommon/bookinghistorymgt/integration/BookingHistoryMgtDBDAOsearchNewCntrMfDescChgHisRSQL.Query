<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOsearchNewCntrMfDescChgHisRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOsearchNewCntrMfDescChgHisRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT BKG_DESC_CNG_ITM_CD
     , CNTR_NO
     , BKG_NO
  FROM 
    (
	    SELECT '7' BKG_DESC_CNG_ITM_CD
            , NOW.CNTR_MF_GDS_DESC CRNT_CTNT
			, NOW.CNTR_NO
            , NOW.BKG_NO
#if (${ca_flg} == 'Y')
          FROM BKG_CNTR_MF_DESC_HIS NOW 
         WHERE NOW.BKG_NO  = @[bkg_no] 
	       AND NOW.CORR_NO = 'TMP0000001' 
#else
          FROM BKG_CNTR_MF_DESC NOW
         WHERE NOW.BKG_NO = @[bkg_no] 
#end
#if ($field_list.size() > 0)
           AND (BKG_NO, CNTR_NO, CNTR_MF_SEQ) NOT IN (
           #foreach($field_id in ${field_list}) 
               #if($velocityCount < $field_list.size()) $field_id,  #else $field_id  #end
           #end
           )
#end 
    )
 WHERE CRNT_CTNT IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
