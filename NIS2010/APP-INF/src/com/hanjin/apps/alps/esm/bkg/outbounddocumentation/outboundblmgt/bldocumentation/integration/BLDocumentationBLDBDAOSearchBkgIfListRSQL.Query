<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchBkgIfListRSQL">
			<desc><![CDATA[BLDocumentationBLDBDAOSearchBkgIfList]]></desc>
			<sql><![CDATA[
SELECT  BK.BKG_NO,
        SKD1.VSL_CD || SKD1.SKD_VOY_NO || SKD1.SKD_DIR_CD VVD,
        BK.POL_CD,
        BK.POD_CD,
        TO_CHAR(VPS_ETD_DT,'YYYY-MM-DD') VPS_ETD_DT
FROM   BKG_VVD VVD, BKG_BOOKING BK, VSK_VSL_PORT_SKD SKD1
  WHERE  1=1
#if (${bkg_no} != '')
    AND BK.BKG_NO = @[bkg_no]
#end
#if (${bl_no} != '')
    AND BK.BKG_NO = @[bl_no]
#end
#if (${pol_cd} != '')
    AND VVD.POL_CD = @[pol_cd]
#end
#if (${pod_cd} != '')
    AND VVD.POD_CD = @[pod_cd]
#end
#if (${fm_dt} != '')
    AND SKD1.VPS_ETD_DT >= TO_DATE(@[fm_dt] ,'YYYY-MM-DD')
#end
#if (${to_dt} != '')
    AND SKD1.VPS_ETD_DT <= TO_DATE(@[to_dt] ,'YYYY-MM-DD') +0.99999
#end
#if (${vvd_cd} != '')
    AND VVD.VSL_CD = SUBSTR(@[vvd_cd],1,4)
    AND VVD.SKD_VOY_NO = SUBSTR(@[vvd_cd],4,4)
    AND VVD.SKD_DIR_CD = SUBSTR(@[vvd_cd],8,1)
#end
    AND  SKD1.VSL_CD = VVD.VSL_CD
    AND  SKD1.SKD_VOY_NO = VVD.SKD_VOY_NO
    AND  SKD1.SKD_DIR_CD = VVD.SKD_DIR_CD
    AND  SKD1.VPS_PORT_CD = VVD.POL_CD
    AND  SKD1.CLPT_IND_SEQ = VVD.POL_CLPT_IND_SEQ
    AND  VVD.BKG_NO = BK.BKG_NO
    AND  VVD.POL_CD = BK.POL_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
