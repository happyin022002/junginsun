<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsManifestListDownloadDBDAOcheckIfPrvPortValidRSQL">
			<desc><![CDATA[s]]></desc>
			<sql><![CDATA[
SELECT --DECODE(SIGN(COUNT(VSL_CD)), 1, 'true', 'false') AS IS_VSL_CD
   SIGN(COUNT(VSL_CD)) AS IS_VSL_CD
   FROM VSK_VSL_PORT_SKD
 WHERE VSL_CD       = SUBSTR (@[vvd], 1, 4)
     AND SKD_VOY_NO = SUBSTR (@[vvd], 5, 4)
     AND SKD_DIR_CD = SUBSTR (@[vvd], 9, 1)
     AND VPS_PORT_CD = @[prv_prot]
     AND NVL(SKD_CNG_STS_CD, ' ') <> 'S'
     AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="prv_prot" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
