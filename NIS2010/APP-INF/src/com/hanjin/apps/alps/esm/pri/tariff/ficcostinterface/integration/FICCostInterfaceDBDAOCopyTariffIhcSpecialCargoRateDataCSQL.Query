<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOCopyTariffIhcSpecialCargoRateDataCSQL">
			<desc><![CDATA[CopyTariffIhcSpecialCargoRateData
2013.08.06 전윤주 [CHM-201326196] Overweight Fixed AMT 를 Local curr. 금액 그대로 IF 하는 컬럼 추가]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_IHC_SPCL_CGO_RT
  (SVC_SCP_CD,
   ORG_DEST_TP_CD,
   IHC_TRF_NO,
   PRC_TRSP_MOD_CD,
   PRC_INLND_TRF_CNTR_TPSZ_CD,
   DG_FLT_PCT_TP_CD,
   DG_RT_AMT,
   DG_RT_RTO,
   MIN_CGO_WGT,
   MAX_CGO_WGT,
   OVR_WGT_FLT_PCT_TP_CD,
   OVR_WGT_RT_AMT,
   OVR_WGT_RT_RTO,
   DCGO_SVC_FLG,
   OVR_WGT_CGO_SVC_FLG,
   LOCL_CURR_CD,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   LOCL_OVR_WGT_RT_AMT )
  SELECT  @[svc_scp_cd]
        , @[org_dest_tp_cd]
        , @[ihc_trf_no] 
        , PRC_TRSP_MOD_CD
        , PRC_INLND_TRF_CNTR_TPSZ_CD
        , DG_FLT_PCT_TP_CD
        , DG_RT_AMT
        , DG_RT_RTO
        , MIN_CGO_WGT
        , MAX_CGO_WGT
        , OVR_WGT_FLT_PCT_TP_CD
        , OVR_WGT_RT_AMT
        , OVR_WGT_RT_RTO
        , DCGO_SVC_FLG
        , OVR_WGT_CGO_SVC_FLG
        , LOCL_CURR_CD
        , @[cre_usr_id]
        , SYSDATE
        , @[upd_usr_id]
        , SYSDATE 
        , LOCL_OVR_WGT_RT_AMT
   FROM PRI_TRF_IHC_SPCL_CGO_RT
  WHERE 1=1
    AND SVC_SCP_CD       = @[ori_svc_scp_cd] 
    AND ORG_DEST_TP_CD   = @[org_dest_tp_cd]
    AND IHC_TRF_NO       = @[ori_ihc_trf_no]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ori_svc_scp_cd" type="12" value="" out="N"/>
				<param name="ori_ihc_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
