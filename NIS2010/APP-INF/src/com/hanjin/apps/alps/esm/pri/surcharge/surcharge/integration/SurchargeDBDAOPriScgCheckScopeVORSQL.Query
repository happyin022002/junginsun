<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOPriScgCheckScopeVORSQL">
			<desc><![CDATA[Service Scope 정합성 체크]]></desc>
			<sql><![CDATA[
SELECT @[trd_cd] TRD_CD,
       @[svc_scp_cd] SCV_SCP_CD,
       CASE
         WHEN EXISTS (SELECT '1'
          FROM PRI_SCG_TRD_SVC_SCP_MAPG
         WHERE TRD_CD = @[trd_cd]
           AND SVC_SCP_CD = @[svc_scp_cd]
           AND ROWNUM = 1) THEN 'Y'
         ELSE 'N'
       END CHK_FLG
  FROM DUAL			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
