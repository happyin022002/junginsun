<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CsScreenDBDAOremoveUsCustSvcInstrUSQL">
			<desc><![CDATA[~~~]]></desc>
			<sql><![CDATA[
UPDATE BKG_USA_CUST_SVC_INSTR 
   SET DELT_FLG     = 'Y'
      ,UPD_USR_ID   = @[upd_usr_id]
      ,UPD_DT       = SYSDATE
WHERE BKG_NO        = @[bkg_no]
AND   INSTR_RMK_SEQ = @[instr_rmk_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="instr_rmk_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
