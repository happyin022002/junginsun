<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCTransportationAdditionalChargeProposalDBDAOExistsLocationVORSQL">
			<desc><![CDATA[Location의 존재 유무를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT LOC_CD AS CD
FROM MDM_LOCATION 
WHERE LOC_CD= @[cd]
AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
