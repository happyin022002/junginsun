<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchCustCntRSQL">
			<desc><![CDATA[Customer Counter 조회]]></desc>
			<sql><![CDATA[
SELECT  COUNT(*) CNT
FROM    BKG_CSTMS_KR_CUST
WHERE   BKG_NO        =   @[bkg_no]
AND CSTMS_DECL_TP_CD  =   @[cstms_decl_tp_cd]
AND DMST_PORT_CD      =   @[port_cd]
AND TRNS_SEQ          =   @[trns_seq]
AND BKG_CUST_TP_CD    IN  ('S',   'C',    'N')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="trns_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
