<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchZoneRSQL">
			<desc><![CDATA[BookingUtilDBDAOSearchZoneRSQL]]></desc>
			<sql><![CDATA[
SELECT count(1) CNT
  FROM mdm_zone
 WHERE loc_cd = @[loc_cd]
#if (${zn_cd}!= '')
   AND zn_cd = @[zn_cd]
#end 
   AND delt_flg	= 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="zn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
