<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchHtsCodeDescRSQL">
			<desc><![CDATA[HTS Code 체크]]></desc>
			<sql><![CDATA[
SELECT 
	HAMO_TRF_CD
,	HAMO_CD_DESC
,	FDA_DECL_FLG
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_HAMO_TRF
WHERE	HAMO_TRF_CD = @[hamo_trf_cd]
AND	DELT_FLG = 'N'
AND	HAMO_TP_CD = @[hamo_tp_cd]
AND ROWNUM =1			]]></sql>
			<params>
				<param name="hamo_trf_cd" type="12" value="" out="N"/>
				<param name="hamo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
