<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeMappingDBDAOUpdateNewLaneOfficeCmcbUSQL">
			<desc><![CDATA[New Lane - Office의 Cmcb를 업데이트한다.]]></desc>
			<sql><![CDATA[
UPDATE SQM_QTA_NEW_LANE_OFC_COST
   SET PA_CM_UC_AMT = @[pa_cm_uc_amt] 
      ,RA_CM_UC_AMT = @[ra_cm_uc_amt]
      ,UPD_USR_ID   = @[upd_usr_id]
      ,UPD_DT       = SYSDATE 
WHERE BSE_TP_CD     = @[bse_tp_cd]
  AND BSE_YR        = @[bse_yr]
  AND BSE_QTR_CD    = DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
  AND OFC_VW_CD     = DECODE(@[ofc_vw_cd],'LOADING','L','CONTRACT','C')
  AND TRD_CD        = @[trd_cd]
  AND DIR_CD        = @[dir_cd]
  AND RLANE_CD      = @[rlane_cd]
  AND RHQ_CD        = @[rhq_cd]
  AND RGN_OFC_CD    = @[rgn_ofc_cd]			]]></sql>
			<params>
				<param name="pa_cm_uc_amt" type="12" value="" out="N"/>
				<param name="ra_cm_uc_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="ofc_vw_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
