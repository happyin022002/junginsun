<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOSearchLstCorrNoRSQL">
			<desc><![CDATA[Last Correction No 조회]]></desc>
			<sql><![CDATA[
SELECT corr_no
  FROM bkg_correction a
 WHERE bkg_no = @[bkg_no]
   AND corr_dt = (SELECT MAX(corr_dt)
                    FROM bkg_correction b
                   WHERE a.bkg_no = b.bkg_no
                     AND b.corr_no not in ('0000000001','TMP0000001'))			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
