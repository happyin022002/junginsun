				<param name="c_cust_addr" type="12" value="" out="N"/>
				<param name="c_cust_cty_nm" type="12" value="" out="N"/>
				<param name="c_eur_cstms_st_nm" type="12" value="" out="N"/>
				<param name="c_eori_no" type="12" value="" out="N"/>
