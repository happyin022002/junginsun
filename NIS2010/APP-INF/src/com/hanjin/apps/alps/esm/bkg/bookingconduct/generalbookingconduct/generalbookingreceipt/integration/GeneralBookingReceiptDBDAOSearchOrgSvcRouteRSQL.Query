<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchOrgSvcRouteRSQL">
			<desc><![CDATA[orgin service mode 조회]]></desc>
			<sql><![CDATA[
SELECT ORG_SVC_MODE
  FROM (SELECT 1 RANK, SVC_MOD_CD ORG_SVC_MODE
          FROM MAS_USA_SVC_MOD
         WHERE ORG_RGN_CD = (SELECT NVL(RGN_CD, ' ') FROM MDM_LOCATION WHERE LOC_CD = @[pol_cd])
           AND DEST_RGN_CD = (SELECT NVL(RGN_CD, ' ') FROM MDM_LOCATION WHERE LOC_CD = @[por_cd])
        UNION ALL
        SELECT 2 RANK, 'ELO' ORG_SVC_MODE
          FROM BKG_EQLZ_PORT
         WHERE SLAN_CD      = @[trunk_lane_cd]
           AND LOC_CD       = @[pol_cd]
           AND EQLZ_PORT_CD = @[por_cd]
           AND SKD_DIR_CD   = @[skd_dir_cd]
        union ALL
        select 3 rank, decode(por.loc_chr_cd||poL.loc_chr_cd
               , 'PRTPRT', 'NLO', decode(@[por_cd], @[pol_cd], 'NLO', 'NIP')) ORG_SVC_MODE
          from mdm_location por, mdm_location pol
         where por.loc_cd = @[por_cd]   
           and por.delt_flg = 'N'
           and pol.loc_cd = @[pol_cd]   
           and pol.delt_flg = 'N'
           AND NVL(@[pre_rly_port_cd], 'X') <> 'X'
        union All			
        select 4 rank, decode(por.loc_chr_cd||poL.loc_chr_cd
               , 'ECPGCP', 'CLO', 'GCPECP', 'CLO'
               , 'ECPECP', 'CLO', 'WCPWCP', 'CLO', 'PRTPRT', 'CLO'
               , decode(@[por_cd], @[pol_cd], 'CLO'
                   , decode(por.loc_chr_cd||poL.loc_chr_cd
                       , 'ECPWCP', 'CML', 'WCPECP', 'CML'
                       , 'GCPWCP', 'CML', 'WCPGCP', 'CML'
                       , 'CIP'))) ORG_SVC_MODE
          from mdm_location por, mdm_location pol
         where por.loc_cd = @[por_cd]   
           and por.delt_flg = 'N'
           and pol.loc_cd = @[pol_cd]   
           and pol.delt_flg = 'N'
           AND NVL(@[pre_rly_port_cd],'X') = 'X')
 where rownum = 1
 order by rank			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="trunk_lane_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pre_rly_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
