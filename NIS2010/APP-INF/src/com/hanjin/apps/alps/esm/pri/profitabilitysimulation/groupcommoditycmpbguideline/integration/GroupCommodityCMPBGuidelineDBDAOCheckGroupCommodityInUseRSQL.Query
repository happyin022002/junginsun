<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GroupCommodityCMPBGuidelineDBDAOCheckGroupCommodityInUseRSQL">
			<desc><![CDATA[CMDT CD가 RATE에서 사용하는지 체크]]></desc>
			<sql><![CDATA[
SELECT PRC_CMDT_DEF_CD AS CD
FROM PRI_CMPB_GLINE_CMDT
WHERE SVC_SCP_CD    = @[svc_scp_cd]
AND CRE_OFC_CD      = @[cre_ofc_cd]
AND GLINE_SEQ       = @[gline_seq]
AND PRS_CUST_TP_CD  = @[cre_usr_id]
AND PRC_CMDT_DEF_CD = @[prc_grp_cmdt_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="prc_grp_cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
