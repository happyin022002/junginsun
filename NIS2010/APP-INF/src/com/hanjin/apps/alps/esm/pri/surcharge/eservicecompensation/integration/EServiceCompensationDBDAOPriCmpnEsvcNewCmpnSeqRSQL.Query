<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EServiceCompensationDBDAOPriCmpnEsvcNewCmpnSeqRSQL">
			<desc><![CDATA[E-SVC Compensation 의 New cmpn_seq 조회]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(PRI_CMPN_ESVC XPKPRI_CMPN_ESVC) */
       CMPN_SEQ +1 AS CMPN_SEQ
FROM PRI_CMPN_ESVC
WHERE SVC_SCP_CD = @[svc_scp_cd]
AND   ROWNUM = 1			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
