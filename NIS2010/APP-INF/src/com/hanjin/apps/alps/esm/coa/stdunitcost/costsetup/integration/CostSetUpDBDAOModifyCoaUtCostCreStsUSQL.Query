<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostSetUpDBDAOModifyCoaUtCostCreStsUSQL">
			<desc><![CDATA[ModifyCoaUtCostCreSts]]></desc>
			<sql><![CDATA[
MERGE INTO COA_UT_COST_CRE_STS A1
USING (
        SELECT REPLACE(@[cost_yrmon],'-','') AS COST_YRMON
               , '00' COST_WK
               , 'MRCD' AS CM_UC_CD
           FROM DUAL       
      ) A2
  ON (    A1.COST_YRMON = A2.COST_YRMON
      AND A1.COST_WK    = A2.COST_WK
      AND A1.CM_UC_CD   = A2.CM_UC_CD
     )
WHEN NOT MATCHED THEN
     INSERT(A1.COST_YRMON, A1.COST_WK, A1.CM_UC_CD, A1.COST_CRE_STS_CD, A1.CRE_USR_ID, A1.CRE_DT, A1.UPD_USR_ID, A1.UPD_DT)
     VALUES(REPLACE(@[cost_yrmon],'-',''), '00', 'MRCD', 'C', @[user_id], SYSDATE, @[user_id], SYSDATE )         
WHEN MATCHED THEN
     UPDATE
        SET A1.UPD_USR_ID      = @[user_id]
           ,A1.UPD_DT          = SYSDATE			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
