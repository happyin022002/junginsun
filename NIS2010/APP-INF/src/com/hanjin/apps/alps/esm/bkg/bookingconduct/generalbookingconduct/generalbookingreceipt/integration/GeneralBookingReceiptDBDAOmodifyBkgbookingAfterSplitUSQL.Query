<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOmodifyBkgbookingAfterSplitUSQL">
			<desc><![CDATA[modifyBkgbookingAfterSplit
2011.06.08 이일민 [CHM-201110982-01] e-SI & DPCS BKG Split & Combine 기능 구현 요청]]></desc>
			<sql><![CDATA[
update bkg_booking
 set split_rsn_cd = @[split_rsn_cd]
 , split_flg = 'Y'
 , split_dt = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,(SELECT POR_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no]))
 , split_rto = split_rto / @[splitCount]
 , vsl_cd = @[vsl_cd] 
 , skd_voy_no = @[skd_voy_no] 
 , skd_dir_cd = @[skd_dir_cd]
 , edi_hld_flg = nvl(@[edi_hld_flg],'N')
 , slan_cd =(SELECT VSL_SLAN_CD FROM VSK_VSL_SKD
	 WHERE vsl_cd = @[vsl_cd]
     AND skd_voy_no = @[skd_voy_no] 
     AND skd_dir_cd = @[skd_dir_cd])
 , upd_usr_id =@[usr_id]
 , upd_dt = sysdate
 , rd_cgo_flg = nvl(@[rd_cgo_flg],'N')
where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="split_rsn_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="splitCount" type="2" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="edi_hld_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="rd_cgo_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
