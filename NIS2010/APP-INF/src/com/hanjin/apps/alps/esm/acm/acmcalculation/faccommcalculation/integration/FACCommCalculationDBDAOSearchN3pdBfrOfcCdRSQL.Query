<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchN3pdBfrOfcCdRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchN3pdBfrOfcCdRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT NVL(A.N3PTY_RCV_OFC_CD, '*') N3RD_BFR_OFC_CD 
  FROM BKG_CHG_RT A, MDM_ORGANIZATION B, MDM_LOCATION C 
 WHERE A.BKG_NO = @[bkg_no]
   AND A.FRT_TERM_CD = 'P' 
   AND A.N3PTY_RCV_OFC_CD = B.OFC_CD 
   AND B.LOC_CD = C.LOC_CD 
   AND C.CONTI_CD IN ('F', 'E') 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
