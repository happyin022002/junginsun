<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrNoticeDBDAOsearchArrNtcEdi312CustInfoRSQL">
			<desc><![CDATA[searchArrNtcEdi312CustInfo]]></desc>
			<sql><![CDATA[
SELECT '{CUST_INFO'                        					   || CHR (10) || 
       'BCS_TP:'         ||  BCST.BKG_CUST_TP_CD              || CHR(10) ||
       'CUST_CD:'        || BCST.CUST_CNT_CD||BCST.CUST_SEQ			    || CHR(10) ||
       'CUST_NM1:'       || NVL(REPLACE(BCST.CUST_NM, CHR(13)||CHR(10), ' '),' ')   || CHR(10) ||
       'CUST_NM2:'       || ''			 				    || CHR(10) ||
       'CUST_ADDR1:'     || NVL(REPLACE(BCST.CUST_ADDR, CHR(13)||CHR(10), ' '),' ') || CHR(10) ||
       'CUST_ADDR2:'     || ''			 				    || CHR(10) ||
       'CUST_ADDR3:'     || ''			 				    || CHR(10) ||
       'CUST_C_NM1:'     || ''			 				    || CHR(10) ||
       'CUST_C_NM2:'     || ''			 				    || CHR(10) ||
       'CUST_CUST_LOC:'  || ''			 				    || CHR(10) ||
       'CUST_STREET:'    || ''			 				    || CHR(10) ||
       'CUST_LOC_CD:'    || ''			 				    || CHR(10) ||
       'CUST_LOC_NM:'    || ''			 				    || CHR(10) ||
       'CUST_ZIP_CD:'    || BCST.CUST_ZIP_ID	    				    || CHR(10) ||
       '}CUST_INFO'      || CHR(10)
 FROM BKG_CUSTOMER BCST
 WHERE BKG_NO=@[bkg_no]
 AND   BCST.CUST_NM IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
