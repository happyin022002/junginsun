<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOremoveCstmsCdConvDescDSQL">
			<desc><![CDATA[removeCstmsCdConvDesc]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_CD_CONV_DESC
SET DELT_FLG = 'Y',
	UPD_USR_ID = @[user_id],
	UPD_DT = SYSDATE
WHERE CNT_CD = @[cnt_cd]
AND CSTMS_DIV_ID = @[cstms_div_id]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cstms_div_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
