<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOSearchExceptionListMgmtRSQL">
			<desc><![CDATA[SearchExceptionListMgmt]]></desc>
			<sql><![CDATA[
SELECT COST_YRMON,
       SC_NO,
       SCC_CD,
       SC_NO||SCC_CD AS SCNO_SCCCD,
	   CTRT_SEQ,
	   CMDT_CD,
	   ACT_SHPR_CNT_CD||DECODE(NVL(ACT_SHPR_CNT_CD,'0'),'0','',LPAD(ACT_SHPR_SEQ,6,'0')) as ACT_SHPR_CD,
	   COST_EXPT_FLG
FROM   MAS_CHSS_EXPT_LIST
WHERE  COST_YRMON  BETWEEN SUBSTR(@[f_yearmonth],1,6)
                   AND     SUBSTR(@[t_yearmonth],1,6)
#if (${f_scno} != '')
	AND    SC_NO = NVL(@[f_scno],SC_NO)
#end
ORDER BY COST_YRMON ASC			]]></sql>
			<params>
				<param name="f_yearmonth" type="12" value="" out="N"/>
				<param name="t_yearmonth" type="12" value="" out="N"/>
				<param name="f_scno" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
