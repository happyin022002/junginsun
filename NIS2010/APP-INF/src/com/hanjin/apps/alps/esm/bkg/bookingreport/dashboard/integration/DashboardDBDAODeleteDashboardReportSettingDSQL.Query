<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAODeleteDashboardReportSettingDSQL">
			<desc><![CDATA[Log in user id  +  bkg ofc 의 DBD_IRR_TP_CD 데이터를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE BKG_DBD_SET 
WHERE BKG_OFC_CD=@[bkg_ofc_cd]
AND CRE_USR_ID=@[usr_id]
AND DBD_IRR_TP_CD=@[dbd_irr_tp_cd]
AND BKG_DBD_SET_TP_CD ='U'			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="dbd_irr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
