<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchEffDtDivRSQL">
			<desc><![CDATA[BLDocumentationBLDBDAOSearchEffDtDivRSQL]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y')
    #if (${div} == 'BKG')
    SELECT 'Y' AS DIV_FLG 
      FROM BKG_BKG_HIS BKG
          ,BKG_HRD_CDG_CTNT CTNT
     WHERE BKG.BKG_NO = @[bkg_no] 
       AND BKG.CORR_NO = 'TMP0000001'
       AND BKG.DEL_CD LIKE CTNT.ATTR_CTNT1||'%'
       AND CTNT.HRD_CDG_ID = 'AUTO_CLAUSE_DATE'
       AND CTNT.ATTR_CTNT2 = 'BKG'
       AND BKG.BKG_CRE_DT >= TO_DATE(CTNT.ATTR_CTNT3,'YYYYMMDDHH24MISS') 
    #elseif (${div} == 'CRD')
    SELECT 'Y' AS DIV_FLG 
      FROM BKG_CNTR_HIS CON
          ,BKG_HRD_CDG_CTNT CTNT
     WHERE CON.BKG_NO = @[bkg_no]
       AND CON.CORR_NO = 'TMP0000001'
       AND CTNT.HRD_CDG_ID = 'AUTO_CLAUSE_DATE'
       AND CTNT.ATTR_CTNT2 = 'CRD'
       AND CON.CGO_RCV_DT >= TO_DATE(CTNT.ATTR_CTNT3,'YYYYMMDDHH24MISS') 
       AND ROWNUM = 1
    #end
#else
    #if (${div} == 'BKG')
    SELECT 'Y' AS DIV_FLG  
      FROM BKG_BOOKING BKG
          ,BKG_HRD_CDG_CTNT CTNT
     WHERE BKG.BKG_NO = @[bkg_no] 
       AND BKG.DEL_CD LIKE CTNT.ATTR_CTNT1||'%'
       AND CTNT.HRD_CDG_ID = 'AUTO_CLAUSE_DATE'
       AND CTNT.ATTR_CTNT2 = 'BKG'
       AND BKG.BKG_CRE_DT >= TO_DATE(CTNT.ATTR_CTNT3,'YYYYMMDDHH24MISS') 
    #elseif (${div} == 'CRD')
    SELECT 'Y' AS DIV_FLG 
      FROM BKG_CONTAINER CON
          ,BKG_HRD_CDG_CTNT CTNT
     WHERE CON.BKG_NO = @[bkg_no]
       AND CTNT.HRD_CDG_ID = 'AUTO_CLAUSE_DATE'
       AND CTNT.ATTR_CTNT2 = 'CRD'
       AND CON.CGO_RCV_DT >= TO_DATE(CTNT.ATTR_CTNT3,'YYYYMMDDHH24MISS') 
       AND ROWNUM = 1
    #end
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
