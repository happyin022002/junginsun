<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOmodifyMemoSplitUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
update bkg_booking
   set SPLIT_RSN_CD = 'M' 
        , SPLIT_FLG = 'Y'
		, BKG_STS_CD= 'S'
        , SPLIT_DT  = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,BKG_COM_USER_LOC_FNC(@[usr_id]))
		, SPLIT_GDT = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,'GMT')
        , UPD_USR_ID= @[usr_id]
        , UPD_DT    = sysdate
 where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
