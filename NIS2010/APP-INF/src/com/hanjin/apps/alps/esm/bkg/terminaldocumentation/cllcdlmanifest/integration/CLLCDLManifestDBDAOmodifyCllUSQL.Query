<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOmodifyCllUSQL">
			<desc><![CDATA[modifyCll]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_TML_CLL SET 
	VSL_CD = SUBSTR(@[in_vvd_cd],1,4)
,	SKD_VOY_NO = SUBSTR(@[in_vvd_cd],5,4)
,	SKD_DIR_CD = SUBSTR(@[in_vvd_cd],9,1)
,	PORT_CD = @[in_pol_cd]
#if (${eta_dt} != '') 
,	ETA_DT = TO_DATE(@[eta_dt],'YYYY-MM-DD HH24:MI')
#end
#if (${etd_dt} != '') 
,	ETD_DT = TO_DATE(@[etd_dt],'YYYY-MM-DD HH24:MI')
#end
#if (${bkg_no} != '') 
,	BKG_NO = @[bkg_no]
#end
,	CNTR_NO = @[cntr_no]
,	TS_CGO_CD = @[ts_cgo_cd]
,	CNTR_TPSZ_CD = @[cntr_tpsz_cd]
,	POR_CD = @[por_cd]
,	POL_CD = @[pol_cd]
,	POD_CD = @[pod_cd]
,	DEL_CD = @[del_cd]
,	SOC_FLG = @[soc_flg]
,	FULL_MTY_CD = @[full_mty_cd]
,	RCV_TERM_CD = SUBSTR(@[rcv_de_term_cd],1,1)
,	DE_TERM_CD = SUBSTR(@[rcv_de_term_cd],2,1)
,	CNTR_SEAL_NO = @[cntr_seal_no]
,	CNTR_LODG_NO = @[cntr_lodg_no]
,	CFM_FLG = @[cfm_flg]
#if (${teu_cntr_qty} != '') 
,	TEU_CNTR_QTY = @[teu_cntr_qty]
#end
#if (${feu_cntr_qty} != '') 
,	FEU_CNTR_QTY = @[feu_cntr_qty]
#end
,	RC_FLG 		= NVL(DECODE(@[rc_flg],'1','Y','0','N',@[rc_flg]),'N')
,	DCGO_FLG 	= NVL(DECODE(@[dcgo_flg],'1','Y','0','N',@[dcgo_flg]),'N')
,	AWK_CGO_FLG = NVL(DECODE(@[awk_cgo_flg],'1','Y','0','N',@[awk_cgo_flg]),'N')
,	BB_CGO_FLG 	= NVL(DECODE(@[bb_cgo_flg],'1','Y','0','N',@[bb_cgo_flg]),'N')
,	RD_CGO_FLG 	= NVL(DECODE(@[rd_cgo_flg],'1','Y','0','N',@[rd_cgo_flg]),'N')
,	BL_RMK 		= @[bl_rmk] 
,	UPD_USR_ID 	= @[upd_usr_id]
,	UPD_DT 		= SYSDATE
,	CNTR_WGT 	= @[cntr_wgt]
,	WGT_TP_CD 	= @[wgt_tp_cd]
,	GRS_CNTR_WGT = @[grs_cntr_wgt]
,	GRS_WGT_UT_CD = @[grs_wgt_ut_cd]
#if (${in_pol_split_no} != '')
,   CLPT_IND_SEQ = @[in_pol_split_no]
#end 
,	VGM_WGT =@[vgm_wgt]
,	VGM_VRFY_SIG_CTNT =@[vgm_vrfy_sig_ctnt]
,	VGM_MZD_TP_CD =@[vgm_mzd_tp_cd]
WHERE	UPD_USR_ID = @[upd_usr_id]
AND	VSL_CD = SUBSTR(@[in_vvd_cd],1,4)
AND	SKD_VOY_NO = SUBSTR(@[in_vvd_cd],5,4)
AND	SKD_DIR_CD = SUBSTR(@[in_vvd_cd],9,1)
AND	PORT_CD = @[in_pol_cd]
#if (${in_pol_split_no} != '')
AND NVL(CLPT_IND_SEQ, '1') = @[in_pol_split_no]
#end 
AND	CNTR_LODG_NO = @[cntr_lodg_no]			]]></sql>
			<params>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="eta_dt" type="12" value="" out="N"/>
				<param name="etd_dt" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="ts_cgo_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="soc_flg" type="12" value="" out="N"/>
				<param name="full_mty_cd" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="cntr_seal_no" type="12" value="" out="N"/>
				<param name="cntr_lodg_no" type="12" value="" out="N"/>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="teu_cntr_qty" type="6" value="" out="N"/>
				<param name="feu_cntr_qty" type="6" value="" out="N"/>
				<param name="rc_flg" type="12" value="" out="N"/>
				<param name="dcgo_flg" type="12" value="" out="N"/>
				<param name="awk_cgo_flg" type="12" value="" out="N"/>
				<param name="bb_cgo_flg" type="12" value="" out="N"/>
				<param name="rd_cgo_flg" type="12" value="" out="N"/>
				<param name="bl_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="wgt_tp_cd" type="12" value="" out="N"/>
				<param name="grs_cntr_wgt" type="12" value="" out="N"/>
				<param name="grs_wgt_ut_cd" type="12" value="" out="N"/>
				<param name="in_pol_split_no" type="12" value="" out="N"/>
				<param name="vgm_wgt" type="12" value="" out="N"/>
				<param name="vgm_vrfy_sig_ctnt" type="12" value="" out="N"/>
				<param name="vgm_mzd_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
