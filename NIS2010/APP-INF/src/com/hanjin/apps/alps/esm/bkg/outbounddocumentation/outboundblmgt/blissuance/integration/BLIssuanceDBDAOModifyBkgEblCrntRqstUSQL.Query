<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOModifyBkgEblCrntRqstUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE BKG_EBL_CRNT_RQST
   SET  SR_STS_CD = @[sr_sts_cd]
#if (${sr_sts_cd} == 'U') 
	,EBL_RJCT_RSN = @[remark]
#end
	,SND_USR_ID = @[usr_id]
	,SND_DT	    = SYSDATE
	,UPD_USR_ID = @[usr_id]
	,UPD_DT	    = SYSDATE
 WHERE 	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="sr_sts_cd" type="12" value="" out="N"/>
				<param name="remark" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
