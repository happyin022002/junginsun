<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchEffDtDivRSQL">
			<desc><![CDATA[날짜, 로직별로 적용되는 로직인지를 확인한다.]]></desc>
			<sql><![CDATA[
#if (${div} == 'BKG')
    SELECT 'Y' AS eff_flg
      FROM BKG_BOOKING BKG
          ,BKG_HRD_CDG_CTNT CTNT
     WHERE BKG.BKG_NO = @[bkg_no] 
       AND CTNT.HRD_CDG_ID = 'BKG_EFF_DT' 
       AND CTNT.ATTR_CTNT1 = @[lgc_nm]
       AND CTNT.ATTR_CTNT2 = @[div]
       AND BKG.BKG_CRE_DT >= TO_DATE(CTNT.ATTR_CTNT3,'YYYYMMDDHH24MISS') 
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="lgc_nm" type="12" value="" out="N"/>
				<param name="div" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
