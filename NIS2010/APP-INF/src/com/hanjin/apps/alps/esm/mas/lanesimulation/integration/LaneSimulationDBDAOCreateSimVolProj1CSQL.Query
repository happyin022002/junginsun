<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOCreateSimVolProj1CSQL">
			<desc><![CDATA[route projection volumn insert.

2010.07.29 이윤정 : CHM-201004777-01 [MAS] MAS 코드매핑 불일치건 조치 요청 : SRC_PRD_CD 삭제]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_SIM_VOL_PRJ(SIM_DT, SIM_NO, SECT_NO, POL_CD, POD_CD, PORT_PAIR_QTY, PORT_PAIR_LOD_QTY, PORT_PAIR_RTO, GRS_RPB_REV, TRD_CD, SUB_TRD_CD, RLANE_CD, SKD_DIR_CD, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT) 
	         SELECT C1.SIM_DT 
	               ,C1.SIM_NO 
	               ,C1.SECT_NO 
	               ,C1.POL_CD 
	               ,C1.POD_CD 
	               ,C1.LOAD_QTY PORT_PAIR_QTY 
	               ,(C1.LOAD_QTY/SUM(C1.LOAD_QTY) OVER (PARTITION BY C1.SECT_NO)) * C1.LOD_TTL_QTY PORT_PAIR_LOD_QTY 
	               ,C1.LOAD_QTY/SUM(C1.LOAD_QTY) OVER (PARTITION BY C1.SECT_NO) PORT_PAIR_RTO 
	               ,C1.REV/C1.LOAD_QTY  GRS_RPB_REV 
	               ,C1.TRD_CD 
	               ,'' SUB_TRD_CD 
	               ,C1.RLANE_CD 
	               ,C1.SKD_DIR_CD 
	               ,@[cre_usr_id] 
	               ,SYSDATE
	               ,@[upd_usr_id] 
	               ,SYSDATE
	           FROM ( 
	                  SELECT B2.SIM_DT 
	                        ,B2.SIM_NO 
	                        ,B2.TRD_CD 
	                        ,B2.RLANE_CD 
	                        ,B2.SKD_DIR_CD 
	                        ,B2.IOC_CD 
	                        ,B2.NUM 
	                        ,B2.SECT_NO 
	                        ,B2.POL_CD 
	                        ,B2.POD_CD 
	                        ,B2.LOD_TTL_QTY 
	                        ,SUM(DECODE(B1.SGRP_COST_CD, 'LOAD', B1.SIM_PERF_AMT)) LOAD_QTY 
	                        ,SUM(DECODE(B1.SGRP_COST_CD, 'FRRE', B1.SIM_PERF_AMT)) REV 
	                    FROM MAS_SIM_SMRY B1 
	                        ,( 
	                          SELECT A1.SIM_DT 
	                                ,A1.SIM_NO 
	                                ,A1.TRD_CD 
	                                ,A1.RLANE_CD 
	                                ,A1.SKD_DIR_CD 
	                                ,A1.IOC_CD 
	                                ,A2.POL_CD 
	                                ,A3.POD_CD 
	                                ,A2.NUM 
	                                ,A2.SECT_NO 
	                                ,A1.LOD_TTL_QTY 
	                            FROM MAS_SIM_SVC_LANE A1 
	                                 ,( 
				#if (${rowcnt} > 0) 
					#set($i = 1)	        	
		        	#foreach(${key} in ${parentArr})
	        		                          SELECT '${key.dTmlCd}' AS POL_CD, '${key.dNum}' AS NUM, '${key.dSectNo}' AS SECT_NO FROM DUAL 
		        		#if(${rowcnt} > $i) 
							UNION ALL
							#set ($i = $i+1)	        		
						#end
                	#end
			    #end
	    	    #if(${rowcnt} == 0 )
	        	                          SELECT ' ' AS POL_CD,  0 AS NUM, '' AS SECT_NO FROM DUAL 
		        #end
	                                  ) a2 
	                                 ,( 
				#if (${rowcnt} > 0) 
					#set($i = 1)	        	
		        	#foreach(${key} in ${parentArr})
	        		                          SELECT '${key.dTmlCd}' AS POD_CD, '${key.dNum}' AS NUM, '${key.dSectNo}' AS SECT_NO FROM DUAL 
		        		#if(${rowcnt} > $i) 
							UNION ALL
							#set ($i = $i+1)	        		
						#end
                	#end
			    #end
	    	    #if(${rowcnt} == 0 )
	        	                          SELECT ' ' AS POD_CD,  0 AS NUM, '' AS SECT_NO FROM DUAL 
		        #end
	                                  ) A3 
	                           WHERE A2.SECT_NO = A3.SECT_NO 
	                             AND A1.SECT_NO = A2.SECT_NO 
	                             AND A1.SIM_DT  = @[sim_dt] 
	                             AND A1.SIM_NO  = @[sim_no] 
	                        ) B2 
	                   WHERE 1=1 
	                     AND B1.POL_CD       = B2.POL_CD 
	                     AND B1.POD_CD       = B2.POD_CD 
	                     AND B1.SGRP_COST_CD IN ('LOAD','FRRE') 
	                     AND B1.COST_YRMON   >= TO_CHAR(ADD_MONTHS(SYSDATE,-${prd_cd}),'YYYYMM') 
	                     AND B1.TRD_CD       = B2.TRD_CD 
	                     AND B1.DIR_CD       = B2.SKD_DIR_CD 
					#if (${trd_cd} != '') 
						 AND B1.TRD_CD       = @[trd_cd] 
					#end
			        #if (${rlane_cd} != '')              
						 AND B1.RLANE_CD     = @[rlane_cd] 
					#end
			        #if(${dir_cd} != '')                
						 AND B1.DIR_CD       = @[dir_cd] 
                    #end
	                   GROUP BY B2.SIM_DT 
	                           ,B2.SIM_NO 
	                           ,B2.TRD_CD 
	                           ,B2.RLANE_CD 
	                           ,B2.SKD_DIR_CD 
	                           ,B2.IOC_CD 
	                           ,B2.NUM 
	                           ,B2.SECT_NO 
	                           ,B2.POL_CD 
	                           ,B2.POD_CD 
	                           ,B2.LOD_TTL_QTY 
	                ) C1 
	          ORDER BY C1.SECT_NO,C1.NUM
			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
