<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchBkgBookingForCopyRSQL">
			<desc><![CDATA[ui에서 check한 Stowage,Stop Off Cargo,Rail Bulk,Hide,Food Grade,Precaution,Remark에 따라 조회하는 항목을 공백으로 바꾼다]]></desc>
			<sql><![CDATA[
SELECT
	 '' BKG_NO
	,'' BL_NO
	,'0' BL_NO_TP
	,'' BL_TP_CD
	,DECODE(NVL(@[dcgo_flg]||@[rc_flg]||@[awk_cgo_flg]||@[bb_cgo_flg], 'N'), 'N', 'F', 'W') BKG_STS_CD
	,BKG_CGO_TP_CD
	,(
		SELECT 	VSL_SLAN_CD 
		FROM 	VSK_VSL_SKD
	 	WHERE 	VSL_CD = SUBSTR(@[bkg_trunk_vvd], 1, 4) 
		AND 	SKD_VOY_NO = SUBSTR(@[bkg_trunk_vvd], 5, 4) 
        AND 	SKD_DIR_CD = SUBSTR(@[bkg_trunk_vvd], 9, 1) 
		AND 	ROWNUM = 1
	) SLAN_CD
	,SVC_SCP_CD
	,SUBSTR(@[bkg_trunk_vvd], 1, 4) VSL_CD
	,SUBSTR(@[bkg_trunk_vvd], 5, 4) SKD_VOY_NO
	,SUBSTR(@[bkg_trunk_vvd], 9, 1) SKD_DIR_CD
	,REV_DIR_CD
	,RCV_TERM_CD
	,DE_TERM_CD
	,POR_CD
	,POR_NOD_CD
	,POL_CD
	,POL_NOD_CD
	,POD_CD
	,POD_NOD_CD
	,OCP_CD
	,DEL_CD
	,DEL_NOD_CD
	,FNL_DEST_CD
	,'' POL_ETD_DT
	,'' POD_ETA_DT
	,MTY_PKUP_YD_CD
	,TO_CHAR(MTY_PKUP_DT,'YYYY-MM-DD HH24:MI:SS') MTY_PKUP_DT
	,FULL_RTN_YD_CD
	,FULL_PKUP_YD_CD
	,MTY_RTN_YD_CD
	,TO_CHAR(MTY_DOR_ARR_DT,'YYYY-MM-DD HH24:MI:SS') MTY_DOR_ARR_DT
	,TO_CHAR(LODG_DUE_DT,'YYYY-MM-DD HH24:MI:SS') LODG_DUE_DT
	,TO_CHAR(DE_DUE_DT,'YYYY-MM-DD HH24:MI:SS') DE_DUE_DT
	,TO_CHAR(IB_DEL_DE_DT,'YYYY-MM-DD HH24:MI:SS') IB_DEL_DE_DT
	,ORG_TRNS_SVC_MOD_CD
	,ORG_TRNS_MOD_CD
	,ORG_SCONTI_CD
	,DEST_TRNS_SVC_MOD_CD
	,DEST_TRNS_MOD_CD
	,DEST_SCONTI_CD
	,DECODE(@[stop_off_flg],'Y',STOP_OFF_LOC_CD,'') STOP_OFF_LOC_CD
	,DECODE(@[stop_off_flg],'Y',STOP_OFF_CNTC_PSON_NM,'') STOP_OFF_CNTC_PSON_NM
	,DECODE(@[stop_off_flg],'Y',STOP_OFF_CNTC_PHN_NO,'') STOP_OFF_CNTC_PHN_NO
	,DECODE(@[stop_off_flg],'Y',STOP_OFF_DIFF_RMK,'') STOP_OFF_DIFF_RMK
	,SLS_RHQ_CD
	,SLS_RGN_OFC_CD
	,@[ofc_cd] BKG_OFC_CD
	,@[usr_id] DOC_USR_ID
	,CTRT_OFC_CD
	,CTRT_SREP_CD
	,CTRT_CUST_CNT_CD
	,CTRT_CUST_SEQ
	,OB_SLS_OFC_CD
	,OB_SREP_CD
	,IB_SLS_OFC_CD
	,EQ_CTRL_OFC_CD
	,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', SYSDATE, GLOBALDATE_PKG.GET_LOCCD_FNC((SELECT OFC_CD FROM COM_USER USR WHERE USR.USR_ID = @[usr_id]))),'YYYY-MM-DD HH24:MI:SS') BKG_CRE_DT
	,'' PORT_CLZ_DT
	,CMDT_CD
	,REP_CMDT_CD
	,DECODE(@[dcgo_flg],'Y',DCGO_FLG,'N') DCGO_FLG
	,DECODE(@[rc_flg],'Y',RC_FLG,'N') RC_FLG 
	,DECODE(@[awk_cgo_flg],'Y',AWK_CGO_FLG,'N') AWK_CGO_FLG
	,DECODE(@[bb_cgo_flg],'Y',BB_CGO_FLG,'N') BB_CGO_FLG
	,RD_CGO_FLG
	,DECODE(@[hngr_flg],'Y',HNGR_FLG,'N') HNGR_FLG
	,DECODE(@[bulk_rail_flg],'Y',RAIL_BLK_CD,'') RAIL_BLK_CD
	,DECODE(@[prct_flg],'Y',PRCT_FLG,'N') PRCT_FLG
	,DECODE(@[spcl_hide_flg],'Y',SPCL_HIDE_FLG,'N') SPCL_HIDE_FLG
	,SOC_FLG
	,EQ_SUBST_FLG
	,DECODE(@[fd_grd_flg],'Y',FD_GRD_FLG,'N') FD_GRD_FLG
	,FLEX_HGT_FLG
	,DECODE(@[stowage_flg],'Y',STWG_CD,'') STWG_CD
	,DECODE(@[stowage_flg],'Y',STWG_RMK,'') STWG_RMK
	,BLCK_STWG_CD
	,ALL_MTR_FLG
	,DBL_BKG_FLG
	,AP_BROG_FLG
	,CUST_TO_ORD_FLG
	,KR_CSTMS_CUST_TP_CD
	,SAM_CNEE_NTFY_FLG
	,ALT_CUST_CFM_FLG
	,decode(instr(nvl(@[dcgo_flg]||@[rc_flg]||@[awk_cgo_flg]||@[bb_cgo_flg], 'N'), 'Y'), 0, 'N', 'Y') WT_RSN_SPCL_CGO_FLG
	,'N' WT_RSN_HLD_FLG
	,SHP_BAK_FLG
	,MNL_BKG_NO_FLG
	,BL_ISS_BLCK_FLG
	,'' CA_INSP_DUE_DT
	,SCAC_CD
	,CHN_AGN_CD
	,USA_CSTMS_FILE_CD
	,CND_CSTMS_FILE_CD
	,TWN_SO_NO
	,'' INTER_RMK
	,@[xter_rmk] XTER_RMK
	,'' INTER_RMK_AUD_FLG
	,'N' SPLIT_FLG
	,'N' HCMT_CMB_FLG
	,'C' BKG_CRE_TP_CD
	,'' TO_BKG_NO
	,@[bkg_no] FM_BKG_NO
	,'' ADV_SHTG_CD
	,'' SPLIT_RSN_CD
	,1 SPLIT_RTO
	,@[sc_no] SC_NO
	,@[rfa_no] RFA_NO
	,TAA_NO
	,AGMT_ACT_CNT_CD
	,AGMT_ACT_CUST_SEQ
	,MTY_CRE_SVR_CD
	,MTY_BKG_STS_CD
	,MTY_POD_FLG
	,MTY_PRE_VVD_FLG
	,MTY_PORT_FLG
	,MTY_SPLIT_AVAL_CD
	,'NIS' XTER_BKG_RQST_CD
	,'' XTER_BKG_RQST_REF_NO
	,'N' SI_FLG
	,'' XTER_SI_CD
	,'' XTER_SI_REF_NO
	,XTER_RQST_AUTO_NTC_FLG
	,EDI_HLD_FLG
	,'' XTER_BKG_KNT
	,'' XTER_SI_KNT
	,'' XTER_RQST_CUST_RMK
	,DECODE(@[bb_cgo_flg],'Y', OVR_VOID_SLT_QTY, 0) OVR_VOID_SLT_QTY
	,'' MY_FWRD_CD
	,'' MY_FWRD_VSL_DESC
	,PRE_RLY_PORT_CD
	,PST_RLY_PORT_CD
	,@[usr_id] CRE_USR_ID
	,SYSDATE CRE_DT
	,@[usr_id] UPD_USR_ID
	,SYSDATE UPD_DT	
	,DECODE(@[spcl_hide_lnr_flg],'Y',SPCL_HIDE_LNR_FLG,'N') SPCL_HIDE_LNR_FLG
	,DECODE(@[fumg_flg],'Y',FUMG_LOC_CD,'') FUMG_LOC_CD
	,DECODE(@[fumg_flg],'Y',FUMG_CNTC_PSON_NM,'') FUMG_CNTC_PSON_NM
	,DECODE(@[fumg_flg],'Y',FUMG_CNTC_PHN_NO,'') FUMG_CNTC_PHN_NO
	,DECODE(@[fumg_flg],'Y',FUMG_DIFF_RMK,'') FUMG_DIFF_RMK
    ,CRR_SOC_FLG
    ,DECODE(@[veh_cmdt_flg],'Y',VEH_CMDT_FLG,'N') VEH_CMDT_FLG
    ,IDA_HLG_TP_CD
    ,NON_DG_CHEM_FLG
FROM
	BKG_BOOKING
WHERE
	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="dcgo_flg" type="12" value="" out="N"/>
				<param name="rc_flg" type="12" value="" out="N"/>
				<param name="awk_cgo_flg" type="12" value="" out="N"/>
				<param name="bb_cgo_flg" type="12" value="" out="N"/>
				<param name="bkg_trunk_vvd" type="12" value="" out="N"/>
				<param name="stop_off_flg" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="hngr_flg" type="12" value="" out="N"/>
				<param name="bulk_rail_flg" type="12" value="" out="N"/>
				<param name="prct_flg" type="12" value="" out="N"/>
				<param name="spcl_hide_flg" type="12" value="" out="N"/>
				<param name="fd_grd_flg" type="12" value="" out="N"/>
				<param name="stowage_flg" type="12" value="" out="N"/>
				<param name="xter_rmk" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="spcl_hide_lnr_flg" type="12" value="" out="N"/>
				<param name="fumg_flg" type="12" value="" out="N"/>
				<param name="veh_cmdt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
