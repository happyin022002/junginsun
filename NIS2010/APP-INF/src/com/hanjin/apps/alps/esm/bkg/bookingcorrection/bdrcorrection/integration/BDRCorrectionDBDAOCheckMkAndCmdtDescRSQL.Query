<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOCheckMkAndCmdtDescRSQL">
			<desc><![CDATA[M&D Mark / Descripton 변경시 면제가능여부를 확인한다.]]></desc>
			<sql><![CDATA[
SELECT MKDESC.MK_DESC MK
      , MKDESC_HIS.MK_DESC MK_HIS
      , MKDESC.CMDT_DESC CMDT
      , MKDESC_HIS.CMDT_DESC CMDT_HIS
  FROM BKG_BL_MK_DESC     MKDESC
      ,BKG_BL_MK_DESC_HIS MKDESC_HIS
 WHERE MKDESC.BKG_NO  = @[bkg_no]
   AND MKDESC_HIS.CORR_NO = 'TMP0000001' 
   AND MKDESC.BKG_NO  = MKDESC_HIS.BKG_NO
   AND MKDESC.MK_SEQ  = MKDESC_HIS.MK_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
