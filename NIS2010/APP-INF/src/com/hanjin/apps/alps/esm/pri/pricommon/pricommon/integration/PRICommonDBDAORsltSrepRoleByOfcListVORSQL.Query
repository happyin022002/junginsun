<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltSrepRoleByOfcListVORSQL">
			<desc><![CDATA[PRICommonDBDAORsltSrepRoleByOfcListVORSQL]]></desc>
			<sql><![CDATA[
SELECT A.USR_ID CD
     , A.USR_NM NM
     , A.OFC_CD ETC2
  FROM COM_USER A
     , COM_USR_ROLE_MTCH B
 WHERE A.USR_ID = B.USR_ID 
   AND A.USE_FLG = 'Y'
   AND B.USR_ROLE_CD IN ('PRI02' , 'PRI17')
   AND OFC_CD = @[etc1] -- SELCMA
 GROUP BY A.USR_ID, A.USR_NM, A.OFC_CD
 ORDER BY A.USR_NM			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
