<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOCreateCmDtlCACSQL">
			<desc><![CDATA[BLDocumentationBLDBDAOCreateCmCACSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_CNTR_MF_DESC_DTL (
    BKG_NO 
#else
INSERT INTO BKG_CNTR_MF_DESC_DTL_HIS (
    BKG_NO 
    , CORR_NO 
#end
	, CNTR_NO
	, CNTR_MF_SEQ
	, MF_DTL_SEQ
	, NCM_NO
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
		, CNTR_NO
		, CNTR_MF_SEQ
		, MF_DTL_SEQ
		, NCM_NO
		, CRE_USR_ID
		, CRE_DT
		, UPD_USR_ID
		, UPD_DT
#if (${copy_type_cd} == 'BKG')
  FROM BKG_CNTR_MF_DESC_DTL_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_CNTR_MF_DESC_DTL
 WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
