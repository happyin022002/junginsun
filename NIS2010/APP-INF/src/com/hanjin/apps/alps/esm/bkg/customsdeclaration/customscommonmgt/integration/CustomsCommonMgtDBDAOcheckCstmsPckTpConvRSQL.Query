<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOcheckCstmsPckTpConvRSQL">
			<desc><![CDATA[checkCstmsPckTpConv]]></desc>
			<sql><![CDATA[
SELECT COUNT(0)
FROM BKG_CSTMS_PCK_TP_CONV
WHERE CNT_CD = @[cnt_cd]
AND PCK_TP_CD = @[pck_tp_cd]
AND CSTMS_PCK_TP_CD = @[cstms_pck_tp_cd]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="cstms_pck_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
