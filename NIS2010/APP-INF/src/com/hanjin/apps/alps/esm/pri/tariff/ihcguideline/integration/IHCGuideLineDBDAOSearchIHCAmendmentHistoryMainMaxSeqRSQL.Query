<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOSearchIHCAmendmentHistoryMainMaxSeqRSQL">
			<desc><![CDATA[Retrieve IHC Tariff Amendment History Maxseq]]></desc>
			<sql><![CDATA[
SELECT IHC_TRF_NO
     , NVL( (SELECT MAX(AMDT_SEQ)
               FROM PRI_TRF_IHC_MN
              WHERE 1=1
                AND SVC_SCP_CD   = @[svc_scp_cd]
                AND ORG_DEST_TP_CD = @[org_dest_tp_cd] 
                AND IHC_TRF_NO   = @[ihc_trf_no] 
                AND FIC_PROP_STS_CD ='C'
            ), '0' ) AMDT_SEQ           
FROM PRI_TRF_IHC_MN 
WHERE 1=1
AND SVC_SCP_CD   = @[svc_scp_cd]
AND ORG_DEST_TP_CD = @[org_dest_tp_cd] 
AND IHC_TRF_NO   = @[ihc_trf_no] 			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
