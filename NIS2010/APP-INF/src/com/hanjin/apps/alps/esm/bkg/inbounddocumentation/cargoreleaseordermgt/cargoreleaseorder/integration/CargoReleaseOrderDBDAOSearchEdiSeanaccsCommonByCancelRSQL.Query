<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchEdiSeanaccsCommonByCancelRSQL">
			<desc><![CDATA[...]]></desc>
			<sql><![CDATA[
SELECT '   '||RPAD('DOR  ',5,' ')||
       RPAD(' ',21,' ')||
       @[usr_id]||
       @[usr_pwd]||
       RPAD(' ',174,' ')||
       RPAD(' ',26,' ')||
       RPAD(' ',8,' ')||
       RPAD(UPPER(@[cre_usr_id]),10,' ')||
       RPAD(' ',100,' ')||
       RPAD(' ',1,' ')||
       '2'||
       RPAD(' ',27,' ')|| 
       LPAD((SUB_1.LEN_1 + length(SUB_1.LEN_2) + 400),6,'0')||CHR(13)||CHR(10)  /* 400 은 입력공통항목 자릿수 */
FROM (
	   SELECT (SELECT LENGTH(RPAD(DECODE(NVL(C.BL_NO,' '),' ',' ','SMLM'||C.BL_NO),35,' ')
                                    ||CHR(13)
                                    ||CHR(10)
                                    ||RPAD(NVL(NVL(A.JP_DO_ID,A.DO_NO),' '),10,' ')
                                    ||CHR(13)
                                    ||CHR(10)
                                    ||RPAD(NVL(NVL(B.CY_OP_CD,D.CY_OPR_ID),' '),5,' ')
                                    ||CHR(13)
                                    ||CHR(10)
                                    ||DECODE( NVL(B.INFO_CGO_FLG,' '), 'N',' ','Y')
                                    ||CHR(13)
                                    ||CHR(10) ) LEN_1
                  FROM   BKG_DO          A
                       , BKG_DO_REF      B
                       , BKG_BOOKING     C
                       , BKG_CSTMS_JP_BL D
                  WHERE A.BKG_NO           = @[bkg_no]
                  AND   A.RLSE_SEQ         = '1'
                  AND   B.BKG_NO(+)        = A.BKG_NO
                  AND   C.BKG_NO           = A.BKG_NO
                  AND   D.BL_NO(+)         = C.BL_NO )    AS LEN_1
               ,(SELECT NVL(@[evnt_cd],' ')||CHR(13)||CHR(10)||    --  '1' 등록
                        LPAD(' ',5,' ')||CHR(13)||CHR(10) AS LEN2
                 FROM DUAL ) AS LEN_2
        FROM DUAL
) SUB_1
WHERE 1=1			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="usr_pwd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="evnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
