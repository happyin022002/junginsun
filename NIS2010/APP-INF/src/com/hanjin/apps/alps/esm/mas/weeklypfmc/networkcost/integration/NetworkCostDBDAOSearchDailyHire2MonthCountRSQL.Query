<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOSearchDailyHire2MonthCountRSQL">
			<desc><![CDATA[SearchDailyHire2MonthCount SELECT]]></desc>
			<sql><![CDATA[
SELECT COUNT(DISTINCT A1.COST_YRMON) cnt
  FROM MAS_MON_VVD A1
      ,MAS_CHRG_VSL_DLY_HIR A2
 WHERE 1 = 1
   AND A1.COST_YRMON = A2.COST_YRMON
   AND A1.VSL_CD     = A2.VSL_CD
   AND A1.SLS_YRMON  LIKE @[sls_yrmon] ||'%'
   AND A1.COST_WK    = @[cost_wk]
   AND A1.DELT_FLG   <> 'Y'
   AND A2.VSL_CD     = NVL(@[vsl_cd], A2.VSL_CD)			]]></sql>
			<params>
				<param name="sls_yrmon" type="12" value="" out="Y"/>
				<param name="cost_wk" type="12" value="" out="Y"/>
				<param name="vsl_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
