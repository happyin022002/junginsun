<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentDBDAOUpdateKpiCreationEditUSQL">
			<desc><![CDATA[UpdateKpiCreationEdit]]></desc>
			<sql><![CDATA[
UPDATE SQM_SPCL_CFM_QTA
   SET LOD_QTY       = @[lod_qty]
      ,GRS_RPB_REV   = @[grs_rpb_rev]
      ,PA_CM_UC_AMT  = @[pa_cm_uc_amt]
      ,RA_CM_UC_AMT  = @[ra_cm_uc_amt]
      ,UPD_USR_ID    = @[upd_usr_id]
      ,UPD_DT        = SYSDATE
 WHERE 1=1
   AND BSE_TP_CD     = @[bse_tp_cd]   -- Q, Y
   AND SPCL_TGT_CD   = @[spcl_tgt_cd] --S,R
   AND BSE_YR        = @[bse_yr]
   AND BSE_QTR_CD    = @[bse_qtr_cd]
   AND TRD_CD        = @[trd_cd]
   AND RLANE_CD      = @[rlane_cd]
   AND DIR_CD        = @[dir_cd]
   AND VSL_CD        = @[vsl_cd]
   AND SKD_VOY_NO    = @[skd_voy_no]
   AND SKD_DIR_CD    = @[skd_dir_cd]
   AND RHQ_CD        = @[rhq_cd]
   AND RGN_OFC_CD    = @[rgn_ofc_cd]			]]></sql>
			<params>
				<param name="lod_qty" type="12" value="" out="N"/>
				<param name="grs_rpb_rev" type="12" value="" out="N"/>
				<param name="pa_cm_uc_amt" type="12" value="" out="N"/>
				<param name="ra_cm_uc_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="spcl_tgt_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
