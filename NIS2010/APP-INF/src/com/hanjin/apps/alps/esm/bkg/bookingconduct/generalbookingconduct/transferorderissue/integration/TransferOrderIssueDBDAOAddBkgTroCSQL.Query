<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOAddBkgTroCSQL">
			<desc><![CDATA[TransferOrderIssueDB::AddBkgTro]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_TRO (
	BKG_NO
,	IO_BND_CD
,	RTN_TRO_FLG
,	TRO_SEQ
,	RCV_TERM_CD
,	RQST_DT
,	RQST_USR_ID
,	ACT_SHPR_CNT_CD
,	ACT_SHPR_SEQ
,	ACT_SHPR_NM
,	ACT_SHPR_PHN_NO
,	ACT_SHPR_ADDR
,	ZN_CD
,	DOR_LOC_CD
,	DOR_PST_NO
,	CFM_FLG
,	CFM_DT
,	DIFF_RMK
,	CNTC_PSON_NM
,	CNTC_FAX_NO
,	CNTC_MPHN_NO
,	CXL_FLG
,   SO_FLG
,   OWNR_TRK_FLG
,   BIZ_RGST_NO 
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[bkg_no]
,	@[io_bnd_cd]
,	NVL(@[rtn_tro_flg], 'N')
,	@[tro_seq]
,	@[rcv_term_cd]
,	sysdate
,	@[rqst_usr_id]
,	@[act_shpr_cnt_cd]
,	@[act_shpr_seq]
,	@[act_shpr_nm]
,	@[cntc_phn_no]
,	@[act_shpr_addr]
#if (${zn_cd} != '') 
,	@[dor_loc_cd]||@[zn_cd]
#else
,	@[zn_cd]
#end
,	@[dor_loc_cd]
,	@[dor_pst_no]
,	NVL(@[cfm_flg], 'N')
,	TO_DATE(@[cfm_dt], 'YYYY-MM-DD HH24:MI')
,	@[diff_rmk]
,	@[cntc_pson_nm]
,	@[cntc_fax_no]
,	@[cntc_mphn_no]
,	NVL(@[cxl_flg], 'N')
,   NVL(@[so_flg], 'N')
,   NVL(@[ownr_trk_flg], 'N')
,   @[biz_rgst_no]
,	@[cre_usr_id]
,	sysdate
,	@[upd_usr_id]
,	sysdate
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="rtn_tro_flg" type="12" value="" out="N"/>
				<param name="tro_seq" type="2" value="" out="N"/>
				<param name="rcv_term_cd" type="12" value="" out="N"/>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="act_shpr_cnt_cd" type="12" value="" out="N"/>
				<param name="act_shpr_seq" type="2" value="" out="N"/>
				<param name="act_shpr_nm" type="12" value="" out="N"/>
				<param name="cntc_phn_no" type="12" value="" out="N"/>
				<param name="act_shpr_addr" type="12" value="" out="N"/>
				<param name="dor_loc_cd" type="12" value="" out="N"/>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="dor_pst_no" type="12" value="" out="N"/>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="cfm_dt" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="cntc_fax_no" type="12" value="" out="N"/>
				<param name="cntc_mphn_no" type="12" value="" out="N"/>
				<param name="cxl_flg" type="12" value="" out="N"/>
				<param name="so_flg" type="12" value="" out="N"/>
				<param name="ownr_trk_flg" type="12" value="" out="N"/>
				<param name="biz_rgst_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
