<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndExpManifestListDownloadDBDAOsearchVesselDepartureRSQL">
			<desc><![CDATA[searchVesselDepartureRSQL]]></desc>
			<sql><![CDATA[
SELECT  A.VSL_CD
       ,A.SKD_VOY_NO
       ,A.SKD_DIR_CD
       ,A.VPS_PORT_CD 
       ,B.CRR_CD
       ,DECODE(C.ETD_DT,NULL,TO_CHAR(A.VPS_ETD_DT,'YYYY-MM-DD'),TO_CHAR(C.ETD_DT,'YYYY-MM-DD'))	AS VPS_ETD_DT
       ,C.CVY_REF_NO
       ,C.CAP_NM
       ,D.CGO_WGT 
       ,E1.TEU_FUL
       ,E2.FEU_FUL
       ,E3.OTH_FUL
       ,E4.TEU_MTY
       ,E5.FEU_MTY
       ,E6.OTH_MTY
       ,Z.ATTR_CTNT2
       ,A.UPD_USR_ID
       ,C.VSL_CD AS CND_VSL_CD
       ,C.VSL_DEP_RPT_SND_DT
       ,B.CRW_KNT 
       ,DECODE(C.ACT_DEP_DT,NULL,
        (
         SELECT  TO_CHAR(ACT_DEP_DT,'YYYY-MM-DD HH24:MI')
           FROM  VSK_ACT_PORT_SKD  D
          WHERE  D.VSL_CD = A.VSL_CD 
            AND  D.SKD_VOY_NO = CASE WHEN A.TURN_PORT_IND_CD IN ('Y', 'N') THEN A.SKD_VOY_NO ELSE A.TURN_SKD_VOY_NO END
            AND  D.SKD_DIR_CD = CASE WHEN A.TURN_PORT_IND_CD IN ('Y', 'N') THEN A.SKD_DIR_CD ELSE A.TURN_SKD_DIR_CD END
            AND  D.VPS_PORT_CD = @[pol_cd]
        ),  TO_CHAR(C.ACT_DEP_DT,'YYYY-MM-DD HH24:MI'))	AS ACT_DEP_DT
  FROM  VSK_VSL_PORT_SKD A
       ,MDM_VSL_CNTR B
       , (SELECT ROW_NUMBER() OVER( ORDER BY DECODE(PORT_CD,'CAVAN',0,1),UPD_DT DESC, ROWNUM) R_INDEX
                 ,CC.VSL_CD
                 ,CC.SKD_VOY_NO
                 ,CC.SKD_DIR_CD
                 ,CC.CVY_REF_NO
                 ,CC.CAP_NM
                 ,CC.VSL_DEP_RPT_SND_DT
                 ,CC.ETD_DT                 
                 ,CC.ACT_DEP_DT
            FROM BKG_CSTMS_CND_XPT_VSL CC
           WHERE 1=1
             AND  CC.VSL_CD     = SUBSTR('HNSG0036W',1,4)
             AND  CC.SKD_VOY_NO = SUBSTR('HNSG0036W', 5,4)
             AND  CC.SKD_DIR_CD = SUBSTR('HNSG0036W', 9)
       ) C
       ,BKG_HRD_CDG_CTNT Z
       ,(
         SELECT  ROUND(NVL(SUM(NVL(D.CGO_WGT,0)),0)/1000) AS CGO_WGT
           FROM  BKG_CSTMS_AMER_BL  D
          WHERE  D.CNT_CD       = 'CA'
            AND  D.IO_BND_CD    ='O'
            AND  D.MF_STS_CD    = 'A'
            AND  D.VSL_CD       = SUBSTR(@[vvd_cd],1,4)
            AND  D.SKD_VOY_NO   = SUBSTR(@[vvd_cd], 5,4)
            AND  D.SKD_DIR_CD   = SUBSTR(@[vvd_cd], 9)
            AND  D.CSTMS_POL_CD = @[pol_cd]
        ) D
       ,(
         SELECT  COUNT(DISTINCT E1.CNTR_NO)                 AS TEU_FUL
           FROM  BKG_CSTMS_AMER_BL  D, BKG_CSTMS_AMER_CNTR E1
          WHERE  D.CNT_CD           = 'CA'
            AND  D.IO_BND_CD        = 'O'
            AND  D.MF_STS_CD        = 'A'
            AND  D.VSL_CD           = SUBSTR(@[vvd_cd],1,4)
            AND  D.SKD_VOY_NO       = SUBSTR(@[vvd_cd], 5,4)
            AND  D.SKD_DIR_CD       = SUBSTR(@[vvd_cd], 9)
            AND  D.CSTMS_POL_CD     = @[pol_cd]
            AND  D.CNT_CD           = E1.CNT_CD   
            AND  D.IO_BND_CD        = E1.IO_BND_CD
            AND  D.BL_NO            = E1.BL_NO    
            AND  D.MF_NO            IS NULL
            AND  E1.CNTR_TPSZ_CD    LIKE '_2'
            AND  E1.FULL_MTY_CD     = 'F'
            AND  E1.IBD_CNTR_STS_CD = 'A'
        ) E1
       ,(
         SELECT  COUNT(DISTINCT E1.CNTR_NO)                 AS FEU_FUL
           FROM  BKG_CSTMS_AMER_BL  D, BKG_CSTMS_AMER_CNTR E1
          WHERE  D.CNT_CD           = 'CA'
            AND  D.IO_BND_CD        = 'O'
            AND  D.MF_STS_CD        = 'A'
            AND  D.VSL_CD           = SUBSTR(@[vvd_cd],1,4)
            AND  D.SKD_VOY_NO       = SUBSTR(@[vvd_cd], 5,4)
            AND  D.SKD_DIR_CD       = SUBSTR(@[vvd_cd], 9)
            AND  D.CSTMS_POL_CD     = @[pol_cd]
            AND  D.CNT_CD           = E1.CNT_CD   
            AND  D.IO_BND_CD        = E1.IO_BND_CD
            AND  D.BL_NO            = E1.BL_NO    
            AND  D.MF_NO            IS NULL
            AND  E1.CNTR_TPSZ_CD    LIKE '_4'
            AND  E1.FULL_MTY_CD     = 'F'
            AND  E1.IBD_CNTR_STS_CD = 'A'
        ) E2
       ,(
         SELECT  COUNT(DISTINCT E1.CNTR_NO)                 AS OTH_FUL
           FROM  BKG_CSTMS_AMER_BL  D, BKG_CSTMS_AMER_CNTR E1
          WHERE  D.CNT_CD           = 'CA'
            AND  D.IO_BND_CD        = 'O'
            AND  D.MF_STS_CD        = 'A'
            AND  D.VSL_CD           = SUBSTR(@[vvd_cd],1,4)
            AND  D.SKD_VOY_NO       = SUBSTR(@[vvd_cd], 5,4)
            AND  D.SKD_DIR_CD       = SUBSTR(@[vvd_cd], 9)
            AND  D.CSTMS_POL_CD     = @[pol_cd]
            AND  D.CNT_CD           = E1.CNT_CD   
            AND  D.IO_BND_CD        = E1.IO_BND_CD
            AND  D.BL_NO            = E1.BL_NO    
            AND  D.MF_NO            IS NULL
            AND  E1.FULL_MTY_CD     = 'F'
            AND  E1.IBD_CNTR_STS_CD = 'A'
            AND  (E1.CNTR_TPSZ_CD LIKE '_5' OR
                  E1.CNTR_TPSZ_CD LIKE '_7' OR
                  E1.CNTR_TPSZ_CD LIKE '_8' OR
                  E1.CNTR_TPSZ_CD LIKE '_9' OR
                  E1.CNTR_TPSZ_CD IN ('DW', 'DX'))
        ) E3
       ,(
         SELECT  COUNT(DISTINCT E1.CNTR_NO)                 AS TEU_MTY
           FROM  BKG_CSTMS_AMER_BL  D, BKG_CSTMS_AMER_CNTR E1
          WHERE  D.CNT_CD           = 'CA'
            AND  D.IO_BND_CD        = 'O'
            AND  D.MF_STS_CD        = 'A'
            AND  D.VSL_CD           = SUBSTR(@[vvd_cd],1,4)
            AND  D.SKD_VOY_NO       = SUBSTR(@[vvd_cd], 5,4)
            AND  D.SKD_DIR_CD       = SUBSTR(@[vvd_cd], 9)
            AND  D.CSTMS_POL_CD     = @[pol_cd]
            AND  D.CNT_CD           = E1.CNT_CD   
            AND  D.IO_BND_CD        = E1.IO_BND_CD
            AND  D.BL_NO            = E1.BL_NO    
            AND  D.MF_NO            IS NULL
            AND  E1.CNTR_TPSZ_CD    LIKE '_2'
            AND  E1.FULL_MTY_CD     = 'M'
            AND  E1.IBD_CNTR_STS_CD = 'A'
        ) E4
       ,(
         SELECT  COUNT(DISTINCT E1.CNTR_NO)                 AS FEU_MTY
           FROM  BKG_CSTMS_AMER_BL  D, BKG_CSTMS_AMER_CNTR E1
          WHERE  D.CNT_CD           = 'CA'
            AND  D.IO_BND_CD        = 'O'
            AND  D.MF_STS_CD        = 'A'
            AND  D.VSL_CD           = SUBSTR(@[vvd_cd],1,4)
            AND  D.SKD_VOY_NO       = SUBSTR(@[vvd_cd], 5,4)
            AND  D.SKD_DIR_CD       = SUBSTR(@[vvd_cd], 9)
            AND  D.CSTMS_POL_CD     = @[pol_cd]
            AND  D.CNT_CD           = E1.CNT_CD   
            AND  D.IO_BND_CD        = E1.IO_BND_CD
            AND  D.BL_NO            = E1.BL_NO    
            AND  D.MF_NO            IS NULL
            AND  E1.CNTR_TPSZ_CD    LIKE '_4'
            AND  E1.FULL_MTY_CD     = 'M'
            AND  E1.IBD_CNTR_STS_CD = 'A'
        ) E5
       ,(
         SELECT  COUNT(DISTINCT E1.CNTR_NO)                 AS OTH_MTY
           FROM  BKG_CSTMS_AMER_BL  D, BKG_CSTMS_AMER_CNTR E1
          WHERE  D.CNT_CD           = 'CA'
            AND  D.IO_BND_CD        = 'O'
            AND  D.MF_STS_CD        = 'A'
            AND  D.VSL_CD           = SUBSTR(@[vvd_cd],1,4)
            AND  D.SKD_VOY_NO       = SUBSTR(@[vvd_cd], 5,4)
            AND  D.SKD_DIR_CD       = SUBSTR(@[vvd_cd], 9)
            AND  D.CSTMS_POL_CD     = @[pol_cd]
            AND  D.CNT_CD           = E1.CNT_CD   
            AND  D.IO_BND_CD        = E1.IO_BND_CD
            AND  D.BL_NO            = E1.BL_NO    
            AND  D.MF_NO            IS NULL
            AND  E1.FULL_MTY_CD     = 'M'
            AND  E1.IBD_CNTR_STS_CD = 'A'
            AND  (E1.CNTR_TPSZ_CD LIKE '_5' OR
                  E1.CNTR_TPSZ_CD LIKE '_7' OR
                  E1.CNTR_TPSZ_CD LIKE '_8' OR
                  E1.CNTR_TPSZ_CD LIKE '_9' OR
                  E1.CNTR_TPSZ_CD IN ('DW', 'DX'))
        ) E6
 WHERE  A.VSL_CD        = B.VSL_CD
   AND  A.VSL_CD        = C.VSL_CD(+)
   AND  A.SKD_VOY_NO    = C.SKD_VOY_NO(+)
   AND  A.SKD_DIR_CD    = C.SKD_DIR_CD(+)
   AND  C.R_INDEX(+)    = 1 /* VVD별로 여러건 저장 되어 우선 순위 1건 조회 */
   AND  B.CRR_CD        = Z.ATTR_CTNT1(+)
   AND  Z.HRD_CDG_ID(+) = 'CND_CSTMS_CRR_CD'
   AND  A.VSL_CD        = SUBSTR(@[vvd_cd],1,4)
   AND  A.SKD_VOY_NO    = SUBSTR(@[vvd_cd], 5,4)
   AND  A.SKD_DIR_CD    = SUBSTR(@[vvd_cd], 9)
   AND  A.VPS_PORT_CD   = @[pol_cd]
   AND  A.CLPT_IND_SEQ  = 1

			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
