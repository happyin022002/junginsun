<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCdlTransmodeRSQL">
			<desc><![CDATA[searchCdlTransmode]]></desc>
			<sql><![CDATA[
SELECT	'X' TRANS_MODE
FROM	BKG_VVD
WHERE	BKG_NO		= @[bkg_no]
AND	VSL_PRE_PST_CD		= 'U'
AND	POL_CD		= @[in_pod_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="in_pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
