,	CRE_OFC_CD = @[cre_ofc_cd]
,	GLINE_SEQ = @[gline_seq]
,	PRS_CUST_TP_CD = @[prs_cust_tp_cd]
,	BSE_SEQ = @[bse_seq]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
