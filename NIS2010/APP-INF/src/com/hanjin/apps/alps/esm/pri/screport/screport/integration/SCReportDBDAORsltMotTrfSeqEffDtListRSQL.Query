<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltMotTrfSeqEffDtListRSQL">
			<desc><![CDATA[PRI_MOT_TRF_MN 에서 SVC_SCP_CD 별 MOT_TRF_SEQ, EFF_DT, FILE_DT, CFM_FLG, CFM_DT 리스트를 조회 한다.]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
      ,MOT_TRF_SEQ
      ,TO_CHAR(EFF_DT,  'YYYY-MM-DD') AS EFF_DT
      ,TO_CHAR(FILE_DT, 'YYYY-MM-DD') AS FILE_DT
      ,TO_CHAR(CFM_DT,  'YYYY-MM-DD') AS CFM_DT
      ,DECODE ( NVL ( CFM_FLG, 'N' ), 'N', 'No', 'Y', 'Yes' ) AS CFM_FLG
  FROM PRI_MOT_TRF_MN
 WHERE SVC_SCP_CD = @[svc_scp_cd]
 ORDER BY EFF_DT DESC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
