<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltSearchTimelyOutOfDateBookingListRSQL">
			<desc><![CDATA[전체 대상 BKG 중 Batch Result 가 F (Fail) 인 목록조회]]></desc>
			<sql><![CDATA[
SELECT O.REGION
 ,B.CTRT_OFC_CD
 ,B.CTRT_SREP_CD
 ,B.BKG_NO
 ,B.VSL_CD ||B.SKD_VOY_NO ||B.SKD_DIR_CD VVD
 ,B.SC_NO
 ,B.RFA_NO
 ,B.TAA_NO
 ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03188' AND INTG_CD_VAL_CTNT = R.RT_CHK_RSLT_CD) RT_CHK_RSLT_CD
 ,B.SVC_SCP_CD -- 여기서부터는 참고 정보
 ,B.BKG_OFC_CD
 ,B.PORT_CLZ_DT
 ,R.RT_APLY_DT
 ,B.BKG_CRE_DT
FROM BKG_BOOKING B
    ,BKG_RATE R
    ,BKG_OFC_LVL_V O
WHERE 1=1
#if (${bkg_from_dt} != '' && ${bkg_to_dt} != '') 
  AND B.PORT_CLZ_DT BETWEEN TO_DATE(@[bkg_from_dt], 'YYYY-MM-DD') AND TO_DATE(@[bkg_to_dt], 'YYYY-MM-DD') + 0.99999
#end
#if (${vvd} != '') 
  AND B.VSL_CD = SUBSTR(@[vvd],1,4)
  AND B.SKD_VOY_NO = SUBSTR(@[vvd],5,4)
  AND B.SKD_DIR_CD = SUBSTR(@[vvd],9,4)
#end
  AND B.BKG_NO = R.BKG_NO
  AND R.RT_CHK_DT IS NOT NULL -- 결과가 생성되지 않은 경우 제외
  AND B.BKG_STS_CD <> 'X'
  AND B.BKG_CGO_TP_CD = 'F'
  AND B.VSL_CD NOT IN ('SMXX','SMYY','SMZZ') --pseudo VSL 제외
  AND B.CTRT_OFC_CD =O.OFC_CD
  AND B.CTRT_OFC_CD = @[ctrt_ofc_cd]
  AND B.CTRT_SREP_CD = @[ctrt_srep_cd]
  AND R.RT_CHK_RSLT_CD = 'F' -- 결과가 fail
ORDER BY O.REGION, B.SVC_SCP_CD, B.BKG_OFC_CD, B.SC_NO, B.RFA_NO, B.TAA_NO, B.BKG_NO			]]></sql>
			<params>
				<param name="bkg_from_dt" type="12" value="" out="N"/>
				<param name="bkg_to_dt" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="ctrt_srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
