<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchCustAdvisoryNoticeCntByBSTDownloadRSQL">
			<desc><![CDATA[searchCustAdvisoryNoticeCntByBSTDownload]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT       
FROM BKG_BOOKING BK
     , BKG_VVD     BV
WHERE BV.VSL_CD     	= SUBSTR(@[vvd], 1,4)
	AND BV.SKD_VOY_NO 	= SUBSTR(@[vvd], 5,4)
	AND BV.SKD_DIR_CD IN (${dir_sts_cd})
	AND BK.BKG_NO = BV.BKG_NO
	AND BK.BKG_STS_CD <>'X'
	AND BK.BKG_CGO_TP_CD != 'P'			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
