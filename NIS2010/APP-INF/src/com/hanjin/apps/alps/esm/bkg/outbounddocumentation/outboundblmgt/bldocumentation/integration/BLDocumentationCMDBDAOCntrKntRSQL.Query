<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCntrKntRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
SELECT COUNT(CNTR_NO) FROM BKG_CNTR_HIS WHERE BKG_NO = @[bkg_no] AND CORR_NO = 'TMP0000001'
#else 
SELECT COUNT(CNTR_NO) FROM BKG_CONTAINER WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
