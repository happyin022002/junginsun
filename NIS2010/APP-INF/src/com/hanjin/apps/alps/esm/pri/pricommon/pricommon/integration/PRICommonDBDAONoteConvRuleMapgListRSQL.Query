<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAONoteConvRuleMapgListRSQL">
			<desc><![CDATA[NOTE_CONV_RULE_CD
PRI_NOTE_CONV_TP_RULE_MAPG
조인해서 해당 데이터만 조회]]></desc>
			<sql><![CDATA[
SELECT    A.NOTE_CONV_RULE_CD CD
        , A.NOTE_CONV_RULE_NM NM
	 FROM PRI_NOTE_CONV_RULE A
    	, PRI_NOTE_CONV_TP_RULE_MAPG B
	WHERE A.NOTE_CONV_RULE_CD = B.NOTE_CONV_RULE_CD
	  AND B.PRC_CTRT_TP_CD = @[etc1]
	  AND B.NOTE_CONV_TP_CD =  @[etc2]
	ORDER BY A.NOTE_CONV_RULE_CD ASC			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
