<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManilaManifestListDownloadDBDAOsearchVvdDtlRSQL">
			<desc><![CDATA[ManilaSearchVvdDtlVO]]></desc>
			<sql><![CDATA[
SELECT  REG_NUMBER,
        ETADT,
        VNAME,
        POLCD,
        DISCHARGE,
        ROWNUM AS SEQ
FROM (
	SELECT 	DISTINCT
			SUBSTR(BKG.POL_CD,1,2) polcd,
	        TO_CHAR(VPS.VPS_ETA_DT,'YYYY-MM-DD') etadt,
	        '' discharge, 
	        SUBSTR(VSL.VSL_ENG_NM,1,10)||BKG.SKD_VOY_NO||BKG.SKD_DIR_CD vname,
	        @[reg_no] reg_number
	 FROM  VSK_VSL_PORT_SKD VPS,
	       MDM_VSL_CNTR VSL,
	       BKG_VVD BKG
	 WHERE VSL.VSL_CD       = BKG.VSL_CD
	   AND VPS.VSL_CD        = BKG.VSL_CD
	   AND VPS.SKD_VOY_no    = BKG.SKD_VOY_no
	   AND VPS.SKD_DIR_CD    = BKG.SKD_DIR_CD
	#if (${vsl_cd}!= '') 	
	              AND BKG.VSL_CD        = @[vsl_cd] 
	#end
	#if (${skd_voy_no}!= '') 
	              AND BKG.SKD_VOY_no    = @[skd_voy_no]
	#end
	#if (${skd_dir_cd}!= '') 
	              AND BKG.SKD_DIR_CD    = @[skd_dir_cd]
	#end
	#if (${pol_cd}!= '') 
	              AND BKG.POL_CD        LIKE @[pol_cd] 
	#end
	#if (${pod_cd}!= '') 
	              AND BKG.POD_CD        LIKE @[pod_cd] 
	#end
	#if (${pod_cd}!= '') 
	              AND VPS.VPS_PORT_CD    LIKE @[pod_cd]
	#end
)				]]></sql>
			<params>
				<param name="reg_no" type="12" value="333333333" out="N"/>
				<param name="vsl_cd" type="12" value="HNCF" out="N"/>
				<param name="skd_voy_no" type="12" value="0026" out="N"/>
				<param name="skd_dir_cd" type="12" value="W" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="PHMNL" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
