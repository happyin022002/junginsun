<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRINoteConversionProposalDBDAOMdmSvcScpAuthRSQL">
			<desc><![CDATA[작성권한 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(SVC_SCP_CD) 
  FROM PRI_SVC_SCP_TRF
 WHERE TRF_PFX_CD 	= @[trf_pfx_cd]
   AND TRF_NO 		= @[trf_no]
   AND SVC_SCP_CD 	IN (
		SELECT SVC_SCP_CD 
		  FROM PRI_AUTHORIZATION
		 WHERE PRC_CTRT_TP_CD = 'S'
		   AND USR_ID = @[cre_usr_id]
		   AND EFF_DT <= SYSDATE
		   AND EXP_DT >= SYSDATE  )			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
