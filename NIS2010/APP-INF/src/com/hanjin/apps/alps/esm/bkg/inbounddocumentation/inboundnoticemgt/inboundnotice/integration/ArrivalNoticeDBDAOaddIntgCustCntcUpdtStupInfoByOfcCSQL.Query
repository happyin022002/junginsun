<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOaddIntgCustCntcUpdtStupInfoByOfcCSQL">
			<desc><![CDATA[Integrated Customer Data Update Setup Master 생성]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_IB_CUST_CNTC_STUP
 (OFC_CD,
  AUTO_UPD_FLG,
  UPD_LOCL_DT,
  UPD_LOCL_GDT,
  CRE_USR_ID,
  CRE_DT,
  UPD_USR_ID,
  UPD_DT)
(SELECT   
  @[ofc_cd],
  @[auto_upd_flg],
  GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,GLOBALDATE_PKG.GET_LOCCD_FNC(@[ofc_cd])),
  GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',SYSDATE,'GMT'),
  @[cre_usr_id],
  SYSDATE,
  @[upd_usr_id],
  SYSDATE
 FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[ofc_cd])			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="auto_upd_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
