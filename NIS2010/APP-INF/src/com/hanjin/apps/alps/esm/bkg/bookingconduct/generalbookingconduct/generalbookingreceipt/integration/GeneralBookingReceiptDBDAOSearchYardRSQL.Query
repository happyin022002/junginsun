<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchYardRSQL">
			<desc><![CDATA[RCV/DLV Term이 Yard 인데 POR/DEL에 속한 YARD가 없을 경우 확인]]></desc>
			<sql><![CDATA[
SELECT	count(1) CNT
FROM	mdm_YARD
WHERE	loc_cd   = @[loc_cd]
#if (${yd_cd}!= '')
AND     yd_cd = @[yd_cd]
#end 
AND		delt_flg = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
