<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOAddBLissueRqstCSQL">
			<desc><![CDATA[B/L 발급 요청 시 요청 정보를 ALPS로 I/F]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BL_ISS_RQST
 ( XTER_RQST_NO 
, XTER_RQST_SEQ 
, BL_ISS_RQST_DT 
, RQST_RCT_LOC_CD 
, BL_NO 
, VSL_CD 
, SKD_VOY_NO 
, SKD_DIR_CD 
, VSL_NM 
, RQST_BL_TP_CD 
, RQST_CO_NM 
, RQST_USR_EML 
, RQST_ATND_NM 
, RQST_PHN_NO 
, BL_RQST_RMK 
, ACT_SHPR_NM 
, ACT_SHPR_RGST_NO 
, TAX_INV_RCVR_CO_NM 
, TAX_INV_RCVR_RGST_NO 
, TAX_INV_RCVR_ATND_NM 
, TAX_INV_RCVR_PHN_NO 
, REMIT_CO_NM 
, REMIT_KND_CD 
, CRR_BANK_CD 
, CRR_BANK_ACCT_NO 
, CRR_REMIT_AMT 
, CRR_REMIT_DT 
, BL_ISS_USR_ID 
, BL_ISS_STS_CD 
, BL_ISS_RMK 
, DELT_FLG 
, CRE_USR_ID 
, CRE_DT 
, UPD_USR_ID 
, UPD_DT 
, CRR_ACCT_CURR_CD 
, BL_ISS_REQ_DT
, CUST_ID
, CRR_USA_BANK_CD
, CRR_USA_BANK_ACCT_NO
, CRR_USA_REMIT_AMT
, CRR_USA_REMIT_DT
, CRR_USA_ACCT_CURR_CD
, BL_ISS_RQST_CD
, BL_RCV_TP_CD
, MNL_BL_OBRD_DT
, MNL_BL_ISS_DT
, CERTI_EXIST_FLG
, FRT_DP_FLG
 )

VALUES ( @[xter_rqst_no] ,
	  (SELECT NVL(MAX(XTER_RQST_SEQ) +1, 1) FROM BKG_BL_ISS_RQST WHERE XTER_RQST_NO = @[xter_rqst_no]),
	  SYSDATE,
      @[rqst_rct_loc_cd] ,
      @[bl_no] ,
      @[vsl_cd] ,
      @[skd_voy_no] ,
      @[skd_dir_cd] ,
      @[vsl_nm] ,
      @[rqst_bl_tp_cd] ,
      @[rqst_co_nm] ,
      @[rqst_usr_eml] ,
      @[rqst_atnd_nm] ,
      @[rqst_phn_no] ,
      @[bl_rqst_rmk] ,
      @[act_shpr_nm] ,
      @[act_shpr_rgst_no] ,
      @[tax_inv_rcvr_co_nm] ,
      @[tax_inv_rcvr_rgst_no] ,
      @[tax_inv_rcvr_atnd_nm] ,
      @[tax_inv_rcvr_phn_no] ,
      @[remit_co_nm] ,
      @[remit_knd_cd] ,
      @[crr_bank_cd] ,
      @[crr_bank_acct_no] ,
      @[crr_remit_amt] ,
      TO_DATE(@[crr_remit_dt],'YYYY/MM/DD HH24:MI:SS') ,
      @[bl_iss_usr_id] ,
      @[bl_iss_sts_cd] ,
      @[bl_iss_rmk] ,
      NVL(@[delt_flg],'N') ,
      'WEB' ,
      sysdate ,
      'WEB' ,
      sysdate ,
      @[crr_acct_curr_cd] ,
	  SYSDATE,
      @[upd_usr_id] ,
      @[crr_usa_bank_cd] ,
      @[crr_usa_bank_acct_no] ,
      @[crr_usa_remit_amt] ,
      TO_DATE(@[crr_usa_remit_dt],'YYYY/MM/DD HH24:MI:SS') ,
      @[crr_usa_acct_curr_cd],
	  nvl(@[bl_iss_rqst_cd],'W'),
      @[bl_rcv_tp_cd],
      TO_DATE(@[mnl_bl_obrd_dt],'YYYY-MM-DD'),
      TO_DATE(@[mnl_bl_iss_dt],'YYYY-MM-DD'),
      @[certi_exist_flg],
      @[frt_dp_flg]
	)			]]></sql>
			<params>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="rqst_rct_loc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vsl_nm" type="12" value="" out="N"/>
				<param name="rqst_bl_tp_cd" type="12" value="" out="N"/>
				<param name="rqst_co_nm" type="12" value="" out="N"/>
				<param name="rqst_usr_eml" type="12" value="" out="N"/>
				<param name="rqst_atnd_nm" type="12" value="" out="N"/>
				<param name="rqst_phn_no" type="12" value="" out="N"/>
				<param name="bl_rqst_rmk" type="12" value="" out="N"/>
				<param name="act_shpr_nm" type="12" value="" out="N"/>
				<param name="act_shpr_rgst_no" type="12" value="" out="N"/>
				<param name="tax_inv_rcvr_co_nm" type="12" value="" out="N"/>
				<param name="tax_inv_rcvr_rgst_no" type="12" value="" out="N"/>
				<param name="tax_inv_rcvr_atnd_nm" type="12" value="" out="N"/>
				<param name="tax_inv_rcvr_phn_no" type="12" value="" out="N"/>
				<param name="remit_co_nm" type="12" value="" out="N"/>
				<param name="remit_knd_cd" type="12" value="" out="N"/>
				<param name="crr_bank_cd" type="12" value="" out="N"/>
				<param name="crr_bank_acct_no" type="12" value="" out="N"/>
				<param name="crr_remit_amt" type="12" value="" out="N"/>
				<param name="crr_remit_dt" type="12" value="" out="N"/>
				<param name="bl_iss_usr_id" type="12" value="" out="N"/>
				<param name="bl_iss_sts_cd" type="12" value="" out="N"/>
				<param name="bl_iss_rmk" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="crr_acct_curr_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="crr_usa_bank_cd" type="12" value="" out="N"/>
				<param name="crr_usa_bank_acct_no" type="12" value="" out="N"/>
				<param name="crr_usa_remit_amt" type="12" value="" out="N"/>
				<param name="crr_usa_remit_dt" type="12" value="" out="N"/>
				<param name="crr_usa_acct_curr_cd" type="12" value="" out="N"/>
				<param name="bl_iss_rqst_cd" type="12" value="" out="N"/>
				<param name="bl_rcv_tp_cd" type="12" value="" out="N"/>
				<param name="mnl_bl_obrd_dt" type="12" value="" out="N"/>
				<param name="mnl_bl_iss_dt" type="12" value="" out="N"/>
				<param name="certi_exist_flg" type="12" value="" out="N"/>
				<param name="frt_dp_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
