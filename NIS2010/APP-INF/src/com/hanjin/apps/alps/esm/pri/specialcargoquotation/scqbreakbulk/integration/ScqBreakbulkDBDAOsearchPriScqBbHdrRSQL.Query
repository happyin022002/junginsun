<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqBreakbulkDBDAOsearchPriScqBbHdrRSQL">
			<desc><![CDATA[PRI_SCQ_BB_MN   PRI_SCQ_PROG

* History
2013.06.26 이혜민 [CHM-201325215] Special cargo Quotation System 수정 요청 - Approver Name 추가, Request ID는 Requester Name으로 변경
2013.07.22 송호진 [CHM-201325102] Special cargo Quotation System 추가 수정 요청건 - Approval 상태 Cancel, Delete 기능 추가
2015.04.23 송호진 [CHM-201533702] SCQ내에 Unit Dual (cm/Inch) System 개발 요청
]]></desc>
			<sql><![CDATA[
SELECT MN.SCQ_RQST_NO                      
     , MN.SCQ_VER_NO                       
     , MN.PROG_STS_CD                      
     , MN.RQST_OFC_CD                      
     , MN.RQST_SREP_CD                                               
     , MN.POL_CD                           
     , MN.POL_YD_CD                        
     , MN.POD_CD                           
     , MN.POD_YD_CD                                              
     , MN.SVC_SCP_CD                       
     , MN.CUST_CNT_CD                      
     , MN.CUST_SEQ                         
     , MN.RCV_TERM_CD                      
     , MN.DE_TERM_CD                       
     , TO_CHAR(MN.PROP_EFF_DT,'YYYY-MM-DD') AS PROP_EFF_DT                     
     , TO_CHAR(MN.PROP_EXP_DT,'YYYY-MM-DD') AS PROP_EXP_DT                     
     , TO_CHAR(MN.APRO_EFF_DT,'YYYY-MM-DD') AS APRO_EFF_DT                     
     , TO_CHAR(MN.APRO_EXP_DT,'YYYY-MM-DD') AS APRO_EXP_DT                     
     , MN.DELT_FLG                         
     , MN.CRE_USR_ID                
     , MN.CRE_DT                           
     , MN.UPD_USR_ID                       
     , MN.UPD_DT   
     , MN.APRO_OFC_CD                         
     , PG.SPCL_CGO_TP_CD                   
     , PG.PROG_SEQ                               
     , PG.PROG_STS_CD    AS PG_PROG_STS_CD         
     , PG.PROG_OFC_CD                       
     , (SELECT USR_NM FROM COM_USER WHERE USR_ID = PG.PROG_USR_ID) PROG_USR_NM
     , PG.PROG_USR_ID                 
     , PG.PROG_DT                          
     , PG.PROG_RMK
     , (SELECT SREP_NM NM
          FROM MDM_SLS_REP
         WHERE SREP_CD      = MN.RQST_SREP_CD
           AND SREP_STS_CD  = 'N'
           AND ROWNUM <= 1) AS RQST_SREP_NM
     , (SELECT CUST_LGL_ENG_NM 
          FROM MDM_CUSTOMER
         WHERE CUST_CNT_CD  = MN.CUST_CNT_CD
           AND CUST_SEQ     = MN.CUST_SEQ
           AND ROWNUM <= 1) AS CUST_LGL_ENG_NM    
     , (SELECT INTG_CD_VAL_DP_DESC 
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID       = 'CD03161'
           AND INTG_CD_VAL_CTNT = MN.PROG_STS_CD
           AND ROWNUM <= 1)     AS PROG_STS_NM  
     , (SELECT 1 FROM PRI_AUTHORIZATION AUTH
         WHERE PRC_CTRT_TP_CD >= ' '
           AND AUTH.SVC_SCP_CD = MN.SVC_SCP_CD        
		   AND USR_ID = @[upd_usr_id]
           AND ROWNUM <= 1) AS AUTH
     , MN.TS_LOC_CD
     , MN.TS_YD_CD
     , MN.PROP_RT_AMT
     , MN.APRO_RT_AMT             
     , NVL2 ( 
       (SELECT 1 FROM PRI_SCQ_BB_MN MX
         WHERE MX.SCQ_RQST_NO = MN.SCQ_RQST_NO
           AND MX.SCQ_VER_NO > MN.SCQ_VER_NO 
           AND ROWNUM <= 1 ), 'N', 'Y' ) AS LAST_FLG        
     , '' AS PRE_PROG_STS_CD
     , '' AS RSLT_FLG
     , ( SELECT  NVL ( MAX ( CN.CNTR_GRP_VER_NO ), 1 )
         FROM    PRI_SCQ_BB_CNTR CN
         WHERE   CN.SCQ_RQST_NO = MN.SCQ_RQST_NO
         AND     CN.SCQ_VER_NO  = MN.SCQ_VER_NO ) AS MAX_CNTR_GRP_NO
     , ( SELECT	 CN1.CRE_USR_ID
		 FROM	 PRI_SCQ_BB_CNTR CN1
         WHERE   CN1.SCQ_RQST_NO = MN.SCQ_RQST_NO
         AND     CN1.SCQ_VER_NO  = MN.SCQ_VER_NO 
         AND     CN1.CNTR_GRP_VER_NO = ( SELECT  NVL ( MAX ( CN2.CNTR_GRP_VER_NO ), 0 )
                                        FROM    PRI_SCQ_BB_CNTR CN2
                                        WHERE   CN2.SCQ_RQST_NO = MN.SCQ_RQST_NO
                                        AND     CN2.SCQ_VER_NO  = MN.SCQ_VER_NO ) 
         AND     ROWNUM = 1
        ) AS LAST_CNTR_GRP_USR_ID
     ,  MN.LANE_CD
     ,  MN.VVD_CD
     ,  TO_CHAR ( MN.VPS_ETA_DT, 'YYYY-MM-DD HH24:MI' ) AS VPS_ETA_DT
     , MN.ACT_CUST_CNT_CD
     , MN.ACT_CUST_SEQ
     , NVL ( MN.ACT_CUST_NM,
       (SELECT CUST_LGL_ENG_NM 
          FROM MDM_CUSTOMER
         WHERE CUST_CNT_CD  = MN.ACT_CUST_CNT_CD
           AND CUST_SEQ     = MN.ACT_CUST_SEQ
           AND ROWNUM <= 1) ) AS ACT_CUST_NM  
     , MN.SCQ_BID_FLG 
     , NVL ( MN.MEAS_SYS_CD, 'M' ) AS MEAS_SYS_CD
     , NVL ( MN.MEAS_SYS_CD, 'M' ) AS MEAS_SYS_CD_VW
  FROM PRI_SCQ_BB_MN MN                   
     , PRI_SCQ_PROG PG                     
 WHERE MN.SCQ_RQST_NO = @[scq_rqst_no]     
   AND MN.SCQ_VER_NO = @[scq_ver_no]       
   AND PG.SCQ_RQST_NO = MN.SCQ_RQST_NO     
   AND PG.SCQ_VER_NO = MN.SCQ_VER_NO  
   AND PG.SPCL_CGO_TP_CD = 'BB'    
ORDER BY PROG_SEQ DESC			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="0000" out="N"/>
				<param name="scq_rqst_no" type="12" value="HAM201306170010" out="N"/>
				<param name="scq_ver_no" type="12" value="001" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
