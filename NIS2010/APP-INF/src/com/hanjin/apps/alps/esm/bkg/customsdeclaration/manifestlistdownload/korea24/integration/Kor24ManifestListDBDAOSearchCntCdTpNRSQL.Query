<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Kor24ManifestListDBDAOSearchCntCdTpNRSQL">
			<desc><![CDATA["N"]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD CNT_CD
     , CUST_SEQ CUST_CD
  FROM BKG_CUSTOMER
 WHERE BKG_NO = @[a_bkg_no]
   AND BKG_NO_SPLIT = @[a_bkg_no_split]
   AND BKG_CUST_TP_CD = 'N'			]]></sql>
			<params>
				<param name="a_bkg_no" type="12" value="" out="N"/>
				<param name="a_bkg_no_split" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
