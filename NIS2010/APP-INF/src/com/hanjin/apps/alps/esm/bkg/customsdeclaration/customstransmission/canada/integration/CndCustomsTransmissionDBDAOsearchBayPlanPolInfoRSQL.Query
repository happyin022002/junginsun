<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndCustomsTransmissionDBDAOsearchBayPlanPolInfoRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT 
(
SELECT TO_CHAR(ACT_DEP_DT, 'YYYYMMDDHH24MI') ACT_DEP_DT
FROM VSK_ACT_PORT_SKD
WHERE	1 = 1
AND	VSL_CD		    = SUBSTR(@[vvd],1, 4)
AND	SKD_VOY_NO		= SUBSTR(@[vvd],5, 4)
AND	SKD_DIR_CD		= SUBSTR(@[vvd],9, 1)
AND	VPS_PORT_CD		= @[pol]
AND	CLPT_IND_SEQ	= '1'
) POL_ACT_DEP_DT,
(
SELECT TO_CHAR(VPS_ETD_DT, 'YYYYMMDDHH24MI') VPS_ETD_DT
FROM VSK_VSL_PORT_SKD T1
WHERE VSL_CD		= SUBSTR(@[vvd],1, 4)
AND	SKD_VOY_NO		= SUBSTR(@[vvd],5, 4)
AND	SKD_DIR_CD		= SUBSTR(@[vvd],9, 1)
AND	VPS_PORT_CD		= @[pol]
AND	CLPT_IND_SEQ	= '1'
) POL_EST_DEP_DT,
(
SELECT TO_CHAR(VPS_ETA_DT, 'YYYYMMDDHH24MI') VPS_ETA_DT
FROM VSK_VSL_PORT_SKD T1
WHERE VSL_CD		= SUBSTR(@[vvd],1, 4)
AND	SKD_VOY_NO		= SUBSTR(@[vvd],5, 4)
AND	SKD_DIR_CD		= SUBSTR(@[vvd],9, 1)
AND	VPS_PORT_CD		= @[pod]
AND	CLPT_IND_SEQ	= '1'
) POd_EST_ARR_DT,
 (
    SELECT VPS_PORT_CD
    FROM VSK_VSL_PORT_SKD A
        WHERE A.VSL_CD 		= SUBSTR(@[vvd],1, 4)
        AND A.SKD_VOY_NO 	= SUBSTR(@[vvd],5, 4)
        AND A.SKD_DIR_CD 	= SUBSTR(@[vvd],9, 1)
         AND NVL(A.SKD_CNG_STS_CD, 'X') != 'S'
        AND A.CLPT_IND_SEQ = 1
        AND A.CLPT_SEQ = (
            SELECT MIN(CLPT_SEQ)
            FROM VSK_VSL_PORT_SKD A 
            WHERE A.VSL_CD 		= SUBSTR(@[vvd],1, 4)
            AND A.SKD_VOY_NO 	= SUBSTR(@[vvd],5, 4)
            AND A.SKD_DIR_CD 	= SUBSTR(@[vvd],9, 1)
            AND NVL(SKD_CNG_STS_CD, 'X') != 'S'
            AND VPS_PORT_CD LIKE 'CA%'
        ) 
 ) FRIST_CA_PORT,
 (
    SELECT TO_CHAR(VPS_ETA_DT, 'YYYYMMDDHH24MI') FRIST_ETA_DT
    FROM VSK_VSL_PORT_SKD A
        WHERE A.VSL_CD 		= SUBSTR(@[vvd],1, 4)
        AND A.SKD_VOY_NO 	= SUBSTR(@[vvd],5, 4)
        AND A.SKD_DIR_CD 	= SUBSTR(@[vvd],9, 1)
         AND NVL(A.SKD_CNG_STS_CD, 'X') != 'S'
        AND A.CLPT_IND_SEQ = 1
        AND A.CLPT_SEQ = (
            SELECT MIN(CLPT_SEQ)
            FROM VSK_VSL_PORT_SKD A 
            WHERE A.VSL_CD 		= SUBSTR(@[vvd],1, 4)
            AND A.SKD_VOY_NO 	= SUBSTR(@[vvd],5, 4)
            AND A.SKD_DIR_CD 	= SUBSTR(@[vvd],9, 1)
            AND NVL(SKD_CNG_STS_CD, 'X') != 'S'
            AND VPS_PORT_CD LIKE 'CA%'
        ) 
 ) FRIST_CA_ETA_DT
FROM DUAL			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
