<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24CustomsTransmissionDBDAOModifyRcvLogBlMvmtRefNoOBUSQL">
			<desc><![CDATA[Eur24CustomsTransmissionDBDAOModifyRcvLogBlMvmtRefNoOBUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_EUR_IO_BL
SET MVMT_REF_NO = @[mvmt_ref_no],
    UPD_DT      = SYSDATE
WHERE  BND_TP_CD ='O' 
  AND (VSL_CD, SKD_VOY_NO, SKD_DIR_CD, BL_NO, CSTMS_PORT_CD) IN
     (SELECT VSL_CD, SKD_VOY_NO, SKD_DIR_CD, BL_NO, CSTMS_PORT_CD
        FROM   BKG_CSTMS_EUR_IO_SND
        WHERE BND_TP_CD = 'O' 
				  AND MSG_SND_NO = @[msg_rcv_no]
       )			]]></sql>
			<params>
				<param name="mvmt_ref_no" type="12" value="" out="N"/>
				<param name="msg_rcv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
