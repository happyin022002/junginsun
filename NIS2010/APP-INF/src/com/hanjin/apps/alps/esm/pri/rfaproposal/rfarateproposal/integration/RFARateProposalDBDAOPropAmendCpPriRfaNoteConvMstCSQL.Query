<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPropAmendCpPriRfaNoteConvMstCSQL">
			<desc><![CDATA[Basic RFA에서 Auto Amend시사용]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RFA_NOTE_CONV (
          NOTE_CONV_MAPG_ID
        , NOTE_CONV_SEQ
        , NOTE_CONV_TP_CD
        , SVC_SCP_CD
        , PROP_NO
        , AMDT_SEQ
        , CHG_RULE_TP_CD
        , NOTE_CONV_CHG_CD
        , NOTE_CONV_RULE_CD
        , EFF_DT
        , EXP_DT
        , RT_APPL_TP_CD
        , RT_OP_CD
        , CURR_CD
        , FRT_RT_AMT
        , PAY_TERM_CD
        , BKG_RAT_UT_CD
        , BKG_PRC_CGO_TP_CD
        , BKG_IMDG_CLSS_CD
        , BKG_CMDT_TP_CD
        , BKG_CMDT_DEF_CD
        , BKG_POR_TP_CD
        , BKG_POR_DEF_CD
        , BKG_POL_TP_CD
        , BKG_POL_DEF_CD
        , BKG_POD_TP_CD
        , BKG_POD_DEF_CD
        , BKG_DEL_TP_CD
        , BKG_DEL_DEF_CD
        , BKG_SLAN_CD
        , BKG_VSL_CD
        , BKG_SKD_VOY_NO
        , BKG_SKD_DIR_CD
        , BKG_MIN_CGO_WGT
        , BKG_MAX_CGO_WGT
        , BKG_SOC_FLG
        , BKG_TS_PORT_TP_CD
        , BKG_TS_PORT_DEF_CD
        , BKG_DIR_CALL_FLG
        , BKG_HNGR_BAR_TP_CD
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT
        , BKG_YD_CD
        , BKG_ESVC_TP_CD
        , PRC_PROG_STS_CD
        , SRC_INFO_CD
        , N1ST_CMNC_AMDT_SEQ
	, MST_RFA_ROUT_ID
)
WITH   NOTE_CONTS AS (
	SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ROUT_NOTE_SEQ, NOTE_CTNT, NOTE_CONV_MAPG_ID, PRC_PROG_STS_CD, SRC_INFO_CD, N1ST_CMNC_AMDT_SEQ, ACPT_USR_ID, ACPT_OFC_CD, ACPT_DT, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT 
	FROM  PRI_RP_SCP_RT_CMDT_RNOTE S
	WHERE  S.PROP_NO = @[new_prop_no]
		AND   S.AMDT_SEQ = @[new_amdt_seq]+1
		AND   S.SVC_SCP_CD = @[svc_scp_cd]
		AND   S.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
		AND   S.ROUT_SEQ = @[new_rout_seq]
		AND   S.SRC_INFO_CD <> 'AD'
)
 ,MST_NOTE_CONTS AS (
	SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ROUT_NOTE_SEQ, NOTE_CTNT, NOTE_CONV_MAPG_ID, PRC_PROG_STS_CD, SRC_INFO_CD, N1ST_CMNC_AMDT_SEQ, ACPT_USR_ID, ACPT_OFC_CD, ACPT_DT, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT 
	FROM  PRI_RP_SCP_RT_CMDT_RNOTE S
	WHERE  S.PROP_NO = @[prop_no]
		AND   S.AMDT_SEQ = @[amdt_seq]
		AND   S.SVC_SCP_CD = @[svc_scp_cd]
		AND   S.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
		AND   S.ROUT_SEQ = @[rout_seq]
		AND   S.SRC_INFO_CD <> 'AD'
)

 
 

SELECT (SELECT NOTE_CONV_MAPG_ID FROM NOTE_CONTS WHERE ROWNUM=1) AS NOTE_CONV_MAPG_ID
     , DENSE_RANK() OVER (PARTITION BY V.NOTE_CONV_MAPG_ID
                          ORDER BY V.NOTE_CONV_MAPG_ID, V.NOTE_CONV_SEQ) AS NOTE_CONV_SEQ
     , V.NOTE_CONV_TP_CD
     , N.SVC_SCP_CD
     ,  @[new_prop_no] AS PROP_NO
     , @[new_amdt_seq]+1  AS AMDT_SEQ
     , V.CHG_RULE_TP_CD
     , V.NOTE_CONV_CHG_CD
     , V.NOTE_CONV_RULE_CD
     , TO_DATE(NVL(@[eff_dt], '9999-12-31'), 'YYYY-MM-DD') AS EFF_DT
     , TO_DATE(NVL(@[exp_dt], '9999-12-31'), 'YYYY-MM-DD') AS EXP_DT
     , V.RT_APPL_TP_CD
     , V.RT_OP_CD
     , V.CURR_CD
     , V.FRT_RT_AMT
     , V.PAY_TERM_CD
     , V.BKG_RAT_UT_CD
     , V.BKG_PRC_CGO_TP_CD
     , V.BKG_IMDG_CLSS_CD
     , V.BKG_CMDT_TP_CD
     , V.BKG_CMDT_DEF_CD
     , V.BKG_POR_TP_CD
     , V.BKG_POR_DEF_CD
     , V.BKG_POL_TP_CD
     , V.BKG_POL_DEF_CD
     , V.BKG_POD_TP_CD
     , V.BKG_POD_DEF_CD
     , V.BKG_DEL_TP_CD
     , V.BKG_DEL_DEF_CD
     , V.BKG_SLAN_CD
     , V.BKG_VSL_CD
     , V.BKG_SKD_VOY_NO
     , V.BKG_SKD_DIR_CD
     , V.BKG_MIN_CGO_WGT
     , V.BKG_MAX_CGO_WGT
     , V.BKG_SOC_FLG
     , V.BKG_TS_PORT_TP_CD
     , V.BKG_TS_PORT_DEF_CD
     , V.BKG_DIR_CALL_FLG
     , V.BKG_HNGR_BAR_TP_CD
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
     , V.BKG_YD_CD
     , V.BKG_ESVC_TP_CD
     , 'I' AS PRC_PROG_STS_CD
     , 'NW' AS SRC_INFO_CD
     , @[new_amdt_seq]+1  AS N1ST_CMNC_AMDT_SEQ
      , CASE WHEN @[mst_rfa_no] IS NULL THEN V.MST_RFA_ROUT_ID
     	    ELSE @[mst_rfa_no] || '_' || LPAD(@[amdt_seq], 3, '0') || '_' || LPAD(@[rout_seq], 3, '0')
       END AS MST_RFA_ROUT_ID
FROM  PRI_RFA_NOTE_CONV V
    , MST_NOTE_CONTS N
WHERE  
	V.NOTE_CONV_MAPG_ID = N.NOTE_CONV_MAPG_ID
	 AND V.SRC_INFO_CD <> 'AD'
 




			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="new_amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="new_rout_seq" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mst_rfa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
