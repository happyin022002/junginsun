<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOSearchIhcTariffNoRSQL">
			<desc><![CDATA[IHC_TRF_NO 조회]]></desc>
			<sql><![CDATA[
SELECT @[svc_scp_cd] || DECODE(@[org_dest_tp_cd],'D','I','O','O') || @[cnt_cd]  || TO_CHAR(SYSDATE, 'YY')  
||  NVL(TO_CHAR(TO_NUMBER(SUBSTR(TRF_NO, 9, 2))+1,'FM09'), '01'  ) IHC_TRF_NO
FROM (
    select MAX( IHC_TRF_NO ) AS TRF_NO
    FROM PRI_TRF_IHC_HDR
    where SVC_SCP_CD = @[svc_scp_cd]  and ORG_DEST_TP_CD = @[org_dest_tp_cd]  and COST_CNT_CD = @[cnt_cd] 
)			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
