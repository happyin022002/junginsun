<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOModifyAutoUploadStatusBkg2USQL">
			<desc><![CDATA[BKG_BOOKING 의 SYS_UPLD_FLG 를 업데이트 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_BOOKING
   SET SYS_UPLD_FLG = @[sys_upld_flg]
     , UPD_USR_ID   = @[upd_usr_id]
     , UPD_DT = SYSDATE
 WHERE BKG_NO = (SELECT BKG_NO 
                   FROM BKG_XTER_RQST_MST
                  WHERE XTER_SNDR_ID  = @[xter_sndr_id]
                    AND xter_rqst_no  = @[xter_rqst_no]
                    AND xter_rqst_seq = @[xter_rqst_seq]
                    AND ROWNUM = 1)
   AND SYS_UPLD_FLG IS NULL			]]></sql>
			<params>
				<param name="sys_upld_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
