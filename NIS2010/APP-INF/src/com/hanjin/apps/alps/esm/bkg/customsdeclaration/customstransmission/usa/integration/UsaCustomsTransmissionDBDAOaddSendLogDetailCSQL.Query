<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOaddSendLogDetailCSQL">
			<desc><![CDATA[dwkim BkG_CSTMS_ADV_SND_LOG_DTL 입력]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_SND_LOG_DTL(
    CNT_CD, IO_BND_CD, SND_DT, HIS_SEQ, DTL_SEQ, 
	CRE_USR_ID, CRE_DT, UPD_USR_ID, 
	EDI_SND_LOG_CTNT
    )
VALUES(
	@[cnt_cd],
	@[io_bnd_cd],
	TO_DATE(@[snd_dt], 'ddmmrrhh24miss'),
	@[his_seq],
	@[dtl_seq],
	@[cre_usr_id],
	SYSDATE,
	@[upd_usr_id],
	@[edi_snd_log_ctnt]
)			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="his_seq" type="12" value="" out="N"/>
				<param name="dtl_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="edi_snd_log_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
