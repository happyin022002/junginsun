<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOAddTariffFeederMainDataCSQL">
			<desc><![CDATA[PRI_TRF_FDR_MN INSERT]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_FDR_MN
  (SVC_SCP_CD,
   ORG_DEST_TP_CD,
   FDR_TRF_NO,
   AMDT_SEQ,
   EFF_DT,
   EXP_DT,
   FIC_PROP_STS_CD,
   TRF_CURR_CD,
   LOCL_CURR_CD,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   CRE_OFC_CD)
values
  (@[svc_scp_cd],
   @[org_dest_tp_cd],
   @[fdr_trf_no],
   0,
   TO_DATE(TO_CHAR(SYSDATE, 'YYYYMMDD'), 'YYYYMMDD'),
   TO_DATE('99991231', 'YYYYMMDD'),
   'I',
   'USD',
   (SELECT B.CURR_CD
      FROM   #if(${rhq_cd} == 'HAMRU')    
                 AOC_EUR_FDR_TRF_HDR  B
             #end 
             #if(${rhq_cd} == 'SINRS')
                 AOC_CHN_FDR_TRF_HDR  B
             #end 
             #if(${rhq_cd} == 'SHARC')
                 AOC_CHN_FDR_TRF_HDR  B
             #end
     WHERE B.COST_TRF_NO = @[cost_trf_no]),
   @[cre_usr_id],
   SYSDATE,
   @[upd_usr_id],
   SYSDATE,
   @[ofc_cd]
)			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="cost_trf_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
