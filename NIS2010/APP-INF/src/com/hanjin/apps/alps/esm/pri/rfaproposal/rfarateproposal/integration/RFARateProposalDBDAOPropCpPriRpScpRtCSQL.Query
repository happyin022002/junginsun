<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPropCpPriRpScpRtCSQL">
			<desc><![CDATA[Proposal Scope Copy PRI_RP_SCP_RT Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_RT
  (PROP_NO,
   AMDT_SEQ,
   SVC_SCP_CD,
   CMDT_HDR_SEQ,
   ROUT_SEQ,
   RT_SEQ,
   RAT_UT_CD,
   PRC_CGO_TP_CD,
   CURR_CD,
   PROP_FRT_RT_AMT,
   BZC_OFRT_RT_AMT,
   ORG_ARB_AMT,
   RAIL_HUB_LOC_CD,
   DEST_ARB_AMT,
   DOR_TRKA_AMT,
   VSL_SLAN_CD,
   GRI_APPL_TP_CD,
   GRI_APPL_AMT,
   PRC_PROG_STS_CD,
   SRC_INFO_CD,
   N1ST_CMNC_AMDT_SEQ,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   FIC_PROP_RT_AMT,
   FIC_GLINE_RT_AMT,
   FIC_GLINE_UPD_DT,
   OPTM_TRSP_MOD_FLG,
   FIC_RT_USE_STS_CD,   
   FIC_DEST_RT_USE_STS_CD, 
   FIC_DEST_PROP_RT_AMT,   
   FIC_DEST_GLINE_RT_AMT,  
   FIC_DEST_GLINE_UPD_DT,  
   DEST_OPTM_TRSP_MOD_FLG, 
   FIC_ORG_RT_USE_STS_CD,  
   FIC_ORG_PROP_RT_AMT,    
   FIC_ORG_GLINE_RT_AMT,   
   FIC_ORG_GLINE_UPD_DT,   
   ORG_OPTM_TRSP_MOD_FLG
,  MST_RFA_ROUT_ID			--------- RFA 효율화를 위한 요청 (1차) (CHM-201640671) 
)
WITH CMDT_HDR AS (
    SELECT A.PROP_NO
         , A.AMDT_SEQ
         , A.SVC_SCP_CD
         , A.CMDT_HDR_SEQ AS OLD_CMDT_HDR_SEQ
         , DENSE_RANK() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD
                              ORDER BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.BLET_DP_SEQ, A.CMDT_HDR_SEQ) AS CMDT_HDR_SEQ
    FROM PRI_RP_SCP_RT_CMDT_HDR A
    WHERE A.PROP_NO = @[prop_no]
    AND   A.AMDT_SEQ = @[amdt_seq]
    AND   A.SVC_SCP_CD = @[svc_scp_cd]
    AND   (
        EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT_CMDT B
            WHERE B.PROP_NO = A.PROP_NO
            AND   B.AMDT_SEQ = A.AMDT_SEQ
            AND   B.SVC_SCP_CD = A.SVC_SCP_CD
            AND   B.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
            AND   B.SRC_INFO_CD <> 'AD'
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT_ACT_CUST C
            WHERE C.PROP_NO = A.PROP_NO
            AND   C.AMDT_SEQ = A.AMDT_SEQ
            AND   C.SVC_SCP_CD = A.SVC_SCP_CD
            AND   C.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
            AND   C.SRC_INFO_CD <> 'AD'
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT_CNOTE D
            WHERE D.PROP_NO = A.PROP_NO
            AND   D.AMDT_SEQ = A.AMDT_SEQ
            AND   D.SVC_SCP_CD = A.SVC_SCP_CD
            AND   D.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
            AND   D.SRC_INFO_CD <> 'AD'
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT_CMDT_ROUT E
            WHERE E.PROP_NO = A.PROP_NO
            AND   E.AMDT_SEQ = A.AMDT_SEQ
            AND   E.SVC_SCP_CD = A.SVC_SCP_CD
            AND   E.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ
            AND   (
                EXISTS (
                    SELECT 'X'
                    FROM PRI_RP_SCP_RT_ROUT_PNT F
                    WHERE F.PROP_NO = E.PROP_NO
                    AND   F.AMDT_SEQ = E.AMDT_SEQ
                    AND   F.SVC_SCP_CD = E.SVC_SCP_CD
                    AND   F.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
                    AND   F.ROUT_SEQ = E.ROUT_SEQ
                    AND   F.SRC_INFO_CD <> 'AD'
                )
                OR EXISTS (
                    SELECT 'X'
                    FROM PRI_RP_SCP_RT_ROUT_VIA G
                    WHERE G.PROP_NO = E.PROP_NO
                    AND   G.AMDT_SEQ = E.AMDT_SEQ
                    AND   G.SVC_SCP_CD = E.SVC_SCP_CD
                    AND   G.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
                    AND   G.ROUT_SEQ = E.ROUT_SEQ
                    AND   G.SRC_INFO_CD <> 'AD'
                )
                OR EXISTS (
                    SELECT 'X'
                    FROM PRI_RP_SCP_RT_CMDT_RNOTE I
                    WHERE I.PROP_NO = E.PROP_NO
                    AND   I.AMDT_SEQ = E.AMDT_SEQ
                    AND   I.SVC_SCP_CD = E.SVC_SCP_CD
                    AND   I.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
                    AND   I.ROUT_SEQ = E.ROUT_SEQ
                    AND   I.SRC_INFO_CD <> 'AD'
                )
                OR EXISTS (
                    SELECT 'X'
                    FROM PRI_RP_SCP_RT J
                    WHERE J.PROP_NO = E.PROP_NO
                    AND   J.AMDT_SEQ = E.AMDT_SEQ
                    AND   J.SVC_SCP_CD = E.SVC_SCP_CD
                    AND   J.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
                    AND   J.ROUT_SEQ = E.ROUT_SEQ
                    AND   J.SRC_INFO_CD <> 'AD'
                )
            )
        )
    )
)
, CMDT_ROUT AS (
    SELECT L.PROP_NO
         , L.AMDT_SEQ
         , L.SVC_SCP_CD
         , K.OLD_CMDT_HDR_SEQ
         , K.CMDT_HDR_SEQ
         , L.ROUT_SEQ AS OLD_ROUT_SEQ
         , DENSE_RANK() OVER (PARTITION BY L.PROP_NO, L.AMDT_SEQ, L.SVC_SCP_CD, K.OLD_CMDT_HDR_SEQ ORDER BY L.PROP_NO, L.AMDT_SEQ, L.SVC_SCP_CD, K.OLD_CMDT_HDR_SEQ, L.ROUT_SEQ) AS ROUT_SEQ
         , MST_ROUT_ID
    FROM CMDT_HDR K
        ,PRI_RP_SCP_RT_CMDT_ROUT L
    WHERE L.PROP_NO = K.PROP_NO
    AND   L.AMDT_SEQ = K.AMDT_SEQ
    AND   L.SVC_SCP_CD = K.SVC_SCP_CD
    AND   L.CMDT_HDR_SEQ = K.OLD_CMDT_HDR_SEQ
#if (${IS_MASTER_COPY} == 'Y') 
---- RFA 효율화를 위한 요청 (1차) (CHM-201640671)
	AND   L.ROUT_SEQ IN (
		#foreach( $rout_seq in ${rout_seq_list}) 
			#if($velocityCount < $rout_seq_list.size()) 
			   '$rout_seq', 
			#else 
			   '$rout_seq' 
			#end 
		#end
		)
#end
    AND   (
        EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT_ROUT_PNT F
            WHERE F.PROP_NO = L.PROP_NO
            AND   F.AMDT_SEQ = L.AMDT_SEQ
            AND   F.SVC_SCP_CD = L.SVC_SCP_CD
            AND   F.CMDT_HDR_SEQ = L.CMDT_HDR_SEQ
            AND   F.ROUT_SEQ = L.ROUT_SEQ
            AND   F.SRC_INFO_CD <> 'AD'
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT_ROUT_VIA G
            WHERE G.PROP_NO = L.PROP_NO
            AND   G.AMDT_SEQ = L.AMDT_SEQ
            AND   G.SVC_SCP_CD = L.SVC_SCP_CD
            AND   G.CMDT_HDR_SEQ = L.CMDT_HDR_SEQ
            AND   G.ROUT_SEQ = L.ROUT_SEQ
            AND   G.SRC_INFO_CD <> 'AD'
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT_CMDT_RNOTE I
            WHERE I.PROP_NO = L.PROP_NO
            AND   I.AMDT_SEQ = L.AMDT_SEQ
            AND   I.SVC_SCP_CD = L.SVC_SCP_CD
            AND   I.CMDT_HDR_SEQ = L.CMDT_HDR_SEQ
            AND   I.ROUT_SEQ = L.ROUT_SEQ
            AND   I.SRC_INFO_CD <> 'AD'
        )
        OR EXISTS (
            SELECT 'X'
            FROM PRI_RP_SCP_RT J
            WHERE J.PROP_NO = L.PROP_NO
            AND   J.AMDT_SEQ = L.AMDT_SEQ
            AND   J.SVC_SCP_CD = L.SVC_SCP_CD
            AND   J.CMDT_HDR_SEQ = L.CMDT_HDR_SEQ
            AND   J.ROUT_SEQ = L.ROUT_SEQ
            AND   J.SRC_INFO_CD <> 'AD'
        )
    )
)
SELECT @[new_prop_no] AS PROP_NO,
       0 AS AMDT_SEQ,
       S.SVC_SCP_CD,
       R.CMDT_HDR_SEQ,
       R.ROUT_SEQ,
       DENSE_RANK() OVER(PARTITION BY S.PROP_NO, S.AMDT_SEQ, S.SVC_SCP_CD, R.CMDT_HDR_SEQ, R.ROUT_SEQ ORDER BY S.PROP_NO, S.AMDT_SEQ, S.SVC_SCP_CD, R.CMDT_HDR_SEQ, R.ROUT_SEQ, S.RT_SEQ) AS RT_SEQ,
       S.RAT_UT_CD,
       S.PRC_CGO_TP_CD,
       S.CURR_CD,
       DECODE((SELECT PROP_STS_CD
                FROM PRI_RP_MN
               WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]),
              'A',
              S.FNL_FRT_RT_AMT,
              S.PROP_FRT_RT_AMT) AS PROP_FRT_RT_AMT,
       S.BZC_OFRT_RT_AMT,
       S.ORG_ARB_AMT,
       S.RAIL_HUB_LOC_CD,
       S.DEST_ARB_AMT,
       S.DOR_TRKA_AMT,
       S.VSL_SLAN_CD,
       'N' AS GRI_APPL_TP_CD,
       0 AS GRI_APPL_AMT,
       'I' AS PRC_PROG_STS_CD,
#if (${IS_MASTER_COPY} == 'Y') 
       'NW' AS SRC_INFO_CD,
#else 
       'PC' AS SRC_INFO_CD,
#end
       0 AS N1ST_CMNC_AMDT_SEQ,
       @[cre_usr_id] AS CRE_USR_ID,
       SYSDATE AS CRE_DT,
       @[upd_usr_id] AS UPD_USR_ID,
       SYSDATE AS UPD_DT,
       DECODE((SELECT PROP_STS_CD
                FROM PRI_RP_MN
               WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]),
              'A',
              S.FIC_FNL_RT_AMT,
              S.FIC_PROP_RT_AMT) AS FIC_PROP_RT_AMT,
       S.FIC_GLINE_RT_AMT,
       S.FIC_GLINE_UPD_DT,
       S.OPTM_TRSP_MOD_FLG,
       S.FIC_RT_USE_STS_CD,       
       S.FIC_DEST_RT_USE_STS_CD, 
       DECODE((SELECT PROP_STS_CD
                FROM PRI_RP_MN
               WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]),
              'A',
              S.FIC_DEST_FNL_RT_AMT,
              S.FIC_DEST_PROP_RT_AMT) AS FIC_DEST_PROP_RT_AMT, 
       S.FIC_DEST_GLINE_RT_AMT,  
       S.FIC_DEST_GLINE_UPD_DT,  
       S.DEST_OPTM_TRSP_MOD_FLG, 
       S.FIC_ORG_RT_USE_STS_CD,  
       DECODE((SELECT PROP_STS_CD
                FROM PRI_RP_MN
               WHERE PROP_NO = @[prop_no]
                     AND AMDT_SEQ = @[amdt_seq]),
              'A',
              S.FIC_ORG_FNL_RT_AMT,
              S.FIC_ORG_PROP_RT_AMT) AS FIC_ORG_PROP_RT_AMT,  
       S.FIC_ORG_GLINE_RT_AMT,   
       S.FIC_ORG_GLINE_UPD_DT,   
       S.ORG_OPTM_TRSP_MOD_FLG       
	  ,CASE WHEN @[mst_rfa_no] IS NULL THEN S.MST_RFA_ROUT_ID
            ELSE @[mst_rfa_no] ||'_'|| LPAD(@[amdt_seq], 3, '0') ||'_'|| LPAD(MST_ROUT_ID, 3, '0')
       END AS MST_RFA_ROUT_ID         ---------------- RFA 효율화를 위한 요청 (1차) (CHM-201640671) 
  FROM CMDT_ROUT     R,
       PRI_RP_SCP_RT S
 WHERE S.PROP_NO = R.PROP_NO
       AND S.AMDT_SEQ = R.AMDT_SEQ
       AND S.SVC_SCP_CD = R.SVC_SCP_CD
       AND S.CMDT_HDR_SEQ = R.OLD_CMDT_HDR_SEQ
       AND S.ROUT_SEQ = R.OLD_ROUT_SEQ
       AND S.SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mst_rfa_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
