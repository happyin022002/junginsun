<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VIPDeductAgreementDBDAOSearchSubTradeListRSQL">
			<desc><![CDATA[Sub trade code list 조회.]]></desc>
			<sql><![CDATA[
SELECT SUB_TRD_CD
FROM MDM_SUB_TRD
WHERE DELT_FLG <> 'Y'
  AND TRD_CD = @[trd_cd]
ORDER BY SUB_TRD_CD ASC
			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
