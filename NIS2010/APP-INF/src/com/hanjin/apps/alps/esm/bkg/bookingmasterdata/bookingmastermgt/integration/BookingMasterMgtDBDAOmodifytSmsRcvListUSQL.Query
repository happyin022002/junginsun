<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOmodifytSmsRcvListUSQL">
			<desc><![CDATA[BookingMasterMgtDBDAOmodifytSmsRcvListUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_CLL_CNG_NTFY_SET
SET RCVR_USR_ID = @[rcvr_usr_id], 
    RCVR_USR_NM = @[rcvr_usr_nm], 
    RCVR_EML    = @[rcvr_eml], 
    OFC_PHN_NO  = @[ofc_phn_no],
    OFC_FAX_NO  = @[ofc_fax_no],
    PHN_NO      = @[phn_no],
    UPD_USR_ID  = @[upd_usr_id],
    UPD_DT      = SYSDATE
    ,FILE_ATCH_FLG = decode(@[file_atch_flg],'1','Y','N')
WHERE SLAN_CD   = @[slan_cd]
  AND DIR_CD    = @[dir_cd]
  AND CRE_SEQ   = @[cre_seq]
  AND LOCL_TS_IND_CD = @[locl_ts_ind_cd]
  AND PORT_CD = @[port_cd]			]]></sql>
			<params>
				<param name="rcvr_usr_id" type="12" value="" out="N"/>
				<param name="rcvr_usr_nm" type="12" value="" out="N"/>
				<param name="rcvr_eml" type="12" value="" out="N"/>
				<param name="ofc_phn_no" type="12" value="" out="N"/>
				<param name="ofc_fax_no" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="file_atch_flg" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="cre_seq" type="12" value="" out="N"/>
				<param name="locl_ts_ind_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
