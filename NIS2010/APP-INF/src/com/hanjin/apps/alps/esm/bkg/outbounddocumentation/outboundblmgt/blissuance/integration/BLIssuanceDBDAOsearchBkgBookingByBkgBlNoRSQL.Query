<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchBkgBookingByBkgBlNoRSQL">
			<desc><![CDATA[searchBkgBookingByBkgBlNo]]></desc>
			<sql><![CDATA[
SELECT *
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_bl_no]
    OR BL_NO = @[bkg_bl_no]			]]></sql>
			<params>
				<param name="bkg_bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
