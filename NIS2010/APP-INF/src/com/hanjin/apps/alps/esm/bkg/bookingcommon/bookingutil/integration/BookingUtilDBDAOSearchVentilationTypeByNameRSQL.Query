<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchVentilationTypeByNameRSQL">
			<desc><![CDATA[Container Ventilation Type Code 조회]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_CTNT AS CLNG_TP_CD
FROM COM_INTG_CD_DTL
WHERE 1=1
AND INTG_CD_ID IN ('CD01592')
AND UPPER(INTG_CD_VAL_DP_DESC) = @[vent_tp_nm]
AND ROWNUM =1			]]></sql>
			<params>
				<param name="vent_tp_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
