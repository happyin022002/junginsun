<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MalaysiaCustomsTransmissionDBDAOSearchMarkInfoRSQL">
			<desc><![CDATA[2012.02.02 변종건 [CHM-201215473-01] Malaysis (MYTPP) Customs Manifest 전송 기능 추가 요청]]></desc>
			<sql><![CDATA[
SELECT    BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 1, 35),'Y') MARK1
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 36, 35),'Y') MARK2
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 71, 35),'Y') MARK3
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 106, 35),'Y') MARK4
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 141, 35),'Y') MARK5
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 176, 35),'Y') MARK6
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 211, 35),'Y') MARK7
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 246, 35),'Y') MARK8
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 281, 35),'Y') MARK9
         ,BKG_SPCLCHAR_CONV_FNC(SUBSTR(MD.MK_DESC, 316, 35),'Y') MARK10
FROM      BKG_BL_MK_DESC MD
WHERE     1 = 1
AND       MD.BKG_NO = @[bkg_no]
AND       MD.MK_DESC IS NOT NULL 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
