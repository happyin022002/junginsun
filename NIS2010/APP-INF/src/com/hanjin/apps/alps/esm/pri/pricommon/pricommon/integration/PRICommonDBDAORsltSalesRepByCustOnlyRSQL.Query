<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltSalesRepByCustOnlyRSQL">
			<desc><![CDATA[Customer 별 Sales Rep. Office Code는 조건에서 제외함]]></desc>
			<sql><![CDATA[
SELECT A.SREP_CD AS CD
     , A.SREP_NM AS NM
     , A.OFC_CD AS ETC1
FROM MDM_SLS_REP A
   , BKG_CUST_SLS_REP B
WHERE A.SREP_CD = B.SREP_CD
AND   B.CUST_CNT_CD = @[etc2]
AND   B.CUST_SEQ = @[etc3]
AND   B.DELT_FLG = 'N'
AND   A.SREP_STS_CD = 'N'
ORDER BY A.SREP_CD			]]></sql>
			<params>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="etc3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
