<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOCheckEuBlackCustomerRSQL">
			<desc><![CDATA[CheckEuBlackCustomer]]></desc>
			<sql><![CDATA[
SELECT ATTR_CTNT2
FROM BKG_HRD_CDG_CTNT
WHERE HRD_CDG_ID='EU_BLACK_LIST'
AND (   (REPLACE(UPPER(TRIM(@[cust_nm])),' ','') = ATTR_CTNT3 AND ATTR_CTNT4 = 'EQUAL')
     OR (REPLACE(UPPER(TRIM(@[cust_nm])),' ','') LIKE ATTR_CTNT3 || '%' AND ATTR_CTNT4 = 'POST'))
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cust_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
