<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BRKGAuditDBDAOSearchAPActualInterfaceMasterDetailCmpnDownExcelRSQL">
			<desc><![CDATA[SearchAPActualInterfaceMasterDetailDownExcel]]></desc>
			<sql><![CDATA[
SELECT 
		   A.FRT_FWRD_CNT_CD||TO_CHAR(A.FRT_FWRD_SEQ,'FM000000') AS FWDR,
           TO_CHAR(A.VNDR_SEQ,'FM000000') AS VNDR,
           B.CUST_LGL_ENG_NM AS FWDR_NAME,
           A.CSR_NO,
           '' BKG_NO,
           '' BL_NO,
           COUNT(*) AS TOT_CNT,
		   ROUND(SUM(A.ACT_IF_COMM_AMT), 2) AS NET_AMT,
		   ROUND(SUM(A.ACT_IF_COMM_AMT * NVL(A.INV_TAX_RT, 0) / 100), 2) AS VAT,
		   ROUND(SUM(A.ACT_IF_COMM_AMT + (A.ACT_IF_COMM_AMT * NVL(A.INV_TAX_RT, 0) / 100)), 2) AS TOT_AMT,
           A.VNDR_SEQ,
           A.AP_OFC_CD,
           TO_CHAR(A.CMPN_IF_DT,'YYYYMMDD') AS IF_DATE,
           DECODE(D.IF_FLG,'Y','Success',D.IF_ERR_RSN) AS IF_RSN,
           DECODE(D.RCV_ERR_FLG,'Y','Success',D.RCV_ERR_RSN) AS RCV_RSN,
           D.IF_FLG AS IF_FLG,
           D.RCV_ERR_FLG AS RCV_FLG,
           D.PAY_AMT,
           D.PAY_DT,
           D.FTU_USE_CTNT1,
           D.PAY_MZD_LU_CD
      FROM AGT_CMPN_COMM A,
           MDM_CUSTOMER B,
           AGT_COMM_BKG_INFO C,
           AP_INV_HDR D
     WHERE A.FRT_FWRD_CNT_CD = B.CUST_CNT_CD
       AND A.FRT_FWRD_SEQ = B.CUST_SEQ
       AND A.BKG_NO = C.BKG_NO
       AND A.CRE_USR_ID != 'COST'
       AND A.AP_OFC_CD IS NOT NULL
       AND C.BL_NO IS NOT NULL
       AND A.CSR_NO = D.CSR_NO(+)
#if(${if_option} == 'BF')
       AND A.COMM_PROC_STS_CD
        IN
         ( 'AS'
         )
#elseif(${if_option} == 'NC')
       AND A.COMM_PROC_STS_CD
        IN
         ( 'CE','IC','CS','CM','CA'
         )
#elseif(${if_option} == 'IF')
       AND A.COMM_PROC_STS_CD
        IN
         ( 'IF'
         )
#end
#if(${if_option} == 'IF')
       AND A.CMPN_IF_DT
   BETWEEN TO_DATE(REPLACE(@[search_dt_fr],'-', ''),'YYYYMMDD')
       AND TO_DATE(REPLACE(@[search_dt_to],'-', ''),'YYYYMMDD')+0.999999
#else
 #if(${sts_option} == '1')
       AND A.VSL_DEP_DT
   BETWEEN TO_DATE(REPLACE(@[search_dt_fr],'-', ''),'YYYYMMDD')
       AND TO_DATE(REPLACE(@[search_dt_to],'-', ''),'YYYYMMDD')+0.999999
 #else
       AND A.CRE_DT
   BETWEEN TO_DATE(REPLACE(@[search_dt_fr],'-', ''),'YYYYMMDD')
       AND TO_DATE(REPLACE(@[search_dt_to],'-', ''),'YYYYMMDD')+0.999999
 #end
#end
#if(${ff_cnt_cd} != '')
       AND A.FRT_FWRD_CNT_CD = SUBSTR(@[ff_cnt_cd], 0, 2)
       AND A.FRT_FWRD_SEQ    = SUBSTR(@[ff_cnt_cd], 3, 6)
#end
#if(${bl_nos} != '')
 #if(${if_option} == 'IF')
       AND A.CSR_NO IN
         ( SELECT
                  F.CSR_NO
             FROM AGT_CMPN_COMM F
            WHERE F.BKG_NO
               IN
                (     SELECT
                             E.BKG_NO        
                        FROM AGT_COMM_BKG_INFO E
                       WHERE E.BL_NO
                          IN
                           ( @[bl_nos]
                           )
                )
         )
 #else
       AND
         ( A.FRT_FWRD_CNT_CD, A.FRT_FWRD_SEQ
         )
        IN
         (     SELECT
                      F.FRT_FWRD_CNT_CD,
                      F.FRT_FWRD_SEQ
                 FROM AGT_CMPN_COMM F
                WHERE F.BKG_NO
                   IN
                    (     SELECT
                                 E.BKG_NO
                            FROM AGT_COMM_BKG_INFO E
                           WHERE E.BL_NO
                              IN
                               ( @[bl_nos]
                               )
                    )
         )
 #end
#end
  GROUP BY 
		   A.FRT_FWRD_CNT_CD||TO_CHAR(A.FRT_FWRD_SEQ,'FM000000'),
           TO_CHAR(A.VNDR_SEQ,'FM000000'), 
           B.CUST_LGL_ENG_NM, A.CSR_NO, A.VNDR_SEQ, A.AP_OFC_CD, 
           TO_CHAR(A.CMPN_IF_DT,'YYYYMMDD'),
           DECODE(D.IF_FLG,'Y','Success',D.IF_ERR_RSN),
           DECODE(D.RCV_ERR_FLG,'Y','Success',D.RCV_ERR_RSN),
           D.IF_FLG, D.RCV_ERR_FLG,
           D.PAY_AMT, D.PAY_DT, D.FTU_USE_CTNT1, D.PAY_MZD_LU_CD
 UNION ALL
    SELECT 
		   A.FRT_FWRD_CNT_CD||TO_CHAR(A.FRT_FWRD_SEQ,'FM000000') AS FWDR,
           TO_CHAR(A.VNDR_SEQ,'FM000000') AS VNDR,
           B.CUST_LGL_ENG_NM AS FWDR_NAME,
           A.CSR_NO AS CSR_NO,
           A.BKG_NO,
           C.BL_NO,
           COUNT(*) AS TOT_CNT,
		   ROUND(SUM(A.ACT_IF_COMM_AMT), 2) AS NET_AMT,
		   ROUND(SUM(A.ACT_IF_COMM_AMT * NVL(A.INV_TAX_RT, 0) / 100), 2) AS VAT,
		   ROUND(SUM(A.ACT_IF_COMM_AMT + (A.ACT_IF_COMM_AMT * NVL(A.INV_TAX_RT, 0) / 100)), 2) AS TOT_AMT,
           NULL AS VNDR_SEQ,
           '' AS AP_OFC_CD,
           NULL AS IF_DATE,
           '' AS IF_RSN,
           '' AS RCV_RSN,
           '' AS IF_FLG,
           '' AS RCV_FLG,
           NULL AS PAY_AMT,
           NULL AS PAY_DT,
           '' AS FTU_USE_CTNT1,
           '' AS PAY_MZD_LU_CD
      FROM AGT_CMPN_COMM A, 
           MDM_CUSTOMER B,
           AGT_COMM_BKG_INFO C,
           AP_INV_HDR D
     WHERE A.FRT_FWRD_CNT_CD = B.CUST_CNT_CD
       AND A.FRT_FWRD_SEQ = B.CUST_SEQ
       AND A.BKG_NO = C.BKG_NO
       AND A.CRE_USR_ID != 'COST'
       AND A.AP_OFC_CD IS NOT NULL
       AND C.BL_NO IS NOT NULL
       AND A.CSR_NO = D.CSR_NO(+)
#if(${if_option} == 'BF')
       AND A.COMM_PROC_STS_CD
        IN
         ( 'AS'
         )
#elseif(${if_option} == 'NC')
       AND A.COMM_PROC_STS_CD
        IN
         ( 'CE','IC','CS','CM','CA'
         )
#elseif(${if_option} == 'IF')
       AND A.COMM_PROC_STS_CD
        IN
         ( 'IF'
         )
#end
#if(${if_option} == 'IF')
       AND A.CMPN_IF_DT
   BETWEEN TO_DATE(REPLACE(@[search_dt_fr],'-', ''),'YYYYMMDD')
       AND TO_DATE(REPLACE(@[search_dt_to],'-', ''),'YYYYMMDD')+0.999999
#else
 #if(${sts_option} == '1')
       AND A.VSL_DEP_DT
   BETWEEN TO_DATE(REPLACE(@[search_dt_fr],'-', ''),'YYYYMMDD')
       AND TO_DATE(REPLACE(@[search_dt_to],'-', ''),'YYYYMMDD')+0.999999
 #else
       AND A.CRE_DT
   BETWEEN TO_DATE(REPLACE(@[search_dt_fr],'-', ''),'YYYYMMDD')
       AND TO_DATE(REPLACE(@[search_dt_to],'-', ''),'YYYYMMDD')+0.999999
 #end
#end
#if(${ff_cnt_cd} != '')
       AND A.FRT_FWRD_CNT_CD = SUBSTR(@[ff_cnt_cd], 0, 2)
       AND A.FRT_FWRD_SEQ    = SUBSTR(@[ff_cnt_cd], 3, 6)
#end
#if(${bl_nos} != '')
 #if(${if_option} == 'IF')
       AND A.CSR_NO IN
         ( SELECT
                  F.CSR_NO
             FROM AGT_CMPN_COMM F
            WHERE F.BKG_NO
               IN
                (     SELECT
                             E.BKG_NO        
                        FROM AGT_COMM_BKG_INFO E
                       WHERE E.BL_NO
                          IN
                           ( @[bl_nos]
                           )
                )
         )
 #else
       AND
         ( A.FRT_FWRD_CNT_CD, A.FRT_FWRD_SEQ
         )
        IN
         (     SELECT
                      F.FRT_FWRD_CNT_CD,
                      F.FRT_FWRD_SEQ
                 FROM AGT_CMPN_COMM F
                WHERE F.BKG_NO
                   IN
                    (     SELECT
                                 E.BKG_NO
                            FROM AGT_COMM_BKG_INFO E
                           WHERE E.BL_NO
                              IN
                               ( @[bl_nos]
                               )
                    )
         )
 #end
#end
  GROUP BY 
		   A.FRT_FWRD_CNT_CD||TO_CHAR(A.FRT_FWRD_SEQ,'FM000000'),
           TO_CHAR(A.VNDR_SEQ,'FM000000'),
           B.CUST_LGL_ENG_NM, A.CSR_NO, A.BKG_NO, C.BL_NO
  ORDER BY 1,4,5 DESC,7 DESC			]]></sql>
			<params>
				<param name="search_dt_fr" type="12" value="" out="Y"/>
				<param name="search_dt_to" type="12" value="" out="Y"/>
				<param name="ff_cnt_cd" type="12" value="" out="Y"/>
				<param name="bl_nos" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
