<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAONetworkCostExceptionListCSQL">
			<desc><![CDATA[Network Cost Exception List 를 저장한다.(Insert)]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_NTWK_EXPT_LIST A
     USING
          (
	      SELECT @[stnd_cost_cd]   AS STND_COST_CD,
	             @[vsl_cd]         AS VSL_CD,
	             @[eff_fm_yrwk]    AS EFF_FM_YRWK,
	             @[eff_to_yrwk]    AS EFF_TO_YRWK,	             
           	     @[upd_usr_id]     AS CRE_USR_ID
                 
	        FROM DUAL
          ) T
        ON (
            T.STND_COST_CD  = A.STND_COST_CD   AND             
            T.VSL_CD        = A.VSL_CD         AND 
            T.EFF_FM_YRWK   = A.EFF_FM_YRWK    AND 
            T.EFF_TO_YRWK   = A.EFF_TO_YRWK
           )
      WHEN NOT MATCHED THEN
           		INSERT (
                         A.STND_COST_CD
                       , A.VSL_CD
                       , A.EFF_FM_YRWK
                       , A.EFF_TO_YRWK
                       , A.DELT_FLG
                       , A.CRE_USR_ID
                       , A.CRE_DT
                       , A.UPD_USR_ID
                       , A.UPD_DT 
                       ) 
                VALUES (
                         T.STND_COST_CD
                       , T.VSL_CD
                       , T.EFF_FM_YRWK
                       , T.EFF_TO_YRWK
                       , 'N'
                       , T.CRE_USR_ID
                       , SYSDATE
                       , T.CRE_USR_ID
                       , SYSDATE
                   	   )			]]></sql>
			<params>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="eff_fm_yrwk" type="12" value="" out="N"/>
				<param name="eff_to_yrwk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
