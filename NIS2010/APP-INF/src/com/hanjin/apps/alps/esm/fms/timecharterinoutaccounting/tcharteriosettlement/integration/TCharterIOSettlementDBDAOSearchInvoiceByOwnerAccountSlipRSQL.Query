<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOSettlementDBDAOSearchInvoiceByOwnerAccountSlipRSQL">
			<desc><![CDATA[Search Invoice by Owner Account Slip]]></desc>
			<sql><![CDATA[
SELECT 	(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL CD WHERE INTG_CD_ID = 'CD01513' AND INTG_CD_VAL_CTNT = FLET_CTRT_TP_CD) FLET_CTRT_TP_CD
FROM 	FMS_CONTRACT FC
WHERE 	FLET_CTRT_NO = @[flet_ctrt_no]			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
