<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOaddAEDoVTYDtHisCSQL">
			<desc><![CDATA[아랍 국가에 한해서 DO validity date를 수정 가능한데 이를 History로 관리하기 위함]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_AE_VTY_DT_HIS
 (BKG_NO          ,
  HIS_SEQ         ,
  DO_VTY_DT       ,
  CRE_USR_ID      ,
  CRE_DT          ,
  UPD_USR_ID      ,
  UPD_DT)
(SELECT   
  @[bkg_no],
  NVL(MAX(HIS_SEQ),0) + 1,
  TO_DATE(REPLACE(@[do_vty_dt],'-',''),'YYYYMMDD'),
  @[cre_usr_id],
  SYSDATE,
  @[upd_usr_id],
  SYSDATE
 FROM BKG_AE_VTY_DT_HIS
 WHERE BKG_NO = @[bkg_no] ) 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="do_vty_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
