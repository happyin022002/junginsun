<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanCustomsTransmissionDBDAOsearchJapanPODInfoRSQL">
			<desc><![CDATA[searchBlMarkDesc]]></desc>
			<sql><![CDATA[
SELECT 
DECODE(L1.ATTR_CTNT2, NULL, DECODE(SUBSTR(L2.LOC_CD,1,2),'AW','AN','CS','CB','EU','ZY','KZ','KA','LI','ZY','RU','RS','TJ','TA','UA','UK','UZ','UB','YU','YE', SUBSTR(L2.LOC_CD,1,2))||'ZZZ',L1.ATTR_CTNT2) POD
FROM BKG_CSTMS_CD_CONV_CTNT L1, MDM_LOCATION L2
WHERE L1.CNT_CD(+) = 'JP'
AND   L1.CSTMS_DIV_ID(+) = 'JAPAN_LOC'
and   L2.loc_cd = @[in_pod_cd]
and   L2.loc_cd = L1.ATTR_CTNT1(+)			]]></sql>
			<params>
				<param name="in_pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
