<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOSearchAgrdNtwkCostRtoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
select 
    RTO.COST_YRMON
    , RTO.COST_YRMON_SEQ
    , RTO.GRP_SEQ
    , RTO.TRD_CD
    , RTO.RLANE_CD
    , RTO.IOC_CD
    , RTO.DIR_CD
    , RTO.BZC_ALOC_TP_CD
    , RTO.BZC_ALOC_RTO
    , RTO.BZC_ALOC_FX_AMT
    , RTO.OVR_USD_ALOC_CHG_FLG
    , RTO.OVR_USD_ALOC_CHG_RTO
    , RTO.LOCL_TS_STS_CD
	, RGST.HUL_BND_CD  
from MAS_AGRD_NTWK_COST_RTO RTO, MAS_LANE_RGST RGST
where 
    RTO.RLANE_CD = RGST.RLANE_CD 
    AND RTO.DIR_CD = RGST.DIR_CD
    AND RTO.TRD_CD = RGST.TRD_CD
    AND RTO.IOC_CD = RGST.IOC_CD
	AND RGST.DELT_FLG = 'N'
	AND RTO.cost_yrmon = replace(@[f_cost_yrmon] ,'-','')
#if (${f_locl_ts_sts_cd} != '')
	and RTO.LOCL_TS_STS_CD = @[f_locl_ts_sts_cd]
#end
order by GRP_SEQ,LOCL_TS_STS_CD			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
				<param name="f_locl_ts_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
