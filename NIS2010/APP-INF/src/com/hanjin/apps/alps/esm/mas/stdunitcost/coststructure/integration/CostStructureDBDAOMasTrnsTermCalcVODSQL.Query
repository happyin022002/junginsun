<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOMasTrnsTermCalcVODSQL">
			<desc><![CDATA[Feeder Term Ratio 삭제   ]]></desc>
			<sql><![CDATA[
DELETE FROM MAS_TRNS_TERM_CALC
 WHERE COST_ACT_GRP_CD  = @[cost_act_grp_cd]
   AND CALC_TERM_CD     = @[calc_term_cd]
   AND WTR_TERM_CD      = @[wtr_term_cd]
   AND WTR_MOD_FLG      = @[wtr_mod_flg]			]]></sql>
			<params>
				<param name="cost_act_grp_cd" type="12" value="" out="N"/>
				<param name="calc_term_cd" type="12" value="" out="N"/>
				<param name="wtr_term_cd" type="12" value="" out="N"/>
				<param name="wtr_mod_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
