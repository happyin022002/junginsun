<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOUpdateAllocForStausUSQL">
			<desc><![CDATA[확정 쿼타에 반영된 오피스들에 대해서 [Allocation = QTA Setting]의 상태를  [업데이트] 합니다.]]></desc>
			<sql><![CDATA[
MERGE INTO SQM_ALOC_QTA X
USING (
        SELECT BSE_YR
              ,TRD_CD
              ,RLANE_CD
              ,DIR_CD
              ,VSL_CD
              ,SKD_VOY_NO
              ,SKD_DIR_CD
              ,RGN_OFC_CD 
          FROM SQM_CFM_QTA
         WHERE 1=1
           AND BSE_TP_CD     = 'Q'
           AND BSE_YR        = @[bse_yr]
           AND OFC_VW_CD     = 'L'
           AND SQM_CNG_TP_CD = 'A'
           AND VSL_CD        = @[vsl_cd]
           AND SKD_VOY_NO    = @[skd_voy_no]
           AND SKD_DIR_CD    = @[skd_dir_cd]
       ) Y
ON  (
         X.BSE_YR     = Y.BSE_YR     
     AND X.TRD_CD     = Y.TRD_CD     
     AND X.RLANE_CD   = Y.RLANE_CD   
     AND X.DIR_CD     = Y.DIR_CD     
     AND X.VSL_CD     = Y.VSL_CD
     AND X.SKD_VOY_NO = Y.SKD_VOY_NO 
     AND X.SKD_DIR_CD = Y.SKD_DIR_CD     
     AND X.RGN_OFC_CD = Y.RGN_OFC_CD 
    )
WHEN MATCHED THEN
     UPDATE SET APLY_FLG   = 'Y'
               ,UPD_USR_ID = @[upd_usr_id]
               ,UPD_DT     = SYSDATE			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
