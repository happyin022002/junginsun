<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAODemDetCostRepbyBKGUSQL">
			<desc><![CDATA[DEM/DET Cost Report by BKG 를 Save(저장 중 - update) 한다.]]></desc>
			<sql><![CDATA[
UPDATE MAS_CHSS_PDM_UPLD
   SET POOL_CD     = @[pool_cd]
	  ,SCC_CD      = @[scc_cd]
      ,BZC_RT      = @[bzc_rt]
	  ,TAX_PCT     = @[tax_pct]       
      ,RT_AMT      = @[rt_amt]
      ,COST_YRMON  = @[cost_yrmon]      
      ,UPD_USR_ID  = @[upd_usr_id]
      ,UPD_DT      = SYSDATE 
 WHERE POOL_CD     = @[pool_cd_org]
   AND SCC_CD      = @[scc_cd_org]
   AND COST_YRMON  = @[cost_yrmon_org] 			]]></sql>
			<params>
				<param name="pool_cd" type="12" value="" out="N"/>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="bzc_rt" type="12" value="" out="N"/>
				<param name="tax_pct" type="12" value="" out="N"/>
				<param name="rt_amt" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pool_cd_org" type="12" value="" out="N"/>
				<param name="scc_cd_org" type="12" value="" out="N"/>
				<param name="cost_yrmon_org" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
