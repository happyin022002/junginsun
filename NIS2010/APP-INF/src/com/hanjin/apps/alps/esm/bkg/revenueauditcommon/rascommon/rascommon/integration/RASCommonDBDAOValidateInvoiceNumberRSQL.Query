<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RASCommonDBDAOValidateInvoiceNumberRSQL">
			<desc><![CDATA[Invoice Number 존재여부 확인]]></desc>
			<sql><![CDATA[
SELECT 'Y'
FROM INV_AR_ISS
WHERE INV_NO = @[input_text]			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
