<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSetupDBDAOsearchRevenueStrcSetChkRSQL">
			<desc><![CDATA[searchRevenueStrcSetChk]]></desc>
			<sql><![CDATA[
SELECT REV_CHG_SEQ ,
  CHG_CD ,
  REV_FM_DT_CD ,
  REV_FM_DT ,
  REV_TO_DT_CD ,
  REV_TO_DT ,
  RHQ_CD AS RHQ_OFC_CD,
  SVC_SCP_CD AS SCP_CD,
  CRE_USR_ID ,
  CRE_DT ,
  UPD_USR_ID ,
  UPD_DT
FROM ACM_AGN_SET_REV_CHG_CD
WHERE 1=1
#if (${rev_chg_seq} != '')
  AND REV_CHG_SEQ <> @[rev_chg_seq]
#end
  AND CHG_CD = @[chg_cd]
  AND ( 
        (@[rev_fm_dt] BETWEEN REV_FM_DT AND REV_TO_DT) 
     OR (@[rev_to_dt] BETWEEN REV_FM_DT AND REV_TO_DT)
     OR (REV_FM_DT BETWEEN @[rev_fm_dt] AND @[rev_to_dt]) 
     OR (REV_TO_DT BETWEEN @[rev_fm_dt] AND @[rev_to_dt])
      )
  AND NVL (RHQ_CD, '*') = NVL (@[rhq_ofc_cd], '*')
  AND NVL (SVC_SCP_CD, '*') = NVL (@[scp_cd], '*')			]]></sql>
			<params>
				<param name="rev_chg_seq" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="rev_fm_dt" type="12" value="" out="N"/>
				<param name="rev_to_dt" type="12" value="" out="N"/>
				<param name="rhq_ofc_cd" type="12" value="" out="N"/>
				<param name="scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
