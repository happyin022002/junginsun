<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffRuleDBDAOPriTrfRuleVOUSQL">
			<desc><![CDATA[   Tariff Rule 수정]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRF_RULE
   SET TRF_RULE_NM			= NVL(@[trf_rule_nm], TRF_RULE_NM)                 
     , TRF_RULE_CTNT		= @[trf_rule_ctnt]                
     , TRF_RULE_CHG_CD		= @[trf_rule_chg_cd]          
     , TRF_RULE_AMDT_TP_CD	= @[trf_rule_amdt_tp_cd]
	 , EFF_DT				= TO_DATE(@[eff_dt], 'YYYYMMDD')
     , EXP_DT				= TO_DATE(@[exp_dt], 'YYYYMMDD')
     , RQST_OFC_CD			= NVL(@[rqst_ofc_cd], RQST_OFC_CD)
     , APRO_OFC_CD			= NVL(@[apro_ofc_cd], APRO_OFC_CD)
     , TRF_RULE_STS_CD		= NVL(@[trf_rule_sts_cd], TRF_RULE_STS_CD)
     , UPD_USR_ID			= @[upd_usr_id]                   
     , UPD_DT				= SYSDATE                         
 WHERE TRF_PFX_CD	= @[trf_pfx_cd]
   AND TRF_NO		= @[trf_no]
   AND TRF_RULE_NO	= @[trf_rule_no]
   AND AMDT_SEQ		= @[amdt_seq]			]]></sql>
			<params>
				<param name="trf_rule_nm" type="12" value="" out="N"/>
				<param name="trf_rule_ctnt" type="12" value="" out="N"/>
				<param name="trf_rule_chg_cd" type="12" value="" out="N"/>
				<param name="trf_rule_amdt_tp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="trf_rule_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_rule_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
