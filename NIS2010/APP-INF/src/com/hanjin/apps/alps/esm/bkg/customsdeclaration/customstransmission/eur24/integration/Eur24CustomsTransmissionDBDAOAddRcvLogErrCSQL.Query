<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24CustomsTransmissionDBDAOAddRcvLogErrCSQL">
			<desc><![CDATA[Eur24CustomsTransmissionDBDAOAddRcvLogErrCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_EUR_RCV_ERR
(
    EDI_RCV_DT,         EDI_RCV_SEQ,     RCV_LOG_ERR_SEQ, 
    CSTMS_ERR_ID,       CSTMS_ERR_MSG,   CSTMS_ERR_REF_NO1,  CSTMS_ERR_REF_NO2,
    CRE_USR_ID,         CRE_DT,          UPD_USR_ID,         UPD_DT
) VALUES 
(
    @[edi_rcv_dt],      @[edi_rcv_seq],    NVL((SELECT MAX(RCV_LOG_ERR_SEQ) FROM BKG_CSTMS_ADV_EUR_RCV_ERR WHERE EDI_RCV_DT = @[edi_rcv_dt] AND EDI_RCV_SEQ = @[edi_rcv_seq]),0)+1,
    @[cstms_err_id],    @[cstms_err_msg],  SUBSTRB(@[cstms_err_ref_no1], 1, 50),  SUBSTRB(@[cstms_err_ref_no2], 1, 50),
    @[cre_usr_id],      SYSDATE,           @[cre_usr_id],         SYSDATE
)			]]></sql>
			<params>
				<param name="edi_rcv_dt" type="12" value="" out="N"/>
				<param name="edi_rcv_seq" type="12" value="" out="N"/>
				<param name="cstms_err_id" type="12" value="" out="N"/>
				<param name="cstms_err_msg" type="12" value="" out="N"/>
				<param name="cstms_err_ref_no1" type="12" value="" out="N"/>
				<param name="cstms_err_ref_no2" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
