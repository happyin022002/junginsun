	@[bkg_no]
,	@[bl_no]
,	'0'
,	DECODE(NVL(@[dcgo_flg]||@[rc_flg]||@[awk_cgo_flg]||@[bb_cgo_flg], 'N'), 'N', 'F', 'W')
,	NVL(@[bkg_cgo_tp_cd],'F')
,	NULL
,	@[svc_scp_cd]
,	SUBSTR(@[bkg_trunk_vvd], 1, 4)
,	SUBSTR(@[bkg_trunk_vvd], 5, 4)
,	SUBSTR(@[bkg_trunk_vvd], 9, 1)
,	@[rev_dir_cd]
,	@[rcv_term_cd]
,	@[de_term_cd]
,	@[bkg_por_cd]
,	DECODE(LENGTH(@[bkg_por_yd_cd]), 2, @[bkg_por_cd]||@[bkg_por_yd_cd], @[bkg_por_yd_cd])
,	@[bkg_pol_cd]
,	DECODE(LENGTH(@[bkg_pol_yd_cd]), 2, @[bkg_pol_cd]||@[bkg_pol_yd_cd], @[bkg_pol_yd_cd])
,	@[bkg_pod_cd]
,	DECODE(LENGTH(@[bkg_pod_yd_cd]), 2, @[bkg_pod_cd]||@[bkg_pod_yd_cd], @[bkg_pod_yd_cd])
,	@[ocp_cd]
,	@[bkg_del_cd]
,	DECODE(LENGTH(@[bkg_del_yd_cd]), 2, @[bkg_del_cd]||@[bkg_del_yd_cd], @[bkg_del_yd_cd])
,	@[fnl_dest_cd]
,   null
,   null
,	@[mty_pkup_yd_cd]
,	TO_DATE(@[mty_pkup_dt],'YYYY-MM-DD')
,	@[full_rtn_yd_cd]
,	@[full_pkup_yd_cd]
,	@[mty_rtn_yd_cd]
,	TO_DATE(@[mty_dor_arr_dt],'YYYY-MM-DD')
,	TO_DATE(@[lodg_due_dt],'YYYY-MM-DD')
,	TO_DATE(@[de_due_dt],'YYYY-MM-DD')
,	TO_DATE(@[ib_del_de_dt],'YYYY-MM-DD')
,	@[org_trns_svc_mod_cd]
,	@[org_trns_mod_cd]
,	(SELECT SCONTI_CD FROM MDM_LOCATION WHERE LOC_CD = @[bkg_por_cd])
,	@[dest_trns_svc_mod_cd]
,	@[dest_trns_mod_cd]
,	(SELECT SCONTI_CD FROM MDM_LOCATION WHERE LOC_CD = @[bkg_del_cd])
,	@[stop_off_loc_cd]
,	@[stop_off_cntc_pson_nm]
,	@[stop_off_cntc_phn_no]
,	@[stop_off_diff_rmk]
,	@[sls_rhq_cd]
,	@[sls_rgn_ofc_cd]
,	@[bkg_ofc_cd]
,	@[cre_usr_id]
,	(SELECT OFC_CD FROM MDM_SLS_REP WHERE SREP_CD = UPPER(@[ob_srep_cd]))
,	UPPER(@[ob_srep_cd])
,	(SELECT FINC_CTRL_OFC_CD FROM MDM_LOCATION WHERE LOC_CD = @[bkg_del_cd])
,	(SELECT EQ_CTRL_OFC_CD FROM MDM_LOCATION WHERE LOC_CD = DECODE(LENGTH(@[mty_pkup_yd_cd]),5,@[mty_pkup_yd_cd],@[bkg_por_cd]) AND DELT_FLG = 'N')
,	GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,NVL(BKG_COM_USER_LOC_FNC(@[cre_usr_id]),@[bkg_por_cd]))
,	GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,'GMT')
,	null
,	@[cmdt_cd]
,	(select rep_cmdt_cd from mdm_commodity where cmdt_cd = @[cmdt_cd])
,	NVL(@[dcgo_flg],'N')
,	NVL(@[rc_flg],'N')
,	NVL(@[awk_cgo_flg],'N')
,	NVL(@[bb_cgo_flg],'N')
,	NVL(@[rd_cgo_flg],'N')
,	NVL(@[hngr_flg],'N')
,	@[rail_blk_cd]
,	NVL(@[prct_flg],'N')
,	NVL(@[spcl_hide_flg],'N')
,	NVL(@[soc_flg],'N')
,	NVL(@[eq_subst_flg],'N')
,	NVL(@[fd_grd_flg],'N')
,	NVL(@[flex_hgt_flg],'N')
,	@[stwg_cd]
,	@[stwg_rmk]
,	@[blck_stwg_cd]
,	NVL(@[all_mtr_flg],'N')
,	NVL(@[dbl_bkg_flg],'N')
,	NVL(@[ap_brog_flg],'N')
,	NVL(@[cust_to_ord_flg],'N')
,	NVL((SELECT 'C'
            FROM BKG_HRD_CDG_CTNT
            WHERE HRD_CDG_ID = 'KR_PANTOS_CUSTOMER'
            AND ATTR_CTNT1 = @[s_cust_cnt_cd]
            AND ATTR_CTNT2 = @[s_cust_seq]), 
		(SELECT DECODE(RVIS_CNTR_CUST_TP_CD, 'B', 'S', 'C') 
		  	  FROM MDM_CUSTOMER
 		 	  WHERE CUST_CNT_CD = @[s_cust_cnt_cd]
		    	AND CUST_SEQ = @[s_cust_seq]))
,	NVL(@[sam_cnee_ntfy_flg],'N')
,	NVL(@[alt_cust_cfm_flg],'N')
,	DECODE(NVL(@[dcgo_flg]||@[rc_flg]||@[awk_cgo_flg]||@[bb_cgo_flg], 'N'), 'N', 'N', 'Y')
,	NVL(@[wt_rsn_hld_flg],'N')
,	NVL(@[shp_bak_flg],'N')
,	case when length(@[bkg_no]) <> 12 and 'Y' = (select 'Y' from bkg_chn_agn        where chn_agn_cd = substr(@[bkg_no], 5, 2) and rownum = 1) 
			then 'Y'
         when length(@[bkg_no]) =  12 and 'Y' = (select 'Y' from bkg_chn_bkg_no_gen where chn_agn_cd = substr(@[bkg_no], 3, 2) and rownum = 1) 
			then 'Y'
		 else NVL(@[mnl_bkg_no_flg],'N') end
,	NVL(@[bl_iss_blck_flg],'N')
,	null
,	@[scac_cd]
,	case when length(@[bkg_no]) <> 12 and 'Y' = (select 'Y' from bkg_chn_agn        where chn_agn_cd = substr(@[bkg_no], 5, 2) and rownum = 1) 
			then substr(@[bkg_no], 5, 2)
         when length(@[bkg_no]) =  12 and 'Y' = (select 'Y' from bkg_chn_bkg_no_gen where chn_agn_cd = substr(@[bkg_no], 3, 2) and rownum = 1) 
			then substr(@[bkg_no], 3, 2)
		 else null end
,	@[usa_cstms_file_cd]
,	@[cnd_cstms_file_cd]
,	@[twn_so_no]
,	@[inter_rmk]
,	@[xter_rmk]
,	NVL(@[inter_rmk_aud_flg],'N')
,	NVL(@[split_flg],'N')
,	NVL(@[hcmt_cmb_flg],'N')
,	'B'
,	1
,	@[sc_no]
,	@[rfa_no]
,	@[taa_no]
,	@[agmt_act_cnt_cd]
,	@[agmt_act_cust_seq]
,	@[mty_cre_svr_cd]
,	@[mty_bkg_sts_cd]
,	@[mty_split_aval_cd]
,	DECODE(NVL(@[xter_bkg_rqst_cd], 'NIS'), 'OFF', 'NIS', NVL(@[xter_bkg_rqst_cd], 'NIS'))
,	@[xter_bkg_rqst_ref_no]
,	'N'
,	null
,	@[xter_si_ref_no]
,	NVL(@[xter_rqst_auto_ntc_flg],'N')
,	@[xter_bkg_knt]
,	@[xter_si_knt]
,	@[xter_rqst_cust_rmk]
,	NVL(@[ovr_void_slt_qty],0)
,	@[my_fwrd_cd]
,	@[my_fwrd_vsl_desc]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	@[pctl_no]
,	NVL(@[edi_hld_flg],'N')
,	NVL(@[port_skp_flg],'N')
,	@[fumg_loc_cd]
,	@[fumg_cntc_pson_nm]
,	@[fumg_cntc_phn_no]
,	@[fumg_diff_rmk]
,	NVL(@[spcl_hide_lnr_flg],'N')
,	NVL(@[crr_soc_flg],'N')
,	NVL(@[veh_cmdt_flg],'N')
,   NVL(@[new_cust_apro_flg],'N')
,   @[new_cust_apro_cmdt_nm]
,   @[new_cust_apro_rmk] 
,   @[ida_hlg_tp_cd]
,   NVL(@[non_dg_chem_flg],'N')
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="dcgo_flg" type="12" value="" out="N"/>
