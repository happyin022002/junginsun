,	ALT_CUST_CFM_FLG
,	WT_RSN_SPCL_CGO_FLG
,	WT_RSN_HLD_FLG
,	SHP_BAK_FLG
,	MNL_BKG_NO_FLG
,	BL_ISS_BLCK_FLG
,	CA_INSP_DUE_DT
,	SCAC_CD
,	CHN_AGN_CD
,	USA_CSTMS_FILE_CD
,	CND_CSTMS_FILE_CD
