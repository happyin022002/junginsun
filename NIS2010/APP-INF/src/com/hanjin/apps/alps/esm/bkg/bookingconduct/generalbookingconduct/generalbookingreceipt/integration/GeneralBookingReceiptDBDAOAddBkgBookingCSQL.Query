,	OB_SLS_OFC_CD
,	OB_SREP_CD
,	IB_SLS_OFC_CD
,	EQ_CTRL_OFC_CD
,	BKG_CRE_DT
, 	BKG_CRE_GDT
,	PORT_CLZ_DT
,	CMDT_CD
,	REP_CMDT_CD
,	DCGO_FLG
,	RC_FLG
,	AWK_CGO_FLG
,	BB_CGO_FLG
,	RD_CGO_FLG
,	HNGR_FLG
,	RAIL_BLK_CD
,	PRCT_FLG
,	SPCL_HIDE_FLG
,	SOC_FLG
,	EQ_SUBST_FLG
,	FD_GRD_FLG
,	FLEX_HGT_FLG
,	STWG_CD
,	STWG_RMK
,	BLCK_STWG_CD
