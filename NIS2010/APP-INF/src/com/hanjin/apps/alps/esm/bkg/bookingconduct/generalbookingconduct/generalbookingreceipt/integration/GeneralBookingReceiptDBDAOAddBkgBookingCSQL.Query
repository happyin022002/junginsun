		<query name="GeneralBookingReceiptDBDAOAddBkgBookingCSQL">
			<desc><![CDATA[Booking정보 저장 ]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BOOKING (
	BKG_NO
,	BL_NO
,	BL_NO_TP
,	BKG_STS_CD
,	BKG_CGO_TP_CD
