,   EDI_HLD_FLG
,	PORT_SKP_FLG
,	FUMG_LOC_CD
,	FUMG_CNTC_PSON_NM
,	FUMG_CNTC_PHN_NO
,	FUMG_DIFF_RMK
,	SPCL_HIDE_LNR_FLG
,	CRR_SOC_FLG
,	VEH_CMDT_FLG
,	NEW_CUST_APRO_FLG
,   NEW_CUST_APRO_CMDT_NM
,	NEW_CUST_APRO_RMK
,   IDA_HLG_TP_CD
,   NON_DG_CHEM_FLG
