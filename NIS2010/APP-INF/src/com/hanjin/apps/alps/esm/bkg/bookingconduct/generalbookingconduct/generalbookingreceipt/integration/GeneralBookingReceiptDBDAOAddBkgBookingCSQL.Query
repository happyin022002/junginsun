,	RFA_NO
,	TAA_NO
,	AGMT_ACT_CNT_CD
,	AGMT_ACT_CUST_SEQ
,	MTY_CRE_SVR_CD
,	MTY_BKG_STS_CD
,	MTY_SPLIT_AVAL_CD
,	XTER_BKG_RQST_CD
,	XTER_BKG_RQST_REF_NO
,	SI_FLG
,	XTER_SI_CD
,	XTER_SI_REF_NO
,	XTER_RQST_AUTO_NTC_FLG
,	XTER_BKG_KNT
,	XTER_SI_KNT
,	XTER_RQST_CUST_RMK
