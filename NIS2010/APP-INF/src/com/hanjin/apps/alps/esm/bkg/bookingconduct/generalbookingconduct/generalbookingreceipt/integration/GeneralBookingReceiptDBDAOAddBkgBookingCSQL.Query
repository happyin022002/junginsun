,	SKD_DIR_CD
,	REV_DIR_CD
,	RCV_TERM_CD
,	DE_TERM_CD
,	POR_CD
,	POR_NOD_CD
,	POL_CD
,	POL_NOD_CD
,	POD_CD
,	POD_NOD_CD
,	OCP_CD
,	DEL_CD
,	DEL_NOD_CD
,	FNL_DEST_CD
,	POL_ETD_DT
,	POD_ETA_DT
,	MTY_PKUP_YD_CD
,	MTY_PKUP_DT
,	FULL_RTN_YD_CD
,	FULL_PKUP_YD_CD
,	MTY_RTN_YD_CD
,	MTY_DOR_ARR_DT
,	LODG_DUE_DT
,	DE_DUE_DT
,	IB_DEL_DE_DT
