<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOCopyTariffFdrMainDataCSQL">
			<desc><![CDATA[CopyTariffFdrMainData
2013.03.05 [CHM-201323352] 전윤주 copy 시 confirm date를 copy한 날짜로, confrim user는 copy한 user 로 변경]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_FDR_MN
  (SVC_SCP_CD,
   ORG_DEST_TP_CD,
   FDR_TRF_NO,
   AMDT_SEQ,
   EFF_DT,
   EXP_DT,
   CFM_DT,
   FIC_PROP_STS_CD,
   TRF_CURR_CD,
   LOCL_CURR_CD,
   CFM_USR_ID,
   CFM_OFC_CD,
   CRE_OFC_CD,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT)
SELECT @[svc_scp_cd]
     , @[org_dest_tp_cd]
     , @[fdr_trf_no]  
     , '0'
     , TO_DATE(@[eff_dt], 'YYYY-MM-DD')
     , TO_DATE('99991231', 'YYYYMMDD')
     , SYSDATE CFM_DT --confrim date는 copy 하는 날짜로(정석환 차장 요청)
     , MN.FIC_PROP_STS_CD
     , MN.TRF_CURR_CD
     , MN.LOCL_CURR_CD
     , @[cre_usr_id] CFM_USR_ID --confirm user를 copy 하는 user로 (정석환 차장 요청)
     , @[cre_ofc_cd] CFM_OFC_CD --confirm office를 copy 하는 user office로 (정석환 차장 요청)
     , @[cre_ofc_cd] CRE_OFC_CD
     , @[cre_usr_id]
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
 FROM PRI_TRF_FDR_MN MN
 WHERE 1 = 1
   AND MN.SVC_SCP_CD     = @[ori_svc_scp_cd] 
   AND MN.ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND MN.FDR_TRF_NO     = @[ori_fdr_trf_no] 
   AND MN.AMDT_SEQ       = @[ori_amdt_seq]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ori_svc_scp_cd" type="12" value="" out="N"/>
				<param name="ori_fdr_trf_no" type="12" value="" out="N"/>
				<param name="ori_amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
