<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOsearchRefTpCdRSQL">
			<desc><![CDATA[Referecne Type이 이미 존재하는지 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_REF_TP_CD
  FROM BKG_REFERENCE
 WHERE BKG_NO = @[bkg_no]
   AND BKG_REF_TP_CD = @[bkg_ref_tp_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_ref_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
