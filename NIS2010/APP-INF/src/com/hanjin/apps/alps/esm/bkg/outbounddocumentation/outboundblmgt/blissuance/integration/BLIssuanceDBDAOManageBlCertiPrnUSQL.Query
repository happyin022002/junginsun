<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOManageBlCertiPrnUSQL">
			<desc><![CDATA[BLIssuanceDBDAOManageBlCertiPrnUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_CERTI
SET BL_CERTI_PRN_FLG = 'Y'
   ,BL_CERTI_STS_CD = 'C'
   ,CMPL_DT = sysdate
   ,UPD_DT = sysdate
   ,UPD_USR_ID = @[upd_usr_id]
WHERE BL_NO = @[bl_no]
  AND BL_CERTI_SEQ = @[bl_certi_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_certi_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
