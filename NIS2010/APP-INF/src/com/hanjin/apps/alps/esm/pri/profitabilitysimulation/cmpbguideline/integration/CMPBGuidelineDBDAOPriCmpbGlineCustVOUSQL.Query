			<sql><![CDATA[
UPDATE PRI_CMPB_GLINE_CUST SET 
	SVC_SCP_CD = @[svc_scp_cd]
,	CRE_OFC_CD = @[cre_ofc_cd]
,	GLINE_SEQ = @[gline_seq]
,	PRS_CUST_TP_CD = @[prs_cust_tp_cd]
,	CUST_CNT_CD = @[cust_cnt_cd]
,	CUST_SEQ = @[cust_seq]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	CRE_OFC_CD = @[cre_ofc_cd]
AND	GLINE_SEQ = @[gline_seq]
AND	PRS_CUST_TP_CD = @[prs_cust_tp_cd]
			]]></sql>
