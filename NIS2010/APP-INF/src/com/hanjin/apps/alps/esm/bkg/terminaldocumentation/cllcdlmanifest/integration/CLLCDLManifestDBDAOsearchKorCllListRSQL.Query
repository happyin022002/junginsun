<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchKorCllListRSQL">
			<desc><![CDATA[searchKorCllList]]></desc>
			<sql><![CDATA[
SELECT 
	ROWNUM SEQ,
	VSL_CD,
	SKD_VOY_NO,
	SKD_DIR_CD,
	POL_CD,
	BKG_POL_CD,
	CNTR_NO,
	BL_NO,
	BKG_NO,
	CNTR_TPSZ_CD,
	MTY_BKG_CD,
	SEAL_NO,
	BL_WGT, 
	WGT_UT_CD,
	RCV_TERM_CD,
	TS_FLG,
	IMDG_CLSS_CD,
	IMDG_UN_NO,
	CDO_TEMP,
	CNTR_VENT_CD,
	HAMO_TRF_CD,
	CMDT_HS_CD,
	CLL_RMK1,
	CLL_RMK2,
	POD_CD,
	BLCK_STWG_CD,
	TS_VSL_CD||TS_SKD_VOY_NO||TS_SKD_DIR_CD TS_VVD_CD,
	OVR_LEN_QTY,
	OVR_WGT_QTY,
	OVR_HGT_QTY,
	MIN_TEMP,
	MAX_TEMP,
	KR_TML_PRCT_ID,
	POD_YD_CD,
	POL_YD_CD,
	MRN_POLUT_FLG,
	STWG_CD,
	XTER_RMK,
	A_POD_CD,
	T_VSL_CD,
	BKG_POL_CD,
	BC_CD,
	VGM_WGT,
	VGM_WGT_UT_CD ,
	VGM_VRFY_SIG_CTNT,
    VGM_MZD_TP_CD
	
FROM
	(	
SELECT 
	--CNTR_LIST_NO,
	A.VSL_CD,
	A.SKD_VOY_NO,
	A.SKD_DIR_CD,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.POL_CD),A.POL_CD) POL_CD,
	--A.POL_CD,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.BKG_POL_CD),A.POL_CD) BKG_POL_CD,
	--A.BKG_POL_CD,
	A.CNTR_NO,
	A.BL_NO,
	A.BKG_NO,
	A.CNTR_TPSZ_CD,
	A.MTY_BKG_CD,
	A.SEAL_NO,
	A.BL_WGT,
	A.WGT_UT_CD,
	SUBSTR(NVL(A.RCV_TERM_CD,' ')||NVL(A.DE_TERM_CD,' '),1,2) RCV_TERM_CD,
	A.KR_CLL_TS_CD TS_FLG,
	DECODE(A.MTY_BKG_CD,'M','2',DECODE(A.KR_CLL_TS_CD, 'TS', '3', 'TT', '4', '1')) AA,
	--UN_LOC_CD,
	A.IMDG_CLSS_ID IMDG_CLSS_CD ,
	A.TML_IMDG_ID IMDG_UN_NO,
	A.CDO_TEMP,
	A.CNTR_VENT_ID CNTR_VENT_CD,
	CM.HAMO_TRF_CD,
	CM.CMDT_HS_CD,
	A.CLL_RMK1,
	A.CLL_RMK2,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.POD_CD),A.POD_CD) POD_CD,
	--A.POD_CD,
	A.BLCK_STWG_CD,
	--HOT_FLG,
	A.TS_VSL_CD,
	A.TS_SKD_VOY_NO,
	A.TS_SKD_DIR_CD,
	--CUST_CNT_CD,
	A.OVR_LEN_QTY,
	A.OVR_WGT_QTY,
	A.OVR_HGT_QTY,
	A.MIN_TEMP,
	A.MAX_TEMP,
	--UPD_DT,
	A.KR_TML_PRCT_ID,
	A.POD_YD_CD,
	A.POL_YD_CD,
	'' MRN_POLUT_FLG,
	--B.STWG_CD,
	--DECODE(A.CLL_RMK1, 'INGU' , '' , B.STWG_CD) STWG_CD,
	A.STWG_CD STWG_CD,
	'' XTER_RMK,
	NVL((SELECT UN_LOC_CD FROM MDM_LOCATION WHERE LOC_CD = A.BKG_POD_CD),A.BKG_POD_CD) A_POD_CD,
	--A.BKG_POD_CD A_POD_CD,
	'' T_VSL_CD,
	'' VVD_CD_NM,	
	'' POL_CD_PRINT,
	'' VPS_ETD,
	B.BLCK_STWG_CD AS BC_CD,
	A.VGM_WGT,
	'KGS' VGM_WGT_UT_CD ,
	A.VGM_VRFY_SIG_CTNT,
    A.VGM_MZD_TP_CD
FROM 
	BKG_CSTMS_TML_KR_CLL A,BKG_BOOKING B, BKG_CNTR_MF_DESC CM
#if (${in_cntr_cfm_flg}!= '') 
	,BKG_CONTAINER C
#end
#if (${in_pol_yd_cd} == '') 
WHERE A.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no],2,3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd],3,3)
#elseif (${in_pol_yd_cd} != '') 
WHERE A.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no],2,3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd],3,3)
AND A.POL_YD_CD = @[in_pol_yd_cd]
#end
AND A.BKG_NO = B.BKG_NO
#if (${in_bkg_sts_cd}!= '') 			
AND B.BKG_STS_CD = @[in_bkg_sts_cd]
#end
#if (${in_cntr_cfm_flg}!= '') 	
AND A.BKG_NO = C.BKG_NO
AND A.CNTR_NO = C.CNTR_NO		
AND C.CNTR_CFM_FLG = @[in_cntr_cfm_flg]
#end
AND A.BKG_NO = CM.BKG_NO (+)
AND A.CNTR_NO = CM.CNTR_NO (+)
AND CM.CNTR_MF_SEQ (+) = 1
#if (${in_sort_type} == '' ) 
order by AA, A.POL_CD, A.POD_CD, A.CNTR_NO
#elseif (${in_sort_type} == '1' ) 
order by AA, A.POL_CD, A.POD_CD, A.CNTR_NO
#elseif (${in_sort_type} == '2' ) 
order by AA, A.POL_CD, A.POD_CD, A.BKG_POD_CD
#elseif (${in_sort_type} == '3' ) 
order by AA, A.TS_VSL_CD||A.TS_SKD_VOY_NO||A.TS_SKD_DIR_CD
#elseif (${in_sort_type} == '4' ) 
order by AA, DECODE(A.BLCK_STWG_CD,'LOC','11','ONE','12','TWO','13','THR','14','FOR','15','FIV', '15.5','SEV', '15.7', 'HOT','16','TRS','17', '18')
#elseif (${in_sort_type} == '5' ) 
order by AA, A.POD_CD, DECODE(A.BLCK_STWG_CD,'LOC','11','ONE','12','TWO','13','THR','14','FOR','15', 'FIV', '15.5','SEV', '15.7', 'HOT','16','TRS','17', '18'), A.CNTR_TPSZ_CD, A.CNTR_NO
#elseif (${in_sort_type} == '6' ) 
order by A.CNTR_NO
#elseif (${in_sort_type} == '7' ) 
order by TO_NUMBER(A.BL_WGT,'99990')
#end
)			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
				<param name="in_bkg_sts_cd" type="12" value="" out="N"/>
				<param name="in_cntr_cfm_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
