<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCQuotationMainDBDAOPriSqMnVOAddCopyToQuotationCSQL">
			<desc><![CDATA[CopyToQuotation]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SQ_MN (
	QTTN_NO
,	QTTN_VER_NO
,	EFF_DT
,	EXP_DT
,	SVC_SCP_CD
,	CUST_CNT_CD
,	CUST_SEQ
,	CNTR_LOD_UT_CD
,	ESTM_MQC_QTY
--,	ESTM_CM_AMT
,	PRC_CUST_TP_CD
,	QTTN_SREP_CD
--,	PRS_XCH_RT_YRMON
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT  @[qttn_no]
,	@[qttn_ver_no]
,	EFF_DT
,	EXP_DT
,	@[svc_scp_cd]
,	CUST_CNT_CD
,	CUST_SEQ
,	CNTR_LOD_UT_CD
,	1
--,	ESTM_CM_AMT
,	PRC_CUST_TP_CD
,	@[qttn_srep_cd]
--,	PRS_XCH_RT_YRMON
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM    PRI_SQ_MN
WHERE   1=1
AND    QTTN_NO = @[qttn_no_from]
AND    QTTN_VER_NO = @[qttn_ver_no_from]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="qttn_srep_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="qttn_no_from" type="12" value="" out="N"/>
				<param name="qttn_ver_no_from" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
