<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchAlpsPoNoRefSeqRSQL">
			<desc><![CDATA[Po_No 조회]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
     , REF_SEQ
     , BKG_REF_TP_CD
     , CUST_REF_NO_CTNT
     , CNTR_NO
     , CNTR_MF_SEQ
     , CPY_DESC_FLG
  FROM BKG_REFERENCE
 WHERE BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
