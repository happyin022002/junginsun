<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaCustomsTransmissionDBDAOsearchIgmVslRSQL">
			<desc><![CDATA[Agent Code,Seq 정보를 조회해 온다.]]></desc>
			<sql><![CDATA[
SELECT 
    IDA_AGN_ID
    ,OTR_DCHG_YD_ID OTR_DCHG_CD --PORT_CD
    ,TO_CHAR( SYSDATE, 'YYMMDD') GEN_DT
FROM BKG_CSTMS_IDA_VSL
WHERE	1=1
AND VSL_CD          = SUBSTR(@[vvd_cd], 1, 4)
AND SKD_VOY_NO      = SUBSTR(@[vvd_cd], 5, 4)
AND SKD_DIR_CD      = SUBSTR(@[vvd_cd], 9, 1)
AND	POD_CD			=	@[pod_cd]			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
