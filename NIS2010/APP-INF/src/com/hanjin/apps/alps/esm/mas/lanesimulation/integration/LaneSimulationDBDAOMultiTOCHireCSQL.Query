<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOMultiTOCHireCSQL">
			<desc><![CDATA[TOC Hire 입력]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_TM_CHTR_OUT_HIR(
                TO_HIR_SEQ
               ,FM_VSL_CLSS_CAPA
               ,TO_VSL_CLSS_CAPA
               ,VSL_TEU_UC_AMT
               ,VSL_DLY_UC_AMT
               ,CRE_USR_ID
               ,CRE_DT
               ,UPD_USR_ID
               ,UPD_DT)
               VALUES(
			   (SELECT NVL(MAX(TO_HIR_SEQ),0) FROM MAS_TM_CHTR_OUT_HIR)+@[to_hir_seq],
			   ,@[fm_vsl_clss_capa]
			   ,@[to_vsl_clss_capa]
			   ,@[vsl_teu_uc_amt]
			   ,@[vsl_dly_uc_amt]
			   ,@[cre_usr_id]
			   ,SYSDATE
			   ,@[upd_usr_id]	
			   ,SYSDATE)			]]></sql>
			<params>
				<param name="to_hir_seq" type="12" value="" out="N"/>
				<param name="fm_vsl_clss_capa" type="12" value="" out="N"/>
				<param name="to_vsl_clss_capa" type="12" value="" out="N"/>
				<param name="vsl_teu_uc_amt" type="12" value="" out="N"/>
				<param name="vsl_dly_uc_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
