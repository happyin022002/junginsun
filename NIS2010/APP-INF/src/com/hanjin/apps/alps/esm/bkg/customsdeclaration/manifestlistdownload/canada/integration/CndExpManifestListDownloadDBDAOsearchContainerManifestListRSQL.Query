<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndExpManifestListDownloadDBDAOsearchContainerManifestListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT  BL_NO
	   ,CNTR_NO
	   ,CMDT_GDS_SEQ
	   ,CNTR_NO
       ,PCK_QTY
       ,AMS_PCK_TP_CD
       ,GRS_WGT
       ,WGT_UT_CD
       ,MK_DESC
       ,CGO_DESC
       ,HAMO_CMDT_CD
       ,NVL(PRIOR_NTC_SND_FLG, 'N') AS PRIOR_NTC_SND_FLG
       ,DECODE(PRIOR_NTC_CFM_FLG,'N','0','Y','1') AS PRIOR_NTC_CFM_FLG
       ,TO_CHAR(PRIOR_NTC_CFM_DT, 'YYYYMMDD') AS PRIOR_NTC_CFM_DT
       ,PRIOR_NTC_RMK
	   ,'' AS BDR_FLG
FROM  BKG_CSTMS_AMER_CNTR_MF
WHERE 1=1
AND   CNT_CD = @[cnt_cd]
AND   BL_NO = @[bl_no] 
--#if (${cntr_no} != '')
--AND   CNTR_NO = [cntr_no]
--#end
ORDER BY CMDT_GDS_SEQ 			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
