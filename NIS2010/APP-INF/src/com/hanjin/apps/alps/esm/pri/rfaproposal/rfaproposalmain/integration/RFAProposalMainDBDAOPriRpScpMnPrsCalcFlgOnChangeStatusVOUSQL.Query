<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPriRpScpMnPrsCalcFlgOnChangeStatusVOUSQL">
			<desc><![CDATA[C/Offer, Request Cancel시 Rate Calc Flag를 수정한다.
*History
C/Offer, Request Cancel시 Rate M/B Flag를 수정 추가]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_MN M
   SET PRS_RT_CMPB_CALC_FLG = @[prs_rt_cmpb_calc_flg]
   ,PRS_RT_MB_FLG       	= @[prs_rt_mb_flg]
   ,UPD_USR_ID          	= @[upd_usr_id]
   ,UPD_DT              	= SYSDATE
 WHERE PROP_NO  = @[prop_no]
   AND AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="prs_rt_cmpb_calc_flg" type="12" value="" out="N"/>
				<param name="prs_rt_mb_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
