<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CRMSalesLeadDBDAOPriCrmSlsLdRSQL">
			<desc><![CDATA[CRM Sales Lead 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
	SLS_LD_NO
,	SLS_LD_NM
,	SLS_LD_CUST_CNT_CD
,	SLS_LD_CUST_SEQ
,	SLS_LD_SREP_CD
,	SLS_LD_ST_DT
,	SLS_LD_STS_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM PRI_CRM_SLS_LD
WHERE	SLS_LD_NO = @[sls_ld_no]
			]]></sql>
			<params>
				<param name="sls_ld_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
