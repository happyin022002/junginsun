<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOcopyExportImportNumberByBkgCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
insert into bkg_xpt_imp_lic(BKG_NO 
,IO_BND_CD 
,XPT_IMP_SEQ 
,CNT_CD 
,XPT_LIC_NO 
,TS_REF_NO 
,PCK_QTY 
,PCK_TP_CD 
,MF_WGT 
,WGT_UT_CD 
,DIVD_FLG 
,DIVD_SEQ 
,DIVD_PCK_QTY 
,DIVD_PCK_TP_CD 
,DIVD_WGT 
,DIVD_WGT_UT_CD 
,SAM_PCK_ID 
,SAM_PCK_QTY 
,SAM_PCK_TP_CD 
,UCR_NO 
,AUS_MF_REF_NO 
,AES_TP_CD 
,AES_INLND_TRNS_PFX_CTNT 
,AES_INLND_TRNS_NO 
,AES_PTA_PFX_CTNT 
,AES_PTA_NO1 
,AES_PTA_NO2 
,AES_PTA_DT 
,AES_PTU_PFX_CTNT 
,AES_PTU_NO 
,AES_PTU_DT 
,AES_DWN_PFX_CTNT 
,AES_DWN_NO 
,AES_DWN_DT 
,AES_EXPT_ID 
,AES_EXPT_CTNT 
,SHPR_TAX_NO 
,SHPR_TAX_CPY_DESC_FLG 
,CNEE_TAX_NO 
,CNEE_TAX_CPY_DESC_FLG 
,NTFY_TAX_NO 
,NTFY_TAX_CPY_DESC_FLG 
,BRZ_DECL_NO 
,BRZ_DECL_CPY_DESC_FLG 
,BRZ_CMDT_CD 
,ID_DECL_CD 
,ID_XPT_NO 
,ID_XPT_NO_ISS_DT 
,ID_OFC_ID
,IDA_IEC_NO 
,IDA_IEC_CPY_DESC_FLG 
,CAED_TP_CD 
,CAED_PFX_CTNT 
,CAED_NO1 
,CAED_NO2 
,CAED_NO3 
,G7_EDI_PFX_CTNT 
,G7_EDI_NO1 
,G7_EDI_NO2 
,B13A_XPT_PFX_CTNT 
,B13A_XPT_DT 
,B13A_XPT_NO1 
,B13A_XPT_NO2 
,MF_SMRY_RPT_PFX_CTNT 
,MF_SMRY_RPT_NO 
,CGO_CTRL_PFX_CTNT 
,CGO_CTRL_NO 
,NDR_REF_PFX_CTNT 
,NDR_REF_ID 
,NDR_REF_CTNT 
,VIN_CTNT
,NG_EXS_ENS_NO
,TG_ECTN_NO
,IDA_SHP_BIL_NO
,IDA_SHP_BIL_ISS_DT
,CRE_USR_ID 
,CRE_DT 
,UPD_USR_ID 
,UPD_DT
)
select @[targetBkg] BKG_NO 
		,IO_BND_CD 
		,(select /*+index_desc ( bkg_xpt_imp_lic XPKBKG_XPT_IMP_LIC)*/ 
					nvl(sum(XPT_IMP_SEQ),0)+ xpt.XPT_IMP_SEQ 
					from bkg_xpt_imp_lic
					where XPT_IMP_SEQ >= 0 
					and rownum <= 1 
					and bkg_no = @[targetBkg] ) XPT_IMP_SEQ 
		,CNT_CD 
		,XPT_LIC_NO 
		,TS_REF_NO 
		,PCK_QTY 
		,PCK_TP_CD 
		,MF_WGT 
		,WGT_UT_CD 
		,DIVD_FLG 
		,DIVD_SEQ 
		,DIVD_PCK_QTY 
		,DIVD_PCK_TP_CD 
		,DIVD_WGT 
		,DIVD_WGT_UT_CD 
		,SAM_PCK_ID 
		,SAM_PCK_QTY 
		,SAM_PCK_TP_CD 
		,UCR_NO 
		,AUS_MF_REF_NO 
		,AES_TP_CD 
		,AES_INLND_TRNS_PFX_CTNT 
		,AES_INLND_TRNS_NO 
		,AES_PTA_PFX_CTNT 
		,AES_PTA_NO1 
		,AES_PTA_NO2 
		,AES_PTA_DT 
		,AES_PTU_PFX_CTNT
		,AES_PTU_NO 
		,AES_PTU_DT 
		,AES_DWN_PFX_CTNT 
		,AES_DWN_NO 
		,AES_DWN_DT 
		,AES_EXPT_ID 
		,AES_EXPT_CTNT 
		,SHPR_TAX_NO 
		,SHPR_TAX_CPY_DESC_FLG 
		,CNEE_TAX_NO 
		,CNEE_TAX_CPY_DESC_FLG 
		,NTFY_TAX_NO 
		,NTFY_TAX_CPY_DESC_FLG 
		,BRZ_DECL_NO 
		,BRZ_DECL_CPY_DESC_FLG 
		,BRZ_CMDT_CD 
		,ID_DECL_CD 
		,ID_XPT_NO 
		,ID_XPT_NO_ISS_DT 
		,ID_OFC_ID
		,IDA_IEC_NO 
		,IDA_IEC_CPY_DESC_FLG 
		,CAED_TP_CD 
		,CAED_PFX_CTNT 
		,CAED_NO1 
		,CAED_NO2 
		,CAED_NO3 
		,G7_EDI_PFX_CTNT 
		,G7_EDI_NO1 
		,G7_EDI_NO2 
		,B13A_XPT_PFX_CTNT 
		,B13A_XPT_DT 
		,B13A_XPT_NO1 
		,B13A_XPT_NO2 
		,MF_SMRY_RPT_PFX_CTNT 
		,MF_SMRY_RPT_NO 
		,CGO_CTRL_PFX_CTNT 
		,CGO_CTRL_NO 
		,NDR_REF_PFX_CTNT 
		,NDR_REF_ID 
		,NDR_REF_CTNT 
        ,VIN_CTNT
		,NG_EXS_ENS_NO
		,TG_ECTN_NO
        ,IDA_SHP_BIL_NO
        ,IDA_SHP_BIL_ISS_DT
		,@[usr_id] CRE_USR_ID 
		,sysdate CRE_DT 
		,@[usr_id] UPD_USR_ID 
		,sysdate UPD_DT
from bkg_xpt_imp_lic xpt
where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="targetBkg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
