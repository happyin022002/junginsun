<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAORsltPriSurchargeLastAccessDateVORSQL">
			<desc><![CDATA[   S/C Proposal/Amendment Surcharge View All]]></desc>
			<sql><![CDATA[
SELECT GEN_SPCL_RT_TP_CD, TO_CHAR(MAX(CRE_DT),'YYYY-MM-DD') CRE_YMD 
FROM PRI_SP_SCP_RT_SCG_ROUT
WHERE 
	PROP_NO = @[prop_no]
	AND AMDT_SEQ = @[amdt_seq]
	AND SVC_SCP_CD = @[svc_scp_cd] 
#if( ${gen_spcl_rt_tp_cd} != 'B'   )
    AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
#end 
GROUP BY  GEN_SPCL_RT_TP_CD
UNION ALL
SELECT 'CUST_TYPE' , PRC_CTRT_CUST_TP_CD
FROM  PRI_SP_CTRT_CUST_TP
WHERE  PROP_NO = @[prop_no]
    AND AMDT_SEQ          =  @[amdt_seq]
    AND PRC_CTRT_PTY_TP_CD = 'C'
			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
