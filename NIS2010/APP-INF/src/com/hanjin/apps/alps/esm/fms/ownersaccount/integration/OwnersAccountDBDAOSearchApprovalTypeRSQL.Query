<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOSearchApprovalTypeRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
-- USD로 3만불 이상인지 판단 ( parameter : orgCur / orgAmt )
SELECT --A.TTL_USD_AMT,
        CASE 
            WHEN A.TTL_USD_AMT >= 30000
            THEN 'GW'
            ELSE 'ALPS'
         END APRO_TYPE
  FROM ( SELECT MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), @[org_cur], 'USD', @[org_amt]) TTL_USD_AMT
           FROM DUAL ) A			]]></sql>
			<params>
				<param name="org_cur" type="12" value="" out="N"/>
				<param name="org_amt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
