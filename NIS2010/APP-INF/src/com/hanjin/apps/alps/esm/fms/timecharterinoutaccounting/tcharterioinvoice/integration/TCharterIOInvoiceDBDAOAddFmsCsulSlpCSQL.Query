<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDBDAOAddFmsCsulSlpCSQL">
			<desc><![CDATA[FMS_CSUL_SLP 테이블에 데이터를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO FMS_CSUL_SLP (
	SLP_TP_CD 
,	SLP_FUNC_CD
,	SLP_OFC_CD
,	SLP_ISS_DT
,	SLP_SER_NO
,	SLP_SEQ_NO
,	ACCT_CD
,	CTR_CD
,	SLP_LOC_CD
,	CSR_CURR_CD
,	CSR_AMT
,	CSR_DESC
,	VNDR_SEQ
,	TRNS_CURR_CD
,	TRNS_AMT
,	STL_FLG
,	INV_SEQ
, FLET_SRC_TP_CD
,	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	REV_DIR_CD
,	ORG_SLP_TP_CD
,	ORG_SLP_FUNC_CD
,	ORG_SLP_OFC_CD
,	ORG_ISS_DT
,	ORG_SLP_SER_NO
,	ORG_SLP_SEQ_NO
,	LSG_GR_NO
,	TO_INV_NO
,	VVD_EFF_DT
,	VVD_EXP_DT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	ACCT_ITM_SEQ
,	OA_LOC_CD
,	OA_INV_DT
,	ATCH_FILE_OA_LNK_ID
) SELECT  
	@[slp_tp_cd]
,	@[slp_func_cd]
,	@[slp_ofc_cd]
,	@[slp_iss_dt]
,	@[slp_ser_no]
,	LPAD(NVL(TO_NUMBER(@[slp_seq_no]),0),4,'0') 
,	@[acct_cd]
,	@[ctr_cd]
,	@[slp_loc_cd]
,	@[csr_curr_cd]
,	@[csr_amt]
,	@[csr_desc]
,	@[vndr_seq]
,	'USD'
,	@[trns_amt] 
,	'N'
,	@[inv_seq]
,   '90'
, @[vsl_cd]    
, @[skd_voy_no]
, @[skd_dir_cd]
, @[rev_dir_cd]
,	@[org_slp_tp_cd]
,	@[org_slp_func_cd]
,	@[org_slp_ofc_cd]
,	@[org_iss_dt]
,	@[org_slp_ser_no]
,	@[org_slp_seq_no]
,	@[lsg_gr_no]
,	@[to_inv_no]
,	TO_DATE(REPLACE(@[vvd_eff_dt],'-',''),'yyyymmdd')
,	TO_DATE(REPLACE(@[vvd_exp_dt],'-',''),'yyyymmdd')
,	@[cre_usr_id]
,	SYSDATE
,	@[cre_usr_id]
,	SYSDATE
,	@[acct_itm_seq]
,	@[oa_loc_cd]
,	@[oa_inv_dt]
,	@[atch_file_oa_lnk_id]
FROM DUAL			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="slp_seq_no" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="ctr_cd" type="12" value="" out="N"/>
				<param name="slp_loc_cd" type="12" value="" out="N"/>
				<param name="csr_curr_cd" type="12" value="" out="N"/>
				<param name="csr_amt" type="12" value="" out="N"/>
				<param name="csr_desc" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="trns_amt" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="org_slp_tp_cd" type="12" value="" out="N"/>
				<param name="org_slp_func_cd" type="12" value="" out="N"/>
				<param name="org_slp_ofc_cd" type="12" value="" out="N"/>
				<param name="org_iss_dt" type="12" value="" out="N"/>
				<param name="org_slp_ser_no" type="12" value="" out="N"/>
				<param name="org_slp_seq_no" type="12" value="" out="N"/>
				<param name="lsg_gr_no" type="12" value="" out="N"/>
				<param name="to_inv_no" type="12" value="" out="N"/>
				<param name="vvd_eff_dt" type="12" value="" out="N"/>
				<param name="vvd_exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="acct_itm_seq" type="12" value="" out="N"/>
				<param name="oa_loc_cd" type="12" value="" out="N"/>
				<param name="oa_inv_dt" type="12" value="" out="N"/>
				<param name="atch_file_oa_lnk_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
