<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOcopyBkgCustomerByBkgCSQL">
			<desc><![CDATA[sourceBkg의 bkg_customer를 targetBkg로 복사한다.]]></desc>
			<sql><![CDATA[
insert into bkg_customer
(BKG_NO
, BKG_CUST_TP_CD
, CUST_CNT_CD
, CUST_SEQ
, CUST_NM
, CUST_ADDR
, CUST_CTY_NM
, CUST_STE_CD
, CSTMS_DECL_CNT_CD
, CUST_ZIP_ID
, CUST_FAX_NO
, CUST_EML
, CUST_TP_CD
, REF_NO
, ADDR_PRN_FLG
, VAL_NM
, VAL_FAX_NO
, VAL_CD
, VAL_USR_ID
, VAL_OFC_CD
, VAL_DT
, MTCH_FLG
, AN_SND_FLG
, CHG_DP_FLG
, IB_CUST_NM
, IB_CUST_ADDR
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
, EUR_CSTMS_ST_NM
, EORI_NO
, END_USR_NM)
select @[targetBkg] BKG_NO
, BKG_CUST_TP_CD
, CUST_CNT_CD
, CUST_SEQ
, CUST_NM
, CUST_ADDR
, CUST_CTY_NM
, CUST_STE_CD
, CSTMS_DECL_CNT_CD
, CUST_ZIP_ID
, CUST_FAX_NO
, CUST_EML
, CUST_TP_CD
, REF_NO
, ADDR_PRN_FLG
, VAL_NM
, VAL_FAX_NO
, VAL_CD
, VAL_USR_ID
, VAL_OFC_CD
, VAL_DT
, MTCH_FLG
, AN_SND_FLG
, CHG_DP_FLG
, IB_CUST_NM
, IB_CUST_ADDR
, @[usr_id] CRE_USR_ID
, sysdate CRE_DT
, @[usr_id] UPD_USR_ID
, sysdate UPD_DT
, EUR_CSTMS_ST_NM
, EORI_NO
, END_USR_NM
from bkg_customer
where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="targetBkg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
