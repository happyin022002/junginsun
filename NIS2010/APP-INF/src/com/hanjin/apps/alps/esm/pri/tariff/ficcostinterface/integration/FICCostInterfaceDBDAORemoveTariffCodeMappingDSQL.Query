<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAORemoveTariffCodeMappingDSQL">
			<desc><![CDATA[TariffCodeMapping 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_PUB_TRF_SVC_SCP_MAPG
 WHERE TRF_PFX_CD = @[trf_pfx_cd]
   AND TRF_NO = @[trf_no]
   AND SVC_SCP_CD = @[svc_scp_cd]
   AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND COST_CNT_CD = @[cost_cnt_cd]			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="cost_cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
