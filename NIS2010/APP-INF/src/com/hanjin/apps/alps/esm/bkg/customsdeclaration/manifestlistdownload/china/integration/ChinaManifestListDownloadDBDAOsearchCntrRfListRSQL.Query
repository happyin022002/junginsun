<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOsearchCntrRfListRSQL">
			<desc><![CDATA[searchCntrRfList]]></desc>
			<sql><![CDATA[
SELECT	BB.BL_NO				AS BL_NO,
	    RC.RC_SEQ				AS RF_SEQ_NO,
	    NVL(RC.CNTR_NO, ' ')	AS CNTR_NO,
	    NVL(RC.CDO_TEMP, 0)		AS CDO_TEMP,
	    @[trans_mode]			AS CHN_MF_SND_IND_CD,
	    @[usr_id]				AS CRE_USR_ID,
	    @[usr_id]				AS UPD_USR_ID
FROM	BKG_RF_CGO RC,
        BKG_BOOKING BB
WHERE   1=1
AND ( #foreach($field_id in ${field_list}) 
      	#if($velocityCount > 1)
      	OR #end      BB.BKG_NO IN ( $field_id )
      #end
)
#if (${bkg_cgo_tp_cd} != '') 
AND     DECODE(BB.BKG_CGO_TP_CD,'P','P','F') = @[bkg_cgo_tp_cd]
#end
AND	    BB.BKG_NO = RC.BKG_NO			]]></sql>
			<params>
				<param name="trans_mode" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_cgo_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
