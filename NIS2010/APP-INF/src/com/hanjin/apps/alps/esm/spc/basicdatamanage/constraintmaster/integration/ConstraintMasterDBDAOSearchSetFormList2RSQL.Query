<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOSearchSetFormList2RSQL">
			<desc><![CDATA[셀렉트박스 선택시 spc_rpt_fom 조회]]></desc>
			<sql><![CDATA[
SELECT MST.PGM_NO       AS PGM_NO,
       MST.RPT_FOM_DESC AS RPT_FOM_DESC,
       DTL.DP_SEQ       AS DP_SEQ,
       DTL.COL_NM       AS COL_NM,
       DTL.DP_NM        AS DP_NM,
	   MST.RPT_FOM_NO AS RPT_FOM_NO

  FROM SPC_RPT_FOM MST, SPC_RPT_FOM_DTL DTL
 WHERE MST.CRE_USR_ID = DTL.CRE_USR_ID(+)
   AND MST.PGM_NO = DTL.PGM_NO(+)
   AND MST.RPT_FOM_NO = DTL.RPT_FOM_NO
   AND MST.RPT_FOM_NO = @[rpt_fom_no]
   AND MST.PGM_NO = @[pgm]
   AND MST.CRE_USR_ID = @[cre_usr_id]
   AND MST.RPT_FOM_NM = @[rpt_fom_nm]
 ORDER BY DP_SEQ			]]></sql>
			<params>
				<param name="rpt_fom_no" type="12" value="" out="N"/>
				<param name="pgm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="rpt_fom_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
