<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAOAddFDRTariffDurationAmendCSQL">
			<desc><![CDATA[Add FDR Tariff Duration Amend]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_FDR_DUR (
            SVC_SCP_CD
          , ORG_DEST_TP_CD  
          , FDR_TRF_NO
          , AMDT_SEQ
          , EFF_DT
          , EXP_DT
          , CRE_USR_ID
          , CRE_DT
          , UPD_USR_ID
          , UPD_DT
         )
    SELECT SVC_SCP_CD
         , ORG_DEST_TP_CD  
         , FDR_TRF_NO
         , @[amdt_seq]+1
         , TO_DATE(@[amdt_eff], 'YYYY-MM-DD')
         , EXP_DT
         , @[cre_usr_id]
         , SYSDATE
         , @[upd_usr_id]
         , SYSDATE
      FROM PRI_TRF_FDR_DUR  
     WHERE 1=1
       AND SVC_SCP_CD = @[svc_scp_cd] 
       AND ORG_DEST_TP_CD = @[org_dest_tp_cd] 
       AND FDR_TRF_NO = @[fdr_trf_no]
       AND AMDT_SEQ   = @[amdt_seq]			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="amdt_eff" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
