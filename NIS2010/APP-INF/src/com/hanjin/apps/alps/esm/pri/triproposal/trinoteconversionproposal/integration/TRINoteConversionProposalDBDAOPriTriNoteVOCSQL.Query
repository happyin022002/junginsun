<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRINoteConversionProposalDBDAOPriTriNoteVOCSQL">
			<desc><![CDATA[PRI_TRI_NOTE INSERT]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRI_NOTE
(
	  TRF_PFX_CD
	, TRF_NO
	, NOTE_SEQ
	, EFF_DT
	, EXP_DT
	, NOTE_CONV_MAPG_ID
	, CFM_FLG
	, CFM_USR_ID
	, CFM_OFC_CD
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
) VALUES (
	  @[trf_pfx_cd]
	, @[trf_no]
	, NVL((SELECT /*+ INDEX_DESC(A XPKPRI_TRI_NOTE) */ NOTE_SEQ
             FROM PRI_TRI_NOTE A
            WHERE TRF_PFX_CD = @[trf_pfx_cd]
			  AND TRF_NO = @[trf_no]
              AND rownum = 1), 0) + 1
	, TO_DATE(@[eff_dt], 'YYYY-MM-DD')
	, TO_DATE(NVL(@[exp_dt], '9999-12-31'), 'YYYY-MM-DD')
	, @[note_conv_mapg_id]
	, NVL(@[cfm_flg],'N')
	, @[cfm_usr_id]
	, @[cfm_ofc_cd]
	, @[cre_usr_id]
	, SYSDATE
	, @[upd_usr_id]
	, SYSDATE
)			]]></sql>
			<params>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="note_conv_mapg_id" type="12" value="" out="N"/>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="cfm_usr_id" type="12" value="" out="N"/>
				<param name="cfm_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
