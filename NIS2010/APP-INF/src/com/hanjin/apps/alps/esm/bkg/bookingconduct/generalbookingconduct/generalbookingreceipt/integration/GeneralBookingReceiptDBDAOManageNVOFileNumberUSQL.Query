<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOManageNVOFileNumberUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')
UPDATE BKG_USA_CSTMS_FILE_NO_HIS SET 
	PCK_QTY = nvl(@[pck_qty], 0)
,	SCAC_CD = @[scac_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	BKG_NO = @[bkg_no]
AND CORR_NO = 'TMP0000001'
AND	USA_CSTMS_FILE_NO = @[usa_cstms_file_no]
#else
UPDATE BKG_USA_CSTMS_FILE_NO SET 
	PCK_QTY = nvl(@[pck_qty], 0)
,	SCAC_CD = @[scac_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	BKG_NO = @[bkg_no]
AND	USA_CSTMS_FILE_NO = @[usa_cstms_file_no]
#end			]]></sql>
			<params>
				<param name="pck_qty" type="6" value="" out="N"/>
				<param name="scac_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="usa_cstms_file_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
