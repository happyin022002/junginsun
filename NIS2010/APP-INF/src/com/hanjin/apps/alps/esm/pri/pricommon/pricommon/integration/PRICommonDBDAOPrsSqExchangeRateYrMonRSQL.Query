<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOPrsSqExchangeRateYrMonRSQL">
			<desc><![CDATA[pri_sp_scp_mn prs_xch_rt_yrmon 을 조회한다. 조회조건 (prop_no , amdt_seq , svc_scp_cd )]]></desc>
			<sql><![CDATA[
select prs_xch_rt_yrmon as cd,
	 prs_xch_rt_yrmon as nm
from pri_sq_mn 
where qttn_no = @[etc1]
and qttn_ver_no = @[etc2]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="etc3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
