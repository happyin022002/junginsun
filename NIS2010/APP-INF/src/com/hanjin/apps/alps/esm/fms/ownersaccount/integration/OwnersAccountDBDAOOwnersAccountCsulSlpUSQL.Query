<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOOwnersAccountCsulSlpUSQL">
			<desc><![CDATA[전표 취소 시 원전표의 CSR_NO 를 PAIR로 넣어준다   ]]></desc>
			<sql><![CDATA[
UPDATE FMS_CSUL_SLP
   SET PAIR_SLP_TP_CD = @[pair_slp_tp_cd],
       PAIR_SLP_FUNC_CD = @[pair_slp_func_cd],
       PAIR_SLP_OFC_CD = @[pair_slp_ofc_cd],
       PAIR_SLP_ISS_DT = @[pair_slp_iss_dt],
       PAIR_SLP_SER_NO = @[pair_slp_ser_no],
       PAIR_SLP_SEQ_NO = @[pair_slp_seq_no]
 WHERE SLP_TP_CD||SLP_FUNC_CD||SLP_OFC_CD||SLP_ISS_DT||SLP_SER_NO||SLP_SEQ_NO = @[slp_tp_cd]||@[slp_func_cd]||@[slp_ofc_cd]||@[slp_iss_dt]||@[slp_ser_no]||@[slp_seq_no]			]]></sql>
			<params>
				<param name="pair_slp_tp_cd" type="12" value="" out="N"/>
				<param name="pair_slp_func_cd" type="12" value="" out="N"/>
				<param name="pair_slp_ofc_cd" type="12" value="" out="N"/>
				<param name="pair_slp_iss_dt" type="12" value="" out="N"/>
				<param name="pair_slp_ser_no" type="12" value="" out="N"/>
				<param name="pair_slp_seq_no" type="12" value="" out="N"/>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="slp_seq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
