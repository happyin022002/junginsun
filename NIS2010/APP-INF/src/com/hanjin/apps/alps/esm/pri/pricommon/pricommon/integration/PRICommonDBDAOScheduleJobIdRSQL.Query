<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOScheduleJobIdRSQL">
			<desc><![CDATA[batch job 실행후 program 명과 파라메터를 이용해 job id를 조회한다.]]></desc>
			<sql><![CDATA[
select 
	max(skd_id) as cd, '' as nm
from  com_skd_mgmt
where pgm_no = @[etc1]
	and skd_para_ctnt = @[etc2]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
