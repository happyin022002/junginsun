<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtCMPBCostDetailVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE  PRI_RP_SCP_RT
   SET  PRS_RESPB_CMPB_AMT = PROP_FRT_RT_AMT + PRS_SCG_AMT - PRS_RESPB_CM_UC_AMT
       ,PRS_PFIT_CMPB_AMT = PROP_FRT_RT_AMT + PRS_SCG_AMT - PRS_PFIT_CM_UC_AMT
       ,PRS_RESPB_OPB_AMT = PROP_FRT_RT_AMT + PRS_SCG_AMT - PRS_RESPB_OPFIT_UC_AMT
 WHERE  PROP_NO = @[prop_no]
   AND  AMDT_SEQ = @[amdt_seq]
   AND  SVC_SCP_CD = @[svc_scp_cd]
   AND  CMDT_HDR_SEQ = @[cmdt_hdr_seq]
   AND  ROUT_SEQ = @[rout_seq]
   AND  RT_SEQ = @[rt_seq]

			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
