<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USDomesticDBDAOAddDomSavUSQL">
			<desc><![CDATA[AddDovSav]]></desc>
			<sql><![CDATA[
-- PER BOX당 saving 금액
UPDATE MAS_USA_DMST_UT_COST
   SET INIT_USA_DMST_SAV_UT_AMT	= (INIT_SIM_MTY_UC_AMT * RAIL_SO_VOL_QTY - USA_DMST_UC_AMT * RAIL_SO_VOL_QTY) / FCNTR_IB_VOL_QTY
     , USA_DMST_SAV_UT_AMT	= (INIT_SIM_MTY_UC_AMT * RAIL_SO_VOL_QTY - USA_DMST_UC_AMT * RAIL_SO_VOL_QTY) / FCNTR_IB_VOL_QTY
     , TRP_SAV_AMT		= (SIM_MTY_UC_AMT * TRP_QTY) - TRP_AMT
     , TRP_CR_UC_AMT		= ((SIM_MTY_UC_AMT * TRP_QTY) - TRP_AMT) / FCNTR_IB_VOL_QTY
     , TRP_CR_UC_INIT_AMT	= ((SIM_MTY_UC_AMT * TRP_QTY) - TRP_AMT) / FCNTR_IB_VOL_QTY
     , EQ_OFFH_SAV_AMT		= (SIM_MTY_UC_AMT * OFFH_TTL_QTY)
     , EQ_OFFH_SAV_UC_AMT	= (SIM_MTY_UC_AMT * OFFH_TTL_QTY) / FCNTR_IB_VOL_QTY
     , EQ_OFFH_SAV_UC_INIT_AMT	= (SIM_MTY_UC_AMT * OFFH_TTL_QTY) / FCNTR_IB_VOL_QTY
     , EQ_OFFH_FNL_UC_AMT	= ((SIM_MTY_UC_AMT * OFFH_TTL_QTY) / FCNTR_IB_VOL_QTY) - (DMST_RAIL_INV_AMT / FCNTR_IB_VOL_QTY)
     , EQ_OFFH_FNL_UC_INIT_AMT	= ((SIM_MTY_UC_AMT * OFFH_TTL_QTY) / FCNTR_IB_VOL_QTY) - (DMST_RAIL_INV_AMT / FCNTR_IB_VOL_QTY)

 WHERE COST_YRMON = REPLACE(@[f_cost_yrmon], '-', '')			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
