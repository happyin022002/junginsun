<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchDoVtyUptoDTHisRSQL">
			<desc><![CDATA[Do validity upto (AE 국가에서만 쓰임) 수정한 history 조회해옴]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(DO_VTY_DT,'YYYY-MM-DD') DO_VTY_DT, 
       TO_CHAR(UPD_DT,'YYYY-MM-DD') UPD_DT,
       UPD_USR_ID
FROM BKG_AE_VTY_DT_HIS
WHERE BKG_NO = @[bkg_no] 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
