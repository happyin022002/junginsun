<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRealCustomerProposalDBDAOAddcopyRealCustomerCSQL">
			<desc><![CDATA[Real Customer 데이터를 Copy 합니다.]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_REAL_CUST(
       PROP_NO
      ,AMDT_SEQ
      ,REAL_CUST_SEQ
      ,CUST_CNT_CD
      ,CUST_SEQ
	  ,CUST_VAL_SGM_CD
      ,PRC_CTRT_CUST_TP_CD
      ,CUST_SREP_CD
      ,CUST_SLS_OFC_CD
      ,REP_CUST_FLG
	  ,CUST_LOC_CD
	  ,CRE_USR_ID
      ,CRE_DT
	  ,UPD_USR_ID
      ,UPD_DT
      )              
SELECT
           @[new_prop_no]
          ,'0'
          ,REAL_CUST_SEQ
          ,CUST_CNT_CD
          ,CUST_SEQ
          ,CUST_VAL_SGM_CD
          ,PRC_CTRT_CUST_TP_CD
          ,CUST_SREP_CD
          ,CUST_SLS_OFC_CD
          ,REP_CUST_FLG
		  ,CUST_LOC_CD
          ,@[cre_usr_id]
          ,SYSDATE
          ,@[upd_usr_id]
          ,SYSDATE
FROM PRI_SP_REAL_CUST
WHERE 1=1
AND PROP_NO  = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
