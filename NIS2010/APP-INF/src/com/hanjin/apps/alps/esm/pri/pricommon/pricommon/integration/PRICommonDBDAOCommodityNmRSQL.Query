<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOCommodityNmRSQL">
			<desc><![CDATA[Commodity Name select]]></desc>
			<sql><![CDATA[
SELECT CMDT_NM
FROM MDM_COMMODITY
WHERE CMDT_CD = @[cmdt_cd]
AND   DELT_FLG ='N'			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
