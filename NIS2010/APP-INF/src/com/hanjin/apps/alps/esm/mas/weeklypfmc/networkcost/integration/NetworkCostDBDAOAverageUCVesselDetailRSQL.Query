<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOAverageUCVesselDetailRSQL">
			<desc><![CDATA[AverageUCVesselDetail]]></desc>
			<sql><![CDATA[
SELECT COST_YRMON
     , STND_COST_CD
     , VSL_CLSS_CAPA
     , VSL_CD
     , DHIR_AMT
     , VSL_KNT
     , UPD_USR_ID
     , SUM(DECODE(DHIR_AMT,0,0,VSL_KNT)) OVER(PARTITION BY COST_YRMON, STND_COST_CD, VSL_CLSS_CAPA) AS GRP_KNT
     , EFF_YRMON
     , EFF_WK
  FROM MAS_POOL_VSL_DLY_HIR
 WHERE VSL_CD      != 'XXXX'
   AND COST_YRMON   = REPLACE(@[f_yearweek],'-','')
   AND STND_COST_CD = @[f_cobcost] 
#if(${f_vsl_cd} != '')
   AND VSL_CD       = @[f_vsl_cd]
#end
ORDER BY VSL_CLSS_CAPA, VSL_CD			]]></sql>
			<params>
				<param name="f_yearweek" type="12" value="" out="N"/>
				<param name="f_cobcost" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
