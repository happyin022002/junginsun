<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndExpCustomsTransmissionDBDAOsearchVesselEtaRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT * 
FROM
(
SELECT 
  @[snd_dt] CURRDATE,
  #if (${hidden_vvd} != '')
  SUBSTR(@[hidden_vvd],1,4) AS VSL_CD,
  SUBSTR(@[hidden_vvd],5,4) AS SKD_VOY_NO,
  SUBSTR(@[hidden_vvd],9,1) AS SKD_DIR_CD,
  #else
  VPS.VSL_CD ,
  VPS.SKD_VOY_NO,
  VPS.SKD_DIR_CD,
  #end
  V.CALL_SGN_NO,
  V.LLOYD_NO,
  V.VSL_ENG_NM,
  NVL( DECODE( VL1.UN_LOC_IND_CD, 'Y', VL1.LOC_CD, VL1.UN_LOC_CD) , VPS.VPS_PORT_CD ) VPS_PORT_CD,
  TO_CHAR(VPS.VPS_ETA_DT, 'YYMMDDHH24MI') VPS_ETA_DT,
  TO_CHAR(VPS.VPS_ETD_DT, 'YYMMDDHH24MI') VPS_ETD_DT,
  NVL( DECODE( VL2.UN_LOC_IND_CD, 'Y', VL2.LOC_CD, VL2.UN_LOC_CD) , VPS2.VPS_PORT_CD ) CA_PORT_CD,
  TO_CHAR(VPS2.VPS_ETA_DT, 'YYMMDDHH24MI') CA_ETA_DT,
  (SELECT CVY_REF_NO
   FROM BKG_CSTMS_CND_VSL
   WHERE 1=1
   #if (${hidden_vvd} != '')
   AND VSL_CD = SUBSTR(@[hidden_vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[hidden_vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[hidden_vvd], 9, 1)
   #else
   AND VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   #end
   AND PORT_CD = @[vps_port_cd]) CVY_REF_NO ,
  CRR_CD,
  NVL(SUBSTR(CND.CVY_REF_NO,1,4),'') CRR_CODE,
  #if (${hidden_vvd} != '')
  (SELECT VSL_CD||IB_CSSM_VOY_NO
   FROM VSK_VSL_PORT_SKD VPS
   WHERE 1=1
   AND VSL_CD = SUBSTR(@[hidden_vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[hidden_vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[hidden_vvd], 9, 1)
   AND VPS_PORT_CD = @[lastpol]
   AND CLPT_IND_SEQ = @[lpol_clpt_ind_seq]) AS CON_VVD
  #else
  VPS.VSL_CD||VPS.IB_CSSM_VOY_NO AS CON_VVD
  #end
FROM VSK_VSL_PORT_SKD VPS, MDM_VSL_CNTR V, VSK_VSL_PORT_SKD VPS2, MDM_LOCATION VL1,MDM_LOCATION VL2, BKG_CSTMS_CND_VSL CND
WHERE VPS.VSL_CD = SUBSTR(@[vvd], 1, 4)
  AND VPS.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
  AND VPS.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
  AND VPS.VPS_PORT_CD = @[lastpol]
  AND NVL(VPS.SKD_CNG_STS_CD, 'X') != 'S'
  AND VPS.VSL_CD = V.VSL_CD
  AND VPS.VSL_CD = VPS2.VSL_CD
  AND VPS.SKD_VOY_NO = VPS2.SKD_VOY_NO
  AND VPS.SKD_DIR_CD = VPS2.SKD_DIR_CD
  AND VPS2.CLPT_SEQ > VPS.CLPT_SEQ
  AND VPS2.CLPT_IND_SEQ > 0
  AND VPS2.VPS_PORT_CD LIKE 'CA%'
  AND NVL(VPS2.SKD_CNG_STS_CD, 'X') != 'S'
  AND VPS.VPS_PORT_CD = VL1.LOC_CD(+)
  AND VPS2.VPS_PORT_CD = VL2.LOC_CD(+)
  AND VPS.VSL_CD = CND.VSL_CD(+)
  AND VPS.SKD_VOY_NO = CND.SKD_VOY_NO(+)
  AND VPS.SKD_DIR_CD = CND.SKD_DIR_CD(+)
ORDER BY VPS2.VPS_ETA_DT
)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="hidden_vvd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="lastpol" type="12" value="" out="N"/>
				<param name="lpol_clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
