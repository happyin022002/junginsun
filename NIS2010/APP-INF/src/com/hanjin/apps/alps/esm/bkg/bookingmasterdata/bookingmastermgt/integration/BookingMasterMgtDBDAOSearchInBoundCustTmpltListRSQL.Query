<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchInBoundCustTmpltListRSQL">
			<desc><![CDATA[ 0192 B/L Customer Information Template 조회			]]></desc>
			<sql><![CDATA[
SELECT     
   UPPER(A.CUST_CNT_CD) AS CUST_CNT_CD
,  UPPER(A.CUST_SEQ) AS CUST_SEQ
,  UPPER(A.TMPLT_SEQ) AS TMPLT_SEQ
,  UPPER(A.CUST_NM) AS CUST_NM
,  UPPER(A.CUST_ADDR) AS CUST_ADDR
,  UPPER(A.CUST_PHN_NO) AS CUST_PHN_NO
,  UPPER(A.ATND_NM) AS PIC
,  UPPER(A.CRE_USR_ID) AS CRE_USR_ID
,  UPPER(A.CUST_FAX_NO) AS CUST_FAX_NO
,  UPPER(A.CUST_CTY_NM) AS CUST_CTY_NM
,  UPPER(A.CUST_STE_CD) AS CUST_STE_CD
,  UPPER(A.CUST_ZIP_CD) AS CUST_ZIP_CD
,  UPPER(A.CUST_EML) AS CUST_EML
,  UPPER(A.TMPLT_RMK) AS TMPLT_RMK
,  UPPER(A.UPD_USR_ID) AS UPD_USR_ID
,  A.CSTMS_DECL_CNT_CD AS CSTMS_DECL_CNT_CD
, UPPER(A.EUR_CSTMS_ST_NM) AS EUR_CSTMS_ST_NM
, UPPER(A.EORI_NO) AS EORI_NO
,  B.LOC_CD || '-' || B.LOC_NM as LOCATION_CODE
FROM   BKG_CUST_TMPLT A
        ,MDM_LOCATION B
        ,MDM_CUSTOMER C
WHERE A.CUST_CNT_CD = C.CUST_CNT_CD
AND   A.CUST_SEQ    = C.CUST_SEQ
AND   C.LOC_CD = B.LOC_CD(+)

AND A.CUST_CNT_CD = @[cust_cnt_cd]
AND   A.CUST_SEQ    = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
