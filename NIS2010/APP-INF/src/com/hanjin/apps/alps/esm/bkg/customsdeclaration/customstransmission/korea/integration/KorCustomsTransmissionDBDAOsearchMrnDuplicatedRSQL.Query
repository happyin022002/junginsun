<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOsearchMrnDuplicatedRSQL">
			<desc><![CDATA[한국 세관 MRN 중복 조회]]></desc>
			<sql><![CDATA[
SELECT VSL_CD 
,SKD_VOY_NO
,SKD_DIR_CD 
,PORT_CD		
,IO_BND_CD		
FROM BKG_CSTMS_KR_MF_REF_NO				
WHERE MRN_NO = @[mrn_no]
AND IO_BND_CD = @[io_bnd_cd]
ORDER BY CRE_DT DESC			]]></sql>
			<params>
				<param name="mrn_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
