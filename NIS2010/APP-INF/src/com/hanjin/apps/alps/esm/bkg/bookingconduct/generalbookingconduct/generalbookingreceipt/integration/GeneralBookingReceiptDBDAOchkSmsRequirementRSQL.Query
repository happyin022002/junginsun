<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOchkSmsRequirementRSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOchkSmsRequirementRSQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM BKG_CSTMS_TML_EDI_SND_LOG A
 WHERE A.BKG_NO = @[bkg_no]
   AND A.FNL_EDI_SND_FLG = 'Y'
   AND A.POL_CD IN ('KRPUS', 'KRKAN')
   AND EXISTS (SELECT 'X'
                 FROM BKG_BOOKING C
                WHERE C.POL_CD IN ('KRPUS', 'KRKAN')
                  AND C.BKG_NO = @[bkg_no]) 
   AND A.KR_CLL_TS_CD IS NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
