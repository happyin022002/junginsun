<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchDischCoListRSQL">
			<desc><![CDATA[한국 세관에 적하 목록 전송시 Discharging Company 조회]]></desc>
			<sql><![CDATA[
SELECT
PORT_CD,
CSTMS_DCHG_CO_ID,
CSTMS_DCHG_CO_NM
FROM BKG_CSTMS_DCHG_CO
WHERE 1=1
#if(${port_cd} != '')
AND PORT_CD = @[port_cd]
#end			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
