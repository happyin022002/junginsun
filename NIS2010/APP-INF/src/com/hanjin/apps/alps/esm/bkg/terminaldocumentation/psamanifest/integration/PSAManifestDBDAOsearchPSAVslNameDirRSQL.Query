<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPSAVslNameDirRSQL">
			<desc><![CDATA[PSA VSL Name과 Voyage Dir을 조회]]></desc>
			<sql><![CDATA[
SELECT UPPER(TRIM(PSA_VSL_NM)) PSA_VSL_NM
     , UPPER(TRIM(PSA_VOY_DIR_CD)) PSA_VOY_DIR_CD
  FROM BKG_CSTMS_PSA_VVD
 WHERE VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
