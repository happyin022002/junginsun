<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOaddCmdInfoCSQL">
			<desc><![CDATA[ROCS(ROTTERDAM) 세관 신고용 신규 CMD(Cargo) 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_RTM_CGO_MF (
	CNTR_NO
,	CNTR_MF_SEQ
,	BKG_NO
,	VSL_CALL_REF_NO
,	PCK_QTY
,	PCK_TP_CD
,	PCK_DESC
,	CNTR_MF_WGT
,	CNTR_WGT_UT_CD
,	CNTR_MF_MEAS_QTY
,	CMDT_MEAS_UT_CD
,	CNTR_MF_MK_DESC
,	CNTR_MF_DESC
,	HAMO_TRF_CD
,	EDI_SEQ
,	CRE_OFC_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[cntr_no]
,	NVL((SELECT MAX(CNTR_MF_SEQ)+1 FROM BKG_CSTMS_RTM_CGO_MF WHERE CNTR_NO=@[cntr_no] AND BKG_NO=@[bkg_no] AND VSL_CALL_REF_NO=@[vsl_call_ref_no]),1)
,	NVL(@[bkg_no],'')
,	NVL(@[vsl_call_ref_no],'')
,	NVL(@[pck_qty],0)
,	@[pck_tp_cd]
,	@[pck_desc]
,	NVL(@[cntr_mf_wgt],0)
,	@[cntr_wgt_ut_cd]
,	NVL(@[cntr_mf_meas_qty],0)
,	@[cmdt_meas_ut_cd]
,	@[cntr_mf_mk_desc]
,	@[cntr_mf_desc]
,	@[hamo_trf_cd]
,	NVL((SELECT MAX(EDI_SEQ)+1 FROM BKG_CSTMS_RTM_CGO_MF WHERE BKG_NO=@[bkg_no] AND VSL_CALL_REF_NO=@[vsl_call_ref_no]),1)
,	@[ofc_cd]
,	@[user_id]
,	sysdate
,	@[user_id]
,	sysdate
)			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="vsl_call_ref_no" type="12" value="" out="N"/>
				<param name="pck_qty" type="6" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="pck_desc" type="12" value="" out="N"/>
				<param name="cntr_mf_wgt" type="6" value="" out="N"/>
				<param name="cntr_wgt_ut_cd" type="12" value="" out="N"/>
				<param name="cntr_mf_meas_qty" type="6" value="" out="N"/>
				<param name="cmdt_meas_ut_cd" type="12" value="" out="N"/>
				<param name="cntr_mf_mk_desc" type="12" value="" out="N"/>
				<param name="cntr_mf_desc" type="12" value="" out="N"/>
				<param name="hamo_trf_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
