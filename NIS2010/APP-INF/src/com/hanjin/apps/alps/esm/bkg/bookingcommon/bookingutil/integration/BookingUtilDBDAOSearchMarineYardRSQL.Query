<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchMarineYardRSQL">
			<desc><![CDATA[pol, pod는 marine terminal이어야 한다]]></desc>
			<sql><![CDATA[
select COUNT(1) CNT 
  from mdm_yard 
 where loc_cd = SUBSTR(@[locCd], 1, 5)
#if (${ydCd}!= '')
AND		YD_CD = @[ydCd]
#end
   and YD_CHR_CD ='N'
   and (YD_FCTY_TP_MRN_TML_FLG = 'Y' or YD_FCTY_TP_BRG_RMP_FLG = 'Y')
   and delt_flg = 'N'
   and rownum = 1
			]]></sql>
			<params>
				<param name="locCd" type="12" value="" out="N"/>
				<param name="ydCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
