<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAO021SpcTeamQtaRtoByVvdRSQL">
			<desc><![CDATA[항차별 Team QTA Ratio를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT TRD_CD, 
       SUB_TRD_CD, 
       RLANE_CD, 
       SLS_REP_OFC_TEAM_CD, 
       VSL_CD,
       SKD_VOY_NO,
       SKD_DIR_CD,
       TEAM_QTA_RTO
  FROM SPC_TEAM_QTA_RTO
 WHERE QTA_APLY_CD = @[type]
#if(${trade} != '')
   AND TRD_CD      = @[trade]
#end
#if(${subtrade} != '')
   AND SUB_TRD_CD  = @[subtrade]
#end
#if(${lane} != '')
   AND RLANE_CD    = @[lane]
#end
 ORDER BY TRD_CD, 
          SUB_TRD_CD, 
          RLANE_CD, 
          DECODE(SLS_REP_OFC_TEAM_CD, 'OTHER', 0, 'SELBK', 1, 'SELBS', 2, 3)			]]></sql>
			<params>
				<param name="type" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="subtrade" type="12" value="" out="N"/>
				<param name="lane" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
