<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchEdiPsaEdoRecTpRSQL">
			<desc><![CDATA[PSA D/O Release Order EDI  전송 Record type and Fuction 값을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN BKG_CGO_TP_CD IN ('F','P') THEN DECODE(@[event_tp],'RL','FN','AT','FU','CC','FD')
              WHEN BKG_CGO_TP_CD ='R' THEN DECODE(@[event_tp],'RL','EN','AT','EU','CC','ED')
              ELSE ''
       END AS REC_TP
FROM BKG_BOOKING
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="event_tp" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
