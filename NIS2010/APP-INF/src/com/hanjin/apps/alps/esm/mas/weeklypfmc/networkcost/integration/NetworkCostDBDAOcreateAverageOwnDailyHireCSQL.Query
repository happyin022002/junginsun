<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOcreateAverageOwnDailyHireCSQL">
			<desc><![CDATA[2012.08.14 이석준 [CHM-201219592-01] AVG-hire by Own-VSL (PA) 화면에 Load excel 기능 추가]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_OWN_VSL_DLY_HIR
 (COST_YRMON,
VSL_CD,
VSL_CLSS_CAPA,
STND_COST_CD,
DHIR_AMT,
OWN_VSL_RMK,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID,
UPD_DT)
 SELECT COST_YRMON
               ,'XXXX' AS VSL_CD
              ,VSL_CLSS_CAPA
              ,STND_COST_CD
              ,SUM(DHIR_AMT)/COUNT(DISTINCT VSL_CD) AMT
              ,'AVG'
              ,@[user_id]
              ,SYSDATE
              ,@[user_id]
              ,SYSDATE
      FROM MAS_OWN_VSL_DLY_HIR
    WHERE COST_YRMON = @[f_yearweek]
    GROUP BY COST_YRMON
              , VSL_CLSS_CAPA
              , STND_COST_CD			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_yearweek" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
