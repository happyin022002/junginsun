<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeAutoRatingDBDAOCheckObsSurchargeRatingUSQL">
			<desc><![CDATA[O/BL Surrender 처리시 OBS Charge 정보가 존재하는지 체크한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y') OBS_EXIST
FROM BKG_CHG_RT 
WHERE BKG_NO=@[bkg_no]
AND CHG_CD = 'OBS'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
