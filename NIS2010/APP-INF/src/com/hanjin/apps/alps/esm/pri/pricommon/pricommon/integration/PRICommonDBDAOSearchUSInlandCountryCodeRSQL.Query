<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOSearchUSInlandCountryCodeRSQL">
			<desc><![CDATA[search US Inland country code ( Add-on management T/F Project )]]></desc>
			<sql><![CDATA[
SELECT CNT_CD AS CD
     , CNT_NM AS NM
  FROM MDM_COUNTRY
 WHERE 1=1
   AND CNT_CD IN ('US','CA')
ORDER BY CNT_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
