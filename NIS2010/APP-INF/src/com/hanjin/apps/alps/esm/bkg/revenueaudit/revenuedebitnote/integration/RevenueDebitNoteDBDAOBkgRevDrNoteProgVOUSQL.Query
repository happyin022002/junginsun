<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOBkgRevDrNoteProgVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE BKG_REV_DR_NOTE_PROG SET 
	RDN_RMK = @[rdn_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	RDN_NO = @[rdn_no]
AND	RVIS_SEQ = @[rvis_seq]
AND	PROG_SEQ = (SELECT MAX(PROG_SEQ)
                FROM   BKG_REV_DR_NOTE_PROG
                WHERE  RDN_NO = @[rdn_no]
                AND	   RVIS_SEQ = @[rvis_seq]
                AND    RDN_STS_CD = @[rdn_sts_cd])			]]></sql>
			<params>
				<param name="rdn_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rdn_no" type="12" value="" out="N"/>
				<param name="rvis_seq" type="2" value="" out="N"/>
				<param name="rdn_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
