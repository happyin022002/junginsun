<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MyanmarCustomsTransmissionDBDAOSearchManifestBlInfoRSQL">
			<desc><![CDATA[SearchManifestBlInfo]]></desc>
			<sql><![CDATA[
SELECT MEAS_QTY
     , MEAS_UT_CD
	 , PCK_QTY
	 , (SELECT PCK_NM FROM MDM_PCK_TP WHERE PCK_CD = C.PCK_TP_CD) PCK_TP_CD
	 , ACT_WGT
	 , WGT_UT_CD
	 , (SELECT SUM(OP_CNTR_QTY)
		  FROM BKG_QUANTITY
		 WHERE BKG_NO = @[bkg_no]) OP_CNTR_QTY
     , TO_CHAR((SELECT OBL_ISS_DT
                  FROM BKG_BL_ISS
                 WHERE BKG_NO = @[bkg_no]),'YYYYMMDD') OBL_ISS_DT
     , TO_CHAR((SELECT BL_OBRD_DT
                  FROM BKG_BL_DOC
                 WHERE BKG_NO = @[bkg_no]),'YYYYMMDD') BL_OBRD_DT
FROM BKG_BL_DOC C
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
