<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchStwgPlanRSQL">
			<desc><![CDATA[SearchStwgPlan]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
    substr(stwg.CNTR_REF_NO,1,10) CNTR_NO 
  , substr(stwg.CNTR_REF_NO,11,1) CNTR_NO_PST
  , stwg.CNTR_REF_NO     FULL_CNTR_NO
  , stwg.CNTR_TPSZ_CD    TPSZ_CD
  , cntr.cnmv_STS_CD
  , stwg.POD_CD      POD_CD
  , cntr.pre_sts_flg
  FROM OPF_BAY_PLN_LDIS stwg, mst_container cntr
 WHERE stwg.VSL_CD     = SUBSTR(@[vvd], 0,4)
   AND stwg.SKD_VOY_NO = SUBSTR(@[vvd], 5,4)
   AND stwg.SKD_DIR_CD = SUBSTR(@[vvd], 9,1)
   AND stwg.POL_CD     = SUBSTR(@[yd_cd], 0, 5)
   AND stwg.CRR_CD     = 'HJS'
   AND stwg.LODG_DCHG_IND_CD = 'L'
   and stwg.cntr_ref_no = cntr.cntr_no(+)
   AND DECODE(stwg.FULL_MTY_CD, 'E', 'P', 'M','P','F') = 'P'
ORDER BY cntr.pre_sts_flg			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
