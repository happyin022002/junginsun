<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOmanageBlCertiStsUSQL">
			<desc><![CDATA[BLIssuanceDBDAOmanageBlCertiStsUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_CERTI
SET BL_CERTI_STS_CD = @[bl_certi_sts_cd]
   ,APRO_DT = DECODE(@[bl_certi_sts_cd],'A',sysdate,'')
   ,RJCT_DT = DECODE(@[bl_certi_sts_cd],'X',sysdate,'')
   ,RJCT_RSN_RMK = DECODE(@[bl_certi_sts_cd],'X',@[rjct_rsn_rmk],RJCT_RSN_RMK)
   ,UPD_DT = sysdate
   ,UPD_USR_ID = @[upd_usr_id]
WHERE BL_NO = @[bl_no]
  AND BL_CERTI_SEQ = @[bl_certi_seq]			]]></sql>
			<params>
				<param name="bl_certi_sts_cd" type="12" value="" out="N"/>
				<param name="rjct_rsn_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bl_certi_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
