<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScOftAutoRatingDAOSearchSurchargeOftFrightListRSQL">
			<desc><![CDATA[ScOftAutoRatingDBDAOSearchSurchargeOftFrightListRSQL
BKG_AUTO_RT_OCN_FRT_TMP 을 SELECT 한다]]></desc>
			<sql><![CDATA[
SELECT 
A.BKG_NO BKG_NO,
A.OFT_CMB_SEQ OFT_CMB_SEQ,
A.OFRT_SEQ OFRT_SEQ,
A.CHG_CD CHG_CD,
A.CHG_UT_AMT CHG_UT_AMT,
A.CURR_CD CURR_CD,
A.CHG_AMT CHG_AMT,
RAT_AS_QTY RAT_AS_QTY,
(SELECT MAX(OFT_CMB_SEQ) FROM BKG_AUTO_RT_OCN_FRT_TMP) MAX_OFT_CMB_SEQ,
(SELECT MAX(OFRT_SEQ) FROM BKG_AUTO_RT_OCN_FRT_TMP) MAX_OFRT_SEQ,
K.SUM_AMOUNT
FROM BKG_AUTO_RT_OCN_FRT_TMP A, 
	(SELECT BKG_NO,OFT_CMB_SEQ,
	SUM(CHG_AMT) SUM_AMOUNT 
	FROM BKG_AUTO_RT_OCN_FRT_TMP
	GROUP BY BKG_NO,OFT_CMB_SEQ ) K
WHERE A.BKG_NO = K.BKG_NO
AND A.OFT_CMB_SEQ = K.OFT_CMB_SEQ
ORDER BY OFRT_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
