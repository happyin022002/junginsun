<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAORsltPriSurchargeViewAllVORSQL">
			<desc><![CDATA[  RFA Quotation Surcharge View All]]></desc>
			<sql><![CDATA[

SELECT  
	A.QTTN_NO
	,A.QTTN_VER_NO
	,A.CMDT_HDR_SEQ
	,A.ROUT_SEQ
	,RT.RT_SEQ
	,I.PRC_CMDT_DEF_NM
	,NVL(B.ROUT_PNT_LOC_DEF_CD ,' ') AS ORG_ROUT_PNT_LOC_DEF_CD
	,NVL(C.ROUT_VIA_PORT_DEF_CD ,' ') AS ORG_ROUT_VIA_PORT_DEF_CD
	,NVL(D.ROUT_VIA_PORT_DEF_CD ,' ') AS DEST_ROUT_VIA_PORT_DEF_CD
	,NVL(E.ROUT_PNT_LOC_DEF_CD ,' ') AS DEST_ROUT_PNT_LOC_DEF_CD


	,RT.RAT_UT_CD
	,RT.PRC_CGO_TP_CD
	,RT.CURR_CD AS RATE_CURR_CD
	
	, RT.QTTN_RT_AMT AS  FRT_RT_AMT
	
	, NVL( SCG.POR_CD ,' ') AS POR_CD
	, NVL( SCG.POL_CD ,' ') AS POL_CD
	, NVL( SCG.POD_CD ,' ') AS POD_CD
	, NVL( SCG.DEL_CD ,' ') AS DEL_CD
	, SCG.TOT_SURCHARGE
	, SCG.CRE_YMD 
	, SCG.CHG_CD 
	, SCG.BKG_RAT_UT_CD
	, SCG.CURR_CD AS SURCHARGE_CURR_CD
	, SCG.TRF_SCG_AMT
	, SCG.ADJ_SCG_AMT
	, SCG.ADJ_SCG_USD_AMT
	,A.CRE_USR_ID
	,A.CRE_DT
	,A.UPD_USR_ID
	,A.UPD_DT
	, DENSE_RANK() OVER (PARTITION BY A.QTTN_NO, A.QTTN_VER_NO ORDER BY A.QTTN_NO, A.QTTN_VER_NO, A.CMDT_HDR_SEQ ) AS SEQ
  FROM PRI_RQ_RT_CMDT_ROUT A
      ,(
		SELECT QTTN_NO
		      ,QTTN_VER_NO
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD ||  DECODE(RCV_DE_TERM_CD, NULL,'', '(' || TERM_NM || ')' ), ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
		  FROM (SELECT QTTN_NO
			      ,QTTN_VER_NO
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_PNT_LOC_DEF_CD 
			      ,ROUT_PNT_LOC_TP_CD
			      ,RCV_DE_TERM_CD
			      ,SRC_INFO_CD
			      ,ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
			      ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL CODE WHERE CODE.INTG_CD_ID = 'CD02070' AND INTG_CD_VAL_CTNT = RCV_DE_TERM_CD) as TERM_NM 
			  FROM PRI_RQ_RT_ROUT_PNT
			 WHERE QTTN_NO = @[qttn_no]
			   AND QTTN_VER_NO = @[qttn_ver_no]
			   AND ORG_DEST_TP_CD = 'O'
		 )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY QTTN_NO
			 ,QTTN_VER_NO
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD
	) B
      ,(
		SELECT QTTN_NO
		      ,QTTN_VER_NO
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
		  FROM (SELECT QTTN_NO
			      ,QTTN_VER_NO
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_VIA_PORT_DEF_CD
			      ,ROUT_VIA_PORT_TP_CD
			      ,SRC_INFO_CD
			      ,ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
			  FROM PRI_RQ_RT_ROUT_VIA
			 WHERE QTTN_NO = @[qttn_no]
			   AND QTTN_VER_NO = @[qttn_ver_no]
			   AND ORG_DEST_TP_CD = 'O'
		 )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY QTTN_NO
			 ,QTTN_VER_NO
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD	
	) C
      ,(
		SELECT QTTN_NO
		      ,QTTN_VER_NO
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD  , ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
		  FROM (SELECT QTTN_NO
			      ,QTTN_VER_NO
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_VIA_PORT_DEF_CD
			      ,ROUT_VIA_PORT_TP_CD
			      ,SRC_INFO_CD
			      ,ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
			  FROM PRI_RQ_RT_ROUT_VIA
			 WHERE QTTN_NO = @[qttn_no]
			   AND QTTN_VER_NO = @[qttn_ver_no]
			   AND ORG_DEST_TP_CD = 'D'
		 )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY QTTN_NO
			 ,QTTN_VER_NO
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD
	) D
      ,(
	      SELECT QTTN_NO
		      ,QTTN_VER_NO
		      ,CMDT_HDR_SEQ
		      ,ROUT_SEQ
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD ||  DECODE(RCV_DE_TERM_CD, NULL,'', '(' || TERM_NM || ')' ) , ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
		  FROM (SELECT QTTN_NO
			      ,QTTN_VER_NO
			      ,CMDT_HDR_SEQ
			      ,ROUT_SEQ
			      ,ORG_DEST_TP_CD
			      ,ROUT_PNT_LOC_DEF_CD
			      ,ROUT_PNT_LOC_TP_CD
			      ,RCV_DE_TERM_CD
			      ,SRC_INFO_CD
			      ,ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
			       ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL CODE WHERE CODE.INTG_CD_ID = 'CD02070' AND INTG_CD_VAL_CTNT = RCV_DE_TERM_CD) as TERM_NM 
			  FROM PRI_RQ_RT_ROUT_PNT
			 WHERE QTTN_NO = @[qttn_no]
			   AND QTTN_VER_NO = @[qttn_ver_no]
			   AND ORG_DEST_TP_CD = 'D'
		 )
		 START WITH RN = 1
		CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY QTTN_NO
			 ,QTTN_VER_NO
			 ,CMDT_HDR_SEQ
			 ,ROUT_SEQ
			 ,ORG_DEST_TP_CD
	) E
      ,(
		SELECT QTTN_NO
		      ,QTTN_VER_NO
		      ,CMDT_HDR_SEQ
		      ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(PRC_CMDT_DEF_NM, ' / ')), 4) AS PRC_CMDT_DEF_NM
		  FROM (SELECT QTTN_NO
			      ,QTTN_VER_NO
			      ,CMDT_HDR_SEQ
			      ,DECODE(PRC_CMDT_TP_CD
				     ,'G'
				     ,(SELECT PRC_GRP_CMDT_DESC
					FROM PRI_RQ_GRP_CMDT
				       WHERE QTTN_NO = A.QTTN_NO
					 AND QTTN_VER_NO = A.QTTN_VER_NO
					 AND PRC_GRP_CMDT_CD = A.PRC_CMDT_DEF_CD
					 AND ROWNUM = 1)
				     ,'R'
				     ,(SELECT REP_CMDT_NM
					FROM MDM_REP_CMDT
				       WHERE REP_CMDT_CD = A.PRC_CMDT_DEF_CD
					 AND ROWNUM = 1)
				     ,'C'
				     ,(SELECT CMDT_NM
					FROM MDM_COMMODITY
				       WHERE CMDT_CD = A.PRC_CMDT_DEF_CD
					 AND ROWNUM = 1)) AS PRC_CMDT_DEF_NM
			      ,SRC_INFO_CD
			      ,ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ ORDER BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, DECODE(PRC_CMDT_TP_CD, 'G', '1', 'C', '2'), PRC_CMDT_DEF_CD) AS RN
			  FROM PRI_RQ_RT_CMDT A
			 WHERE QTTN_NO = @[qttn_no]
			   AND QTTN_VER_NO = @[qttn_ver_no]
		 )
		 START WITH RN = 1
		CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
		       AND PRIOR RN = RN - 1
		 GROUP BY QTTN_NO
			 ,QTTN_VER_NO
			 ,CMDT_HDR_SEQ
      ) I
      , (
		SELECT 	PRI_ROUT.QTTN_NO, PRI_ROUT.QTTN_VER_NO
			, PRI_ROUT.CMDT_HDR_SEQ, PRI_ROUT.ROUT_SEQ, PRI_ROUT.RT_SEQ
			, PRI_ROUT.POR_CD, PRI_ROUT.POL_CD, PRI_ROUT.POD_CD, PRI_ROUT.DEL_CD
			, TO_CHAR(PRI_ROUT.CRE_DT,'YYYY-MM-DD') CRE_YMD 
			, PRI_SCG.CHG_CD 
			, PRI_SCG.BKG_RAT_UT_CD
			, PRI_SCG.CURR_CD
			, PRI_SCG.TRF_SCG_AMT
			, PRI_SCG.ADJ_SCG_AMT
			, PRI_SCG.ADJ_SCG_USD_AMT
			, SUM( PRI_SCG.ADJ_SCG_USD_AMT ) OVER( PARTITION BY PRI_ROUT.QTTN_NO, PRI_ROUT.QTTN_VER_NO, PRI_ROUT.CMDT_HDR_SEQ, PRI_ROUT.ROUT_SEQ, PRI_ROUT.RT_SEQ ) AS TOT_SURCHARGE
		FROM PRI_RQ_RT_SCG_ROUT PRI_ROUT
			, PRI_RQ_RT_SCG PRI_SCG
		WHERE
			PRI_ROUT.QTTN_NO = PRI_SCG.QTTN_NO
			AND PRI_ROUT.QTTN_VER_NO = PRI_SCG.QTTN_VER_NO
			AND PRI_ROUT.CMDT_HDR_SEQ = PRI_SCG.CMDT_HDR_SEQ
			AND PRI_ROUT.ROUT_SEQ = PRI_SCG.ROUT_SEQ
			AND PRI_ROUT.RT_SEQ = PRI_SCG.RT_SEQ
			AND PRI_ROUT.QTTN_NO = @[qttn_no]
			AND PRI_ROUT.QTTN_VER_NO = @[qttn_ver_no]

      ) SCG
      , PRI_RQ_RT RT
 WHERE  I.QTTN_NO = A.QTTN_NO
	AND I.QTTN_VER_NO = A.QTTN_VER_NO
	AND I.CMDT_HDR_SEQ = A.CMDT_HDR_SEQ

	AND A.QTTN_NO = B.QTTN_NO
	AND A.QTTN_VER_NO = B.QTTN_VER_NO
	AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
	AND A.ROUT_SEQ = B.ROUT_SEQ



	AND A.QTTN_NO = RT.QTTN_NO
	AND A.QTTN_VER_NO = RT.QTTN_VER_NO
	AND A.CMDT_HDR_SEQ = RT.CMDT_HDR_SEQ
	AND A.ROUT_SEQ = RT.ROUT_SEQ


	AND RT.QTTN_NO = SCG.QTTN_NO(+)
	AND RT.QTTN_VER_NO = SCG.QTTN_VER_NO(+)
	AND RT.CMDT_HDR_SEQ = SCG.CMDT_HDR_SEQ(+)
	AND RT.ROUT_SEQ = SCG.ROUT_SEQ(+)
	AND RT.RT_SEQ = SCG.RT_SEQ(+)




	AND A.QTTN_NO = C.QTTN_NO(+)
	AND A.QTTN_VER_NO = C.QTTN_VER_NO(+)
	AND A.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ(+)
	AND A.ROUT_SEQ = C.ROUT_SEQ(+)
	AND A.QTTN_NO = D.QTTN_NO(+)
	AND A.QTTN_VER_NO = D.QTTN_VER_NO(+)
	AND A.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
	AND A.ROUT_SEQ = D.ROUT_SEQ(+)
	AND A.QTTN_NO = E.QTTN_NO
	AND A.QTTN_VER_NO = E.QTTN_VER_NO
	AND A.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
	AND A.ROUT_SEQ = E.ROUT_SEQ
	AND A.QTTN_NO = @[qttn_no]
	AND A.QTTN_VER_NO = @[qttn_ver_no]
 ORDER BY  A.QTTN_NO, A.QTTN_VER_NO, A.CMDT_HDR_SEQ
	,I.PRC_CMDT_DEF_NM || A.CMDT_HDR_SEQ
	,B.ROUT_PNT_LOC_DEF_CD || A.ROUT_SEQ
	,C.ROUT_VIA_PORT_DEF_CD || A.ROUT_SEQ
	,D.ROUT_VIA_PORT_DEF_CD || A.ROUT_SEQ
	,E.ROUT_PNT_LOC_DEF_CD  || A.ROUT_SEQ
	,RT.RAT_UT_CD    || RT.RT_SEQ
	,RT.PRC_CGO_TP_CD || RT.RT_SEQ
	,RT.CURR_CD       || RT.RT_SEQ
	, SCG.POR_CD
	, SCG.POL_CD
	, SCG.POD_CD
	, SCG.DEL_CD
	, SCG.CHG_CD
			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
