<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOWebBkgManualUploadSetupVOCSQL">
			<desc><![CDATA[WebBkgManualUploadSetupVO]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_SYS_UPLD_BLCK_STUP 
            (BKG_OFC_CD
            , BLCK_SEQ
            , VSL_SLAN_CD
            , VSL_CD
            , SKD_VOY_NO
            , DIR_CD
            , CUST_CNT_CD
            , CUST_SEQ
            , POL_CNT_CD
            , POL_CD
            , POD_CNT_CD
            , POD_CD
            , DELT_FLG
            , CRE_USR_ID
            , CRE_DT
            , UPD_USR_ID
            , UPD_DT
			, XTER_RMK
			, LODG_DIR_CD)
VALUES(
        @[bkg_ofc_cd]
        , (SELECT NVL(MAX(BLCK_SEQ), 0)+1 FROM BKG_SYS_UPLD_BLCK_STUP WHERE BKG_OFC_CD = @[bkg_ofc_cd])
        , @[vsl_slan_cd]
        , substr(@[vvd], 1, 4)
        , substr(@[vvd], 5, 4)
        , substr(@[vvd], 9, 1)
        , SUBSTR(@[cust_cnt_cd],1,2)
        , SUBSTR(@[cust_cnt_cd],3)
        , @[pol_cnt_cd]
        , @[pol_cd]
        , @[pod_cnt_cd]
        , @[pod_cd]
        , 'N'
        , @[cre_usr_id]
        , SYSDATE
        , @[upd_usr_id]
        , SYSDATE
		, @[xter_rmk]
		, @[lodg_dir_cd]
    )			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="pol_cnt_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cnt_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="xter_rmk" type="12" value="" out="N"/>
				<param name="lodg_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
