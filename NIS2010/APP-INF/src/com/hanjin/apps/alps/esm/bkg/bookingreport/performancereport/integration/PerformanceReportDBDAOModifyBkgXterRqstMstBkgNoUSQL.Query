<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOModifyBkgXterRqstMstBkgNoUSQL">
			<desc><![CDATA[PerformanceReportDBDAOModifyBkgXterRqstMstBkgNoUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_XTER_RQST_MST
   SET BKG_NO = @[bkg_no],
       SPLIT_STS_CD = @[split_sts_cd], --add
       BL_SPLIT_NO = @[bl_split_no],  --add
	   BL_SPLIT_TTL_KNT = @[bl_split_ttl_knt],  --add
       UPD_USR_ID = @[usr_id],
       UPD_DT = SYSDATE
 WHERE XTER_RQST_NO = @[sr_no]
   AND FAX_LOG_REF_NO =@[fax_log_ref_no]
   AND XTER_RQST_SEQ = @[max_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="split_sts_cd" type="12" value="" out="N"/>
				<param name="bl_split_no" type="12" value="" out="N"/>
				<param name="bl_split_ttl_knt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="fax_log_ref_no" type="12" value="" out="N"/>
				<param name="max_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
