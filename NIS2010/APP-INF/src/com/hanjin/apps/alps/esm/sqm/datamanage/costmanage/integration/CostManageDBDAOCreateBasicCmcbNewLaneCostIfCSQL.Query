<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostManageDBDAOCreateBasicCmcbNewLaneCostIfCSQL">
			<desc><![CDATA[선택한 노선에 대해서 "New Lane & Office CMCB" 화면에서 등록했던 정보를 I/F.]]></desc>
			<sql><![CDATA[
INSERT INTO SQM_QTA_LANE_OFC_COST (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,TRD_CD
      ,RLANE_CD
      ,DIR_CD
      ,RGN_OFC_CD
      ,RHQ_CD
      ,GID_PA_CM_UC_AMT
      ,GID_RA_CM_UC_AMT
      ,PA_CM_UC_AMT
      ,RA_CM_UC_AMT
      ,ADD_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT A.BSE_TP_CD
      ,A.BSE_YR
      ,A.BSE_QTR_CD
      ,B.OFC_VW_CD
      ,B.TRD_CD
      ,B.RLANE_CD
      ,B.DIR_CD
      ,B.RGN_OFC_CD
      ,B.RHQ_CD
      ,B.GID_PA_CM_UC_AMT
      ,B.GID_RA_CM_UC_AMT
      ,B.PA_CM_UC_AMT
      ,B.RA_CM_UC_AMT
      ,'Y' AS ADD_FLG
      ,@[cre_usr_id] AS CRE_USR_ID
      ,SYSDATE       AS CRE_DT
      ,@[upd_usr_id] AS UPD_USR_ID
      ,SYSDATE       AS UPD_DT
  FROM SQM_QTA_NEW_LANE          A
      ,SQM_QTA_NEW_LANE_OFC_COST B
 WHERE A.BSE_TP_CD  = @[bse_tp_cd]
   AND A.BSE_YR     = @[bse_yr]
   AND A.BSE_QTR_CD = DECODE(@[bse_tp_cd], 'Y', '00', @[bse_qtr_cd])
   AND A.TRD_CD     = @[trd_cd]
   AND A.RLANE_CD   = @[rlane_cd]
   AND A.DIR_CD     = @[dir_cd]
   AND A.BSE_TP_CD  = B.BSE_TP_CD
   AND A.BSE_YR     = B.BSE_YR
   AND A.BSE_QTR_CD = B.BSE_QTR_CD
   AND A.TRD_CD     = B.TRD_CD
   AND A.RLANE_CD   = B.RLANE_CD
   AND A.DIR_CD     = B.DIR_CD			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
