<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterSrchSetForListRSQL">
			<desc><![CDATA[searchXterSrchSetForList]]></desc>
			<sql><![CDATA[
SELECT DECODE(ROWNUM,1,'AND (', 'OR ') || '(1=1 ' || 
  DECODE(XTER_SNDR_ID, NULL,' ', ' AND XTER_SNDR_ID =''' ||XTER_SNDR_ID || '''')||  
  DECODE(CHN_AGN_CD,        NULL,' ', ' AND SUBSTR(BKG_NO,5,2) = ''' || CHN_AGN_CD || '''') || 
  DECODE(DOC_TP_CD,         NULL,' ', ' AND DOC_TP_CD = ''' || DOC_TP_CD || '''') || 
  DECODE(BKG_UPLD_STS_CD,   NULL,' ', 'D', ' AND RQST_DELT_FLG = ''Y''', ' AND BKG_UPLD_STS_CD = ''' || BKG_UPLD_STS_CD || '''') || 
  DECODE(NON_RT_STS_CD,		NULL,' ', 'F', ' AND NON_RT_STS_CD = ''F''', 'R', ' AND NON_RT_STS_CD = ''R''') ||
  DECODE(XTER_RQST_VIA_CD,  NULL,' ', ' AND XTER_RQST_VIA_CD = ''' || XTER_RQST_VIA_CD || '''') || 
  DECODE(ORG_CNT_CD,        NULL,' ', ' AND ''' || ORG_CNT_CD || ''' IN (POR.CNT_CD, POL.CNT_CD)') || 
  DECODE(DEL_CONTI_CD,      NULL,' ', ' AND MST.DEL_CN ' ||
                                        ' IN (''' || DECODE(DEL_CONTI_CD,  'E', 'F'',''E', DEL_CONTI_CD) || ''')') ||
  DECODE(HNDL_OFC_CD,       NULL,' ', ' AND MST.HNDL_OFC_CD = ''' || HNDL_OFC_CD || '''') ||   
  DECODE(CUST_CNT_CD,       NULL,' ', ' AND decode('''||srch.bkg_cust_tp_cd||''', ''F'', ff_cnt, ''C'', cn_cnt, sh_cnt) = '''||srch.cust_cnt_cd||'''')||
  DECODE(CUST_SEQ, 0, ' ',  NULL,' ', ' AND decode('''||srch.bkg_cust_tp_cd||''', ''F'', ff_seq, ''C'', cn_seq, sh_seq) = '''||srch.CUST_SEQ||'''')||
  DECODE(CUST_NM,       	NULL,' ', ' AND decode('''||srch.bkg_cust_tp_cd||''', ''F'', ff_nm,  ''C'', cn_nm,  sh_nm)  like '''||srch.CUST_NM||'%''')||
  DECODE(SLAN_CD,           NULL,' ', ' AND SLAN_CD = ''' || SLAN_CD || '''') || 
  DECODE(SREP_CD,           NULL,' ', ' AND (SREP_CD = DECODE(MST.DOC_TP_CD, ''S'', '''||SREP_CD||''', '' '')' || 
                        ' OR (sh_cnt, sh_seq) IN ' || 
                        '(SELECT CUST.cust_CNT_CD, CUST.CUST_SEQ '||
                        '   FROM MDM_CUSTOMER CUST, BKG_CUST_SLS_REP SREP '|| 
                        '  WHERE CUST.cust_CNT_CD   = SREP.cust_CNT_CD '|| 
                        '    AND CUST.CUST_SEQ      = SREP.CUST_SEQ '|| 
                        '    AND SREP.DELT_FLG      = ''N'''|| 
--						'    AND SREP.SREP_CD       = '''|| SREP_CD ||'''))')||
                        '    AND SREP.SREP_CD       = DECODE(MST.DOC_TP_CD, ''S'', '' '','''|| SREP_CD ||'''))'||
						' OR (ff_cnt, ff_seq) IN ' || 
                        '(SELECT CUST.cust_CNT_CD, CUST.CUST_SEQ '||
                        '   FROM MDM_CUSTOMER CUST, BKG_CUST_SLS_REP SREP '|| 
                        '  WHERE CUST.cust_CNT_CD   = SREP.cust_CNT_CD '|| 
                        '    AND CUST.CUST_SEQ      = SREP.CUST_SEQ '|| 
                        '    AND SREP.DELT_FLG      = ''N'''|| 
                        '    AND SREP.SREP_CD       = DECODE(MST.DOC_TP_CD, ''S'', '' '','''|| SREP_CD ||''')))')||
  ')' CUST_NM
FROM BKG_XTER_SRCH_SET srch
WHERE USR_ID = @[usr_id]
AND SET_SLCT_FLG = 'Y'
AND NVL(DOC_TP_CD,'X') <> 'D'			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
