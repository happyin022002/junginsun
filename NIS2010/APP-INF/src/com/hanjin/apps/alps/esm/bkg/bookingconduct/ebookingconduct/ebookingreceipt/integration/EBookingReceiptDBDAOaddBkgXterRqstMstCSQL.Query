<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOaddBkgXterRqstMstCSQL">
			<desc><![CDATA[addBkgXterRqstMst
2010.12.30 이일민 [CHM-201007165-01] Split 01-Simple EDI 개발 요청]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_XTER_RQST_MST
(
XTER_RQST_SEQ
#if (''==${rqst_dt})
,RQST_DT
#end
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
,BKG_NO
,BL_NO_CTNT
#if ($table_date_columns.size() > 0)
     #foreach($tableColumn IN ${table_columns})
		,${tableColumn}
     #end
     #foreach($tableDateColumns IN ${table_date_columns})
        ,${tableDateColumns}
     #end
#else
     #foreach($tableColumn IN ${table_columns})
        ,${tableColumn}
     #end
#end
#if ('' != ${ctrt_nm})
,CTRT_NM
#end
)
VALUES
(
@[rqst_seq]
#if(''==${rqst_dt})
,SYSDATE
#end
,'SYSTEM'
,sysdate
,'SYSTEM'
,sysdate
,@[bkg_no]
,@[bl_no_ctnt]
#if ($table_date_columns.size() > 0)
     #foreach($tableValue IN ${table_values})
		,'${tableValue}'
     #end
     #foreach($tableDateValue IN ${table_date_values})
        ,to_date('${tableDateValue}','yyyyMMddhh24miss')
     #end
#else
     #foreach($tableValue IN ${table_values})
        ,'${tableValue}'
     #end
#end
#if ('' != ${ctrt_nm})
,@[ctrt_nm]
#end
)			]]></sql>
			<params>
				<param name="rqst_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no_ctnt" type="12" value="" out="N"/>
				<param name="ctrt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
