<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOaddCarrierBatchNoOBCSQL">
			<desc><![CDATA[UsaCustomsTransmissionDBDAOaddCarrierBatchNoOBCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO  BKG_CSTMS_ADV_EDI_BL_RSPN(
    CNT_CD, BL_NO, CRR_BAT_NO, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
SELECT	'US' CNT_CD, BKG_NO, @[crr_bat_no], @[cre_usr_id], sysdate, @[cre_usr_id], sysdate
FROM BKG_BOOKING IT
WHERE IT.BKG_NO  in (SELECT COLUMN_VALUE FROM TABLE(BKG_SPLIT_CLOB_FNC(${bl_params},',')) WHERE COLUMN_VALUE IS NOT NULL)			]]></sql>
			<params>
				<param name="crr_bat_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
