<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOmodifyArrivalNoticeEnsEtaUSQL">
			<desc><![CDATA[ENS ETA 수정 값으로 업데이트]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_EUR_BL
   SET CSTMS_ESTM_ARR_DT = TO_DATE(@[init_eta_dt], 'YYYY-MM-DD HH24:MI')
 WHERE 1=1
   AND VSL_CD = SUBSTR(@[vvd],1,4)
   AND SKD_VOY_NO = SUBSTR(@[vvd],5,4)
   AND SKD_DIR_CD = SUBSTR(@[vvd],9,1)
   AND CSTMS_PORT_CD = @[cstms_port_cd]			]]></sql>
			<params>
				<param name="init_eta_dt" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
