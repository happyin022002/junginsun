			<sql><![CDATA[
INSERT INTO PRI_RP_DMDT (
	PROP_NO
,	AMDT_SEQ
,	DMDT_FT_TP_CD
,	PRC_PROG_STS_CD
,	SRC_INFO_CD
,	N1ST_CMNC_AMDT_SEQ
,	ACPT_USR_ID
,	ACPT_OFC_CD
,	ACPT_DT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[prop_no]
,	@[amdt_seq]
,	@[dmdt_ft_tp_cd]
,	@[prc_prog_sts_cd]
,	@[src_info_cd]
,	@[n1st_cmnc_amdt_seq]
,	@[acpt_usr_id]
,	@[acpt_ofc_cd]
,	TO_DATE(REPLACE(@[acpt_dt],'-',''), 'YYYYMMDD')
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
