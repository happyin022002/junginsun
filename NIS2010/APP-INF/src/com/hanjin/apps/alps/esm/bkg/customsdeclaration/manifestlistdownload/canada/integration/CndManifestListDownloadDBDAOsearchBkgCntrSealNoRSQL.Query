<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndManifestListDownloadDBDAOsearchBkgCntrSealNoRSQL">
			<desc><![CDATA[searchBkgCntrSealNo]]></desc>
			<sql><![CDATA[
#if (${mf_no} == '') 
	SELECT  'CA' AS CNT_CD
	       ,@[bl_no] AS BL_NO
	       ,'CTM' AS CSTMS_DIV_ID
	       ,CNTR_NO
	       ,CNTR_SEAL_SEQ AS SEAL_NO_SEQ
	       ,CNTR_SEAL_NO AS SEAL_NO
	       ,@[upd_usr_id]     AS UPD_USR_ID
	  FROM  BKG_CNTR_SEAL_NO
	 WHERE  BKG_NO = @[bkg_no]
       AND TRIM(CNTR_SEAL_NO) != '-'  /*제외*/
#else 
	SELECT DISTINCT 'CA' AS CNT_CD
	       ,@[bl_no] AS BL_NO
	       ,'CTM' AS CSTMS_DIV_ID
	       ,S.CNTR_NO
	       ,S.CNTR_SEAL_SEQ AS SEAL_NO_SEQ
	       ,S.CNTR_SEAL_NO AS SEAL_NO
	       ,@[upd_usr_id]     AS UPD_USR_ID
	  FROM  BKG_CNTR_SEAL_NO S
	       ,BKG_CNTR_MF_DESC D
	 WHERE  S.BKG_NO = @[bkg_no]
	   AND  S.BKG_NO = D.BKG_NO
	   AND  S.CNTR_NO = D.CNTR_NO
	   AND  D.CNTR_MF_NO = @[bl_no]
       AND TRIM(S.CNTR_SEAL_NO) != '-'  /*제외*/
#end			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
