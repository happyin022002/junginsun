<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOBkgVgmClzSetVOCSQL">
			<desc><![CDATA[VGM CCT Insert를 위한 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_VRFD_WGT_CLZ_SET (
	VSL_SLAN_CD
,	CONTI_CD
,   LOC_CD
,	YD_CD
,	DEST_CNT_CD
,	VGM_CLZ_TP_CD
,	VGM_ITVAL_HRS
,	VGM_CLZ_DY_CD
,	VGM_CLZ_DY_HRS
,	XCLD_HOL_FLG
,   VVD_CD
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[vsl_slan_cd]
,	@[conti_cd]
,   @[loc_cd]
,   DECODE(@[yd_cd],'ALL','*',@[yd_cd])
,	@[dest_cnt_cd]
,	@[vgm_clz_tp_cd]
,	@[vgm_itval_hrs]
,	@[vgm_clz_dy_cd]
,	to_number(substr(@[vgm_clz_dy_hrs],1,2)) * 60 + to_number(substr(@[vgm_clz_dy_hrs],3,2))
,	@[xcld_hol_flg]
,   DECODE(@[vvd_cd],'ALL','*',@[vvd_cd])
,	'N'
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
 
)			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="dest_cnt_cd" type="12" value="" out="N"/>
				<param name="vgm_clz_tp_cd" type="12" value="" out="N"/>
				<param name="vgm_itval_hrs" type="12" value="" out="N"/>
				<param name="vgm_clz_dy_cd" type="12" value="" out="N"/>
				<param name="vgm_clz_dy_hrs" type="12" value="" out="N"/>
				<param name="xcld_hol_flg" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
