<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAORemoveBkgClauseLockDSQL">
			<desc><![CDATA[RemoveBkgClauseLock]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CLUZ_LCK
WHERE	BKG_NO = @[bkg_no]
   AND CLUZ_LCK_TP_CD = @[cluz_lck_tp_cd]
   AND CLUZ_LCK_SEQ = @[cluz_lck_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cluz_lck_tp_cd" type="12" value="" out="N"/>
				<param name="cluz_lck_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
