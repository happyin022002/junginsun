<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMSummaryDBDAORsltPrsAmendmentCmSummarySimulation1StTypeVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT  PRC_CTRT_NO AS SC_NO
			, LOAD
			, G_REV
			, COST_CM_OFFICE AS  COST_OFFICE
			, COST_CM_TRADE AS  COST_TRADE
			, COST_OP_OFFICE
			, CM_OFFICE
			, CM_TRADE
			, OP AS OP_OFFICE
			, CM_OFFICE / DECODE(LOAD,0,1,LOAD) AS CMPB_OFFICE
			, CM_TRADE /  DECODE(LOAD,0,1,LOAD) AS CMPB_TRADE
			, OP /  DECODE(LOAD,0,1,LOAD) AS OPB_OFFICE
			, UPD_FLG
		FROM (
			SELECT  SR_CST_DTL.PRC_CTRT_NO 
				, SUM(SR_CST_DTL.PRS_CRNT_LOD_QTY  / DECODE(@[frm_pfmc_unit],'FEU',2,'TEU',1) )      AS LOAD            -- Load(Previous)
				, SUM(SR_CST_DTL.PRS_RESPB_CM_UC_AMT * SR_CST_DTL.PRS_CRNT_LOD_QTY)   AS COST_CM_OFFICE  -- Office Profit/ CM -- Cost(Previous)
				, SUM(SR_CST_DTL.PRS_PFIT_CM_UC_AMT * SR_CST_DTL.PRS_CRNT_LOD_QTY)    AS COST_CM_TRADE   --Trade Profit/ CM   -- Cost(Previous)
				, SUM(SR_CST_DTL.PRS_RESPB_OPFIT_UC_AMT * SR_CST_DTL.PRS_CRNT_LOD_QTY) AS COST_OP_OFFICE -- Office Profit/ OP -- Cost(Previous)
				, SUM(SR_CST_DTL.TEU_FRT_REV  * SR_CST_DTL.PRS_CRNT_LOD_QTY)         AS G_REV            -- Gross Revenue(Previous)
				, SUM(SR_CST_DTL.PRS_CRNT_LOD_QTY   *  ( SR_CST_DTL.TEU_FRT_REV  - SR_CST_DTL.PRS_RESPB_CM_UC_AMT ) ) AS CM_OFFICE
				, SUM(SR_CST_DTL.PRS_CRNT_LOD_QTY   *  ( SR_CST_DTL.TEU_FRT_REV  - SR_CST_DTL.PRS_PFIT_CM_UC_AMT ) ) AS CM_TRADE
				, SUM(SR_CST_DTL.PRS_CRNT_LOD_QTY   *  ( SR_CST_DTL.TEU_FRT_REV  - SR_CST_DTL.PRS_RESPB_OPFIT_UC_AMT - SR_CST_DTL.PRS_RESPB_CM_UC_AMT ) ) AS OP
				, MAX(SR_CST_DTL.UPD_USR_ID) AS UPD_FLG
			FROM  PRI_PRS_CTRT_SMRY_COST_TMP SR_CST_DTL
			GROUP BY SR_CST_DTL.PRC_CTRT_NO 
		) MN
		WHERE UPD_FLG = 'Y'			]]></sql>
			<params>
				<param name="frm_pfmc_unit" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
