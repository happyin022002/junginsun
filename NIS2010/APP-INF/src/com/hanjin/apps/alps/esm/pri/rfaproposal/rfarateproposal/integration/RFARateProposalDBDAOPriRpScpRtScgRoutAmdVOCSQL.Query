<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtScgRoutAmdVOCSQL">
			<desc><![CDATA[AMEND REQUEST]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_RT_SCG_ROUT(
         PROP_NO
        ,AMDT_SEQ
        ,SVC_SCP_CD
        ,CMDT_HDR_SEQ
        ,ROUT_SEQ
        ,RT_SEQ
        ,POR_CD
        ,POL_CD
        ,POD_CD
        ,DEL_CD
        ,RCV_TERM_CD
        ,DE_TERM_CD
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
)
SELECT   SCG.PROP_NO
        ,SCG.AMDT_SEQ + 1
        ,SCG.SVC_SCP_CD
        ,SCG.CMDT_HDR_SEQ
        ,SCG.ROUT_SEQ
        ,SCG.RT_SEQ
        ,SCG.POR_CD
        ,SCG.POL_CD
        ,SCG.POD_CD
        ,SCG.DEL_CD
        ,SCG.RCV_TERM_CD
        ,SCG.DE_TERM_CD
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[upd_usr_id]
        ,SYSDATE
FROM 
    PRI_RP_SCP_RT_SCG_ROUT SCG
   ,PRI_RP_SCP_RT RT
WHERE
    SCG.PROP_NO     = @[prop_no]
AND SCG.AMDT_SEQ    = @[amdt_seq]
AND SCG.PROP_NO             = RT.PROP_NO
AND SCG.AMDT_SEQ            = RT.AMDT_SEQ
AND SCG.SVC_SCP_CD          = RT.SVC_SCP_CD
AND SCG.CMDT_HDR_SEQ        = RT.CMDT_HDR_SEQ
AND SCG.ROUT_SEQ            = RT.ROUT_SEQ
AND SCG.RT_SEQ              = RT.RT_SEQ
AND RT.SRC_INFO_CD          <> 'AD'						]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
