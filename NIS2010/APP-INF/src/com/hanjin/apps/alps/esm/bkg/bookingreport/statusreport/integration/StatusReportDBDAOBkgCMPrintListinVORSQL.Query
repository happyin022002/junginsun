<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOBkgCMPrintListinVORSQL">
			<desc><![CDATA[search]]></desc>
			<sql><![CDATA[
SELECT A.BL_NO
      ,A.BKG_NO
	  ,A.BL_NO_TP
      ,A.BL_TP_CD
      ,A.OB_SREP_CD
      ,A.POL_CD
      ,A.RCV_TERM_CD
	  ,A.DE_TERM_CD
	  ,(SELECT USR_NM FROM COM_USER WHERE USR_ID = A.DOC_USR_ID) AS DOC_USR_NM
      ,A.DOC_USR_ID
      ,B.PCK_QTY
      ,NVL(B.PCK_QTY,0) PCK_QTY
      ,TO_CHAR(NVL(B.CNTR_MF_WGT,0),999999999999990.099) CNTR_MF_WGT
      ,TO_CHAR(NVL(B.MEAS_QTY,0),999999999999990.099) MEAS_QTY
      ,B.RD_CGO_FLG
      ,DECODE(B.DCGO_FLG,'N','','Y') DCGO_FLG
      ,DECODE(B.AWK_CGO_FLG,'N','','Y') AWK_CGO_FLG
      ,DECODE(B.HNGR_FLG,'N','','Y') HNGR_FLG
      ,REPLACE(B.CNTR_MF_GDS_DESC,CHR(13) || CHR(10),' ') CNTR_MF_GDS_DESC
      ,REPLACE(B.CNTR_MF_MK_DESC,CHR(13) || CHR(10),' ') CNTR_MF_MK_DESC
      ,B.HAMO_TRF_CD
      ,B.PO_NO
      ,B.CMDT_HS_CD
      ,C.ADV_SHTG_CD
      ,C.CNTR_NO
      ,C.CNTR_TPSZ_CD
      ,BKG_JOIN_FNC(CURSOR( SELECT CNTR_SEAL_NO FROM BKG_CNTR_SEAL_NO WHERE BKG_NO = C.BKG_NO AND CNTR_NO = C.CNTR_NO)) CNTR_SEAL_NO
      ,E.CUST_CNT_CD
      ,E.CUST_SEQ
	  ,REPLACE(E.CUST_NM,CHR(13) || CHR(10),' ') CUST_NM
	  ,'CM' TAB_TP
  FROM BKG_BOOKING          A
      ,BKG_CNTR_MF_DESC     B
      ,BKG_CONTAINER        C
      ,BKG_VVD              D
      ,BKG_CUSTOMER         E
 WHERE D.VSL_CD = @[vsl_cd]
   AND D.SKD_VOY_NO = @[skd_voy_no]
   AND D.SKD_DIR_CD = @[skd_dir_cd]
   AND A.BKG_NO = C.BKG_NO
   AND A.BKG_NO = D.BKG_NO
   AND A.BKG_NO = E.BKG_NO
   AND E.BKG_CUST_TP_CD(+) = 'S'
   AND C.BKG_NO = B.BKG_NO(+)
   AND C.CNTR_NO = B.CNTR_NO(+)
   AND C.CNTR_NO IS NOT NULL
#if (${pol_cd} != '') 
   AND D.POL_CD = @[pol_cd]
#end
#if (${pol_yd_cd} != '') 
   AND D.POL_YD_CD = @[pol_yd_cd]
#end
#if (${pod_cd} != '') 
   AND D.POD_CD = @[pod_cd]
#end
#if (${pod_yd_cd} != '') 
   AND D.POD_YD_CD = @[pod_yd_cd]
#end
#if (${rcv_term_cd} != '') 
   AND A.RCV_TERM_CD = @[rcv_term_cd]
#end
#if (${de_term_cd} != '') 
   AND A.DE_TERM_CD = @[de_term_cd]
#end
#if (${bkg_ofc_cd} != '') 
   AND A.BKG_OFC_CD = @[bkg_ofc_cd]
#end
#if (${cre_usr_id} != '') 
   AND A.CRE_USR_ID = @[cre_usr_id]
#end
#if (${bkg_no} != '') 
   AND A.BKG_NO = @[bkg_no]
#end
#if (${bl_no} != '') 
   AND A.BL_NO = @[bl_no]
#end
#if (${ob_srep_cd} != '') 
   AND A.OB_SREP_CD = @[ob_srep_cd]
#end
#if (${cntr_no} != '') 
   AND C.CNTR_NO = @[cntr_no]
#end
#if (${cust_cnt_cd} != '') 
   AND E.CUST_CNT_CD(+) = @[cust_cnt_cd]
   AND E.CUST_SEQ(+)	= @[cust_seq]
#end
#if (${cust_nm} != '')
   AND E.CUST_NM LIKE '%' || @[cust_nm] || '%'
#end
 ORDER BY C.CNTR_NO ASC
 --ORDER BY A.BL_NO ASC			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="rcv_term_cd" type="12" value="" out="N"/>
				<param name="de_term_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="ob_srep_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
