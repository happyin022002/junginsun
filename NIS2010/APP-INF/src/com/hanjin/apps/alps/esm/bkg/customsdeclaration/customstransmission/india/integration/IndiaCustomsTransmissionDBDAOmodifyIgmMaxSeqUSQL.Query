<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaCustomsTransmissionDBDAOmodifyIgmMaxSeqUSQL">
			<desc><![CDATA[Agent Code별 전송번호 업데이트 하는 오퍼레이션]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_IDA_TP_RQST_SEQ SET 
	GEN_SEQ = @[gen_seq]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	IDA_SHPR_ID = @[ida_shpr_cd]
AND	LOCL_AGN_ID = @[locl_agn_cd]
AND	GEN_DT = TO_DATE(@[gen_dt], 'YYMMDD')
AND	GEN_SEQ = @[gen_seq]			]]></sql>
			<params>
				<param name="gen_seq" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ida_shpr_cd" type="12" value="" out="N"/>
				<param name="locl_agn_cd" type="12" value="" out="N"/>
				<param name="gen_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
