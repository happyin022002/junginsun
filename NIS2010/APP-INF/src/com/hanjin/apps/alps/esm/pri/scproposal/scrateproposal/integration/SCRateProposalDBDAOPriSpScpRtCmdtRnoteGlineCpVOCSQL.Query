<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAOPriSpScpRtCmdtRnoteGlineCpVOCSQL">
			<desc><![CDATA[CMDT note 입력]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_RT_CMDT_RNOTE
  (PROP_NO
  ,AMDT_SEQ
  ,SVC_SCP_CD
  ,GEN_SPCL_RT_TP_CD
  ,CMDT_HDR_SEQ
  ,ROUT_SEQ
  ,ROUT_NOTE_SEQ
  ,NOTE_CLSS_CD
  ,CHG_CD
  ,NOTE_CTNT
  ,NOTE_CONV_MAPG_ID
  ,NOTE_CHG_TP_CD
  ,PRC_PROG_STS_CD
  ,SRC_INFO_CD
  ,N1ST_CMNC_AMDT_SEQ
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT)
  SELECT @[prop_no]
        ,@[amdt_seq]
        ,@[svc_scp_cd]
        ,@[gen_spcl_rt_tp_cd]
        ,CMDT_HDR_SEQ
        ,ROUT_SEQ
        ,ROUT_NOTE_SEQ
        ,NOTE_CLSS_CD
        ,CHG_CD
        ,NOTE_CTNT
        ,SYS_GUID()
        ,NULL
        ,'I'
        ,'GC'
        ,@[amdt_seq]
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[upd_usr_id]
        ,SYSDATE
    FROM (SELECT DENSE_RANK() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, PRC_CUST_TP_CD ORDER BY CMDT_HDR_SEQ) AS CMDT_HDR_SEQ
                ,DENSE_RANK() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, PRC_CUST_TP_CD, CMDT_HDR_SEQ ORDER BY ROUT_SEQ) AS ROUT_SEQ
                ,DENSE_RANK() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, PRC_CUST_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY ROUT_NOTE_SEQ) AS ROUT_NOTE_SEQ
                ,NOTE_CLSS_CD
                ,CHG_CD
                ,NOTE_CTNT
            FROM (SELECT B.SVC_SCP_CD
                        ,B.GLINE_SEQ
                        ,B.PRC_CUST_TP_CD
                        ,B.CMDT_HDR_SEQ
                        ,C.ROUT_SEQ
                        ,D.ROUT_NOTE_SEQ
                        ,D.NOTE_CLSS_CD
                        ,D.CHG_CD
                        ,D.NOTE_CTNT
                    FROM (SELECT T.SVC_SCP_CD
                                ,T.GLINE_SEQ
                                ,RANK() OVER(PARTITION BY SVC_SCP_CD ORDER BY EFF_DT DESC) AS RNK
                            FROM PRI_SG_MN T
                           WHERE T.SVC_SCP_CD = @[svc_scp_cd]
                             AND TO_DATE(@[eff_dt], 'YYYY-MM-DD') BETWEEN T.EFF_DT AND T.EXP_DT
                             AND T.CFM_FLG = 'Y') A
                        ,PRI_SG_RT_CMDT_HDR B
                        ,PRI_SG_RT_CMDT_ROUT C
                        ,PRI_SG_RT_CMDT_RNOTE D
                   WHERE A.SVC_SCP_CD = B.SVC_SCP_CD
                     AND A.GLINE_SEQ = B.GLINE_SEQ
                     AND B.SVC_SCP_CD = C.SVC_SCP_CD
                     AND B.GLINE_SEQ = C.GLINE_SEQ
                     AND B.PRC_CUST_TP_CD = C.PRC_CUST_TP_CD
                     AND B.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ
                     AND C.SVC_SCP_CD = D.SVC_SCP_CD(+)
                     AND C.GLINE_SEQ = D.GLINE_SEQ(+)
                     AND C.PRC_CUST_TP_CD = D.PRC_CUST_TP_CD(+)
                     AND C.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
                     AND C.ROUT_SEQ = D.ROUT_SEQ(+)
                     AND A.RNK = 1
                     AND B.PRC_CUST_TP_CD = @[prc_cust_tp_cd]
                     AND TO_DATE(@[eff_dt], 'YYYY-MM-DD') BETWEEN B.EFF_DT AND B.EXP_DT) T
           WHERE EXISTS (SELECT 'OK'
                           FROM PRI_SG_RT S, PRI_SP_MQC MN, PRI_SP_SCP_MQC SCP
                          WHERE S.SVC_SCP_CD = T.SVC_SCP_CD
                            AND S.GLINE_SEQ = T.GLINE_SEQ
                            AND S.PRC_CUST_TP_CD = T.PRC_CUST_TP_CD
                            AND S.CMDT_HDR_SEQ = T.CMDT_HDR_SEQ
                            AND S.ROUT_SEQ = T.ROUT_SEQ
                            AND MN.PROP_NO = @[prop_no]
                            AND MN.AMDT_SEQ = @[amdt_seq]
                            AND MN.PROP_NO = SCP.PROP_NO
                            AND MN.AMDT_SEQ = SCP.AMDT_SEQ
                            AND SCP.SVC_SCP_CD = @[svc_scp_cd]
                            AND DECODE(SCP.PROP_SCP_MQC_QTY
                                      ,0
                                      ,MN.PROP_MQC_QTY
                                      ,SCP.PROP_SCP_MQC_QTY) BETWEEN S.MQC_RNG_FM_QTY AND S.MQC_RNG_TO_QTY) 
        )
   WHERE NOTE_CLSS_CD IS NOT NULL			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="prc_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
