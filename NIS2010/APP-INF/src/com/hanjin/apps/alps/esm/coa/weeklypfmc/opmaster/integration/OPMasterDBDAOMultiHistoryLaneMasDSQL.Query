<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPMasterDBDAOMultiHistoryLaneMasDSQL">
			<desc><![CDATA[MAS 테이블 동시 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MAS_LANE_TP_HIS 
 WHERE TRD_CD   = @[trd_cd]
   AND RLANE_CD = @[rlane_cd]
   AND DIR_CD   = @[dir_cd]
   AND IOC_CD   = @[ioc_cd]
   AND LANE_SEQ = @[lane_seq]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="lane_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
