<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PercentBaseChargeDBDAORemovePercentBaseChargeGroupingDSQL">
			<desc><![CDATA[delete CHG_CD]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_SCG_PCT_BSE_CHG
      WHERE PCT_BSE_CD = @[pct_bse_cd]
        AND CHG_CD = @[chg_cd]
        AND PCT_BSE_CHG_SEQ = @[pct_bse_chg_seq]			]]></sql>
			<params>
				<param name="pct_bse_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="pct_bse_chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
