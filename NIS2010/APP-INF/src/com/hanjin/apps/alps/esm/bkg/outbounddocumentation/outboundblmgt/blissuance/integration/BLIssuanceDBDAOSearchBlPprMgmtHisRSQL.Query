<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchBlPprMgmtHisRSQL">
			<desc><![CDATA[Web OBL배부내역 History를 조회한다.]]></desc>
			<sql><![CDATA[
select DTRB_YR
      ,RHQ_CD
      ,OFC_CD
      ,CUST_CNT_CD
      ,CUST_SEQ
      ,HIS_SEQ
      ,PRE_CTNT
      ,CRNT_CTNT
      ,TO_CHAR(CRE_DT,'YYYY-MM-DD HH24:MI') AS CRE_DT
      ,CRE_USR_ID
      ,UPD_USR_ID
      ,TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI') AS UPD_DT
  from BKG_CUST_BL_PPR_MGMT_HIS
 WHERE 1=1
#if (${dtrb_yr} != '') 
   AND DTRB_YR = @[dtrb_yr]
#end 
#if (${rhq_cd} != '') 
   AND RHQ_CD = @[rhq_cd]
#end   
#if (${ofc_cd} != '') 
   AND OFC_CD = @[ofc_cd]
#end   
#if (${cust_cnt_cd} != '') 
   AND CUST_CNT_CD = @[cust_cnt_cd]
#end 
#if (${cust_seq} != '') 
   AND CUST_SEQ = @[cust_seq]
#end			]]></sql>
			<params>
				<param name="dtrb_yr" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
