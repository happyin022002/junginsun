<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQHoldingDBDAOSearchEQCntrHldCostRSQL">
			<desc><![CDATA[EQ Holding Cost 조회(MAS용)]]></desc>
			<sql><![CDATA[
SELECT COST_YRMON
      ,TPSZ_CD
      ,TTL_DYS
      ,SEA_DYS
      ,ORG_RAIL_DYS
      ,DEST_RAIL_DYS
      ,FULL_DMT
      ,MT_LAND
      ,SUB_TOT
      ,BOX_CNT
      ,AVG_DYS
      ,MT_SEA_DYS
  FROM MAS_CNTR_SECT_DAY_CALC
  WHERE COST_YRMON BETWEEN TO_CHAR(ADD_MONTHS(TO_DATE(@[f_cost_yrmon],'YYYYMM'), -4),'YYYYMM') AND 
			TO_CHAR(ADD_MONTHS(TO_DATE(@[f_cost_yrmon],'YYYYMM'), -2),'YYYYMM')
	#if (${f_cntr_tpsz_cd} != '') 
		AND TPSZ_CD = @[f_cntr_tpsz_cd]
	#end
  ORDER BY TPSZ_CD, COST_YRMON			]]></sql>
			<params>
				<param name="f_cost_yrmon" type="12" value="" out="N"/>
				<param name="f_cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
