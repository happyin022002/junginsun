<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchDoNoRSQL">
			<desc><![CDATA[India 및 EU Cargo Release시 Split된 booking에 대한 최근 DO번호를 조회한다. 단 SPLIT인 경우만 유효한 값을 제공한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(DO_NO_SPLIT, '00', NULL, DO_NO) DO_NO
FROM (SELECT /*+ INDEX_DESC (BKG_DO XPKBKG_DO) */
             DO_NO
           , DO_NO_SPLIT
           , COUNT(1) CNT
      FROM BKG_DO
      WHERE BKG_NO = @[bkg_no]
        AND ROWNUM = 1
      GROUP BY DO_NO, DO_NO_SPLIT 
      )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
