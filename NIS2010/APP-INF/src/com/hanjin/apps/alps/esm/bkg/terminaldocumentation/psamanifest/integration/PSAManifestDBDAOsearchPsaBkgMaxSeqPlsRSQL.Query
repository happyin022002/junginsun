<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPsaBkgMaxSeqPlsRSQL">
			<desc><![CDATA[PSA BKG 테이블의 Max Seq에 +1한 값을 구한다]]></desc>
			<sql><![CDATA[
SELECT NVL( MAX( BKG_SEQ ), 0) + 1 PB_SEQ
  FROM BKG_CSTMS_PSA_BKG
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
