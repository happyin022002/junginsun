<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCmpnAuditDBDAOSearchVndrSeqCheckRSQL">
			<desc><![CDATA[SearchVndrSeqCheck]]></desc>
			<sql><![CDATA[
/* Vendor의 MDM_VENDOR를 조회한다. */
SELECT '*' VENDOR 
  FROM MDM_VENDOR 
 WHERE VNDR_SEQ = @[vndr_seq] 
AND NVL(DELT_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
