<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAOFicRouteGroupVORSQL">
			<desc><![CDATA[RouteGroup 조회]]></desc>
			<sql><![CDATA[
SELECT 
#if (${add_on_flag} == 'Y' )
	PRI_ADDON_RATE_CALCULATE_PKG.PRI_GETFICROUTEGROUP_FNC(@[eff_dt], @[svc_scp_cd], @[prc_io_bnd_cd], @[rout_pnt_loc_def_cd], @[bse_port_def_cd], @[rcv_de_term_cd], @[prc_trsp_mod_cd]) AS FIC_RT_USE_STS_CD
     , PRI_ADDON_RATE_CALCULATE_PKG.PRI_getBasePortList_FNC(@[eff_dt], @[svc_scp_cd], @[prc_io_bnd_cd], @[rout_pnt_loc_def_cd], @[rcv_de_term_cd], NULL, 'N') AS BASE_PORT_LIST
     , (SELECT TRIM(REGEXP_SUBSTR(DATA, '[^|]+', 1, 2))
        FROM   (SELECT PRI_ADDON_RATE_CALCULATE_PKG.PRI_getFICTransByRoute_FNC(@[eff_dt], @[svc_scp_cd], @[prc_io_bnd_cd], @[rout_pnt_loc_def_cd], @[bse_port_def_cd], @[rcv_de_term_cd]) DATA FROM DUAL)
       ) PRC_TRSP_MOD_CD
#else
	PRI_FIC_RATE_CALCULATE_PKG.PRI_GETFICROUTEGROUP_FNC(@[eff_dt], @[svc_scp_cd], @[rout_pnt_loc_def_cd], @[bse_port_def_cd], @[rcv_de_term_cd], @[prc_trsp_mod_cd]) AS FIC_RT_USE_STS_CD
     , PRI_FIC_RATE_CALCULATE_PKG.PRI_getBasePortList_FNC(@[eff_dt], @[svc_scp_cd], @[rout_pnt_loc_def_cd], @[rcv_de_term_cd], NULL, 'N') AS BASE_PORT_LIST
     , (SELECT TRIM(REGEXP_SUBSTR(DATA, '[^|]+', 1, 2))
        FROM   (SELECT PRI_FIC_RATE_CALCULATE_PKG.PRI_getFICTransByRoute_FNC(@[eff_dt], @[svc_scp_cd], @[rout_pnt_loc_def_cd], @[bse_port_def_cd], @[rcv_de_term_cd]) DATA FROM DUAL)
       ) PRC_TRSP_MOD_CD
#end
     , @[eff_dt] EFF_DT
     , @[svc_scp_cd] SVC_SCP_CD
     , @[rout_pnt_loc_def_cd] ROUT_PNT_LOC_DEF_CD
     , @[bse_port_def_cd] BSE_PORT_DEF_CD
     , @[rcv_de_term_cd] RCV_DE_TERM_CD
FROM DUAL			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="prc_io_bnd_cd" type="12" value="" out="N"/>
				<param name="rout_pnt_loc_def_cd" type="12" value="" out="N"/>
				<param name="bse_port_def_cd" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="prc_trsp_mod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
