<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOMultiWeeklyTargetVVD3USQL">
			<desc><![CDATA[MultiWeeklyTargetVVD UPDATE3]]></desc>
			<sql><![CDATA[
UPDATE MAS_MON_VVD SET
   DELT_FLG   = @[delt_flg]
  ,UPD_USR_ID = @[upd_usr_id]
  ,UPD_DT     = SYSDATE
WHERE 1=1
  AND TRD_CD     = @[trd_cd]
  AND RLANE_CD   = @[rlane_cd]
  AND IOC_CD     = @[ioc_cd]
  AND VSL_CD     = @[vsl_cd]
  AND SKD_VOY_NO = @[skd_voy_no]
  AND DIR_CD     = @[dir_cd]
			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="trd_cd" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="ioc_cd" type="12" value="" out="Y"/>
				<param name="vsl_cd" type="12" value="" out="Y"/>
				<param name="skd_voy_no" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
