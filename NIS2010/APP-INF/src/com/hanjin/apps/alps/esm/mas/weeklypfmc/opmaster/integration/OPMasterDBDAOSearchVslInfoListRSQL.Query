<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPMasterDBDAOSearchVslInfoListRSQL">
			<desc><![CDATA[Vessel Info]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
      ,VSL_ENG_NM
      ,CRR_CD
      ,CNTR_DZN_CAPA
      ,CNTR_VSL_CLSS_CAPA		
  FROM MDM_VSL_CNTR
 WHERE DELT_FLG = 'N'
#if (${f_vsl_cd} != '') 
   AND VSL_CD = @[f_vsl_cd]
#end
#if (${f_crr_cd} != '') 
   AND CRR_CD = @[f_crr_cd]
#end			]]></sql>
			<params>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
				<param name="f_crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
