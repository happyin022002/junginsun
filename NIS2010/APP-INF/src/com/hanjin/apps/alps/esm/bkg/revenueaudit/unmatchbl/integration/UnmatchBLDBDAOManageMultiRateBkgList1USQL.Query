<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOManageMultiRateBkgList1USQL">
			<desc><![CDATA[Multi Rate BKG List for Audit(1) 수정]]></desc>
			<sql><![CDATA[
UPDATE	BKG_REV_OCN_FRT_MLT_RAT_USR
SET		UPD_DT = SYSDATE
		,UPD_USR_ID = @[upd_usr_id]
#if (${btn} == 'SAVE')
		,USR_UPD_CTNT = @[usr_upd_ctnt]
		,USR_INS_AMT = @[usr_ins_amt]
#end
#if (${btn} == 'CONFIRM')
		,USR_UPD_CTNT = @[usr_upd_ctnt]
		,USR_INS_AMT = @[usr_ins_amt]
		,CFM_USR_ID = @[upd_usr_id]
		,CFM_DT = SYSDATE
		,USR_UPD_CFM_FLG = 'Y'
#end
#if (${btn} == 'RELEASE')
		,CFM_USR_ID = NULL
		,CFM_DT = NULL
		,USR_UPD_CFM_FLG = 'N'
#end
WHERE	BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_upd_ctnt" type="12" value="" out="N"/>
				<param name="usr_ins_amt" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
