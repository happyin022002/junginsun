<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOUpdateVslLayupTotCopyUSQL">
			<desc><![CDATA[COA_MNL_COST_STUP 테이블의 tot값을 업데이트]]></desc>
			<sql><![CDATA[
UPDATE COA_MNL_COST_STUP 
SET OTR_EXPN_AMT = @[tot_sum]
	,UPD_USR_ID = @[usr_id]
	,UPD_DT = SYSDATE
WHERE COST_YRMON = (SELECT COST_YR||EQ_WK FROM COA_WK_PRD WHERE COST_YR = SUBSTR(REPLACE(@[f_tar_week],'-',''),1,4) AND COST_WK = SUBSTR(REPLACE(@[f_tar_week],'-',''),5,2))
    AND COST_WK = SUBSTR(REPLACE(@[f_tar_week],'-',''),5,2)
	AND RLANE_CD = @[rlane_cd]			]]></sql>
			<params>
				<param name="tot_sum" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="f_tar_week" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
