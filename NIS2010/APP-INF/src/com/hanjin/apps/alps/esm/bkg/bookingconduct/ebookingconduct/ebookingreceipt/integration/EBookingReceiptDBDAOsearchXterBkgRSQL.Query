<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterBkgRSQL">
			<desc><![CDATA[searchXterBkg
2011.02.09 김영철 [CHM-201108366-01] E-SVC 화면 PICKUP DATE 항목 추가요청
2013.05.13 임재관 [CHM-201324602] e-Booking & SI Process 상 Special 정보 column 추가
2018.05.28 이인영 소문자로 들어온 입력값을 화면에서 대문자로 표시함.]]></desc>
			<sql><![CDATA[
select mst.bkg_no
        , mst.xter_rqst_no
        , mst.XTER_RQST_VIA_CD
        , (select INTG_CD_VAL_DESC from com_intg_cd_dtl where INTG_CD_ID = 'CD01622' and INTG_CD_VAL_CTNT = mst.XTER_RQST_VIA_CD) XTER_RQST_VIA_NM
        , MST.BKG_UPLD_STS_CD 
        , (SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01630' AND INTG_CD_VAL_CTNT = MST.BKG_UPLD_STS_CD) BKG_UPLD_STS_NM
        , MST.DOC_TP_CD
        , mst.bl_no_ctnt
        , decode(nvl((select count(1) from bkg_booking where bkg_no = mst.bkg_no), 0), 0, 'No', 'Yes') ALPS
        , mst.hbl_knt
        , UPPER(sh.cnt_cd)   sh_cnt_cd
        , UPPER(sh.cust_seq) sh_cust_seq
        , UPPER(sh.cust_nm)  sh_cust_nm
        , UPPER(ff.cnt_cd)   ff_cnt_cd
        , UPPER(ff.cust_seq) ff_cust_seq
        , UPPER(ff.cust_nm)  ff_cust_nm
        , UPPER(mst.VSL_CD||SKD_VOY_NO||SKD_DIR_CD) vvd
        , UPPER(mst.VSL_NM) VSL_NM
        , UPPER(mst.SREP_CD) SREP_CD
        , UPPER(mst.por_Cd) POR_CD
        , UPPER(mst.por_nm) POR_NM
        , UPPER(mst.pol_Cd) POL_CD
        , UPPER(mst.pol_nm) POL_NM
        , UPPER(mst.pod_Cd) POD_CD
        , UPPER(mst.pod_nm) POD_NM
        , UPPER(mst.del_Cd) DEL_CD
        , UPPER(mst.del_nm) DEL_NM
        , UPPER(SUBSTR(MST.POR_NOD_CD,6,2)) POR_YD_CD
        , UPPER(SUBSTR(MST.POL_NOD_CD,6,2)) POL_YD_CD
        , UPPER(SUBSTR(MST.POD_NOD_CD,6,2)) POD_YD_CD
        , UPPER(SUBSTR(MST.DEL_NOD_CD,6,2)) DEL_YD_CD
        , UPPER(mst.fnl_dest_nm) FNL_DEST_NM
        , mst.FRT_TERM_CD
        , (select INTG_CD_VAL_DESC from com_intg_cd_dtl where INTG_CD_ID = 'CD00764' and INTG_CD_VAL_CTNT = mst.RCV_TERM_CD) rcv_term
        , (select INTG_CD_VAL_DESC from com_intg_cd_dtl where INTG_CD_ID = 'CD00765' and INTG_CD_VAL_CTNT = mst.DE_TERM_CD) dlv_term
        , UPPER(mst.TWN_SO_NO) TWN_SO_NO
        , UPPER(decode(length(trim(mst.ctrt_no)), 8, 'SC', 9, 'SC', 'RFA')) SC_RFA
        , UPPER(mst.ctrt_no) CTRT_NO
        , UPPER(mst.cmdt_Cd) CMDT_CD
        , UPPER(mst.cmdt_desc) CMDT_DESC
        , mst.USA_CSTMS_FILE_ctnt
        , mst.CND_CSTMS_FILE_CD
        , mst.ESTM_WGT
		, DECODE(SUBSTR(mst.ESTM_WGT_UT_CD,1,1),'K','KGS',mst.ESTM_WGT_UT_CD) ESTM_WGT_UT_CD
        , mst.DCGO_FLG
        , mst.RC_FLG
        , mst.AWK_CGO_FLG
        , mst.BB_CGO_FLG
        , mst.SOC_FLG
        , to_char(mst.RQST_RTN_DT, 'yyyy-mm-dd') return_dt
        , to_char(mst.RQST_DEP_DT, 'yyyy-mm-dd') departure_Dt
        , to_char(mst.RQST_ARR_DT, 'yyyy-mm-dd') arrival_dt       
        , mst.XTER_BKG_RMK1||mst.XTER_BKG_RMK2 
        || CHR(13)
        || REPLACE(BKG_JOIN_FNC( CURSOR(
                                SELECT WFG_MF_NO
                                  FROM BKG_XTER_WFG WFG
                                 WHERE WFG.XTER_SNDR_ID = MST.XTER_SNDR_ID
                                   AND WFG.XTER_RQST_NO = MST.XTER_RQST_NO
                                   AND WFG.XTER_RQST_SEQ = MST.XTER_RQST_SEQ)), ',', CHR(13))
        || CASE WHEN XTER_RQST_VIA_CD = 'SIM' AND DOC_TP_CD ='B' THEN CHR(13) || ex.cust_nm END rmk 
        , decode(mst.AUTO_EML_FLG, 'Y', 'Yes', 'No') auto_notification
        , mst.CNTC_EML
        , mst.CNTC_NM
        , mst.CNTC_PHN_AREA_NO||mst.CNTC_PHN_NO||mst.CNTC_XTN_PHN_NO tel
        , mst.CNTC_MPHN_NO mobile
        , mst.CNTC_FAX_AREA_NO||mst.CNTC_FAX_NO fax
        , MST.SI_CNTC_EML
        , MST.SI_CNTC_NM
        , MST.SI_CNTC_PHN_AREA_NO || MST.SI_CNTC_PHN_NO || MST.SI_CNTC_XTN_PHN_NO AS SI_TEL
        , MST.SI_CNTC_MPHN_NO AS SI_MOBILE
        , MST.SI_CNTC_FAX_AREA_NO || MST.SI_CNTC_FAX_NO AS SI_FAX
		, mst.XTER_BKG_RQST_STS_CD
		, MST.SNACCS_SPLIT_NO
		, mst.XTER_CHG_ARR_FLG
		, MST.XTER_AGN_DP_FLG
		, MST.XTER_LIST_DP_FLG
		, MST.XTER_BL_TP_CD AS WY_BL_FLG
		, NVL(MST.BL_ISS_LOC_CD, MST.BL_ISS_LOC_NM) AS BL_ISS_LOC_CD
		, MST.BL_ISS_DT
		, MST.INCL_RT_BL_KNT
		, MST.XCLD_RT_BL_KNT
		, NVL(mst.XTER_RQST_ACPT_CD, 'X') XTER_RQST_ACPT_CD
		, mst.XTER_RQST_ACPT_USR_ID
		, (SELECT USR_NM FROM COM_USER WHERE UPPER(USR_ID) = UPPER(mst.XTER_RQST_ACPT_USR_ID)) XTER_RQST_ACPT_USR_NM
		, MST.SCAC_CD
		, MST.CUST_REF_NO CUST_REF_NO
		, NVL((SELECT TO_CHAR(DTL.DOR_RQST_DT,'yyyy-mm-dd')
				 	FROM BKG_XTER_TRO TRO, BKG_XTER_TRO_DTL DTL
					WHERE TRO.XTER_SNDR_ID = MST.XTER_SNDR_ID
					  AND TRO.XTER_RQST_NO = MST.XTER_RQST_NO
					  AND TRO.XTER_RQST_SEQ = MST.XTER_RQST_SEQ
					  AND TRO.XTER_SNDR_ID = DTL.XTER_SNDR_ID
					  AND TRO.XTER_RQST_NO = DTL.XTER_RQST_NO
					  AND TRO.XTER_RQST_SEQ = DTL.XTER_RQST_SEQ
					  AND ROWNUM = 1),TO_CHAR(MST.MTY_PKUP_DT,'yyyy-mm-dd')) MTY_PKUP_DT
		, ( SELECT TO_CHAR(DTL.DOR_RQST_DT,'yyyy-mm-dd')
		 	FROM BKG_XTER_TRO TRO, BKG_XTER_TRO_DTL DTL
			WHERE TRO.XTER_SNDR_ID = MST.XTER_SNDR_ID
			  AND TRO.XTER_RQST_NO = MST.XTER_RQST_NO
			  AND TRO.XTER_RQST_SEQ = MST.XTER_RQST_SEQ
			  AND TRO.XTER_SNDR_ID = DTL.XTER_SNDR_ID
			  AND TRO.XTER_RQST_NO = DTL.XTER_RQST_NO
			  AND TRO.XTER_RQST_SEQ = DTL.XTER_RQST_SEQ
			  AND ROWNUM = 1) TRO_PKUP_DT
        , MST.SPLIT_STS_CD 
		, nvl(NON_NEGO_RT_INCL_KNT,0) NON_NEGO_RT_INCL_KNT
        , nvl(NON_NEGO_RT_XCLD_KNT,0) NON_NEGO_RT_XCLD_KNT
		, DECODE(DOC_TP_CD,'B',MST.XTER_RQST_NO) BKG_REF_NO -- BKG_REF_NO
	    , DECODE(DOC_TP_CD,'B',DECODE(MST.XTER_SNDR_ID,'SEANACCS',CUST_REF_NO, SHPR_REF_NO)) BKG_SH_REF_NO -- BKG_SH_REF_NO
	    , DECODE(DOC_TP_CD,'B',FWRD_REF_NO) BKG_FF_REF_NO -- BKG_FF_REF_NO
       	, DECODE(DOC_TP_CD,'S',MST.XTER_RQST_NO) SI_REF_NO -- SI_REF_NO
       	, DECODE(DOC_TP_CD,'S',DECODE(MST.XTER_SNDR_ID,'SEANACCS',CUST_REF_NO, SHPR_REF_NO)) SI_SH_REF_NO -- SI_SH_REF_NO
       	, DECODE(DOC_TP_CD,'S',FWRD_REF_NO) SI_FF_REF_NO -- SI_FF_REF_NO
        , INV_REF_NO -- FINV
        , XPT_MRN_NO AS EXT_MRN_NO-- XMRN
		, MST.CUST_ID
		, NVL(MST.FLEX_HGT_FLG, 'N') FLEX_HGT_FLG
		, MST.SYS_UPLD_FLG
		, MST.SI_AUD_FLG
		, MST.CMPB_RT_FLG
		, MST.HNDL_OFC_CD
		, mst.spc_ctrlr_rmk
  from bkg_xter_rqst_mst mst, bkg_xter_cust sh, bkg_xter_cust ff, bkg_xter_cust ex
 where mst.xter_sndr_id  = sh.xter_sndr_id(+)
   and mst.xter_rqst_no  = sh.xter_rqst_no(+)
   and mst.xter_rqst_seq = sh.xter_rqst_seq(+)
   and 'S'               = sh.xter_cust_tp_cd(+)
   and mst.xter_sndr_id  = ff.xter_sndr_id(+)
   and mst.xter_rqst_no  = ff.xter_rqst_no(+)
   and mst.xter_rqst_seq = ff.xter_rqst_seq(+)
   and 'F'               = ff.xter_cust_tp_cd(+)
   and mst.xter_sndr_id  = ex.xter_sndr_id(+)
   and mst.xter_rqst_no  = ex.xter_rqst_no(+)
   and mst.xter_rqst_seq = ex.xter_rqst_seq(+)
   and 'E'               = ex.xter_cust_tp_cd(+)
   and mst.xter_sndr_id      = @[sender_id]
   and mst.xter_rqst_no      = @[rqst_no]
   and mst.xter_rqst_seq     = @[rqst_seq]			]]></sql>
			<params>
				<param name="sender_id" type="12" value="C1T0W" out="N"/>
				<param name="rqst_no" type="12" value="A5EM130703Q001002" out="N"/>
				<param name="rqst_seq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
