<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOCreateAverageMonthCopyCSQL">
			<desc><![CDATA[Average UC 월단가를 복사해서 생성한다.]]></desc>
			<sql><![CDATA[
 INSERT
   INTO COA_OP_AVG_UT_COST
        (
                COST_YRMON
              , TRD_CD
              , RLANE_CD
              , STND_COST_CD
              , OP_LANE_TP_CD
              , OP_COST_AMT
              , BSA_CAPA
              , OP_UC_AMT
              , VVD_RMK
              , CRE_USR_ID
              , CRE_DT
              , UPD_USR_ID
              , UPD_DT
              , COST_USE_TP_CD
              , DIR_CD
        )
 SELECT @[f_tar_mon]
      , TRD_CD
      , RLANE_CD
      , STND_COST_CD
      , OP_LANE_TP_CD
      , OP_COST_AMT
      , BSA_CAPA
      , OP_UC_AMT
      , VVD_RMK
      , @[user_id]
      , SYSDATE
      , @[user_id]
      , SYSDATE
      , COST_USE_TP_CD
      , DIR_CD
   FROM COA_OP_AVG_UT_COST
  WHERE COST_YRMON     = @[f_src_mon]
    AND COST_USE_TP_CD = @[cost_use_tp_cd]			]]></sql>
			<params>
				<param name="f_tar_mon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_src_mon" type="12" value="" out="N"/>
				<param name="cost_use_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
