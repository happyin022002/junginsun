<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOModifySpcJoOwnVslWeightUSQL">
			<desc><![CDATA[ModifySpcJoOwnVslWeight UPDATE]]></desc>
			<sql><![CDATA[
UPDATE BSA_JNT_OP_BZC 
    SET OWNR_VSL_WGT = @[ownr_vsl_wgt] 
       ,UPD_USR_ID   = @[upd_usr_id]
       ,UPD_DT       = SYSDATE 
 WHERE BSA_SEQ  = @[bsa_seq] 
   AND RLANE_CD = @[rlane_cd]
   AND DIR_CD   = @[dir_cd]
   AND TRD_CD   = @[trd_cd]
   AND VOP_CD   = @[vop_cd]
   AND VSL_CAPA = @[vsl_capa]			]]></sql>
			<params>
				<param name="ownr_vsl_wgt" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="bsa_seq" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
				<param name="trd_cd" type="12" value="" out="Y"/>
				<param name="vop_cd" type="12" value="" out="Y"/>
				<param name="vsl_capa" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
