<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCllLoadSumByPodRSQL">
			<desc><![CDATA[searchCllLoadSumByPod]]></desc>
			<sql><![CDATA[
SELECT *
FROM (

#if (${in_pgm_no} == 'ESM_BKG_0951') 
/**************************** ESM_BKG_0951에서 조회된 경우  *****************************/
		SELECT POD_CD GUBUN_CD,
			BLCK_STWG_CD AS GUBUN_CD2,
			'' GUBUN_CD3,
			1 ORDER_GUBUN,
			DECODE(SUM(LOCAL_40H_5 + LOCAL_40H_9 + LOCAL_40H_8), 0, '', SUM(LOCAL_40H_5 + LOCAL_40H_9 + LOCAL_40H_8)) LOCAL_40H,
			DECODE(SUM(LOCAL_40), 0, '', SUM(LOCAL_40)) LOCAL_40,
			DECODE(SUM(LOCAL_20), 0, '', SUM(LOCAL_20)) LOCAL_20,
			DECODE(SUM(LOCAL_45), 0, '', SUM(LOCAL_45)) LOCAL_45,
			DECODE(SUM(TS_40H_5 + TS_40H_9 + TS_40H_8) , 0, '', SUM(TS_40H_5 + TS_40H_9 + TS_40H_8)) TS_40H,
			DECODE(SUM(TS_40), 0, '', SUM(TS_40)) TS_40,
			DECODE(SUM(TS_20), 0, '', SUM(TS_20)) TS_20,
			DECODE(SUM(TS_45), 0, '', SUM(TS_45)) TS_45,
			DECODE(SUM(MTY_40H_5 + MTY_40H_9 + MTY_40H_8), 0, '', SUM(MTY_40H_5 + MTY_40H_9 + MTY_40H_9)) MTY_40H,
			DECODE(SUM(MTY_40), 0, '', SUM(MTY_40)) MTY_40,
			DECODE(SUM(MTY_20), 0, '', SUM(MTY_20)) MTY_20,
			DECODE(SUM(MTY_45), 0, '', SUM(MTY_45)) MTY_45,
			DECODE(SUM(NVL(LOCAL_40H_5,0) + NVL(LOCAL_40H_9,0) + NVL(LOCAL_40H_8,0) + NVL(TS_40H_5,0) + NVL(TS_40H_9,0) + NVL(TS_40H_8,0) + NVL(MTY_40H_5,0) + NVL(MTY_40H_9,0)+ NVL(MTY_40H_8,0)), 0, '', SUM(NVL(LOCAL_40H_5,0) + NVL(LOCAL_40H_9,0) + NVL(LOCAL_40H_8,0) + NVL(TS_40H_5,0) + NVL(TS_40H_9,0) + NVL(TS_40H_8,0) + NVL(MTY_40H_5,0) + NVL(MTY_40H_9,0) + NVL(MTY_40H_8,0))) SUM_40H,
			DECODE(SUM(NVL(LOCAL_40,0) + NVL(TS_40,0) + NVL(MTY_40,0)), 0, '', SUM(NVL(LOCAL_40,0) + NVL(TS_40,0) + NVL(MTY_40,0))) SUM_40,
			DECODE(SUM(NVL(LOCAL_20,0) + NVL(TS_20,0) + NVL(MTY_20,0)), 0, '', SUM(NVL(LOCAL_20,0) + NVL(TS_20,0) + NVL(MTY_20,0))) SUM_20,
			DECODE(SUM(NVL(LOCAL_45,0) + NVL(TS_45,0) + NVL(MTY_45,0)), 0, '', SUM(NVL(LOCAL_45,0) + NVL(TS_45,0) + NVL(MTY_45,0))) SUM_45,
			ROUND(DECODE(SUM(WGT + WGT_MT),0,'',SUM(WGT + WGT_MT)), 1) WGT_MT,
			--NVL(ROUND(SUM(WGT), 0), 1) WGT_MT,
			CLPT_SEQ
		FROM (
				SELECT MAX(DECODE(BK.POD_CD,'XXXXX','XXXXX',NVL(LO.UN_LOC_CD,BV.POD_CD))) POD_CD,
					BK.BLCK_STWG_CD AS BLCK_STWG_CD,
					MAX(DECODE(BK.POD_CD,'XXXXX',100,VPS.CLPT_SEQ)) CLPT_SEQ,
					1,
				
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'5',BQ.OP_CNTR_QTY,0),0)) LOCAL_40H_5,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'8',BQ.OP_CNTR_QTY,0),0)) LOCAL_40H_8,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'9',BQ.OP_CNTR_QTY,0),0)) LOCAL_40H_9,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '5', 0, '7', 0, '8', 0,'9', 0, BQ.OP_CNTR_QTY),0)) LOCAL_40,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0),0)) LOCAL_20,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',BQ.OP_CNTR_QTY,0),0)) LOCAL_45,

					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'5',BQ.OP_CNTR_QTY,0))) TS_40H_5,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'8',BQ.OP_CNTR_QTY,0))) TS_40H_8,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'9',BQ.OP_CNTR_QTY,0))) TS_40H_9,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '5', 0, '7', 0, '8', 0, '9', 0, BQ.OP_CNTR_QTY))) TS_40,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0))) TS_20,
					DECODE(BKG_CGO_TP_CD, 'P', 0, 'R', 0, DECODE(BV.POL_CD,BK.POL_CD, 0, DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',BQ.OP_CNTR_QTY,0))) TS_45,

					DECODE(BKG_CGO_TP_CD, 'P', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'5',BQ.OP_CNTR_QTY,0), 0, 'R', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'5',BQ.OP_CNTR_QTY,0), 0) MTY_40H_5,
					DECODE(BKG_CGO_TP_CD, 'P', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'8',BQ.OP_CNTR_QTY,0), 0, 'R', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'8',BQ.OP_CNTR_QTY,0), 0) MTY_40H_8,
					DECODE(BKG_CGO_TP_CD, 'P', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'9',BQ.OP_CNTR_QTY,0), 0, 'R', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'9',BQ.OP_CNTR_QTY,0), 0) MTY_40H_9,
					DECODE(BKG_CGO_TP_CD, 'P', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '5', 0, '7', 0, '8', 0, '9', 0, BQ.OP_CNTR_QTY), 0, 'R', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2', 0, '5', 0, '7', 0, '8', 0, '9', 0, BQ.OP_CNTR_QTY), 0) MTY_40,
					DECODE(BKG_CGO_TP_CD, 'P', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0), 0, 'R', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'2',BQ.OP_CNTR_QTY,0), 0) MTY_20,
					DECODE(BKG_CGO_TP_CD, 'P', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',BQ.OP_CNTR_QTY,0), 0, 'R', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD,2,1),'7',BQ.OP_CNTR_QTY,0), 0) MTY_45,

					SUM(TP.CNTR_TPSZ_TARE_WGT*BQ.OP_CNTR_QTY) WGT_MT,
					MAX(DECODE(NVL(DOC.WGT_UT_CD, 0), 'LBS', ROUND (NVL(DOC.ACT_WGT, 0)*0.4536, 3), NVL(DOC.ACT_WGT, 0))) WGT
				FROM BKG_BOOKING BK
			, BKG_BL_DOC DOC
			, BKG_VVD BV
			, BKG_QUANTITY BQ
			, VSK_VSL_PORT_SKD VPS
			, MDM_CNTR_TP_SZ TP
			, MDM_LOCATION LO
				WHERE BK.BKG_NO = BV.BKG_NO
				AND BK.BKG_NO = BQ.BKG_NO
				AND BK.BKG_NO = DOC.BKG_NO
				AND BQ.CNTR_TPSZ_CD = TP.CNTR_TPSZ_CD
				AND BV.VSL_CD = VPS.VSL_CD
				AND BV.SKD_VOY_NO = VPS.SKD_VOY_NO
				AND BV.SKD_DIR_CD = VPS.SKD_DIR_CD
				AND BV.POD_CD = VPS.VPS_PORT_CD
				AND VPS.VPS_PORT_CD = LO.LOC_CD
				AND BV.VSL_CD     = @[in_vsl_cd]
				AND BV.SKD_VOY_NO = @[in_skd_voy_no]
				AND BV.SKD_DIR_CD = @[in_skd_dir_cd]
				AND BV.POL_CD = @[in_pol_cd]
				AND SUBSTR(BV.POL_YD_CD, 6, 2) like @[in_pol_yd_cd]||'%'
				AND BK.BKG_STS_CD IN ( 'F','W')
				AND BQ.CNTR_TPSZ_CD NOT IN ('Q2','Q4')
				AND VPS.CLPT_IND_SEQ = '1'
				AND NVL(BK.SPLIT_RSN_CD,' ') <> 'M'
				GROUP BY BK.BKG_NO, BK.BLCK_STWG_CD, BKG_CGO_TP_CD, BV.POL_CD, BK.POL_CD, BQ.CNTR_TPSZ_CD, BQ.OP_CNTR_QTY )
		GROUP BY POD_CD, BLCK_STWG_CD, 1, CLPT_SEQ
		ORDER BY CLPT_SEQ
#else 
/***************************** 기본 조회 *****************************/
SELECT POD_CD GUBUN_CD,
  BLCK_STWG_CD AS GUBUN_CD2,   --'' GUBUN_CD2,
  '' GUBUN_CD3,
  1 ORDER_GUBUN,
  DECODE(SUM(LOCAL_40H_5 + LOCAL_40H_8 + LOCAL_40H_9), 0, '', SUM(LOCAL_40H_5 + LOCAL_40H_8 + LOCAL_40H_9)) LOCAL_40H,
  DECODE(SUM(LOCAL_40), 0, '', SUM(LOCAL_40)) LOCAL_40,
  DECODE(SUM(LOCAL_20), 0, '', SUM(LOCAL_20)) LOCAL_20,
  DECODE(SUM(LOCAL_45), 0, '', SUM(LOCAL_45)) LOCAL_45,
  DECODE(SUM(TS_40H_5 + TS_40H_8 + TS_40H_9) , 0, '', SUM(TS_40H_5 + TS_40H_8 + TS_40H_9)) TS_40H,
  DECODE(SUM(TS_40), 0, '', SUM(TS_40)) TS_40,
  DECODE(SUM(TS_20), 0, '', SUM(TS_20)) TS_20,
  DECODE(SUM(TS_45), 0, '', SUM(TS_45)) TS_45,
  DECODE(SUM(MTY_40H_5 + MTY_40H_8 + MTY_40H_9), 0, '', SUM(MTY_40H_5 + MTY_40H_8 + MTY_40H_9)) MTY_40H,
  DECODE(SUM(MTY_40), 0, '', SUM(MTY_40)) MTY_40,
  DECODE(SUM(MTY_20), 0, '', SUM(MTY_20)) MTY_20,
  DECODE(SUM(MTY_45), 0, '', SUM(MTY_45)) MTY_45,
  DECODE(SUM(LOCAL_40H_5 + LOCAL_40H_8 + LOCAL_40H_9 + TS_40H_5 + TS_40H_9 + MTY_40H_5 + MTY_40H_9), 0, '', SUM(LOCAL_40H_5 + LOCAL_40H_8 + LOCAL_40H_9 + TS_40H_5 + TS_40H_9 + MTY_40H_5 + MTY_40H_9)) SUM_40H,
  DECODE(SUM(LOCAL_40 + TS_40 + MTY_40), 0, '', SUM(LOCAL_40 + TS_40 + MTY_40)) SUM_40,
  DECODE(SUM(LOCAL_20 + TS_20 + MTY_20), 0, '', SUM(LOCAL_20 + TS_20 + MTY_20)) SUM_20,
  DECODE(SUM(LOCAL_45 + TS_45 + MTY_45), 0, '', SUM(LOCAL_45 + TS_45 + MTY_45)) SUM_45,
  NVL(ROUND(SUM(WGT), 0), 1) WGT_MT,
  CLPT_SEQ
FROM (
    SELECT NVL(LO.UN_LOC_CD, CLL.POD_CD) POD_CD,
       BKG.BLCK_STWG_CD AS BLCK_STWG_CD,
      BQ.CLPT_SEQ CLPT_SEQ,
      1,
      DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.mty_bkg_cd, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '5', COUNT(CLL.BKG_NO), 0), 0), 0) LOCAL_40H_5,
      DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.mty_bkg_cd, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '8', COUNT(CLL.BKG_NO), 0), 0), 0) LOCAL_40H_8,
      DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.mty_bkg_cd, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '9', COUNT(CLL.BKG_NO), 0), 0), 0) LOCAL_40H_9,
      DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.mty_bkg_cd, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '5', 0, '7', 0, '8', 0, '9', 0, COUNT(CLL.BKG_NO)), 0), 0) LOCAL_40,
      DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.mty_bkg_cd, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0), 0), 0) LOCAL_20,
      DECODE(CLL.KR_CLL_TS_CD, NULL, DECODE(CLL.mty_bkg_cd, 'F', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0), 0), 0) LOCAL_45,
      
      DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '5', COUNT(CLL.BKG_NO), 0), 'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '5', COUNT(CLL.BKG_NO), 0), 0) TS_40H_5,
      DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '8', COUNT(CLL.BKG_NO), 0), 'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '8', COUNT(CLL.BKG_NO), 0), 0) TS_40H_8,
      DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '9', COUNT(CLL.BKG_NO), 0), 'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '9', COUNT(CLL.BKG_NO), 0), 0) TS_40H_9,
      DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '5', 0, '7', 0, '9', 0, COUNT(CLL.BKG_NO)), 'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '5', 0, '7', 0, '8',0,'9', 0, COUNT(CLL.BKG_NO)), 0) TS_40,
      DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0), 'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0), 0) TS_20,
      DECODE(CLL.KR_CLL_TS_CD, 'TT', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0), 'TS', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0), 0) TS_45,
      
      DECODE(CLL.mty_bkg_cd, 'M', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '5', COUNT(CLL.BKG_NO), 0), 0) MTY_40H_5,
      DECODE(CLL.mty_bkg_cd, 'M', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '8', COUNT(CLL.BKG_NO), 0), 0) MTY_40H_8,
      DECODE(CLL.mty_bkg_cd, 'M', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '9', COUNT(CLL.BKG_NO), 0), 0) MTY_40H_9,
      DECODE(CLL.mty_bkg_cd, 'M', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', 0, '5', 0, '7', 0, '8', 0, '9', 0, COUNT(CLL.BKG_NO)), 0) MTY_40,
      DECODE(CLL.mty_bkg_cd, 'M', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '2', COUNT(CLL.BKG_NO), 0), 0) MTY_20,
      DECODE(CLL.mty_bkg_cd, 'M', DECODE(SUBSTR(CLL.CNTR_TPSZ_CD, 2, 1), '7', COUNT(CLL.BKG_NO), 0), 0) MTY_45,

      SUM(TP.CNTR_TPSZ_TARE_WGT*BQ.OP_CNTR_QTY) WGT_MT,
      SUM(DECODE(NVL(CLL.WGT_UT_CD, 0), 'LBS', ROUND (NVL(CLL.BL_WGT, 0)*0.4536, 3), NVL(CLL.BL_WGT, 0))) WGT
    FROM BKG_CSTMS_TML_KR_CLL CLL,
         BKG_BOOKING BKG,
       (SELECT QTY.BKG_NO,
          MAX(QTY.CNTR_TPSZ_CD) CNTR_TPSZ_CD,
          MAX(QTY.OP_CNTR_QTY) OP_CNTR_QTY,
          VPS.CLPT_SEQ CLPT_SEQ
        FROM BKG_QUANTITY QTY,
          BKG_CSTMS_TML_KR_CLL CLL,
          VSK_VSL_PORT_SKD VPS,
          MDM_LOCATION LO
        WHERE QTY.CNTR_TPSZ_CD NOT IN ('Q2', 'Q4')
          AND QTY.BKG_NO = CLL.BKG_NO
          AND CLL.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no], 2, 3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd], 3, 3)
          AND VPS.VSL_CD = @[in_vsl_cd]
          AND VPS.SKD_VOY_NO = @[in_skd_voy_no]
          AND VPS.SKD_DIR_CD = @[in_skd_dir_cd]
          AND VPS.VPS_PORT_CD = LO.LOC_CD
          AND (DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.UN_LOC_CD
              OR DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.LOC_CD)
          AND LO.CALL_PORT_FLG = 'Y'
          AND LO.DELT_FLG = 'N'
          AND VPS.CLPT_IND_SEQ = '1'
        GROUP BY QTY.BKG_NO, VPS.CLPT_SEQ ) BQ,
      (SELECT CNTR_TPSZ_CD, CNTR_TPSZ_TARE_WGT  FROM MDM_CNTR_TP_SZ) TP ,
		VSK_VSL_PORT_SKD VPS,
      MDM_LOCATION LO
    WHERE CLL.CNTR_LIST_NO = @[in_vsl_cd]||SUBSTR(@[in_skd_voy_no], 2, 3)||@[in_skd_dir_cd]||SUBSTR(@[in_pol_cd], 3, 3)
      AND CLL.POD_CD IS NOT NULL
      AND NVL(CLL.POL_YD_CD, ' ') like @[in_pol_yd_cd]||'%'
      AND CLL.BKG_NO = BQ.BKG_NO
      AND CLL.BKG_NO = BKG.BKG_NO
      AND BQ.CNTR_TPSZ_CD = TP.CNTR_TPSZ_CD
		AND VPS.VSL_CD = @[in_vsl_cd]
          AND VPS.SKD_VOY_NO = @[in_skd_voy_no]
          AND VPS.SKD_DIR_CD = @[in_skd_dir_cd]
          AND VPS.VPS_PORT_CD = LO.LOC_CD
      AND (DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.UN_LOC_CD
          OR DECODE(CLL.POD_CD, 'USSE1', 'USSEA', 'USSE8', 'USSEA', 'CAVA1', 'CAVAN', 'CAYVR', 'CAVAN', 'CAPR1', 'CAPRR', CLL.POD_CD) = LO.LOC_CD)
      AND LO.CALL_PORT_FLG = 'Y'
      AND LO.DELT_FLG = 'N'
      AND VPS.CLPT_IND_SEQ = '1'
    GROUP BY NVL(LO.UN_LOC_CD, CLL.POD_CD), BKG.BLCK_STWG_CD, BQ.CLPT_SEQ, CLL.KR_CLL_TS_CD, CLL.MTY_BKG_CD, CLL.CNTR_TPSZ_CD )
GROUP BY POD_CD, 1, BLCK_STWG_CD, CLPT_SEQ
ORDER BY CLPT_SEQ

#end
)			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voy_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
