<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOPriCmpbGrpCmdtVORSQL">
			<desc><![CDATA[PRI_CMPB_GRP_CMDT 테이블 조회]]></desc>
			<sql><![CDATA[
SELECT GRP_CMDT_SEQ || ',' || CRE_OFC_CD || ',' || GLINE_SEQ AS SEQ
     , PRC_GRP_CMDT_CD AS CD
     , PRC_GRP_CMDT_DESC AS NM
  FROM PRI_CMPB_GRP_CMDT
 WHERE SVC_SCP_CD = @[svc_scp_cd]
#if (${cre_ofc_cd} != '') 
   AND CRE_OFC_CD = @[cre_ofc_cd]
#end
#if (${gline_seq} != '') 
   AND GLINE_SEQ = @[gline_seq]
#end
ORDER BY PRC_GRP_CMDT_CD ASC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
