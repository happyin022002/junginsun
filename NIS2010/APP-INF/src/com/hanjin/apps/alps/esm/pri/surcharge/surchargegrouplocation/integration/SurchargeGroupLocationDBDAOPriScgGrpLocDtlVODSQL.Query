<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeGroupLocationDBDAOPriScgGrpLocDtlVODSQL">
			<desc><![CDATA[Group Location detail delete]]></desc>
			<sql><![CDATA[
delete from pri_scg_grp_loc_dtl
where	svc_scp_cd = @[svc_scp_cd]
and	chg_cd = @[chg_cd]
and	grp_loc_seq = @[grp_loc_seq]
and	grp_loc_dtl_seq = @[grp_loc_dtl_seq]
			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="grp_loc_seq" type="12" value="" out="N"/>
				<param name="grp_loc_dtl_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
