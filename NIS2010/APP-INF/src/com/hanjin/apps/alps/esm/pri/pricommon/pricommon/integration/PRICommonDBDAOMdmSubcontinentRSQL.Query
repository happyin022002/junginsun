<sqls>
	<querys>
		<query name="PRICommonDBDAOMdmSubcontinentRSQL">
			<desc><![CDATA[Combo데이터를 생성하기 위해 MDM_SUBCONTINENT 테이블 데이터를 조회하는 SQL]]></desc>
			<sql><![CDATA[
SELECT 
  SCONTI_CD AS CD
, SCONTI_NM AS NM
FROM MDM_SUBCONTINENT
WHERE	DELT_FLG = 'N'
ORDER BY SCONTI_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
