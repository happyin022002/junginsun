<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOSearchVvdListByCharterRSQL">
			<desc><![CDATA[TCharterIOInvoiceDAOSearchVvdListByCharterRSQL]]></desc>
			<sql><![CDATA[
SELECT VVD
  FROM (
		SELECT VVD, VED_DT
		  FROM (
				SELECT FV.VSL_CD || FV.SKD_VOY_NO || FV.SKD_DIR_CD || FV.REV_DIR_CD VVD,
					   FV.VED_DT
				  FROM FMS_CONTRACT FC, FMS_VVD FV
				 WHERE FC.FLET_CTRT_NO = @[flet_ctrt_no]
				   AND FC.VSL_CD = FV.VSL_CD
				   AND @[rev_yrmon] BETWEEN FV.VST_DT AND FV.VED_DT
				UNION
				SELECT FV.VSL_CD || FV.SKD_VOY_NO || FV.SKD_DIR_CD || FV.REV_DIR_CD VVD,
					   FV.VED_DT
				  FROM FMS_ID_VSL FI, FMS_VVD FV 
				 WHERE FI.FLET_CTRT_NO = @[flet_ctrt_no]
				   AND FI.USE_FLG = 'Y'
				   AND FI.VSL_CD = FV.VSL_CD || '1234'
				   AND @[rev_yrmon] BETWEEN FV.VST_DT AND FV.VED_DT
			   ) ORDER BY VED_DT DESC
	   )			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
