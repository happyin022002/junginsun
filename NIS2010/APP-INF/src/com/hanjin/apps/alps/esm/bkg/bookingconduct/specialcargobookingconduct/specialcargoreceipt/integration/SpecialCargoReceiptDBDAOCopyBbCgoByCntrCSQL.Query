<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOCopyBbCgoByCntrCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BB_CGO
            (BKG_NO
,            BB_CGO_SEQ
,            RCV_TERM_CD
,            DE_TERM_CD
,            PCK_QTY
,            PCK_TP_CD
,            GRS_WGT
,            NET_WGT
,            WGT_UT_CD
,            DIM_LEN
,            DIM_WDT
,            DIM_HGT 
,            CGO_WGT
,            STWG_INSTR_DESC
,            BB_DCGO_SEQ
,            CMDT_CD
,            SLNG_PNT_FLG
,            GRAV_CTR_DESC
,            PCK_DTL_DESC
,            CGO_LODG_MZD_CD
,            SCR_DNG_CTNT
,            SPCL_RQST_DESC
,            DIFF_RMK
,            RQST_DT
,            RQST_USR_ID
,            SPCL_CGO_APRO_CD
,            CRE_USR_ID
,            CRE_DT
,            UPD_USR_ID
,            UPD_DT
,            CGO_DCHG_MZD_CD
            ) 
    SELECT @[tgt_bkg_no]
,          ((SELECT NVL(SUM(BB_CGO_SEQ), 0)
             FROM   BKG_BB_CGO
             WHERE  BKG_NO = @[tgt_bkg_no]) + ROW_NUMBER() OVER(PARTITION BY BKG_NO ORDER BY BB_CGO_SEQ))
,          RCV_TERM_CD
,          DE_TERM_CD
,          PCK_QTY
,          PCK_TP_CD
,          GRS_WGT
,          NET_WGT
,          WGT_UT_CD
,          DIM_LEN
,          DIM_WDT
,          DIM_HGT
,          CGO_WGT
,          STWG_INSTR_DESC
,          BB_DCGO_SEQ
,          CMDT_CD
,          SLNG_PNT_FLG
,          GRAV_CTR_DESC
,          PCK_DTL_DESC
,          CGO_LODG_MZD_CD
,          SCR_DNG_CTNT
,          SPCL_RQST_DESC
,          DIFF_RMK
,          RQST_DT
,          RQST_USR_ID
,          SPCL_CGO_APRO_CD
,          @[cre_usr_id]
,          SYSDATE
,          @[cre_usr_id]
,          SYSDATE
,          CGO_DCHG_MZD_CD
    FROM   BKG_BB_CGO
    WHERE  BKG_NO = @[src_bkg_no]
    AND    CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="tgt_bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="src_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
