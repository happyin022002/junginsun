<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchOfficeCodeRSQL">
			<desc><![CDATA[searchOfficeCode]]></desc>
			<sql><![CDATA[
SELECT 
	   OFC_CD,
       OFC_ENG_NM,
       OFC_KRN_NM
FROM MDM_ORGANIZATION
WHERE OFC_CD = @[ofc_cd]
    AND NVL(DELT_FLG,'N') = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
