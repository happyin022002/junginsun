<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAORsltTriRtListVORSQL">
			<desc><![CDATA[TRI Rate 조회]]></desc>
			<sql><![CDATA[
SELECT T.TRI_PROP_NO
      ,T.AMDT_SEQ
      ,T.RAT_UT_CD
      ,T.PRC_CGO_TP_CD
      ,T.CURR_CD
      ,T.PROP_FRT_RT_AMT
      ,T.COFFR_FRT_RT_AMT
      ,T.FNL_FRT_RT_AMT
      ,T.NOTE_CTNT
      ,T.NOTE_CONV_MAPG_ID
      ,TO_CHAR(T.EFF_DT, 'YYYY-MM-DD') AS EFF_DT
      ,TO_CHAR(T.EXP_DT, 'YYYY-MM-DD') AS EXP_DT
      ,T.PRS_SCG_AMT
      ,T.PRS_RESPB_CM_UC_AMT
      ,T.PRS_RESPB_OPFIT_UC_AMT
      ,T.PRS_RESPB_CMPB_AMT
      ,T.PRS_GID_CMPB_AMT
      ,T.PRS_RESPB_OPB_AMT
      ,T.PRS_RESPB_CMPB_AMT - T.PRS_GID_CMPB_AMT AS DIFF
      ,T.PRS_PFIT_CM_UC_AMT
      ,T.PRS_PFIT_CMPB_AMT
      ,T.PRS_UPD_DT
      ,T.TRI_RQST_OFC_CD
      ,T.TRI_RQST_USR_ID
      ,(SELECT USR_NM
          FROM COM_USER
         WHERE USR_ID = T.TRI_RQST_USR_ID
           AND ROWNUM = 1) AS TRI_RQST_USR_NM
      ,T.TRI_APRO_OFC_CD
      ,T.TRI_APRO_USR_ID
      ,(SELECT USR_NM
          FROM COM_USER
         WHERE USR_ID = T.TRI_APRO_USR_ID
           AND ROWNUM = 1) AS TRI_APRO_USR_NM
      ,T.PROP_STS_CD
      ,(SELECT INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID = 'CD02395'
           AND INTG_CD_VAL_CTNT = T.PROP_STS_CD
           AND ROWNUM = 1) AS PROP_STS_NM
      ,TO_CHAR(T.PUB_DT, 'YYYY-MM-DD') AS PUB_DT
      ,T.GRI_APPL_TP_CD
      ,T.GRI_APPL_AMT
      ,TO_CHAR(T.EML_SND_DT, 'YYYY-MM-DD') AS EML_SND_DT
      ,NVL2(M.TRI_NO, SUBSTR(M.TRI_NO, 1, 6) || '-' || SUBSTR(M.TRI_NO, 7, 4) || '-' || SUBSTR(M.TRI_NO, 11), NULL) AS TRI_NO
      ,M.CMDT_CD
      ,M.PRS_RT_CMPB_CALC_FLG
      ,T.TRI_RMK
  FROM PRI_TRI_RT T, PRI_TRI_MN M
 WHERE T.TRI_PROP_NO = M.TRI_PROP_NO
   AND T.TRI_PROP_NO = @[tri_prop_no]
 ORDER BY T.AMDT_SEQ DESC			]]></sql>
			<params>
				<param name="tri_prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
