<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTAuditDBDAOCSRDetailforCommissionDtrbRSQL">
			<desc><![CDATA[ESM_AGT-0043 화면 조회]]></desc>
			<sql><![CDATA[
SELECT ROWNUM SEQ,
    INV_NO,
    NET_AMT,
    TAX_AMT,
    TOT_AMT
FROM (SELECT ATTR_CTNT1 AS INV_NO,
        SUM(DECODE(DTRB_COA_ACCT_CD,'111821',0,INV_AMT)) NET_AMT,
        SUM(DECODE(DTRB_COA_ACCT_CD,'111821',INV_AMT,0)) TAX_AMT,
        SUM(INV_AMT) TOT_AMT
    FROM AP_INV_DTRB
    WHERE CSR_NO = @[csr_no] -- //:csrNo
    GROUP BY ATTR_CTNT1
)			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
