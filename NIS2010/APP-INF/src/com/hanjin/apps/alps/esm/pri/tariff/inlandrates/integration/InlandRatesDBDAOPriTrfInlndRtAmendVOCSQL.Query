<sqls>
	<querys>
		<query name="InlandRatesDBDAOPriTrfInlndRtAmendVOCSQL">
			<desc><![CDATA[amend 시 복사]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_INLND_RT (
       TRF_PFX_CD
     , TRF_NO
     , TRF_INLND_SEQ
     , AMDT_SEQ
     , TRF_INLND_RT_SEQ
     , INLND_RT_BSE_LOC_CD
     , INLND_RT_BSE_LOC_ZIP_CD
     , INLND_RT_TERM_CD
     , INLND_RT_VIA_LOC_CD
     , PRC_INLND_RT_TRSP_MOD_CD
     , INLND_RT_LMT_WGT
     , INLND_RT_MIN_LMT_WGT
     , INLND_RT_LMT_WGT_UT_CD
     , PRC_CGO_TP_CD
     , CURR_CD
     , INLND_BX_RT_AMT
     , INLND_20FT_RT_AMT
     , INLND_40FT_RT_AMT
     , INLND_40FT_HC_RT_AMT
     , INLND_45FT_RT_AMT
     , INLND_ONE_WY_BX_RT_AMT
     , INLND_ONE_WY_20FT_RT_AMT
     , INLND_ONE_WY_40FT_RT_AMT
     , INLND_ONE_WY_40FT_HC_RT_AMT
     , INLND_ONE_WY_45FT_RT_AMT
     , INLND_RT_RMK
     , N1ST_CMNC_AMDT_SEQ
     , SRC_INFO_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
)

SELECT TRF_PFX_CD
     , TRF_NO
     , TRF_INLND_SEQ
     , AMDT_SEQ + 1
     , TRF_INLND_RT_SEQ
     , INLND_RT_BSE_LOC_CD
     , INLND_RT_BSE_LOC_ZIP_CD
     , INLND_RT_TERM_CD
     , INLND_RT_VIA_LOC_CD
     , PRC_INLND_RT_TRSP_MOD_CD
     , INLND_RT_LMT_WGT
     , INLND_RT_MIN_LMT_WGT
     , INLND_RT_LMT_WGT_UT_CD
     , PRC_CGO_TP_CD
     , CURR_CD
     , INLND_BX_RT_AMT
     , INLND_20FT_RT_AMT
     , INLND_40FT_RT_AMT
     , INLND_40FT_HC_RT_AMT
     , INLND_45FT_RT_AMT
     , INLND_ONE_WY_BX_RT_AMT
     , INLND_ONE_WY_20FT_RT_AMT
     , INLND_ONE_WY_40FT_RT_AMT
     , INLND_ONE_WY_40FT_HC_RT_AMT
     , INLND_ONE_WY_45FT_RT_AMT
     , INLND_RT_RMK
     , N1ST_CMNC_AMDT_SEQ
     , SRC_INFO_CD
     , @[cre_usr_id]
     , SYSDATE
     , @[cre_usr_id]
     , SYSDATE
  FROM PRI_TRF_INLND_RT
 WHERE TRF_PFX_CD		= @[trf_pfx_cd]
   AND TRF_NO			= @[trf_no]
   AND AMDT_SEQ			= @[amdt_seq] - 1
   AND TRF_INLND_SEQ	= @[trf_inlnd_seq]
   AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="trf_inlnd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
