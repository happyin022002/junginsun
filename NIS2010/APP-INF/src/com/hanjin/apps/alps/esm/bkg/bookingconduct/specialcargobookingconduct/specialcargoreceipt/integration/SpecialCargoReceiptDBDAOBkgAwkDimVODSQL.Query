<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOBkgAwkDimVODSQL">
			<desc><![CDATA[BkgAwkDimVO_delete]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')

DELETE FROM BKG_AWK_DIM_HIS
WHERE	BKG_NO = @[bkg_no]
AND	CORR_NO = 'TMP0000001'

#else

DELETE FROM BKG_AWK_DIM
WHERE	BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
