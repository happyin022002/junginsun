<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOcheckBkgBlackListRSQL">
			<desc><![CDATA[checkBkgBlackList]]></desc>
			<sql><![CDATA[
SELECT BLCK_KW_NM
FROM BKG_BLCK_KW_LIST A,
     BKG_XTER_CUST B
WHERE A.BLCK_KW_TP_CD='BLA'
  AND B.XTER_SNDR_ID = @[sender_id]
  AND B.XTER_RQST_NO = @[rqst_no]
  AND B.XTER_RQST_SEQ = @[rqst_seq]
  AND INSTR(REPLACE(UPPER(TRIM(B.CUST_NM)),' ','') , BLCK_KW_CTNT) > 0
  AND ROWNUM = 1			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
