<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchSiFlgRSQL">
			<desc><![CDATA[SI_FLG 값을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(SI_FLG,'N') SI_FLG 
FROM   BKG_BOOKING
WHERE  BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
