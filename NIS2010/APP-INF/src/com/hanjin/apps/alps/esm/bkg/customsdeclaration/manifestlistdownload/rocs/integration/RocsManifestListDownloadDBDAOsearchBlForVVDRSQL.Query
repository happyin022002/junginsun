<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchBlForVVDRSQL">
			<desc><![CDATA[CRN, VVD로 BL list 찾기]]></desc>
			<sql><![CDATA[
SELECT RBL.VSL_CALL_REF_NO AS VSL_CALL_REF_NO_OLD
     , RBL.BKG_NO
	 , RBL.VSL_CD
	 , RBL.SKD_VOY_NO
	 , RBL.SKD_DIR_CD
 FROM BKG_CSTMS_RTM_BL RBL
WHERE VVD.BKG_NO = RBL.BKG_NO
  AND RBL.VSL_CALL_REF_NO = @[vsl_call_ref_no_old]
  AND RBL.VSL_CD = @[vsl_cd]
  AND RBL.SKD_VOY_NO = @[skd_voy_no]
  AND RBL.SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="vsl_call_ref_no_old" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
