<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOaddBkgKrWhfPortRtCSQL">
			<desc><![CDATA[I]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_KR_WHF_PORT_RT
( CNTR_BLK_DIV_CD
,PORT_CD
,IO_BND_CD
,DC_RTO_NO
,TEU_PRC
,FEU_PRC
,HC_PRC
,TEU_AMT_RT
,FEU_AMT_RT
,HC_AMT_RT 
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
,EFF_FM_DT
,EFF_TO_DT
,PORT_SEQ
) VALUES (
 @[cntr_blk_div_cd]
,@[port_cd]
,@[io_bnd_cd]
,@[dc_rto_no]
,@[teu_prc]
,@[feu_prc]
,@[hc_prc]
,@[teu_amt_rt]
,@[feu_amt_rt]
,@[hc_amt_rt] 
,@[cre_usr_id]
,SYSDATE
,@[upd_usr_id]
,SYSDATE
,TO_DATE(@[eff_fm_dt], 'YYYY-MM-DD')
,TO_DATE(@[eff_to_dt], 'YYYY-MM-DD')
,(SELECT NVL(MAX(PORT_SEQ), 0)+1 FROM BKG_KR_WHF_PORT_RT
	WHERE CNTR_BLK_DIV_CD = @[cntr_blk_div_cd]
	AND PORT_CD         = @[port_cd]
	AND IO_BND_CD       = @[io_bnd_cd]
	AND DC_RTO_NO       = @[dc_rto_no])
)			]]></sql>
			<params>
				<param name="cntr_blk_div_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dc_rto_no" type="12" value="" out="N"/>
				<param name="teu_prc" type="12" value="" out="N"/>
				<param name="feu_prc" type="12" value="" out="N"/>
				<param name="hc_prc" type="12" value="" out="N"/>
				<param name="teu_amt_rt" type="12" value="" out="N"/>
				<param name="feu_amt_rt" type="12" value="" out="N"/>
				<param name="hc_amt_rt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
