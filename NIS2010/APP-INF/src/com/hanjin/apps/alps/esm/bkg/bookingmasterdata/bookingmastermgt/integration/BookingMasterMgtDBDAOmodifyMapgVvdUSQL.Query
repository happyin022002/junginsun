<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOmodifyMapgVvdUSQL">
			<desc><![CDATA[타선사와 VVD 표현 방식이 다른 경우 매핑되는 데이터를 수정합니다]]></desc>
			<sql><![CDATA[
UPDATE BKG_MAPG_VVD
   SET MAPG_VSL_NM = UPPER(TRIM(REPLACE(@[mapg_vsl_nm], CHR(13)||CHR(10), '')))
     , MAPG_VOY_DIR_NM = @[mapg_voy_dir_nm]
     , UPD_DT = SYSDATE
     , UPD_USR_ID = @[usr_id]
 WHERE VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="mapg_vsl_nm" type="12" value="" out="N"/>
				<param name="mapg_voy_dir_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
