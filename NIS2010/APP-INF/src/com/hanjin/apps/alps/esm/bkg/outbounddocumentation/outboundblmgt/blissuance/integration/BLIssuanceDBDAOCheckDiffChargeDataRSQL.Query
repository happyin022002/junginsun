<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOCheckDiffChargeDataRSQL">
			<desc><![CDATA[CA Confirm시 Charge 데이터 바뀐 것이 있는지 체크]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN RT_SEQ IS NOT NULL THEN 'Y' ELSE 'N' END FLG
FROM
(
    SELECT RT_SEQ
          ,FRT_TERM_CD
          ,TRF_ITM_NO
          ,CGO_CATE_CD
          ,IMDG_CLSS_CD
          ,CHG_CD
          ,CURR_CD
          ,RAT_UT_CD
          ,RAT_AS_QTY
          ,CHG_UT_AMT
          ,CHG_AMT
          ,RCV_TERM_CD
          ,DE_TERM_CD
          ,N3PTY_RCV_OFC_CD
          ,N3PTY_CUST_CNT_CD
          ,N3PTY_CUST_SEQ
          ,FRT_INCL_XCLD_DIV_CD
          ,INV_STS_CD
          ,PRN_HDN_FLG
          ,AUTO_RAT_CD
          ,APLY_XCH_RTO
          ,AGMT_RAT_UT_CD
          ,NOTE_RT_SEQ
          ,PROP_NO
          ,AMDT_SEQ
          ,SVC_SCP_CD
          ,GEN_SPCL_RT_TP_CD
          ,CMDT_HDR_SEQ
          ,ROUT_SEQ
          ,FX_RT_FLG
          ,SOC_FLG
    FROM BKG_CHG_RT_HIS
    WHERE BKG_NO =@[bkg_no]
    AND CORR_NO = 'TMP0000001'
        MINUS
    SELECT RT_SEQ
          ,FRT_TERM_CD
          ,TRF_ITM_NO
          ,CGO_CATE_CD
          ,IMDG_CLSS_CD
          ,CHG_CD
          ,CURR_CD
          ,RAT_UT_CD
          ,RAT_AS_QTY
          ,CHG_UT_AMT
          ,CHG_AMT
          ,RCV_TERM_CD
          ,DE_TERM_CD
          ,N3PTY_RCV_OFC_CD
          ,N3PTY_CUST_CNT_CD
          ,N3PTY_CUST_SEQ
          ,FRT_INCL_XCLD_DIV_CD
          ,INV_STS_CD
          ,PRN_HDN_FLG
          ,AUTO_RAT_CD
          ,APLY_XCH_RTO
          ,AGMT_RAT_UT_CD
          ,NOTE_RT_SEQ
          ,PROP_NO
          ,AMDT_SEQ
          ,SVC_SCP_CD
          ,GEN_SPCL_RT_TP_CD
          ,CMDT_HDR_SEQ
          ,ROUT_SEQ
          ,FX_RT_FLG
          ,SOC_FLG
    FROM BKG_CHG_RT
    WHERE BKG_NO =@[bkg_no]
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
