<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSplitCombineDBDAOsearchCntrForSplitRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
select cntr_no
        , cntr_tpsz_cd
        , cnmv_sts_cd 
  from bkg_container
 where bkg_no = @[bkg_no] 
 ORDER BY CNTR_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
