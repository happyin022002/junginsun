    VOY_DIFF_WK,
    RLANE_CD   ,
    DIR_CD     ,
    TRD_CD     ,
    SUB_TRD_CD ,
    MAX_LDF_RTO,
    CRE_USR_ID ,
    CRE_DT     ,
    UPD_USR_ID ,
    UPD_DT
) VALUES (
    @[bse_yr]             ,
    @[voy_diff_wk]        ,
    @[rlane_cd]           ,
    @[dir_cd]             ,
    @[trd_cd]             ,
    @[sub_trd_cd]         ,
    NVL(@[max_ldf_rto], 0),
    @[cre_usr_id]         ,
    SYSDATE               ,
    @[upd_usr_id]         ,
