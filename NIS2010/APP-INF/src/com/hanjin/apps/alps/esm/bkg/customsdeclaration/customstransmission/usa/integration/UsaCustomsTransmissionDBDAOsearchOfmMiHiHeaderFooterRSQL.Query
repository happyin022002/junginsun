<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchOfmMiHiHeaderFooterRSQL">
			<desc><![CDATA[dwkim OFM 생성용 헤더 조회]]></desc>
			<sql><![CDATA[
SELECT
	'$$$MSGSTART & SMLM & SHACUST & CAMIR & '||TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC ('KRACY', sysdate, 'USNYC'),'ddmmrrhh24miss')||CHR(10)||
	'VSL_LLOYDCODE | '||@[vsl_lloyd]||CHR(10)||
	'VSL_FULLNAME | ' ||substr(@[vsl_eng_nm],1,23)||CHR(10)||
	'VSL_VOYAGE | '	  ||substr(@[vvd],5,4)||CHR(10)||
	'VSL_DIRECTION | '||substr(@[vvd],9,1)||CHR(10)||
	'VSL_FLAG | '	  ||@[vsl_flag]||CHR(10)||
	'BL_COUNT | '	  ||@[bl_cnt]||CHR(10)||
	'POL_LOC | '	  ||@[pol_name]||CHR(10)||
	'POL_ETD | '	  ||@[vps_etd_dt]||CHR(10)||
	'POD_LOC | '	  ||@[pod_name]||CHR(10)||
	'POD_ETA | '	  ||@[vps_eta_dt]||CHR(10) HEADER,
	'' FOOTER
FROM DUAL			]]></sql>
			<params>
				<param name="vsl_lloyd" type="12" value="" out="N"/>
				<param name="vsl_eng_nm" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="vsl_flag" type="12" value="" out="N"/>
				<param name="bl_cnt" type="12" value="" out="N"/>
				<param name="pol_name" type="12" value="" out="N"/>
				<param name="vps_etd_dt" type="12" value="" out="N"/>
				<param name="pod_name" type="12" value="" out="N"/>
				<param name="vps_eta_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
