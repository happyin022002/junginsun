<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchEmptyBookingRSQL">
			<desc><![CDATA[Empty Booking 정보 조회]]></desc>
			<sql><![CDATA[
select bk.bkg_no
	, bk.bl_no
	, bk.bl_no_tp
	, bk.bl_tp_cd
	, bk.vsl_cd
	, bk.skd_voy_no
	, bk.skd_dir_cd
	, bk.vsl_cd||bk.skd_voy_no||bk.skd_dir_cd bkg_trunk_vvd
	, bk.sls_rhq_cd
	, bk.mty_pkup_yd_cd org_yd_cd
	, rt.cgo_rcv_dt
	, to_char(cntr.cnmv_evnt_dt ,'yyyymmdd hh24:mi') cnmv_evnt_dt
	, to_char(bk.bkg_cre_dt, 'yyyy-mm-dd') bkg_cre_dt
	, bk.pol_cd bkg_pol_cd
	, bk.pod_cd bkg_pod_cd
	, bk.mty_bkg_sts_cd
    , bk.bkg_sts_cd
	, vvd.pol_cd
	, vvd.pod_cd
	, bk.split_flg
	, trim(bk.inter_rmk) inter_rmk
from bkg_booking bk
	, bkg_vvd vvd
	, bkg_rate rt
	, bkg_container cntr
where bk.bkg_no = rt.bkg_no(+)
and   bk.bkg_no = cntr.bkg_no(+)
and   bk.bkg_no = vvd.bkg_no
and   vvd.vsl_pre_pst_cd = 'T'
#if (${bkg_no} != '') 
AND    bk.bkg_no = @[bkg_no]
#else 
AND    bk.bl_no = @[bl_no]
#end
and   bk.bkg_cgo_tp_cd = 'P'
and   rownum = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
