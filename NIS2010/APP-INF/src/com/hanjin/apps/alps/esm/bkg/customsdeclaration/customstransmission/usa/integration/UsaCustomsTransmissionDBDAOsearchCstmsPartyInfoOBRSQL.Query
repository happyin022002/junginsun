<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCstmsPartyInfoOBRSQL">
			<desc><![CDATA[UsaCustomsTransmissionDBDAOsearchCstmsPartyInfoOBRSQL]]></desc>
			<sql><![CDATA[
SELECT 
 RPAD('N00'||RPAD(BKG_SPCLCHAR_CONV_FNC(  A.CSTMS_PTY_TP_CD , 'X' ),3,' ')  ||
             RPAD( NVL( RTRIM(BKG_SPCLCHAR_CONV_FNC( RTRIM(A.CSTMS_PTY_NM), 'X')  ),' ')           , 35,' ')||
             RPAD(BKG_SPCLCHAR_CONV_FNC(  DECODE( A.CSTMS_PTY_TP_CD, 'CB','17','SNP','2','BKP','1','ST','1') , 'X' ),2,' ')||
		     RPAD( NVL( RTRIM(BKG_SPCLCHAR_CONV_FNC( RTRIM(  A.CSTMS_PTY_ID ), 'X')  ),' ') ,17,' '),80,' ') BUF27
FROM BKG_CSTMS_ADV_ORZ_PTY A, BKG_BOOKING BK, BKG_VVD C, BKG_CUSTOMER D
WHERE C.BKG_NO = @[bl_no]
AND C.BKG_NO = BK.BKG_NO
AND C.BKG_NO = D.BKG_NO
AND D.BKG_CUST_TP_CD = DECODE(BK.CUST_TO_ORD_FLG, 'Y', 'N', 'C')
AND NVL(A.CUST_CNT_CD, 'XXX') IN ('XXX', D.CUST_CNT_CD)
AND NVL(A.CUST_SEQ, 999999) IN (999999, D.CUST_SEQ)
AND NVL(A.SC_NO, 'XXX') IN('XXX', BK.SC_NO)
AND NVL(A.POD_CD, 'XXX') IN ('XXX', BK.POD_CD)
AND NVL(A.DEL_CD, 'XXX') IN ('XXX', BK.DEL_CD)
AND NVL(A.POD_YD_NO,'XX') = DECODE( A.POD_YD_NO,null ,'XX',   SUBSTR(BK.POD_NOD_CD,6,2)  ) 
AND NVL(A.DEL_YD_NO,'XX') = DECODE( A.DEL_YD_NO,null ,'XX',   SUBSTR(BK.DEL_NOD_CD,6,2)  ) 
AND A.DELT_FLG = 'N'
AND C.VSL_CD     = SUBSTR(@[vvd],1,4) 
AND C.SKD_VOY_NO = SUBSTR(@[vvd],5,4) 
AND C.SKD_DIR_CD = SUBSTR(@[vvd],9,1)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
