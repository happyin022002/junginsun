<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchUsaCstmsDownloadRSQL">
			<desc><![CDATA[미 세관신고로 download 이후 VVD, POD, DEL을 변경한 경우]]></desc>
			<sql><![CDATA[
select count(1) CNT
  from bkg_cstms_adv_bl
 where bl_no = @[bl_no]
   and cnt_cd = 'US'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
