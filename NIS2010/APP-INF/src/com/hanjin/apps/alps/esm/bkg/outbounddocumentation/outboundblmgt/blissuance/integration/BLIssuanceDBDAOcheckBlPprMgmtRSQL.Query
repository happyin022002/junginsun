<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOcheckBlPprMgmtRSQL">
			<desc><![CDATA[BLIssuanceDBDAOcheckBlPprMgmtRSQL]]></desc>
			<sql><![CDATA[
SELECT 'Y'
  FROM BKG_CUST_BL_PPR_MGMT
 WHERE DTRB_YR = @[dtrb_yr]
   AND RHQ_CD = @[rhq_cd]
   and OFC_CD = @[ofc_cd]
   and CUST_CNT_CD = @[cust_cnt_cd]
   and CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="dtrb_yr" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
