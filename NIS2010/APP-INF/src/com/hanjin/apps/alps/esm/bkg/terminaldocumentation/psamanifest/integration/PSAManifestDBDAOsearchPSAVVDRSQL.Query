<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchPSAVVDRSQL">
			<desc><![CDATA[Target Port에 일정기간내 접안 예정 스케줄의 VVD를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VPS.VSL_CD VSL_CD
     , VPS.SKD_VOY_NO SKD_VOY_NO
     , VPS.SKD_DIR_CD SKD_DIR_CD
     , PSA.PVN PSA_VSL_NM
  FROM VSK_VSL_PORT_SKD VPS
     , (SELECT VSL_CD, MAX(PSA_VSL_NM) PVN
          FROM BKG_CSTMS_PSA_VVD
         GROUP BY VSL_CD) PSA
 WHERE VPS.VPS_PORT_CD = @[port_cd]   /* PORT */
   AND VPS.VPS_ETB_DT BETWEEN TO_DATE(@[etb_dt1], 'YYYY-MM-DD') AND TO_DATE(@[etb_dt2], 'YYYY-MM-DD')
   AND NVL(SKD_CNG_STS_CD, ' ') <> 'S'
   AND NVL(TURN_PORT_IND_CD, ' ') NOT IN ('D', 'V', 'F')
   AND VPS.VSL_CD = PSA.VSL_CD(+)
 GROUP BY VPS.VSL_CD, VPS.SKD_VOY_NO, VPS.SKD_DIR_CD, PSA.PVN			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="etb_dt1" type="12" value="" out="N"/>
				<param name="etb_dt2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
