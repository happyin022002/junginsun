<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchRdCgoRSQL">
			<desc><![CDATA[searchRdCgo]]></desc>
			<sql><![CDATA[
SELECT 'X' RD_EXIST FROM BKG_CONTAINER WHERE BKG_NO = @[bkg_no] AND CNTR_NO = @[cntr_no] AND RD_CGO_FLG <> 'N'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
