<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriRtCostVOUSQL">
			<desc><![CDATA[pri_sq_rt 테이블에 cost를 update한다.]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRI_RT
SET 
#if (${cost_tp} == 'C') 
	PRS_RESPB_CM_UC_AMT = @[respb_usd_ttl_amt]
	, PRS_RESPB_CMPB_AMT = PROP_FRT_RT_AMT + PRS_SCG_AMT - @[respb_usd_ttl_amt]
#else 
	PRS_RESPB_OPFIT_UC_AMT = @[respb_usd_ttl_amt]
	, PRS_RESPB_OPB_AMT = PROP_FRT_RT_AMT + PRS_SCG_AMT - @[respb_usd_ttl_amt]
#end
WHERE  TRI_PROP_NO        = @[tri_prop_no]
   AND  AMDT_SEQ       = @[amdt_seq]
			]]></sql>
			<params>
				<param name="respb_usd_ttl_amt" type="12" value="" out="N"/>
				<param name="tri_prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
