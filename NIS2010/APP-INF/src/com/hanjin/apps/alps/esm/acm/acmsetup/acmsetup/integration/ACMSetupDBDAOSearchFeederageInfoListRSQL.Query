<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSetupDBDAOSearchFeederageInfoListRSQL">
			<desc><![CDATA[SearchFeederageInfo
2016.08.05 김상현 [CHM-201642499] ALPS > ACM VIP Agreement Creation 상 SC/RFA No. 추가 요청]]></desc>
			<sql><![CDATA[
SELECT POL_CD,
  POD_CD,
  CNTR_TPSZ_CD,
  DDCT_AMT,
  FM_EFF_DT,
  TO_EFF_DT,
  DELT_FLG
FROM ACM_OTR_FDRG_DDCT
WHERE 1=1
#if (${pol_cd} != '')
  AND POL_CD LIKE '%' || @[pol_cd] || '%'
#end
#if (${pod_cd} != '')
  AND POD_CD LIKE '%' || @[pod_cd] || '%'
#end
#if (${delt_flg} != 'Y')
  AND DELT_FLG = 'N'
#end
ORDER BY POL_CD, POD_CD, CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
