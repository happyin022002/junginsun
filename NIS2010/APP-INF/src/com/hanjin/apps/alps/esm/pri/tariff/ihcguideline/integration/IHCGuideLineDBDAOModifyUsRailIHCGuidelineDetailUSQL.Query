<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOModifyUsRailIHCGuidelineDetailUSQL">
			<desc><![CDATA[Modify US Rail IHC Guideline Detail]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRF_IHC_RT
   SET GLINE_20FT_RAIL_FRT_RT_AMT = @[gline_20ft_rail_frt_rt_amt]
     , GLINE_40FT_RAIL_FRT_RT_AMT = @[gline_40ft_rail_frt_rt_amt]
	 , GLINE_20FT_FRT_RT_AMT = NVL(@[gline_20ft_rail_frt_rt_amt],0) + NVL(GLINE_20FT_TRK_FRT_RT_AMT,0)
     , GLINE_40FT_FRT_RT_AMT = NVL(@[gline_40ft_rail_frt_rt_amt],0) + NVL(GLINE_40FT_TRK_FRT_RT_AMT,0)
     , N1ST_CMNC_AMDT_SEQ = @[amdt_seq]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT     = SYSDATE
WHERE 1=1
  AND SVC_SCP_CD = @[svc_scp_cd]
  AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
  AND IHC_TRF_NO = @[ihc_trf_no]
  AND AMDT_SEQ = @[amdt_seq]
  AND IHC_CGO_TP_CD = @[ihc_cgo_tp_cd]
  AND HUB_LOC_CD = @[hub_loc_cd]
  AND BSE_PORT_LOC_CD = @[bse_port_loc_cd]
  AND PRC_TRSP_MOD_CD IN ('A','R')
  AND OPTM_TRSP_MOD_FLG = 'Y'
  AND SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="gline_20ft_rail_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_40ft_rail_frt_rt_amt" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="ihc_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="hub_loc_cd" type="12" value="" out="N"/>
				<param name="bse_port_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
