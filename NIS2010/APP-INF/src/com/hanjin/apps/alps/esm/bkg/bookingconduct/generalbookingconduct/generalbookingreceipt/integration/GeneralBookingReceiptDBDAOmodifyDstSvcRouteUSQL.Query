<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOmodifyDstSvcRouteUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
update bkg_Booking
set DEST_TRNS_SVC_MOD_CD = @[dstSvcRouteCd]
#if(${cod_rqst_seq}!='')
, de_term_cd = (select new_de_term_cd 
				  from bkg_cod 
				 where bkg_no = @[cod_bkg_no] 
				   and cod_rqst_seq = @[cod_rqst_seq])
#end
where bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="dstSvcRouteCd" type="12" value="abc" out="N"/>
				<param name="cod_bkg_no" type="12" value="SHAYSB25689" out="N"/>
				<param name="cod_rqst_seq" type="12" value="1" out="N"/>
				<param name="bkg_no" type="12" value="SHAYSB25689" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
