<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ModelManageDBDAOCheckHOUserRSQL">
			<desc><![CDATA[입력하는 사람이 본사 소속인지 확인합니다.]]></desc>
			<sql><![CDATA[
SELECT 'Y'
  FROM COM_USER         U,
       MDM_ORGANIZATION M
 WHERE U.USR_ID   = @[usr_id]
   AND U.OFC_CD   = M.OFC_CD
   AND M.OFC_KND_CD = 1
   AND U.OFC_CD <> 'SELCTY' --TEMP 지역본부로 인지			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
