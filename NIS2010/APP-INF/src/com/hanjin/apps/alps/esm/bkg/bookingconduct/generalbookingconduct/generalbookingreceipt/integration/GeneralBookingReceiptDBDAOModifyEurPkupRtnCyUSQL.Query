<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModifyEurPkupRtnCyUSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOModifyEurPkupRtnCyUSQL.Qurey]]></desc>
			<sql><![CDATA[
MERGE INTO BKG_BOOKING BK
 USING (SELECT TRO.BKG_NO, 
               TRO.CNTR_PKUP_YD_CD, 
               TRO.CNTR_RTN_YD_CD
          FROM BKG_EUR_TRO TRO
             , BKG_EUR_TRO_DTL DTL
         WHERE TRO.BKG_NO      = DTL.BKG_NO
           AND TRO.IO_BND_CD   = DTL.IO_BND_CD   
           AND TRO.TRO_SEQ     = DTL.TRO_SEQ
           AND TRO.BKG_NO      = @[bkg_no]
           AND TRO.CXL_FLG     = 'N'
           AND TRO.CFM_FLG     = 'N'
		   AND TRO.IO_BND_CD   = 'O'
           AND ROWNUM = 1) TRO
           ON (BK.BKG_NO = TRO.BKG_NO)
  WHEN MATCHED THEN
UPDATE 
   SET BK.MTY_PKUP_YD_CD = TRO.CNTR_PKUP_YD_CD
     , BK.FULL_RTN_YD_CD = TRO.CNTR_RTN_YD_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
