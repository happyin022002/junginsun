<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOSearchBlCntrSealNoRSQL">
			<desc><![CDATA[ESM_BKG_1107에서 CNTR하위 SEAL정보를 가져오는 SQL]]></desc>
			<sql><![CDATA[
SELECT 
  VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD,
  BL_NO,
  CSTMS_PORT_CD,
  CNTR_NO,
  SEAL_NO_SEQ,
  SEAL_NO,
  SEAL_PTY_TP_CD,
  SEAL_PTY_NM,
  SEAL_KND_CD
FROM BKG_CSTMS_EUR_SEAL_NO
WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
  AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
  AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
  AND BL_NO = @[bl_no]
  AND CSTMS_PORT_CD = @[cstms_port_cd]
#if (${cntr_no} != '')
  AND CNTR_NO = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
