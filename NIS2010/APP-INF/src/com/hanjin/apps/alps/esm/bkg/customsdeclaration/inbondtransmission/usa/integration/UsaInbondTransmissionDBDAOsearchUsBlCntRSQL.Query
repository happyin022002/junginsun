<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInbondTransmissionDBDAOsearchUsBlCntRSQL">
			<desc><![CDATA[dwkim Us bl 건수를 확인.]]></desc>
			<sql><![CDATA[
SELECT    COUNT(A.BL_NO)
                FROM    BKG_CSTMS_ADV_BL A, BKG_CSTMS_ADV_IBD IBD
                WHERE  A.BL_NO  = IBD.BL_NO
                AND    A.CNT_CD = IBD.CNT_CD
                AND    A.POD_CD LIKE 'US%'
                AND    IBD.CSTMS_CLR_TP_CD    IN ('I','L')
                AND    A.BL_NO = @[bl_no]
                AND    A.CNT_CD = 'US'			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
