<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqAwkwardDBDAOsearchPriScqSvcScpRSQL">
			<desc><![CDATA[MDM_LOCATION
MDM_SVC_SCP_LMT]]></desc>
			<sql><![CDATA[
SELECT DISTINCT B.SVC_SCP_CD AS SVC_SCP_CD
 FROM  MDM_LOCATION A, MDM_SVC_SCP_LMT B,
       MDM_LOCATION C, MDM_SVC_SCP_LMT D
 WHERE A.LOC_CD = @[pol_cd]
   AND A.RGN_CD = B.RGN_CD
   AND C.LOC_CD = @[pod_cd]
   AND C.RGN_CD = D.RGN_CD
   AND B.ORG_DEST_CD = 'O'
   AND D.ORG_DEST_CD = 'D'
   AND B.SVC_SCP_CD = D.SVC_SCP_CD
 ORDER BY SVC_SCP_CD			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
