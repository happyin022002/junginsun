<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOMultiAvgHireOwnVslDtrbUSQL">
			<desc><![CDATA[MultiAvgHireOwnVslDtrb]]></desc>
			<sql><![CDATA[
UPDATE MAS_OWN_VSL_DLY_HIR_DTRB 
   SET DHIR_AMT   = @[dhir_amt]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT     = SYSDATE
 WHERE COST_YRMON = SUBSTR(@[cost_yr],1,4)||SUBSTR(@[cost_yr],6,2)
   AND COST_WK    = '00'
   AND VSL_CD     = @[vsl_cd]
   AND STND_COST_CD = @[stnd_cost_cd]			]]></sql>
			<params>
				<param name="dhir_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yr" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
