<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOremoveSpecialListDSQL">
			<desc><![CDATA[special UN No 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CSTMS_EUR_DG_SPCL
WHERE	IMDG_UN_NO = @[old_imdg_un_no]
AND     ANR_SPCL_TP_ID = @[anr_spcl_tp_id]			]]></sql>
			<params>
				<param name="old_imdg_un_no" type="12" value="" out="N"/>
				<param name="anr_spcl_tp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
