<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchVslCdByLloydNoRSQL">
			<desc><![CDATA[VSL_CD를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
  FROM (
        SELECT DECODE(CNT.CNT, 1, VSL.VSL_CD, SKD.VSL_CD) VSL_CD
              ,DECODE(CNT.CNT, 1, SYSDATE, SKD.CRE_DT) CRE_DT
          FROM MDM_VSL_CNTR VSL
              ,VSK_VSL_SKD SKD
              ,(
               SELECT COUNT(*) CNT
                 FROM MDM_VSL_CNTR
                WHERE LLOYD_NO = @[lloyd_no]
                  AND DELT_FLG = 'N'
               ) CNT
         WHERE VSL.LLOYD_NO  = @[lloyd_no]
           AND VSL.DELT_FLG = 'N'
           AND VSL.VSL_CD = SKD.VSL_CD(+)
           AND SKD.SKD_VOY_NO(+) = @[skd_voy_no]
           AND SKD.SKD_DIR_CD(+) = @[skd_dir_cd]
        ORDER BY SKD.CRE_DT DESC
       )
 WHERE CRE_DT IS NOT NULL
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="lloyd_no" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
