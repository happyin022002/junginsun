<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EServiceCompensationDBDAOPriSpHdrVOSCNoRSQL">
			<desc><![CDATA[sc no 조회]]></desc>
			<sql><![CDATA[
SELECT 
    (SELECT
        	TRIM(SC_NO)
     FROM   PRI_SP_HDR
     WHERE	SC_NO = @[sc_no]
    ) AS SC_NO
FROM DUAL			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
