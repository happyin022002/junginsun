<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchDgInfoAtLocalRSQL">
			<desc><![CDATA[구주위험물 테이블들에서 데이타를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT 
         SUM(SUB1.SEQ) OVER (ORDER BY SUB1.BL_NO, SUB1.POL_CD, SUB1.POD_CD, SUB1.CNTR_NO, SUB1.CELL_PSN_NO) SEQ 
		,SUB1.CNTR_CNT			-- TOTAL CONTAINER COUNT
        ,SUB1.BL_NO MERGE_BL_NO
        ,SUB1.BL_NO              -- BL_NO
		,SUB1.BKG_NO
        ,SUB1.POL_CD             -- POL_CD
        ,SUB1.POD_CD             -- POD_CD
        ,SUB1.CNTR_NO            -- Container No
        ,SUB1.CNTR_CGO_SEQ       -- Container Cargo Seq
		,DECODE(NVL(SUB1.CELL_PSN_NO, ''), '', '', LPAD(SUB1.CELL_PSN_NO, 7,0 )) CELL_PSN_NO	-- Cell Position
        ,SUB1.IMDG_CLSS_CD       -- Class
        ,SUB1.IMDG_UN_NO         -- UN No.
        ,SUB1.IMDG_UN_NO_SEQ     -- UN No SEQ.
		,SUB1.IMDG_COMP_GRP_CD
        ,SUB1.ANR_SPCL_TP_ID AS DG_SHORT_NM     -- S.D/G
        ,SUB1.FLSH_PNT_CDO_TEMP  				-- Flash Point
        ,SUB1.ANR_ROLE_DIV_CD AS AGENT  		-- Agent
        ,SUB1.ANR_CRR_TP_CD AS C_TYPE     		-- Carriage Type
        ,SUB1.FDR_SVC_RQST_NO    -- SSR(Feeder)
        ,SUB1.IMDG_PCK_GRP_CD    -- Package Group
        ,SUB1.OUT_IMDG_PCK_QTY1  -- Outer Package QTY
        ,SUB1.OUT_IMDG_PCK_CD1   -- Outer Package Code
        ,SUB1.EMS_NO             -- EMS
        ,SUB1.NET_WGT            -- Net Weight
        ,SUB1.GRS_WGT            -- Gross Weight
        ,SUB1.EUR_PCK_DESC AS PACKAGES 					-- Packages
        ,SUB1.PRP_SHP_NM         						-- Substance
        ,SUB1.HZD_DESC           						-- Hazardous Contents
        ,SUB1.ANR_FWRD_ID AS FWRD_ID 					-- Forwarder Code
        ,SUB1.FWRD_NM          							-- Forwarder Name
	    ,SUB1.EUR_DCGO_MRN_POLUT_CD	DCGO_MRN_POLUT_CD	-- Marine Pollutant
        ,SUB1.IMDG_LMT_QTY_FLG							-- Limited quantity

       	,SUB1.FDR_VSL_NM           -- Feeder Name
       	,SUB1.FDR_VSL_LLOYD_NO     -- Feeder Lloyd No
       	,SUB1.FDR_VVD_ID           -- Feeder VVD
		,TO_CHAR(SUB1.CRR_DT, 'YYYYMMDD')	CRR_DT	   -- on carriage date
		,SUB1.IMDG_SUBS_RSK_LBL_CD1-- Sub Label 1
		,SUB1.IMDG_SUBS_RSK_LBL_CD2-- Sub Label 2
		,SUB1.IMDG_SUBS_RSK_LBL_CD3-- Sub Label 3
		,SUB1.IMDG_SUBS_RSK_LBL_CD4-- Sub Label 4

   		,NVL(SUB1.MSG_FUNC_ID, '') SEND_TYPE
        ,NVL(SUB1.SCR_FILE_NO, '') SCR_FILE_NO
   		,SUB1.MSG_SND_NO MSG_SND_NO
   		,SUB1.FIRST_MSG_SND_NO
 		,COUNT(DISTINCT SUB1.BL_NO) OVER(PARTITION BY SUB1.FIRST_MSG_SND_NO ) AS GROUP_CNT
        ,SUB1.NET_EXPLO_WGT
FROM 
    (
		SELECT 
        	DECODE(LAG(A.BL_NO) OVER ( ORDER BY A.BL_NO, A.POL_CD, A.POD_CD, A.CNTR_NO, 
                                    DECODE(NVL(BAY.CELL_PSN_NO, ''), '', A.CELL_PSN_NO, BAY.CELL_PSN_NO) ) , A.BL_NO, 0, 1) SEQ  
			,COUNT(DISTINCT A.CNTR_NO) OVER() CNTR_CNT
        	,A.BL_NO              -- BL_NO
			,C.BKG_NO
		    ,A.POL_CD             -- POL_CD
		    ,A.POD_CD             -- POD_CD
		    ,A.CNTR_NO            -- Container No
			,A.CNTR_CGO_SEQ		  -- Container Cargo Seq
		    ,DECODE(NVL(BAY.CELL_PSN_NO, ''), '', A.CELL_PSN_NO, BAY.CELL_PSN_NO) AS CELL_PSN_NO -- Cell Position
		    ,A.IMDG_CLSS_CD       -- Class
		    ,A.IMDG_UN_NO         -- UN No.
		    ,A.IMDG_UN_NO_SEQ     -- UN No SEQ.
			,SIUN.IMDG_COMP_GRP_CD
		    ,A.ANR_SPCL_TP_ID     -- S.D/G
		    ,A.FLSH_PNT_CDO_TEMP  -- Flash Point
		    ,A.ANR_ROLE_DIV_CD    -- Agent
		    ,A.ANR_CRR_TP_CD      -- Carriage Type
		    ,A.FDR_SVC_RQST_NO    -- SSR(Feeder)
		    ,A.IMDG_PCK_GRP_CD    -- Package Group
		    ,A.OUT_IMDG_PCK_QTY1  -- Outer Package QTY
		    ,A.OUT_IMDG_PCK_CD1   -- Outer Package Code
		    ,A.EMS_NO             -- EMS
		    ,A.NET_WGT            -- Net Weight
		    ,A.GRS_WGT            -- Gross Weight
		    ,A.EUR_PCK_DESC       -- Packages
		    ,A.PRP_SHP_NM         -- Substance
		    ,A.HZD_DESC           -- Hazardous Contents
		    ,A.ANR_FWRD_ID
		    ,B.FWRD_NM            -- Forwarder Name
	        ,A.EUR_DCGO_MRN_POLUT_CD
        	,A.IMDG_LMT_QTY_FLG

        	,A.FDR_VSL_NM           -- Feeder Name
        	,A.FDR_VSL_LLOYD_NO     -- Feeder Lloyd No
        	,A.FDR_VVD_ID           -- Feeder VVD
			,A.CRR_DT				-- on carriage date
			,A.IMDG_SUBS_RSK_LBL_CD1-- Sub Label 1
            ,A.IMDG_SUBS_RSK_LBL_CD2-- Sub Label 2
            ,A.IMDG_SUBS_RSK_LBL_CD3-- Sub Label 3
            ,A.IMDG_SUBS_RSK_LBL_CD4-- Sub Label 4
        	
            ,DECODE(LOG.MSG_FUNC_ID, 'O', DECODE(LOG.ACK_RCV_STS_CD, 'R', '', LOG.MSG_FUNC_ID), LOG.MSG_FUNC_ID) MSG_FUNC_ID

            ,LOG.MSG_SND_NO
  			,F_LOG.SCR_FILE_NO
            ,F_LOG.MSG_SND_NO   FIRST_MSG_SND_NO

            ,A.NET_EXPLO_WGT
        	
		FROM BKG_CSTMS_EUR_DG A
		     , BKG_CSTMS_EUR_DG_FWRD B
			 ,SCG_IMDG_UN_NO SIUN
			 ,BKG_BOOKING C
		     , (
		            SELECT 
		                A.BAY_PLN_ID
		                ,A.VSL_CD
		                ,A.ETA_DT
		                ,A.ETD_DT
		                ,B.EUR_DG_CNTR_ID CNTR_NO
		                ,B.CELL_PSN_NO
		                ,B.POL_CD
		                ,B.POD_CD
		            FROM BKG_CSTMS_EUR_DG_BAY_PLN A
		                 ,BKG_CSTMS_EUR_DG_BAY_DTL B
		            WHERE A.BAY_PLN_ID = B.BAY_PLN_ID 
		            AND   A.VSL_CD            = SUBSTR(@[vvd_cd], 1, 4)
		            AND   A.VSL_VOY_DIR_NO    = SUBSTR(@[vvd_cd], 5, 5)
					AND   A.BAY_PLN_ID > ' '
	            	AND   B.EUR_DG_CNTR_ID > ' '
		            AND   B.BAY_PLN_ID  = @[bay_pln_id]
		        ) BAY
		     
#if (${ui_type} == 'ESM_BKG_0965') 
            , (
                SELECT
                     A.EUR_DG_DECL_TP_CD
                    ,A.VSL_CD
                    ,A.SKD_VOY_NO
                    ,A.SKD_DIR_CD
                    ,A.PORT_CD
                    ,B.BL_NO
                    ,B.CNTR_NO
                    ,B.CNTR_CGO_SEQ
                    ,A.MSG_FUNC_ID
                    ,B.MSG_SND_NO
                    ,B.EDI_RSPN_SEQ
                    ,C.ACK_RCV_STS_CD
                FROM BKG_CSTMS_EUR_DG_SND A
	                ,BKG_CSTMS_EUR_DG_EDI_RSPN B
					,BKG_CSTMS_EUR_DG_RCV C
                WHERE A.EUR_DG_DECL_TP_CD = @[d_type]                                                                            
                AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                                                    
                AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                                                    
                AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                                                    
                AND   A.PORT_CD     = @[port_cd]                                                                                 
                AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                AND   A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                AND   A.MSG_SND_NO = B.MSG_SND_NO
				AND   A.EUR_EDI_MSG_TP_ID= C.EUR_EDI_MSG_TP_ID(+)
                AND   A.MSG_SND_NO = C.MSG_RCV_NO(+)
                AND   (B.BL_NO, B.MSG_SND_NO) IN (
                                            SELECT
                                                B.BL_NO, MAX(B.MSG_SND_NO)
                                            FROM BKG_CSTMS_EUR_DG_SND A
                                            ,BKG_CSTMS_EUR_DG_EDI_RSPN B
                                        	WHERE A.EUR_DG_DECL_TP_CD = @[d_type]                                                    
                                        	AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                            
                                        	AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                            
                                        	AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                            
                                       	 	AND   A.PORT_CD     = @[port_cd]                                                         
                                            AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                                            AND   A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                                            AND   A.MSG_SND_NO = B.MSG_SND_NO
											--AND '0' = INSTR(A.MSG_SND_NO, 'AC')
                							GROUP BY A.EUR_EDI_MSG_TP_ID, B.BL_NO
                                            )
                
                
            ) LOG

            , (
                SELECT
                     A.EUR_DG_DECL_TP_CD
                    ,A.VSL_CD
                    ,A.SKD_VOY_NO
                    ,A.SKD_DIR_CD
                    ,A.PORT_CD
                    ,B.BL_NO
                    ,B.CNTR_NO
                    ,B.CNTR_CGO_SEQ
                    ,A.MSG_FUNC_ID
                    ,B.MSG_SND_NO
                    ,B.EDI_RSPN_SEQ
                    ,(SELECT /*+ INDEX_DESC(BKG_CSTMS_EUR_DG_RCV XPKBKG_CSTMS_EUR_DG_RCV)*/ SCR_FILE_NO
  						FROM BKG_CSTMS_EUR_DG_RCV
  						WHERE EUR_EDI_MSG_TP_ID = 'IFD'
  						AND MSG_RCV_NO = A.MSG_SND_NO
 						AND ROWNUM = 1
					) SCR_FILE_NO
                FROM BKG_CSTMS_EUR_DG_SND A
                	,BKG_CSTMS_EUR_DG_EDI_RSPN B
                WHERE A.EUR_DG_DECL_TP_CD = @[d_type]                                                                            
                AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                                                    
                AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                                                    
                AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                                                    
                AND   A.PORT_CD     = @[port_cd]                                                                                 
                AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                AND   A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                AND   A.MSG_SND_NO = B.MSG_SND_NO
                AND   A.MSG_FUNC_ID = 'O'
                AND   (B.BL_NO, B.MSG_SND_NO) IN (
                                            SELECT
                                                 B.BL_NO, MAX(B.MSG_SND_NO)
                                            FROM BKG_CSTMS_EUR_DG_SND A
                                            ,BKG_CSTMS_EUR_DG_EDI_RSPN B
                                        	WHERE A.EUR_DG_DECL_TP_CD = @[d_type]                                                    
                                        	AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                            
                                        	AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                            
                                        	AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                            
                                       	 	AND   A.PORT_CD     = @[port_cd]                                                         
                                            AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                                            AND   A.EUR_EDI_MSG_TP_ID = B.EUR_EDI_MSG_TP_ID
                                            AND   A.MSG_SND_NO = B.MSG_SND_NO
                                            AND   A.MSG_FUNC_ID = 'O'
											GROUP BY A.EUR_EDI_MSG_TP_ID, B.BL_NO
                                            )
                
                
            ) F_LOG
#else
			-- 0966
            , (
                SELECT
                     A.EUR_DG_DECL_TP_CD
					,A.VSL_CD
                    ,A.SKD_VOY_NO
                    ,A.SKD_DIR_CD
                    ,A.PORT_CD
                    ,A.MSG_FUNC_ID
                    ,A.MSG_SND_NO
                    ,B.ACK_RCV_STS_CD 

				FROM BKG_CSTMS_EUR_DG_SND A, BKG_CSTMS_EUR_DG_RCV B
                WHERE A.EUR_EDI_MSG_TP_ID= B.EUR_EDI_MSG_TP_ID(+)
                AND   A.MSG_SND_NO = B.MSG_RCV_NO(+)

                AND   A.EUR_DG_DECL_TP_CD = @[d_type]                                                                            
                AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                                                    
                AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                                                    
                AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                                                    
                AND   A.PORT_CD     = @[port_cd]                                                                                 
                AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                AND   A.SND_DT = (
                                            SELECT
                                                MAX(A.SND_DT)
                                            FROM BKG_CSTMS_EUR_DG_SND A
                                            ,BKG_CSTMS_EUR_DG_EDI_RSPN B
                                        	WHERE A.EUR_DG_DECL_TP_CD = @[d_type]                                                    
                                        	AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                            
                                        	AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                            
                                        	AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                            
                                       	 	AND   A.PORT_CD     = @[port_cd]                                                         
                                            AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                							GROUP BY A.EUR_EDI_MSG_TP_ID
                                            )
                
                
            ) LOG

            , (
                SELECT
                     A.EUR_DG_DECL_TP_CD
					,A.VSL_CD
                    ,A.SKD_VOY_NO
                    ,A.SKD_DIR_CD
                    ,A.PORT_CD
                    ,A.MSG_FUNC_ID
                    ,A.MSG_SND_NO
                    ,(SELECT /*+ INDEX_DESC(BKG_CSTMS_EUR_DG_RCV XPKBKG_CSTMS_EUR_DG_RCV)*/ SCR_FILE_NO
  						FROM BKG_CSTMS_EUR_DG_RCV
  						WHERE EUR_EDI_MSG_TP_ID = 'IFD'
  						AND MSG_RCV_NO = A.MSG_SND_NO
 						AND ROWNUM = 1
					) SCR_FILE_NO
                FROM BKG_CSTMS_EUR_DG_SND A
                WHERE A.EUR_DG_DECL_TP_CD = @[d_type]                                                                            
                AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                                                    
                AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                                                    
                AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                                                    
                AND   A.PORT_CD     = @[port_cd]                                                                                 
                AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                AND   A.MSG_FUNC_ID = 'O'
                AND   A.SND_DT = (
                                            SELECT
                                                 MAX(A.SND_DT)
                                            FROM BKG_CSTMS_EUR_DG_SND A
                                        	WHERE A.EUR_DG_DECL_TP_CD = @[d_type]                                                    
                                        	AND   A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)                                            
                                        	AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)                                            
                                        	AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)                                            
                                       	 	AND   A.PORT_CD     = @[port_cd]                                                         
                                            AND   A.EUR_EDI_MSG_TP_ID ='IFD'
                                            AND   A.MSG_FUNC_ID = 'O'
											GROUP BY A.EUR_EDI_MSG_TP_ID
                                      )
                
                
            ) F_LOG
#end            
		WHERE A.VSL_CD      = SUBSTR(@[vvd_cd], 1, 4)
		AND   A.SKD_VOY_NO  = SUBSTR(@[vvd_cd], 5, 4)
		AND   A.SKD_DIR_CD  = SUBSTR(@[vvd_cd], 9, 1)
		AND   A.PORT_CD     = @[port_cd]
		
		#if (${d_type} != '') 
		AND   A.EUR_DG_DECL_TP_CD = @[d_type]
		#end

		AND   A.BL_NO 		= C.BL_NO

		AND   A.ANR_FWRD_ID = B.ANR_FWRD_ID(+)
		AND   A.VSL_CD      = BAY.VSL_CD(+)
		AND   A.POL_CD      = BAY.POL_CD(+)
		AND   A.POD_CD      = BAY.POD_CD(+)
		AND   A.CNTR_NO     = BAY.CNTR_NO(+)

		AND   A.EUR_DG_DECL_TP_CD = LOG.EUR_DG_DECL_TP_CD(+)
        AND   A.VSL_CD      = LOG.VSL_CD(+)
        AND   A.SKD_VOY_NO  = LOG.SKD_VOY_NO(+)
        AND   A.SKD_DIR_CD  = LOG.SKD_DIR_CD(+)
        AND   A.PORT_CD     = LOG.PORT_CD(+)

		AND   A.EUR_DG_DECL_TP_CD = F_LOG.EUR_DG_DECL_TP_CD(+)
        AND   A.VSL_CD      = F_LOG.VSL_CD(+)
        AND   A.SKD_VOY_NO  = F_LOG.SKD_VOY_NO(+)
        AND   A.SKD_DIR_CD  = F_LOG.SKD_DIR_CD(+)
        AND   A.PORT_CD     = F_LOG.PORT_CD(+)

		AND   A.IMDG_UN_NO	= 	SIUN.IMDG_UN_NO(+)
		AND   A.IMDG_UN_NO_SEQ = SIUN.IMDG_UN_NO_SEQ(+)
		

#if (${ui_type} == 'ESM_BKG_0965') 
        AND   A.BL_NO       = LOG.BL_NO(+)
        AND   A.CNTR_NO     = LOG.CNTR_NO(+)
        AND   A.CNTR_CGO_SEQ = LOG.CNTR_CGO_SEQ(+)
        AND   A.BL_NO       = F_LOG.BL_NO(+)
        AND   A.CNTR_NO     = F_LOG.CNTR_NO(+)
        AND   A.CNTR_CGO_SEQ = F_LOG.CNTR_CGO_SEQ(+)
#end

    
		) SUB1
ORDER BY SEQ, BL_NO, POL_CD, POD_CD, CNTR_NO, CNTR_CGO_SEQ			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="bay_pln_id" type="12" value="" out="N"/>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
