<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAYearlyPlanDBDAODeleteBsaBudJntOpBzcDSQL">
			<desc><![CDATA[BSA_BUD_JNT_OP_BZC 테이블의 데이터를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE
  FROM BSA_BUD_JNT_OP_BZC
 WHERE 1=1

#if (${trd_cd} != 'All')
   AND TRD_CD   = @[trd_cd]
#end

#if (${rlane_cd} != 'All')
   AND RLANE_CD = @[rlane_cd]
#end			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
