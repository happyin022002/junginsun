<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOSearchDraftBlImageCustCodeYNRSQL">
			<desc><![CDATA[UserSetupMgtDBDAOSearchDraftBlImageCustCodeYNRSQL]]></desc>
			<sql><![CDATA[
SELECT 
    DECODE(COUNT(CUST_SEQ),0,'N','Y') AS CUST_CODE_YN 
FROM MDM_CUSTOMER 
WHERE (CUST_CNT_CD || LPAD(TO_CHAR(CUST_SEQ),6,'0')) = @[cust_code]
			]]></sql>
			<params>
				<param name="cust_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
