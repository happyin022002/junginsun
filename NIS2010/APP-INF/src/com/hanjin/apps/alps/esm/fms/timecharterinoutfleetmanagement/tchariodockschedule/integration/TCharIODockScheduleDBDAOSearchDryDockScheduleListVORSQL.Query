<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharIODockScheduleDBDAOSearchDryDockScheduleListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
select a.vsl_cd,
		d.slan_cd,
		b.vsl_dznd_capa,
		to_char(a.dck_fm_dt,'yyyymmdd') last_dck_fm_dt,
		to_char(a.dck_to_dt,'yyyymmdd') last_dck_to_dt,
		a.dck_loc_cd last_dck_loc_cd,
        (select shp_yd_nm from fms_shp_yd fs where fs.yd_seq = a.yd_seq) last_ship_yard,
		a.flet_dck_svey_tp_cd last_flet_dck_svey_tp_cd,
		to_char(na.next_dck_fm_dt,'yyyymmdd') next_dck_fm_dt,
		to_char(na.next_dck_to_dt,'yyyymmdd') next_dck_to_dt,
		na.next_dck_loc_cd,
        (select shp_yd_nm from fms_shp_yd fs where fs.yd_seq = na.yd_seq) next_ship_yard,
		na.next_flet_dck_svey_tp_cd,
		to_char(nc.dck_fm_dt,'yyyymmdd') rec_dck_fm_dt,
		to_char(nc.dck_to_dt,'yyyymmdd') rec_dck_to_dt
		from fms_dck_skd a, fms_contract b,
			(select vsl_cd, flet_ctrt_no
			from (
					select vsl_cd, flet_ctrt_no, row_number() over(partition by vsl_cd order by flet_ctrt_no desc) row_num
					from fms_contract
					where flet_ctrt_tp_cd <> 'TO'
				)
			where row_num = 1
			union all
			select vsl_cd, flet_ctrt_no
			from (
					select vsl_cd, flet_ctrt_no, row_number() over(partition by vsl_cd order by flet_ctrt_no desc) row_num
					from fms_id_vsl a
					where not exists (select null from fms_contract where flet_ctrt_tp_cd <> 'TO' and vsl_cd = a.vsl_cd)
				)
			where row_num = 1
			) c,
			(select vsl_cd, slan_cd
			 from (
					select vsl_cd, slan_cd, vps_eta_dt, row_number() over(partition by vsl_cd order by vps_eta_dt desc) lane_num
					from vsk_vsl_port_skd
					where vps_eta_dt between to_date(@[fr_duration]||'0101','yyyymmdd') and to_date(@[to_duration]||'1231','yyyymmdd')
				)
			 where lane_num = 1) d,
			(select a.vsl_cd, a.dck_fm_dt, 
					b.dck_fm_dt next_dck_fm_dt, b.dck_to_dt next_dck_to_dt, b.dck_loc_cd next_dck_loc_cd, b.yd_seq, b.flet_dck_svey_tp_cd next_flet_dck_svey_tp_cd
			 from   (
					select vsl_cd, dck_fm_dt, row_number() over(partition by vsl_cd order by dck_fm_dt asc) row_num
					From fms_dck_skd
					where dck_sel_cd = 'E') a,
				   (
					select vsl_cd, dck_fm_dt, dck_to_dt, dck_loc_cd, flet_dck_svey_tp_cd, yd_seq,
							row_number() over(partition by vsl_cd order by dck_fm_dt asc) row_num
					From fms_dck_skd
					where dck_sel_cd = 'E') b
			 where a.vsl_cd = b.vsl_cd
			 and   a.row_num = b.row_num-1) na,
			(select vsl_cd, dck_fm_dt, dck_to_dt
			 from   fms_dck_skd
			 where dck_sel_cd = 'C') nc

		#if (${ownr_seq} != "")
			 ,(select vndr_seq from mdm_vendor
			 	where flet_mgmt_ownr_vndr_seq = @[ownr_seq]) e
		#end

		where a.vsl_cd = c.vsl_cd(+)
		and   a.vsl_cd = d.vsl_cd(+)
		and   b.flet_ctrt_no(+) = c.flet_ctrt_no
		and   a.vsl_cd = na.vsl_cd(+)
		and   a.dck_fm_dt = na.dck_fm_dt(+)
		and   a.vsl_cd = nc.vsl_cd(+)
		#if (${flet_ctrt_tp_cd} != "")
		and   b.flet_ctrt_tp_cd like @[flet_ctrt_tp_cd]||'%'
		#end
		#if (${vsl_dznd_capa_fr} != "")
		and   b.vsl_dznd_capa >= @[vsl_dznd_capa_fr]
		#end
		#if (${vsl_dznd_capa_to} != "")
		and   b.vsl_dznd_capa <= @[vsl_dznd_capa_to]
		#end
		
		#if (${lane_cd} != "")
		and   d.slan_cd like @[lane_cd]||'%'
		#end

		#if (${ownr_seq} != "")
		and   b.vndr_seq = e.vndr_seq
		#end

		#if (${ydSeq} != "")
		and   (a.yd_seq = @[ydSeq] or na.yd_seq = @[ydSeq])
		#end

		and	  a.vsl_cd like @[vsl_cd]||'%'
		and   a.dck_sel_cd = 'E'
		and   a.flet_dck_svey_tp_cd like @[flet_dck_svey_tp_cd]||'%'
		
		#if (${reflection_cd} == "I")
		
		and a.phs_out_dt <= @[to_duration]||'1231' and a.phs_in_dt >= @[fr_duration]
		
		#else
		
		and a.dck_fm_dt <= to_date(@[to_duration]||'1231','yyyymmdd') and a.dck_to_dt >= to_date(@[fr_duration]||'0101','yyyymmdd')
		
		#end
		
union all
select a.vsl_cd,
		d.slan_cd,
		b.vsl_dznd_capa,
		null last_dck_fm_dt,
		null last_dck_to_dt,
		null last_dck_loc_cd,
        null last_ship_yard,
		null last_flet_dck_svey_tp_cd,
		null next_dck_fm_dt,
		null next_dck_to_dt,
		null next_dck_loc_cd,
        null next_ship_yard,
		null next_flet_dck_svey_tp_cd,
		to_char(a.dck_fm_dt,'yyyymmdd') rec_dck_fm_dt,
		to_char(a.dck_to_dt,'yyyymmdd') rec_dck_to_dt
		from fms_dck_skd a, fms_contract b,
			(select vsl_cd, flet_ctrt_no
			from (
					select vsl_cd, flet_ctrt_no, row_number() over(partition by vsl_cd order by flet_ctrt_no desc) row_num
					from fms_contract
					where flet_ctrt_tp_cd <> 'TO'
				)
			where row_num = 1
		union all
		select vsl_cd, flet_ctrt_no
			from (
					select vsl_cd, flet_ctrt_no, row_number() over(partition by vsl_cd order by flet_ctrt_no desc) row_num
					from fms_id_vsl a
					where not exists (select null from fms_contract where flet_ctrt_tp_cd <> 'TO' and vsl_cd = a.vsl_cd)
				)
			where row_num = 1
			) c,
			(select vsl_cd, slan_cd
			 from (
					select vsl_cd, slan_cd, vps_eta_dt, row_number() over(partition by vsl_cd order by vps_eta_dt desc) lane_num
					from vsk_vsl_port_skd
					where vps_eta_dt between to_date(@[fr_duration]||'0101','yyyymmdd') and to_date(@[to_duration]||'1231','yyyymmdd')
				)
			 where lane_num = 1) d

		#if (${ownr_seq} != "")
			 ,(select vndr_seq from mdm_vendor
			 	where flet_mgmt_ownr_vndr_seq = @[ownr_seq]) e
		#end

		where a.vsl_cd = c.vsl_cd(+)
		and   a.vsl_cd = d.vsl_cd(+)
		and   b.flet_ctrt_no(+) = c.flet_ctrt_no
		#if (${flet_ctrt_tp_cd} != "")
		and   b.flet_ctrt_tp_cd like @[flet_ctrt_tp_cd]||'%'
		#end
		#if (${vsl_dznd_capa_fr} != "")
		and   b.vsl_dznd_capa >= @[vsl_dznd_capa_fr]
		#end
		#if (${vsl_dznd_capa_to} != "")
		and   b.vsl_dznd_capa <= @[vsl_dznd_capa_to]
		#end
		
		#if (${lane_cd} != "")
		and   d.slan_cd like @[lane_cd]||'%'
		#end

		#if (${ownr_seq} != "")
		and   b.vndr_seq = e.vndr_seq
		#end
		and	  a.vsl_cd like @[vsl_cd]||'%'
		and   a.dck_sel_cd = 'C'
		and   a.flet_dck_svey_tp_cd like @[flet_dck_svey_tp_cd]||'%'
		and   not exists (select null from fms_dck_skd where vsl_cd = a.vsl_cd and dck_sel_cd = 'E') 
		and a.dck_fm_dt <= to_date(@[to_duration]||'1231','yyyymmdd') and a.dck_to_dt >= to_date(@[fr_duration]||'0101','yyyymmdd')			]]></sql>
			<params>
				<param name="fr_duration" type="12" value="" out="N"/>
				<param name="to_duration" type="12" value="" out="N"/>
				<param name="ownr_seq" type="12" value="" out="N"/>
				<param name="flet_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_dznd_capa_fr" type="12" value="" out="N"/>
				<param name="vsl_dznd_capa_to" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="ydSeq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="flet_dck_svey_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
