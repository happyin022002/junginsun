<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDBDAOFmsOwnrAcctSlpInterfaceCSQL">
			<desc><![CDATA[ERP에서 발생한 용대선관련 본지점 계정에 대해 환대체 정보를 수신 Insert]]></desc>
			<sql><![CDATA[
INSERT INTO FMS_OWNR_ACCT_SLP( 
	SLP_TP_CD,
	SLP_FUNC_CD,
	SLP_OFC_CD,
	SLP_ISS_DT,
	SLP_SER_NO,
	SLP_SEQ_NO,
	ACCT_CD,
	CTR_CD,
	EFF_DT,
	VSL_CD,
	SKD_VOY_NO,
	SKD_DIR_CD,
	REV_DIR_CD,
	N1ST_CURR_CD,
	N1ST_AMT,
	N2ND_CURR_CD,
	N2ND_AMT,
	LOCL_XCH_RT_AMT,
	AP_DESC,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT)
VALUES( 
	@[slp_tp_cd],
	@[slp_func_cd],
	@[slp_ofc_cd],
	@[slp_iss_dt],
	@[slp_ser_no],
	@[slp_seq_no],
	@[acct_cd],
	@[ctr_cd],
	@[eff_dt],
	@[vsl_cd],
	@[skd_voy_no],
	SUBSTR(@[skd_dir_cd],1,1),
	SUBSTR(@[rev_dir_cd],2,1),
	@[n1st_curr_cd],
	@[n1st_amt],
	@[n2nd_curr_cd],
	@[n2nd_amt],
	@[locl_xch_rt_amt],
	HJSEAI_PKG.h_decode(@[ap_desc],'ERP','UTF8'),
	'FNS056-0001',
	SYSDATE,
	'FNS056-0001',
	SYSDATE)			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="slp_seq_no" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="ctr_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
				<param name="n1st_curr_cd" type="12" value="" out="N"/>
				<param name="n1st_amt" type="3" value="" out="N"/>
				<param name="n2nd_curr_cd" type="12" value="" out="N"/>
				<param name="n2nd_amt" type="3" value="" out="N"/>
				<param name="locl_xch_rt_amt" type="3" value="" out="N"/>
				<param name="ap_desc" type="12" value="" out="N"/>
			</params>
		</query>
