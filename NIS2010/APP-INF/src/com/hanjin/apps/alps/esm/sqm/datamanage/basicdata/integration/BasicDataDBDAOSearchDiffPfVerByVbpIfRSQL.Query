<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOSearchDiffPfVerByVbpIfRSQL">
			<desc><![CDATA[Target VVD Fix시 PF Skd Group MGMT에서 생성된 PF SKD VER과 다른것이 있는지 조회합니다_VBP에서 조회]]></desc>
			<sql><![CDATA[
SELECT A1.RLANE_CD||'-'||A1.PF_SKD_TP_CD AS DIFF_PF_VER
FROM 
    ( 
        SELECT DISTINCT
               VBP.TRD_CD
              ,VBP.RLANE_CD
              ,VBP.SUB_TRD_CD
              ,VSK.PF_SKD_TP_CD
          FROM SQM_QTA_TGT_VVD_IF VBP
              ,SQM_QTA_LANE_MGMT LANE
              ,VSK_BUD_VSL_SKD VSK
              ,( SELECT TRD_CD
                      ,RLANE_CD
                      ,DIR_CD
                      ,SUB_TRD_CD
                      ,ROUND(SUM(LOD_QTY)/53,0) LOD_QTY
                  FROM SQM_PERF_IF
                 WHERE 1=1
                   AND BSE_TP_CD   = @[f_bse_tp_cd] 
                   AND BSE_YR      = @[f_bse_yr]
                   AND BSE_QTR_CD  = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
                   AND SUB_TRD_CD  = 'IP'
                   AND OFC_VW_CD   = 'C'
                   AND SQM_LVL_CD  = '2'
                   AND QTA_TGT_CD  = 'D'
                 GROUP BY TRD_CD, RLANE_CD, DIR_CD,SUB_TRD_CD
                )PFMC
         WHERE 1=1
           AND VBP.TRD_CD      = LANE.TRD_CD
           AND VBP.RLANE_CD    = LANE.RLANE_CD
           AND VBP.SUB_TRD_CD  = LANE.SUB_TRD_CD
           AND VBP.DIR_CD      = NVL(LANE.LANE_DIR_CD,VBP.DIR_CD)
           AND VBP.TRD_CD      = PFMC.TRD_CD(+)
           AND VBP.RLANE_CD    = PFMC.RLANE_CD(+)
           AND VBP.SUB_TRD_CD  = PFMC.SUB_TRD_CD(+)
           AND VBP.DIR_CD      = PFMC.DIR_CD (+)
           AND VBP.VSL_CD      = VSK.VSL_CD
           AND VBP.SKD_VOY_NO  = VSK.SKD_VOY_NO
           AND VBP.DIR_CD      = VSK.SKD_DIR_CD
           AND SUBSTR(VBP.RLANE_CD,1,3) = VSK.VSL_SLAN_CD
           AND VBP.DIR_CD      = PFMC.DIR_CD(+)
           AND VBP.TRD_CD      = PFMC.TRD_CD(+)
           AND VBP.RLANE_CD    = PFMC.RLANE_CD(+)
           AND VBP.SUB_TRD_CD  = PFMC.SUB_TRD_CD(+)
           AND VBP.BSE_TP_CD   = @[f_bse_tp_cd] 
           AND VBP.BSE_YR      = @[f_bse_yr]
           AND VBP.BSE_MON     BETWEEN '01' AND '12'
        #if (${f_bse_tp_cd} == 'Y')
           AND VBP.BSE_QTR_CD  = '00'
           AND VBP.BSE_WK      BETWEEN '00' AND '53'
        #else
           AND VBP.BSE_QTR_CD  = @[f_bse_qtr_cd]
           AND VBP.BSE_WK      BETWEEN @[f_fm_wk] AND @[f_to_wk]
        #end
           AND VBP.DELT_FLG    =  'N'
           AND LANE.SQM_ACT_FLG=  'Y'
           AND VBP.TRD_CD      = 'IAS'
       ) A1
WHERE 1=1
AND NOT EXISTS (SELECT 'X' 
                FROM (
                       SELECT DISTINCT SUB_TRD_CD, RLANE_CD, PF_SVC_TP_CD
                        FROM SQM_SCTR_PF_GRP
                        WHERE 1=1
                        AND BSE_TP_CD  = @[f_bse_tp_cd] 
                        AND BSE_YR     = @[f_bse_yr]
                        AND BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
                        ) A2
                WHERE A2.SUB_TRD_CD||A2.RLANE_CD||A2.PF_SVC_TP_CD =  A1.SUB_TRD_CD||A1.RLANE_CD||A1.PF_SKD_TP_CD)
ORDER BY RLANE_CD , PF_SKD_TP_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
