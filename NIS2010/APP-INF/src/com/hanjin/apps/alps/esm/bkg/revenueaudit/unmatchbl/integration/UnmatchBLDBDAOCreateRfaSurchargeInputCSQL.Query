			<sql><![CDATA[
INSERT INTO BKG_AUTO_RT_OCN_FRT_TMP (
     BKG_NO                ,OFT_CMB_SEQ       ,USR_ID       ,OFRT_SEQ     ,CNTR_TPSZ_CD	  , EQ_SUBST_CNTR_TPSZ_CD
    ,CTRT_CNTR_TPSZ_CD     ,RCV_TERM_CD       ,DE_TERM_CD   ,DRY_CGO_FLG  ,AWK_CGO_FLG
    ,DCGO_FLG              ,RC_FLG            ,BB_CGO_FLG   ,SOC_FLG      ,IMDG_CLSS_CD 
    ,PRC_GEN_SPCL_RT_TP_CD ,PRC_CMDT_HDR_SEQ  ,PRC_ROUT_SEQ ,PRC_RT_SEQ   ,OP_CNTR_QTY  
    ,CHG_CD                ,CURR_CD           
    ,CHG_UT_AMT   
    ,RAT_AS_QTY            ,CHG_AMT      
    ,RAT_UT_CD             ,CGO_CATE_CD       ,FRT_INCL_XCLD_DIV_CD
    ,FRT_TERM_CD           ,PROP_NO           ,AMDT_SEQ     ,SVC_SCP_CD   ,MST_RFA_ROUT_ID
    ,CRE_USR_ID            ,CRE_DT            ,UPD_USR_ID   ,UPD_DT
)
VALUES (
     @[bkg_no]              ,@[oft_cmb_seq]      ,@[usr_id]       ,@[ofrt_seq]    ,@[cntr_tpsz_cd], @[eq_subst_cntr_tpsz_cd]
    ,@[ctrt_cntr_tpsz_cd]   ,@[rcv_term_cd]      ,@[de_term_cd]   ,@[dry_cgo_flg] ,@[awk_cgo_flg]
    ,@[dcgo_flg]            ,@[rc_flg]           ,@[bb_cgo_flg]   ,@[soc_flg]     ,@[imdg_clss_cd] 
    ,'S'                    ,@[prc_cmdt_hdr_seq] ,@[prc_rout_seq] ,@[prc_rt_seq]  ,@[op_cntr_qty] 
    ,@[chg_cd]              ,@[curr_cd] 
    ,@[chg_ut_amt]
    ,@[rat_as_qty]          ,NVL(TO_NUMBER(@[chg_ut_amt]),0) * NVL(TO_NUMBER(@[rat_as_qty]),0)
    ,@[rat_ut_cd]           ,@[cgo_cate_cd]      ,@[incl_oft_flg]
    ,@[frt_term_cd]          ,@[prop_no]          ,@[amdt_seq]      ,@[svc_scp_cd] ,@[mst_rfa_rout_id]
    ,@[usr_id]              ,SYSDATE             ,@[usr_id]        ,SYSDATE
)			]]></sql>
			<params>
