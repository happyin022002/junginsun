<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchFirstVVDRSQL">
			<desc><![CDATA[SELECT FIRST VVD]]></desc>
			<sql><![CDATA[
SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD F_VVD
FROM BKG_VVD
WHERE BKG_NO = @[bkg_no]
AND VSL_PRE_PST_CD IN ('S','T')
AND ROWNUM=1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
