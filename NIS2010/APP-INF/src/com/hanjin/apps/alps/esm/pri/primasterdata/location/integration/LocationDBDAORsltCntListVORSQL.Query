<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAORsltCntListVORSQL">
			<desc><![CDATA[2013.01.23 전윤주 [CHM-201322530] MDM 테이블 delete flag 체크 로직 추가
]]></desc>
			<sql><![CDATA[
SELECT A.CNT_CD
	 , A.CNT_NM
	 , B.CONTI_CD
	 , A.SCONTI_CD
  FROM MDM_COUNTRY A
	 , MDM_SUBCONTINENT B
 WHERE A.SCONTI_CD = B.SCONTI_CD
   AND A.DELT_FLG = 'N'
   AND B.DELT_FLG = 'N'
#if (${cnt_cd} != '') 
   AND A.CNT_CD LIKE  UPPER(@[cnt_cd] || '%')
#end
#if (${cnt_nm} != '') 
   AND UPPER(A.CNT_NM) LIKE UPPER('%' || @[cnt_nm] || '%')
#end
 ORDER BY A.CNT_CD ASC			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cnt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
