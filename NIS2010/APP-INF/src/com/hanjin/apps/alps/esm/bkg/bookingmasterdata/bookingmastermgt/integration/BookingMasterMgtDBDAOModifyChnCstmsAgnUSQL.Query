<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOModifyChnCstmsAgnUSQL">
			<desc><![CDATA[BKG_CSTMS_CHN_AGN_STUP update]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_CHN_AGN_STUP 
SET    CHN_CSTMS_AGN_NM = @[chn_cstms_agn_nm], 
       EDI_RCV_ID = @[edi_rcv_id],
       EDI_SND_ID = @[edi_snd_id],
       CHN_CSTMS_AGN_RMK = @[chn_cstms_agn_rmk],
	   CHN_CSTMS_AGN_FULL_NM = @[chn_cstms_agn_full_nm],
	   CHN_CSTMS_AGN_PIC_NM  = @[chn_cstms_agn_pic_nm] ,
	   CHN_CSTMS_AGN_PHN_NO = @[chn_cstms_agn_phn_no] ,
	   CHN_CSTMS_AGN_EML     = @[chn_cstms_agn_eml] ,
	   CHN_CSTMS_AGN_ADDR   = @[chn_cstms_agn_addr]  ,
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = SYSDATE
WHERE AGN_CTRL_OFC_CD  = @[agn_ctrl_ofc_cd]
AND   POD_CD           = @[pod_cd]
AND   CHN_CSTMS_AGN_CD = @[chn_cstms_agn_cd]
AND   SLAN_CD          = @[slan_cd]			]]></sql>
			<params>
				<param name="chn_cstms_agn_nm" type="12" value="" out="N"/>
				<param name="edi_rcv_id" type="12" value="" out="N"/>
				<param name="edi_snd_id" type="12" value="" out="N"/>
				<param name="chn_cstms_agn_rmk" type="12" value="" out="N"/>
				<param name="chn_cstms_agn_full_nm" type="12" value="" out="N"/>
				<param name="chn_cstms_agn_pic_nm" type="12" value="" out="N"/>
				<param name="chn_cstms_agn_phn_no" type="12" value="" out="N"/>
				<param name="chn_cstms_agn_eml" type="12" value="" out="N"/>
				<param name="chn_cstms_agn_addr" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agn_ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="chn_cstms_agn_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
