<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOcopyEurTroDgSeqBySplitCSQL">
			<desc><![CDATA[copyEurTroDgSeqBySplit]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_EUR_TRO_DG_SEQ
(
       BKG_NO
      ,IO_BND_CD
      ,TRO_SEQ
      ,TRO_DCGO_SEQ
      ,DCGO_SEQ
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT DISTINCT
       @[target_bkg_no]
      ,A.IO_BND_CD
      ,A.TRO_SEQ
      ,A.TRO_DCGO_SEQ
      ,A.DCGO_SEQ
      ,@[usr_id]
      ,SYSDATE
      ,@[usr_id]
      ,SYSDATE
  FROM BKG_EUR_TRO_DG_SEQ A
      ,SCE_TRO_MAPG B
 WHERE A.BKG_NO = @[source_bkg_no]
   AND B.BKG_NO = @[target_bkg_no]
   AND A.IO_BND_CD = B.IO_BND_CD
   AND A.TRO_SEQ = B.TRO_SEQ
   AND EXISTS (SELECT 'X'
                 FROM BKG_EUR_TRO C
                WHERE C.BKG_NO = A.BKG_NO
                  AND C.TRO_SEQ = A.TRO_SEQ
                  AND C.IO_BND_CD = A.IO_BND_CD
                  AND C.CFM_FLG = 'Y')			]]></sql>
			<params>
				<param name="target_bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="source_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
