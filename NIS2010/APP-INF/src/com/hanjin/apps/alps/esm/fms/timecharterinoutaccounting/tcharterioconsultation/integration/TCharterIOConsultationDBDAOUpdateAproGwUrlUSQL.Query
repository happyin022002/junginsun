<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOUpdateAproGwUrlUSQL">
			<desc><![CDATA[GW에서 결과 값 전송
AR_INV_HDR 의  GW Url, Request_id 업데이트]]></desc>
			<sql><![CDATA[
UPDATE AR_INV_HDR  
SET GW_CSR_RQST_ID = @[request_id]
    ,GW_APRO_URL_CTNT = @[gw_url]
WHERE CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="request_id" type="12" value="" out="N"/>
				<param name="gw_url" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
