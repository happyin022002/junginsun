<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpaceAllocationManageDBDAOSearchOfficeBKGInControlDSQL">
			<desc><![CDATA[DELETE Control Office BKG]]></desc>
			<sql><![CDATA[
DELETE	
	SPC_BKG_CTRL_OPT_DTL
WHERE
	TRD_CD = @[trd_cd]
	AND SUB_TRD_CD = @[sub_trd_cd]
--	AND RLANE_CD = @  [rlane_cd]
--	AND DIR_CD = @   [dir_cd]
	AND BKG_CTRL_TP_CD = @[bkg_ctrl_tp_cd]
    AND BKG_CTRL_DTL_CD = @[bkg_ctrl_dtl_cd]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="bkg_ctrl_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_ctrl_dtl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
