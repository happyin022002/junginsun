<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQHoldingDBDAOSearchCntrPdmInvtListRSQL">
			<desc><![CDATA[Container PDM by Inventory 조회]]></desc>
			<sql><![CDATA[
SELECT  COST_YR||'-'||BSE_QTR_CD COST_YR_QTR,
        TPSZ_CD,
        INVT_CNTR_QTY,
        COST_AMT,
        PD_COST_AMT
FROM MAS_CNTR_EQ_COST
WHERE COST_YR||BSE_QTR_CD BETWEEN @[fr_year]||@[fr_qtr] AND @[to_year]||@[to_qtr]			]]></sql>
			<params>
				<param name="fr_year" type="12" value="" out="N"/>
				<param name="fr_qtr" type="12" value="" out="N"/>
				<param name="to_year" type="12" value="" out="N"/>
				<param name="to_qtr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
