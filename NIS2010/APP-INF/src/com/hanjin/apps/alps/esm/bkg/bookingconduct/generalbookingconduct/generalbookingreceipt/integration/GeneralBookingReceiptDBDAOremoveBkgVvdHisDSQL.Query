<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOremoveBkgVvdHisDSQL">
			<desc><![CDATA[cod를 위해 bkg_vvd_his table에서 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_VVD_HIS
WHERE	BKG_NO = @[bkg_no]
AND	CORR_NO = @[corr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="corr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
