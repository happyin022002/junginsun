			<sql><![CDATA[
update pri_sp_scp_rt_scg
set
	adj_scg_amt = @[adj_scg_amt], 
	adj_scg_usd_amt = @[adj_scg_usd_amt], 
	trf_scg_rmk = @[trf_scg_rmk], 
