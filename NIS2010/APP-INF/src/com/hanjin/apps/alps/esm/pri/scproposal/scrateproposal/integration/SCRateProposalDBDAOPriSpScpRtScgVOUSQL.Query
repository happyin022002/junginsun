				<param name="adj_scg_usd_amt" type="12" value="" out="N"/>
				<param name="trf_scg_rmk" type="12" value="" out="N"/>
				<param name="adj_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
