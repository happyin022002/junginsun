<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOmodifyVesselInfoUSQL">
			<desc><![CDATA[modifyVesselInfo]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_JP_VSL SET 
	CSTMS_MF_ID = @[cstms_mf_cd]
#if (${lodg_wgt}!= '') 
,	LODG_WGT = @[lodg_wgt]
#end
#if (${mf_rmk}!= '') 
,	MF_RMK = @[mf_rmk]
#end
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	POD_CD = @[pod_cd]			]]></sql>
			<params>
				<param name="cstms_mf_cd" type="12" value="" out="N"/>
				<param name="lodg_wgt" type="6" value="" out="N"/>
				<param name="mf_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
