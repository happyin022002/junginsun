<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOModifyEurTroCyInfoUSQL">
			<desc><![CDATA[return cy정보를 업데이트 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_EUR_TRO
   SET CNTR_RTN_YD_CD = @[cntr_rtn_yd_cd]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = sysdate
 WHERE BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]
   AND HLG_TP_CD = 'M'
   AND IO_BND_CD ='I'   
   AND CFM_FLG = 'Y'			]]></sql>
			<params>
				<param name="cntr_rtn_yd_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
