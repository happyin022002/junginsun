<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOOwnersAccountCsulSlpsRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT FCS.SLP_TP_CD
,	FCS.SLP_FUNC_CD
,	FCS.SLP_OFC_CD
,	FCS.SLP_ISS_DT
,	FCS.SLP_SER_NO
,	FCS.SLP_SEQ_NO
,	FCS.ACCT_CD
,	FCS.CTR_CD
,	FCS.SLP_LOC_CD
,	FCS.CSR_CURR_CD
,	FCS.CSR_AMT
,	FCS.CSR_DESC
,	FCS.VNDR_SEQ
,	FCS.TRNS_CURR_CD
,	FCS.TRNS_AMT
,	FCS.VAT_FLG
,	FCS.STL_FLG
,	FCS.INV_SEQ
,	FCS.FLET_SRC_TP_CD
,	FCS.VSL_CD
,	FCS.SKD_VOY_NO
,	FCS.SKD_DIR_CD
,	FCS.REV_DIR_CD
,	FCS.ORG_SLP_FUNC_CD
,	FCS.ORG_SLP_OFC_CD
,	FCS.ORG_ISS_DT
,	FCS.ORG_SLP_SER_NO
,	FCS.ORG_SLP_SEQ_NO
,   (SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR WHERE VNDR_SEQ = FCS.VNDR_SEQ) VNDR_LGL_ENG_NM
,	FCS.VSL_CD||FCS.SKD_VOY_NO||FCS.SKD_DIR_CD||FCS.REV_DIR_CD VVD_CD
,	FCS.TO_INV_NO
,	FCS.CRE_USR_ID
,	FCS.CRE_DT
,	FCS.UPD_USR_ID
,	FCS.UPD_DT
,	FCS.ACCT_ITM_SEQ
,   (SELECT MI.ACCT_ITM_NM
       FROM FMS_ACCT_CATE MC, FMS_ACCT_ITM MI
      WHERE MC.FLET_ACCT_CATE_CD = 'OW'
        AND MC.ACCT_CD = MI.ACCT_CD
        AND MC.ACCT_ITM_SEQ = MI.ACCT_ITM_SEQ
        AND MC.ACCT_ITM_SEQ = FCS.ACCT_ITM_SEQ) ACCT_ITM_NM
,	FCS.OA_LOC_CD
,	FCS.OA_INV_DT
,	FCS.ATCH_FILE_OA_LNK_ID
,	FCS.PAIR_SLP_TP_CD
,	FCS.PAIR_SLP_FUNC_CD
,	FCS.PAIR_SLP_OFC_CD
,	FCS.PAIR_SLP_ISS_DT
,	FCS.PAIR_SLP_SER_NO
,	FCS.PAIR_SLP_SEQ_NO
,	(SELECT COUNT(1) 
       FROM FMS_OWNR_ACCT_ATCH_FILE 
      WHERE ATCH_FILE_OA_LNK_ID = FCS.SLP_TP_CD||FCS.SLP_FUNC_CD||FCS.SLP_OFC_CD||FCS.SLP_ISS_DT||FCS.SLP_SER_NO||FCS.SLP_SEQ_NO) ATCH_FILE_OA_LNK_CNT -- Attach File Count
, (SELECT DECODE(P.OA_STL_STS_CD, 'RC', 'Received',
                                   'EA', 'Editing Attachment',       
                                   'HD', 'Holding',  
                                   'CN', 'Cancelled',  
                                   'RF', 'Refund',  
                                   'ST', 'Settled','')
   FROM FMS_OWNR_ACCT_SLP P
   WHERE 1 = 1
     AND FCS.SLP_TP_CD   = P.SLP_TP_CD
     AND FCS.SLP_FUNC_CD = P.SLP_FUNC_CD
     AND FCS.SLP_OFC_CD  = P.SLP_OFC_CD
     AND FCS.SLP_ISS_DT  = P.SLP_ISS_DT
     AND FCS.SLP_SER_NO  = P.SLP_SER_NO
     AND FCS.SLP_SEQ_NO  = P.SLP_SEQ_NO)  OA_STL_STS_CD

  FROM FMS_CONSULTATION FC, FMS_CSUL_SLP FCS
 WHERE 1=1
   AND FC.SLP_TP_CD = FCS.SLP_TP_CD
   AND FC.SLP_FUNC_CD = FCS.SLP_FUNC_CD
   AND FC.SLP_OFC_CD = FCS.SLP_OFC_CD
   AND FC.SLP_ISS_DT = FCS.SLP_ISS_DT
   AND FC.SLP_SER_NO = FCS.SLP_SER_NO
#if(${s_flg} == 'R1')
   AND FCS.SLP_TP_CD||FCS.SLP_FUNC_CD||FCS.SLP_OFC_CD||FCS.SLP_ISS_DT||FCS.SLP_SER_NO||FCS.SLP_SEQ_NO = @[csr_no]
#else
   AND FCS.SLP_TP_CD||FCS.SLP_FUNC_CD||FCS.SLP_OFC_CD||FCS.SLP_ISS_DT||FCS.SLP_SER_NO = @[csr_no]
#end
ORDER BY 6			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
