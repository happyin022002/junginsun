<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupCommodityQuotationDBDAOPriRqGrpCmdtDtlVOAddGlineCopyTPWCSQL">
			<desc><![CDATA[GLINE COPY TPW]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RQ_GRP_CMDT_DTL (
	QTTN_NO
,	QTTN_VER_NO
,	GRP_CMDT_SEQ
,	GRP_CMDT_DTL_SEQ
,	PRC_CMDT_TP_CD
,	PRC_CMDT_DEF_CD
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT @[qttn_no] AS PROP_NO
     , @[qttn_ver_no] AS AMDT_SEQ
     , @[new_grp_cmdt_seq] AS GRP_CMDT_SEQ
     , @[new_grp_cmdt_dtl_seq] AS GRP_CMDT_DTL_SEQ
     , PRC_CMDT_TP_CD
     , PRC_CMDT_DEF_CD
     , 'GC' AS SRC_INFO_CD
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM PRI_RG_GRP_CMDT_DTL
WHERE SVC_SCP_CD = @[svc_scp_cd]
AND   GLINE_SEQ = @[gline_seq]
AND   GRP_CMDT_SEQ = @[grp_cmdt_seq]
AND   GRP_CMDT_DTL_SEQ = @[grp_cmdt_dtl_seq]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="new_grp_cmdt_seq" type="12" value="" out="N"/>
				<param name="new_grp_cmdt_dtl_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="grp_cmdt_seq" type="12" value="" out="N"/>
				<param name="grp_cmdt_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
