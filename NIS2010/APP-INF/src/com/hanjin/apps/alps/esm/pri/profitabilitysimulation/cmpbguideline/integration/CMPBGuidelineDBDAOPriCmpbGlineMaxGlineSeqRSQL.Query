<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMPBGuidelineDBDAOPriCmpbGlineMaxGlineSeqRSQL">
			<desc><![CDATA[max GlineSeq select]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(MAX(GLINE_SEQ)+1,1) AS MAX_GLINE_SEQ
FROM PRI_CMPB_GLINE_MN
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND     CRE_OFC_CD = @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
