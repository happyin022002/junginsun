<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchSpclReqInVORSQL">
			<desc><![CDATA[spcl cgo 재 req를 위한 seq를 조회한다.]]></desc>
			<sql><![CDATA[
select 'R' apro_cd, nvl(dcgo_seq, 0) dcgo_seq, 0 rc_seq, 0 awk_cgo_seq, 0 bb_cgo_seq
  from bkg_dg_cgo
 where bkg_no = @[bkg_no]
   and SPCL_CGO_APRO_CD in ('R', 'Y', 'P')
union all
select 'R' apro_cd, 0 dcgo_seq, nvl(rc_seq, 0) rc_seq, 0 awk_cgo_seq, 0 bb_cgo_seq
  from bkg_rf_cgo
 where bkg_no = @[bkg_no]
   and SPCL_CGO_APRO_CD in ('R', 'Y', 'P')
union all
select 'R' apro_cd, 0  dcgo_seq, 0 rc_seq, nvl(awk_cgo_seq, 0) awk_cgo_seq, 0 bb_cgo_seq
  from bkg_awk_cgo
 where bkg_no = @[bkg_no]
   and SPCL_CGO_APRO_CD in ('R', 'Y', 'P')
union all
select 'R' apro_cd, 0 dcgo_seq, 0 rc_seq, 0 awk_cgo_seq, nvl(bb_cgo_seq, 0) bb_cgo_seq
  from bkg_bb_cgo
 where bkg_no = @[bkg_no]
   and SPCL_CGO_APRO_CD in ('R', 'Y', 'P')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
