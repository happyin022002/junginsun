<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOApplyToPLOP51CSQL">
			<desc><![CDATA[ApplyTo PL OP5_1]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_PFIT_LSS_SMRY A
     USING (SELECT
                VSL_CD                                                                                                  AS VSL_CD
               ,SKD_VOY_NO                                                                                              AS SKD_VOY_NO
               ,DIR_CD                                                                                                  AS DIR_CD
               ,IOC_CD                                                                                                  AS IOC_CD
               ,RLANE_CD                                                                                                AS RLANE_CD
               ,TRD_CD                                                                                                  AS TRD_CD
               ,CNTR_TPSZ_CD                                                                                            AS CNTR_TPSZ_CD
               ,DECODE(NO,1,STND_COST_CD,2,'TSCTRB00', 3,'CMMT0000',4,'HJSAMT00',5,'CHTOUT00','INTTRDTS')               AS STND_COST_CD
               ,SUM(DECODE(NO,1,ESTM_PL_AMT,2,TS_CTRB_BSE_COST_AMT,3,EXTERNAL_TS,4,HJS_SLS_AMT,5,CO_AMT,INTERNAL_TS))   AS ESTM_PL_AMT
               ,0                                                                                                       AS RA_PL_AMT
               ,0                                                                                                       AS ACT_PL_AMT
               ,0                                                                                                       AS ACCL_RT_AMT
               ,'X'                                                                                                     AS PL_DESC
               ,'SELHO'                                                                                                 AS AGMT_SGN_OFC_CD
               ,'SELHO'                                                                                                 AS SLS_OFC_CD
               ,SUM(DECODE(NO,1,HJS_SLS_AMT2,0))                                                                        AS HJS_SLS_AMT2  /*HJS비율 금액*/
               ,SUM(DECODE(NO,1,CO_AMT,0))                                                                              AS CO_AMT
               ,SUM(DECODE(NO,1,N5TH_ASGN_AMT,0))                                                                       AS N5TH_ASGN_AMT
               ,SUM(DECODE(NO,1,ADJ_TRD_OTR_FNL_AMT,0))                                                                 AS ADJ_TRD_OTR_FNL_AMT
              FROM (SELECT
                         VSL_CD                        VSL_CD
                        ,SKD_VOY_NO                    SKD_VOY_NO
                        ,DIR_CD                        DIR_CD
                        ,IOC_CD                        IOC_CD
                        ,RLANE_CD                      RLANE_CD
                        ,TRD_CD                        TRD_CD
                        ,CNTR_TPSZ_CD                  CNTR_TPSZ_CD
                        ,STND_COST_CD                  STND_COST_CD
                        ,SUM(ESTM_PL_AMT)              ESTM_PL_AMT
                        ,SUM(TS_CTRB_BSE_COST_AMT)     TS_CTRB_BSE_COST_AMT
                        ,SUM(CMMT_BSE_COST_AMT)        CMMT_BSE_COST_AMT
                        ,SUM(HJS_SLS_AMT)              HJS_SLS_AMT
                        ,SUM(HJS_SLS_AMT2)             HJS_SLS_AMT2    /*HJS비율 금액*/
                        ,SUM(CO_AMT)                   CO_AMT          /*CHT OUT비율 금액*/
                        ,SUM(INTERNAL_TS)              INTERNAL_TS
                        ,SUM(EXTERNAL_TS)              EXTERNAL_TS
                        ,SUM(N5TH_ASGN_AMT)            N5TH_ASGN_AMT
                        ,SUM(ADJ_TRD_OTR_FNL_AMT)      ADJ_TRD_OTR_FNL_AMT

                      FROM (
                            SELECT
                                 A.VSL_CD                                                                                  AS VSL_CD
                                ,A.SKD_VOY_NO                                                                              AS SKD_VOY_NO
                                ,A.DIR_CD                                                                                  AS DIR_CD
                                ,A.IOC_CD                                                                                  AS IOC_CD
                                ,A.RLANE_CD                                                                                AS RLANE_CD
                                ,A.TRD_CD                                                                                  AS TRD_CD
                                ,'XXXX'                                                                                    AS CNTR_TPSZ_CD
                                ,B.STND_COST_CD                                                                            AS STND_COST_CD
                                ,CASE WHEN A.TRD_CD IN ('IAS','IES','IMS') THEN NVL(B.N5TH_ASGN_AMT, 0) + NVL(B.IPT_ASGN_AMT, 0)+ NVL(B.CO_AMT,0) 
									  ELSE NVL(B.HJS_SLS_AMT, 0) + NVL(B.N5TH_ASGN_AMT, 0) + NVL(B.IPT_ASGN_AMT, 0)+NVL(B.CO_AMT,0) END AS ESTM_PL_AMT
                                ,NVL(B.TS_CTRB_BSE_COST_AMT,0)                                                             AS TS_CTRB_BSE_COST_AMT
                                ,NVL(B.CMMT_BSE_COST_AMT,0)                                                                AS CMMT_BSE_COST_AMT
                                ,NVL(B.HJS_SLS_AMT,0) + NVL(B.IPT_ASGN_AMT, 0)                                             AS HJS_SLS_AMT

                                ,CASE WHEN A.TRD_CD IN ('IAS','IES','IMS') THEN NVL(B.N5TH_ASGN_AMT, 0) + NVL(B.IPT_ASGN_AMT, 0) 
									  ELSE NVL(B.HJS_SLS_AMT,0) + NVL(B.N5TH_ASGN_AMT, 0) + NVL(B.IPT_ASGN_AMT, 0) END AS HJS_SLS_AMT2

                                ,NVL(B.CO_AMT,0)                                                                           AS CO_AMT
                                ,CASE WHEN (A.TRD_CD = 'IAS') THEN B.N5TH_TS_ASGN_AMT ELSE 0 END                           AS INTERNAL_TS
                                ,CASE WHEN (A.TRD_CD <> 'IAS') THEN B.N5TH_TS_ASGN_AMT ELSE 0 END                          AS EXTERNAL_TS

								/* Allocation Results (Commitment Base)에서의 1st TS Alloc.의 계산로직은  (N5TH_ASGN_AMT-HJS_SLS_AMT) 임 */
                                ,CASE WHEN A.TRD_CD IN ('IAS','IES','IMS') THEN NVL(B.N5TH_ASGN_AMT, 0) - NVL(B.HJS_SLS_AMT,0) 
									  ELSE NVL(B.N5TH_ASGN_AMT, 0) END AS N5TH_ASGN_AMT 
                                ,NVL(B.ADJ_TRD_OTR_FNL_AMT, 0)                                                             AS ADJ_TRD_OTR_FNL_AMT 
                              FROM MAS_MON_VVD A
                                 , MAS_VVD_HIR B
                             WHERE A.VSL_CD     = B.VSL_CD
                               AND A.SKD_VOY_NO = B.SKD_VOY_NO
                               AND A.DIR_CD     = B.DIR_CD
                               AND A.TRD_CD     = B.TRD_CD
                               AND A.RLANE_CD   = B.RLANE_CD
                               AND A.IOC_CD     = B.IOC_CD

                               #if (${trd_cd} != '')
                                  AND A.TRD_CD = @[trd_cd]
                               #end
                               #if (${rlane_cd} != '')
                                  AND A.RLANE_CD = @[rlane_cd]
                               #end
                               #if (${ioc_cd} != '')
                                  AND A.IOC_CD = @[ioc_cd]
                               #end
                               #if (${vsl_cd} != '')
                                  AND A.VSL_CD = @[vsl_cd]
                               #end
                               #if (${skd_voy_no} != '')
                                  AND A.SKD_VOY_NO = @[skd_voy_no]
                               #end
                               #if (${dir_cd} != '')
                                  AND A.DIR_CD = @[dir_cd]
                               #end

                               #if (${priod} == 'M')
                                   #if (${fmMonth} != '')
                                       AND A.COST_YRMON BETWEEN @[cost_yrmon_s] AND @[cost_yrmon_e]
                                   #else
                                       AND A.COST_YRMON LIKE @[cost_yrmon] || '%'
                                   #end
                               #elseif (${priod} == 'W')
                                   AND A.SLS_YRMON LIKE @[sls_yrmon]
                                   #if (${fmWeek} != '')
                                       AND A.COST_WK BETWEEN @[cost_wk_s] AND @[cost_wk_e]
                                   #end
                               #end

                               AND NVL(A.DELT_FLG,'N') = 'N'
                           )
                         GROUP BY VSL_CD
                                 ,SKD_VOY_NO
                                 ,DIR_CD
                                 ,IOC_CD
                                 ,RLANE_CD
                                 ,TRD_CD
                                 ,CNTR_TPSZ_CD
                                 ,STND_COST_CD
                  )
                 ,(SELECT CPY_NO NO
                     FROM COM_CPY_NO
                    WHERE CPY_NO BETWEEN 1 AND 6)
           GROUP BY
               VSL_CD
              ,SKD_VOY_NO
              ,DIR_CD
              ,IOC_CD
              ,RLANE_CD
              ,TRD_CD
              ,CNTR_TPSZ_CD
              ,DECODE(NO,1,STND_COST_CD,2,'TSCTRB00', 3,'CMMT0000',4,'HJSAMT00',5,'CHTOUT00','INTTRDTS')
          ) B
     ON (    A.TRD_CD       = B.TRD_CD
         AND A.RLANE_CD     = B.RLANE_CD
         AND A.IOC_CD       = B.IOC_CD
         AND A.VSL_CD       = B.VSL_CD
         AND A.SKD_VOY_NO   = B.SKD_VOY_NO
         AND A.SKD_DIR_CD   = B.DIR_CD
         AND A.STND_COST_CD = B.STND_COST_CD
         AND A.CNTR_TPSZ_CD = B.CNTR_TPSZ_CD)
     WHEN MATCHED THEN
        UPDATE
           SET A.N5TH_ESTM_PL_AMT  = B.ESTM_PL_AMT
              ,A.UPD_USR_ID   = @[upd_usr_id]
              ,A.UPD_DT       = SYSDATE
              ,A.N5TH_HJS_SLS_AMT  = B.HJS_SLS_AMT2
              ,A.ADJ_TRD_OTR_FNL_AMT        = B.ADJ_TRD_OTR_FNL_AMT 
              ,A.N5TH_ASGN_AMT = B.N5TH_ASGN_AMT
              ,A.N5TH_CO_AMT       = B.CO_AMT
     WHEN NOT MATCHED THEN
        INSERT(
             A.VSL_CD
           , A.SKD_VOY_NO
           , A.SKD_DIR_CD
           , A.IOC_CD
           , A.RLANE_CD
           , A.TRD_CD
           , A.CNTR_TPSZ_CD
           , A.STND_COST_CD
           , A.N5TH_ESTM_PL_AMT
           , A.RA_PL_AMT
           , A.ACT_PL_AMT
           , A.ACCL_RT_AMT
           , A.PL_DESC
           , A.AGMT_SGN_OFC_CD
           , A.SLS_OFC_CD
           , A.N5TH_HJS_SLS_AMT
           , A.N5TH_CO_AMT
           , A.N5TH_ASGN_AMT
           , A.ADJ_TRD_OTR_FNL_AMT
           , A.CRE_USR_ID
           , A.CRE_DT
           , A.UPD_USR_ID
           , A.UPD_DT
        )VALUES(
             B.VSL_CD
           , B.SKD_VOY_NO
           , B.DIR_CD
           , B.IOC_CD
           , B.RLANE_CD
           , B.TRD_CD
           , B.CNTR_TPSZ_CD
           , B.STND_COST_CD
           , B.ESTM_PL_AMT
           , B.RA_PL_AMT
           , B.ACT_PL_AMT
           , B.ACCL_RT_AMT
           , B.PL_DESC
           , B.AGMT_SGN_OFC_CD
           , B.SLS_OFC_CD
           , B.HJS_SLS_AMT2
           , B.CO_AMT
           , B.N5TH_ASGN_AMT
           , B.ADJ_TRD_OTR_FNL_AMT
           , @[cre_usr_id]
           , SYSDATE
           , @[upd_usr_id]
           , SYSDATE
        )			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="ioc_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="cost_yrmon_s" type="12" value="" out="N"/>
				<param name="cost_yrmon_e" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="sls_yrmon" type="12" value="" out="N"/>
				<param name="cost_wk_s" type="12" value="" out="N"/>
				<param name="cost_wk_e" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
