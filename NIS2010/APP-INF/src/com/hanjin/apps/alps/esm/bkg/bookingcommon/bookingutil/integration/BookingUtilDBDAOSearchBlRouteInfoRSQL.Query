<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchBlRouteInfoRSQL">
			<desc><![CDATA[BookingUtilDBDAOSearchBlRouteInfo]]></desc>
			<sql><![CDATA[
SELECT 
	POR_CD POR_CODE,
	POR_NM POR_NAME,
	POL_CD POL_CODE,
	POL_NM POL_NAME,
	POD_CD POD_CODE,
	POD_NM POD_NAME,
	DEL_CD DEL_CODE,
	DEL_NM DEL_NAME
FROM BKG_BL_DOC
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
