<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAOSearchFeederStatusRSQL">
			<desc><![CDATA[ESM_PRI_7013 :: 엑셀 업로드일 경우 FEEDER 상태를 체크]]></desc>
			<sql><![CDATA[
SELECT FIC_PROP_STS_CD
  FROM PRI_TRF_FDR_MN X
 WHERE X.SVC_SCP_CD = @[svc_scp_cd]
   AND X.ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND X.FDR_TRF_NO = @[fdr_trf_no]
   AND X.AMDT_SEQ   = @[amdt_seq]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
