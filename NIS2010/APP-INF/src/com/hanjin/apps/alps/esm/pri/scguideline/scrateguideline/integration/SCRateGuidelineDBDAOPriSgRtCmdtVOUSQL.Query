<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateGuidelineDBDAOPriSgRtCmdtVOUSQL">
			<desc><![CDATA[CMDT수정]]></desc>
			<sql><![CDATA[
UPDATE pri_sg_rt_cmdt SET 
       prc_cmdt_tp_cd = @[prc_cmdt_tp_cd],
       prc_cmdt_def_cd = @[prc_cmdt_def_cd],
       upd_usr_id = @[upd_usr_id],
       upd_dt = sysdate
 WHERE svc_scp_cd = @[svc_scp_cd]
   AND gline_seq = @[gline_seq]
   AND prc_cust_tp_cd = @[prc_cust_tp_cd]
   AND cmdt_hdr_seq = @[cmdt_hdr_seq]
   AND cmdt_seq = @[cmdt_seq]			]]></sql>
			<params>
				<param name="prc_cmdt_tp_cd" type="12" value="" out="N"/>
				<param name="prc_cmdt_def_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="2" value="" out="N"/>
				<param name="prc_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="cmdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
