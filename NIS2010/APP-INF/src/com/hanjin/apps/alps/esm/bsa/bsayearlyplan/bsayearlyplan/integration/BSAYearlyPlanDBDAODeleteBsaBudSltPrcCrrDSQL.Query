<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAYearlyPlanDBDAODeleteBsaBudSltPrcCrrDSQL">
			<desc><![CDATA[BSA_BUD_SLT_PRC_CRR의 데이타를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE
  FROM BSA_BUD_SLT_PRC_CRR
 WHERE 1=1
   AND (TRD_CD, RLANE_CD, DIR_CD, BSA_SLT_COST_TP_CD, SLT_PRC_SEQ) IN ( SELECT TRD_CD, RLANE_CD, DIR_CD, BSA_SLT_COST_TP_CD, SLT_PRC_SEQ
                                                                          FROM BSA_BUD_SLT_PRC
                                                                         WHERE 1=1
#if (${trd_cd} != 'All')
                                                                           AND TRD_CD   = @[trd_cd]
#end

#if (${rlane_cd} != 'All')
                                                                           AND RLANE_CD = @[rlane_cd]
#end
                                                                      )			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
