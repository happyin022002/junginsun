<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupCommodityQuotationDBDAOPriRqGrpCmdtDtlVOAddGlineCopyCSQL">
			<desc><![CDATA[cmdt dtl gline copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RQ_GRP_CMDT_DTL (
	QTTN_NO
,	QTTN_VER_NO
,	GRP_CMDT_SEQ
,	GRP_CMDT_DTL_SEQ
,	PRC_CMDT_TP_CD
,	PRC_CMDT_DEF_CD
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT  @[qttn_no]
,       @[qttn_ver_no]
,       GRP_CMDT_SEQ
,		GRP_CMDT_DTL_SEQ
,       PRC_CMDT_TP_CD
,       PRC_CMDT_DEF_CD
,       'GC'
,       @[cre_usr_id]
,       SYSDATE
,       @[upd_usr_id]
,       SYSDATE
FROM    PRI_RG_GRP_CMDT_DTL
WHERE   1=1
AND    (SVC_SCP_CD, GLINE_SEQ) =  ( 
									SELECT  SVC_SCP_CD
                                    ,       GLINE_SEQ
                                      FROM (
                                            SELECT SVC_SCP_CD
                                                 , GLINE_SEQ
                                                 , ROW_NUMBER() OVER (ORDER BY EFF_DT DESC) AS SEQ
                                             FROM  PRI_RG_MN
                                             WHERE SVC_SCP_CD = @[svc_scp_cd]
                                             AND   CFM_FLG = 'Y'
                                             AND    TO_CHAR(EFF_DT,'YYYY-MM-DD') <= @[eff_dt]
           									 AND    TO_CHAR(EXP_DT,'YYYY-MM-DD') >= @[eff_dt]
                                            )
                                      WHERE SEQ = 1
                                  )			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
