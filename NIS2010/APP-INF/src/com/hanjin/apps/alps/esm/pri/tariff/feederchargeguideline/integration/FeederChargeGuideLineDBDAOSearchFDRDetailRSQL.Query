<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAOSearchFDRDetailRSQL">
			<desc><![CDATA[Search FDR Detail ( FDRDetailVO 생성)

* History
2013.03.16 [CHM-201534279] 전지예 Pricing Feeder/IHC tariff 45" 칼럼 추가 안]]></desc>
			<sql><![CDATA[
SELECT SVC_SCP_CD
     , ORG_DEST_TP_CD
     , FDR_TRF_NO
     , AMDT_SEQ
	 , RT_SEQ
     , PNT_LOC_CD
     , (SELECT LOC_NM
          FROM MDM_LOCATION
         WHERE LOC_CD = PNT_LOC_CD
           AND DELT_FLG = 'N') AS PNT_LOC_NM
     , BSE_PORT_LOC_CD
     , RCV_DE_TERM_CD
     , GLINE_20FT_FRT_RT_AMT
     , GLINE_40FT_FRT_RT_AMT
     , GLINE_45FT_FRT_RT_AMT -- 45' Cost 추가
     , COST_20FT_FRT_RT_AMT
     , COST_40FT_FRT_RT_AMT
     , COST_45FT_FRT_RT_AMT -- 45' Cost 추가
     , LOCL_CURR_COST_20FT_FRT_RT_AMT
     , LOCL_CURR_COST_40FT_FRT_RT_AMT
     , LOCL_CURR_COST_45FT_FRT_RT_AMT -- 45' Cost 추가
     , TRSP_20FT_COST_AMT
     , TRSP_40FT_COST_AMT
     , TRSP_45FT_COST_AMT -- 45' Cost 추가
     , MTY_TRSP_20FT_COST_AMT
     , MTY_TRSP_40FT_COST_AMT
     , MTY_TRSP_45FT_COST_AMT -- 45' Cost 추가
     , TML_20FT_COST_AMT
     , TML_40FT_COST_AMT
     , TML_45FT_COST_AMT -- 45' Cost 추가
     , MB_20FT_RTO
     , MB_40FT_RTO
     , MB_45FT_RTO -- 45' Cost 추가
     , RC_SVC_FLG
     , GLINE_RF_20FT_FRT_RT_AMT
     , GLINE_RF_40FT_FRT_RT_AMT
     , COST_RF_20FT_FRT_RT_AMT
     , COST_RF_40FT_FRT_RT_AMT
     , LOCL_CURR_COST_RF_20FT_RT_AMT
     , LOCL_CURR_COST_RF_40FT_RT_AMT
     , TRSP_RF_20FT_COST_AMT
     , TRSP_RF_40FT_COST_AMT
     , MTY_TRSP_RF_20FT_COST_AMT
     , MTY_TRSP_RF_40FT_COST_AMT
     , TML_RF_20FT_COST_AMT
     , TML_RF_40FT_COST_AMT
     , MB_RF_20FT_RTO
     , MB_RF_40FT_RTO
     , N1ST_CMNC_AMDT_SEQ
     , NEXT_N1ST_CMNC_AMDT_SEQ
     , (SELECT TO_CHAR(EFF_DT, 'YYYYMMDD')
          FROM PRI_TRF_FDR_MN
         WHERE SVC_SCP_CD = SUB.SVC_SCP_CD
           AND ORG_DEST_TP_CD = SUB.ORG_DEST_TP_CD
           AND FDR_TRF_NO = SUB.FDR_TRF_NO
           AND AMDT_SEQ = SUB.N1ST_CMNC_AMDT_SEQ) AS EFF_DT
     , (SELECT CASE
          WHEN M.AMDT_SEQ = SUB.AMDT_SEQ THEN DECODE(TO_CHAR(M.EXP_DT, 'YYYYMMDD'),'99991231', NULL , TO_CHAR(M.EXP_DT, 'YYYYMMDD'))
          WHEN M.AMDT_SEQ <> SUB.AMDT_SEQ THEN TO_CHAR(M.EFF_DT - 1, 'YYYYMMDD')
          WHEN M.EFF_DT <= N.EXP_DT THEN TO_CHAR(M.EFF_DT - 1, 'YYYYMMDD')
               ELSE DECODE(TO_CHAR(N.EXP_DT, 'YYYYMMDD'),'99991231', NULL , TO_CHAR(N.EXP_DT, 'YYYYMMDD')) END AS EXP_DT
          FROM PRI_TRF_FDR_MN M, PRI_TRF_FDR_MN N
         WHERE M.FDR_TRF_NO = SUB.FDR_TRF_NO
           AND N.FDR_TRF_NO = SUB.FDR_TRF_NO
           AND M.ORG_DEST_TP_CD = SUB.ORG_DEST_TP_CD
           AND N.ORG_DEST_TP_CD = SUB.ORG_DEST_TP_CD
           AND M.AMDT_SEQ = @[amdt_seq]
           AND N.AMDT_SEQ = M.AMDT_SEQ - DECODE('1', SUB.AMDT_SEQ, 0, 0, 0, 1)
           AND M.SVC_SCP_CD = SUB.SVC_SCP_CD
           AND N.SVC_SCP_CD = SUB.SVC_SCP_CD) AS EXP_DT
     , SRC_INFO_CD
     , FDR_RT_RMK
     , LOCL_CURR_CD
     , LOCL_CURR_CD AS LOCL_CURR_CD_RF
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , '' AS RHQ_CD
  FROM (SELECT SVC_SCP_CD
             , ORG_DEST_TP_CD
             , FDR_TRF_NO
             , AMDT_SEQ
             , RT_SEQ
             , PNT_LOC_CD
             , BSE_PORT_LOC_CD
             , RCV_DE_TERM_CD
             , GLINE_20FT_FRT_RT_AMT
             , GLINE_40FT_FRT_RT_AMT
             , CASE WHEN @[rhq_cd] = 'HAMRU'
                    THEN GLINE_45FT_FRT_RT_AMT
                    ELSE GLINE_40FT_FRT_RT_AMT
                END GLINE_45FT_FRT_RT_AMT -- 45' Cost 추가
             , COST_20FT_FRT_RT_AMT
             , COST_40FT_FRT_RT_AMT
             , CASE WHEN @[rhq_cd] = 'HAMRU'
                    THEN COST_45FT_FRT_RT_AMT
                    ELSE COST_40FT_FRT_RT_AMT
                END COST_45FT_FRT_RT_AMT -- 45' Cost 추가
             , LOCL_CURR_COST_20FT_FRT_RT_AMT
             , LOCL_CURR_COST_40FT_FRT_RT_AMT
             , CASE WHEN @[rhq_cd] = 'HAMRU'
                    THEN LOCL_CURR_COST_45FT_FRT_RT_AMT
                    ELSE LOCL_CURR_COST_40FT_FRT_RT_AMT
                END LOCL_CURR_COST_45FT_FRT_RT_AMT -- 45' Cost 추가
             , TRSP_20FT_COST_AMT
             , TRSP_40FT_COST_AMT
             , CASE WHEN @[rhq_cd] = 'HAMRU'
                    THEN TRSP_45FT_COST_AMT
                    ELSE TRSP_40FT_COST_AMT
                END TRSP_45FT_COST_AMT -- 45' Cost 추가
             , MTY_TRSP_20FT_COST_AMT
             , MTY_TRSP_40FT_COST_AMT
             , CASE WHEN @[rhq_cd] = 'HAMRU'
                    THEN MTY_TRSP_45FT_COST_AMT
                    ELSE MTY_TRSP_40FT_COST_AMT
                END MTY_TRSP_45FT_COST_AMT -- 45' Cost 추가
             , TML_20FT_COST_AMT
             , TML_40FT_COST_AMT
             , CASE WHEN @[rhq_cd] = 'HAMRU'
                    THEN TML_45FT_COST_AMT
                    ELSE TML_40FT_COST_AMT
                END TML_45FT_COST_AMT -- 45' Cost 추가
             , MB_20FT_RTO
             , MB_40FT_RTO
             , CASE WHEN @[rhq_cd] = 'HAMRU'
                    THEN MB_45FT_RTO
                    ELSE MB_40FT_RTO
                END MB_45FT_RTO -- 45' Cost 추가
             , RC_SVC_FLG
             , GLINE_RF_20FT_FRT_RT_AMT
             , GLINE_RF_40FT_FRT_RT_AMT
             , COST_RF_20FT_FRT_RT_AMT
             , COST_RF_40FT_FRT_RT_AMT
             , LOCL_CURR_COST_RF_20FT_RT_AMT
             , LOCL_CURR_COST_RF_40FT_RT_AMT
             , TRSP_RF_20FT_COST_AMT
             , TRSP_RF_40FT_COST_AMT
             , MTY_TRSP_RF_20FT_COST_AMT
             , MTY_TRSP_RF_40FT_COST_AMT
             , TML_RF_20FT_COST_AMT
             , TML_RF_40FT_COST_AMT
             , MB_RF_20FT_RTO
             , MB_RF_40FT_RTO
             , N1ST_CMNC_AMDT_SEQ
             , LEAD(N1ST_CMNC_AMDT_SEQ) OVER(PARTITION BY SVC_SCP_CD, FDR_TRF_NO, RT_SEQ ORDER BY AMDT_SEQ) AS NEXT_N1ST_CMNC_AMDT_SEQ
             , SRC_INFO_CD
             , FDR_RT_RMK
             , LOCL_CURR_CD
             , CRE_USR_ID
             , CRE_DT
             , UPD_USR_ID
             , UPD_DT
          FROM PRI_TRF_FDR_RT
         WHERE 1=1
           AND (AMDT_SEQ = @[amdt_seq] OR
               (AMDT_SEQ = @[amdt_seq] - 1 AND
               SRC_INFO_CD IN ('NW', 'PC', 'PM', 'GC', 'GM', 'AM')))
           AND SVC_SCP_CD = @[svc_scp_cd]
           AND FDR_TRF_NO = @[fdr_trf_no]
           AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
      ) SUB
 WHERE NEXT_N1ST_CMNC_AMDT_SEQ IS NULL
    OR N1ST_CMNC_AMDT_SEQ <> NEXT_N1ST_CMNC_AMDT_SEQ
 ORDER BY RT_SEQ , AMDT_SEQ			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
