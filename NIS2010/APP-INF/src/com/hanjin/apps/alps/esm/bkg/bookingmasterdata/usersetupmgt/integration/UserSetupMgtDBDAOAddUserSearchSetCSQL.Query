<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOAddUserSearchSetCSQL">
			<desc><![CDATA[UserSetupMgtDBDAOAddUserSearchSetCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CUST_SRCH_SET (
    USR_ID,
    SET_SUB_SEQ,
    CUST_CNT_CD,
    CUST_SEQ,
    SET_SLCT_FLG,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT    
)
VALUES (
    @[usr_id],
    nvl((select /*+ index_desc(bkg_cust_srch_set XPKBKG_CUST_SRCH_SET)  */
                 set_sub_seq
                from bkg_cust_srch_set
                where usr_id = @[usr_id]
                and   rownum = 1),0)+1,

    @[cust_cnt_cd],
    @[cust_seq],
    DECODE(@[set_slct_flg],0,'N','Y'),
    @[upd_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="set_slct_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
