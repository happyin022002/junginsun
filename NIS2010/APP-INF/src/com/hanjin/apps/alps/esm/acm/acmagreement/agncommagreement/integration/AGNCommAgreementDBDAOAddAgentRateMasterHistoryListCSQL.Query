<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommAgreementDBDAOAddAgentRateMasterHistoryListCSQL">
			<desc><![CDATA[AddAgentRateMasterHistoryList]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_AGN_AGMT_MST_HIS
       (AGN_CD,
        AGN_AGMT_NO,
        AGMT_HIS_NO,
        AGMT_FM_DT_CD,
        AGMT_FM_DT,
        AGMT_TO_DT_CD,
        AGMT_TO_DT,
        AGN_AGMT_RMK,
        DELT_FLG,
        CRE_GDT,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT,AGMT_DOC_NO,AGMT_DOC_DESC)
VALUES (@[agn_cd],
        @[agn_agmt_no],        
        @[agmt_his_no],
        @[agmt_fm_dt_cd],
        @[agmt_fm_dt],
        @[agmt_to_dt_cd],
        @[agmt_to_dt],
        @[agn_agmt_rmk],
        @[delt_flg],
        GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS', SYSDATE, 'GMT'),
        @[usr_id],
        SYSDATE,
        @[usr_id],
        SYSDATE,@[agmt_doc_no],@[agmt_doc_desc])			]]></sql>
			<params>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="agn_agmt_no" type="12" value="" out="N"/>
				<param name="agmt_his_no" type="12" value="" out="N"/>
				<param name="agmt_fm_dt_cd" type="12" value="" out="N"/>
				<param name="agmt_fm_dt" type="12" value="" out="N"/>
				<param name="agmt_to_dt_cd" type="12" value="" out="N"/>
				<param name="agmt_to_dt" type="12" value="" out="N"/>
				<param name="agn_agmt_rmk" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="agmt_doc_no" type="12" value="" out="N"/>
				<param name="agmt_doc_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
