<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchMaxSrHisSeqRSQL">
			<desc><![CDATA[BKG_SR_HIS 테이블에서 MAX SR HIS SEQ가져옴]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(SR_HIS_SEQ),0) SR_HIS_SEQ FROM BKG_SR_HIS WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
