<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransshipmentMgtDBDAOsearchPortTurnTimeVVDListRSQL">
			<desc><![CDATA[TransshipmentMgtDBDAOsearchPortTurnTimeVVDListRSQL]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT VSL_CD||SKD_VOY_NO||SKD_DIR_CD AS VVD
FROM	CIM_PORT_TURN_TM_SMRY
WHERE 1=1

#if (${period} == 'M') 
AND	TGT_MVMT_DT	BETWEEN  TO_CHAR(TO_DATE(@[from_dt],'YYYY-MM'),'YYYYMMDD')	AND TO_CHAR(LAST_DAY(TO_DATE(@[to_dt],'YYYY-MM')),'YYYYMMDD')
#else 
AND	TGT_YRWK	BETWEEN  REPLACE(@[from_dt],'-','')	AND REPLACE(@[to_dt],'-','')
#end


#if (${location} != '')
	AND     VL_LOC_CD   LIKE   @[location]||'%'
#end



#if (${p_lane} != '')
AND     SLAN_CD     IN  (@[p_lane])
#end
			]]></sql>
			<params>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="location" type="12" value="" out="N"/>
				<param name="p_lane" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
