<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostSetUpDBDAOSearchInterfaceDataRSQL">
			<desc><![CDATA[A/P에서 I/F된 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT WP.COST_YR, AP.RMONTH AS REV_MONTH, WP.COST_WK,
       NVL(SUM(DECODE(AP.MGA_ACCOUNT,'51102000',ACTUAL_USD_AMT)),0) AS MT_STEVE,
       NVL(SUM(DECODE(AP.MGA_ACCOUNT,'51302000',ACTUAL_USD_AMT)),0) AS MT_TRANS,
       NVL(SUM(ACTUAL_USD_AMT),0) AS TTL_AMT
  FROM MAS_HJSAP_INVOICE_AMT_IF AP,
       MAS_WK_PRD               WP
 WHERE WP.COST_YR         = SUBSTR(AP.RMONTH,1,4)
   AND AP.RMONTH = REPLACE(@[f_yearweek],'-','')
   AND AP.INTERFACE_DATE  BETWEEN WP.SLS_FM_DT AND WP.SLS_TO_DT
 GROUP BY WP.COST_YR, AP.RMONTH, WP.COST_WK
 ORDER BY WP.COST_YR, WP.COST_WK, AP.RMONTH			]]></sql>
			<params>
				<param name="f_yearweek" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
