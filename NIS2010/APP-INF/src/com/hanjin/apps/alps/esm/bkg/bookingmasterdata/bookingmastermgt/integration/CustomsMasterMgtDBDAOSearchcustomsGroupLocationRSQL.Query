<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsMasterMgtDBDAOSearchcustomsGroupLocationRSQL">
			<desc><![CDATA[ 0354 Canada ACI: Location of Goods Setup - Loc Code 조회			]]></desc>
			<sql><![CDATA[
SELECT  GDS_LOC_SEQ
	  , POD_CD
      , POD_YD_NO
      , (SELECT YD_NM FROM MDM_YARD WHERE YD_CD = A.POD_CD||POD_YD_NO ) YD_DESC
      , POD_CD POD_CD_CPY
      , DEL_CD
	  , HUB_LOC_CD
	  , TRSP_MOD_ID
	  , GDS_DESC
	  , CSTMS_CD
	  , ( SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = A.POD_CD ) POD_DESC
	  , ( SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = A.DEL_CD ) DEL_DESC
	  , ( SELECT LOC_NM FROM MDM_LOCATION WHERE LOC_CD = A.HUB_LOC_CD ) HUB_DESC
	  , '' UPD_USR_ID
	  , '' P_POD_CD
	  , '' P_DEL_CD
	  , '' P_HUB_LOC_CD
FROM   BKG_CSTMS_CND_GDS_LOC A /* INBOND_LOC_GOODS */
WHERE POD_CD     = @[p_pod_cd]
AND   DEL_CD     LIKE '%'||@[p_del_cd]||'%'

#if (${p_hub_loc_cd} != '') 
	AND   HUB_LOC_CD LIKE '%'||@[p_hub_loc_cd]||'%'
#end			]]></sql>
			<params>
				<param name="p_pod_cd" type="12" value="" out="N"/>
				<param name="p_del_cd" type="12" value="" out="N"/>
				<param name="p_hub_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
