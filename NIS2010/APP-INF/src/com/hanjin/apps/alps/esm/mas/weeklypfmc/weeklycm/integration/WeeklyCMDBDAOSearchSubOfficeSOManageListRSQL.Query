<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOSearchSubOfficeSOManageListRSQL">
			<desc><![CDATA[Office의 Sub Office 조회]]></desc>
			<sql><![CDATA[
SELECT OFC_CD AS F_CTRL_OFC_CD
FROM MDM_ORGANIZATION
WHERE DELT_FLG = 'N'
CONNECT BY NOCYCLE PRIOR OFC_CD = PRNT_OFC_CD
START WITH OFC_CD = @[f_ctrl_ofc_cd] 			]]></sql>
			<params>
				<param name="f_ctrl_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
