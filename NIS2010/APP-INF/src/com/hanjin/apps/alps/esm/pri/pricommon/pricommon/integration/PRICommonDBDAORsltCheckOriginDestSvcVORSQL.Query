<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAORsltCheckOriginDestSvcVORSQL">
			<desc><![CDATA[Region Code, Contry Code가 MDM_SVC_SCP_LMT 에 Origin, Dest에 맞춰서 존재 하는지 확인한다.]]></desc>
			<sql><![CDATA[
SELECT RGN_CD
FROM MDM_REGION LOC
WHERE 
#if( ${etc1} == 'C' )
	CNT_CD = @[cd]
#else
	RGN_CD = @[cd]
#end
AND LOC.DELT_FLG = 'N'
AND EXISTS (
    SELECT 'F' FROM MDM_SVC_SCP_LMT LMT
    WHERE SVC_SCP_CD = @[svc_scp_cd]
        AND ORG_DEST_CD =  @[etc2]
        AND LMT.RGN_CD = LOC.RGN_CD
		AND LMT.DELT_FLG = 'N'
		AND ROWNUM = 1
)
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
