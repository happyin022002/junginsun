<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMSummaryDBDAORsltPriCMSummaryCustomerSelectedListVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.CUST_CNT_CD||LPAD(A.CUST_SEQ, 6, '0') AS CUST_CD,OFC_CD,
  A.CUST_CNT_CD,
  A.CUST_SEQ,
  A.CUST_LGL_ENG_NM
FROM MDM_CUSTOMER A
WHERE A.DELT_FLG = 'N'
AND (A.CUST_CNT_CD , A.CUST_SEQ) IN (
#foreach( ${key} in ${cust_list}) 
	#if($velocityCount != 1 ) 
		UNION ALL
	#end
	SELECT 
			substr('$key',1,2),substr('$key',3)
	FROM DUAL
#end
)
AND A.NMD_CUST_FLG = 'N'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
