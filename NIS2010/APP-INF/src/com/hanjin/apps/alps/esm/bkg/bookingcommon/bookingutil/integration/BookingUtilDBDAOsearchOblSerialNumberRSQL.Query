<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOsearchOblSerialNumberRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
	AUTH.OBL_INTER_SER_NO,
	AUTH.OBL_INTER_SER_NO || '|' || AUTH.OBL_INTER_SER_NO_CHK_USR_ID || '|' || TO_CHAR(AUTH.OBL_INTER_SER_NO_CHK_DT, 'YYYY-MM-DD') AS OBL_INTER_SER_NO_CHK_USR_ID
#if (${search_type} != '') 
	,AUTH.BKG_NO
	,AUTH.OBL_INTER_SER_NO_CHK_USR_ID
#end
FROM BKG_INET_BL_PRN_AUTH AUTH, BKG_BOOKING BKG
WHERE AUTH.BKG_NO = @[bkg_no]
AND AUTH.BKG_NO = BKG.BKG_NO
AND AUTH.DELT_FLG = 'N'
AND NVL(BKG.BL_TP_CD, 'B') <> 'W'
AND AUTH.OBL_INTER_SER_NO IS NOT NULL
#if (${checked} == 'Y') 
AND AUTH.OBL_INTER_SER_NO_CHK_USR_ID IS NOT NULL
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
