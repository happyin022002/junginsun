<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOSearchCngItemFlagRSQL">
			<desc><![CDATA[BDRCorrectionDBDAOSearchCngItemFlagRSQL]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_CLASS(@[bkg_no], 'TMP0000001'), 1,1) RAT_FLG
     , SUBSTR(BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_CLASS(@[bkg_no], 'TMP0000001'), 2,1) EXPN_FLG
     , '' DOC_PERF_EXPT_CD
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_A(@[bkg_no], 'TMP0000001') RT_CORR_FLG     
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_B(@[bkg_no], 'TMP0000001') CHG_TERM_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_C(@[bkg_no], 'TMP0000001') RCVDE_TERM_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_D(@[bkg_no], 'TMP0000001') ROUT_CORR_FLG   
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_E(@[bkg_no], 'TMP0000001') CUST_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_F(@[bkg_no], 'TMP0000001') QTY_CORR_FLG 
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_G(@[bkg_no], 'TMP0000001') MEAS_QTY_CORR_FLG   
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_H(@[bkg_no], 'TMP0000001') CMDT_CORR_FLG     
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_I(@[bkg_no], 'TMP0000001') TRNK_VSL_CORR_FLG
     , BKG_CORR_KIND_PKG.BKG_CORR_KIND_PKG_J(@[bkg_no], 'TMP0000001') PRPST_VSL_CORR_FLG
 FROM BKG_CORRECTION
WHERE BKG_NO  = @[bkg_no]
  AND CORR_NO = 'TMP0000001'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
