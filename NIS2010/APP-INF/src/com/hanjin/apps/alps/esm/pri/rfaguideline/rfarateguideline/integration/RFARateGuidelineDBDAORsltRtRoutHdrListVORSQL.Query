<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateGuidelineDBDAORsltRtRoutHdrListVORSQL">
			<desc><![CDATA[Rate - Route조회]]></desc>
			<sql><![CDATA[
SELECT A.SVC_SCP_CD
      ,A.GLINE_SEQ
      ,A.CMDT_HDR_SEQ
      ,A.ROUT_SEQ
      ,A.DIR_CALL_FLG
      ,B.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD
      ,C.ROUT_VIA_PORT_DEF_CD AS ORG_ROUT_VIA_PORT_DEF_CD
      ,D.ROUT_VIA_PORT_DEF_CD AS DEST_ROUT_VIA_PORT_DEF_CD
      ,E.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD
      ,A.CRE_USR_ID
      ,A.CRE_DT
      ,A.UPD_USR_ID
      ,A.UPD_DT
  FROM PRI_RG_RT_CMDT_ROUT A
      ,(SELECT SVC_SCP_CD
              ,GLINE_SEQ
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD, ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
          FROM (SELECT SVC_SCP_CD
                      ,GLINE_SEQ
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_DEF_CD
                      ,ROUT_PNT_LOC_TP_CD
                      ,RCV_DE_TERM_CD
                      ,ROW_NUMBER() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                  FROM PRI_RG_RT_ROUT_PNT
                 WHERE SVC_SCP_CD = @[svc_scp_cd]
                   AND GLINE_SEQ = @[gline_seq]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'O')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY SVC_SCP_CD
                 ,GLINE_SEQ
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) B
      ,(SELECT SVC_SCP_CD
              ,GLINE_SEQ
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
          FROM (SELECT SVC_SCP_CD
                      ,GLINE_SEQ
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_DEF_CD
                      ,ROUT_VIA_PORT_TP_CD
                      ,ROW_NUMBER() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                  FROM PRI_RG_RT_ROUT_VIA
                 WHERE SVC_SCP_CD = @[svc_scp_cd]
                   AND GLINE_SEQ = @[gline_seq]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'O')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY SVC_SCP_CD
                 ,GLINE_SEQ
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) C
      ,(SELECT SVC_SCP_CD
              ,GLINE_SEQ
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
          FROM (SELECT SVC_SCP_CD
                      ,GLINE_SEQ
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_DEF_CD
                      ,ROUT_VIA_PORT_TP_CD
                      ,ROW_NUMBER() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                  FROM PRI_RG_RT_ROUT_VIA
                 WHERE SVC_SCP_CD = @[svc_scp_cd]
                   AND GLINE_SEQ = @[gline_seq]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'D')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY SVC_SCP_CD
                 ,GLINE_SEQ
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) D
      ,(SELECT SVC_SCP_CD
              ,GLINE_SEQ
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD, ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
          FROM (SELECT SVC_SCP_CD
                      ,GLINE_SEQ
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_DEF_CD
                      ,ROUT_PNT_LOC_TP_CD
                      ,RCV_DE_TERM_CD
                      ,ROW_NUMBER() OVER(PARTITION BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY SVC_SCP_CD, GLINE_SEQ, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                  FROM PRI_RG_RT_ROUT_PNT
                 WHERE SVC_SCP_CD = @[svc_scp_cd]
                   AND GLINE_SEQ = @[gline_seq]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'D')
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY SVC_SCP_CD
                 ,GLINE_SEQ
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) E
 WHERE A.SVC_SCP_CD = B.SVC_SCP_CD(+)
   AND A.GLINE_SEQ = B.GLINE_SEQ(+)
   AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = B.ROUT_SEQ(+)
   AND A.SVC_SCP_CD = C.SVC_SCP_CD(+)
   AND A.GLINE_SEQ = C.GLINE_SEQ(+)
   AND A.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = C.ROUT_SEQ(+)
   AND A.SVC_SCP_CD = D.SVC_SCP_CD(+)
   AND A.GLINE_SEQ = D.GLINE_SEQ(+)
   AND A.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = D.ROUT_SEQ(+)
   AND A.SVC_SCP_CD = E.SVC_SCP_CD(+)
   AND A.GLINE_SEQ = E.GLINE_SEQ(+)
   AND A.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = E.ROUT_SEQ(+)
   AND A.SVC_SCP_CD = @[svc_scp_cd]
   AND A.GLINE_SEQ = @[gline_seq]
   AND A.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
 ORDER BY B.SORT_KEY
         ,E.SORT_KEY
         ,C.SORT_KEY
         ,D.SORT_KEY
         ,A.DIR_CALL_FLG DESC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="ACE" out="N"/>
				<param name="gline_seq" type="2" value="1" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
