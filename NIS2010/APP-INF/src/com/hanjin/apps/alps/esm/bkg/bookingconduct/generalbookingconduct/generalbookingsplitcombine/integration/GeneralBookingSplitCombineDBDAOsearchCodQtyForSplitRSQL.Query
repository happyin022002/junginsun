<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSplitCombineDBDAOsearchCodQtyForSplitRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
select qty.cntr_tpsz_cd
        , (op_cntr_qty - cod_qty) bkg_qty  
        , cod_qty                          
  from bkg_quantity qty
        , (select cntr_tpsz_cd, count(1) cod_qty
          from (select (select cntr_tpsz_cd from mst_container mst where mst.cntr_no = cod.cntr_no) cntr_tpsz_cd        
                  from bkg_cod_cntr cod
                 where bkg_no = @[bkg_no]
                   and cod_rqst_seq = 1)
         group by cntr_tpsz_cd) cod
 where qty.cntr_tpsz_cd = cod.cntr_tpsz_cd
   and qty.bkg_no = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
