<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchBkgStaffEmlRSQL">
			<desc><![CDATA[GeneralBookingReceiptDBSearchBkgStaffEmlRSQL]]></desc>
			<sql><![CDATA[
SELECT (BKG_JOIN_FNC(CURSOR(SELECT DISTINCT EML 
                              FROM (
                              
                                        SELECT CTRT_SREP_CD CD
                                             , MDM.SREP_EML EML
                                          FROM BKG_BOOKING  BK
                                             , MDM_SLS_REP  MDM
                                         WHERE BK.BKG_NO = @[bkg_no]
                                           AND BK.CTRT_SREP_CD = MDM.SREP_CD
                                        
                                        UNION ALL
                                        SELECT OB_SREP_CD   CD
                                             , MDM.SREP_EML EML
                                          FROM BKG_BOOKING  BK
                                             , MDM_SLS_REP  MDM
                                         WHERE BK.BKG_NO = @[bkg_no]
                                           AND BK.OB_SREP_CD = MDM.SREP_CD

                                  )
                             WHERE EML IS NOT NULL
                            ),';')
        ) EML
  FROM DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
