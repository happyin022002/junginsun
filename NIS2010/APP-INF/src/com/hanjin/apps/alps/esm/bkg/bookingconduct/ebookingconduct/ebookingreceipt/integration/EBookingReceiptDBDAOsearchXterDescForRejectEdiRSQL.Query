<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterDescForRejectEdiRSQL">
			<desc><![CDATA[searchXterDescForRejectEdi]]></desc>
			<sql><![CDATA[
SELECT DECODE(SUBSTR(XTER_SNDR_ID, 1, 4), 'INTT', REPLACE(GDS_DESC,CHR(13)||CHR(10),CHR(10)||'DESC:'), 'TRAD', REPLACE(GDS_DESC,CHR(13)||CHR(10),CHR(10)||'DESC:'), NULL) BL_DESC
  FROM BKG_XTER_RQST_MST
 WHERE XTER_SNDR_ID = @[sender_id]
   AND XTER_RQST_NO = @[rqst_no]
   AND XTER_RQST_SEQ= @[rqst_seq]			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
