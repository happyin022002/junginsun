<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOCntrCmBkgInfoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT A.BKG_NO
,      A.BL_NO
,      A.BL_TP_CD
,      A.BKG_STS_CD
,      A.BKG_CGO_TP_CD
,      A.VSL_CD
,      A.SKD_VOY_NO
,      A.SKD_DIR_CD
,      A.RCV_TERM_CD
,      A.DE_TERM_CD
,      A.POR_CD
,      A.POL_CD
,      A.POD_CD
,      A.DEL_CD
,      A.CMDT_CD
,      A.REP_CMDT_CD
,      (SELECT CMDT_NM FROM MDM_COMMODITY WHERE CMDT_CD=A.CMDT_CD) CMDT_NM
,      (SELECT BDR_FLG FROM BKG_BL_DOC WHERE BKG_NO=A.BKG_NO) BDR_FLG
,      (SELECT OBL_ISS_FLG FROM BKG_BL_ISS WHERE BKG_NO=A.BKG_NO) OBL_ISS_FLG
,      B.CNTR_NO
,      B.PCK_QTY
,      B.PCK_TP_CD
,      B.CNTR_WGT
,      B.WGT_UT_CD
,      B.MEAS_QTY
,      B.MEAS_UT_CD
,      B.CNTR_VOL_QTY 
,      B.RCV_TERM_CD
,      B.DE_TERM_CD
,      B.DCGO_FLG
,      B.AWK_CGO_FLG
,      B.HNGR_FLG
,      B.MF_CFM_FLG
,      B.CNTR_DP_SEQ
,      B.CRE_USR_ID
,      B.UPD_USR_ID
FROM   BKG_BOOKING A, BKG_CONTAINER B
WHERE  A.BKG_NO = B.BKG_NO
AND    B.CNTR_NO = @[cntr_no]
AND    A.VSL_CD || A.SKD_VOY_NO || A.SKD_DIR_CD = @[vvd]
AND    A.BKG_STS_CD <> 'X'
ORDER BY A.BKG_NO, B.CNTR_DP_SEQ			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
