<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupCommodityQuotationDBDAOPriRqGrpCmdtDtlVOAddCopyToQuotationCSQL">
			<desc><![CDATA[AddCopyToQuotation]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RQ_GRP_CMDT_DTL (
	QTTN_NO
,	QTTN_VER_NO
,	GRP_CMDT_SEQ
,	GRP_CMDT_DTL_SEQ
,	PRC_CMDT_TP_CD
,	PRC_CMDT_DEF_CD
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT  @[qttn_no]
,	@[qttn_ver_no]
,	GRP_CMDT_SEQ
,	GRP_CMDT_DTL_SEQ
,	PRC_CMDT_TP_CD
,	PRC_CMDT_DEF_CD
,	SRC_INFO_CD
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM    PRI_RQ_GRP_CMDT_DTL
WHERE   1=1
AND    QTTN_NO = @[qttn_no_from]
AND    QTTN_VER_NO = @[qttn_ver_no_from]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="qttn_no_from" type="12" value="" out="N"/>
				<param name="qttn_ver_no_from" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
