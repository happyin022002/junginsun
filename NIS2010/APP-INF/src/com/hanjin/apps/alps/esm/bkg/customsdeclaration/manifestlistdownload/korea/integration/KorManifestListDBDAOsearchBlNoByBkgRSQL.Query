<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchBlNoByBkgRSQL">
			<desc><![CDATA[BKG_NO 로 B/L NO 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(BL_NO, ' ') BL_NO
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
