<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOBkgEdiGrpDSQL">
			<desc><![CDATA[BkgEdiGrp]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_EDI_GRP
      WHERE ESVC_GRP_CD = @[esvc_grp_cd]
        AND CO_CD = SUBSTRB(@[co_cd],1,1)
			]]></sql>
			<params>
				<param name="esvc_grp_cd" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
