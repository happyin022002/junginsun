<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOAverageUCVesselUtCopyCSQL">
			<desc><![CDATA[AverageUCVesselUtCopy]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_POOL_UT_COST (
  COST_YRMON
, STND_COST_CD
, TRD_CD
, SUB_TRD_CD
, RLANE_CD
, DIR_CD
, HUL_BND_CD
, EFF_FM_YRMON
, EFF_TO_YRMON
, TTL_AMT
, TGT_LOD_QTY
, TEU_UC_AMT
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
)
SELECT @[f_tar_mon] AS COST_YRMON
, STND_COST_CD
, TRD_CD
, SUB_TRD_CD
, RLANE_CD
, DIR_CD
, HUL_BND_CD
, EFF_FM_YRMON
, EFF_TO_YRMON
, TTL_AMT
, TGT_LOD_QTY
, TEU_UC_AMT
, @[user_id] CRE_USR_ID
, SYSDATE CRE_DT
, @[user_id] UPD_USR_ID
, SYSDATE UPD_DT
  FROM MAS_POOL_UT_COST
 WHERE COST_YRMON   = @[f_src_mon]
   AND STND_COST_CD = @[f_cobcost]			]]></sql>
			<params>
				<param name="f_tar_mon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_src_mon" type="12" value="" out="N"/>
				<param name="f_cobcost" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
