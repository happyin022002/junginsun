<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrNoticeDBDAOsearchPodListByVVDRSQL">
			<desc><![CDATA[searchPodListByVVD]]></desc>
			<sql><![CDATA[
SELECT DISTINCT POD_CD AS POD_CD
  FROM BKG_VVD BVVD
 WHERE 1=1
  AND BVVD.VSL_CD     = SUBSTR(@[vvd],1,4) 
  AND BVVD.SKD_VOY_NO = SUBSTR(@[vvd],5,4) 
  AND BVVD.SKD_DIR_CD = SUBSTR(@[vvd],9,1) 			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
