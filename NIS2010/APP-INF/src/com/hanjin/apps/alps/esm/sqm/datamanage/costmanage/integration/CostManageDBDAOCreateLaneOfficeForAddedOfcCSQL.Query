<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostManageDBDAOCreateLaneOfficeForAddedOfcCSQL">
			<desc><![CDATA[추가된 Office에 대해서 Lane-Office정보를 생성한다.

* History 
2014.08.19 이혜민 [CHM-201431600] Office가 하나도 생성되지 않은 Lane에는 넣지 않고. Office가 이미 생성된 Lane에만 insert.]]></desc>
			<sql><![CDATA[
INSERT INTO SQM_QTA_LANE_OFC (
     BSE_TP_CD
    ,BSE_YR
    ,BSE_QTR_CD
    ,OFC_VW_CD
    ,TRD_CD
    ,SUB_TRD_CD
    ,RLANE_CD
    ,DIR_CD
    ,RGN_OFC_CD
    ,RHQ_CD
    ,SQM_ACT_FLG
    ,ADD_FLG
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT   
	)
SELECT 
       A1.BSE_TP_CD
      ,A1.BSE_YR
      ,A1.BSE_QTR_CD
      ,A1.OFC_VW_CD
      ,A2.TRD_CD
      ,A2.SUB_TRD_CD
      ,A2.RLANE_CD
      ,A2.DIR_CD
      ,A3.RGN_OFC_CD
      ,A3.RHQ_CD
      ,'N'            SQM_ACT_FLG
      ,'N'            ADD_FLG
      ,@[cre_usr_id]  CRE_USR_ID
      ,SYSDATE        CRE_DT
      ,@[upd_usr_id]  UPD_USR_ID
      ,SYSDATE        UPD_DT
  FROM SQM_DAT_RLT A1
      ,(
          SELECT DISTINCT
                 TRD_CD
                ,RLANE_CD
                ,SUB_TRD_CD
                ,NVL(LANE_DIR_CD, DECODE(CPY_NO, 0, 'E', 'W')) AS DIR_CD
                ,SQM_ACT_FLG
            FROM SQM_QTA_LANE_MGMT
                ,COM_CPY_NO
           WHERE SQM_ACT_FLG = 'Y'
             AND CPY_NO      < 2
        ) A2
      ,SQM_QTA_OFC A3
      ,SQM_DIR_CONV A4
 WHERE 1=1
   AND A1.BSE_TP_CD   = @[bse_tp_cd]
   AND A1.BSE_YR      = @[bse_yr]
   AND A1.BSE_QTR_CD  = DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
   AND A1.TRD_CD      = A2.TRD_CD
   AND A1.RLANE_CD    = DECODE(UPPER(A1.RLANE_CD), 'ALL',A1.RLANE_CD, A2.RLANE_CD)
   AND A1.CONV_DIR_CD = NVL(A4.CONV_DIR_CD, A2.DIR_CD)
   AND A1.RHQ_CD      = A3.RHQ_CD
   AND A4.BSE_TP_CD(+) = @[bse_tp_cd]
   AND A4.BSE_YR(+)    = @[bse_yr]
   AND A4.BSE_QTR_CD(+)= DECODE(@[bse_tp_cd],'Y','00',@[bse_qtr_cd])
   AND A4.TRD_CD(+)    = A2.TRD_CD
   AND A4.RLANE_CD(+)  = A2.RLANE_CD
   AND A4.DIR_CD(+)    = A2.DIR_CD
   AND A3.RGN_OFC_CD  = @[rgn_ofc_cd]
   AND EXISTS (SELECT * 
               FROM SQM_QTA_LANE_OFC B1
               WHERE A1.BSE_TP_CD    = B1.BSE_TP_CD   
                AND  A1.BSE_YR       = B1.BSE_YR         
                AND  A1.BSE_QTR_CD   = B1.BSE_QTR_CD     
                AND  A1.OFC_VW_CD    = B1.OFC_VW_CD      
                AND  A2.TRD_CD       = B1.TRD_CD         
                AND  A2.SUB_TRD_CD   = B1.SUB_TRD_CD     
                AND  A2.RLANE_CD     = B1.RLANE_CD       
                AND  A2.DIR_CD       = B1.DIR_CD      
               )
 ORDER BY A1.OFC_VW_CD, A2.TRD_CD, A2.RLANE_CD, A1.CONV_DIR_CD, A1.RHQ_CD, A3.RGN_OFC_CD			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
