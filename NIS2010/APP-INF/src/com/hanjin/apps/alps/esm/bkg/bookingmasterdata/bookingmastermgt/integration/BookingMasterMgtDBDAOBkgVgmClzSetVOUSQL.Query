<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOBkgVgmClzSetVOUSQL">
			<desc><![CDATA[VGM CCT 업데이트를 위한 쿼리]]></desc>
			<sql><![CDATA[
UPDATE BKG_VRFD_WGT_CLZ_SET SET 
	VGM_CLZ_TP_CD = @[vgm_clz_tp_cd]
,	VGM_ITVAL_HRS = @[vgm_itval_hrs]
,	VGM_CLZ_DY_CD = @[vgm_clz_dy_cd]
,	VGM_CLZ_DY_HRS = to_number(substr(@[vgm_clz_dy_hrs],1,2)) * 60 + to_number(substr(@[vgm_clz_dy_hrs],3,2))
,	XCLD_HOL_FLG = @[xcld_hol_flg]
,	DELT_FLG = 'N'
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	VSL_SLAN_CD = @[vsl_slan_cd]
AND	YD_CD = DECODE(@[yd_cd],'ALL','*',@[yd_cd])
AND LOC_CD = @[loc_cd]
AND	DEST_CNT_CD = @[dest_cnt_cd]
AND CONTI_CD = @[conti_cd]
AND	VVD_CD = DECODE(@[vvd_cd],'ALL','*',@[vvd_cd])  			]]></sql>
			<params>
				<param name="vgm_clz_tp_cd" type="12" value="" out="N"/>
				<param name="vgm_itval_hrs" type="12" value="" out="N"/>
				<param name="vgm_clz_dy_cd" type="12" value="" out="N"/>
				<param name="vgm_clz_dy_hrs" type="12" value="" out="N"/>
				<param name="xcld_hol_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="dest_cnt_cd" type="12" value="" out="N"/>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
