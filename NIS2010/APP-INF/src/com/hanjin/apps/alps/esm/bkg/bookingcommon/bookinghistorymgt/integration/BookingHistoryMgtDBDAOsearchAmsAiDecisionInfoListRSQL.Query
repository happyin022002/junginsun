<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOsearchAmsAiDecisionInfoListRSQL">
			<desc><![CDATA[미세관 AI 전송 FLAG 판단 정보 조회]]></desc>
			<sql><![CDATA[
-- IMPORT ADVANCED
SELECT B.MF_SND_DT
      ,DECODE(B.CNT_CD, 'US', USA_CSTMS_FILE_CD, CND_CSTMS_FILE_CD) AS CSTMS_FILE_TP_CD
      ,DECODE(B.CNT_CD, 'US', 'AI_SND', 'CA_SND') AS BKG_DOC_PROC_TP_CD
      ,B.CNT_CD
      ,B.MF_NO
      ,B.BL_NO
  FROM BKG_CSTMS_ADV_BL B
      ,BKG_BOOKING      A
 WHERE B.CNT_CD     IN ('US', 'CA')
   AND B.BKG_NO     = A.BKG_NO
   AND B.BKG_NO     = @[bkg_no]
   AND (B.MF_SND_DT IS NOT NULL OR B.AMDT_SND_DT IS NOT NULL)

