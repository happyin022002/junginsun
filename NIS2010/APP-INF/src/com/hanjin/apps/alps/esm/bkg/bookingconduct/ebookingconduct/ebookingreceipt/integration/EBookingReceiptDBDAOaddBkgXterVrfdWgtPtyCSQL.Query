<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOaddBkgXterVrfdWgtPtyCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_XTER_VRFD_WGT_PTY ( cntr_no, XTER_SNDR_ID, XTER_VGM_RQST_NO, XTER_VGM_SEQ, VGM_PTY_TP_CD, VGM_PTY_ID, AUTH_PSON_NM, VGM_PTY_ADDR, VGM_PTY_EML, VGM_PTY_CNTC_PHN_NO, VGM_PTY_FAX_NO, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT )
VALUES ( UPPER(@[cntr_no]),
               @[xter_sndr_id],
               @[xter_vgm_rqst_no],
               (select nvl(max(xter_vgm_seq), 1) from BKG_XTER_VRFD_WGT_RQST where xter_sndr_id = @[xter_sndr_id] and xter_vgm_rqst_no = @[xter_vgm_rqst_no] and cntr_no = UPPER(@[cntr_no])),
               @[vgm_pty_tp_cd],
               @[vgm_pty_id],
               upper(@[auth_pson_nm]),
               @[vgm_pty_addr],
               @[vgm_pty_eml],
               @[vgm_pty_cntc_phn_no],
               @[vgm_pty_fax_no],
               @[cre_usr_id],
               sysdate,
               @[upd_usr_id],
               sysdate )			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_vgm_rqst_no" type="12" value="" out="N"/>
				<param name="vgm_pty_tp_cd" type="12" value="" out="N"/>
				<param name="vgm_pty_id" type="12" value="" out="N"/>
				<param name="auth_pson_nm" type="12" value="" out="N"/>
				<param name="vgm_pty_addr" type="12" value="" out="N"/>
				<param name="vgm_pty_eml" type="12" value="" out="N"/>
				<param name="vgm_pty_cntc_phn_no" type="12" value="" out="N"/>
				<param name="vgm_pty_fax_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
