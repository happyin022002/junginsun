<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOSearchAttachmentListRSQL">
			<desc><![CDATA[RDN 증빙 자료(첨부파일)를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT R.RDN_NO, 
       R.RVIS_SEQ, 
       R.ATCH_FILE_LNK_ID,
       F.ATCH_FILE_LNK_SEQ,
       C.FILE_SAV_ID,
       C.FILE_UPLD_NM,
       C.FILE_SZ_CAPA,
       C.FILE_PATH_URL,
       '' CRE_USR_ID,
       '' UPD_USR_ID
FROM BKG_REV_DR_NOTE R, BKG_ATCH_FILE F, COM_UPLD_FILE C
WHERE R.RDN_NO = @[rdn_no]
AND R.RVIS_SEQ = @[rvis_seq]
AND R.ATCH_FILE_LNK_ID = F.ATCH_FILE_LNK_ID
AND F.FILE_SAV_ID = C.FILE_SAV_ID			]]></sql>
			<params>
				<param name="rdn_no" type="12" value="" out="N"/>
				<param name="rvis_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
