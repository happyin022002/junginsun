<sqls>
	<querys>
		<query name="StatusReportDBDAOSearchBLStatusList1RSQL">
			<desc><![CDATA[StatusReportDBDAOSearchBLStatusList1RSQL
* History
2012.03.26 김보배 [CHM-201216886] [BKG] [Booking Status Report] Vessel Name 아이템 조회결과 제한 제거
2012.04.06 김보배 [CHM-201217102] [BKG] [Booking Status Report] Item option 추가
2014.07.15 신선희 [CHM-201430931] [BKG] [Booking Status Report] FUMG_LOC_CD, SPCL_HIDE_LNR_FLG 컬럼추가
2014.09.19 신선희 [CHM-201432022] [BKG] [Booking Status Report] SI_CNTC_PSON_EML,SI_CNTC_PSON_PHN_NO 추가
2014.11.03 신선희 SPOT GUIDE RFA 조회조건 추가, ETB 출력 추가
2014.12.15 신선희 VEHICLE 조회조건 추가
2014.12.30 신선희 CTRT_CNG_TP_CD, PRE_SC_NO, PRE_RFA_NO, PRE_TAA_NO 추가
2015.01.20 신선희 RTRO_KND_CD 조회조건, 출력 추가
2015.06.22 COA_MON_VVD ==>MAS_MON_VVD,  COA_RGST_BKG ==>MAS_RGST_BKG 변경]]></desc>
			<sql><![CDATA[
SELECT QQ.*, KK.RTRO_KND_CD_NM
FROM (
    SELECT
          ROWNUM RNUM
        , ${orderby_title_sql} ORDERBY_TITLE
        , TRIM(TO_CHAR(SUM(TEU) OVER() ,'999999990.99')) TOTAL_TEU
        , TRIM(TO_CHAR(SUM(FEU) OVER() ,'999999990.99')) TOTAL_FEU
        , TRIM(TO_CHAR(ROUND(SUM(BKG_ACTWGT_QTY) OVER() /1000) ,'999,999,999'))  TOTAL_WGT
		, TRIM(TO_CHAR(SUM(BKG_MEA_QTY) OVER() ,'999,999,990.999')) TOTAL_MEA
        ,ZZ.*
    FROM (
            SELECT
              COUNT(1) OVER() TOTAL_CNT
    	    , COUNT(DISTINCT BKG_NO) OVER() TOTAL_BKG
	        , COUNT(DISTINCT BL_NO) OVER() TOTAL_BL
            , COUNT(DISTINCT ${orderby_select} ) OVER() ORDERBY_CNT
            , @[last_orderby] LAST_ORDERBY
            , @[orderby] ORDERBY
            , '' ROWS_PER_PAGE
            , '' CURR_PAGE
            , Z.*
            /*Z Alias*/
            FROM (
                    SELECT Y.* ,COST_WK
                    /*Y Alias*/
                    FROM (
                            SELECT
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'D2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_D2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'D3',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_D3,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'D4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_D4,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'D5',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_D5,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'D7',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_D7,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'D9',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_D9,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'DX',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_DX,

                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'R2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_R2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'R4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_R4,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'R5',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_R5,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'R9',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_R9,

                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'F2',NVL(OP_CNTR_QTY,0),'A2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_F2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'F4',NVL(OP_CNTR_QTY,0),'A4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_F4,
                                    --( SELECT SUM(DECODE(CNTR_TPSZ_CD,'F5',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_F5,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'F5',NVL(OP_CNTR_QTY,0),'A5',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_F5,

                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'T2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_T2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'T4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_T4,

                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'P2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_P2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'P4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_P4,

                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'O2',NVL(OP_CNTR_QTY,0),'S2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_O2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'O4',NVL(OP_CNTR_QTY,0),'S4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_O4,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'O5',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_O5,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'O7',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_O7,

                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'Z2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_Z2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'Z4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_Z4,

                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'Q2',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_Q2,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'Q4',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_Q4,
                                    ( SELECT SUM(DECODE(CNTR_TPSZ_CD,'Q5',NVL(OP_CNTR_QTY,0),0)) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) RD_TOTAL_Q5,

                                    ( SELECT TRIM(TO_CHAR(SUM(DECODE(GREATEST('2', SUBSTR(CNTR_TPSZ_CD, 2, 1)), '2', NVL(OP_CNTR_QTY, 0), 0)) , '999999990.99')) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) TEU,
                                    ( SELECT TRIM(TO_CHAR(SUM(DECODE(GREATEST('2', SUBSTR(CNTR_TPSZ_CD, 2, 1)), '2', 0, NVL(OP_CNTR_QTY, 0))) , '999999990.99')) FROM BKG_QUANTITY WHERE BKG_NO = X.BKG_NO ) FEU,
                                    ( SELECT TRIM(TO_CHAR(SUM(DECODE(GREATEST('2', SUBSTR(CNTR_TPSZ_CD, 2, 1)), '2', 1, 0)) , '999999990.99')) FROM BKG_CONTAINER WHERE BKG_NO = X.BKG_NO ) ACTIVITY_TEU,
                                    ( SELECT TRIM(TO_CHAR(SUM(DECODE(GREATEST('2', SUBSTR(CNTR_TPSZ_CD, 2, 1)), '2', 0, 1)) , '999999990.99')) FROM BKG_CONTAINER WHERE BKG_NO = X.BKG_NO ) ACTIVITY_FEU

                                , ( SELECT /*+ index_desc (XPT_IMP XPKBKG_XPT_IMP_LIC) */ DECODE(XPT_IMP.AES_TP_CD,'AE',XPT_IMP.AES_INLND_TRNS_PFX_CTNT||XPT_IMP.AES_INLND_TRNS_NO,
                                                                   'PA',XPT_IMP.AES_PTA_PFX_CTNT||' '||XPT_IMP.AES_PTA_NO1||'-'||XPT_IMP.AES_PTA_NO2||' '||TO_CHAR(XPT_IMP.AES_PTA_DT,'MM/DD/YYYY'),
                                                                   'PU',XPT_IMP.AES_PTU_PFX_CTNT||' '||XPT_IMP.AES_PTU_NO||' '||TO_CHAR(XPT_IMP.AES_PTU_DT,'MM/DD/YYYY'),
                                                                   'DN',XPT_IMP.AES_DWN_PFX_CTNT||' '||XPT_IMP.AES_DWN_NO||' '||TO_CHAR(XPT_IMP.AES_DWN_DT,'MM/DD/YYYY'),
                                                                   'EX',DECODE(XPT_IMP.AES_EXPT_CTNT,'L',HARD.ATTR_CTNT7,
                                                                                                     'T',HARD.ATTR_CTNT6,
                                                                                                     'E',HARD.ATTR_CTNT5,
                                                                                                     'C',HARD.ATTR_CTNT4,
                                                                                                     'U',HARD.ATTR_CTNT3,XPT_IMP.AES_EXPT_CTNT,' '),' ')
                                     FROM BKG_XPT_IMP_LIC XPT_IMP, BKG_HRD_CDG_CTNT HARD
                                     WHERE XPT_IMP.BKG_NO    = X.BKG_NO
                                     AND   XPT_IMP.IO_BND_CD ='O'
                                     AND   XPT_IMP.CNT_CD = 'US'
                                     AND   HARD.HRD_CDG_ID   = 'AES_COLUMN_NAME'
                                     AND   ROWNUM = 1
                                   ) AS AES_NO
                                 , ( SELECT /*+ index_desc (XPT_IMP XPKBKG_XPT_IMP_LIC) */ DECODE(XPT_IMP.CAED_TP_CD,'CE', XPT_IMP.CAED_PFX_CTNT       ||XPT_IMP.CAED_NO1       ||XPT_IMP.CAED_NO2    ||XPT_IMP.CAED_NO3,
                                                                      'G7', XPT_IMP.G7_EDI_PFX_CTNT     ||XPT_IMP.G7_EDI_NO1     ||XPT_IMP.G7_EDI_NO2,
                                                                      'SM', XPT_IMP.MF_SMRY_RPT_PFX_CTNT||XPT_IMP.MF_SMRY_RPT_NO,
                                                                      'EX', XPT_IMP.B13A_XPT_PFX_CTNT   ||XPT_IMP.B13A_XPT_NO1   ||XPT_IMP.B13A_XPT_NO2 ||TO_CHAR(XPT_IMP.B13A_XPT_DT,'YYYYMMDDHHMM'),
                                                                      'IT', XPT_IMP.CGO_CTRL_PFX_CTNT   ||XPT_IMP.CGO_CTRL_NO,
                                                                      'ND', XPT_IMP.NDR_REF_PFX_CTNT    ||NDR_REF_ID,' ')
                                     FROM BKG_XPT_IMP_LIC XPT_IMP
                                     WHERE XPT_IMP.BKG_NO      = X.BKG_NO
                                     AND   XPT_IMP.IO_BND_CD   ='O'
                                     AND   XPT_IMP.CNT_CD = 'CA'
                                     AND   ROWNUM = 1
                                   ) AS CAED_NO
                                 , ( SELECT MAX(NVL(VPS.SHP_CALL_NO,' '))
                                      FROM  VSK_VSL_PORT_SKD VPS, BKG_VVD VVD
                                      WHERE 1=1
									  AND   X.BKG_NO = VVD.BKG_NO
									  AND   VVD.POL_CD LIKE 'MYPKG%'
                                      AND   VVD.VSL_CD      = VPS.VSL_CD
                                      AND   VVD.SKD_VOY_NO  = VPS.SKD_VOY_NO
                                      AND   VVD.SKD_DIR_CD  = VPS.SKD_DIR_CD
									  AND   VVD.POL_CD 		= VPS.VPS_PORT_CD
                                      AND   CLPT_IND_SEQ ='1'
                                  ) AS   SHP_CALL_NO /*CRN*/

                                 , ( SELECT VSL_ENG_NM FROM MDM_VSL_CNTR VSL, BKG_VVD VVD
                                     WHERE 1=1
									 AND X.BKG_NO = VVD.BKG_NO
									 AND X.POL_CD = VVD.POL_CD
									 AND X.VSL_CD = VVD.VSL_CD
                                     AND X.SKD_VOY_NO = VVD.SKD_VOY_NO
                                     AND X.SKD_DIR_CD = VVD.SKD_DIR_CD
									 AND VVD.VSL_CD = VSL.VSL_CD
									 AND ROWNUM = 1
                                  ) AS VSL_ENG_NM  /*Vessel_name*/
                                 , ( SELECT C.ECC_CD
                                     FROM MDM_LOCATION A, MDM_EQ_ORZ_CHT C
                                     WHERE 1 = 1
                                     AND A.SCC_CD = C.SCC_CD
                                     AND A.LOC_CD like SUBSTR(ESTM_IB_MTY_RTN_YD_CD,0,5) || '%'
                                     AND A.CNT_CD =  SUBSTR(ESTM_IB_MTY_RTN_YD_CD,0,2)
                                     AND NVL(A.DELT_FLG, 'N') <> 'Y'
                                  ) ECC_CD
                                  , X.*
                                ,CASE 
                                     WHEN (SELECT COUNT(*) FROM BKG_BL_CERTI K WHERE K.BL_NO = X.BKG_NO AND K.BL_CERTI_STS_CD IN ('I') AND K.TMPLT_FLG = 'N') = 0 AND
                                          (SELECT COUNT(*) FROM BKG_BL_CERTI K WHERE K.BL_NO = X.BL_NO  AND K.BL_CERTI_STS_CD = 'C' AND K.TMPLT_FLG = 'N') = 0 AND
                                          NVL((SELECT 'Y' FROM BKG_IMG_STO K WHERE K.BKG_NO = X.BL_NO AND K.RIDR_TP_CD = 'K' AND ROWNUM =1 ),'N') != 'Y'
                                     THEN 'N'
                                     ELSE 'Y'
                                END BL_CERTI     
 
                            /*X Alias*/
                            FROM (
                                    SELECT
												#if (${vvd_cd} == '')
													#if (${bkg_from_dt} != '')
                                           				#if (${l_ofc_cd} != '')
                                            				#if(${l_ofc_cd_sub} != '')
                                             					/*+ NO_MERGE(VB DMT_OFC_LVL_V) USE_NL(VB MDM_LOCATION XPT_IMP) OPT_PARAM('_optimizer_skip_scan_enabled','FALSE') */
                                            				#else
                                             					/*+ NO_MERGE(VB) USE_NL(VB MDM_LOCATION XPT_IMP) OPT_PARAM('_optimizer_skip_scan_enabled','FALSE') */
                                            				#end
														#else
															/*+ NO_MERGE(VB) USE_NL(VB MDM_LOCATION XPT_IMP) OPT_PARAM('_optimizer_skip_scan_enabled','FALSE') */
                                           				#end
            		                               #end
												#end
											  DISTINCT
                                              VB.BKG_NO          AS BKG_NO
                                            , VB.BL_NO||VB.BL_TP_CD AS BL_NO
                                            , VB.BKG_STS_CD      AS BKG_STS_CD
                                            , VB.BDR_FLG         AS BDR_FLG
                                            , VB.SI_FLG          AS SI_FLG
                                            , VB.BKG_OFC_CD      AS BKG_OFC_CD
                                            , VB.CTRT_OFC_CD     AS CTRT_OFC_CD
                                            , VB.OB_SLS_OFC_CD   AS OB_SLS_OFC_CD
                                            , VB.EQ_CTRL_OFC_CD  AS BKG_CTRL_OFC_CD
                                            , MDM_LOCATION.EQ_CTRL_OFC_CD AS EQ_CTRL_OFC_CD

	                                        , VB.INTER_RMK
                                            , DECODE(VB.XTER_RMK,NULL,'N','Y')  AS REMARK
											, DECODE(VB.INTER_RMK,NULL,'N','Y') AS INTER_REMARK
											, DECODE(RA.RT_INTER_RMK,NULL,'N','Y') AS CHG_INTER_REMARK
											, DECODE(RA.DIFF_RMK,NULL,'N','Y') AS CHG_XTER_REMARK
                                            , SUBSTR(REPLACE(REPLACE(REPLACE(VB.XTER_RMK, CHR(13)||CHR(10), ' '),CHR(13),' '), CHR(10), ' '),1,200)  AS REMARK_DETAIL        
											, SUBSTR(VB.INTER_RMK,1,200) AS INTER_REMARK_DETAIL
											, SUBSTR(RA.RT_INTER_RMK,1,200) AS CHG_INTER_REMARK_DETAIL
											, SUBSTR(RA.DIFF_RMK,1,200) AS CHG_XTER_REMARK_DETAIL
											, CASE WHEN VB.BKG_CRE_DT >= TO_DATE('20141125','YYYYMMDD') THEN RA.OFT_MSS_FLG 
														WHEN SUBSTR(VB.SC_NO,1,3) = 'DUM' OR SUBSTR(VB.RFA_NO,1,3) = 'DUM' OR SUBSTR(VB.TAA_NO,1,3) = 'DUM' THEN 'Y' 
														WHEN RA.OFT_MSS_FLG = 'N' THEN 'N'
														WHEN RA.OFT_MSS_FLG = 'Y' THEN NVL((SELECT 'N'
																														FROM BKG_CHG_RT RT
														  															    WHERE RT.BKG_NO = RA.BKG_NO
																														AND ROWNUM = 1), 'Y')
											  END AS RATE_CHK_STS
                                            , VB.CMDT_CD         AS COMMODITY
                                            , VB.CMDT_NM         AS CMDT_NM
                                            , VB.REP_CMDT_CD     AS REP
                                            /* TEU */
                                            , VB.RCV_TERM_CD     AS RCV_TERM_CD
                                            , VB.DE_TERM_CD      AS DE_TERM_CD
                                            , VB.DOC_USR_ID      AS DOC_USR_ID
                                            , VB.CTRT_SREP_CD    AS CTRT_SREP_CD
                                            , VB.OB_SREP_CD      AS OB_SREP_CD
        	                                , VB.FRT_TERM_CD	 AS FRT_TERM_CD
                                            , VB.SC_NO           AS SC_NO
                                            , NVL(VB.RFA_NO,' ') AS RFA_NO
    	                                    , VB.TAA_NO
                                            ,(SELECT XPT_IMP.AES_TP_CD FROM BKG_XPT_IMP_LIC XPT_IMP WHERE XPT_IMP.BKG_NO = VB.BKG_NO AND XPT_IMP.IO_BND_CD ='O' AND XPT_IMP.CNT_CD = 'US' AND ROWNUM = 1) ITN_TYPE
                                            ,(SELECT XPT_IMP.CAED_TP_CD FROM BKG_XPT_IMP_LIC XPT_IMP WHERE XPT_IMP.BKG_NO = VB.BKG_NO AND XPT_IMP.IO_BND_CD ='O' AND XPT_IMP.CNT_CD = 'CA' AND ROWNUM = 1) CAED_TYPE
                                            ,(SELECT DECODE(XPT_IMP.AES_TP_CD,NULL,'N','Y') FROM BKG_XPT_IMP_LIC XPT_IMP WHERE XPT_IMP.BKG_NO = VB.BKG_NO AND XPT_IMP.IO_BND_CD ='O' AND XPT_IMP.CNT_CD = 'US' AND ROWNUM = 1) ITN_FLG
                                            ,(SELECT DECODE(XPT_IMP.CAED_TP_CD,NULL,'N','Y') FROM BKG_XPT_IMP_LIC XPT_IMP WHERE XPT_IMP.BKG_NO = VB.BKG_NO AND XPT_IMP.IO_BND_CD ='O' AND XPT_IMP.CNT_CD = 'CA' AND ROWNUM = 1) CAED_FLG
                                         -- , XPT_IMP.AES_TP_CD	 AS ITN_TYPE
                                         -- , XPT_IMP.CAED_TP_CD AS	CAED_TYPE
                                         -- , DECODE(XPT_IMP.AES_TP_CD,NULL,'N','Y')  AS ITN_FLG
                                         -- , DECODE(XPT_IMP.CAED_TP_CD,NULL,'N','Y') AS CAED_FLG

                                            , TO_CHAR(VB.BKG_CRE_DT,'YYYY-MM-DD')  AS BKG_CRE_DT
                                            , NVL(DECODE(VB.SHIPPER,'0',' ',SUBSTR(VB.SHIPPER,1,2)||LTRIM(TO_CHAR(SUBSTR(VB.SHIPPER,3),'000000'))),' ')        AS SHIPPER
                                            , NVL(DECODE(VB.CONSIGNEE,'0',' ',SUBSTR(VB.CONSIGNEE,1,2)||LTRIM(TO_CHAR(SUBSTR(VB.CONSIGNEE,3),'000000'))),' ')  AS CONSIGNEE
                                            , NVL(DECODE(VB.NTFY,'0',' ',SUBSTR(VB.NTFY,1,2)||LTRIM(TO_CHAR(SUBSTR(VB.NTFY,3),'000000'))),' ') AS NTFY
                                            , NVL(DECODE(VB.FFDR,'0',' ',SUBSTR(VB.FFDR,1,2)||LTRIM(TO_CHAR(SUBSTR(VB.FFDR,3),'000000'))),' ') AS FFDR
    										, VB.SHIPPER   AS SHIPPER_CD
    										, VB.CONSIGNEE AS CONSIGNEE_CD
                                            , REPLACE(VB.SHPR_NAME,CHR(13)||CHR(10),' ')      AS SHPR_NAME
                                            , REPLACE(VB.CONSIGNEE_NAME,CHR(13)||CHR(10),' ') AS CNEE_NAME
                                            , VB.NTFY_NAME       AS NTFY_NAME
                                            , VB.ANTY_NAME       AS ANTY_NAME
                                            , VB.FFDR_NAME       AS FFDR_NAME
                                            , VB.EXPT_NAME       AS EXPT_NAME
    										, VB.S_ADDR /* Shipper Address   */
    										, VB.C_ADDR /* Consignee Address */
    										, VB.N_ADDR /* Notify Address    */
                                            , DECODE(VB.CUST_TO_ORD_FLG,'N',VB.CONSIGNEE_NAME,VB.NTFY_NAME) AS CUST_TO_ORD_NAME

                                            , VB.SLAN_CD         AS SLAN_CD
                                            , VB.TRUNK_VVD       AS TRUNK_VVD
                                            , VB.TRUNK_POL       AS TRUNK_POL
                                            , VB.TRUNK_POD       AS TRUNK_POD
                                            , VB.POR_CD          AS POR_CD
                                            , VB.DEL_CD          AS DEL_CD
                                            , VB.ORG_TRNS_SVC_MOD_CD    AS ORG_TRNS_SVC_MOD_CD
                                            , VB.DEST_TRNS_SVC_MOD_CD   AS DEST_TRNS_SVC_MOD_CD
											#if (${vvd_cd} != '')
											, VB.POR_NOD_CD             AS POR_NOD_CD
											#else
											, VB.POR_NODE               AS POR_NOD_CD
											#end
    										, VB.DEL_NOD_CD             AS DEL_NOD_CD
                                            , VB.FNL_DEST_CD            AS FNL_DEST_CD

                                            , VB.KEY_POL_CD       AS POL_CD
                                            , VB.KEY_POD_CD       AS POD_CD
                                            , VB.PRE_1_VVD        AS PRE_1_VVD
                                            , VB.PRE_2_VVD        AS PRE_2_VVD
                                            , VB.PRE_3_VVD        AS PRE_3_VVD
                                            , VB.PRE_4_VVD        AS PRE_4_VVD
                                            , VB.POST_1_VVD       AS POST_1_VVD
                                            , VB.POST_2_VVD       AS POST_2_VVD
                                            , VB.POST_3_VVD       AS POST_3_VVD
                                            , VB.POST_4_VVD       AS POST_4_VVD
                                            , VB.PRE_1_POL_CD     AS PRE_1_POL_CD
                                            , VB.PRE_2_POL_CD     AS PRE_2_POL_CD
                                            , VB.PRE_3_POL_CD     AS PRE_3_POL_CD
                                            , VB.PRE_4_POL_CD     AS PRE_4_POL_CD
                                            , VB.PRE_1_POD_CD     AS PRE_1_POD_CD
                                            , VB.PRE_2_POD_CD     AS PRE_2_POD_CD
                                            , VB.PRE_3_POD_CD     AS PRE_3_POD_CD
                                            , VB.PRE_4_POD_CD     AS PRE_4_POD_CD
                                            , VB.POST_1_POL_CD    AS POST_1_POL_CD
                                            , VB.POST_2_POL_CD    AS POST_2_POL_CD
                                            , VB.POST_3_POL_CD    AS POST_3_POL_CD
                                            , VB.POST_4_POL_CD    AS POST_4_POL_CD
