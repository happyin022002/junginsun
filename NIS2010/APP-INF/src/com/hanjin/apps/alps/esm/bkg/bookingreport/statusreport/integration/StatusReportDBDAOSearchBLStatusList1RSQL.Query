                                                 AND @[fv_pod_cd]||@[fv_pod_yard_cd] IN ( VB.POST_1_POD_YD_CD  , VB.POST_2_POD_YD_CD  , VB.POST_3_POD_YD_CD  , VB.POST_4_POD_YD_CD)
                                                #else
                                                 AND @[fv_pod_cd] IN ( VB.POST_1_POD_CD , VB.POST_2_POD_CD , VB.POST_3_POD_CD , VB.POST_4_POD_CD)
                                                #end


                                                 #if (${fv_pol_local} != '')
                                                    AND VB.POD_CD = NVL( (SELECT @[fv_pod_cd] FROM DUAL
                                                                          WHERE  @[fv_pod_cd] IN (VB.POST_4_POD_CD, VB.POST_3_POD_CD, VB.POST_2_POD_CD ) )
                                                                       , VB.POST_1_POD_CD)
                                                 #end
                                               #end
                                           #end

                                           #if (${fv_pod_cd} == '' && ${fv_pod_yard_cd} != '')
                                             #if (${fv_pre_pst_cd} == 'PR')
                                                 AND @[fv_pod_yard_cd] IN ( SUBSTR(VB.PRE_1_POD_YD_CD,-2)  , SUBSTR(VB.PRE_2_POD_YD_CD,-2)  , SUBSTR(VB.PRE_3_POD_YD_CD,-2)  , SUBSTR(VB.PRE_4_POD_YD_CD,-2))
                                               #elseif (${fv_pre_pst_cd} == 'PO')
                                                 AND @[fv_pod_yard_cd] IN ( SUBSTR(VB.POST_1_POD_YD_CD,-2)  , SUBSTR(VB.POST_2_POD_YD_CD,-2)  , SUBSTR(VB.POST_3_POD_YD_CD,-2)  , SUBSTR(VB.POST_4_POD_YD_CD,-2))
                                               #end
                                           #end

										   #if (${cust_tp_cd_s} != '' || ${cust_tp_cd_c} != '' || ${cust_tp_cd_n} != '' || ${cust_tp_cd_f} != '' || ${cust_tp_cd_a} != '')
                                            /* 57~61 cust_tp_cd_s,c,n,f,a*/
   											  #if(${cust_cnt_cd} !='' && ${cust_seq} != '')
											/* 63 cust_cnt_cd    *//* 64 cust_seq *//* 66 cust_tp_cd*/
												AND ( 1=2
														#if (${cust_tp_cd_s} !='')
															OR VB.SHIPPER = @[cust_cnt_cd]||TO_NUMBER(@[cust_seq])
														#end
														#if (${cust_tp_cd_c} !='')
															OR CONSIGNEE = @[cust_cnt_cd]||TO_NUMBER(@[cust_seq])
														#end
														#if (${cust_tp_cd_n} !='')
															OR NTFY = @[cust_cnt_cd]||TO_NUMBER(@[cust_seq])
														#end
														#if (${cust_tp_cd_f} !='')
															OR FFDR = @[cust_cnt_cd]||TO_NUMBER(@[cust_seq])
														#end
														#if (${cust_tp_cd_a} !='')
															/* 예비*/
														#end
													)
											   #end


										#end

										#if (${sp_cargo_dg} !=''||${sp_cargo_rf} != ''||${sp_cargo_ak} != ''||${sp_cargo_bb} != ''||${sp_cargo_hg} != ''||${sp_cargo_soc} != ''||${sp_cargo_eq} != ''||${sp_cargo_rd} != ''||${sp_cargo_pm} != ''||${sp_cargo_pc} != ''||${sp_cargo_fg} != ''||${sp_cargo_hd} != ''||${sp_cargo_rb} != ''||${sp_cargo_fu} != ''||${sp_cargo_li} != ''||${sp_vehicle} != ''||${non_dg_chem_flg} != ''  )
										/* 67 sp_cargo_dg  *//* 68 sp_cargo_rf    *//* 69 sp_cargo_ak *//* 70 sp_cargo_bb *//* 71 sp_cargo_hg    *//* 72 sp_cargo_soc   *//* 73 sp_cargo_eq    *//* 74 sp_cargo_rd    *//* 75 sp_cargo_pm    *//* 76 sp_cargo_pc    *//* 77 sp_cargo_fg    *//* 78 sp_cargo_hd    *//* 79 sp_cargo_rb    */
										     AND ( 1=2
										  #if (${sp_cargo_dg} != '')
										  /* 67 sp_cargo_dg  */
										  	OR VB.DCGO_FLG = 'Y'
										  #end
										  #if (${sp_cargo_rf} != '')
										  /* 68 sp_cargo_rf    */
											OR VB.RC_FLG = 'Y'
										  #end
										  #if (${sp_cargo_ak} != '')
										  /* 69 sp_cargo_ak */
											OR VB.AWK_CGO_FLG = 'Y'
										  #end
										  #if (${sp_cargo_ak} != '')
										  /* 69 sp_cargo_ak */
										    OR VB.AWK_CGO_FLG = 'Y'
										  #end
										  #if (${sp_cargo_bb} != '')
										  /* 70 sp_cargo_bb */
											OR VB.BB_CGO_FLG = 'Y'
										  #end
										  #if (${sp_cargo_hg} != '')
										  /* 71 sp_cargo_hg    */
										    OR VB.HNGR_FLG = 'Y'
										  #end
										  #if (${sp_cargo_soc} != '')
										  /* 72 sp_cargo_soc   */
											OR VB.SOC_FLG = 'Y'
										  #end
										  #if (${sp_cargo_eq} != '')
										  /* 73 sp_cargo_eq    */
										    OR VB.EQ_SUBST_FLG = 'Y'
										  #end
										  #if (${sp_cargo_rd} != '')
										  /* 74 sp_cargo_rd    */
										    OR VB.RD_CGO_FLG = 'Y'
										  #end
										  #if (${sp_cargo_pm} != '')
										  /* 75 sp_cargo_pm    */
										    OR VB.HOT_DE_FLG = 'Y'
										  #end
										  #if (${sp_cargo_pc} != '')
										  /* 76 sp_cargo_pc    */
										    OR VB.PRCT_FLG = 'Y'
										  #end
										  #if (${sp_cargo_fg} != '')
										  /* 77 sp_cargo_fg    */
										    OR VB.FD_GRD_FLG = 'Y'
										  #end
										  #if (${sp_cargo_hd} != '')
										  /* 78 sp_cargo_hd    */
										    OR VB.SPCL_HIDE_FLG = 'Y'
										  #end
										  #if (${sp_cargo_rb} != '')
										  /* 79 sp_cargo_rb    */
										    OR VB.RAIL_BLK_CD IS NOT NULL
										  #end
										  #if (${sp_cargo_fu} != '')
										  /* sp_cargo_fu    */
										    OR VB.FUMG_LOC_CD IS NOT NULL
										  #end
										  #if (${sp_cargo_li} != '')
										  /* sp_cargo_li    */
										    OR VB.SPCL_HIDE_LNR_FLG = 'Y'
										  #end
										   #if (${non_dg_chem_flg} != '')
										  /* non_dg_chem_flg    */
										    OR VB.NON_DG_CHEM_FLG = 'Y'
										  #end
											
										  #if (${sp_vehicle} != '')
										  /* sp_vehicle    */
										    OR VB.VEH_CMDT_FLG = 'Y'
										  #end										  										  
										  )
										#end

                                           #if (${cargo_tp_f} != '' || ${cargo_tp_p} != '' || ${cargo_tp_r} != '')
                                            /* 80~82 cargo_tp_f,p,r     */
                                            AND VB.BKG_CGO_TP_CD IN ( @[cargo_tp_f], @[cargo_tp_p], @[cargo_tp_r])
                                           #end

                                           #if (${bkg_sts_cd_f} != '' || ${bkg_sts_cd_x} != '' || ${bkg_sts_cd_a} != '' || ${bkg_sts_cd_w} != '')
                                            /* 83 bkg_sts_cd_f*//* 84 bkg_sts_cd_x*//* 85 bkg_sts_cd_a*//* 86 bkg_sts_cd_w*/
                                            AND VB.BKG_STS_CD IN ( @[bkg_sts_cd_f], @[bkg_sts_cd_x], @[bkg_sts_cd_a], @[bkg_sts_cd_w])
											#else /* 선택되지 않을경우 기본 STATUS */
                                            AND  VB.BKG_STS_CD IN ('F', 'W', 'A')
                                           #end

                                           #if (${non_sp_cargo} != '')
                                            /* 87 non_sp_cargo   */
                                            AND VB.WT_RSN_SPCL_CGO_FLG = 'Y'
                                           #end
                                           #if (${holding} != '')
                                            /* 88 holding*/
                                            AND VB.WT_RSN_HLD_FLG = 'Y'
                                           #end

                                           #if (${bl_type_a} != '')
                                            /* 89 bl_type_a      */
                                            AND VB.ADV_SHTG_CD = 'A'
                                           #end
                                           #if (${bl_type_s} != '')
                                            /* 90 bl_type_s      */
                                            AND VB.ADV_SHTG_CD = 'S'
                                           #end
                                           #if (${rev} != '')
                                            /* 91 rev            */
                                            AND EXISTS ( SELECT 'Y' FROM BKG_CHG_RT WHERE BKG_NO = VB.BKG_NO)
                                           #end
                                           #if (${non_rev} != '')
                                            /* 92 non_rev        */
                                            AND NOT EXISTS ( SELECT 'Y' FROM BKG_CHG_RT WHERE BKG_NO = VB.BKG_NO)
                                           #end

                                           #if (${aes_y} != '')
                                            /* 93 aes_y          */
                                            AND XPT_IMP.AES_TP_CD IS NOT NULL
                                            AND XPT_IMP.CNT_CD = 'US'
                                           #end
                                           #if (${aes_n} != '')
                                            /* 94 aes_n          */
                                            AND NVL(XPT_IMP.AES_TP_CD, 'X') = 'X'
                                            AND NVL(XPT_IMP.CNT_CD,'US') = 'US'
                                           #end

                                           #if (${stop_cargo} != '')
                                            /* 95 stop_cargo     */
                                            AND VB.STOP_CGO_FLG ='Y'
                                           #end
                                           #if (${ro_y} != '')
                                            /* 96 ro_y           */
											AND EXISTS ( SELECT 'Y'
											             FROM BKG_ROLL_OVR WHERE BKG_NO = VB.BKG_NO
											             GROUP BY BKG_NO
											             HAVING COUNT(BKG_NO) > @[ro_y]
														)
                                           #end

                                           #if (${caed_y} != '')
                                           /* 98 caed_y         */
                                            AND XPT_IMP.CAED_TP_CD IS NOT NULL
                                            AND XPT_IMP.CNT_CD = 'CA'  
                                           #end
                                           #if (${caed_n} != '')
                                            /* 99 caed_n         */
                                            AND NVL(XPT_IMP.CAED_TP_CD, 'X') = 'X'
                                            AND NVL(XPT_IMP.CNT_CD,'CA') = 'CA'
                                           #end                                           

                                           #if (${certi_y} != '')
                                            /* 101 certi_y        */
                                            AND EXISTS ( SELECT 'Y' FROM BKG_IMG_STO WHERE BKG_NO = VB.BKG_NO
                                           							#if (${certi_d} != '' || ${certi_a} != '' || ${certi_b} != '' || ${certi_g} != '' || ${certi_c} != '')
                                           							AND RIDR_TP_CD IN (@[certi_d], @[certi_a], @[certi_b], @[certi_g], @[certi_c])
                                           							#end
                                            						)
                                           #end

                                           #if (${certi_n} != '')
                                            /* 102 certi_n        */
                                            AND NOT EXISTS ( SELECT 'Y' FROM BKG_IMG_STO WHERE BKG_NO = VB.BKG_NO
                                            								#if (${certi_d} != '' || ${certi_a} != '' || ${certi_b} != '' || ${certi_g} != '' || ${certi_c} != '')
		                                           							AND RIDR_TP_CD IN (@[certi_d], @[certi_a], @[certi_b], @[certi_g], @[certi_c])
		                                           							#end
		                                            						)
                                           #end

										   #if (${rate_check} != '')
											AND @[rate_check] =  (CASE WHEN VB.BKG_CRE_DT >= TO_DATE('20141125','YYYYMMDD') THEN RA.OFT_MSS_FLG 
																						WHEN SUBSTR(VB.SC_NO,1,3) = 'DUM' OR SUBSTR(VB.RFA_NO,1,3) = 'DUM' OR SUBSTR(VB.TAA_NO,1,3) = 'DUM' THEN 'Y' 
																						WHEN RA.OFT_MSS_FLG = 'N' THEN 'N'
																						WHEN RA.OFT_MSS_FLG = 'Y' THEN NVL((SELECT 'N'
																															FROM BKG_CHG_RT RT
														  																	WHERE RT.BKG_NO = RA.BKG_NO
																															AND ROWNUM = 1), 'Y') END )
										   #end

                                           #if (${aloc_sts_cd_f} != '' || ${aloc_sts_cd_s} != '' )
                                            /* 103 aloc_sts_cd_f*//* 104 aloc_sts_cd_s*/
                                            AND B.ALOC_STS_CD IN (@[aloc_sts_cd_f], @[aloc_sts_cd_s])
                                           #end

                                           #if (${non_rd_sts_f} != '' || ${non_rd_sts_r} != '' )
                                            /* 105 non_rd_sts_f*//* 106 non_rd_sts_r*/
                                            AND B.NON_RT_STS_CD IN (@[non_rd_sts_f], @[non_rd_sts_r])
                                           #end

                                           #if (${spot_guide_rfa_no} != '')  
                                            /* 107 spot_guide_rfa_no*/
                                            AND SUBSTR(VB.RFA_NO,6,1) ='G'
											AND SUBSTR(VB.RFA_NO,1,3) <> 'DUM'
                                           #end

											#if (${master_rfa_no} != '')
                                            /* 107 master_rfa_no*/
                                            AND SUBSTR(VB.RFA_NO,6,1) ='M'
											AND SUBSTR(VB.RFA_NO,1,3) <> 'DUM'
                                           #end

											#if (${basic_rfa_no} != '')
                                            /* 107 basic_rfa_no*/
                                            AND SUBSTR(VB.RFA_NO,6,1) ='B'
											AND SUBSTR(VB.RFA_NO,1,3) <> 'DUM'
                                           #end

                                       
                                           #if (${ctrt_cng_tp_cd} != '')
                                            /* 108 ctrt_cng_tp_cd*/
                                            AND VB.CTRT_CNG_TP_CD IN (${ctrt_cng_tp_cd})
                                           #end                                           
                                           
                                           #if (${rtro_knd_cd} != '')
                                            /* 109 rtro_knd_cd*/
                                            AND VB.RTRO_KND_CD IN (${rtro_knd_cd})
                                           #end     

                                           #if (${web_upd_sts} == 'B')
                                             #if (${web_sts_cd_a} != '' && ${web_sts_cd_m} != '')
                                              AND B.SYS_UPLD_FLG in ('Y','N')
                                              AND B.XTER_BKG_RQST_CD = 'WEB'

                                             #elseif (${web_sts_cd_a} != '')
                                              AND B.SYS_UPLD_FLG = 'Y'
                                              AND B.XTER_BKG_RQST_CD = 'WEB'

                                             #elseif (${web_sts_cd_m} != '')
                                              AND B.SYS_UPLD_FLG = 'N'
                                              AND B.XTER_BKG_RQST_CD = 'WEB'
                                             #end     

				                         #elseif (${web_upd_sts} == 'S')
				                           #if (${web_sts_cd_a} != '')
				                           AND EXISTS( 
				                               SELECT 'Y'
				                                 FROM BKG_XTER_RQST_MST A
				                                WHERE A.BKG_NO = VB.BKG_NO
				                                  AND A.XTER_RQST_VIA_CD = 'WEB'
				                                  AND A.DOC_TP_CD = 'S'
				                                  AND A.SYS_UPLD_FLG = 'Y'/*AUTO*/
				                               )
				                           #elseif (${web_sts_cd_m} != '')
				                           AND NOT EXISTS( 
				                               SELECT 'Y'
				                                 FROM BKG_XTER_RQST_MST A
				                                WHERE A.BKG_NO = VB.BKG_NO
				                                  AND A.XTER_RQST_VIA_CD = 'WEB'
				                                  AND A.DOC_TP_CD = 'S'
				                                  AND A.SYS_UPLD_FLG = 'Y'/*AUTO*/
				                                 )
				                           #end
				                         #end
                                 ) X
								 WHERE 1=1
                                 #if(${bkg_sts_cd_i} != '')
                                 AND VVD_CNT <> SKD_CNT
                                 #end
                                            #if (${zone_cd} == 'OCN')
                                             /*17 zone_cd OCN*/
                                             --AND (SELECT CONTI_CD FROM MDM_LOCATION WHERE LOC_CD = VB.POR_CD) <> (SELECT CONTI_CD FROM MDM_LOCATION WHERE LOC_CD = VB.DEL_CD)
                                             AND NVL(IOC_CD ,IOC_CD2) = 'O'
                                            #end
                                            #if (${zone_cd} == 'IPT')
                                             /* 17 zone_cd IPT*/
                                             --AND (SELECT CONTI_CD FROM MDM_LOCATION WHERE LOC_CD = VB.POR_CD) = (SELECT CONTI_CD FROM MDM_LOCATION WHERE LOC_CD = VB.DEL_CD)
                                             AND NVL(IOC_CD ,IOC_CD2) = 'I'
                                            #end
                                            #if (${zone_cd} == 'DOM')
											 /* 17 zone_cd IPT*/
                                             AND (SUBSTR(por_cd,1,2) = SUBSTR(del_cd,1,2))
                                            #end




                          ) Y
                          ,MAS_RGST_BKG COA_BKG, MAS_MON_VVD COA_VVD
                        WHERE 1=1
                        AND Y.BKG_NO     = COA_BKG.BKG_NO(+)
                        AND COA_BKG.TRD_CD = COA_VVD.TRD_CD(+)
                        AND COA_BKG.RLANE_CD = COA_VVD.RLANE_CD(+)
                        AND COA_BKG.IOC_CD = COA_VVD.IOC_CD(+)
                        AND COA_BKG.VSL_CD = COA_VVD.VSL_CD(+)
                        AND COA_BKG.SKD_VOY_NO = COA_VVD.SKD_VOY_NO(+)
                        AND COA_BKG.DIR_CD = COA_VVD.DIR_CD(+)

                         #if (${crn_no_flag} != '')
                          /* 100 crn_no_flag    */  
                          --AND NVL(SHP_CALL_NO, ' ') <> ' '
                         #end

                         #if (${mty_rtn_ecc} != '')
                           AND Y.ECC_CD IN  (${mty_rtn_ecc})
                         #end

                        AND BL_CERTI = NVL(@[bl_certi],BL_CERTI)
                   ) Z
   			    ORDER BY ${orderby},BKG_NO
	   )ZZ
 ) QQ
 , ( SELECT INTG_CD_VAL_DESC RTRO_KND_CD_NM, INTG_CD_VAL_CTNT RTRO_KND_CD FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03367' ) KK 
WHERE RNUM BETWEEN NVL(@[rows_per_page],50) * (NVL(@[curr_page],1) - 1) + 1
           AND NVL(@[rows_per_page],50) *  NVL(@[curr_page],1)           
           AND QQ.RTRO_KND_CD = KK.RTRO_KND_CD(+)			]]></sql>
