<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqListDBDAOsearchBbHandlingCstRSQL">
			<desc><![CDATA[Break Bulk Handling Cost & Rate]]></desc>
			<sql><![CDATA[
SELECT SUM ( DECODE ( CST.ROUT_SEQ , 1, CST.COST_AMT, 0 ) ) POL_SUM ,
  SUM ( DECODE ( CST.ROUT_SEQ , 2, CST.COST_AMT, 0 ) ) POD_SUM ,
  MAX ( C.PROP_RT_AMT ) PROP_RT ,
  MAX ( C.APRO_RT_AMT ) APRO_RT
FROM PRI_SCQ_BB_ROUT_COST CST ,
  (
    SELECT SCQ_RQST_NO,
      SCQ_VER_NO,
      ROUT_SEQ ,
      MAX(ROUT_SEQ_VER_NO) MAX_ROUT_SEQ_VER_NO
    FROM PRI_SCQ_BB_ROUT
    WHERE SCQ_RQST_NO = @[scq_rqst_no]
      and SCQ_VER_NO = @[scq_ver_no]
    GROUP BY SCQ_RQST_NO, SCQ_VER_NO, ROUT_SEQ ) A,
  PRI_SCQ_BB_MN C
WHERE CST.SCQ_RQST_NO = A.SCQ_RQST_NO
  AND CST.SCQ_VER_NO = A.SCQ_VER_NO
  AND CST.ROUT_SEQ = A.ROUT_SEQ
  AND CST.ROUT_SEQ_VER_NO = A.MAX_ROUT_SEQ_VER_NO
  AND CST.SCQ_RQST_NO = C.SCQ_RQST_NO
  AND CST.SCQ_VER_NO = C.SCQ_VER_NO
GROUP BY CST.SCQ_RQST_NO, CST.SCQ_VER_NO 			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
				<param name="scq_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
