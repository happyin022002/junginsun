<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOmodifyBkgKrWhfRtIfArInvUSQL">
			<desc><![CDATA[u]]></desc>
			<sql><![CDATA[
UPDATE BKG_KR_WHF_RT A

#if (${cancel_flag} != 'Y') 
   SET A.AR_IF_FLG = 'Y',
       A.AR_IF_DT = SYSDATE,
       A.AR_IF_GDT = GLOBALDATE_PKG.TIME_CONV_FNC(PORT_CD, SYSDATE, 'GMT'),
#else 
   SET A.AR_IF_FLG = 'N',
       A.AR_IF_DT = '',
       A.AR_IF_GDT = '',
#end
       A.UPD_USR_ID = @[upd_usr_id],
       A.UPD_DT = SYSDATE
 WHERE A.VSL_CD = @[vsl_cd]
   AND A.SKD_VOY_NO = @[skd_voy_no]
   AND A.SKD_DIR_CD = @[skd_dir_cd]
   AND A.WHF_BND_CD = @[whf_bnd_cd]
   AND A.BL_NO = @[bl_no]
   AND A.CHG_RT_SEQ = (SELECT MAX(B.CHG_RT_SEQ) FROM BKG_KR_WHF_RT B WHERE B.VSL_CD = A.VSL_CD
                                                                       AND B.SKD_VOY_NO = A.SKD_VOY_NO
                                                                       AND B.SKD_DIR_CD = A.SKD_DIR_CD
                                                                       AND B.WHF_BND_CD = A.WHF_BND_CD
                                                                       AND B.BL_NO = A.BL_NO)			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="whf_bnd_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
