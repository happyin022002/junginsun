<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDAOCustomAcctCateVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
select
	b.acct_cd,
	a.acct_eng_nm,
	b.acct_itm_seq,
	b.acct_itm_nm,
	max(decode(	c.flet_acct_cate_cd, 'OT', 'Y', 'N')) chk_other_exp,
	max(decode(	c.flet_acct_cate_cd, 'CH', 'Y', 'N')) chk_charterer,
	max(decode(	c.flet_acct_cate_cd, 'OW', 'Y', 'N')) chk_owner,
	max(decode(	c.flet_acct_cate_cd, 'PP', 'Y', 'N')) chk_prepaymentp,
	max(decode(	c.flet_acct_cate_cd, 'PS', 'Y', 'N')) chk_prepayments,
	max(decode(	c.flet_acct_cate_cd, 'MS', 'Y', 'N')) chk_manual_slip,
	max(decode(	c.flet_acct_cate_cd, 'OF', 'Y', 'N')) chk_off_hire,
	max(decode(	c.flet_acct_cate_cd, 'VV', 'Y', 'N')) chk_vvd_required,
	max(decode(	c.flet_acct_cate_cd, 'BU', 'Y', 'N')) chk_bodbor_if,
	max(decode(	c.flet_acct_cate_cd, 'OT', 'OTY', 'OTN')) prev_other_exp,
	max(decode(	c.flet_acct_cate_cd, 'CH', 'CHY', 'CHN')) prev_charterer,
	max(decode(	c.flet_acct_cate_cd, 'OW', 'OWY', 'OWN')) prev_owner,
	max(decode(	c.flet_acct_cate_cd, 'PP', 'PPY', 'PPN')) prev_prepaymentp,
	max(decode(	c.flet_acct_cate_cd, 'PS', 'PSY', 'PSN')) prev_prepayments,
	max(decode(	c.flet_acct_cate_cd, 'MS', 'MSY', 'MSN')) prev_manual_slip,
	max(decode(	c.flet_acct_cate_cd, 'OF', 'OFY', 'OFN')) prev_off_hire,
	max(decode(	c.flet_acct_cate_cd, 'VV', 'VVY', 'VVN')) prev_vvd_required,
	max(decode(	c.flet_acct_cate_cd, 'BU', 'BUY', 'BUN')) prev_bodbor_if,
	'' flet_acct_cate_cd,
	'' cre_usr_id,
	'' upd_usr_id
from 	mdm_account a, fms_acct_itm b, fms_acct_cate c
where 	a.acct_cd = b.acct_cd
and     b.acct_cd = c.acct_cd(+)
and     b.acct_itm_seq = c.acct_itm_seq(+)
and     b.acct_cd not in ('610411', '612911', '710311', '712911')
group by b.acct_cd, a.acct_eng_nm, b.acct_itm_seq,b.acct_itm_nm
order by b.acct_cd, b.acct_itm_nm			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
