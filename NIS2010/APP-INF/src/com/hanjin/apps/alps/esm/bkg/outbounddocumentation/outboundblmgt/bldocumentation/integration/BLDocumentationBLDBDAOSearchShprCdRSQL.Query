<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationDBDAOsearchShprCdRSQL">
			<desc><![CDATA[searchShprCd]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD || CUST_SEQ AS SHIPPER_CD
FROM BKG_CUSTOMER
WHERE BKG_NO = @[bkg_no]
AND BKG_CUST_TP_CD = 'S'
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
