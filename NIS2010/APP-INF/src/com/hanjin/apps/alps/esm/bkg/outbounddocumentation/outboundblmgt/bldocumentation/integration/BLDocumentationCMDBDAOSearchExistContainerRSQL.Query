<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOSearchExistContainerRSQL">
			<desc><![CDATA[SearchExistContainer]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
SELECT 1
FROM   BKG_CNTR_HIS
WHERE  @[bkg_no] = BKG_NO
AND    @[cntr_no] = CNTR_NO
AND    'TMP0000001' = CORR_NO
#else 
SELECT 1
FROM   BKG_CONTAINER
WHERE  @[bkg_no] = BKG_NO
AND    @[cntr_no] = CNTR_NO
#end
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
