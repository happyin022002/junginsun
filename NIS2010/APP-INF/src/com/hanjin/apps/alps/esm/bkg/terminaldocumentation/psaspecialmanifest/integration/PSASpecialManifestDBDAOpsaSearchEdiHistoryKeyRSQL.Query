<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSASpecialManifestDBDAOpsaSearchEdiHistoryKeyRSQL">
			<desc><![CDATA[수신정보 키값(MSG_SND_NO) 조회]]></desc>
			<sql><![CDATA[
SELECT
    @[msgTpId] || TO_CHAR(SYSDATE, 'YYMMDD') || LTRIM(TO_CHAR(BKG_CSTMS_PSA_DG_SEQ.NEXTVAL,'009')) || '01' AS KEY_VAL
FROM DUAL			]]></sql>
			<params>
				<param name="msgTpId" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
