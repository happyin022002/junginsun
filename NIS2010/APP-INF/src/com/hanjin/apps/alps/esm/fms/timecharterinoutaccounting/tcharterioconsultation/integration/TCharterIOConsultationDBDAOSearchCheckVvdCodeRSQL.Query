<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOConsultationDBDAOSearchCheckVvdCodeRSQL">
			<desc><![CDATA[TCharterIOConsultationDBDAOSearchCheckVvdCodeRSQL]]></desc>
			<sql><![CDATA[
SELECT    VSL_CD
       || SKD_VOY_NO
       || SKD_DIR_CD
       || REV_DIR_CD BUNKER_VVD
  FROM FMS_VVD
 WHERE    VSL_CD
       || SKD_VOY_NO
       || SKD_DIR_CD
       || REV_DIR_CD = @[bunker_vvd]			]]></sql>
			<params>
				<param name="bunker_vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
