<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAOCheckLocCodeRSQL">
			<desc><![CDATA[MDM_LOCATION에 LOC_CD가 존재하는지 체크]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM MDM_LOCATION A
 WHERE A.DELT_FLG = 'N'
       AND LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
