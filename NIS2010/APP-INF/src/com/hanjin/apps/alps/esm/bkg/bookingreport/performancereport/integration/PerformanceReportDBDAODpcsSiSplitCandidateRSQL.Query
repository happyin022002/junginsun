<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAODpcsSiSplitCandidateRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT /*+RULE */ X.BKG_NO
,      X.DOC_TP_CD
,      X.XTER_RQST_NO
,      X.SPLIT_STS_CD
,      X.XTER_BKG_RQST_STS_CD
,      X.BKG_UPLD_STS_CD
,      X.XTER_RQST_SEQ
,      TO_CHAR(X.RQST_DT,'YYYY-MM-DD HH24:MI') AS RQST_DT
,      X.POL_CD
,      TO_CHAR(X.RQST_DEP_DT,'YYYY-MM-DD') AS RQST_DEP_DT
,      X.VSL_CD || X.SKD_VOY_NO || X.SKD_DIR_CD AS VVD_CD 
,      B.EDI_HLD_FLG
,      (SELECT COUNT(1) FROM  BKG_XTER_CNTR C WHERE C.XTER_SNDR_ID = X.XTER_SNDR_ID AND C.XTER_RQST_NO = X.XTER_RQST_NO AND C.XTER_RQST_SEQ = X.XTER_RQST_SEQ) CNTR_CNT
,      BKG_JOIN_FNC(CURSOR(SELECT C.CNTR_NO FROM  BKG_XTER_CNTR C WHERE C.XTER_SNDR_ID = X.XTER_SNDR_ID AND C.XTER_RQST_NO = X.XTER_RQST_NO AND C.XTER_RQST_SEQ = X.XTER_RQST_SEQ),',') CNTR_DESC
,      REPLACE(SUBSTR(X.XTER_BKG_RMK1,1,100), CHR(13) || CHR(10), ' ') AS XTER_RMK
,      X.XTER_BKG_RMK1 AS XTER_RMK_ALL
,      X.PCK_QTY
,      X.PCK_TP_CD
--,      X.NET_WGT
--,      X.NET_WGT_UT_CD
,      X.ESTM_WGT AS NET_WGT
,      X.ESTM_WGT_UT_CD AS NET_WGT_UT_CD
,      X.MEAS_QTY
,      X.MEAS_UT_CD
,      X.XTER_SNDR_ID
,      TO_CHAR(VPS.VPS_ETD_DT,'YYYY-MM-DD') AS VPS_ETD_DT	
,      CASE WHEN B.BKG_NO IS NULL OR B.BKG_STS_CD = 'X' THEN 'N' ELSE 'Y' END AS BKG_VALID_FLG
,	   '' SEARCH_TYPE
FROM   BKG_XTER_RQST_MST X
,      BKG_BOOKING B
,      BKG_VVD VVD
,      VSK_VSL_PORT_SKD VPS
WHERE  1=1
AND    X.BKG_NO       = B.BKG_NO(+)          
AND    VPS.VSL_CD     = VVD.VSL_CD
AND    VPS.SKD_VOY_NO     =   VVD.SKD_VOY_NO
AND    VPS.SKD_DIR_CD     =   VVD.SKD_DIR_CD
AND    VPS.CLPT_IND_SEQ   =   VVD.POL_CLPT_IND_SEQ
AND	   VPS.VPS_PORT_CD    =   VVD.POL_CD
AND    VVD.BKG_NO = B.BKG_NO
AND    VVD.VSL_PRE_PST_CD || VVD.VSL_SEQ =   (SELECT MIN(BV.VSL_PRE_PST_CD || BV.VSL_SEQ) 
                                              FROM   BKG_VVD BV
                                              WHERE  BV.BKG_NO = VVD.BKG_NO)


AND    X.BKG_NO = (SELECT X2.BKG_NO FROM BKG_XTER_RQST_MST X2 WHERE X2.XTER_SNDR_ID = @[xter_sndr_id] AND X2.XTER_RQST_NO = @[xter_rqst_no] AND X2.XTER_RQST_SEQ = @[xter_rqst_seq])
AND    X.DOC_TP_CD = 'S'
AND    X.XTER_BKG_RQST_STS_CD IN ('C','U')  

ORDER BY X.XTER_RQST_NO,X.XTER_RQST_SEQ			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
