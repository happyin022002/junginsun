<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOSearchOffhireCnfmListNonConfirmRSQL">
			<desc><![CDATA[Off Hire Non Confirm 목록]]></desc>
			<sql><![CDATA[
SELECT  O.VSL_CD
       ,O.SKD_VOY_NO
	   ,O.SKD_DIR_CD
	   ,O.VNOR_VSL_STS_CD
       ,O.VNOR_FM_PORT_CD
       ,TO_CHAR(O.VNOR_OFFH_FM_DT,'YYYYMMDD') AS VNOR_OFFH_FM_DT
       ,TO_CHAR(O.VNOR_OFFH_FM_DT,'HH24MI') AS VNOR_OFFH_FM_DT_HM
       ,TO_CHAR(O.VNOR_OFFH_TO_DT,'YYYYMMDD') AS VNOR_OFFH_TO_DT
       ,TO_CHAR(O.VNOR_OFFH_TO_DT,'HH24MI') AS VNOR_OFFH_TO_DT_HM
       ,F.VNOR_ITM_CD
       ,F.VNOR_ITM_OFC_CD
       ,F.VNOR_ITM_UT_CD
       ,F.VNOR_ITM_VAL
       ,F.VNOR_ITM_FLET_ADD_CD
       ,F.VNOR_ITM_RMK
       ,F.ATCH_FILE_OP_KNT
       ,(SELECT COUNT(1) FROM FMS_ATCH_FILE WHERE ATCH_FILE_LNK_ID = F.ATCH_FILE_FLET_LNK_ID ) AS ATCH_FILE_FLET_KNT
       ,TO_CHAR(F.UPD_DT, 'yyyy-mm-dd hh24:mi:ss') AS UPD_DT              
       ,(SELECT U.USR_NM FROM COM_USER U WHERE U.USR_ID = F.CRE_USR_ID) AS UPD_USR_ID
	   ,F.VNOR_SEQ
       ,F.VNOR_ITM_SEQ
	   ,F.ATCH_FILE_OP_LNK_ID 
       ,F.ATCH_FILE_FLET_LNK_ID 
	   ,O.VNOR_OFFH_KND_CD
FROM FMS_VNOR O, FMS_VNOR_ITM F
WHERE O.VSL_CD = F.VSL_CD
  AND O.VNOR_SEQ = F.VNOR_SEQ  
  AND F.VNOR_ITM_PROC_CD = 'N'
ORDER BY  O.VNOR_OFFH_FM_DT, O.VSL_CD, O.SKD_VOY_NO			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
