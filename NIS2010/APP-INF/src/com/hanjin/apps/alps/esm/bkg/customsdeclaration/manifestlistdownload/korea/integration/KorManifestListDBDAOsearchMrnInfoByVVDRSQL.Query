<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchMrnInfoByVVDRSQL">
			<desc><![CDATA[VVD/PORT 로 MRN 정보 조회]]></desc>
			<sql><![CDATA[
SELECT MRN_NO
	 , MRN_CHK_NO
	 , VSL_CD||SKD_VOY_NO||SKD_DIR_CD VVD
 	 , PORT_CD
	 , IO_BND_CD
 	 , TO_CHAR(SND_DT, 'YYYY-MM-DD hh24:mi') SND_DT
 	 , TO_CHAR(RSLT_ACK_DT, 'YYYY-MM-DD hh24:mi') RSLT_DT
	 , TRSM_MSG_TP_ID RSLT
	 , ERR_MSG
	 , CEIL(SYSDATE - NVL(EDI_SND_DT, SYSDATE)) SEND_IND
  FROM BKG_CSTMS_KR_MF_REF_NO
 WHERE VSL_CD = @[in_vsl_cd]
   AND SKD_VOY_NO = @[in_skd_voyage_no]
   AND SKD_DIR_CD = @[in_skd_dir_cd]
   AND PORT_CD = @[in_port]
   AND IO_BND_CD = @[in_mrn_mode]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="in_vsl_cd" type="12" value="" out="N"/>
				<param name="in_skd_voyage_no" type="12" value="" out="N"/>
				<param name="in_skd_dir_cd" type="12" value="" out="N"/>
				<param name="in_port" type="12" value="" out="N"/>
				<param name="in_mrn_mode" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
