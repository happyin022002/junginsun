<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqAwkwardDBDAOsearchEmailTargetUserListRSQL">
			<desc><![CDATA[승인권자가 Approval , reject 시 해당 requester email 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT TO_USER FROM 
(
SELECT USR_EML AS TO_USER 
FROM COM_USER
WHERE USR_ID = (
                SELECT PROG_USR_ID FROM PRI_SCQ_PROG 
                WHERE 1=1
                AND PROG_SEQ = (SELECT MAX(PROG_SEQ) FROM PRI_SCQ_PROG 
                                WHERE 1=1
                                AND SPCL_CGO_TP_CD = 'AK' 
                                AND PROG_STS_CD = 'Q'
                                AND SCQ_RQST_NO = @[scq_rqst_no]
                                )
                AND SPCL_CGO_TP_CD = 'AK' 
                AND PROG_STS_CD = 'Q'
                AND SCQ_RQST_NO = @[scq_rqst_no]                
				)
UNION ALL
SELECT SREP_EML AS TO_USER
FROM MDM_SLS_REP 
WHERE SREP_CD = (SELECT RQST_SREP_CD FROM PRI_SCQ_AWK_MN
                 WHERE 1=1
                 AND SCQ_RQST_NO = @[scq_rqst_no]   
                 AND SCQ_VER_NO = (SELECT MAX(SCQ_VER_NO) 
                                   FROM PRI_SCQ_AWK_MN
                                   WHERE 1=1
                                   AND SCQ_RQST_NO = @[scq_rqst_no])
                 )
)			]]></sql>
			<params>
				<param name="scq_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
