<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchEdiRcvSndIdCtRSQL">
			<desc><![CDATA[...]]></desc>
			<sql><![CDATA[
SELECT 'CT' AS EDI_MSG_ID, C.EDI_RCV_ID, C.EDI_SND_ID, C.YD_CD
  FROM BKG_CGO_RLSE A,
       BKG_BOOKING  B,
       BKG_EDI_YD   C
 WHERE A.BL_NO  = @[bl_no] 
   AND A.BL_NO  = B.BL_NO
   AND C.PORT_CD = @[pod_cd]
   AND C.PORT_CD IN (
					SELECT ATTR_CTNT1 FROM BKG_CSTMS_CD_CONV_CTNT
					WHERE CNT_cD='US'
					AND CSTMS_DIV_ID='US_CR_CT_PORT_STUP'
					)
   AND B.SLAN_CD IN (C.SLAN_CD1,C.SLAN_CD2,C.SLAN_CD3,C.SLAN_CD4,C.SLAN_CD5,
                     C.SLAN_CD6,C.SLAN_CD7,C.SLAN_CD8,C.SLAN_CD9,C.SLAN_CD10)
   AND (C.FULL_RLSE_EDI_CD = '1' OR C.FULL_RLSE_EDI_CD = '2')
   AND ROWNUM	= 1
   AND 'CT1'    = @[edi_knd]
   AND NVL(A.MRN_TML_EDI_SND_CD,' ') <> 'T'
   AND C.EDI_RCV_ID IN (
						SELECT ATTR_CTNT1 FROM BKG_CSTMS_CD_CONV_CTNT
						WHERE CNT_cD='US'
						AND CSTMS_DIV_ID='US_CR_CT_TML_STUP'
						)			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="edi_knd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
