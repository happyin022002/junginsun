<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TAAProposalDBDAOCreTaaNoRSQL">
			<desc><![CDATA[TAA NO를 생성합니다.]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(@[ofc_cd], 0, 3) || SUBSTR(TO_CHAR(SYSDATE, 'YYYY'), -2) || 'T'
       || LPAD(NVL(MAX(SUBSTR (TAA_NO, 7, 4)), 0) + 1, 4, '0') TAA_NO
FROM PRI_TAA_HDR
WHERE TAA_NO LIKE SUBSTR(@[ofc_cd], 0, 3) || SUBSTR(TO_CHAR(SYSDATE, 'YYYY'), -2) || 'T%'
			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
