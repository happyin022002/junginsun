<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOSearchLaneOfficeListRSQL">
			<desc><![CDATA[2013.12.10 PEJ [CHM-201328059] QTA Edit_Office Add 팝업 추가
                                                 선택한 노선의 Lane-Office Relation Setting List를 조회한다]]></desc>
			<sql><![CDATA[
SELECT TRD_CD
      ,RLANE_CD
      ,DIR_CD
      ,RHQ_CD
      ,RGN_OFC_CD
      ,DECODE(SQM_ACT_FLG, 'Y', 1, 0) AS SQM_ACT_FLG
      ,BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,SUB_TRD_CD
      ,'' AS F_USR_ID -- Creation시 사용하기 위해서 추가함
  FROM SQM_QTA_LANE_OFC
 WHERE BSE_TP_CD  = @[f_bse_tp_cd]
   AND BSE_YR     = @[f_bse_yr]
   AND BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
   AND OFC_VW_CD  = @[f_ofc_vw_cd]
   AND TRD_CD     = 'IAS'
   AND RLANE_CD   = @[f_rlane_cd]
   AND DIR_CD     = @[f_dir_cd]
   AND RHQ_CD     = @[f_rhq_cd]			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_rhq_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
