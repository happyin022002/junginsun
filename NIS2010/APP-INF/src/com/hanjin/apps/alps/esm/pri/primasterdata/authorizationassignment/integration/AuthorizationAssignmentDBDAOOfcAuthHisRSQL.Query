<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationAssignmentDBDAOOfcAuthHisRSQL">
			<desc><![CDATA[PRI_AUTH_APRO_OFC_HIS 테이블 조회]]></desc>
			<sql><![CDATA[
SELECT

	  AUTH_APRO_HIS_SEQ
	, PRC_CTRT_TP_CD
	, PRC_OFC_AUTH_TP_CD
	, AUTH_APRO_OFC_CD
	, (SELECT OFC_ENG_NM FROM MDM_ORGANIZATION WHERE OFC_CD = AUTH_APRO_OFC_CD) AUTH_APRO_OFC_NM
	, AUTH_APRO_DESC
	, AUTH_APRO_USE_FLG
	, PROG_USR_ID
	, (SELECT USR_NM FROM COM_USER WHERE USR_ID = PROG_USR_ID) PROG_USR_NM
	, PROG_OFC_CD
	, TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('GMT',PROG_GDT , GLOBALDATE_PKG.GET_LOCCD_FNC('SELSC')),'YYYY-MM-DD') AS PROG_GDT
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT

	  FROM PRI_AUTH_APRO_OFC_HIS
	 WHERE PRC_CTRT_TP_CD = @[prc_ctrt_tp_cd]
	   AND PRC_OFC_AUTH_TP_CD = @[prc_ofc_auth_tp_cd]
	 ORDER BY AUTH_APRO_HIS_SEQ DESC			]]></sql>
			<params>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="prc_ofc_auth_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
