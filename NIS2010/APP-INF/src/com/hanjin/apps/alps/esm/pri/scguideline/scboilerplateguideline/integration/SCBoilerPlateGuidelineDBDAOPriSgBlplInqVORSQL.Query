<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateGuidelineDBDAOPriSgBlplInqVORSQL">
			<desc><![CDATA[Boiler Plate Inquiry]]></desc>
			<sql><![CDATA[
SELECT 
	BLPL_HDR_SEQ,
	BLPL_SEQ,
	BLPL_TIT_NM,
	DP_SEQ,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
FROM PRI_SG_BLPL
WHERE	BLPL_HDR_SEQ = @[blpl_hdr_seq]
ORDER BY DP_SEQ			]]></sql>
			<params>
				<param name="blpl_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
