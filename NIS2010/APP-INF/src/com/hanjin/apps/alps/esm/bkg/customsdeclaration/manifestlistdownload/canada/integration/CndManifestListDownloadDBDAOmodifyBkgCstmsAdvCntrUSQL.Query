<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndManifestListDownloadDBDAOmodifyBkgCstmsAdvCntrUSQL">
			<desc><![CDATA[modifyBkgCstmsAdvCntr
]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_CNTR
   SET RAIL_CRR_REF_NO = @[rail_crr_ref_no]
      ,USA_IB_TRSP_NO = @[usa_ib_trsp_no]
WHERE CNT_CD = @[cnt_cd]
AND BL_NO = @[bl_no]
AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="rail_crr_ref_no" type="12" value="" out="N"/>
				<param name="usa_ib_trsp_no" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
