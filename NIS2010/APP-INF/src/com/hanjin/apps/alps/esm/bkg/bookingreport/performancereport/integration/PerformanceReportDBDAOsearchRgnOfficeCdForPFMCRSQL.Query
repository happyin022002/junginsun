<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOsearchRgnOfficeCdForPFMCRSQL">
			<desc><![CDATA[BKG_DOC_PERF_OFC Table에 등록된 모둔 Region office를 불러온다]]></desc>
			<sql><![CDATA[
SELECT DISTINCT BDPO.RGN_OFC_CD AS REGION_CD
FROM BKG_DOC_PERF_OFC BDPO
ORDER BY DECODE(BDPO.RGN_OFC_CD,'HAMRU','1','SHARC','2',BDPO.RGN_OFC_CD)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
