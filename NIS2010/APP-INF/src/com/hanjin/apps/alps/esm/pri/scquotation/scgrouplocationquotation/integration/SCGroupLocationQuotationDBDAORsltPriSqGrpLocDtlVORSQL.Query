<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupLocationQuotationDBDAORsltPriSqGrpLocDtlVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	A.QTTN_NO
,	A.QTTN_VER_NO
,	A.GRP_LOC_SEQ
,	A.GRP_LOC_DTL_SEQ
,	A.LOC_CD
,	B.LOC_NM
,	A.SRC_INFO_CD
,   (SELECT INTG_CD_VAL_DP_DESC 
    from COM_INTG_CD_DTL
    WHERE INTG_CD_ID = 'CD02200'
    AND   INTG_CD_VAL_CTNT = A.SRC_INFO_CD) AS SRC_INFO_NM
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,	A.UPD_DT
FROM PRI_SQ_GRP_LOC_DTL A
,	 MDM_LOCATION B
WHERE	A.QTTN_NO = @[qttn_no]
AND	A.QTTN_VER_NO = @[qttn_ver_no]
AND	A.GRP_LOC_SEQ = @[grp_loc_seq]
AND A.LOC_CD = B.LOC_CD(+)
ORDER BY A.LOC_CD, B.LOC_NM			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="grp_loc_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
