<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchDescRSQL">
			<desc><![CDATA[BKG No로 B/L Description조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(CSTMS_DESC,' ') CSTMS_DESC
      , BDR_FLG
  FROM BKG_BL_DOC
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
