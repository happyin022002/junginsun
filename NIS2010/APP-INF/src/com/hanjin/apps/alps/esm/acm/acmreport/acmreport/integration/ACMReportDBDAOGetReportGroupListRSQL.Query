<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMReportDBDAOGetReportGroupListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT SLCT_ITM_FOM_SEQ,
       SLCT_ITM_FOM_DESC
  FROM AGT_RPT_ITM_INFO_MST
 WHERE CRE_USR_ID = @[usr_id]
 ORDER BY SLCT_ITM_FOM_SEQ
			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
