<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOsearchPkupNtcFormDelListRSQL">
			<desc><![CDATA[searchPkupNtcFormDelList]]></desc>
			<sql><![CDATA[
SELECT *
FROM   (
        SELECT 'ALL' NAME
              ,'ALL' VAL
        FROM   DUAL
        
        UNION
        
        SELECT DECODE(DEL_CD,'*','ALL',DEL_CD) NAME
              ,DECODE(DEL_CD,'*','ALL',DEL_CD) VAL
        FROM   BKG_PKUP_NTC_STUP
        WHERE  PKUP_NTC_SND_TP_CD = @[ntc_snd_tp_cd]
        AND	   OFC_CD             = @[ofc_cd]
       )
ORDER BY DECODE(NAME,'ALL','1',NAME)
			]]></sql>
			<params>
				<param name="ntc_snd_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
