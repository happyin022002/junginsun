<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterRqstNoticeOfcRSQL">
			<desc><![CDATA[Office 정보에서 xter rqst 접수시 email을 받을 주소를 조회해온다.]]></desc>
			<sql><![CDATA[
select decode(mst.doc_tp_cd, 'B', bkg_ntfc_eml, si_ntfc_eml) EML
  from bkg_xter_rqst_mst mst, bkg_Esvc_ofc_eml eml
 where mst.XTER_SNDR_ID = @[sender_id]
   and mst.xter_rqst_no = @[rqst_no]
   and mst.xter_rqst_seq= @[rqst_seq]
   and mst.hndl_ofc_cd  = eml.hndl_ofc_cd			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
