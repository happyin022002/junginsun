<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOSearchApOfcCd2RSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT O.AP_OFC_CD
FROM MDM_ORGANIZATION O, COM_USER U
WHERE 1 = 1
  AND O.DELT_FLG = 'N'
  AND U.OFC_CD = O.OFC_CD
  AND U.USR_ID = @[usr_id]
  AND  O.AP_CTRL_OFC_CD = (SELECT M.AP_CTRL_OFC_CD
                            FROM  MDM_ORGANIZATION M
                            WHERE M.OFC_CD = @[ofc_cd])			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
