<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPropCpPriRpHdrCSQL">
			<desc><![CDATA[Proposal Copy PRI_SP_HDR Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_HDR (
	PROP_NO
,	PRC_PROP_CRE_TP_CD
,	ORG_PROP_NO
,	ORG_AMDT_SEQ
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
-------------------------    RFA 효율화를 위한 요청 (1차) (CHM-201640671) 
,   MST_RFA_NO
,   MST_AMDT_SEQ
)
SELECT 
	@[new_prop_no]
,	'P'
,	@[prop_no]
,	@[amdt_seq]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
-------------------------    RFA 효율화를 위한 요청 (1차) (CHM-201640671) 
#if (${IS_MASTER} != '') 
,	(SELECT RFA_NO   FROM PRI_RP_HDR WHERE PROP_NO = @[prop_no])
,	@[amdt_seq]
#else 
,	(SELECT MST_RFA_NO   FROM PRI_RP_HDR WHERE PROP_NO = @[prop_no])
,	(SELECT MST_AMDT_SEQ FROM PRI_RP_HDR WHERE PROP_NO = @[prop_no])
#end
FROM DUAL			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
