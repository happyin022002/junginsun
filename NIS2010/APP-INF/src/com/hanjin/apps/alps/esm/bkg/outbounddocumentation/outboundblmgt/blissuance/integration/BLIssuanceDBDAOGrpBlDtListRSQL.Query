<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOGrpBlDtListRSQL">
			<desc><![CDATA[GrpBlDtList
2011.04.11 이일민 [CHM-201109940-01] B/L Issue & Onboard date update 보완 요청
2011.04.25 이일민 [CHM-201110395] BL Issue & Onboard date update 화면 보완 요청]]></desc>
			<sql><![CDATA[
select BKG_NO
      ,BL_NO
      ,bl_tp_cd
      ,bl_rdy_tp_cd
      ,CUST_CD
      ,CUST_NM
      ,CNEE_CD
      ,CNEE_NM
      ,FWDR_CD
      ,FWDR_NM
      ,BL_OBRD_TP_CD
      ,BL_OBRD_DT
      ,BL_OBRD_DT_SD
      ,OBL_ISS_FLG
      ,OBL_RLSE_FLG
      ,OBL_ISS_DT
      ,OBL_ISS_DT_SD
      ,OBL_ISS_OFC_CD
      ,OBL_ISS_USR_ID
      ,CRE_USR_ID
      ,UPD_USR_ID
      ,BL_RDY_FLG
      ,credit_chk
      ,CUST_TO_ORD_FLG
      ,FLG_RATE
      ,FLG_MD
      ,FLG_PPD
      ,DO_ISS_DT
      ,FLG_DO
      ,BL_RCV_TP    
      ,BL_RCV_AT
      ,POL_ETD_DT
      ,POL_ETA_DT
      ,CGO_RCV_DT
      ,OBL_SRND_FLG
      ,BL_ISS_NO
      ,POD_CD
      ,DEL_CD
      ,IR_BL_TYPE
      ,IR_BL_TYPE1
      ,OBL_ISS_RMK
      ,ORG_PPD_RCV_CD
      ,CNTC_PSON_EML      
	  ,CASE WHEN MND>0 AND CNTR>0 AND CUST>0 THEN 'Y' ELSE 'N' END OB_INFO_ISS_RDY_FLG
from
(
        SELECT T1.BKG_NO
        ,      T1.BL_NO
        ,      T1.BL_TP_CD
        ,      T2.BL_RDY_TP_CD
        ,      (SELECT CUST_CNT_CD||CUST_SEQ FROM BKG_CUSTOMER WHERE BKG_NO=T1.BKG_NO AND BKG_CUST_TP_CD='S') CUST_CD
        ,      (SELECT REPLACE(REPLACE(CUST_NM, CHR(10), ''), CHR(13), ' ') FROM BKG_CUSTOMER WHERE BKG_NO=T1.BKG_NO AND BKG_CUST_TP_CD='S') CUST_NM
        ,      (SELECT CUST_CNT_CD||CUST_SEQ FROM BKG_CUSTOMER WHERE BKG_NO=T1.BKG_NO AND BKG_CUST_TP_CD='C') CNEE_CD
        ,      (SELECT REPLACE(REPLACE(CUST_NM, CHR(10), ''), CHR(13), ' ') FROM BKG_CUSTOMER WHERE BKG_NO=T1.BKG_NO AND BKG_CUST_TP_CD='C') CNEE_NM
        ,      (SELECT CUST_CNT_CD||CUST_SEQ FROM BKG_CUSTOMER WHERE BKG_NO=T1.BKG_NO AND BKG_CUST_TP_CD='F') FWDR_CD
        ,      (SELECT REPLACE(REPLACE(CUST_NM, CHR(10), ''), CHR(13), ' ') FROM BKG_CUSTOMER WHERE BKG_NO=T1.BKG_NO AND BKG_CUST_TP_CD='F') FWDR_NM
        ,      T1.BL_OBRD_TP_CD
        ,      TO_CHAR (T1.BL_OBRD_DT, 'YYYY-MM-DD') BL_OBRD_DT
        ,      '' BL_OBRD_DT_SD
        ,      NVL(T2.OBL_ISS_FLG,'N') OBL_ISS_FLG
        ,      NVL(T2.OBL_RLSE_FLG,'N') OBL_RLSE_FLG
        ,      TO_CHAR (T2.OBL_ISS_DT, 'YYYY-MM-DD') OBL_ISS_DT
        ,      '' OBL_ISS_DT_SD
        ,      T2.OBL_ISS_OFC_CD
        ,      T2.OBL_ISS_USR_ID
        ,      T2.CRE_USR_ID
        ,      T2.UPD_USR_ID
        ,      NVL(T2.BL_RDY_FLG,'N') BL_RDY_FLG
        ,      '' credit_chk
        ,      T1.CUST_TO_ORD_FLG
        ,      CASE WHEN (
                    SELECT COUNT(BKG_NO)
                    FROM BKG_CHG_RT
                    WHERE BKG_NO=T1.BKG_NO) > 0
               OR (
                    SELECT COUNT(BKG_NO)
                    FROM BKG_RATE
                    WHERE BKG_NO = T1.BKG_NO
                    AND RT_BL_TP_CD IN ('C', 'B')) > 0
               OR (
                    SELECT COUNT(BKG_NO)
                    FROM BKG_BOOKING
                    WHERE BKG_NO = T1.BKG_NO
                    AND CHN_AGN_CD IS NOT NULL) > 0 THEN 'Y' ELSE 'N' END FLG_RATE
        ,	   CASE WHEN (
                    SELECT COUNT(BKG_NO)
                    FROM BKG_BL_MK_DESC
                    WHERE BKG_NO=T1.BKG_NO
                    AND CMDT_DESC IS NOT NULL) > 0 THEN 'Y' ELSE 'N' END FLG_MD
        , 	   CASE WHEN (T2.ORG_PPD_RCV_CD = 'N' AND ( select count(BKG_NO) from BKG_CHG_RT where FRT_TERM_CD = 'P' and N3PTY_RCV_OFC_CD IS NULL AND BKG_NO=T1.BKG_NO ) > 0)  
               OR (T2.ORG_N3PTY_PPD_CD = 'N' AND ( select count(BKG_NO) from BKG_CHG_RT where FRT_TERM_CD = 'P' and N3PTY_RCV_OFC_CD IS NOT NULL AND BKG_NO=T1.BKG_NO) > 0)   
               THEN 'Y' ELSE 'N' END FLG_PPD 
        ,      CASE WHEN 
                    (SELECT EVNT_DT AS DO_ISSUE_DATE
                    FROM BKG_DO_DTL DTL
                    WHERE DTL.BKG_NO = T1.BKG_NO
                    AND DTL.RLSE_STS_CD = DECODE(SUBSTR(T1.DEL_CD, 1, 2), 'JP', 'D', 'R')
                    AND ROWNUM = 1              
                UNION ALL
                    SELECT CGO.MRN_TML_EDI_LST_SND_DT AS DO_ISSUE_DATE
                    FROM BKG_CGO_RLSE CGO,
                    BKG_CSTMS_ADV_BL AMS
                WHERE AMS.BKG_NO = T1.BKG_NO
                AND CGO.FRT_CLT_FLG = 'Y'
                AND CGO.OBL_RDEM_FLG = 'Y'
                AND CGO.CSTMS_CLR_CD = 'Y'
                AND CGO.BL_NO = AMS.BL_NO
                AND AMS.CNT_CD = 'US' 
                AND ROWNUM = 1  ) IS NOT NULL THEN 'Y' ELSE 'N' END DO_ISS_DT
        , 		CASE WHEN (SELECT (SELECT DO_NO FROM BKG_DO WHERE BKG_NO = DTL.BKG_NO  AND RLSE_SEQ= DTL.RLSE_SEQ ) AS DO_ISSUE_NO
                    FROM BKG_DO_DTL DTL,BKG_BOOKING BKG
                    WHERE DTL.BKG_NO = T1.BKG_NO
                    AND BKG.BKG_NO = DTL.BKG_NO
                    AND ROWNUM = 1
                    AND DTL.RLSE_STS_CD = DECODE(SUBSTR(BKG.DEL_CD, 1, 2), 'JP', 'D', 'R')) IS NOT NULL THEN 'Y' ELSE 'N' END AS FLG_DO
        ,		CASE WHEN T2.OTR_DOC_CGOR_FLG = 'Y' THEN 'L' ELSE T1.BL_TP_CD END BL_RCV_TP
        ,		CASE WHEN T2.OTR_DOC_CGOR_FLG = 'Y' THEN T2.OTR_DOC_RCV_OFC_CD ELSE T2.OBL_RDEM_OFC_CD END BL_RCV_AT
        ,		TO_CHAR ((SELECT SKD.VPS_ETD_DT
                    FROM VSK_VSL_PORT_SKD SKD
                    WHERE SKD.VSL_CD = T1.VSL_CD
                    AND SKD.SKD_VOY_NO = T1.SKD_VOY_NO
                    AND SKD.SKD_DIR_CD = T1.SKD_DIR_CD
                    AND SKD.VPS_PORT_CD = T1.POL_CD
                    AND SKD.CLPT_IND_SEQ = T1.POL_CLPT_IND_SEQ ),'YYYY-MM-DD') AS POL_ETD_DT
        ,		TO_CHAR ((SELECT SKD.VPS_ETA_DT
                    FROM VSK_VSL_PORT_SKD SKD
                    WHERE SKD.VSL_CD = T1.VSL_CD
                    AND SKD.SKD_VOY_NO = T1.SKD_VOY_NO
                    AND SKD.SKD_DIR_CD = T1.SKD_DIR_CD
                    AND SKD.VPS_PORT_CD = T1.POL_CD
                    AND SKD.CLPT_IND_SEQ = T1.POL_CLPT_IND_SEQ ),'YYYY-MM-DD') AS POL_ETA_DT 
        ,		TO_CHAR ((SELECT MAX(CGO_RCV_DT) AS CGO_RCV_DT FROM BKG_CONTAINER
                WHERE BKG_NO = T1.BKG_NO),'YYYY-MM-DD') AS CGO_RCV_DT
        ,		NVL(T2.OBL_SRND_FLG,'N') AS OBL_SRND_FLG
        ,       T2.BL_CPY_KNT AS BL_ISS_NO
        ,       POD_CD
        ,		DEL_CD
        ,       NVL(T1.BL_TP_CD,'B') AS IR_BL_TYPE
        ,       CASE WHEN T1.BL_TP_CD = 'W' THEN 'SWB'
                     WHEN (SELECT MAX(N1ST_PRN_DT||N2ND_PRN_DT) 
                             FROM BKG_INET_BL_PRN_AUTH 
                            WHERE BKG_NO = T1.BKG_NO) is not null THEN 'Web OB/L'
                    WHEN T2.OBL_RLSE_FLG = 'Y' THEN 'OB/L'
                    ELSE 'B'
                END AS IR_BL_TYPE1
        ,       T2.OBL_ISS_RMK
        ,       NVL(T2.ORG_PPD_RCV_CD,'N') AS ORG_PPD_RCV_CD
        ,       (SELECT CNTC_PSON_EML FROM BKG_CNTC_PSON WHERE BKG_NO = T1.BKG_NO AND BKG_CNTC_PSON_TP_CD = 'SI') AS CNTC_PSON_EML  
        ,       (SELECT     --A.BKG_NO BKG_NO, 
                                --DECODE((NVL(B.PCK_QTY, '0')||NVL(B.ACT_WGT, '0')||NVL(B.CSTMS_DESC, '*')), '00*', 0, 1) MND_CNT 
                                DECODE(NVL(B.CSTMS_DESC, '*'), '*', 0, 1) MND_CNT 
                     FROM       BKG_BOOKING A, BKG_BL_DOC B
                     WHERE      A.BKG_NO = T1.BKG_NO
                     AND        A.BKG_NO = B.BKG_NO(+) ) MND
        ,       (SELECT     --A.BKG_NO, 
                                NVL(COUNT(B.CNTR_NO), 0) CNTR_CNT
                     FROM       BKG_BOOKING A, BKG_CONTAINER B
                     WHERE      A.BKG_NO = T1.BKG_NO
                     AND        A.BKG_NO = B.BKG_NO(+)
                     GROUP BY   A.BKG_NO ) CNTR
        ,       (SELECT     --A.BKG_NO, 
                                DECODE(NVL(CUST_NM, '*'), '*', 0, 1) CUST_CNT
                     FROM       BKG_BOOKING A, BKG_CUSTOMER B
                     WHERE      A.BKG_NO = T1.BKG_NO
                     AND        A.BKG_NO = B.BKG_NO(+)
                     AND        BKG_CUST_TP_CD(+) = 'C') CUST
        FROM   (SELECT BKG_NO
        ,              BL_NO
        ,              BL_TP_CD
        ,              BL_OBRD_TP_CD
        ,              BL_OBRD_DT
        ,              CUST_TO_ORD_FLG
        ,              DEL_CD
        ,              VSL_CD
        ,              SKD_VOY_NO
        ,              SKD_DIR_CD
        ,              POL_CD
        ,              POL_CLPT_IND_SEQ
        ,              VVD_SEQ
        ,              POD_CD
                FROM   (SELECT A.BKG_NO
        ,                      A.BL_NO
        ,                      A.BL_TP_CD
        ,                      B.BL_OBRD_TP_CD
        ,                      B.BL_OBRD_DT
        ,                      ROW_NUMBER() OVER (PARTITION BY A.BKG_NO ORDER BY C.VSL_PRE_PST_CD || C.VSL_SEQ) AS NUM
        ,                      A.CUST_TO_ORD_FLG
        ,                      A.DEL_CD
        ,                      C.VSL_CD
        ,                      C.SKD_VOY_NO
        ,                      C.SKD_DIR_CD
        ,                      C.POL_CD
        ,                      C.POL_CLPT_IND_SEQ
        ,                      C.VSL_PRE_PST_CD || C.VSL_SEQ  AS VVD_SEQ
        ,                      A.POD_CD
                        FROM   BKG_BOOKING A, BKG_BL_DOC B, BKG_VVD C
                        WHERE  A.BKG_NO = B.BKG_NO
                        AND    B.BKG_NO = C.BKG_NO
        #if (${bkg_no} != '')
                        AND    A.BKG_NO IN ( ${bkg_no} )
        #end
                        AND    A.BKG_STS_CD <> 'X'
                        AND    A.BKG_CGO_TP_CD <> 'P'
                        AND    A.POL_CD = C.POL_CD
        #if (${bkg_ofc_cd} != '')
                        AND    A.BKG_OFC_CD= @[bkg_ofc_cd]
        #end
        #if (${rfa_no} != '')
                        AND    A.RFA_NO = @[rfa_no]
        #end
        #if (${sc_no} != '')
                        AND    A.SC_NO = @[sc_no]
        #end
        #if (${ob_srep_cd} != '')
                        AND    A.OB_SREP_CD = @[ob_srep_cd]
        #end
        #if (${bkg_from_dt} != '')
            #if (${type_date} == 'B')
                        AND A.BKG_CRE_DT >= TO_DATE(REPLACE(@[bkg_from_dt],'-',''),'RRRRMMDD')
            #elseif (${type_date} == 'O')         
                        AND B.BL_OBRD_DT >= TO_DATE(REPLACE(@[bkg_from_dt],'-',''),'RRRRMMDD')
            #end
        #end
        #if (${bkg_to_dt} != '')
            #if (${type_date} == 'B')
                        AND A.BKG_CRE_DT < TO_DATE(REPLACE(@[bkg_to_dt],'-',''),'RRRRMMDD') + 1
            #elseif (${type_date} == 'O')  
                        AND B.BL_OBRD_DT < TO_DATE(REPLACE(@[bkg_to_dt],'-',''),'RRRRMMDD') + 1
            #end
        #end
        #if (${shipper_cd} != '')
            #if (${cust_seq} != '') 
                AND    EXISTS ( SELECT 'Y' FROM BKG_CUSTOMER WHERE BKG_NO = A.BKG_NO AND CUST_CNT_CD=@[cust_cnt_cd] AND CUST_SEQ=@[cust_seq] AND BKG_CUST_TP_CD='S' AND ROWNUM = 1 )
            #else 
                AND    EXISTS ( SELECT 'Y' FROM BKG_CUSTOMER WHERE BKG_NO = A.BKG_NO AND CUST_CNT_CD=@[cust_cnt_cd] AND BKG_CUST_TP_CD='S' AND ROWNUM = 1 )
            #end
        #end
        #if (${fwdr_cd} != '')
            #if (${f_cust_seq} != '') 
                AND    EXISTS ( SELECT 'Y' FROM BKG_CUSTOMER WHERE BKG_NO = A.BKG_NO AND CUST_CNT_CD=@[f_cust_cnt_cd] AND CUST_SEQ=@[f_cust_seq] AND BKG_CUST_TP_CD='F' AND ROWNUM = 1 )
            #else 
                AND    EXISTS ( SELECT 'Y' FROM BKG_CUSTOMER WHERE BKG_NO = A.BKG_NO AND CUST_CNT_CD=@[f_cust_cnt_cd] AND BKG_CUST_TP_CD='F' AND ROWNUM = 1 )
            #end
        #end
        #if (${cnee_cd} != '')
            #if (${cnee_seq} != '') 
                AND    EXISTS ( SELECT 'Y' FROM BKG_CUSTOMER WHERE BKG_NO = A.BKG_NO AND CUST_CNT_CD=@[cnee_cd] AND CUST_SEQ=@[cnee_seq] AND BKG_CUST_TP_CD='C' AND ROWNUM = 1 )
            #else 
                AND    EXISTS ( SELECT 'Y' FROM BKG_CUSTOMER WHERE BKG_NO = A.BKG_NO AND CUST_CNT_CD=@[cnee_cd] AND BKG_CUST_TP_CD='C' AND ROWNUM = 1 )
            #end
        #end
        #if (${vvd} != '')
                        AND    C.VSL_CD = @[vsl_cd]
                        AND    C.SKD_VOY_NO = @[voy_no]
                        AND    C.SKD_DIR_CD = @[dir_cd]
                        AND    C.POL_CD = @[pol_cd]
                        AND    C.POL_CLPT_IND_SEQ = @[pol_clpt_ind_seq]
        #end
        #if (${trunk_vvd} != '')
                        AND    A.VSL_CD = @[trunk_vsl_cd]
                        AND    A.SKD_VOY_NO = @[trunk_voy_no]
                        AND    A.SKD_DIR_CD = @[trunk_dir_cd]                
        #end
                        ORDER BY C.VSL_PRE_PST_CD, C.VSL_SEQ)
                WHERE  NUM = 1) T1, BKG_BL_ISS T2
        WHERE  T1.BKG_NO = T2.BKG_NO(+)
        #if (${obl_iss_flg} == 'Y') 
        AND    @[obl_iss_flg] = T2.OBL_ISS_FLG 
        #end
        #if (${obl_rlse_flg} == 'Y')
        AND    @[obl_rlse_flg] = T2.OBL_RLSE_FLG 
        #end
        #if (${bl_rdy_flg} == 'Y')
        AND    @[bl_rdy_flg] = T2.BL_RDY_FLG
        #end
        #if (${obl_iss_flg} == 'N') 
        AND    (@[obl_iss_flg] = T2.OBL_ISS_FLG OR T2.BKG_NO IS NULL)
        #end
        #if (${obl_rlse_flg} == 'N')
        AND    (@[obl_rlse_flg] = T2.OBL_RLSE_FLG OR T2.OBL_RLSE_FLG IS NULL)
        #end
        #if (${bl_rdy_flg} == 'N')
        AND    (@[bl_rdy_flg] = T2.BL_RDY_FLG OR T2.BL_RDY_FLG IS NULL)
        #end
)			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="ob_srep_cd" type="12" value="" out="N"/>
				<param name="bkg_from_dt" type="12" value="" out="N"/>
				<param name="bkg_to_dt" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="f_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="f_cust_seq" type="12" value="" out="N"/>
				<param name="cnee_cd" type="12" value="" out="N"/>
				<param name="cnee_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="trunk_vsl_cd" type="12" value="" out="N"/>
				<param name="trunk_voy_no" type="12" value="" out="N"/>
				<param name="trunk_dir_cd" type="12" value="" out="N"/>
				<param name="obl_iss_flg" type="12" value="" out="N"/>
				<param name="obl_rlse_flg" type="12" value="" out="N"/>
				<param name="bl_rdy_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
