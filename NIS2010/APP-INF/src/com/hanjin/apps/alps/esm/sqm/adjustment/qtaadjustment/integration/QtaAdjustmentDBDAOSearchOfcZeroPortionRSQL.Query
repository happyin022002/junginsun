<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOSearchOfcZeroPortionRSQL">
			<desc><![CDATA[Adjustment by VVD, Portion Adjustement by HO, RHQ 화면에서 Creation 전에 
RHQ에는 Portion을 부여했으나 
해당 RHQ 산하의 Office에게 Portion을 부여하지 않은 RHQ List를 조회합니다.

* 2015.06.15 김용습 [CHM-201536164] 주간 MAS Open에 따른 타모듈 프로그램 적용 요청]]></desc>
			<sql><![CDATA[
SELECT DECODE(D.OFC_VW_CD,'C','Contract','L','Loading')||'/'||D.TRD_CD||'/'||D.RLANE_CD||'/'||D.DIR_CD||'B/'||D.HUL_BND_CD||'/'||D.RHQ_CD AS OfcZeroPortion
FROM (
--02에게 Portion을 배부한것만 추림
        SELECT NVL(A.OFC_VW_CD,  B.OFC_VW_CD)   OFC_VW_CD
              ,NVL(A.TRD_CD,     B.TRD_CD)      TRD_CD
              ,NVL(A.SUB_TRD_CD, B.SUB_TRD_CD)  SUB_TRD_CD
              ,NVL(A.RLANE_CD,   B.RLANE_CD)    RLANE_CD
              ,NVL(A.DIR_CD,     B.DIR_CD)      DIR_CD
              ,NVL(A.RHQ_CD,     B.RHQ_CD)      RHQ_CD
              ,NVL(A.HUL_BND_CD, B.HUL_BND_CD)  HUL_BND_CD
        FROM (
                SELECT DISTINCT A1.*, A3.HUL_BND_CD
                FROM SQM_CFM_QTA_POTN_MGMT A1, SQM_QTA_LANE_OFC A2, MAS_LANE_RGST A3, SQM_DAT_RLT A4
                WHERE 1=1
                AND A1.BSE_TP_CD    = @[f_bse_tp_cd]
                AND A1.BSE_YR       = @[f_bse_yr]
                AND A1.BSE_QTR_CD   = DECODE(@[f_bse_tp_cd],'Y','00',@[f_bse_qtr_cd])
#if (${f_ofc_vw_cd} != '' && ${f_ofc_vw_cd} != 'All')
                AND A1.OFC_VW_CD    = @[f_ofc_vw_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
                AND A1.TRD_CD       = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                AND A1.RLANE_CD     IN (${f_rlane_cd})
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                AND A1.DIR_CD       = @[f_dir_cd]
#end
#if (${f_trd_dir} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
                AND A3.HUL_BND_CD   = @[f_hul_bnd_cd]
#end
#if (${f_click} != 'on' && ${f_conv_dir_cd} != '' && ${f_conv_dir_cd} != 'All')
                AND A1.CONV_DIR_CD  = @[f_conv_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
                AND A3.HUL_BND_CD   = @[f_trd_dir]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
                AND A4.OB_DIV_CD    = @[f_ob_div_cd]
#end
                AND A1.QTA_STEP_CD  = '02'
                AND A1.LOD_POTN_RTO <> 0
                
                AND A1.BSE_TP_CD    = A2.BSE_TP_CD
                AND A1.BSE_YR       = A2.BSE_YR
                AND A1.BSE_QTR_CD   = A2.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A2.OFC_VW_CD
                AND A1.TRD_CD       = A2.TRD_CD
                AND A1.RLANE_CD     = A2.RLANE_CD
                AND A1.DIR_CD       = A2.DIR_CD
                AND A1.RHQ_CD       = A2.RHQ_CD
                AND A2.SQM_ACT_FLG = 'Y'

                AND A1.TRD_CD       = A3.TRD_CD
                AND A1.RLANE_CD     = A3.RLANE_CD
                AND A1.DIR_CD       = A3.DIR_CD
                AND A3.DELT_FLG     = 'N'

                AND A1.BSE_TP_CD    = A4.BSE_TP_CD
                AND A1.BSE_YR       = A4.BSE_YR
                AND A1.BSE_QTR_CD   = A4.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A4.OFC_VW_CD
                AND A1.TRD_CD       = A4.TRD_CD
                AND A1.CONV_DIR_CD  = A4.CONV_DIR_CD
                AND A1.RHQ_CD       = A4.RHQ_CD
             )A 
        FULL OUTER JOIN 
            (
                SELECT DISTINCT A1.*, A3.HUL_BND_CD
                FROM SQM_QTA_POTN_MGMT A1, SQM_QTA_LANE_OFC A2, MAS_LANE_RGST A3, SQM_DAT_RLT A4
                WHERE 1=1
                AND A1.BSE_TP_CD    = @[f_bse_tp_cd]
                AND A1.BSE_YR       = @[f_bse_yr]
                AND A1.BSE_QTR_CD   = DECODE(@[f_bse_tp_cd],'Y','00',@[f_bse_qtr_cd])
#if (${f_ofc_vw_cd} != '' && ${f_ofc_vw_cd} != 'All')
                AND A1.OFC_VW_CD    = @[f_ofc_vw_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
                AND A1.TRD_CD       = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                AND A1.RLANE_CD     IN (${f_rlane_cd})
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                AND A1.DIR_CD       = @[f_dir_cd]
#end
#if (${f_trd_dir} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
                AND A3.HUL_BND_CD   = @[f_hul_bnd_cd]
#end
#if (${f_click} != 'on' && ${f_conv_dir_cd} != '' && ${f_conv_dir_cd} != 'All')
                AND A1.CONV_DIR_CD  = @[f_conv_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
                AND A3.HUL_BND_CD   = @[f_trd_dir]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
                AND A4.OB_DIV_CD    = @[f_ob_div_cd]
#end
                AND A1.QTA_STEP_CD  = '02'
                AND A1.LOD_POTN_RTO <> 0
                
                AND A1.BSE_TP_CD    = A2.BSE_TP_CD
                AND A1.BSE_YR       = A2.BSE_YR
                AND A1.BSE_QTR_CD   = A2.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A2.OFC_VW_CD
                AND A1.TRD_CD       = A2.TRD_CD
                AND A1.RLANE_CD     = A2.RLANE_CD
                AND A1.DIR_CD       = A2.DIR_CD
                AND A1.RHQ_CD       = A2.RHQ_CD
                AND A2.SQM_ACT_FLG = 'Y'

                AND A1.TRD_CD       = A3.TRD_CD
                AND A1.RLANE_CD     = A3.RLANE_CD
                AND A1.DIR_CD       = A3.DIR_CD
                AND A3.DELT_FLG     = 'N'

                AND A1.BSE_TP_CD    = A4.BSE_TP_CD
                AND A1.BSE_YR       = A4.BSE_YR
                AND A1.BSE_QTR_CD   = A4.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A4.OFC_VW_CD
                AND A1.TRD_CD       = A4.TRD_CD
                AND A1.CONV_DIR_CD  = A4.CONV_DIR_CD
                AND A1.RHQ_CD       = A4.RHQ_CD
            )B
        ON  A.BSE_TP_CD    = B.BSE_TP_CD 
        AND A.BSE_YR       = B.BSE_YR     
        AND A.BSE_QTR_CD   = B.BSE_QTR_CD 
        AND A.OFC_VW_CD    = B.OFC_VW_CD 
        AND A.QTA_STEP_CD  = B.QTA_STEP_CD
        AND A.TRD_CD       = B.TRD_CD    
        AND A.RLANE_CD     = B.RLANE_CD  
        AND A.DIR_CD       = B.DIR_CD     
        AND A.RHQ_CD       = B.RHQ_CD     
        AND A.RGN_OFC_CD   = B.RGN_OFC_CD
        AND A.HUL_BND_CD   = B.HUL_BND_CD
     ) C
--03,04,05 의 SUM이 0인것     
    ,(
        SELECT NVL(A.OFC_VW_CD,   B.OFC_VW_CD)      OFC_VW_CD
              ,NVL(A.QTA_STEP_CD, B.QTA_STEP_CD)    QTA_STEP_CD
              ,NVL(A.TRD_CD,      B.TRD_CD)         TRD_CD
              ,NVL(A.SUB_TRD_CD,  B.SUB_TRD_CD)     SUB_TRD_CD
              ,NVL(A.RLANE_CD,    B.RLANE_CD)       RLANE_CD
              ,NVL(A.DIR_CD,      B.DIR_CD)         DIR_CD
              ,NVL(A.RHQ_CD,      B.RHQ_CD)         RHQ_CD
              ,NVL(A.HUL_BND_CD,  B.HUL_BND_CD)     HUL_BND_CD
              ,NVL(SUM(A.LOD_POTN_RTO), SUM(B.LOD_POTN_RTO)) LOD_POTN_RTO 
        FROM (
                SELECT DISTINCT A1.*, A3.HUL_BND_CD
                FROM SQM_CFM_QTA_POTN_MGMT A1, SQM_QTA_LANE_OFC A2, MAS_LANE_RGST A3, SQM_DAT_RLT A4
                WHERE 1=1
                AND A1.BSE_TP_CD    = @[f_bse_tp_cd]
                AND A1.BSE_YR       = @[f_bse_yr]
                AND A1.BSE_QTR_CD   = DECODE(@[f_bse_tp_cd],'Y','00',@[f_bse_qtr_cd])
#if (${f_ofc_vw_cd} != '' && ${f_ofc_vw_cd} != 'All')
                AND A1.OFC_VW_CD    = @[f_ofc_vw_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
                AND A1.TRD_CD       = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                AND A1.RLANE_CD     IN (${f_rlane_cd})
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                AND A1.DIR_CD       = @[f_dir_cd]
#end
#if (${f_trd_dir} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
                AND A3.HUL_BND_CD   = @[f_hul_bnd_cd]
#end
#if (${f_click} != 'on' && ${f_conv_dir_cd} != '' && ${f_conv_dir_cd} != 'All')
                AND A1.CONV_DIR_CD  = @[f_conv_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
                AND A3.HUL_BND_CD   = @[f_trd_dir]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
                AND A4.OB_DIV_CD    = @[f_ob_div_cd]
#end
                AND A1.QTA_STEP_CD  <> '02'
                
                AND A1.BSE_TP_CD    = A2.BSE_TP_CD
                AND A1.BSE_YR       = A2.BSE_YR
                AND A1.BSE_QTR_CD   = A2.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A2.OFC_VW_CD
                AND A1.TRD_CD       = A2.TRD_CD
                AND A1.RLANE_CD     = A2.RLANE_CD
                AND A1.DIR_CD       = A2.DIR_CD
                AND A1.RHQ_CD       = A2.RHQ_CD
                AND A1.RGN_OFC_CD   = A2.RGN_OFC_CD
                AND A2.SQM_ACT_FLG = 'Y'

                AND A1.TRD_CD       = A3.TRD_CD
                AND A1.RLANE_CD     = A3.RLANE_CD
                AND A1.DIR_CD       = A3.DIR_CD
                AND A3.DELT_FLG     = 'N'

                AND A1.BSE_TP_CD    = A4.BSE_TP_CD
                AND A1.BSE_YR       = A4.BSE_YR
                AND A1.BSE_QTR_CD   = A4.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A4.OFC_VW_CD
                AND A1.TRD_CD       = A4.TRD_CD
                AND A1.CONV_DIR_CD  = A4.CONV_DIR_CD
                AND A1.RHQ_CD       = A4.RHQ_CD

             )A 
        FULL OUTER JOIN 
            (
                SELECT DISTINCT A1.*, A3.HUL_BND_CD
                FROM SQM_QTA_POTN_MGMT A1, SQM_QTA_LANE_OFC A2, MAS_LANE_RGST A3, SQM_DAT_RLT A4
                WHERE 1=1
                AND A1.BSE_TP_CD    = @[f_bse_tp_cd]
                AND A1.BSE_YR       = @[f_bse_yr]
                AND A1.BSE_QTR_CD   = DECODE(@[f_bse_tp_cd],'Y','00',@[f_bse_qtr_cd])
#if (${f_ofc_vw_cd} != '' && ${f_ofc_vw_cd} != 'All')
                AND A1.OFC_VW_CD    = @[f_ofc_vw_cd]
#end
#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
                AND A1.TRD_CD       = @[f_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                AND A1.RLANE_CD     IN (${f_rlane_cd})
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                AND A1.DIR_CD       = @[f_dir_cd]
#end
#if (${f_trd_dir} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
                AND A3.HUL_BND_CD   = @[f_hul_bnd_cd]
#end
#if (${f_click} != 'on' && ${f_conv_dir_cd} != '' && ${f_conv_dir_cd} != 'All')
                AND A1.CONV_DIR_CD  = @[f_conv_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
                AND A3.HUL_BND_CD   = @[f_trd_dir]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
                AND A4.OB_DIV_CD    = @[f_ob_div_cd]
#end
                AND A1.QTA_STEP_CD  <> '02'
                
                AND A1.BSE_TP_CD    = A2.BSE_TP_CD
                AND A1.BSE_YR       = A2.BSE_YR
                AND A1.BSE_QTR_CD   = A2.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A2.OFC_VW_CD
                AND A1.TRD_CD       = A2.TRD_CD
                AND A1.RLANE_CD     = A2.RLANE_CD
                AND A1.DIR_CD       = A2.DIR_CD
                AND A1.RHQ_CD       = A2.RHQ_CD
                AND A1.RGN_OFC_CD   = A2.RGN_OFC_CD
                AND A2.SQM_ACT_FLG = 'Y'

                AND A1.TRD_CD       = A3.TRD_CD
                AND A1.RLANE_CD     = A3.RLANE_CD
                AND A1.DIR_CD       = A3.DIR_CD
                AND A3.DELT_FLG     = 'N'

                AND A1.BSE_TP_CD    = A4.BSE_TP_CD
                AND A1.BSE_YR       = A4.BSE_YR
                AND A1.BSE_QTR_CD   = A4.BSE_QTR_CD
                AND A1.OFC_VW_CD    = A4.OFC_VW_CD
                AND A1.TRD_CD       = A4.TRD_CD
                AND A1.CONV_DIR_CD  = A4.CONV_DIR_CD
                AND A1.RHQ_CD       = A4.RHQ_CD

            )B
        ON  A.BSE_TP_CD    = B.BSE_TP_CD 
        AND A.BSE_YR       = B.BSE_YR     
        AND A.BSE_QTR_CD   = B.BSE_QTR_CD 
        AND A.OFC_VW_CD    = B.OFC_VW_CD 
        AND A.QTA_STEP_CD  = B.QTA_STEP_CD
        AND A.TRD_CD       = B.TRD_CD    
        AND A.RLANE_CD     = B.RLANE_CD  
        AND A.DIR_CD       = B.DIR_CD     
        AND A.RHQ_CD       = B.RHQ_CD     
        AND A.RGN_OFC_CD   = B.RGN_OFC_CD
        AND A.HUL_BND_CD   = B.HUL_BND_CD
        GROUP BY NVL(A.OFC_VW_CD,   B.OFC_VW_CD) 
                ,NVL(A.QTA_STEP_CD, B.QTA_STEP_CD) 
                ,NVL(A.TRD_CD,      B.TRD_CD) 
                ,NVL(A.SUB_TRD_CD,  B.SUB_TRD_CD)
                ,NVL(A.RLANE_CD,    B.RLANE_CD)
                ,NVL(A.DIR_CD,      B.DIR_CD) 
                ,NVL(A.RHQ_CD,      B.RHQ_CD)
                ,NVL(A.HUL_BND_CD,  B.HUL_BND_CD)
        HAVING NVL(SUM(A.LOD_POTN_RTO), SUM(B.LOD_POTN_RTO)) = 0
        ORDER BY NVL(A.OFC_VW_CD,   B.OFC_VW_CD) 
                ,NVL(A.QTA_STEP_CD, B.QTA_STEP_CD) 
                ,NVL(A.TRD_CD,      B.TRD_CD) 
                ,NVL(A.SUB_TRD_CD,  B.SUB_TRD_CD)
                ,NVL(A.RLANE_CD,    B.RLANE_CD) 
                ,NVL(A.DIR_CD,      B.DIR_CD) 
                ,NVL(A.RHQ_CD,      B.RHQ_CD)
           
    
     ) D
WHERE C.OFC_VW_CD   = D.OFC_VW_CD
AND   C.TRD_CD      = D.TRD_CD
AND   C.SUB_TRD_CD  = D.SUB_TRD_CD 
AND   C.RLANE_CD    = D.RLANE_CD
AND   C.DIR_CD      = D.DIR_CD
AND   C.RHQ_CD      = D.RHQ_CD
AND   C.HUL_BND_CD  = D.HUL_BND_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_hul_bnd_cd" type="12" value="" out="N"/>
				<param name="f_conv_dir_cd" type="12" value="" out="N"/>
				<param name="f_trd_dir" type="12" value="" out="N"/>
				<param name="f_ob_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
