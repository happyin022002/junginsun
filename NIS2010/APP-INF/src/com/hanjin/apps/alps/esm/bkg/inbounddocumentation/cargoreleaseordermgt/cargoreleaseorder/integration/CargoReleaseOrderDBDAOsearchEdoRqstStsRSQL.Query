<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchEdoRqstStsRSQL">
			<desc><![CDATA[KT-NET을 통해 들어온 E-DO 승인 요청 정보에 대한 Ststus를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
X.*
, 
CASE ( SELECT  COUNT(1)
			  FROM  BKG_EDO_CNTR_PTY_TRSP EC
			 WHERE EC.EDO_RQST_NO = X.EDO_RQST_NO
			 AND ROWNUM =1
      )
WHEN 0 THEN 'N'
ELSE 'Y'
END AS CNTR_SELF_TRSP_FLAG
 
FROM (
SELECT IMST.EDO_RQST_NO           AS EDO_RQST_NO           -- Request 번호
     , MAX(IMST.EDO_RQST_SEQ_5JN) AS EDO_RQST_SEQ_5JN -- D/O 발급 요청 SEQ 번호
     , MAX(IMST.EDO_RQST_SEQ_5JM) AS EDO_RQST_SEQ_5JM -- 자가운송신청서 SEQ 번호
     , MAX(IMST.EDO_RQST_SEQ_5JK) AS EDO_RQST_SEQ_5JK -- 보세운송요청동의서 SEQ 번호   
     , MAX(M5JN.EDO_TP_CD)        AS DO_ISSUE              -- D/O 발급 요청
     , MAX(M5JN.EDO_ACK_CD)       AS DO_EDO_ACK_CD         -- D/O 발급 요청 ACK
     , MAX(M5JM.EDO_TP_CD)        AS SELF_TRANSPORTATION   -- 자가운송신청서
     , MAX(M5JM.EDO_ACK_CD)       AS SELF_EDO_ACK_CD       -- 자가운송신청서 ACK
     , MAX(M5JK.EDO_TP_CD)        AS BONDED_TRANSPORTATION -- 보세운송요청동의서
     , MAX(M5JK.EDO_ACK_CD)       AS BONDED_EDO_ACK_CD     -- 보세운송요청동의서 ACK
     , MAX(EPTY.PTY_RGST_NO)      AS MS_PTY_RGST_NO        -- D/O 신청자 
     , MAX(RPTY.PTY_RGST_NO)      AS PR_PTY_RGST_NO        -- 세금계산서 공급 받는자  
     , MAX(RPTY.PTY_EML)          AS PR_PTY_EML            -- 세금계산서 공급 받는자 email 
     , (SELECT DECODE(COUNT(*),0,'N','Y') 
        FROM BKG_EDO_LOG
        WHERE BKG_NO      = @[bkg_no]
        AND   EDO_RSLT_CD = 'E'
        AND   ROWNUM=1 )          AS ERR_FLG
FROM ( SELECT /*+ USE_NL(IMST_MAX IMST) */
                SUBSTR(MAX(TO_CHAR(IMST.EDO_RCT_DT,'YYYYMMDDHH24MISS')||IMST.EDO_RQST_NO),15) AS EDO_RQST_NO
              , IMST.EDO_TP_CD
              , SUBSTR(MAX( NVL(TO_CHAR(IMST.EDO_RCT_DT,'YYYYMMDDHH24MISS'), '00000000000000') ||CASE WHEN IMST.EDO_TP_CD = '5JN' THEN IMST.EDO_RQST_SEQ END),15) AS EDO_RQST_SEQ_5JN
              , SUBSTR(MAX( NVL(TO_CHAR(IMST.EDO_RCT_DT,'YYYYMMDDHH24MISS'), '00000000000000') ||CASE WHEN IMST.EDO_TP_CD = '5JM' THEN IMST.EDO_RQST_SEQ END),15) AS EDO_RQST_SEQ_5JM
              , SUBSTR(MAX( NVL(TO_CHAR(IMST.EDO_RCT_DT,'YYYYMMDDHH24MISS'), '00000000000000') ||CASE WHEN IMST.EDO_TP_CD = '5JK' THEN IMST.EDO_RQST_SEQ END),15) AS EDO_RQST_SEQ_5JK
           FROM BKG_EDO_MST IMST
          WHERE IMST.BKG_NO  = @[bkg_no]
            AND NVL(IMST.DELT_FLG, ' ') <> 'Y'
          GROUP BY IMST.EDO_TP_CD
       )IMST
     , BKG_EDO_MST M5JN -- DO 신청
     , BKG_EDO_MST M5JM -- 자가운송
     , BKG_EDO_MST M5JK -- 보세운송
     , BKG_EDO_PTY_TRSP EPTY  -- 신청자 
     , BKG_EDO_PTY_TRSP RPTY  -- 세금계산서 공급 받는자
 WHERE IMST.EDO_RQST_NO      = M5JN.EDO_RQST_NO(+)
   AND IMST.EDO_RQST_SEQ_5JN = M5JN.EDO_RQST_SEQ(+)
   AND IMST.EDO_RQST_NO      = M5JM.EDO_RQST_NO(+)
   AND IMST.EDO_RQST_SEQ_5JM = M5JM.EDO_RQST_SEQ(+)
   AND IMST.EDO_RQST_NO      = M5JK.EDO_RQST_NO(+)
   AND IMST.EDO_RQST_SEQ_5JK = M5JK.EDO_RQST_SEQ(+)
   AND EPTY.EDO_RQST_NO(+)   = IMST.EDO_RQST_NO      
   AND EPTY.EDO_PTY_CD(+)    = 'MS'
   AND EPTY.EDO_RQST_SEQ(+)  = IMST.EDO_RQST_SEQ_5JN    
   AND RPTY.EDO_RQST_NO(+)   = IMST.EDO_RQST_NO      
   AND RPTY.EDO_PTY_CD(+)    = 'PR'
   AND RPTY.EDO_RQST_SEQ(+)  = IMST.EDO_RQST_SEQ_5JN    
 GROUP BY IMST.EDO_RQST_NO
) X
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
