<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAOSearchDashboardReportColumnRSQL">
			<desc><![CDATA[Report Item Option 화면에서 옵션을 설정하기 위해 먼저 불러오는 레포트 리스트 Retrieve 문]]></desc>
			<sql><![CDATA[
SELECT
A.RPT_ID
,   A.DBD_IRR_TP_CD
,	A.COL_NM 
,	A.DP_NM  
,	A.DP_SEQ 
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,	A.UPD_DT
FROM BKG_DBD_RPT_COL A
ORDER BY A.DP_SEQ 			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
