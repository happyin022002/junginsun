<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOOwnersAccountConsultationCancellationCSQL">
			<desc><![CDATA[Owner's Account Cancellation메인 전표 저장   ]]></desc>
			<sql><![CDATA[
INSERT INTO FMS_CONSULTATION (
	SLP_TP_CD
,	SLP_FUNC_CD
,	SLP_OFC_CD
,	SLP_ISS_DT
,	SLP_SER_NO	-- 5
,	CSR_CURR_CD
,	CSR_AMT
,	CSR_USR_ID
,	CSR_DESC
,   DIFF_AMT	-- 10
,   RQST_AMT
,	RQST_DT
,	EFF_DT
,	EVID_TP_CD
,	CRE_USR_ID	-- 15
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	OA_INTER_MM_DESC
,	OA_INV_DT	-- 20
,   OA_IF_FLG
) VALUES( 
	@[slp_tp_cd]
,	@[slp_func_cd]
,	@[slp_ofc_cd]
,	@[slp_iss_dt]
,	@[slp_ser_no]	-- 5
,	@[csr_curr_cd]
,	@[csr_amt]
,	@[cre_usr_id]
,	@[csr_desc]
,	0	-- 10
,	@[csr_amt]
,	(SELECT TO_CHAR(SYSDATE + (SELECT DECODE(SUBSTR(GEN_PAY_TERM_CD, 1, 1), 'O', 0, GEN_PAY_TERM_CD) GEN_PAY_TERM_CD
                                 FROM MDM_VENDOR
                           		WHERE VNDR_SEQ = @[vndr_seq]) , 'YYYYMMDD')
  	   FROM DUAL)
,	(SELECT TO_CHAR(DECODE(A.CNT, 1, MAX_EFF, MIN_EFF), 'YYYYMMDD') EFF_DT
  	   FROM (SELECT MIN(LAST_DAY(TO_DATE(EFF_YRMON, 'YYYYMM'))) MIN_EFF,
               MAX(TRUNC(TO_DATE(EFF_YRMON, 'YYYYMM'), 'MONTH')) MAX_EFF,
               COUNT(P.EFF_YRMON) CNT
          	   FROM AP_PERIOD P
         	  WHERE P.SYS_DIV_CD = '17'
           		AND P.AR_AP_DIV_CD = 'P'
           		AND P.CLZ_STS_CD = 'O'
           		AND OFC_CD = (SELECT S.AR_HD_QTR_OFC_CD
                  				FROM MDM_ORGANIZATION S
                 			   WHERE S.OFC_CD = @[slp_ofc_cd]) ) A
	)
,	'5'
,	@[cre_usr_id]	-- 15
,	SYSDATE
,	@[cre_usr_id]
,	SYSDATE
,	@[oa_inter_mm_desc]
,	(SELECT TO_CHAR(SYSDATE, 'YYYYMMDD')
  	   FROM DUAL)	-- 20
,   'Y'
)			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_iss_dt" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="csr_curr_cd" type="12" value="" out="N"/>
				<param name="csr_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="csr_desc" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="oa_inter_mm_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
