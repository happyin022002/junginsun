<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurCustomsTransmissionDBDAOsearchIERcvAckRSQL">
			<desc><![CDATA[Received Ack from Irish Manifest]]></desc>
			<sql><![CDATA[
SELECT RCV.MSG_ACK_REF_NO
FROM BKG_CSTMS_EUR_DG_RCV RCV
WHERE  RCV.EUR_EDI_MSG_TP_ID = 'CTA'
AND RCV.CRE_USR_ID ='IEREVENUE'
AND RCV.MSG_ACK_CD ='128'
AND RCV.VVD_CD = @[vvd_cd]
#if (${mode_type} == 'I')
AND RCV.PORT_CD = @[pod_cd]
#else
AND RCV.PORT_CD = @[pol_cd]
#end
AND RCV.IO_BND_CD = @[mode_type]
AND rownum=1			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="mode_type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
