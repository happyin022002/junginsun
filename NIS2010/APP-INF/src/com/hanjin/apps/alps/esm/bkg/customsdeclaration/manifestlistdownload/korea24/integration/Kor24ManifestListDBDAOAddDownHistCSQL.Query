<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Kor24ManifestListDBDAOAddDownHistCSQL">
			<desc><![CDATA[AddDownHist]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_ADV_KR_DL_HIS
( MRN_NO
, MRN_CHK_NO
, VSL_CD
, SKD_VOY_NO
, SKD_DIR_CD
, DL_SEQ
, POL_CD
, POD_CD
, OFC_CD
, UPD_USR_ID
, BL_KNT
, KR_CSTMS_DL_ACT_CD
, DELT_DT
)VALUES(
  @[mrn_nbr]
, @[mrn_chk]
, SUBSTR(@[vvd_cd],1,4)
, SUBSTR(@[vvd_cd],5,4)
, SUBSTR(@[vvd_cd],9,1)
, @[kdh_seq]
, DECODE(@[bound],'O',TRIM(NVL(@[mrn_port],' ')),NULL)
, DECODE(@[bound],'I',TRIM(NVL(@[mrn_port],' ')),NULL)
, @[office]
, @[username]
, @[mrn_update_cnt]
, 'M'
, TO_DATE(@[action_time],'YYYYMMDD HH24:MI:SS'))			]]></sql>
			<params>
				<param name="mrn_nbr" type="12" value="" out="N"/>
				<param name="mrn_chk" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="kdh_seq" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="mrn_port" type="12" value="" out="N"/>
				<param name="office" type="12" value="" out="N"/>
				<param name="username" type="12" value="" out="N"/>
				<param name="mrn_update_cnt" type="12" value="" out="N"/>
				<param name="action_time" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
