<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOaddCstmAckMsgDtlCSQL">
			<desc><![CDATA[응답 세부 메시지 INSERT]]></desc>
			<sql><![CDATA[
INSERT 
  INTO BKG_CSTMS_KR_RCV_LOG_DTL
     ( MSG_LOG_TP_CD
     , RCV_DT
     , RCV_SEQ
     , FLT_FILE_REF_NO
     , LOG_DTL_SEQ
     , EDI_RCV_MSG
     , KR_CSTMS_RJCT_RSN1
     , KR_CSTMS_RJCT_RSN2
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     )
SELECT @[msg_log_tp_cd]
     , TO_DATE(@[rcv_dt], 'YYYYMMDDHH24:MI:SS')
     , @[rcv_seq]
     , @[flt_file_ref_no]
     , @[log_dtl_seq]
     , @[edi_rcv_msg]
     , @[kr_cstms_rjct_rsn1]
     , @[kr_cstms_rjct_rsn2]
     , @[user_id]
     , SYSDATE
     , @[user_id]
     , SYSDATE
FROM DUAL			]]></sql>
			<params>
				<param name="msg_log_tp_cd" type="12" value="" out="N"/>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="rcv_seq" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="log_dtl_seq" type="12" value="" out="N"/>
				<param name="edi_rcv_msg" type="12" value="" out="N"/>
				<param name="kr_cstms_rjct_rsn1" type="12" value="" out="N"/>
				<param name="kr_cstms_rjct_rsn2" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
