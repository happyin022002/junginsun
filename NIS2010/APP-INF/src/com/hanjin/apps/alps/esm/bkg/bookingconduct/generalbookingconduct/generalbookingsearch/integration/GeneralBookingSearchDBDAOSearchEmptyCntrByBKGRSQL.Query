<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchEmptyCntrByBKGRSQL">
			<desc><![CDATA[Empty Repo Bkg에 속해 있는 Container No List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  CNTR.CNTR_NO,
        CNTR.CNTR_TPSZ_CD,
        CNTR.MCNTR_BDL_NO,
        CNTR.BDL_BTM_FLG,
		(SELECT INTG_CD_VAL_DP_DESC 
			FROM COM_INTG_CD_DTL 
			WHERE INTG_CD_ID = 'CD02012' 
			AND INTG_CD_VAL_CTNT = MST.CNTR_HNGR_RCK_CD) CNTR_HNGR_RCK_CD,
		MST.MNR_HNGR_BAR_TP_CD,
		MST.CNTR_HNGR_BAR_ATCH_KNT
#if (${ca_flg} == 'Y') 
FROM    BKG_CNTR_HIS CNTR, MST_CONTAINER MST
#else 
FROM    BKG_CONTAINER CNTR, MST_CONTAINER MST
#end
WHERE   CNTR.BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y')
AND     CNTR.CORR_NO = 'TMP0000001'
#end
AND		CNTR.CNTR_NO = MST.CNTR_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="SIN312293601" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
