<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOAverageUCVesselUtADDCSQL">
			<desc><![CDATA[Average U/C_Vessel Pooling2 (OP Fixed & Variable cost, etc) Insert
2015.07.13 손진환 [CHM-201536797] ALPS MAS의 Pooling 화면 Row Add & Save 버튼 추가 CSR]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_POOL_UT_COST (
          COST_YRMON
		, STND_COST_CD
        , TRD_CD
		, SUB_TRD_CD
        , RLANE_CD
        , DIR_CD
        , HUL_BND_CD
		, EFF_FM_YRMON
		, EFF_TO_YRMON
		, TTL_AMT
		, TGT_LOD_QTY
		, TEU_UC_AMT
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT
)

 SELECT @[cost_yrmon] AS COST_YRMON
	  , @[stnd_cost_cd] AS STND_COST_CD
      , @[trd_cd]     AS TRD_CD
	  , @[sub_trd_cd]     AS SUB_TRD_CD
      , @[rlane_cd]   AS RLANE_CD
      , @[dir_cd]     AS DIR_CD
      , @[hul_bnd_cd] AS HUL_BND_CD
	  , TRIM(REPLACE(SUBSTR(@[eff_yrmon], 0, 7), '-', '')) AS EFF_FM_YRMON
	  , TRIM(REPLACE(SUBSTR(@[eff_yrmon], 9), '-', '')) AS EFF_TO_YRMON
	  , @[ttl_amt] AS TTL_AMT
	  , @[tgt_lod_qty] AS TGT_LOD_QTY
	  , @[teu_uc_amt] AS TEU_UC_AMT
      , @[cre_usr_id] AS CRE_USR_ID
      , SYSDATE       AS CRE_DT
      , @[upd_usr_id] AS UPD_USR_ID
      , SYSDATE       AS UPD_DT
 FROM DUAL			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="hul_bnd_cd" type="12" value="" out="N"/>
				<param name="eff_yrmon" type="12" value="" out="N"/>
				<param name="ttl_amt" type="12" value="" out="N"/>
				<param name="tgt_lod_qty" type="12" value="" out="N"/>
				<param name="teu_uc_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
