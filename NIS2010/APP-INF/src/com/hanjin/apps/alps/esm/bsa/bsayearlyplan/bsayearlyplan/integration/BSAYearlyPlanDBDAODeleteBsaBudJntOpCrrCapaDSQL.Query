<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAYearlyPlanDBDAODeleteBsaBudJntOpCrrCapaDSQL">
			<desc><![CDATA[BSA_BUD_JNT_OP_CRR_CAPA 테이블 데이타 삭제]]></desc>
			<sql><![CDATA[
DELETE
  FROM BSA_BUD_JNT_OP_CRR_CAPA
 WHERE 1=1
   AND (TRD_CD, RLANE_CD, DIR_CD, VOP_CD, VSL_CAPA, BSA_SEQ) IN ( SELECT TRD_CD, RLANE_CD, DIR_CD, VOP_CD, VSL_CAPA, BSA_SEQ
                                                                    FROM BSA_BUD_JNT_OP_BZC
                                                                   WHERE 1=1
#if (${trd_cd} != 'All')
                                                                     AND TRD_CD   = @[trd_cd]
#end

#if (${rlane_cd} != 'All')
                                                                     AND RLANE_CD = @[rlane_cd]
#end
                                                                )			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
