<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCommonDBDAOGetMdmLocationInfoRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT LOC_CD AS VALUE0,
       SCC_CD AS VALUE1,
       LOC_NM AS VALUE2,
       CNT_CD AS VALUE3
  FROM MDM_LOCATION A
 WHERE LOC_CD = @[value0]
   AND NVL(DELT_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="value0" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
