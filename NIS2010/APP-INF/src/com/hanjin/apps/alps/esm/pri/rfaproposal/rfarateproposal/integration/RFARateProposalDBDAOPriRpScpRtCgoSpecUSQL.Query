<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtCgoSpecUSQL">
			<desc><![CDATA[PRI_RP_SCP_RT_CGO_SPEC 테이블 수정]]></desc>
			<sql><![CDATA[
UPDATE PRI_RP_SCP_RT_CGO_SPEC SET 
       ACT_CGO_LEN = @[act_cgo_len]
     , ACT_CGO_WDT = @[act_cgo_wdt]
     , ACT_CGO_HGT = @[act_cgo_hgt]
     , ACT_CGO_WGT = @[act_cgo_wgt]
     , CGO_SPEC_RMK = @[cgo_spec_rmk]
 WHERE PROP_NO = @[prop_no]
   AND AMDT_SEQ = @[amdt_seq]
   AND SVC_SCP_CD = @[svc_scp_cd]
   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]     
   AND ROUT_SEQ = @[rout_seq]
   AND RT_SEQ = @[rt_seq]
			]]></sql>
			<params>
				<param name="act_cgo_len" type="6" value="" out="N"/>
				<param name="act_cgo_wdt" type="6" value="" out="N"/>
				<param name="act_cgo_hgt" type="6" value="" out="N"/>
				<param name="act_cgo_wgt" type="6" value="" out="N"/>
				<param name="cgo_spec_rmk" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="2" value="" out="N"/>
				<param name="rout_seq" type="2" value="" out="N"/>
				<param name="rt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
