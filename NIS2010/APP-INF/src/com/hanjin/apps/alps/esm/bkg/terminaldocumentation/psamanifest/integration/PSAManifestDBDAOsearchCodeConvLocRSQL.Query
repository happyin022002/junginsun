<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchCodeConvLocRSQL">
			<desc><![CDATA[Un Code로 들어오는 Location은 NIS Code로 Coversion]]></desc>
			<sql><![CDATA[
SELECT 'X'
FROM   MDM_LOCATION
WHERE  LOC_CD = @[port_cd]
AND    DELT_FLG <> 'Y'			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
