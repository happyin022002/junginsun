<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BRKGAuditDBDAOModifyBRKGCommInfoAgtCmpnCommUSQL">
			<desc><![CDATA[ModifyBRKGCommInfoAgtCmpnComm]]></desc>
			<sql><![CDATA[
UPDATE AGT_CMPN_COMM
       SET FRT_FWRD_CNT_CD   = @[frt_fwrd_cnt_cd],
           FRT_FWRD_SEQ      = @[frt_fwrd_seq],
           VNDR_CNT_CD       = @[vndr_cnt_cd],
           VNDR_SEQ          = @[vndr_seq],
	       CMPN_REF_NO       = @[cmpn_ref_no],
	       UPD_USR_ID        = @[upd_usr_id],
	       UPD_DT            = SYSDATE
     WHERE BKG_NO            = @[bkg_no]
       AND CMPN_SEQ          = @[cmpn_seq]
       AND COMM_PROC_STS_CD != 'IF'			]]></sql>
			<params>
				<param name="frt_fwrd_cnt_cd" type="12" value="" out="Y"/>
				<param name="frt_fwrd_seq" type="12" value="" out="Y"/>
				<param name="vndr_cnt_cd" type="12" value="" out="Y"/>
				<param name="vndr_seq" type="12" value="" out="Y"/>
				<param name="cmpn_ref_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="bkg_no" type="12" value="" out="Y"/>
				<param name="cmpn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
