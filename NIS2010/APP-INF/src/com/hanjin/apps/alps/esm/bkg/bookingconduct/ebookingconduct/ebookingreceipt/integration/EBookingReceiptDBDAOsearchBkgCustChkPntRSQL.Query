<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchBkgCustChkPntRSQL">
			<desc><![CDATA[searchBkgCustChkPnt]]></desc>
			<sql><![CDATA[
SELECT (SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END
           FROM BKG_CUSTOMER A
                ,BKG_CUST_CHK_PNT B
          WHERE A.BKG_NO =@[bkg_no]
            AND A.BKG_CUST_TP_CD ='S'
            AND A.CUST_CNT_CD = B.CUST_CNT_CD
            AND A.CUST_SEQ = B.CUST_SEQ
            AND B.CHK_PNT_TP_CD = 'CU') CU_YN,
        (SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END
           FROM BKG_CUSTOMER A
                ,BKG_CUST_CHK_PNT B
          WHERE A.BKG_NO =@[bkg_no]
            AND A.BKG_CUST_TP_CD ='S'
            AND A.CUST_CNT_CD = B.CUST_CNT_CD
            AND A.CUST_SEQ = B.CUST_SEQ
            AND B.CHK_PNT_TP_CD = 'CN') CN_YN,
        (SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END
           FROM BKG_CUSTOMER A
                ,BKG_CUST_CHK_PNT B
          WHERE A.BKG_NO =@[bkg_no]
            AND A.BKG_CUST_TP_CD ='S'
            AND A.CUST_CNT_CD = B.CUST_CNT_CD
            AND A.CUST_SEQ = B.CUST_SEQ
            AND B.CHK_PNT_TP_CD = 'MD') MD_YN,
        (SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END
           FROM BKG_CUSTOMER A
                ,BKG_CUST_CHK_PNT B
          WHERE A.BKG_NO =@[bkg_no]
            AND A.BKG_CUST_TP_CD ='S'
            AND A.CUST_CNT_CD = B.CUST_CNT_CD
            AND A.CUST_SEQ = B.CUST_SEQ
            AND B.CHK_PNT_TP_CD = 'CM') CM_YN,
        (SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END
           FROM BKG_CUSTOMER A
                ,BKG_CUST_CHK_PNT B
          WHERE A.BKG_NO =@[bkg_no]
            AND A.BKG_CUST_TP_CD ='S'
            AND A.CUST_CNT_CD = B.CUST_CNT_CD
            AND A.CUST_SEQ = B.CUST_SEQ
            AND B.CHK_PNT_TP_CD = 'EM') EM_YN,
        (SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END
           FROM BKG_CUSTOMER A
                ,BKG_CUST_CHK_PNT B
          WHERE A.BKG_NO =@[bkg_no]
            AND A.BKG_CUST_TP_CD ='S'
            AND A.CUST_CNT_CD = B.CUST_CNT_CD
            AND A.CUST_SEQ = B.CUST_SEQ
            AND B.CHK_PNT_TP_CD = 'RA') RA_YN,
        (SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' ELSE 'N' END
           FROM BKG_CUSTOMER A
                ,BKG_CUST_CHK_PNT B
          WHERE A.BKG_NO =@[bkg_no]
            AND A.BKG_CUST_TP_CD ='S'
            AND A.CUST_CNT_CD = B.CUST_CNT_CD
            AND A.CUST_SEQ = B.CUST_SEQ
            AND B.CHK_PNT_TP_CD = 'CO') CO_YN
   FROM DUAL 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
