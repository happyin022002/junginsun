<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupCommodityProposalDBDAOPriScpGrpCmdtTermsDtlUSQL">
			<desc><![CDATA[* N1_cmnc_dt 변경 - Proposal이거나 Filing시 filing Date가 더 늦을 경우 변경]]></desc>
			<sql><![CDATA[
UPDATE PRI_SP_SCP_GRP_CMDT_DTL
   SET N1ST_CMNC_AMDT_SEQ  = @[amdt_seq]
      ,UPD_USR_ID    = @[upd_usr_id]
      ,UPD_DT        = SYSDATE 
 WHERE PROP_NO       = @[prop_no]
AND    AMDT_SEQ      = @[amdt_seq]
AND    SVC_SCP_CD = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
