<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOSearchGsoOfcListRSQL">
			<desc><![CDATA[gso에 속한 Sub offices를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  OFC_CD AS BKG_OFC
FROM    MDM_ORGANIZATION 
WHERE DELT_FLG = 'N'
START WITH OFC_CD = NVL(@[gso],OFC_CD)
CONNECT BY PRIOR OFC_CD = PRNT_OFC_CD			]]></sql>
			<params>
				<param name="gso" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
