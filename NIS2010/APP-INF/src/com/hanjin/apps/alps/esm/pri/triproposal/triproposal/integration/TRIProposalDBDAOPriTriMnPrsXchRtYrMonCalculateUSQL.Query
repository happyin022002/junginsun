<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriMnPrsXchRtYrMonCalculateUSQL">
			<desc><![CDATA[ ]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRI_MN
   SET PRS_XCH_RT_YRMON = LEAST(TO_CHAR(SYSDATE, 'YYYYMM'), ( SELECT MAX(ACCT_XCH_RT_YRMON) FROM GL_MON_XCH_RT ))
 WHERE TRI_PROP_NO = @[tri_prop_no]
   AND EXISTS ( SELECT 'O' FROM PRI_TRI_RT WHERE TRI_PROP_NO = @[tri_prop_no] AND AMDT_SEQ = @[amdt_seq] AND PROP_STS_CD IN ( 'I', 'R') )			]]></sql>
			<params>
				<param name="tri_prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
