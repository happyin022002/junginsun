<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchEBLMaxHisSeqRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(EBL_HIS_SEQ)+1,1) MAX_EBL_HIS_SEQ
  FROM BKG_EBL_CRNT_RQST_HIS
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
