<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOCreateQtyCACSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOCreateQtyCACSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_QUANTITY (
    BKG_NO 
#else
INSERT INTO BKG_QTY_HIS (
    BKG_NO 
    , CORR_NO 
#end
    , CNTR_TPSZ_CD 
    , OP_CNTR_QTY 
    , ACT_CNTR_QTY 
    , DCGO_QTY 
    , AWK_CGO_QTY 
    , RC_QTY 
    , BB_CGO_QTY 
    , SOC_QTY 
    , EQ_SUBST_CNTR_TPSZ_CD 
    , EQ_SUBST_CGO_QTY 
    , MER_HNGR_QTY 
    , CRR_HNGR_QTY 
    , CRR_HNGR_SGL_BAR_QTY 
    , CRR_HNGR_DBL_BAR_QTY 
    , CRR_HNGR_TPL_BAR_QTY 
    , ORG_CNTR_QTY 
    , DEST_CNTR_QTY 
    , OB_TRO_QTY 
    , IB_TRO_QTY 
    , FLEX_HGT_FLG 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
        , CNTR_TPSZ_CD 
        , OP_CNTR_QTY 
        , ACT_CNTR_QTY 
        , DCGO_QTY 
        , AWK_CGO_QTY 
        , RC_QTY 
        , BB_CGO_QTY 
        , SOC_QTY 
        , EQ_SUBST_CNTR_TPSZ_CD 
        , EQ_SUBST_CGO_QTY 
        , MER_HNGR_QTY 
        , CRR_HNGR_QTY 
        , CRR_HNGR_SGL_BAR_QTY 
        , CRR_HNGR_DBL_BAR_QTY 
        , CRR_HNGR_TPL_BAR_QTY 
        , ORG_CNTR_QTY 
        , DEST_CNTR_QTY 
        , OB_TRO_QTY 
        , IB_TRO_QTY 
        , FLEX_HGT_FLG 
        , CRE_USR_ID 
        , CRE_DT 
        , UPD_USR_ID 
        , sysdate
#if (${copy_type_cd} == 'BKG')
  FROM BKG_QTY_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_QUANTITY
 WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
