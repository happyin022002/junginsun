			<sql><![CDATA[
INSERT INTO PRI_AUTH_APRO_USR_HIS
(
  AUTH_APRO_HIS_SEQ
, PRC_CTRT_TP_CD
, PRC_USR_AUTH_TP_CD
, AUTH_APRO_USR_ID
, AUTH_APRO_USR_OFC_CD
, AUTH_APRO_DESC
, AUTH_APRO_USE_FLG
, PROG_USR_ID
, PROG_OFC_CD
, PROG_GDT
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
)

VALUES(
		 			(SELECT NVL(MAX(AUTH_APRO_HIS_SEQ),0)+1 FROM PRI_AUTH_APRO_USR_HIS)
				  , @[prc_ctrt_tp_cd]
				  , @[prc_usr_auth_tp_cd]
