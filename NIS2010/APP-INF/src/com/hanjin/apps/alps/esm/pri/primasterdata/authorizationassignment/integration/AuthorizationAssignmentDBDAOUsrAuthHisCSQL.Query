				  , @[prog_ofc_cd]
				  , GLOBALDATE_PKG.TIME_CONV_FNC(GLOBALDATE_PKG.GET_LOCCD_FNC('SELSC'), SYSDATE,'GMT')
				  , @[cre_usr_id]
				  , sysdate
				  , @[upd_usr_id]
				  , sysdate
				  )			]]></sql>
			<params>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="prc_usr_auth_tp_cd" type="12" value="" out="N"/>
