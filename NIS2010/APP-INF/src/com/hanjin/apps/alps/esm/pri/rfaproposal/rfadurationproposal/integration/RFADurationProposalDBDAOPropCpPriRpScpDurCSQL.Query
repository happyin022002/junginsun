<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFADurationProposalDBDAOPropCpPriRpScpDurCSQL">
			<desc><![CDATA[Proposal Scope Copy PRI_RP_SCP_DUR Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_SCP_DUR (
      PROP_NO
    , AMDT_SEQ
    , SVC_SCP_CD
    , CTRT_EFF_DT
    , CTRT_EXP_DT
    , PRC_PROG_STS_CD
    , SRC_INFO_CD
	, N1ST_CMNC_AMDT_SEQ
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
)
SELECT @[new_prop_no] AS PROP_NO
     , 0 AS AMDT_SEQ
     , SVC_SCP_CD
#if (${IS_MASTER_COPY} == 'Y') 
	--------- RFA 효율화를 위한 요청 (1차) (CHM-201640671) 
     , TO_DATE(NVL(@[eff_dt], '99991231'), 'YYYYMMDD') AS CTRT_EFF_DT
     , TO_DATE(NVL(@[exp_dt], '99991231'), 'YYYYMMDD') AS CTRT_EXP_DT
     , 'I' AS PRC_PROG_STS_CD
     , 'NW' AS SRC_INFO_CD
#else 
     , TO_DATE('99991231', 'YYYYMMDD') AS CTRT_EFF_DT
     , TO_DATE('99991231', 'YYYYMMDD') AS CTRT_EXP_DT
     , 'I' AS PRC_PROG_STS_CD
     , 'PC' AS SRC_INFO_CD
#end
     , 0 AS N1ST_CMNC_AMDT_SEQ
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
FROM PRI_RP_SCP_DUR
WHERE PROP_NO = @[prop_no]
AND   AMDT_SEQ = @[amdt_seq]
AND   SVC_SCP_CD = @[svc_scp_cd]
AND   SRC_INFO_CD <> 'AD'			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
