                               0,
                               0,
                               0,
                               0,
        
                               -- 20140728 추가(S)
                               0,
                               0,
                               0
        					   -- 20140728 추가(E)
                          FROM FCST_DATA
                        UNION ALL
                        SELECT IOC_TS_CD,
                               OFC_CD   ,
                               CUST_CTRL_CD,
        
                               -- 20140728 추가(S)
                               USA_BKG_MOD_CD,
