        					   -- 20140728 추가(E)
        
                               NULL AS ALC_VOL,
                               NULL AS ALC_HC ,
                               NULL AS ALC_45 ,
                               NULL AS ALC_53 ,
                               NULL AS ALC_RF ,
                               NULL AS ALC_WGT,
        
                               -- 20140728 추가(S)
                               NULL AS ALC_D2 ,
                               NULL AS ALC_D4 ,
                               NULL AS ALC_RD ,
        					   -- 20140728 추가(E)
        
                               0,
