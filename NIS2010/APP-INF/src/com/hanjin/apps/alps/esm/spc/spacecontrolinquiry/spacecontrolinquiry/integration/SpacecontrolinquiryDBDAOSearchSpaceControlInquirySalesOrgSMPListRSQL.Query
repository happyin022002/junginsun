        					   -- 20140728 추가(E)
        
                               NULL AS ALC_VOL,
                               NULL AS ALC_HC ,
                               NULL AS ALC_45 ,
                               NULL AS ALC_53 ,
                               NULL AS ALC_RF ,
                               NULL AS ALC_WGT,
        
                    		   -- 20140728 추가(S)
                               NULL AS ALC_D2 ,
                               NULL AS ALC_D4 ,
                               NULL AS ALC_RD ,
        					   -- 20140728 추가(E)
        
                               0    AS BKG_VOL,
                               0    AS BKG_20 ,
                               0    AS BKG_40 ,
                               0    AS BKG_HC ,
                               0    AS BKG_45 ,
                               0    AS BKG_53 ,
                               0    AS BKG_RF ,
                               0    AS BKG_WGT,
        
                    		   -- 20140728 추가(S)
                               0    AS BKG_D2 ,
                               0    AS BKG_D4 ,
                               0    AS BKG_RD   
        					   -- 20140728 추가(E)
                          FROM QTA_DATA
                        UNION ALL
                        SELECT IOC_TS_CD,
