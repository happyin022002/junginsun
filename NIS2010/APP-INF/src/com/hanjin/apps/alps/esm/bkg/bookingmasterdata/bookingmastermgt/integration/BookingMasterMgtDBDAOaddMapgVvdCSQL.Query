<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOaddMapgVvdCSQL">
			<desc><![CDATA[타선사와 VVD 표현 방식이 다른 경우 매핑되는 데이터를 추가합니다]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_MAPG_VVD 
     ( VSL_CD
     , SKD_VOY_NO
     , SKD_DIR_CD
     , MAPG_VSL_NM
     , MAPG_VOY_DIR_NM
     , CRE_DT
     , CRE_USR_ID
     , UPD_DT
     , UPD_USR_ID
     )
VALUES 
     ( @[vsl_cd]
     , @[skd_voy_no]
     , @[skd_dir_cd]
     , UPPER(TRIM(REPLACE(@[mapg_vsl_nm], CHR(13)||CHR(10), '')))
     , UPPER(TRIM(@[mapg_voy_dir_nm]))
     , SYSDATE
     , @[usr_id]
     , SYSDATE
     , @[usr_id]
     )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="mapg_vsl_nm" type="12" value="" out="N"/>
				<param name="mapg_voy_dir_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
