<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndExpCustomsTransmissionDBDAOsearchVesselArrival2RSQL">
			<desc><![CDATA[searchVesselArrival2]]></desc>
			<sql><![CDATA[
SELECT  
      /* 포트가 추가 되어 포트별로 설정이 가능한데, 같은 포트에서 셋업된 것을 우선적으로 적용한다. CVY_REF_NO는 VVD별로 대부분 같다. */
		SUBSTR(MAX(DECODE(A.CSTMS_PORT_CD, B.PORT_CD,'1','0') || B.CVY_REF_NO),2) CVY_REF_NO
  FROM  BKG_CSTMS_AMER_BL A, BKG_CSTMS_CND_XPT_VSL B
 WHERE  A.VSL_CD = B.VSL_CD
   AND  A.SKD_VOY_NO = B.SKD_VOY_NO
   AND  A.SKD_DIR_CD = B.SKD_DIR_CD
   AND  A.CNT_CD = 'CA'
   AND  A.IO_BND_CD = 'O'
   AND  A.BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
