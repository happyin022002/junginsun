<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOSearchGwApprFileAttachRSQL">
			<desc><![CDATA[2016.04.05 FMS Owner's Account Attach FILE 조회하여 GW로 전송하기위한 FILE정보]]></desc>
			<sql><![CDATA[
WITH FILES AS (SELECT 'FMS' AS L_FILE_MODULE_ID,
               F.FILE_SAV_ID AS L_FILE_SAV_ID
          FROM FMS_CSUL_SLP S,
               FMS_OWNR_ACCT_ATCH_FILE F,
               COM_UPLD_FILE C
         WHERE 1=1
           AND S.SLP_TP_CD||S.SLP_FUNC_CD||S.SLP_OFC_CD||S.SLP_ISS_DT||S.SLP_SER_NO = @[csr_no]
           AND S.ATCH_FILE_OA_LNK_ID = F.ATCH_FILE_OA_LNK_ID
           AND F.FILE_SAV_ID = C.FILE_SAV_ID )
SELECT A.L_FILE_MODULE_ID,
       A.L_FILE_SAV_ID ,
       B.FILE_UPLD_NM AS L_FILE_NM
  FROM FILES A,
       COM_UPLD_FILE B
 WHERE A.L_FILE_SAV_ID = B.FILE_SAV_ID
   AND B.DELT_FLG = 'N'
 GROUP BY A.L_FILE_MODULE_ID,
       A.L_FILE_SAV_ID , 
       B.FILE_UPLD_NM			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
