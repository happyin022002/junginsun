<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAOSearchStoredDashboardReportColumnRSQL">
			<desc><![CDATA[해당 사용자가 저장한 report form 의 report column 을 조회한다]]></desc>
			<sql><![CDATA[
SELECT 
        D.RPT_FOM_NO
        , D.RPT_ID
        , D.DBD_IRR_TP_CD
        , D.DP_NM
        , D.DP_SEQ
        , D.CRE_DT
        , D.CRE_USR_ID
        , D.UPD_DT
        , D.UPD_USR_ID
FROM BKG_DBD_RPT_FOM_DTL D
, BKG_DBD_RPT_FOM F
WHERE F.CRE_USR_ID = @[usr_id]
AND F.RPT_FOM_NM = @[rpt_fom_nm]
AND F.RPT_FOM_NO = D.RPT_FOM_NO
AND F.CRE_USR_ID = D.CRE_USR_ID
ORDER BY D.DP_SEQ			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="rpt_fom_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
