<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FICCostInterfaceDBDAOCheckCopyServiceScopeInitialRSQL">
			<desc><![CDATA[check exist IHC tariff's status]]></desc>
			<sql><![CDATA[
SELECT C.CNT_NM
     , HDR.SVC_SCP_CD 
     , HDR.IHC_TRF_NO 
     , HDR.COST_CNT_CD
     , HDR.ORG_DEST_TP_CD
     , MN.AMDT_SEQ
     , MN.EFF_DT
     , MN.FIC_PROP_STS_CD
FROM PRI_TRF_IHC_MN MN
   , (  
        SELECT A.IHC_TRF_NO
             , MN.SVC_SCP_CD
             , A.COST_CNT_CD
             , MN.ORG_DEST_TP_CD
             , MAX(MN.AMDT_SEQ) AMDT_SEQ
          FROM PRI_TRF_IHC_MN MN
             , (    SELECT SVC_SCP_CD
                         , COST_CNT_CD
                         , ORG_DEST_TP_CD
                         , MAX(IHC_TRF_NO) IHC_TRF_NO
                      FROM PRI_TRF_IHC_HDR
                     WHERE 1=1
                       AND SVC_SCP_CD = @[svc_scp_cd]
                       AND COST_CNT_CD IN ( #foreach(${key} in ${cost_cnt_cd})
                                                #if($velocityCount < $cost_cnt_cd.size())
                                                    '$key', 
                                                #else  
                                                    '$key'
                                                #end 
					                       #end )                          
                       AND ORG_DEST_TP_CD = @[org_dest_tp_cd]
                       GROUP BY SVC_SCP_CD, COST_CNT_CD, ORG_DEST_TP_CD
               ) A
         WHERE 1=1
           AND MN.IHC_TRF_NO = A.IHC_TRF_NO
      GROUP BY A.IHC_TRF_NO, MN.SVC_SCP_CD, A.COST_CNT_CD, MN.ORG_DEST_TP_CD
      ) HDR
    , MDM_COUNTRY C
WHERE 1=1
  AND MN.SVC_SCP_CD  = HDR.SVC_SCP_CD
  AND MN.ORG_DEST_TP_CD = HDR.ORG_DEST_TP_CD
  AND MN.IHC_TRF_NO = HDR.IHC_TRF_NO
  AND HDR.COST_CNT_CD = C.CNT_CD
  AND MN.FIC_PROP_STS_CD = 'I'			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
