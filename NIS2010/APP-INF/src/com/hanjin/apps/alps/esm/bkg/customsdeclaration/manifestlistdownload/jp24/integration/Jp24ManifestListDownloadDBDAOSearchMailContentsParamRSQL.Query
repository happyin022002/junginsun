<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOSearchMailContentsParamRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT BL.BL_NO,
       L_MDM.LOC_NM AS POL_NM,
       CNTR.VSL_ENG_NM,
       CNTR.CALL_SGN_NO,
       BL.POD_CD,
       D_MDM.LOC_NM AS POD_NM

  FROM BKG_CSTMS_ADV_JP_BL BL,
       MDM_LOCATION L_MDM,
       MDM_LOCATION D_MDM,
       MDM_VSL_CNTR CNTR

 WHERE BL.VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND BL.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND BL.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
#if (${bl_no} != '')
   AND BL.BL_NO = @[bl_no]
#end
   AND BL.POL_CD = @[pol_cd]
   AND BL.RVIS_CNTR_CUST_TP_CD = 'N'
   AND BL.POL_CD = L_MDM.LOC_CD
   AND BL.POD_CD = D_MDM.LOC_CD
   AND BL.VSL_CD = CNTR.VSL_CD			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
