<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQHoldingDBDAOCreateEqHoldingMonthCopyCSQL">
			<desc><![CDATA[2012.08.02 이석준[CHM-201219334-01] EQ Holding Cost 화면 Month Copy 기능 추가]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_HLD_COST
(
COST_YRMON,
CNTR_CHSS_DIV_CD,
EQ_TPSZ_CD,
STND_COST_CD,
CHSS_HLD_UC_AMT,
TTL_HLD_AMT,
EQ_BX_KNT,
EQ_HLD_DYS,
CHSS_USA_QTY,
COST_ASS_BSE_CD,
DELT_FLG,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID,
UPD_DT
)
SELECT
@[f_tar_mon] AS COST_YRMON,
CNTR_CHSS_DIV_CD,
EQ_TPSZ_CD,
STND_COST_CD,
CHSS_HLD_UC_AMT,
TTL_HLD_AMT,
EQ_BX_KNT,
EQ_HLD_DYS,
CHSS_USA_QTY,
COST_ASS_BSE_CD,
DELT_FLG,
@[user_id] AS CRE_USR_ID,
SYSDATE AS CRE_DT,
@[user_id] AS UPD_USR_ID,
SYSDATE AS UPD_DT
FROM MAS_HLD_COST
WHERE COST_YRMON     = @[f_src_mon]			]]></sql>
			<params>
				<param name="f_tar_mon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_src_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
