<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOmodifyEdoReviewFlagUSQL">
			<desc><![CDATA[E-DO Review Flag 를 Y 로 업데이트]]></desc>
			<sql><![CDATA[
UPDATE BKG_EDO_MST
   SET EDO_RVW_FLG    = 'Y'
     , EDO_RVW_USR_ID = @[usr_id]
     , EDO_RVW_DT     =  SYSDATE
 WHERE EDO_RQST_NO    =  @[rqstNo]
   AND EDO_RQST_SEQ = ( SELECT MAX(EDO_RQST_SEQ)
                            FROM BKG_EDO_MST
                           WHERE EDO_RQST_NO  = @[rqstNo]
                             AND EDO_TP_CD    = @[tpCd] )
   AND EDO_TP_CD    = @[tpCd]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="rqstNo" type="12" value="" out="N"/>
				<param name="tpCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
