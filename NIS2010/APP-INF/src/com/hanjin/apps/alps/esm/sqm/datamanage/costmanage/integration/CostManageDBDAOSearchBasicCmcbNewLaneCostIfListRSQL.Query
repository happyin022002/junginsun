<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostManageDBDAOSearchBasicCmcbNewLaneCostIfListRSQL">
			<desc><![CDATA["New Lane & Office CMCB" 화면에서 등록했던 정보 조회]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
         A.TRD_CD
        ,A.RLANE_CD
		,A.DIR_CD
        ,NVL(C.ADD_FLG, 'N') AS ADD_FLG
    FROM SQM_QTA_NEW_LANE          A
        ,SQM_QTA_NEW_LANE_OFC_COST B
        ,SQM_QTA_LANE_OFC_COST     C
   WHERE A.BSE_TP_CD  = @[f_bse_tp_cd]
     AND A.BSE_YR     = @[f_bse_yr]
     AND A.BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
     AND A.BSE_TP_CD  = B.BSE_TP_CD
     AND A.BSE_YR     = B.BSE_YR
     AND A.BSE_QTR_CD = B.BSE_QTR_CD
     AND A.TRD_CD     = B.TRD_CD
     AND A.RLANE_CD   = B.RLANE_CD
     AND A.DIR_CD     = B.DIR_CD
     AND A.BSE_TP_CD  = C.BSE_TP_CD  (+)
     AND A.BSE_YR     = C.BSE_YR     (+)
     AND A.BSE_QTR_CD = C.BSE_QTR_CD (+)
     AND A.TRD_CD     = C.TRD_CD     (+)
     AND A.RLANE_CD   = C.RLANE_CD   (+)
     AND A.DIR_CD     = C.DIR_CD     (+)
ORDER BY TRD_CD
        ,RLANE_CD			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
