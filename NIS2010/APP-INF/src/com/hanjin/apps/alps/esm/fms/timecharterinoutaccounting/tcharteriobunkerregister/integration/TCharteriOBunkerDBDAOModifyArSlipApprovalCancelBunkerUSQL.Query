<sqls>
	<querys>
		<query name="TCharteriOBunkerDBDAOModifyArSlipApprovalCancelBunkerUSQL">
			<desc><![CDATA[TCharteriOBunkerDBDAOModifyArSlipApprovalCancelBunkerUSQL]]></desc>
			<sql><![CDATA[
UPDATE FMS_BUNKER SET 
	AR_SLP_TP_CD = NULL
,	AR_SLP_FUNC_CD = NULL
,	AR_SLP_OFC_CD = NULL
,	AR_SLP_ISS_DT = NULL
,	AR_SLP_SER_NO = NULL
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE AR_SLP_TP_CD 	 = SUBSTR(@[csr_no],1,2)
  AND AR_SLP_FUNC_CD = SUBSTR(@[csr_no],3,1)
  AND AR_SLP_OFC_CD  = SUBSTR(@[csr_no],4,6)
  AND AR_SLP_ISS_DT  = SUBSTR(@[csr_no],10,6)
  AND AR_SLP_SER_NO  = SUBSTR(@[csr_no],16,5)			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
