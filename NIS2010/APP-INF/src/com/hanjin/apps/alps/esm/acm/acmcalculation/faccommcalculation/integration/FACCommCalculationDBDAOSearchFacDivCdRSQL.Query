<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FACCommCalculationDBDAOSearchFacDivCdRSQL">
			<desc><![CDATA[FACCommCalculationDBDAOSearchFacDivCdRSQL]]></desc>
			<sql><![CDATA[
SELECT FAC_DIV_CD, SUBSTR(FAC_DIV_CD, 1, 1) FAC_DIV_CD_1
  FROM ACM_FAC_COMM
 WHERE BKG_NO = @[bkg_no]
	AND FAC_SEQ = @[fac_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="fac_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
