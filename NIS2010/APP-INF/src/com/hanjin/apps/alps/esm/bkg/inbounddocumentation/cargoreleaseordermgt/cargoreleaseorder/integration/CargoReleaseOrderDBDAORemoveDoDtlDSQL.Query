<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAORemoveDoDtlDSQL">
			<desc><![CDATA[해당 BKG_NO, RLSE_SEQ를  기준으로 Cancel 상태 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_DO_DTL 
 WHERE BKG_NO      = @[bkg_no] 
   AND RLSE_SEQ    = @[rlse_seq]
   AND RLSE_STS_CD = 'C'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rlse_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
