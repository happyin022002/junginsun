<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAORemoveStoredDashboardReportColumnDSQL">
			<desc><![CDATA[사용자가 설정한 dashboard report column 을 삭제한다]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_DBD_RPT_FOM_DTL 
WHERE RPT_FOM_NO =@[rpt_fom_no] 
AND CRE_USR_ID = @[usr_id]
			]]></sql>
			<params>
				<param name="rpt_fom_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
