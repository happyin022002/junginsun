<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOModifyAttachFileLinkIdUSQL">
			<desc><![CDATA[BKG_REV_DR_NOTE에 File Link ID 업데이트]]></desc>
			<sql><![CDATA[
UPDATE BKG_REV_DR_NOTE
SET ATCH_FILE_LNK_ID = @[atch_file_lnk_id]
WHERE RDN_NO = @[rdn_no]
AND RVIS_SEQ = @[rvis_seq]			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="rdn_no" type="12" value="" out="N"/>
				<param name="rvis_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
