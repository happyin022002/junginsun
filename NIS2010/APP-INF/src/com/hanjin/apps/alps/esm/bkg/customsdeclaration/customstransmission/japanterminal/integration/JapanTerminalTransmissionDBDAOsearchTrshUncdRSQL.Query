<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanTerminalTransmissionDBDAOsearchTrshUncdRSQL">
			<desc><![CDATA[Pre Pod 2 값을 가져온다.]]></desc>
			<sql><![CDATA[
SELECT NVL((SELECT POD_CD FROM BKG_VVD WHERE BKG_NO=@[bkg_no] AND VSL_PRE_PST_CD <> 'T' AND POL_CD= @[pol_cd] ), 
'') TRSH_UNCD --(select POD_CD from BKG_VVD  WHERE BKG_NO=bkg_no AND VSL_PRE_PST_CD='T')
FROM BKG_VVD 
WHERE BKG_NO= @[bkg_no]
AND VSL_CD=@[vsl_cd]
AND SKD_VOY_NO=@[skd_voy_no]
AND SKD_DIR_CD=@[skd_dir_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
