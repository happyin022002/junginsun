<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOmodifySrCrntRqstForMaxHisSeqUSQL">
			<desc><![CDATA[DPCS에서 신규 생성한 SR HISTORY SEQ를 UPDATE]]></desc>
			<sql><![CDATA[
UPDATE  BKG_SR_CRNT_RQST RQ                    
SET     RQ.MAX_HIS_SEQ      = @[sr_his_seq] 
#if (${sr_sts_cd} == 'XX') 
,       RQ.SR_CRNT_STS_CD   = 'XX'   
#end
WHERE  RQ.BKG_NO       = @[bkg_no]   
#if (${sr_sts_cd} == 'XX')  
AND    RQ.SR_NO     != @[sr_no]
#else 
AND    RQ.SR_NO   	= @[sr_no]
#end			]]></sql>
			<params>
				<param name="sr_his_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
