<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FeederChargeGuideLineDBDAOInsertFDRGuidelineDetailCSQL">
			<desc><![CDATA[Insert FDR Guideline Detail

* History
2013.03.16 [CHM-201534279] 전지예 Pricing Feeder/IHC tariff 45" 칼럼 추가 안]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRF_FDR_RT
  (SVC_SCP_CD,
   ORG_DEST_TP_CD,
   FDR_TRF_NO,
   AMDT_SEQ,
   RT_SEQ,
   N1ST_CMNC_AMDT_SEQ,
   PNT_LOC_CD,
   BSE_PORT_LOC_CD,
   RCV_DE_TERM_CD,
   GLINE_20FT_FRT_RT_AMT,
   GLINE_40FT_FRT_RT_AMT,
   GLINE_45FT_FRT_RT_AMT, -- 45' Cost 추가
   COST_20FT_FRT_RT_AMT,
   COST_40FT_FRT_RT_AMT,
   COST_45FT_FRT_RT_AMT, -- 45' Cost 추가
   LOCL_CURR_COST_20FT_FRT_RT_AMT,
   LOCL_CURR_COST_40FT_FRT_RT_AMT,
   LOCL_CURR_COST_45FT_FRT_RT_AMT, -- 45' Cost 추가
   TRSP_20FT_COST_AMT,
   TRSP_40FT_COST_AMT,
   TRSP_45FT_COST_AMT, -- 45' Cost 추가
   TML_20FT_COST_AMT,
   TML_40FT_COST_AMT,
   TML_45FT_COST_AMT, -- 45' Cost 추가
   GLINE_RF_20FT_FRT_RT_AMT,
   GLINE_RF_40FT_FRT_RT_AMT,
   COST_RF_20FT_FRT_RT_AMT,
   COST_RF_40FT_FRT_RT_AMT,
   LOCL_CURR_COST_RF_20FT_RT_AMT,
   LOCL_CURR_COST_RF_40FT_RT_AMT,
   TRSP_RF_20FT_COST_AMT,
   TRSP_RF_40FT_COST_AMT,
   TML_RF_20FT_COST_AMT,
   TML_RF_40FT_COST_AMT,
   RC_SVC_FLG,   
   WTR_RCV_TERM_CD,
   WTR_DE_TERM_CD,
   RHQ_CD,
   LOCL_CURR_CD,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT,
   SRC_INFO_CD,
   FDR_RT_RMK)
SELECT @[svc_scp_cd],
       @[org_dest_tp_cd],
       @[fdr_trf_no],
       @[amdt_seq],
       NVL(MAX(RT_SEQ), 0) + 1,
       @[n1st_cmnc_amdt_seq],
       @[pnt_loc_cd],
       @[bse_port_loc_cd],
       @[rcv_de_term_cd],
       @[gline_20ft_frt_rt_amt],
       @[gline_40ft_frt_rt_amt],
       @[gline_45ft_frt_rt_amt], -- 45' cost 추가
       @[cost_20ft_frt_rt_amt],
       @[cost_40ft_frt_rt_amt],
       @[cost_45ft_frt_rt_amt], -- 45' cost 추가
       @[locl_curr_cost_20ft_frt_rt_amt],
       @[locl_curr_cost_40ft_frt_rt_amt],
       @[locl_curr_cost_45ft_frt_rt_amt], -- 45' cost 추가
       @[trsp_20ft_cost_amt],
       @[trsp_40ft_cost_amt],
       @[trsp_45ft_cost_amt], -- 45' cost 추가
       @[tml_20ft_cost_amt],
       @[tml_40ft_cost_amt],
       @[tml_45ft_cost_amt], -- 45' cost 추가
       @[gline_rf_20ft_frt_rt_amt],
       @[gline_rf_40ft_frt_rt_amt],
       @[cost_rf_20ft_frt_rt_amt],
       @[cost_rf_40ft_frt_rt_amt],
       @[locl_curr_cost_rf_20ft_rt_amt],
       @[locl_curr_cost_rf_40ft_rt_amt],
       @[trsp_rf_20ft_cost_amt],
       @[trsp_rf_40ft_cost_amt],
       @[tml_rf_20ft_cost_amt],
       @[tml_rf_40ft_cost_amt],
       @[rc_svc_flg],
       @[wtr_rcv_term_cd],
       @[wtr_de_term_cd],
       @[rhq_cd],
       @[locl_curr_cd],
       @[cre_usr_id],
       SYSDATE,
       @[upd_usr_id],
       SYSDATE,
       @[src_info_cd],
       @[fdr_rt_rmk]
FROM PRI_TRF_FDR_RT
WHERE SVC_SCP_CD       = @[svc_scp_cd] 
AND ORG_DEST_TP_CD   = @[org_dest_tp_cd]
AND FDR_TRF_NO       = @[fdr_trf_no]
AND AMDT_SEQ         = @[amdt_seq]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="n1st_cmnc_amdt_seq" type="12" value="" out="N"/>
				<param name="pnt_loc_cd" type="12" value="" out="N"/>
				<param name="bse_port_loc_cd" type="12" value="" out="N"/>
				<param name="rcv_de_term_cd" type="12" value="" out="N"/>
				<param name="gline_20ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_40ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_45ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="cost_20ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="cost_40ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="cost_45ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="locl_curr_cost_20ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="locl_curr_cost_40ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="locl_curr_cost_45ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="trsp_20ft_cost_amt" type="12" value="" out="N"/>
				<param name="trsp_40ft_cost_amt" type="12" value="" out="N"/>
				<param name="trsp_45ft_cost_amt" type="12" value="" out="N"/>
				<param name="tml_20ft_cost_amt" type="12" value="" out="N"/>
				<param name="tml_40ft_cost_amt" type="12" value="" out="N"/>
				<param name="tml_45ft_cost_amt" type="12" value="" out="N"/>
				<param name="gline_rf_20ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="gline_rf_40ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="cost_rf_20ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="cost_rf_40ft_frt_rt_amt" type="12" value="" out="N"/>
				<param name="locl_curr_cost_rf_20ft_rt_amt" type="12" value="" out="N"/>
				<param name="locl_curr_cost_rf_40ft_rt_amt" type="12" value="" out="N"/>
				<param name="trsp_rf_20ft_cost_amt" type="12" value="" out="N"/>
				<param name="trsp_rf_40ft_cost_amt" type="12" value="" out="N"/>
				<param name="tml_rf_20ft_cost_amt" type="12" value="" out="N"/>
				<param name="tml_rf_40ft_cost_amt" type="12" value="" out="N"/>
				<param name="rc_svc_flg" type="12" value="" out="N"/>
				<param name="wtr_rcv_term_cd" type="12" value="" out="N"/>
				<param name="wtr_de_term_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="src_info_cd" type="12" value="" out="N"/>
				<param name="fdr_rt_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
