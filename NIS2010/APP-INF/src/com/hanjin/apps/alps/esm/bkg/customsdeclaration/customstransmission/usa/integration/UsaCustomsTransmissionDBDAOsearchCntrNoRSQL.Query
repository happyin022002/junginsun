<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchCntrNoRSQL">
			<desc><![CDATA[searchCntrNo]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN CNTR_NO LIKE TRIM(@[cntr_no]) || '%' THEN TRIM(@[cntr_no])
            ELSE SUBSTR(@[cntr_no], 1, 4) || TRIM(SUBSTR(@[cntr_no], 6))
        END CNTR_NO
  FROM MST_CONTAINER
 WHERE CNTR_NO LIKE TRIM(@[cntr_no]) || '%'
    OR (
        SUBSTR(@[cntr_no], 5, 1) = '0'
        AND
        CNTR_NO LIKE SUBSTR(@[cntr_no], 1, 4) || TRIM(SUBSTR(@[cntr_no], 6)) || '%'
       )
			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
