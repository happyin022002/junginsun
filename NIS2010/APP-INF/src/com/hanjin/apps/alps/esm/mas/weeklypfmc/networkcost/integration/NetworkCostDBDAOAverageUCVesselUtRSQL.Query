<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOAverageUCVesselUtRSQL">
			<desc><![CDATA[AverageUCVesselUt]]></desc>
			<sql><![CDATA[
SELECT COST_YRMON
     , STND_COST_CD
     , TRD_CD
     , SUB_TRD_CD
     , RLANE_CD
     , DIR_CD
     , HUL_BND_CD
     , TO_CHAR(TO_DATE(EFF_FM_YRMON,'YYYYMM'),'YYYY-MM')||'~'||TO_CHAR(TO_DATE(EFF_TO_YRMON,'YYYYMM'),'YYYY-MM') AS EFF_YRMON
     , TTL_AMT
     , TGT_LOD_QTY
     , TEU_UC_AMT 
     , UPD_USR_ID
  FROM MAS_POOL_UT_COST
 WHERE STND_COST_CD = @[f_cobcost]
   AND COST_YRMON   = REPLACE(@[f_yearweek], '-', '')
#if(${f_trd_cd} != '')
   AND TRD_CD     = @[f_trd_cd]
#end
#if(${f_rlane_cd} != '')
   AND RLANE_CD   = @[f_rlane_cd]
#end
#if(${f_dir_cd} != '')
   AND DIR_CD     = @[f_dir_cd]
#end
#if(${f_hul_bnd_cd} != '')
   AND HUL_BND_CD = @[f_hul_bnd_cd]
#end
 ORDER BY COST_YRMON,
       TRD_CD,
       SUB_TRD_CD,
       RLANE_CD,
       DIR_CD			]]></sql>
			<params>
				<param name="f_cobcost" type="12" value="" out="N"/>
				<param name="f_yearweek" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_hul_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
