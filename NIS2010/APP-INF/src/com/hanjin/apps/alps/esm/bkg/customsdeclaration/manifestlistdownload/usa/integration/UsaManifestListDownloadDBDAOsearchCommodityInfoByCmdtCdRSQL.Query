<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchCommodityInfoByCmdtCdRSQL">
			<desc><![CDATA[searchCommodityInfoByCmdtCd]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(SUBSTR(D.CMDT_DESC,1,45)) AS CMDT_DESC
FROM   BKG_BL_MK_DESC D, BKG_CNTR_MF_DESC C
WHERE  C.CMDT_CD = @[cmdt_cd]
AND    C.BKG_NO = D.BKG_NO			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
