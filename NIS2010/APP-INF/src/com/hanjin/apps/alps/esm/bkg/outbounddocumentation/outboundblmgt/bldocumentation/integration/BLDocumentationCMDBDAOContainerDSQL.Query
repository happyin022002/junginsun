<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOContainerDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
DELETE FROM BKG_CNTR_HIS 
WHERE  BKG_NO = @[bkg_no]
AND    CORR_NO = 'TMP0000001'
AND    CNTR_NO = @[cntr_no]
#else 
DELETE FROM BKG_CONTAINER 
WHERE  BKG_NO = @[bkg_no]
AND    CNTR_NO = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
