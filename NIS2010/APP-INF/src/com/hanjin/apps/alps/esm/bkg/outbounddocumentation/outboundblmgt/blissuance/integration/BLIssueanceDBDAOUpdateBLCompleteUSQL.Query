<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssueanceDBDAOUpdateBLCompleteUSQL">
			<desc><![CDATA[BL Data Complete 업데이트]]></desc>
			<sql><![CDATA[
UPDATE BKG_BL_ISS
SET  BL_RDY_FLG = @[bl_ready_checkbox]
	,BL_RDY_OFC_CD = @[bl_ready_office]
	,BL_RDY_USR_ID = @[bl_ready_by]
	,BL_RDY_DT = TO_DATE(TO_CHAR(SYSDATE,'YYYY-MM-DD'),'YYYY-MM-DD HH24:MI:SS')
	,UPD_USR_ID = @[upd_usr_id]
	,UPD_DT = SYSDATE
WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bl_ready_checkbox" type="12" value="" out="N"/>
				<param name="bl_ready_office" type="12" value="" out="N"/>
				<param name="bl_ready_by" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
