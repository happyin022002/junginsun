<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MultiDimensionRPTDBDAOSearchMultiDimension068ListRSQL">
			<desc><![CDATA[EQ 회송기여도 RPT 조회]]></desc>
			<sql><![CDATA[
SELECT A1.PCTL_NO
  ,A1.BKG_NO
  ,A4.COST_ROUT_NO
  ,A1.POR_CD POR
  ,A1.POL_CD POL
  ,A1.N1ST_TS_PORT_CD TS1
  ,A1.N2ND_TS_PORT_CD TS2
  ,A1.N3RD_TS_PORT_CD TS3
  ,A1.POD_CD POD
  ,A1.DEL_CD DEL
  ,A1.OB_ITCHG_CTNT INTER1
  ,A1.IB_ITCHG_CTNT INTER2
  ,A1.DG_SPCL_FLG SPCL_DG
  ,A1.RF_SPCL_FLG SPCL_RF
  ,A1.SPCL_AWK_CGO_FLG SPCL_AK
  ,A1.BB_SPCL_FLG SPCL_BB
  ,A3.N1ST_RLANE_CD LANE1
  ,A3.N2ND_RLANE_CD LANE2
  ,A3.N3RD_RLANE_CD LANE3
  ,A3.N4TH_RLANE_CD LANE4
  ,A3.CLT_OFC_CD
  ,A3.SLS_OFC_CD,A3.SHIPPER
  ,A3.IOC_CD,A3.VVD
  ,A3.DTERM
  ,A3.RTERM
  ,A3.RCMDT
  ,A3.SHPR_NM
  ,A3.RLANE_CD
  , NVL(A1.TTL_TZTM_HRS, 0) / 24 HRS
FROM (SELECT DISTINCT J.BKG_NO
      	,J.PCTL_NO 
		,K.POR_CD
      	,K.POL_CD 
		,K.N1ST_TS_PORT_CD
      	,K.N2ND_TS_PORT_CD 
		,K.N3RD_TS_PORT_CD
      	,K.POD_CD 
		,K.DEL_NOD_CD
      	,K.OB_ITCHG_CTNT 
		,K.IB_ITCHG_CTNT
      	,K.DEL_CD 
		,K.DG_SPCL_FLG
      	,K.RF_SPCL_FLG 
		,K.SPCL_AWK_CGO_FLG
      	,K.BB_SPCL_FLG 
		,K.TTL_TZTM_HRS
    FROM SCE_COP_HDR J, PRD_PROD_CTL_MST K   
    WHERE J.BKG_NO = @[f_bkg_no] 
        AND J.PCTL_NO = K.PCTL_NO ) A1,
    (SELECT BKG_NO
      	,RLANE_CD
		,N1ST_RLANE_CD
      	,N2ND_RLANE_CD
		,N3RD_RLANE_CD
      	,N4TH_RLANE_CD,NVL(AGMT_SGN_OFC_CD, SLS_OFC_CD) CLT_OFC_CD
      	,SLS_OFC_CD
		,SHPR_CNT_CD || SHPR_CUST_SEQ SHIPPER
      	,IOC_CD, VSL_CD || SKD_VOY_NO || DIR_CD VVD
      	,BKG_DE_TERM_CD DTERM,BKG_RCV_TERM_CD RTERM
      	,REP_CMDT_CD RCMDT
      	,SHPR_NM
    FROM COA_RGST_BKG   
    WHERE BKG_NO = @[f_bkg_no] 
    /* Booking Info에 적용 (kevin.kim) */
      AND BL_NO_TP IN ('M','0')      
      AND BKG_STS_CD IN ('F','S') 
      AND BKG_CGO_TP_CD NOT IN ( 'P')) A3,
    (SELECT DISTINCT BKG_NO
        ,COST_ROUT_NO
    FROM COA_BKG_REV_DTL
    WHERE BKG_NO = @[f_bkg_no]) A4   
WHERE A1.BKG_NO = A3.BKG_NO 
  AND A1.BKG_NO = A4.BKG_NO			]]></sql>
			<params>
				<param name="f_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
