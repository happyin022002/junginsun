<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsReportDBDAOsearchAmsReportBaplieListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT 
    A.ID cntr_no ,
    A.OPR_CD,
    A.POL pol,
    A.POD_ISO pod,
    A.SZTP,
	CASE WHEN (SELECT B.BKG_CGO_TP_CD
    FROM BKG_VVD V, BKG_CONTAINER C,  BKG_BOOKING B
    WHERE A.ID = A.ID
      AND A.ID = C.CNTR_NO(+)
      AND V.VSL_CD = A.VSL_CD
      AND V.SKD_VOY_NO = A.VOY_NO
      AND V.SKD_DIR_CD = A.DIR_CD
      AND V.POL_CD = A.POL
      AND V.POD_CD = A.POD_ISO
      AND V.BKG_NO = C.BKG_NO
	  AND V.BKG_NO = B.BKG_NO
      AND ROWNUM = 1 ) = 'R' THEN 'Rev.Empty'
  	ELSE DECODE (A.FE, 'E','Empty','F','Full','')
	END FM_IND, 
    DECODE(SUBSTR(A.SZTP,  1, 1), 	'R',	'REEFER',
				 					'O',	'OPEN TOP',
				 					'F',	'FLAT RACK',
				 					'P',	'PLATFFORM',
				 					'D',	'DRY',
				 					'T',	'TANK',
				 					'A',	'ADJUSTABLE',
				 					'S',	'OPEN TOP SLIDING',
				 					'Q',	'DEAD SPACE',
				 					'XX') fe,
    A.BAY||A.ROWW||A.TIER sti_pos,
    A.IMDG,
    A.UNNO,
    A.VSL_CD || A.VOY_NO || A.DIR_CD search_vvd_cd,
    A.PORT_CD l_pol,
    TO_CHAR(L.SND_DT,'YYYY-MM-DD HH24:MI') sent_time,
    L.POD_CD o_pod,
	L.ACK_RCV_STS_CD cust_result,
	L.RSPN_ERR_RSLT_CD||NVL2(L.RSPN_ERR_DESC, '('||L.RSPN_ERR_DESC||')', null) cust_rspn,
	L.CSTMS_RMK1 description,
	TO_CHAR(L.RCV_DT,'YYYY-MM-DD HH24:MI') receive_date,
	'' vvd,
	NVL(@[excludeca], '') excludeca,
	'' vsl_eng_nm, -- VO 필드추가용.
	'' vsl_voy, -- VO 필드추가용.
	'' crr_cd, -- VO 필드추가용.
	'' TMP1, -- 임시필드.
	'' TMP2, -- 임시필드.
	'' TMP3, -- 임시필드.
	'' TMP4, -- 임시필드.
	'' TMP5  -- 임시필드.
FROM BAY_PLAN A
     LEFT OUTER JOIN BKG_CSTMS_ADV_STWG_CNTR L ON (     A.VSL_CD  = L.VSL_CD
                                                    AND A.VOY_NO  = L.SKD_VOY_NO
                                                    AND A.DIR_CD  = L.SKD_DIR_CD
                                                    AND A.ID      = L.CNTR_NO
                                                    AND A.PORT_CD = L.LODG_PORT_CD
                                                    /* 미주 신고건만 조회 변경 2018.05.28 */
                                                    AND EXISTS    ( SELECT SL.STWG_SND_ID
                                                                      FROM BKG_CSTMS_ADV_STWG_SND_LOG SL
                                                                     WHERE SL.SND_PROC_ID  IN ('STW') -- US BAPLIE건만 조회
                                                                       AND SL.VSL_CD     =  L.VSL_CD
                                                                       AND SL.SKD_VOY_NO =  L.SKD_VOY_NO
                                                                       AND SL.SKD_DIR_CD =  L.SKD_DIR_CD
                                                                       AND SL.STWG_SND_ID = L.STWG_SND_ID
                                                                   )
                                                     )
     , VSK_VSL_PORT_SKD V
WHERE 
	1=1
#if (${vvd} != '')
	and A.VSL_CD 	= SUBSTR(@[vvd],1, 4)
    and A.VOY_NO 	= SUBSTR(@[vvd],5, 4)
    and A.DIR_CD 	= SUBSTR(@[vvd],9, 1)
#end
#if (${last_pol} != '')
    and A.PORT_CD 	= @[last_pol]
#end
#if (${pol} != '')
    AND A.POL 		= @[pol]
#end
#if (${pod} != '')
    AND A.POD_ISO 	= @[pod]
#end
#if (${date_search} =='on')
	#if (${cust_arr_exp} == 'SND')
        AND L.SND_DT
        BETWEEN TO_DATE(REPLACE(@[fromd], '-', '') || REPLACE(@[fromt], ':', '') ,'YYYYMMDDHH24MI')
        AND TO_DATE(REPLACE(@[tod], '-', '')   || REPLACE(@[tot], ':', '')   ,'YYYYMMDDHH24MI')
	#elseif (${cust_arr_exp} == 'RCV')
        AND L.RCV_DT
        BETWEEN TO_DATE(REPLACE(@[fromd], '-', '') || REPLACE(@[fromt], ':', '') ,'YYYYMMDDHH24MI')
        AND TO_DATE(REPLACE(@[tod], '-', '')   || REPLACE(@[tot], ':', '')   ,'YYYYMMDDHH24MI')
	#end
#end
#if (${cntropr} != '')
    AND A.OPR_CD 	= @[cntropr]
#end
   
#if (${excludeca} == 'EXCLUDECA')
	AND A.POD_ISO 	NOT LIKE 'CA%' 
#else
	-- 20100426 하동일, 경종윤 
    -- Exclude Canada Import가 이닐 경우 CA, US말고도 미주지역을 거쳐가는 다른 port도 나와야함
	--AND (POD_ISO 	LIKE 'CA%' OR POD_ISO LIKE 'US%') 
#end

	-- 20100608 하동일, 경종윤
	-- 미국 Port 이후 부분만 FROB로 신고 되도록 로직 추가 
	AND A.VSL_CD = V.VSL_CD
	AND A.VOY_NO = V.SKD_VOY_NO
	AND A.DIR_CD = V.SKD_DIR_CD
	AND A.POD_ISO = V.VPS_PORT_CD
	AND V.CLPT_IND_SEQ = 1
	AND V.CLPT_SEQ >= (
                    SELECT MIN(CLPT_SEQ)
                    FROM VSK_VSL_PORT_SKD 
                    WHERE 1=1
				#if (${vvd} != '')
                    AND VSL_CD      = SUBSTR(@[vvd],1, 4)
                    AND SKD_VOY_NO  = SUBSTR(@[vvd],5, 4)
                    AND SKD_DIR_CD  = SUBSTR(@[vvd],9, 1)
				#end
                    AND VPS_PORT_CD LIKE 'US%'
                    AND NVL(SKD_CNG_STS_CD, 'X') != 'S'
                    AND CLPT_IND_SEQ = 1
                  )
/* US 신고건만 조회 변경 2018.05.28 
    AND A.VSL_CD = L.VSL_CD(+)
    AND A.VOY_NO = L.SKD_VOY_NO(+)
    AND A.DIR_CD = L.SKD_DIR_CD(+)
    AND A.ID     = L.CNTR_NO(+)
    AND A.PORT_CD = L.LODG_PORT_CD(+)
*/
AND A.PLAN_TYPE= ( SELECT MIN(PLAN_TYPE) FROM BAY_PLAN BB
                 WHERE A.VSL_CD = BB.VSL_CD
                 AND A.VOY_NO = BB.VOY_NO
                 AND A.DIR_CD = BB.DIR_CD
                 AND A.PORT_CD = BB.PORT_CD
                 AND A.ID = BB.ID
                 AND A.CALL_IND = BB.CALL_IND
                 )

AND NOT EXISTS (
                    SELECT 'X'
                    FROM BKG_CSTMS_CD_CONV_CTNT
                    WHERE CNT_CD = 'US'
                    AND CSTMS_DIV_ID= 'BAPLIE_XCLD_CNTR_CD'
                    AND ATTR_CTNT1 = A.ID
                )

#if (${allerror} == 'ERR') 
	and A.POD_ISO <> L.POD_CD
#end
GROUP BY  
    A.ID
    ,A.OPR_CD
    ,A.POL
    ,A.POD_ISO
    ,A.SZTP
    ,SUBSTR(A.SZTP,  1, 1)
    ,A.BAY||A.ROWW||A.TIER
    ,A.IMDG
    ,A.UNNO
    ,A.VSL_CD || A.VOY_NO || A.DIR_CD
    ,A.PORT_CD
    ,TO_CHAR(L.SND_DT,'YYYY-MM-DD HH24:MI')
    ,L.POD_CD
	,L.ACK_RCV_STS_CD
	,L.RSPN_ERR_RSLT_CD||NVL2(L.RSPN_ERR_DESC, '('||L.RSPN_ERR_DESC||')', null)
	,L.CSTMS_RMK1
	,L.RCV_DT
    ,A.VSL_CD
    ,A.VOY_NO
    ,A.DIR_CD
    ,A.FE			]]></sql>
			<params>
				<param name="excludeca" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="last_pol" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
				<param name="fromd" type="12" value="" out="N"/>
				<param name="fromt" type="12" value="" out="N"/>
				<param name="tod" type="12" value="" out="N"/>
				<param name="tot" type="12" value="" out="N"/>
				<param name="cntropr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
