<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAOPriRpScpRtScgRoutCostDetailVODSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[

MERGE INTO PRI_RP_SCP_RT_SCG_ROUT A
USING (
	SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, CMDT_HDR_SEQ, ROUT_SEQ, RT_SEQ
	  FROM PRI_RP_SCP_RT 
	 WHERE PROP_NO = @[prop_no]
   		AND  AMDT_SEQ = @[amdt_seq]
   		AND  SVC_SCP_CD = @[svc_scp_cd]
   		AND  CMDT_HDR_SEQ = @[cmdt_hdr_seq]
   		AND  ROUT_SEQ = @[rout_seq]
   		AND  RT_SEQ = @[rt_seq]
	   AND PRC_PROG_STS_CD IN ( 'I', 'R' )
	   AND SRC_INFO_CD <> 'AD'    
) B
ON ( 
		A.PROP_NO = B.PROP_NO
	AND A.AMDT_SEQ = B.AMDT_SEQ
	AND A.SVC_SCP_CD = B.SVC_SCP_CD
	AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
	AND A.ROUT_SEQ = B.ROUT_SEQ
	AND A.RT_SEQ = B.RT_SEQ
)
WHEN MATCHED THEN 
    UPDATE SET A.UPD_USR_ID = 'XXX'
    DELETE WHERE 1=1
			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cmdt_hdr_seq" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
