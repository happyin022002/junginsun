<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateQuotationDBDAOPriSqRtCmdtHdrVOAddCopyToQuotationCSQL">
			<desc><![CDATA[AddCopyToQuotation]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SQ_RT_CMDT_HDR (
	QTTN_NO
,	QTTN_VER_NO
,	GEN_SPCL_RT_TP_CD
,	CMDT_HDR_SEQ
,	SRC_INFO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT  @[qttn_no]
,	@[qttn_ver_no]
,	GEN_SPCL_RT_TP_CD
,	CMDT_HDR_SEQ
,	SRC_INFO_CD
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM    PRI_SQ_RT_CMDT_HDR
WHERE   1=1
AND     QTTN_NO = @[qttn_no_from]
AND     QTTN_VER_NO = @[qttn_ver_no_from]
#if (${gen_spcl_rt_tp_cd_from} != '')
AND	GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd_from]
#end			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="qttn_no_from" type="12" value="" out="N"/>
				<param name="qttn_ver_no_from" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd_from" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
