<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AncsManifestListDownloadDBDAOmodifyBkgCstmsAnrBl3USQL">
			<desc><![CDATA[u]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ANR_BL X
       SET X.ANR_MSG_STS_CD = 'N',
           VVD_SEQ = (SELECT NVL(MAX(B.VVD_SEQ),0) + 1
                        FROM BKG_CSTMS_ANR_VVD A, BKG_CSTMS_ANR_BL B
                       WHERE A.SVC_RQST_NO = SUBSTR(@[anr_decl_no], 1, 6)
                         AND B.VSL_CD = A.VSL_CD
                         AND B.SKD_VOY_NO = A.SKD_VOY_NO
                         AND B.SKD_DIR_CD = A.SKD_DIR_CD)
     WHERE (X.VSL_CD, X.SKD_VOY_NO, X.SKD_DIR_CD, X.BKG_NO) IN  (
            SELECT Z.VSL_CD, Z.SKD_VOY_NO, Z.SKD_DIR_CD, Z.BKG_NO
              FROM BKG_CSTMS_ANR_BL_LOG Z
             WHERE Z.ANR_DECL_NO = @[anr_decl_no]
               AND Z.REF_SEQ = @[ref_seq]
               AND Z.EDI_RCV_STS_CD = 'A')			]]></sql>
			<params>
				<param name="anr_decl_no" type="12" value="" out="N"/>
				<param name="ref_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
