<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOSearchTroEdiDtlRSQL">
			<desc><![CDATA[TransferOrderIssueDBDAOSearchTroEdiDtl]]></desc>
			<sql><![CDATA[
SELECT RPAD(NVL(TO_CHAR(DOR_ARR_DT, 'RRRRMMDDHH24'), ' '), 10) ||
       RPAD(NVL(TO_CHAR(DOR_ARR_DT, 'RRRRMMDDHH24'), ' '), 10) ||
       RPAD(CNTR_TPSZ_CD, 2)                                    ||
       RPAD(TRUNC(NVL(TRO_QTY, '0')), 3)                        ||
       RPAD(SUBSTR(NVL(PKUP_YD_CD, '     '), 3, 5), 5)          ||
       RPAD(SUBSTR(NVL(RTN_YD_CD,  '     '), 3, 5), 5)          ||
       RPAD('  ', 7) AS STR_FLATFILE
  FROM BKG_TRO_DTL
 WHERE BKG_NO       = @[bkg_no]
   AND IO_BND_CD    = 'O'
   AND RTN_TRO_FLG  = @[rtn_tro_flg]
   AND TRO_SEQ      = @[tro_seq]
   AND TRO_QTY 		<> 0			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rtn_tro_flg" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
