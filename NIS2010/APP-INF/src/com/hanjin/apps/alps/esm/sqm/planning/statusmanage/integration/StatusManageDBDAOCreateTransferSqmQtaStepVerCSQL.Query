<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusManageDBDAOCreateTransferSqmQtaStepVerCSQL">
			<desc><![CDATA[Qta Transfer 시 연간 정보로 1Q SQM_QTA_STEP_VER 생성]]></desc>
			<sql><![CDATA[
INSERT INTO SQM_QTA_STEP_VER (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,QTA_STEP_CD
      ,QTA_VER_NO
      ,TRD_CD
      ,CONV_DIR_CD
      ,SQM_VER_STS_CD
      ,CFM_GDT
      ,CRE_OFC_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT 'Q'    AS BSE_TP_CD
      ,BSE_YR AS BSE_YR
      ,'1Q'   AS BSE_QTR_CD
      ,OFC_VW_CD
      ,QTA_STEP_CD
      ,SUBSTR(QTA_VER_NO, 1, 11) || '1Q' || SUBSTR(QTA_VER_NO, -2) AS QTA_VER_NO
      ,TRD_CD
      ,CONV_DIR_CD
      ,SQM_VER_STS_CD
      ,CFM_GDT
      ,CRE_OFC_CD
      ,@[f_usr_id] AS CRE_USR_ID
      ,SYSDATE     AS CRE_DT
      ,@[f_usr_id] AS UPD_USR_ID
      ,SYSDATE     AS UPD_DT
  FROM SQM_QTA_STEP_VER
 WHERE BSE_TP_CD  = 'Y'
   AND BSE_YR     = @[f_bse_yr]
   AND BSE_QTR_CD = '00'			]]></sql>
			<params>
				<param name="f_usr_id" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
