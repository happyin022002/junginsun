<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOAddQueueDetailReturn1CSQL">
			<desc><![CDATA[2011.03 김기종 [CHM-201109394-01] DPCS고도화일환으로  말레이지아 LOCAL TIME 변경]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_SR_HIS
(
    SR_KND_CD,
    SR_NO,
    BKG_NO,
    SR_HIS_SEQ,
    
    SR_STS_CD,
    SR_PROC_STS_CD,
    
    ATND_USR_ID,/*로그인 ID*/
    ST_DT,
    SR_PROC_UPD_DT,

    FNT_OFC_RTN_CD,
    DIFF_RMK,
    SR_RTN_TO_STS_CD,
    
    RTN_TO_USR_ID,/*로그인 ID*/
    RTN_TO_RTN_STS_CD,
    
    RTN_TO_RTN_USR_ID,/*로그인 ID*/
    RTN_DT,
    
    RSN_BKG_MN_FLG,    /*BKG Main*/
    RSN_CUST_INFO_FLG, /*Customer INFO*/
    RSN_FRT_CHG_FLG,   /*FRT & Charge*/
    RSN_CNTR_FLG,      /*Container*/
    RSN_CNTR_MF_FLG,   /*Container Manifest*/
    
    RSN_DCGO_FLG,      /*Danger*/
    RSN_AWK_CGO_FLG,   /*Awkward*/
    RSN_RC_FLG,        /*Reefer*/
    RSN_BB_CGO_FLG,    /*B/Bulk*/
    RSN_RLY_PORT_FLG,  /*RLY VVD & Port*/
    
    RSN_NEW_BKG_FLG,   /*New BKG*/
    RSN_SPLIT_FLG,     /*BKG Split*/
    RSN_BL_INFO_FLG,   /*BL Inform*/
    RSN_HBL_FLG,       /*NVO House BL*/
    RSN_MK_DESC_FLG,   /*Customer Verification*/
    RTN_TO_USR_EML,

    CRE_USR_ID,/*로그인 ID*/
    CRE_DT,
    UPD_USR_ID,/*로그인 ID*/
    UPD_DT,
    EML_SUBJ_CTNT,
    EML_CPY_TO_CUST_FLG,
    SR_PROC_HRS,
    BL_DOC_WRK_HRS,
    BL_DOC_OVT_HRS,
    HOL_FLG
) 
( 
SELECT SUBSTR(@[src_cd],1,1) SR_KND_CD
     , @[sr_no] SR_NO
     , @[bkg_no] BKG_NO
     , NVL(SR_HIS_SEQ+1,0) SR_HIS_SEQ
     , 'RR'
     , 'R' /* 상수 */
     , @[usr_id]
     , GLOBALDATE_PKG.TIME_LOCAL_FNC('MYPKG')
     , AST_DT
     , DECODE(@[ui_grp_cd], 'I', 'I', 'R', 'R', 'S', 'S','C','C', @[ui_grp_cd]) /* UI WRK_GRP_CD 상에서 FO 'S', INPUTER 'I', RATER 'R' */
     ,@[message]
     , DECODE(@[ui_grp_cd], 'I', 'I', 'R', 'R', 'S', 'S','C','C', @[ui_grp_cd]) /* UI WRK_GRP_CD 상에서 FO 'S', INPUTER 'I', RATER 'R' */

     #if (${ui_grp_cd} == 'I') 
	 , BL_DOC_INP_USR_ID
     #elseif (${ui_grp_cd} == 'R') 
	 , BL_RT_USR_ID
     #elseif (${ui_grp_cd} == 'S') 
     , FNT_OFC_CD 
     #elseif (${ui_grp_cd} == 'P') 
     , (SELECT M.OFC_CD FROM MDM_SLS_REP M,BKG_BOOKING BK WHERE BK.OB_SREP_CD = M.SREP_CD AND BK.BKG_NO = @[bkg_no])
     #elseif (${ui_grp_cd} == 'C') 	 
     , 'Customer'
	 #end 

     , BKG_GET_TOKEN_FNC(CRNT_RQST,6) AS RTN_TO_RTN_STS_CD
     
     , @[usr_id]
     , GLOBALDATE_PKG.TIME_LOCAL_FNC('MYPKG')
                                    
     , NVL(@[rsn_bkg_mn_flg]    , 'N')  /*BKG Main*/            
     , NVL(@[rsn_cust_info_flg] , 'N')  /*Customer INFO*/       
     , NVL(@[rsn_frt_chg_flg]   , 'N')  /*FRT & Charge*/        
     , NVL(@[rsn_cntr_flg]      , 'N')  /*Container*/           
     , NVL(@[rsn_cntr_mf_flg]   , 'N')  /*Container Manifest*/  
																														 
     , NVL(@[rsn_dcgo_flg]      , 'N')  /*Danger*/              
     , NVL(@[rsn_awk_cgo_flg]   , 'N')  /*Awkward*/             
     , NVL(@[rsn_rc_flg]        , 'N')  /*Reefer*/              
     , NVL(@[rsn_bb_cgo_flg]    , 'N')  /*B/Bulk*/              
     , NVL(@[rsn_rly_port_flg]  , 'N')  /*RLY VVD & Port*/      
																														 
     , NVL(@[rsn_new_bkg_flg]   , 'N')  /*New BKG*/             
     , NVL(@[rsn_split_flg]     , 'N')  /*BKG Split*/           
     , NVL(@[rsn_bl_info_flg]   , 'N')  /*BL Inform*/           
     , NVL(@[rsn_hbl_flg]       , 'N')  /*NVO House BL*/        
     , NVL(@[cust_verif_flg]    , 'N')  /*Customer Verification*/

     , @[rtn_to_usr_eml]
     , @[usr_id]
     , SYSDATE
     , @[usr_id]
     , SYSDATE
     , @[eml_subj_ctnt]
     , @[eml_cpy_to_cust_flg]
     , GLOBALDATE_PKG.TIME_LOCAL_FNC('MYPKG') - TO_DATE(BKG_GET_TOKEN_FNC(CRNT_RQST,7),'YYYY-MM-DD HH24:MI:SS')
    -- , BKG_GET_TOKEN_FNC(CRNT_RQST,1) AS SR_CRNT_STS_CD
     --, BKG_GET_TOKEN_FNC(CRNT_RQST,2) AS SR_CRNT_INFO_CD     
     --, BKG_GET_TOKEN_FNC(CRNT_RQST,5) AS RTN_FM_USR_ID   
     , BIZ_ELT
     , OVT
     , HOL_FLG  
FROM (  SELECT 
                (SELECT 
                     SR_CRNT_STS_CD    ||','||
                     SR_CRNT_INFO_CD   ||','||
                     RTN_TO_USR_ID     ||','||
                     RTN_TO_RTN_USR_ID ||','||
                     RTN_FM_USR_ID     ||','||
                     RTN_TO_RTN_STS_CD ||','||
					 TO_CHAR(DPCS_DOC_FM_DT,'YYYY-MM-DD HH24:MI:SS')
                 FROM BKG_SR_CRNT_RQST X
               WHERE SR_KND_CD = @[src_cd] /* 0421의 SRC_CD*/
  				 AND  SR_NO    = @[sr_no]
				 AND  BKG_NO   = @[bkg_no]
				 AND SR_AMD_TP_CD = @[sr_knd_cd]/* 0421의 SR_KND_CD*/
                 AND SR_AMD_SEQ = (SELECT MAX(SR_AMD_SEQ) 
                                      FROM BKG_SR_CRNT_RQST
                                     WHERE SR_KND_CD = X.SR_KND_CD
                                       AND SR_NO = X.SR_NO
                                       AND BKG_NO = X.BKG_NO
                                       AND SR_AMD_TP_CD = X.SR_AMD_TP_CD
                                    )
                 ) CRNT_RQST
            , (SELECT	MAX(SR_HIS_SEQ)
                FROM BKG_SR_HIS
                WHERE SR_KND_CD = @[src_cd] /* 0421의 SRC_CD*/
  				  AND  SR_NO    = @[sr_no]
				  AND  BKG_NO   = @[bkg_no]                
               ) SR_HIS_SEQ	
            , AST_DT
            , BIZ_ELT
            , OVT
            , HOL_FLG	
            , BL_DOC_INP_USR_ID
            , BL_RT_USR_ID
            , FNT_OFC_CD	
        FROM BKG_SR_CRNT_RQST X, 
            (SELECT  AST_DT, AEND_DT, 
                     SUM(CASE WHEN TO_CHAR(T.ST_FM_DT,'D') IN (1,7) OR T.HOL_FLG = 'Y' THEN T.ST_FM_DT
                              WHEN T.AEND_DT BETWEEN T.ST_FM_DT AND T.ST_TO_DT THEN T.AEND_DT
                              WHEN T.ST_TO_DT < T.AEND_DT THEN T.ST_TO_DT
                              WHEN T.AEND_DT < T.ST_FM_DT THEN T.ST_FM_DT
                              ELSE T.ST_TO_DT + 1
                         END
                     -
                         CASE WHEN TO_CHAR(T.ST_FM_DT,'D') IN (1,7) OR T.HOL_FLG = 'Y' THEN T.ST_FM_DT
                              WHEN T.AST_DT BETWEEN T.ST_FM_DT AND T.ST_TO_DT THEN T.AST_DT 
                              WHEN T.ST_FM_DT > T.AST_DT THEN T.ST_FM_DT 
                              WHEN T.AST_DT > T.ST_TO_DT THEN T.ST_TO_DT 
                              ELSE T.ST_FM_DT -1
                              END) BIZ_ELT,     
                     T.AEND_DT - T.AST_DT -
                     SUM(CASE WHEN TO_CHAR(T.ST_FM_DT,'D') IN (1,7) OR T.HOL_FLG = 'Y' THEN T.ST_FM_DT
                              WHEN T.AEND_DT BETWEEN T.ST_FM_DT AND T.ST_TO_DT THEN T.AEND_DT
                              WHEN T.ST_TO_DT < T.AEND_DT THEN T.ST_TO_DT
                              WHEN T.AEND_DT < T.ST_FM_DT THEN T.ST_FM_DT
                              ELSE T.ST_TO_DT + 1
                         END
                     -
                         CASE WHEN TO_CHAR(T.ST_FM_DT,'D') IN (1,7) OR T.HOL_FLG = 'Y' THEN T.ST_FM_DT
                              WHEN T.AST_DT BETWEEN T.ST_FM_DT AND T.ST_TO_DT THEN T.AST_DT 
                              WHEN T.ST_FM_DT > T.AST_DT THEN T.ST_FM_DT 
                              WHEN T.AST_DT > T.ST_TO_DT THEN T.ST_TO_DT 
                              ELSE T.ST_FM_DT -1
                         END) OVT,
                     MAX(T.HOL_FLG) HOL_FLG
               FROM (SELECT AST_DT, AEND_DT, 
                            ST_FM_DT, ST_TO_DT,
							(SELECT DECODE(COUNT(DISTINCT HOL_DT),0,'N','Y')
			                   FROM DMT_HOLIDAY H, MDM_ORGANIZATION O,MDM_LOCATION L
				              WHERE O.OFC_CD = (SELECT BKG_OFC_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no])
                                AND O.LOC_CD = L.LOC_CD
								AND (H.CNT_CD = L.CNT_CD OR H.CNT_CD = ' ')
				                AND (H.RGN_CD = L.RGN_CD OR H.RGN_CD = ' ')
                				AND (H.STE_CD = L.STE_CD OR H.STE_CD = ' ')
			                    AND (H.LOC_CD = L.LOC_CD OR H.LOC_CD = ' ')
                                AND HOL_DT BETWEEN TRUNC(ST_FM_DT) AND TRUNC(ST_TO_DT)) HOL_FLG
                     FROM (SELECT T.AST_DT, T.AEND_DT,
                                  --SIGN(X.DPCS_DOC_FM_DT-TRUNC(X.DPCS_DOC_FM_DT)-0.5) 오전 : -1, 오후 : 1이 리턴
                                  --Overnigt인 경우만 Working time이 오전인 경우 하루 전으로 계산함
                                  TO_DATE(TO_CHAR(T.AST_DT,'YYYYMMDD')||W.DOC_WRK_ST_HRMNT,'YYYYMMDDHH24MI') 
                                    - DECODE(T.AST_DT,'Y',DECODE(SIGN(T.AST_DT- TO_DATE(TO_CHAR(T.AST_DT,'YYYYMMDD')||W.DOC_WRK_ST_HRMNT,'YYYYMMDDHH24MI')),-1,1,0),0) 
                                    + CNT -1 ST_FM_DT,
                                  TO_DATE(TO_CHAR(T.AST_DT+DECODE(W.DOC_WRK_OVN_FLG,'Y',1,0),'YYYYMMDD')||W.DOC_WRK_END_HRMNT,'YYYYMMDDHH24MI') 
                                    - DECODE(W.DOC_WRK_OVN_FLG,'Y',DECODE(SIGN(T.AST_DT- TO_DATE(TO_CHAR(T.AST_DT,'YYYYMMDD')||W.DOC_WRK_ST_HRMNT,'YYYYMMDDHH24MI')),-1,1,0),0) 
                                    + CNT -1 ST_TO_DT
                      FROM BKG_DPCS_OFC_WRK_TM W, 
                            (SELECT TO_DATE(@[wrk_st_tm],'YYYYMMDD HH24:MI:SS') AST_DT, 
                                    GLOBALDATE_PKG.TIME_LOCAL_FNC('MYPKG') AEND_DT 
                             FROM DUAL) T,
                            (SELECT ROWNUM CNT FROM DICT) C
                      WHERE W.BKG_OFC_CD = (SELECT BKG_OFC_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no])
                      AND C.CNT < T.AEND_DT - T.AST_DT + 3)) T
               WHERE TRUNC(T.AEND_DT) >= TRUNC(T.ST_FM_DT) -- 필요할까?
               AND ROWNUM = 1
               GROUP BY T.AST_DT, T.AEND_DT
               ) T	
               WHERE SR_KND_CD   = @[src_cd] /* 0421의 SRC_CD*/
               AND SR_NO        = @[sr_no]    
               AND BKG_NO       = @[bkg_no]
               AND SR_AMD_TP_CD = @[sr_knd_cd]/* 0421의 SR_KND_CD*/ 
               AND SR_AMD_SEQ = (SELECT NVL(MAX(SR_AMD_SEQ),0)
                                  FROM  BKG_SR_CRNT_RQST
                                  WHERE SR_KND_CD = X.SR_KND_CD
                                  AND SR_AMD_TP_CD = X.SR_AMD_TP_CD
                                  AND BKG_NO = X.BKG_NO
                                  AND SR_NO = X.SR_NO)
)
WHERE ROWNUM = 1
  )			]]></sql>
			<params>
				<param name="src_cd" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ui_grp_cd" type="12" value="" out="N"/>
				<param name="message" type="12" value="" out="N"/>
				<param name="rsn_bkg_mn_flg" type="12" value="" out="N"/>
				<param name="rsn_cust_info_flg" type="12" value="" out="N"/>
				<param name="rsn_frt_chg_flg" type="12" value="" out="N"/>
				<param name="rsn_cntr_flg" type="12" value="" out="N"/>
				<param name="rsn_cntr_mf_flg" type="12" value="" out="N"/>
				<param name="rsn_dcgo_flg" type="12" value="" out="N"/>
				<param name="rsn_awk_cgo_flg" type="12" value="" out="N"/>
				<param name="rsn_rc_flg" type="12" value="" out="N"/>
				<param name="rsn_bb_cgo_flg" type="12" value="" out="N"/>
				<param name="rsn_rly_port_flg" type="12" value="" out="N"/>
				<param name="rsn_new_bkg_flg" type="12" value="" out="N"/>
				<param name="rsn_split_flg" type="12" value="" out="N"/>
				<param name="rsn_bl_info_flg" type="12" value="" out="N"/>
				<param name="rsn_hbl_flg" type="12" value="" out="N"/>
				<param name="cust_verif_flg" type="12" value="" out="N"/>
				<param name="rtn_to_usr_eml" type="12" value="" out="N"/>
				<param name="eml_subj_ctnt" type="12" value="" out="N"/>
				<param name="eml_cpy_to_cust_flg" type="12" value="" out="N"/>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
				<param name="wrk_st_tm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
