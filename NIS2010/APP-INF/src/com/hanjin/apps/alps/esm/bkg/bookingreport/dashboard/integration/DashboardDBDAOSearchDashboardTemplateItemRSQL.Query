<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAOSearchDashboardTemplateItemRSQL">
			<desc><![CDATA[template에 정의된 Item List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
  A.RPT_FOM_NM
, B.DBD_IRR_TP_CD
, C.COL_NM
FROM BKG_DBD_RPT_FOM A, BKG_DBD_RPT_FOM_DTL B
, BKG_DBD_RPT_COL C
WHERE 1=1
AND A.RPT_FOM_NO = @[f_rpt_fom_no]
AND A.RPT_FOM_NO = B.RPT_FOM_NO
AND B.RPT_ID = C.RPT_ID
AND B.DBD_IRR_TP_CD = C.DBD_IRR_TP_CD
ORDER BY B.DP_SEQ			]]></sql>
			<params>
				<param name="f_rpt_fom_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
