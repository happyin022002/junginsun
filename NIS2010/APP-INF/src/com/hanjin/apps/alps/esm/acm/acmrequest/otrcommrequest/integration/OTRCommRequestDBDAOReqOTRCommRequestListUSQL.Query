<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OTRCommRequestDBDAOReqOTRCommRequestListUSQL">
			<desc><![CDATA[ReqOTRCommRequestList]]></desc>
			<sql><![CDATA[
UPDATE ACM_AGN_OTR_COMM
       SET
         (  AC_STS_CD,
            RQST_USR_ID,
            RQST_DT,
            RQST_GDT,
            UPD_USR_ID,
            UPD_DT
         )
         =
         (     SELECT
                     'RS'                                                 AS AC_STS_CD,
                     @[usr_id]                                            AS RQST_USR_ID,
                     SYSDATE                                              AS RQST_DT,
                     GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SYSDATE,'GMT')  AS RQST_GDT,
                     @[usr_id]                                            AS UPD_USR_ID,
                     SYSDATE                                              AS UPD_DT
                FROM MDM_ORGANIZATION
               WHERE OFC_CD             = @[agn_cd]
                 AND NVL(DELT_FLG, 'N') = 'N'
         )
     WHERE OTR_COMM_NO  = @[otr_comm_no]
       AND AGN_CD       = @[agn_cd]
       AND IO_BND_CD    = @[io_bnd_cd]
       AND AC_TP_CD     = @[ac_tp_cd]
       AND AC_SEQ       = @[ac_seq]
       AND AC_STS_CD
        IN
         ( 'CS'
         )			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="otr_comm_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ac_tp_cd" type="12" value="" out="N"/>
				<param name="ac_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
