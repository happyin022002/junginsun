<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAORsltRtRoutHdrHistoryListVORSQL">
			<desc><![CDATA[Rate - Route조회
* 2013.10.25 전윤주 [CHM-201327107] S/C Note 하 Chassis 항목 추가
* 2015.04.06 송호진 [CHM-201534007] Fixed index 개발
]]></desc>
			<sql><![CDATA[
WITH MN AS (
  SELECT LGCY_IF_FLG FROM PRI_SP_MN WHERE PROP_NO = @[prop_no] AND AMDT_SEQ = @[amdt_seq] AND ROWNUM = 1
)
SELECT /*+ USE_HASH(A B C D E F G H) */
       A.PROP_NO
      ,A.AMDT_SEQ
      ,A.SVC_SCP_CD
      ,A.GEN_SPCL_RT_TP_CD
      ,A.CMDT_HDR_SEQ
      ,A.ROUT_SEQ
      ,B.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) +
              NVL(DECODE(G.SRC_INFO_CD, NULL, 0, 'AD', 0, 1), 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS ORG_ROUT_PNT_LOC_DEF_CD_PROP
      ,C.ROUT_VIA_PORT_DEF_CD AS ORG_ROUT_VIA_PORT_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) +
              NVL(DECODE(G.SRC_INFO_CD, NULL, 0, 'AD', 0, 1), 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS ORG_ROUT_VIA_PORT_DEF_CD_PROP
      ,D.ROUT_VIA_PORT_DEF_CD AS DEST_ROUT_VIA_PORT_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) +
              NVL(DECODE(G.SRC_INFO_CD, NULL, 0, 'AD', 0, 1), 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS DEST_ROUT_VIA_PORT_DEF_CD_PROP
      ,E.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) +
              NVL(DECODE(G.SRC_INFO_CD, NULL, 0, 'AD', 0, 1), 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS DEST_ROUT_PNT_LOC_DEF_CD_PROP
      ,SUBSTR(REGEXP_SUBSTR(F.NOTE_CLSS_NM, ', GRI') ||
              REGEXP_SUBSTR(F.NOTE_CLSS_NM, ', Surcharge') ||
              REGEXP_SUBSTR(F.NOTE_CLSS_NM, ', DEM/DET') ||
              REGEXP_SUBSTR(F.NOTE_CLSS_NM, ', Chassis') ||
              REGEXP_SUBSTR(F.NOTE_CLSS_NM, ', Others')
             ,3) AS NOTE_CLSS_NM
      ,DECODE(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) + NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) +
              NVL(DECODE(G.SRC_INFO_CD, NULL, 0, 'AD', 0, 1), 0) + NVL(H.ND_CNT, 0)
             ,0
             ,'FontStrikethru=''TRUE'';'
             ,'') AS NOTE_CLSS_NM_PROP
      ,REPLACE(F.NOTE_TOOLTIP, '^|^', CHR(13) || CHR(13)) AS NOTE_CLSS_NM_TOOLTIP
      ,G.DIR_CALL_FLG
      ,(NVL(B.ND_CNT, 0) + NVL(C.ND_CNT, 0) + NVL(D.ND_CNT, 0) +
       NVL(E.ND_CNT, 0) + NVL(F.ND_CNT, 0) +
       NVL(DECODE(G.SRC_INFO_CD, NULL, 0, 'AD', 0, 1), 0) + NVL(H.ND_CNT, 0)) AS ND_CNT
      ,(NVL(B.NA_CNT, 0) + NVL(C.NA_CNT, 0) + NVL(D.NA_CNT, 0) +
       NVL(E.NA_CNT, 0) + NVL(F.NA_CNT, 0) +
       NVL(DECODE(G.N1ST_CMNC_AMDT_SEQ, G.AMDT_SEQ, 1, 0), 0) + NVL(H.NA_CNT, 0)) AS NA_CNT
      ,DECODE((SELECT DECODE(LGCY_IF_FLG, 'Y', 0, AMDT_SEQ) AS AMDT_SEQ FROM PRI_SP_MN WHERE PROP_NO = A.PROP_NO AND AMDT_SEQ = A.AMDT_SEQ AND ROWNUM = 1)
             ,0
             ,''
             ,DECODE(NVL(B.NA_CNT, 0) + NVL(C.NA_CNT, 0) + NVL(D.NA_CNT, 0) + NVL(E.NA_CNT, 0) + NVL(F.NA_CNT, 0) +
                     NVL(DECODE(G.N1ST_CMNC_AMDT_SEQ, G.AMDT_SEQ, 1, 0), 0) + NVL(H.NA_CNT, 0)
                    ,0
                    ,''
                    ,'color:red;')) AS ROW_PROPERTIES
      ,A.NOTE_DP_SEQ
      ,A.N1ST_CMNC_AMDT_SEQ
      ,A.CRE_USR_ID
      ,A.CRE_DT
      ,A.UPD_USR_ID
      ,A.UPD_DT
      ,@[note_dp_seq] || '.' || ROW_NUMBER() OVER (PARTITION BY A.PROP_NO, A.AMDT_SEQ, A.SVC_SCP_CD, A.GEN_SPCL_RT_TP_CD, A.CMDT_HDR_SEQ ORDER BY B.SORT_KEY, E.SORT_KEY, C.SORT_KEY, D.SORT_KEY, G.DIR_CALL_FLG DESC) AS RN
      ,DECODE ( A.FX_RT_FLG, 'Y', '1', 'N', '0', '0' ) AS FX_RT_FLG
  FROM PRI_SP_SCP_RT_CMDT_ROUT A, MN
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(N1ST_CMNC_AMDT_SEQ, AMDT_SEQ, 1, 0)) AS NA_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD, ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,N1ST_CMNC_AMDT_SEQ
                      ,SVC_SCP_CD
                      ,GEN_SPCL_RT_TP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_DEF_CD
                      ,ROUT_PNT_LOC_TP_CD
                      ,RCV_DE_TERM_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                  FROM PRI_SP_SCP_RT_ROUT_PNT, MN
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'O'
                   AND SRC_INFO_CD <> DECODE(LGCY_IF_FLG, 'Y', 'AD', 'X'))
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,GEN_SPCL_RT_TP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) B
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(N1ST_CMNC_AMDT_SEQ, AMDT_SEQ, 1, 0)) AS NA_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,N1ST_CMNC_AMDT_SEQ
                      ,SVC_SCP_CD
                      ,GEN_SPCL_RT_TP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_DEF_CD
                      ,ROUT_VIA_PORT_TP_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                  FROM PRI_SP_SCP_RT_ROUT_VIA, MN
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'O'
                   AND SRC_INFO_CD <> DECODE(LGCY_IF_FLG, 'Y', 'AD', 'X'))
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,GEN_SPCL_RT_TP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) C
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(N1ST_CMNC_AMDT_SEQ, AMDT_SEQ, 1, 0)) AS NA_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, ', ')), 3) AS ROUT_VIA_PORT_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2') || ROUT_VIA_PORT_DEF_CD, '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,N1ST_CMNC_AMDT_SEQ
                      ,SVC_SCP_CD
                      ,GEN_SPCL_RT_TP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_VIA_PORT_DEF_CD
                      ,ROUT_VIA_PORT_TP_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD) AS RN
                  FROM PRI_SP_SCP_RT_ROUT_VIA, MN
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'D'
                   AND SRC_INFO_CD <> DECODE(LGCY_IF_FLG, 'Y', 'AD', 'X'))
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,GEN_SPCL_RT_TP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) D
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(N1ST_CMNC_AMDT_SEQ, AMDT_SEQ, 1, 0)) AS NA_CNT
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD, ', ')), 3) AS ROUT_PNT_LOC_DEF_CD
              ,MAX(SYS_CONNECT_BY_PATH(DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2') || ROUT_PNT_LOC_DEF_CD || DECODE(RCV_DE_TERM_CD, NULL, '0', 'Y', '1', 'D', '2', 'S', '3', 'T', '4', 'I', '5', 'L', '6', 'O', '7', 'U', '8'), '|')) AS SORT_KEY
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,N1ST_CMNC_AMDT_SEQ
                      ,SVC_SCP_CD
                      ,GEN_SPCL_RT_TP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,ORG_DEST_TP_CD
                      ,ROUT_PNT_LOC_DEF_CD
                      ,ROUT_PNT_LOC_TP_CD
                      ,RCV_DE_TERM_CD
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD, DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD) AS RN
                  FROM PRI_SP_SCP_RT_ROUT_PNT, MN
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND ORG_DEST_TP_CD = 'D'
                   AND SRC_INFO_CD <> DECODE(LGCY_IF_FLG, 'Y', 'AD', 'X'))
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,GEN_SPCL_RT_TP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ
                 ,ORG_DEST_TP_CD) E
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(N1ST_CMNC_AMDT_SEQ, AMDT_SEQ, 1, 0)) AS NA_CNT
              ,MAX(SYS_CONNECT_BY_PATH(NOTE_CLSS_NM, ', ')) AS NOTE_CLSS_NM
              ,SUBSTR(MAX(SYS_CONNECT_BY_PATH(NOTE_TOOLTIP, '^|^')), 4) AS NOTE_TOOLTIP
          FROM (SELECT PROP_NO
                      ,AMDT_SEQ
                      ,N1ST_CMNC_AMDT_SEQ
                      ,SVC_SCP_CD
                      ,GEN_SPCL_RT_TP_CD
                      ,CMDT_HDR_SEQ
                      ,ROUT_SEQ
                      ,(SELECT INTG_CD_VAL_DP_DESC
                          FROM COM_INTG_CD_DTL
                         WHERE INTG_CD_ID = 'CD01711'
                           AND INTG_CD_VAL_CTNT = NOTE_CLSS_CD
                           AND ROWNUM = 1) AS NOTE_CLSS_NM
                      ,'<' || (SELECT INTG_CD_VAL_DP_DESC
                                 FROM COM_INTG_CD_DTL
                                WHERE INTG_CD_ID = 'CD01711'
                                  AND INTG_CD_VAL_CTNT = NOTE_CLSS_CD
                                  AND ROWNUM = 1) || '>' || CHR(13) ||
                       NOTE_CTNT AS NOTE_TOOLTIP
                      ,PRC_PROG_STS_CD
                      ,SRC_INFO_CD
                      ,ROW_NUMBER() OVER(PARTITION BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ ORDER BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, ROUT_SEQ, DECODE(NOTE_CLSS_CD, 'G', '1', 'S', '2', 'D', '3', 'C', '4', 'O', '5')) AS RN
                  FROM PRI_SP_SCP_RT_CMDT_RNOTE, MN
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND SRC_INFO_CD <> DECODE(LGCY_IF_FLG, 'Y', 'AD', 'X'))
         START WITH RN = 1
        CONNECT BY PRIOR ROUT_SEQ = ROUT_SEQ
               AND PRIOR RN = RN - 1
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,GEN_SPCL_RT_TP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ) F
      ,PRI_SP_SCP_RT_ROUT_DIR G
      ,(SELECT PROP_NO
              ,AMDT_SEQ
              ,SVC_SCP_CD
              ,GEN_SPCL_RT_TP_CD
              ,CMDT_HDR_SEQ
              ,ROUT_SEQ
              ,SUM(DECODE(SRC_INFO_CD, 'AD', 0, 1)) AS ND_CNT
              ,SUM(DECODE(N1ST_CMNC_AMDT_SEQ, AMDT_SEQ, 1, 0)) AS NA_CNT
          FROM PRI_SP_SCP_RT, MN
         WHERE PROP_NO = @[prop_no]
           AND AMDT_SEQ = @[amdt_seq]
           AND SVC_SCP_CD = @[svc_scp_cd]
           AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
           AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
           AND SRC_INFO_CD <> DECODE(LGCY_IF_FLG, 'Y', 'AD', 'X')
         GROUP BY PROP_NO
                 ,AMDT_SEQ
                 ,SVC_SCP_CD
                 ,GEN_SPCL_RT_TP_CD
                 ,CMDT_HDR_SEQ
                 ,ROUT_SEQ) H
 WHERE A.PROP_NO = B.PROP_NO
   AND A.AMDT_SEQ = B.AMDT_SEQ
   AND A.SVC_SCP_CD = B.SVC_SCP_CD
   AND A.GEN_SPCL_RT_TP_CD = B.GEN_SPCL_RT_TP_CD
   AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ
   AND A.ROUT_SEQ = B.ROUT_SEQ
   AND A.PROP_NO = C.PROP_NO(+)
   AND A.AMDT_SEQ = C.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = C.SVC_SCP_CD(+)
   AND A.GEN_SPCL_RT_TP_CD = C.GEN_SPCL_RT_TP_CD(+)
   AND A.CMDT_HDR_SEQ = C.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = C.ROUT_SEQ(+)
   AND A.PROP_NO = D.PROP_NO(+)
   AND A.AMDT_SEQ = D.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = D.SVC_SCP_CD(+)
   AND A.GEN_SPCL_RT_TP_CD = D.GEN_SPCL_RT_TP_CD(+)
   AND A.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = D.ROUT_SEQ(+)
   AND A.PROP_NO = E.PROP_NO
   AND A.AMDT_SEQ = E.AMDT_SEQ
   AND A.SVC_SCP_CD = E.SVC_SCP_CD
   AND A.GEN_SPCL_RT_TP_CD = E.GEN_SPCL_RT_TP_CD
   AND A.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ
   AND A.ROUT_SEQ = E.ROUT_SEQ
   AND A.PROP_NO = F.PROP_NO(+)
   AND A.AMDT_SEQ = F.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = F.SVC_SCP_CD(+)
   AND A.GEN_SPCL_RT_TP_CD = F.GEN_SPCL_RT_TP_CD(+)
   AND A.CMDT_HDR_SEQ = F.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = F.ROUT_SEQ(+)
   AND A.PROP_NO = G.PROP_NO(+)
   AND A.AMDT_SEQ = G.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = G.SVC_SCP_CD(+)
   AND A.GEN_SPCL_RT_TP_CD = G.GEN_SPCL_RT_TP_CD(+)
   AND A.CMDT_HDR_SEQ = G.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = G.ROUT_SEQ(+)
   AND A.PROP_NO = H.PROP_NO(+)
   AND A.AMDT_SEQ = H.AMDT_SEQ(+)
   AND A.SVC_SCP_CD = H.SVC_SCP_CD(+)
   AND A.GEN_SPCL_RT_TP_CD = H.GEN_SPCL_RT_TP_CD(+)
   AND A.CMDT_HDR_SEQ = H.CMDT_HDR_SEQ(+)
   AND A.ROUT_SEQ = H.ROUT_SEQ(+)
   AND A.PROP_NO = @[prop_no]
   AND A.AMDT_SEQ = @[amdt_seq]
   AND A.SVC_SCP_CD = @[svc_scp_cd]
   AND A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
   AND A.CMDT_HDR_SEQ = @[cmdt_hdr_seq]
#if (${IS_CONVERSION} == 'N') 
   AND (EXISTS (SELECT 'OK'
          FROM (SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, 0 AS ROUT_SEQ
                  FROM PRI_SP_SCP_RT_CMDT, MN
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND AMDT_SEQ = N1ST_CMNC_AMDT_SEQ
                   AND SRC_INFO_CD <> DECODE(MN.LGCY_IF_FLG, 'Y', 'AD', 'X')
                 GROUP BY PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ
                UNION
                SELECT PROP_NO, AMDT_SEQ, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, CMDT_HDR_SEQ, 0 AS ROUT_SEQ
                  FROM PRI_SP_SCP_RT_ACT_CUST, MN
                 WHERE PROP_NO = @[prop_no]
                   AND AMDT_SEQ = @[amdt_seq]
                   AND SVC_SCP_CD = @[svc_scp_cd]
                   AND GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
                   AND CMDT_HDR_SEQ = @[cmdt_hdr_seq]
                   AND AMDT_SEQ = N1ST_CMNC_AMDT_SEQ
