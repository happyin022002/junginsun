<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOcheckBookingRSQL">
			<desc><![CDATA[checkBooking]]></desc>
			<sql><![CDATA[
SELECT  B.BL_NO
  FROM  BKG_BOOKING B
       ,BKG_CONTAINER C
       ,BKG_VVD V
 WHERE  B.BKG_NO = C.BKG_NO
   AND  B.BKG_NO = V.BKG_NO
   AND  V.VSL_CD = SUBSTR(@[vvd], 1,4)
   AND  V.SKD_VOY_NO = SUBSTR(@[vvd], 5,4)
   AND  V.SKD_DIR_CD = SUBSTR(@[vvd], 9,1)
   AND  DECODE(@[bound], 'I', V.POD_CD, V.POL_CD) = @[port]
   AND  B.BL_NO > ' '
   AND  B.BKG_STS_CD IN ('F','W')
   AND  ROWNUM = 1			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="port" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
