<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQBalanceDBDAOCoaCntrRepoShtgInfoVOCSQL">
			<desc><![CDATA[* 2012.03.22 김종준 [CHM-201217091-01] EMU 관련 로직 보완/변경 검토 요청의 건 - COA_CNTR_REPO_SHTG_INFO 테이블의 인서트]]></desc>
			<sql><![CDATA[
INSERT INTO COA_CNTR_REPO_SHTG_INFO
(
 COST_YRMON      
,CNTR_TPSZ_CD    
,ECC_CD          
,LCC_CD          
,RCC_CD          
,EQ_REPO_CR_RTO
,IMBAL_AMT       
,MB_AMT
,UPD_USR_ID
,CRE_USR_ID      
,CRE_DT          
,CNTR_ORG_DEST_CD
)VALUES(
		@[cost_yrmon]
	   ,REPLACE(@[cntr_tpsz_cd], 'RD', 'R')
	   ,@[ecc_cd]
	   ,(SELECT DISTINCT LCC_CD FROM COA_LOCATION_V WHERE ECC_CD =@[ecc_cd] )
	   ,(SELECT DISTINCT RCC_CD FROM COA_LOCATION_V WHERE ECC_CD =@[ecc_cd] )
	   ,@[eq_repo_cr_rto] / 100 
	   ,@[imbal_amt] 
	   ,@[mb_amt] / 100 
 	   ,@[upd_usr_id]
	   ,@[cre_usr_id]
	   ,SYSDATE
	   ,@[cntr_org_dest_cd]
		)			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="eq_repo_cr_rto" type="12" value="" out="N"/>
				<param name="imbal_amt" type="12" value="" out="N"/>
				<param name="mb_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cntr_org_dest_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
