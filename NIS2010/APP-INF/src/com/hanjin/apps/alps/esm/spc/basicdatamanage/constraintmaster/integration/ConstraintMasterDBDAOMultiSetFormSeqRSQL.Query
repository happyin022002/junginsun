<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOMultiSetFormSeqRSQL">
			<desc><![CDATA[rpt_fom_no seq+1구하기]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(MAX(TO_NUMBER(rpt_fom_no)),0)+1 seq 
 FROM SPC_RPT_FOM
  WHERE PGM_NO     = @[pgm_no] 
    AND CRE_USR_ID = @[cre_usr_id]
  GROUP BY CRE_USR_ID			]]></sql>
			<params>
				<param name="pgm_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
