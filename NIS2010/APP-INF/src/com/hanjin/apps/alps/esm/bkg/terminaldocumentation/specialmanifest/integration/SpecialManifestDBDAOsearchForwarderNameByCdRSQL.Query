<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchForwarderNameByCdRSQL">
			<desc><![CDATA[Forward Code로 Forward Name을 조회한다.]]></desc>
			<sql><![CDATA[

SELECT FWRD_NM
FROM BKG_CSTMS_EUR_DG_FWRD
WHERE ANR_FWRD_ID = @[frm_fwrd_cd]			]]></sql>
			<params>
				<param name="frm_fwrd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
