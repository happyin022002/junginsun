<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshOdcyReqDBDAOAddShippingRequestCSQL">
			<desc><![CDATA[AddShippingRequest]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CUST_SHP_RQST (
      BKG_NO
    , CNTR_NO
    , CNTR_SEQ
    , CNTR_TPSZ_CD
    , CNTR_SEAL_NO
    , PCK_QTY
    , PCK_TP_CD
    , CNTR_WGT
    , WGT_UT_CD
    , MEAS_QTY
    , MEAS_UT_CD
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
    , CNTR_VOL_QTY
    , VNDR_SEQ
)
VALUES
(
  @[bkg_no]
 ,@[cntr_no]
 ,(SELECT NVL(MAX(CNTR_SEQ) +1, 1) FROM BKG_CUST_SHP_RQST WHERE BKG_NO = @[bkg_no] AND CNTR_NO = @[cntr_no])
 ,@[cntr_tpsz_cd]
 ,@[cntr_seal_no]
 ,@[pck_qty]
 ,@[pck_tp_cd]
 ,@[cntr_wgt]
 ,@[wgt_ut_cd]
 ,@[meas_qty]
 ,@[meas_ut_cd]
 ,'SPPUSR'
 ,SYSDATE
 ,'SPPUSR'
 ,SYSDATE
 ,@[cntr_vol_qty]
 ,@[vndr_seq]
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_seal_no" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="meas_ut_cd" type="12" value="" out="N"/>
				<param name="cntr_vol_qty" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
