<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAORsltPriQuotationCreationDateVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT  CRE_DT  AS CD, TO_CHAR(CRE_DT,'YYYY-MM-DD') AS NM
	FROM PRI_RQ_MN
	WHERE QTTN_NO = @[qttn_no]
		AND QTTN_VER_NO = @[qttn_ver_no]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
