<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOCheckLocCodeRSQL">
			<desc><![CDATA[Check Location Code]]></desc>
			<sql><![CDATA[
SELECT LOC_CD 
FROM MDM_LOCATION 
WHERE 1=1 
AND LOC_CD = @[loc_cd]
AND DELT_FLG ='N'
			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
