<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOUpdateVGMForOnlyMVMTUSQL">
			<desc><![CDATA[CTM에서 같은 MVMT로 VGM이 없는 EDI를 보냈다가
그 후 한 번 VGM 값이 있는 EDI를 재전송할 때를 위한 업데이트문
이미 BKG에 VGM값이 있으면 업데이트 해주지 않음]]></desc>
			<sql><![CDATA[
UPDATE BKG_CONTAINER
SET  VGM_WGT           = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN TO_NUMBER(@[vgm_wgt_qty]) ELSE VGM_WGT END) 
    ,VGM_WGT_UT_CD     = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN DECODE(@[vgm_wgt_ut_cd], 'KGM', 'KGS','LBR','LBS', @[vgm_wgt_ut_cd]) ELSE VGM_WGT_UT_CD END)
    ,VGM_VRFY_SIG_CTNT = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN @[vgm_sig_ctnt] ELSE VGM_VRFY_SIG_CTNT END)
    ,VGM_MZD_TP_CD     = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN @[vgm_mzd_tp_cd] ELSE VGM_MZD_TP_CD END)
    ,UPD_USR_ID        = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN @[upd_usr_id] ELSE UPD_USR_ID END)
    ,UPD_DT            = (CASE WHEN NVL(VGM_WGT,0) = 0 THEN SYSDATE ELSE UPD_DT END)
WHERE BKG_NO = @[bkg_no]
AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="vgm_wgt_qty" type="12" value="" out="N"/>
				<param name="vgm_wgt_ut_cd" type="12" value="" out="N"/>
				<param name="vgm_sig_ctnt" type="12" value="" out="N"/>
				<param name="vgm_mzd_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
