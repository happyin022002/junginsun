<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MaxLoadFactorDBDAOMultiSpcMaxLodFctrUSQL">
			<desc><![CDATA[MAX  L/F UPDATE]]></desc>
			<sql><![CDATA[
UPDATE SPC_MAX_LOD_FCTR
   SET MAX_LDF_RTO = @[max_ldf_rto],
       UPD_USR_ID  = @[upd_usr_id] ,
       UPD_DT      = SYSDATE
 WHERE BSE_YR      = @[bse_yr]
   AND VOY_DIFF_WK = @[voy_diff_wk]
   AND RLANE_CD    = @[rlane_cd]
   AND DIR_CD      = @[dir_cd]			]]></sql>
			<params>
				<param name="max_ldf_rto" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="voy_diff_wk" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
