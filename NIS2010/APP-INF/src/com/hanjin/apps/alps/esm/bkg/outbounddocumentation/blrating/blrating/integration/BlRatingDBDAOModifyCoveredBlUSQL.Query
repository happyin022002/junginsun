<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOModifyCoveredBlUSQL">
			<desc><![CDATA[ModifyCoveredBl
 * 2011.04.18 이일민 [CHM-201110112] BKG HISTORY 추가 요청 - bill type change 
]]></desc>
			<sql><![CDATA[
#if (${caflag} == 'Y') 
 UPDATE BKG_RT_HIS
 SET 
    RT_BL_TP_CD = @[bl_cvrd_tp_cd]
 WHERE   
	BKG_NO = 
	(
	    SELECT BKG_NO FROM BKG_BKG_HIS WHERE BL_NO =  substr(@[bl_no],0,12)  AND CORR_NO = 'TMP0000001'
	)

#else 

 UPDATE BKG_RATE
 SET 
    RT_BL_TP_CD = @[bl_cvrd_tp_cd]
 WHERE   
	BKG_NO = 
	(
	    SELECT BKG_NO FROM BKG_BOOKING WHERE BL_NO =  substr(@[bl_no],0,12) 
	)
#end			]]></sql>
			<params>
				<param name="bl_cvrd_tp_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
