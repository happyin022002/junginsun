<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSASpecialManifestDBDAOremovePsaDgListDSQL">
			<desc><![CDATA[위험물 정보들을 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CSTMS_EUR_DG
WHERE	EUR_DG_DECL_TP_CD = @[d_type]
AND	VSL_CD = SUBSTR(@[vvd_cd], 1, 4)
AND	SKD_VOY_NO = SUBSTR(@[vvd_cd], 5, 4)
AND	SKD_DIR_CD = SUBSTR(@[vvd_cd], 9, 1)
AND	PORT_CD = @[port_cd]
AND	BL_NO = @[bl_no]
AND	CNTR_NO = @[cntr_no]
AND	CNTR_CGO_SEQ = @[cntr_cgo_seq]			]]></sql>
			<params>
				<param name="d_type" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cgo_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
