<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustRPTDAOMRNRSQL">
			<desc><![CDATA[BKG_CSTMS_KR_DL_HIS 테이블을 MRN으로 조회한다.]]></desc>
			<sql><![CDATA[
SELECT MRN_NO||MRN_CHK_NO			   AS MRN
     , VSL_CD||SKD_VOY_NO||SKD_DIR_CD  AS VVD
     , NVL(POL_CD, ' ')				   AS POL
     , NVL(POD_CD, ' ')				   AS POD
     , NVL(OFC_CD, ' ')				   AS OFFICE
     , NVL(CRE_USR_ID, ' ')			   AS USERID
     , NVL(BL_KNT, 0)				   AS BLCOUNT
     , NVL(KR_CSTMS_DL_ACT_CD, ' ')	   AS AC	
     , TO_CHAR(CRE_DT, 'YYYYMMDD')	   AS DT
     , TO_CHAR(CRE_DT, 'HH24:MI:SS')   AS DT2
  FROM BKG_CSTMS_KR_DL_HIS
#if(${txt_mrn} != '')
 WHERE MRN_NO		= SUBSTR(@[txt_mrn], 1, 10)
   AND MRN_CHK_NO	= SUBSTR(@[txt_mrn], 11, 1)
#end			]]></sql>
			<params>
				<param name="txt_mrn" type="12" value="08HJSCE5989" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
