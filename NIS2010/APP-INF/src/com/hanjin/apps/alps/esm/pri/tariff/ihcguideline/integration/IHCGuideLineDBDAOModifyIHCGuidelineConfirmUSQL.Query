<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IHCGuideLineDBDAOModifyIHCGuidelineConfirmUSQL">
			<desc><![CDATA[modify IHCGuideline confirm date]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRF_IHC_MN
   SET CFM_DT     = SYSDATE
     , FIC_PROP_STS_CD = @[fic_prop_sts_cd]
     , CFM_USR_ID = @[cfm_usr_id]
     , CFM_OFC_CD = @[cfm_ofc_cd]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT     = SYSDATE
 WHERE SVC_SCP_CD       = @[svc_scp_cd] 
   AND ORG_DEST_TP_CD   = @[org_dest_tp_cd]
   AND IHC_TRF_NO       = @[ihc_trf_no]
   AND AMDT_SEQ         = @[amdt_seq]			]]></sql>
			<params>
				<param name="fic_prop_sts_cd" type="12" value="" out="N"/>
				<param name="cfm_usr_id" type="12" value="" out="N"/>
				<param name="cfm_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="ihc_trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
