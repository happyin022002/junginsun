<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteConversionProposalDBDAOPriScStandardNoteConvDurationUSQL">
			<desc><![CDATA[stanard note duration update ì‹œ conversion duration update]]></desc>
			<sql><![CDATA[
UPDATE PRI_SC_NOTE_CONV SET	
	   EFF_DT					= CASE WHEN ((TO_CHAR(EFF_DT,'YYYY-MM-DD') > @[eff_dt] AND TO_CHAR(EFF_DT,'YYYY-MM-DD') < @[exp_dt] 
	                                          AND TO_CHAR(EFF_DT,'YYYY-MM-DD') != @[eff_dt_hidden]) OR TO_CHAR(EFF_DT,'YYYY-MM-DD') = '9999-12-31' ) 
        	                           THEN EFF_DT
        	                           WHEN (TO_CHAR(EFF_DT,'YYYY-MM-DD') = @[eff_dt_hidden])
        	                           THEN TO_DATE(@[eff_dt],'YYYY-MM-DD')
			                           ELSE TO_DATE(@[eff_dt],'YYYY-MM-DD') END	                               
	 , EXP_DT					= CASE WHEN ((TO_CHAR(EXP_DT,'YYYY-MM-DD') < @[exp_dt] AND TO_CHAR(EXP_DT,'YYYY-MM-DD') > @[eff_dt] 
	                                          AND TO_CHAR(EXP_DT,'YYYY-MM-DD') != @[exp_dt_hidden]) OR TO_CHAR(EXP_DT,'YYYY-MM-DD') = '9999-12-31' )  
        	                           THEN EXP_DT
        	                           WHEN (TO_CHAR(EXP_DT,'YYYY-MM-DD') = @[exp_dt_hidden])
        	                           THEN TO_DATE(@[exp_dt],'YYYY-MM-DD')
        	                           ELSE TO_DATE(@[exp_dt],'YYYY-MM-DD') END

WHERE NOTE_CONV_MAPG_ID IN (
                    		SELECT NOTE_CONV_MAPG_ID
                    		  FROM PRI_SG_STND_NOTE_CTNT
                    		 WHERE SVC_SCP_CD 				= @[svc_scp_cd]
                    		   AND NOTE_HDR_SEQ 			= @[note_hdr_seq]
                    	   )			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="eff_dt_hidden" type="12" value="" out="N"/>
				<param name="exp_dt_hidden" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="note_hdr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
