<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchTotalCustomerRSQL">
			<desc><![CDATA[SearchTotalCustomer]]></desc>
			<sql><![CDATA[
SELECT COUNT(*)
  	FROM MDM_CUSTOMER A
 	WHERE A.DELT_FLG <> 'Y'
	AND NVL(NMD_CUST_FLG, 'N') = 'N' 
#if(${cust_cnt_cd} != '')
	AND A.CUST_CNT_CD||lpad(A.CUST_SEQ,6,0) LIKE @[cust_cnt_cd] || '%'
#end

#if(${cust_nm} != '')
	#if(${include} == 'on')
		AND upper(A.CUST_LGL_ENG_NM) LIKE '%' || upper(@[cust_nm]) || '%'
	#else
		AND upper(A.CUST_LGL_ENG_NM) LIKE upper(@[cust_nm]) || '%'
	#end
#end

#if(${ofc_cd} != '')
AND A.OFC_CD LIKE @[ofc_cd] || '%'
#end

#if (${no_use} != '') 
  AND (A.SLS_DELT_EFF_DT IS NULL OR A.SLS_DELT_EFF_DT IS NOT NULL)
#else
  AND A.SLS_DELT_EFF_DT IS NULL
#end
			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
