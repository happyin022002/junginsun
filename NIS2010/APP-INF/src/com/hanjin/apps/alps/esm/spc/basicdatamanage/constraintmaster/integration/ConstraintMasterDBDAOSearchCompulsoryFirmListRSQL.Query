            , (SELECT WM_CONCAT( CNTR_TPSZ_CD ) FROM BKG_QUANTITY WHERE BKG_NO = BK.BKG_NO ) AS CNTR_TPSZ_CD
            , (SELECT SUM( DECODE(SUBSTR(CNTR_TPSZ_CD, 2, 1), '2', OP_CNTR_QTY, 0) ) FROM BKG_QUANTITY
                WHERE BKG_NO = BK.BKG_NO
