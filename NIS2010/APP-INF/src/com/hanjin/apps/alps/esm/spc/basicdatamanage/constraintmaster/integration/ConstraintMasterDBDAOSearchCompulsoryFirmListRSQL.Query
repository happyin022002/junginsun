, VVD_DATA AS (
SELECT BKG_NO
     , MAX(DECODE(RN, 1, VVD)) VVD1
     , MAX(DECODE(RN, 2, VVD)) VVD2
     , MAX(DECODE(RN, 3, VVD)) VVD3
     , MAX(DECODE(RN, 4, VVD)) VVD4
     , MAX(DECODE(RN, 1, POL_CD)) POL1
     , MAX(DECODE(RN, 2, POL_CD)) POL2
     , MAX(DECODE(RN, 3, POL_CD)) POL3
     , MAX(DECODE(RN, 4, POL_CD)) POL4
     , MAX(DECODE(RN, 1, POL_ETB)) POL_ETB1
     , MAX(DECODE(RN, 2, POL_ETB)) POL_ETB2
     , MAX(DECODE(RN, 3, POL_ETB)) POL_ETB3
     , MAX(DECODE(RN, 4, POL_ETB)) POL_ETB4
  FROM (SELECT V.BKG_NO
             , V.VSL_CD||V.SKD_VOY_NO||V.SKD_DIR_CD VVD
             , V.POL_CD
             , NVL(S.VPS_ETB_DT, S.INIT_ETB_DT) POL_ETB
             , ROW_NUMBER() OVER (PARTITION BY V.BKG_NO ORDER BY V.VSL_PRE_PST_CD, V.VSL_SEQ) RN
          FROM BKG_VVD V
             , VSK_VSL_PORT_SKD S
         WHERE 1=1
		 #if(${f_bkg_no} != '')
		   AND V.BKG_NO =@[f_bkg_no]
		 #else 
   		   AND V.BKG_NO IN (SELECT BKG_NO FROM BKG_DATA)			
		 #end
           AND V.VSL_CD = S.VSL_CD(+)
           AND V.SKD_VOY_NO = S.SKD_VOY_NO(+)
           AND V.SKD_DIR_CD = S.SKD_DIR_CD(+)
           AND V.POL_CD = S.VPS_PORT_CD(+)
           AND V.POL_CLPT_IND_SEQ = S.CLPT_IND_SEQ(+)
           AND V.POL_YD_CD = S.YD_CD(+))
 GROUP BY BKG_NO
)
SELECT
AA.BKG_NO
      ,AA.BKG_STS_CD
      ,AA.STANDBY_TYPE
      ,AA.STANDBY_REASON
      ,AA.ALOC_SVC_CD
      ,AA.ALOC_STS_CD
      ,AA.CFM_RQST_FLG
      ,AA.CNDDT_CFM_FLG
      ,AA.CFM_USR_ID
      ,AA.CFM_DT
      ,AA.YR_MON_WK
#if($c_name.size() != 0)
  #foreach(${list} in ${c_name})
      , ${list}
  #end
#end
      ,AA.FEU
      ,AA.TEU
      ,AA.CUST_CTRL_CD
      , (SELECT SUM( NVL(D.BKG_REV,0) + NVL(D.BKG_OFT_REV,0) + NVL(D.BKG_MISC_REV,0) + NVL(D.SCR_CHG_REV,0) + NVL(D.DMDT_COM_AMT,0) - NVL(D.RA_CM_COST_TTL_AMT, 0))
        /SUM(DECODE(SPC_GET_CNTR_SZ_FNC(D.SPCL_CNTR_TPSZ_CD), '2', 1, 2) * D.BKG_QTY) FROM MAS_BKG_EXPN_DTL D WHERE D.BKG_NO = AA.BKG_NO) MAS_CMPB
       --PA_CM_COST_TTL_AMT, RA_CM_COST_TTL_AMT
      , AA.INIT_CMPB
	  , (SELECT MAX(RV.CMPB_AMT) KEEP (DENSE_RANK LAST ORDER BY RV.REV_COST_SEQ) FROM BKG_REV_COST RV WHERE RV.BKG_NO = AA.BKG_NO) AS CMPB
      , AA.BKG_CRE_DT , LST_SB_RMK
FROM (SELECT A.BKG_NO
          ,A.BKG_STS_CD
          ,A.STANDBY_TYPE
          ,A.STANDBY_REASON
          ,A.ALOC_SVC_CD
          ,DECODE(A.ALOC_STS_CD||A.BKG_CFM_FLG,'SY', 'Fs', A.ALOC_STS_CD) ALOC_STS_CD
          ,A.CFM_RQST_FLG
          ,A.CNDDT_CFM_FLG
          ,A.CFM_USR_ID
          ,A.CFM_DT , LST_SB_RMK
#if($c_name.size() != 0)
#foreach(${list} in ${c_name})
	#if( ${list} == 'sub_trd_cd')
          ,A.${list}
	#elseif( ${list} != 'sc_no' && ${list} != 'rfa_no' && ${list} != 'ob_sls_ofc_cd' )
          ,${list}
	#end
#end
#end
          ,A.FEU
          ,A.TEU
          ,A.SEASON
          --,AGMT_CNT_CD
          --,AGMT_CUST_SEQ
          ,A.SC_NO
          ,A.RFA_NO
#if (${f_cust_tp} != '')
           ,MCC.CUST_CTRL_CD
#else
           ,'' AS CUST_CTRL_CD
#end
           , A.OB_SLS_OFC_CD
           , A.INIT_CMPB
           , A.BKG_CRE_DT
           , A.YR_MON_WK
    FROM BKG_DATA A, SPC_MDL_CUST_CTRL MCC, VVD_DATA V
WHERE 1=1
		AND MCC.TRD_CD(+) = A.TRD_CD
--            AND MCC.SUB_TRD_CD(+) = A.SUB_TRD_CD
		AND MCC.COST_YRWK(+) = SUBSTR(A.SEASON,1,6)
		AND MCC.VER_SEQ(+) = SUBSTR(A.SEASON,8)
		AND MCC.CUST_CNT_CD(+) = A.CTRT_CUST_CNT_CD
		AND MCC.CUST_SEQ(+) = A.CTRT_CUST_SEQ
#if (${f_cust_tp} != '' && ${f_cust_tp} != 'C' && ${f_cust_tp} != '1')
		AND MCC.CUST_CTRL_CD = @[f_cust_tp]
#end
#if (${f_sb_rsn_tp_cd} != '')
		AND NVL(STANDBY_TYPE,'X') = @[f_sb_rsn_tp_cd]
#end
#if (${f_cust_tp} != '' && ${f_cust_tp} != '1' && ${f_cust_tp} == 'C')
		AND NVL(A.SC_NO, NVL(A.RFA_NO, 'X')) = NVL(MCC.SC_NO(+), NVL(MCC.RFA_NO(+), 'X'))
#end

#if (${f_ofc_vw} != '' && ${f_ofc_vw} != 'null')
			AND OB_SLS_OFC_CD = @[f_ofc_vw]
#end

#if (${f_acct_cd} != '' && ${f_acct_cd} != '1')
		AND ACOUNT_CD = @[f_acct_cd]
#end
        AND A.BKG_NO = V.BKG_NO
GROUP BY A.BKG_NO
	  , A.BKG_STS_CD
	  , A.STANDBY_TYPE
	  , A.STANDBY_REASON
	  , A.ALOC_SVC_CD
	  , DECODE(A.ALOC_STS_CD||A.BKG_CFM_FLG,'SY', 'Fs', A.ALOC_STS_CD)
	  , A.CFM_RQST_FLG
	  , A.CNDDT_CFM_FLG
	  , A.CFM_USR_ID
	  , A.CFM_DT 
      , LST_SB_RMK 
#if($c_name.size() != 0)
#foreach(${list} in ${c_name})
	#if( ${list} == 'sub_trd_cd')
          ,A.${list}
	#elseif( ${list} != 'sc_no' && ${list} != 'rfa_no' && ${list} != 'ob_sls_ofc_cd' )
          ,${list}
	#end
#end
#end
	  , A.FEU
	  , A.TEU
	  , A.SEASON
	  , A.SC_NO
	  , A.RFA_NO
#if (${f_cust_tp} != '')
	  , MCC.CUST_CTRL_CD
#end
	  , A.OB_SLS_OFC_CD
      , A.INIT_CMPB
      , A.BKG_CRE_DT
      , A.YR_MON_WK
) AA
#if (${f_cust_tp} == 'C')
WHERE NVL(CUST_CTRL_CD,'C') = @[f_cust_tp]
#end
ORDER BY ALOC_STS_CD, BKG_NO

