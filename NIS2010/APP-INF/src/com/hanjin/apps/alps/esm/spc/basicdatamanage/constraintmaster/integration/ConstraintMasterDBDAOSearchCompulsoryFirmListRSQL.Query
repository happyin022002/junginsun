                         DECODE( DECODE( L.TRD_CD, NULL, 'N','Y'), 'Y', COST_YRWK||'-'||VER_SEQ, '200001-1')
                    FROM SPC_MDL_VER_MST MST
                   WHERE W.COST_YR||W.COST_WK BETWEEN MST.VER_ST_YRWK AND MST.VER_END_YRWK
                     AND MST.CFM_FLG = 'Y'
                     AND MST.TRD_CD  = L.TRD_CD
                     AND ROWNUM    = 1) AS SEASON
            , BK.CTRT_CUST_SEQ
            , BK.CTRT_CUST_CNT_CD
