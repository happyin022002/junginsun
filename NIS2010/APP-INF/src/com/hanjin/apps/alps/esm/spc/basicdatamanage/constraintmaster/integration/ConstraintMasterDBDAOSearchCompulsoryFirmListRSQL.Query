                WHERE BKG_NO = SSB.BKG_NO
                  AND BKG_CTRL_TP_CD = DECODE(@[f_aloc_sts_cd],'A','A','S')
              ) AS ALOC_SVC_CD
            , BK.ALOC_STS_CD
