                AND CNTR_TPSZ_CD = QTY.CNTR_TPSZ_CD
#end
            ) AS TEU
            , (SELECT SUM( DECODE(SUBSTR(CNTR_TPSZ_CD, 2, 1), '2', 0, OP_CNTR_QTY) ) FROM BKG_QUANTITY
                WHERE BKG_NO = BK.BKG_NO
