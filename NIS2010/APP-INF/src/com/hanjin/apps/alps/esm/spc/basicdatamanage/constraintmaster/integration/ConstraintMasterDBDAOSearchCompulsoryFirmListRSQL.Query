            , BK.POL_CD                POL_LOC_CD
            , BK.POL_NOD_CD            POL_NOD_CD
            , 'X' AS TS_SLAN_CD
            , 'X' AS TS_DIR_CD
            , 'X' AS TS_POL_CD
            , 'X' AS TS_POD_CD
            , BK.POD_CD                POD_LOC_CD
            , BK.POD_NOD_CD            POD_NOD_CD
            , BK.DEL_CD                DEL_LOC_CD
            , BK.DEL_NOD_CD            DEL_NOD_CD
            , (SELECT SCC_CD FROM MDM_LOCATION LOC WHERE LOC.LOC_CD = BK.DEL_CD) DEL_SCC_CD
            , BK.VSL_CD || BK.SKD_VOY_NO || BK.SKD_DIR_CD VVD
---------------------------------------------------------------------------------------------<
            , DECODE(L.IOC_CD, 'T', DECODE(OFC.N2ND_PRNT_OFC_CD, 'SINRS', OFC.N4TH_PRNT_OFC_CD, BK.SLS_RHQ_CD, OFC.N4TH_PRNT_OFC_CD, 
		DECODE(L.RLANE_CD, 'WAFIE', OFC.N4TH_PRNT_OFC_CD, OFC.N2ND_PRNT_OFC_CD)), OFC.N4TH_PRNT_OFC_CD) OB_SLS_OFC_CD
--------------------------------------------------------------------------------------------->
            , DECODE(BK.DCGO_FLG,'Y','DG', DECODE(BK.RD_CGO_FLG, 'Y', 'RD','')) DGRD
            , BK.SC_NO
            , BK.RFA_NO
            , BK.CTRT_CUST_CNT_CD || NVL(LPAD(BK.CTRT_CUST_SEQ,6,'0'),'') C_CUST
            , NVL2(BK.CTRT_CUST_CNT_CD,(SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER CC WHERE CC.CUST_CNT_CD = BK.CTRT_CUST_CNT_CD AND CC.CUST_SEQ = BK.CTRT_CUST_SEQ), '') AS C_CUST_NM
            , BK.AGMT_ACT_CNT_CD  || NVL(LPAD(BK.AGMT_ACT_CUST_SEQ,6,'0'),'') A_CUST
            , NVL2(BK.AGMT_ACT_CNT_CD,(SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER CC WHERE CC.CUST_CNT_CD = BK.AGMT_ACT_CNT_CD AND CC.CUST_SEQ = BK.AGMT_ACT_CUST_SEQ), '') AS A_CUST_NM
            , SHPR.CUST_CNT_CD    || NVL(LPAD(SHPR.CUST_SEQ,6,'0'),'') SHRP
            , NVL2(SHPR.CUST_CNT_CD,(SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER CC WHERE CC.CUST_CNT_CD = SHPR.CUST_CNT_CD AND CC.CUST_SEQ = SHPR.CUST_SEQ), '') AS SHRP_NM
            , CNEE.CUST_CNT_CD    || NVL(LPAD(CNEE.CUST_SEQ,6,'0'),'') CNEE
            , NVL2(CNEE.CUST_CNT_CD,(SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER CC WHERE CC.CUST_CNT_CD = CNEE.CUST_CNT_CD AND CC.CUST_SEQ = CNEE.CUST_SEQ), '') AS CNEE_NM
            , FWDR.CUST_CNT_CD    || NVL(LPAD(FWDR.CUST_SEQ,6,'0'),'') FWDR
            , NVL2(FWDR.CUST_CNT_CD,(SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER CC WHERE CC.CUST_CNT_CD = FWDR.CUST_CNT_CD AND CC.CUST_SEQ = FWDR.CUST_SEQ), '') AS FWDR_NM
            , BK.CMDT_CD
