<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterSIBlRiderRSQL">
			<desc><![CDATA[RSQL]]></desc>
			<sql><![CDATA[
SELECT
MST.BKG_NO
,IMG.IMG_SEQ
,IMG.RIDR_TP_CD
,IMG.DCGO_SEQ
,IMG.AWK_CGO_SEQ
,IMG.BB_CGO_SEQ
,IMG.FILE_NM
,UPLD.FILE_SZ_CAPA AS FILE_SIZE
,IMG.FILE_PATH_RMK
,IMG.FILE_SAV_ID
,IMG.FILE_DESC
,MST.XTER_SNDR_ID
,MST.XTER_RQST_NO
,MST.XTER_RQST_SEQ
,'I' IBFLAG
FROM BKG_XTER_IMG_STO IMG, COM_UPLD_FILE UPLD, BKG_XTER_RQST_MST MST
WHERE MST.XTER_SNDR_ID  = @[xter_sndr_id]
AND MST.XTER_RQST_NO  = @[xter_rqst_no]
AND MST.XTER_RQST_SEQ = @[xter_rqst_seq]
AND MST.XTER_SNDR_ID  = IMG.XTER_SNDR_ID
AND MST.XTER_RQST_NO  = IMG.XTER_RQST_NO
AND MST.XTER_RQST_SEQ = IMG.XTER_RQST_SEQ 
AND IMG.FILE_SAV_ID = UPLD.FILE_SAV_ID
AND IMG.RIDR_TP_CD ='G'
ORDER BY IMG.IMG_SEQ			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
