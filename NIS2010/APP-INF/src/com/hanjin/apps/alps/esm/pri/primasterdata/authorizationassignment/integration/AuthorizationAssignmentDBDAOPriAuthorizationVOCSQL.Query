    , USR_ID
    , EFF_DT
    , EXP_DT
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
) VALUES( 
      @[prc_ctrt_tp_cd]
    , @[svc_scp_cd]
    , @[usr_id]
    , TO_DATE(@[eff_dt],'YYYYMMDD')
    , TO_DATE('99991231','YYYYMMDD')
    , @[cre_usr_id]
    , SYSDATE
    , @[cre_usr_id]
    , SYSDATE
)			]]></sql>
			<params>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
