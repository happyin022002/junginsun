<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchDgBayPlnIdRSQL">
			<desc><![CDATA[Bay Plan ID 조회]]></desc>
			<sql><![CDATA[
SELECT VSL_CD || LTRIM(TO_CHAR(BKG_CSTM_EDI_SEQ.NEXTVAL,'0000009'))
FROM MDM_VSL_CNTR
WHERE CALL_SGN_NO = @[eur_dg_call_sgn_no]			]]></sql>
			<params>
				<param name="eur_dg_call_sgn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
