<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StatusReportDBDAOSurchargeSummaryRSQL">
			<desc><![CDATA[Surcharge Summary Report]]></desc>
			<sql><![CDATA[
#if (${tab_cd} == '1')
	SELECT RHQ_BKG_OFC_CD
	       ,GSO_BKG_OFC_CD
	       ,BKG_OFC_CD
	#if (${charge_cd} == '1')
		   ,CORR_OFC_CD
	#elseif (${charge_cd} == '4')
		   ,OBL_ISS_OFC_CD
	#elseif(${charge_cd} == '5')
	   		/* 
	          Booking RHQ를 SHARC로 선택하면 아래의 테이블에 Prepaid Office와 PPD-3rd Party office를  
	          Booking RHQ를 SHARC 외에 다른 RHQ를 선택하면 아래의 테이블에 Collect Office와 CCT-3rd party Office를 체크 함.
	        */
	   		#if (${p_rhq_bkg_ofc_cd} == 'SHARC')
			   ,BKG_GET_TOKEN_FNC(PPD_CLT_RCV_OFC_CD,1) AS PPD_RCV_OFC_CD
			#else
			   ,BKG_GET_TOKEN_FNC(PPD_CLT_RCV_OFC_CD,2) AS CLT_OFC_CD 
			#end
	#end
	       ,COUNT(BKG_NO) BL_CNT
	       ,SUM(DECODE(BL_RATE_CNT,'Y',1,0)) BL_RATE_CNT
	       ,ROUND(SUM(DECODE(BL_RATE_CNT,'Y',1,0)) / COUNT(BKG_NO) * 100,2) APPL_RATIO
		   ,'USD' CURR_CD
	       ,TRIM(TO_CHAR(SUM(NVL(( SELECT ROUND(SUM(BCR.CHG_AMT/GMXR.USD_LOCL_XCH_RT),2)
			FROM BKG_RATE BR
			     ,BKG_CHG_RT BCR
			     ,GL_MON_XCH_RT GMXR
			WHERE BR.BKG_NO = BB.BKG_NO
			AND   BR.BKG_NO = BCR.BKG_NO
			AND   TO_CHAR(BR.RT_APLY_DT,'YYYYMM') = GMXR.ACCT_XCH_RT_YRMON
			AND   BCR.CURR_CD = GMXR.CURR_CD
	#if (${charge_cd} == '1')       
			AND BCR.CHG_CD IN ('BCC','MCF') 
	#elseif (${charge_cd} == '2')
			AND BCR.CHG_CD = 'OBS'
	#elseif (${charge_cd} == '3')
			AND BCR.CHG_CD = 'BLR'
	#elseif (${charge_cd} == '4' ||${charge_cd} == '5' ||${charge_cd} == '6' || ${charge_cd} == '7') -- 4 LBP, 5 TPF, 6 CTC, 7 LSI
			AND BCR.CHG_CD = DECODE(@[charge_cd],'4','LBP','5','TPF','6','CTC','7','LSI')
	#end
			AND   GMXR.ACCT_XCH_RT_LVL = '1'),0)),'999,999,999,990.99')) RATING_TTL
	FROM (
#elseif (${tab_cd} == '2')
	SELECT SVC_SCP_CD
	       ,RHQ_BKG_OFC_CD
	       ,GSO_BKG_OFC_CD
	       ,BKG_OFC_CD
	       ,POR_CD
	       ,POL_CD
	       ,POD_CD
	       ,DEL_CD
	       ,CONTRACT_NO
	       ,CUSTOMER_CODE
	       ,CUSTOMER_NAME
	       ,COUNT(BKG_NO) BL_CNT
	       ,SUM(DECODE(BL_RATE_CNT,'Y',1,0)) BL_RATE_CNT
	       ,ROUND(SUM(DECODE(BL_RATE_CNT,'Y',1,0)) / COUNT(BKG_NO) * 100,2) APPL_RATIO
		   ,'USD' CURR_CD
	       ,TRIM(TO_CHAR(SUM(NVL(( SELECT ROUND(SUM(BCR.CHG_AMT/GMXR.USD_LOCL_XCH_RT),2)
			FROM BKG_RATE BR
			     ,BKG_CHG_RT BCR
			     ,GL_MON_XCH_RT GMXR
			WHERE BR.BKG_NO = BB.BKG_NO
			AND   BR.BKG_NO = BCR.BKG_NO
			AND   TO_CHAR(BR.RT_APLY_DT,'YYYYMM') = GMXR.ACCT_XCH_RT_YRMON
			AND   BCR.CURR_CD = GMXR.CURR_CD
	#if (${charge_cd} == '1')       
			AND BCR.CHG_CD IN ('BCC','MCF')
	#elseif (${charge_cd} == '2')
			AND BCR.CHG_CD = 'OBS'
	#elseif (${charge_cd} == '3')
			AND BCR.CHG_CD = 'BLR'
	#end
			AND   GMXR.ACCT_XCH_RT_LVL = '1'),0)),'999,999,999,990.99')) RATING_TTL
	FROM (
#end   


SELECT /*+ USE_NL(BB COLB) ORDERED */
        COLB.OFC_N2ND_LVL_CD  RHQ_BKG_OFC_CD
       ,COLB.OFC_N5TH_LVL_CD GSO_BKG_OFC_CD
       ,BB.BKG_OFC_CD 
#if (${tab_cd} == '1')
	#if (${charge_cd} == '4')    
       , (SELECT OBL_ISS_OFC_CD FROM BKG_BL_ISS ISS WHERE ISS.BKG_NO = BB.BKG_NO) OBL_ISS_OFC_CD
	#end
#end
       ,BB.BKG_NO
       ,BB.SVC_SCP_CD
       ,BB.POR_CD
       ,BB.POL_CD
       ,BB.POD_CD
       ,BB.DEL_CD
       ,NVL(BB.SC_NO,NVL(BB.RFA_NO,BB.TAA_NO)) CONTRACT_NO
#if (${tab_cd} == '2')
			 ,CASE WHEN BB.SC_NO IS NOT NULL THEN 
                  (SELECT RSCP.CUST_CNT_CD||LPAD(RSCP.CUST_SEQ,6,'0')
                    FROM PRI_SP_CTRT_PTY RSCP 
                    WHERE RSCP.PROP_NO IN (SELECT PSH.PROP_NO 
                                               FROM PRI_SP_HDR PSH 
                                              WHERE PSH.SC_NO = BB.SC_NO)
                    AND RSCP.PRC_CTRT_PTY_TP_CD = 'C'
                    AND ROWNUM = 1  )
             WHEN BB.RFA_NO IS NOT NULL THEN 
                  (SELECT PRM.CTRT_CUST_CNT_CD||LPAD(PRM.CTRT_CUST_SEQ,6,'0')
                    FROM PRI_RP_MN PRM 
                    WHERE PRM.PROP_NO IN (SELECT PRH.PROP_NO 
                                               FROM PRI_RP_HDR PRH 
                                              WHERE PRH.RFA_NO = BB.RFA_NO)
                    AND ROWNUM = 1  )
             WHEN BB.TAA_NO IS NOT NULL THEN 
                  (SELECT PTM.CTRT_CUST_CNT_CD||LPAD(PTM.CTRT_CUST_SEQ,6,'0')
                    FROM PRI_TAA_MN PTM 
                    WHERE PTM.TAA_PROP_NO IN (SELECT PTH.TAA_PROP_NO 
                                               FROM PRI_TAA_HDR PTH 
                                              WHERE PTH.TAA_NO = BB.TAA_NO)
                    AND ROWNUM = 1  )
        END CUSTOMER_CODE
			 ,CASE WHEN BB.SC_NO IS NOT NULL THEN 
                  (SELECT RSCP.CTRT_PTY_NM
                    FROM PRI_SP_CTRT_PTY RSCP 
                    WHERE RSCP.PROP_NO IN (SELECT PSH.PROP_NO 
                                               FROM PRI_SP_HDR PSH 
                                              WHERE PSH.SC_NO = BB.SC_NO)
                    AND RSCP.PRC_CTRT_PTY_TP_CD = 'C'
                    AND ROWNUM = 1  )
             WHEN BB.RFA_NO IS NOT NULL THEN 
                  (SELECT MC.CUST_LGL_ENG_NM
                    FROM PRI_RP_MN PRM, MDM_CUSTOMER MC
                    WHERE PRM.PROP_NO IN (SELECT PRH.PROP_NO 
                                               FROM PRI_RP_HDR PRH 
                                              WHERE PRH.RFA_NO = BB.RFA_NO)
                    AND MC.CUST_CNT_CD = PRM.CTRT_CUST_CNT_CD
                    AND MC.CUST_SEQ = PRM.CTRT_CUST_SEQ                                                  
                    AND ROWNUM = 1  )
             WHEN BB.TAA_NO IS NOT NULL THEN 
                  (SELECT MC.CUST_LGL_ENG_NM
                    FROM PRI_TAA_MN PTM, MDM_CUSTOMER MC 
                    WHERE PTM.TAA_PROP_NO IN (SELECT PTH.TAA_PROP_NO 
                                               FROM PRI_TAA_HDR PTH 
                                              WHERE PTH.TAA_NO = BB.TAA_NO)
                    AND MC.CUST_CNT_CD = PTM.CTRT_CUST_CNT_CD
                    AND MC.CUST_SEQ = PTM.CTRT_CUST_SEQ                                                  
                    AND ROWNUM = 1  )
        END CUSTOMER_NAME
#end

#if (${charge_cd} == '1')
       ,(SELECT BC.CORR_OFC_CD FROM BKG_CORRECTION BC
                   WHERE BC.CORR_DT BETWEEN TO_DATE(replace(@[fr_dt],'-',''),'YYYYMMDD') AND TO_DATE(replace(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
                     AND BC.CORR_NO <> '0000000001'
                     AND BC.CORR_CXL_FLG = 'N'
					 --AND BC.RT_CORR_FLG = 'N'           /* CA KIND A */
					 --AND BC.CHG_TERM_CORR_FLG = 'N'	 	/* CA KIND B */
					 --AND BC.CA_OTR_RSN_CORR_FLG = 'N'	/* CA KIND K */
                     AND (BC.RCVDE_TERM_CORR_FLG = 'Y' OR BC.ROUT_CORR_FLG = 'Y' 
                         OR BC.CUST_CORR_FLG = 'Y' OR BC.QTY_CORR_FLG = 'Y' OR BC.MEAS_QTY_CORR_FLG = 'Y'
                         OR BC.CMDT_CORR_FLG = 'Y' OR BC.TRNK_VSL_CORR_FLG = 'Y' OR BC.PRPST_VSL_CORR_FLG = 'Y')
                     AND BC.CA_RSN_CD = 'M'
					 AND BC.BKG_NO = BB.BKG_NO
	#if (${p_sel_ofc_cd} != '')
                     AND BC.CORR_OFC_CD IN (${p_sel_ofc_cd}) 
	#end  
					 AND ROWNUM = 1) CORR_OFC_CD

	#if (${charge_cd} == '1') 
       ,DECODE((SELECT COUNT(*)
                 FROM BKG_CHG_RT BCR 
                WHERE BCR.BKG_NO = BB.BKG_NO
                  AND BCR.CHG_CD IN ('BCC','MCF')),0,'N','Y') BL_RATE_CNT
	#end
#elseif (${charge_cd} == '2')
       ,DECODE((SELECT COUNT(*)
                 FROM BKG_CHG_RT BCR 
                WHERE BCR.BKG_NO = BB.BKG_NO
                  AND BCR.CHG_CD = 'OBS'),0,'N','Y') BL_RATE_CNT
#elseif (${charge_cd} == '3')
       ,DECODE((SELECT COUNT(*)
                 FROM BKG_CHG_RT BCR 
                WHERE BCR.BKG_NO = BB.BKG_NO
                  AND BCR.CHG_CD = 'BLR'),0,'N','Y') BL_RATE_CNT

#end
#if (${charge_cd} == '4' ||${charge_cd} == '5' ||${charge_cd} == '6' || ${charge_cd} == '7') -- 4 LBP, 5 TPF, 6 CTC, 7 LSI
       ,DECODE((SELECT COUNT(*)
                 FROM BKG_CHG_RT BCR 
                WHERE BCR.BKG_NO = BB.BKG_NO
                  AND BCR.CHG_CD = DECODE(@[charge_cd],'4','LBP','5','TPF','6','CTC','7','LSI')),0,'N','Y') BL_RATE_CNT
#end                  
#if (${charge_cd} == '5')
		/* Prepaid Office */
		,( SELECT  R.PPD_RCV_OFC_CD||','||R.CLT_OFC_CD
             FROM BKG_RATE R
            WHERE R.BKG_NO = BB.BKG_NO
		  ) AS PPD_CLT_RCV_OFC_CD
#end               
FROM BKG_BOOKING BB
     ,MAS_OFC_LVL COLB
WHERE 1=1

#if (${charge_cd} == '1')
AND BB.BKG_NO IN (SELECT BC.BKG_NO
                    FROM BKG_CORRECTION BC
                   WHERE BC.CORR_DT BETWEEN TO_DATE(replace(@[fr_dt],'-',''),'YYYYMMDD') AND TO_DATE(replace(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
                     AND BC.CORR_NO <> '0000000001'
                     AND BC.CORR_CXL_FLG = 'N'
					 --AND BC.RT_CORR_FLG = 'N'           /* CA KIND A */
					 --AND BC.CHG_TERM_CORR_FLG = 'N'	 	/* CA KIND B */
					 --AND BC.CA_OTR_RSN_CORR_FLG = 'N'	/* CA KIND K */
                     AND (BC.RCVDE_TERM_CORR_FLG = 'Y' OR BC.ROUT_CORR_FLG = 'Y' 
                         OR BC.CUST_CORR_FLG = 'Y' OR BC.QTY_CORR_FLG = 'Y' OR BC.MEAS_QTY_CORR_FLG = 'Y'
                         OR BC.CMDT_CORR_FLG = 'Y' OR BC.TRNK_VSL_CORR_FLG = 'Y' OR BC.PRPST_VSL_CORR_FLG = 'Y')
                     AND BC.CA_RSN_CD = 'M'
	#if (${p_sel_ofc_cd} != '')
                     AND BC.CORR_OFC_CD IN (${p_sel_ofc_cd}) 
	#end                     
                 )

#elseif (${charge_cd} == '2')
AND BB.BKG_NO IN (SELECT BDPS.BKG_NO
                    FROM BKG_DOC_PROC_SKD BDPS
                   WHERE BDPS.BKG_DOC_PROC_TP_CD = 'OBLSRD'
                     AND BDPS.EVNT_DT BETWEEN TO_DATE(replace(@[fr_dt],'-',''),'YYYYMMDD') AND TO_DATE(replace(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
	#if (${p_sel_ofc_cd} != '')
                     AND EXISTS (SELECT 'X' 
								 FROM COM_USER CU
								 WHERE BDPS.CRE_USR_ID = CU.USR_ID
								 AND   TRIM(CU.OFC_CD) IN (${p_sel_ofc_cd}) )
	#end
                     )
#elseif (${charge_cd} == '3')
AND BB.BKG_NO IN (SELECT BDPS.BKG_NO
                    FROM BKG_DOC_PROC_SKD BDPS
                   WHERE BDPS.BKG_DOC_PROC_TP_CD = 'OBLISS'
                     AND BDPS.DOC_PROC_SEQ >= 2
                     AND BDPS.EVNT_DT BETWEEN TO_DATE(replace(@[fr_dt],'-',''),'YYYYMMDD') AND TO_DATE(replace(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
	#if (${p_sel_ofc_cd} != '')
                     AND EXISTS (SELECT 'X' 
								 FROM COM_USER CU
								 WHERE BDPS.CRE_USR_ID = CU.USR_ID
								 AND   TRIM(CU.OFC_CD) IN (${p_sel_ofc_cd}) )
	#end
					)

#elseif (${charge_cd} == '4')
/* charge_cd 4 LBP */
AND BB.BKG_NO IN (
                    SELECT /*+ INDEX_DESC(ISS XAK1BKG_BL_ISS)*/
						  ISS.BKG_NO
                    FROM  BKG_BL_ISS ISS ,BKG_CHG_RT BCR1
                    WHERE 1=1
                      AND ISS.OBL_ISS_DT BETWEEN TO_DATE(replace(@[fr_dt],'-',''),'YYYYMMDD') AND TO_DATE(replace(@[to_dt],'-',''),'YYYYMMDD') + 0.99999

	#if (${p_sel_ofc_cd} != '')
                     AND OBL_ISS_OFC_CD  IN (${p_sel_ofc_cd})
	#end
					 AND BCR1.BKG_NO = ISS.BKG_NO
                  	 AND BCR1.CHG_CD = 'LBP'

					)
#elseif (${charge_cd} == '5' || ${charge_cd} == '6' || ${charge_cd} == '7')
/*  charge_cd 5 TPF, 6 CTC, 7 LSI */
AND BB.BKG_NO IN (  SELECT /*+ INDEX_DESC(B XAK2BKG_BL_DOC) */
                          B.BKG_NO
                      FROM BKG_BL_DOC B
					#if (${charge_cd} == '6' || ${charge_cd} == '7')
                         , BKG_CHG_RT BCR1
					#end
                     WHERE B.BL_OBRD_DT BETWEEN TO_DATE(replace(@[fr_dt],'-',''),'YYYYMMDD') AND TO_DATE(replace(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
					#if (${charge_cd} == '6' || ${charge_cd} == '7')
                       /*  charge_cd 6 CTC, 7 LSI */
                        AND BCR1.BKG_NO = B.BKG_NO
                  		AND BCR1.CHG_CD = DECODE(@[charge_cd],'6','CTC','7','LSI')
					#end


				    #if (${charge_cd} == '5')

				   		/* 
				          Booking RHQ를 SHARC로 선택하면 아래의 테이블에 Prepaid Office와 PPD-3rd Party office를  
				          Booking RHQ를 SHARC 외에 다른 RHQ를 선택하면 아래의 테이블에 Collect Office와 CCT-3rd party Office를 체크 함.
				        */
				   		#if (${p_rhq_bkg_ofc_cd} == 'SHARC')
				           	#if (${p_sel_ofc_cd} != '')
				   	        	/* Prepaid Office 체크 */
				   				AND EXISTS ( SELECT 'Y'
				   				               FROM BKG_RATE R
				   		    		          WHERE 1=1
				           		    		    AND R.BKG_NO = B.BKG_NO
				   				                AND R.PPD_RCV_OFC_CD IN (${p_sel_ofc_cd})
				   					#if (${p_sel_ofc_cd2} != '')
				           				        AND EXISTS (SELECT 'Y'  FROM BKG_CHG_RT C WHERE R.BKG_NO = C.BKG_NO AND C.CHG_CD = 'TPF'	AND C.FRT_TERM_CD = 'P' AND C.N3PTY_RCV_OFC_CD IN (${p_sel_ofc_cd2}) AND ROWNUM=1)   
				   					#end
												AND ROWNUM =1 
				   				             )
				   			#else
				   					#if (${p_sel_ofc_cd2} != '')
				   						/* PPD-3rd Party office만 입력 됨 */
				           				AND EXISTS (SELECT 'Y'  FROM BKG_CHG_RT C WHERE C.BKG_NO = B.BKG_NO AND C.CHG_CD = 'TPF'	AND C.FRT_TERM_CD = 'P' AND C.N3PTY_RCV_OFC_CD IN (${p_sel_ofc_cd2}) AND ROWNUM=1)   
				   					#end
			
				           	#end
				   		#else

				           	#if (${p_sel_ofc_cd} != '')
				   	        	/* Collect Office 체크 */
				   				AND EXISTS ( SELECT 'Y'
				   				               FROM BKG_RATE R
				   		    		          WHERE 1=1
				           		    		    AND R.BKG_NO = B.BKG_NO
				   				                AND R.CLT_OFC_CD IN (${p_sel_ofc_cd})
				   					#if (${p_sel_ofc_cd2} != '')
				           				        AND EXISTS (SELECT 'Y'  FROM BKG_CHG_RT C WHERE R.BKG_NO = C.BKG_NO AND C.CHG_CD = 'TPF'	AND C.FRT_TERM_CD = 'C' AND C.N3PTY_RCV_OFC_CD IN (${p_sel_ofc_cd2})  AND ROWNUM=1)   
				   					#end
												AND ROWNUM =1 
				   				             )
				   			#else
				   					#if (${p_sel_ofc_cd2} != '')
				   						/* CCT-3rd Party office만 입력 됨 */
				           				AND EXISTS (SELECT 'Y'  FROM BKG_CHG_RT C WHERE C.BKG_NO = B.BKG_NO AND C.CHG_CD = 'TPF'	AND C.FRT_TERM_CD = 'C' AND C.N3PTY_RCV_OFC_CD IN (${p_sel_ofc_cd2})  AND ROWNUM=1)   
				   					#end
			
				           	#end

				        #end
				/* charge5 TPF  */
			    #end
	)
  #if (${charge_cd} == '5')
       /* TPF 일경우 F: Full Cargo 만 조회함.*/
          AND BB.BKG_CGO_TP_CD = 'F'
  #end
#else
   AND 1 = 2
#end
 
#if (${p_rhq_bkg_ofc_cd} != '')
	AND TRIM(COLB.OFC_N2ND_LVL_CD) = @[p_rhq_bkg_ofc_cd]
#end
#if (${p_gso_bkg_ofc_cd} != '')
	AND TRIM(COLB.OFC_N5TH_LVL_CD) IN (${p_gso_bkg_ofc_cd})
#end
#if (${p_ctrt_ofc_cd} != '' || ${p_rhq_ctrt_ofc_cd} != '')
	AND BB.CTRT_OFC_CD IN (SELECT COLC.OFC_CD FROM MAS_OFC_LVL COLC WHERE 1=1
	#if (${p_rhq_ctrt_ofc_cd} != '')
		AND COLC.OFC_N2ND_LVL_CD = @[p_rhq_ctrt_ofc_cd]
	#end
	#if (${p_ctrt_ofc_cd} != '')
		AND COLC.OFC_CD IN (${p_ctrt_ofc_cd})
	#end
	)
#end
#if (${ctr_rfa_no} != '')
    #if (${ctr_rfa_cd} == 'C')
        AND BB.SC_NO = @[ctr_rfa_no]
    #elseif (${ctr_rfa_cd} == 'R')
        AND BB.RFA_NO = @[ctr_rfa_no]
    #elseif (${ctr_rfa_cd} == 'T')
        AND BB.TAA_NO = @[ctr_rfa_no]
    #end
#end
#if (${p_por_cd} != '')
    AND BB.POR_CD = @[p_por_cd]
#end
#if (${p_pol_cd} != '')
    AND BB.POL_CD = @[p_pol_cd]
#end
#if (${p_pod_cd} != '')
    AND BB.POD_CD = @[p_pod_cd]
#end
#if (${p_del_cd} != '')
    AND BB.DEL_CD = @[p_del_cd]
#end
#if (${p_svc_scp_cd} != '')
    AND BB.SVC_SCP_CD IN (${p_svc_scp_cd})
#end
AND BB.BKG_STS_CD <> 'X'    
AND BB.BKG_CGO_TP_CD <> 'P' 
AND BB.BKG_OFC_CD = COLB.OFC_CD
AND TO_CHAR(BB.BKG_CRE_DT,'YYYYMM') BETWEEN COLB.OFC_APLY_FM_YRMON AND COLB.OFC_APLY_TO_YRMON
) BB

#if (${tab_cd} == '1')
	GROUP BY RHQ_BKG_OFC_CD
	       ,GSO_BKG_OFC_CD
	       ,BKG_OFC_CD
	#if (${charge_cd} == '1')
		   ,CORR_OFC_CD
	#elseif (${charge_cd} == '5')
	   		#if (${p_rhq_bkg_ofc_cd} == 'SHARC')
			   ,BKG_GET_TOKEN_FNC(PPD_CLT_RCV_OFC_CD,1)
			#else
			   ,BKG_GET_TOKEN_FNC(PPD_CLT_RCV_OFC_CD,2)
			#end
	#elseif (${charge_cd} == '4')
		   ,OBL_ISS_OFC_CD
	#end


#elseif (${tab_cd} == '2')
	GROUP BY SVC_SCP_CD
	       ,RHQ_BKG_OFC_CD
	       ,GSO_BKG_OFC_CD
	       ,BKG_OFC_CD
	       ,POR_CD
	       ,POL_CD
	       ,POD_CD
	       ,DEL_CD
	       ,CONTRACT_NO
	       ,CUSTOMER_CODE
	       ,CUSTOMER_NAME
       
#end
ORDER BY 1,2,3			]]></sql>
			<params>
				<param name="charge_cd" type="12" value="" out="N"/>
				<param name="fr_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="p_rhq_bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="p_rhq_ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="ctr_rfa_no" type="12" value="" out="N"/>
				<param name="p_por_cd" type="12" value="" out="N"/>
				<param name="p_pol_cd" type="12" value="" out="N"/>
				<param name="p_pod_cd" type="12" value="" out="N"/>
				<param name="p_del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
