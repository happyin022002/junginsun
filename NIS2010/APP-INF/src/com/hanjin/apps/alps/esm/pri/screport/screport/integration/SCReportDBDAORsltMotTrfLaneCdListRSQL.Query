<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltMotTrfLaneCdListRSQL">
			<desc><![CDATA[MOT Tariff 에서 조회 조건에 사용되는 선택된 Service Scope 별 MOT Filing Lane Code ( CD03269 ) List 조회]]></desc>
			<sql><![CDATA[
SELECT  B.INTG_CD_VAL_CTNT AS CD
    ,   B.INTG_CD_VAL_DP_DESC AS NM
FROM    
    (   SELECT  DISTINCT MOT_FILE_LANE_CD
        FROM    PRI_MOT_FILE_LOC_PPT 
        WHERE   SVC_SCP_CD = @[svc_scp_cd]
        ) A
    ,   COM_INTG_CD_DTL B
WHERE   B.INTG_CD_ID = 'CD03269'
AND     A.MOT_FILE_LANE_CD = B.INTG_CD_VAL_CTNT
ORDER   BY B.INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
