<sqls>
	<querys>
		<query name="NetworkCostDBDAOSearchNWCreRStatusListRSQL">
			<desc><![CDATA[SearchNWCreRStatusList SELECT]]></desc>
			<sql><![CDATA[
SELECT
    A.STND_COST_CD      AS STND_COST_CD
   ,B.STND_COST_NM      AS STND_COST_NM
   ,A.VSL_OSHP_CD       AS VSL_OSHP_CD
   ,A.VOP_CD            AS VOP_CD
   ,A.CRE_STS_CD        AS CRE_STS_CD
   ,A.CRE_SLCT_FLG      AS CRE_SLCT_FLG
FROM COA_NTWK_COST_CRE A
    ,COA_STND_ACCT     B
WHERE  A.STND_COST_CD = B.STND_COST_CD (+)
ORDER BY STND_COST_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
