<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOmodifyDgCNTRInfoUSQL">
			<desc><![CDATA[다른 항차로 이미 D/L 된 BKG이있을 경우에, Dup Error를 피하고 Update 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_KR_DG_CGO
   SET DCGO_SEQ   = @[doc_no]
     , MSN_NO     = @[msn_no]
     , VSL_CD     = SUBSTR(@[vvd], 1, 4) 
     , SKD_VOY_NO = SUBSTR(@[vvd], 5, 4) 
     , SKD_DIR_CD = SUBSTR(@[vvd], 9, 1) 
     , POL_CD    = @[pol_cd]
     , POD_CD    = @[pod_cd]
     , IMDG_CLSS_CD = @[imdg_clss_cd]
     , BL_NO      = @[bl_no]
     , MF_CERTI_NO = @[certi_no]
     , CERTI_SEQ_NO = @[certi_seq_no]
     , DCHG_KND_CD  = @[job]
     , PRP_SHP_NM = @[substance]
     , NET_WGT = @[net_weight]
     , UPD_DT   = SYSDATE 
     , UPD_USR_ID   = @[user_id]
     , SND_DT  = NULL 
     , SND_USR_ID  = NULL
     , IMDG_COMP_GRP_CD = @[imdg_comp_grp_cd]
     , CGO_SEQ_NO = @[ib_seq]
 WHERE BKG_NO           = @[bkg_no]
   AND CSTMS_DECL_TP_CD = @[cstms_decl_tp_cd]
   AND CNTR_SEQ         = @[cntr_seq]
   AND CNTR_NO          = @[cntr_no]
   AND IMDG_UN_NO       = @[imdg_un_no]			]]></sql>
			<params>
				<param name="doc_no" type="12" value="" out="N"/>
				<param name="msn_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="certi_no" type="12" value="" out="N"/>
				<param name="certi_seq_no" type="12" value="" out="N"/>
				<param name="job" type="12" value="" out="N"/>
				<param name="substance" type="12" value="" out="N"/>
				<param name="net_weight" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="imdg_comp_grp_cd" type="12" value="" out="N"/>
				<param name="ib_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_seq" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
