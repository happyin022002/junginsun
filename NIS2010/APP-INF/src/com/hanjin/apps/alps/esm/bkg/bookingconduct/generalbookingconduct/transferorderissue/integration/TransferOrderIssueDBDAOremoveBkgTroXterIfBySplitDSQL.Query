<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOremoveBkgTroXterIfBySplitDSQL">
			<desc><![CDATA[delete]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_TRO_XTER_IF
WHERE	BKG_NO = @[bkg_no]
AND	IO_BND_CD = 'O'
AND	RTN_TRO_FLG = @[rtn_tro_flg]
AND	TRO_SEQ = @[tro_seq]
AND 0 = (select count(*) from bkg_tro_dtl
			WHERE	BKG_NO = @[bkg_no]
			AND	IO_BND_CD = 'O'
			AND	TRO_SEQ = @[tro_seq])			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="rtn_tro_flg" type="12" value="" out="N"/>
				<param name="tro_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
