<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSplitCombineDBDAOsearchQtyForSplitRSQL">
			<desc><![CDATA[split시 참고할 qty data를 조회한다.]]></desc>
			<sql><![CDATA[
select cntr_tpsz_cd
        , op_cntr_qty
        ,eq_subst_cntr_tpsz_cd,eq_subst_cgo_qty
  from bkg_quantity
#if (${bkg_no} !='' )
 where bkg_no = @[bkg_no] 
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
