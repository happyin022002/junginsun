<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOCmGoodsDescRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT A.BKG_NO
,      A.PCK_CMDT_DESC
,      A.CNTR_CMDT_DESC
FROM   BKG_BL_DOC A
WHERE  A.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
