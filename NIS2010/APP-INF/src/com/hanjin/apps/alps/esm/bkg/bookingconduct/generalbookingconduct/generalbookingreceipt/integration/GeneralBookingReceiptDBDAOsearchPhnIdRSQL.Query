<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOsearchPhnIdRSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOsearchPhnIdRSQL]]></desc>
			<sql><![CDATA[
SELECT 
SLAN_CD,
DIR_CD,
CRE_SEQ,
OFC_CD,
RCVR_USR_ID,
(SELECT USR_NM FROM COM_USER WHERE USR_ID = RCVR_USR_ID) USR_NM,
RCVR_EML,
PHN_NO,
CRE_USR_ID,
CRE_DT,
UPD_USR_ID,
UPD_DT
,FILE_ATCH_FLG
FROM BKG_CLL_CNG_NTFY_SET
WHERE SLAN_CD = @[slan_cd]
AND DIR_CD = @[dir_cd]
AND LOCL_TS_IND_CD = 'L'
#if (${pol_cd} != "")
	AND PORT_CD = @[pol_cd]
#end			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
