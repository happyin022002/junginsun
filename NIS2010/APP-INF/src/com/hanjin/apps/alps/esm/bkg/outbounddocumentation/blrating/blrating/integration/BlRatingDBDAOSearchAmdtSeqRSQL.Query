<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOSearchAmdtSeqRSQL">
			<desc><![CDATA[Freight & Charge 화면(ESM_BKG_0079_08)화면에서 SC No. or RFA No. or TAA No.옆 검색 버튼을 누를 때 팝업을 연결하기 위한 필수조건인 Amend Sequend를 조회한다]]></desc>
			<sql><![CDATA[
-- BlRatingDBDAOSearchAmdtSeqRSQL.Query
#if( ${ctrt_type} == 'sc' )
SELECT AMDT_SEQ
  FROM PRI_SP_MN
 WHERE PROP_NO = (SELECT PROP_NO 
                    FROM PRI_SP_HDR
                   WHERE SC_NO = @[ctrt_no])
   AND TO_DATE(@[application_date],'YYYYMMDD') BETWEEN EFF_DT AND NVL(EXP_DT,TO_DATE('99991231','YYYYMMDD'))
   AND PROP_STS_CD = 'F'
#elseif( ${ctrt_type} == 'rfa')
SELECT AMDT_SEQ
  FROM PRI_RP_MN
 WHERE PROP_NO = (SELECT PROP_NO 
                    FROM PRI_RP_HDR
                   WHERE RFA_NO = @[ctrt_no])
   AND TO_DATE(@[application_date],'YYYYMMDD') BETWEEN EFF_DT AND NVL(EXP_DT,TO_DATE('99991231','YYYYMMDD'))
   AND PROP_STS_CD = 'A'
#elseif( ${ctrt_type} == 'taa')
SELECT AMDT_SEQ
  FROM PRI_TAA_MN
 WHERE TAA_PROP_NO = (SELECT TAA_PROP_NO 
                        FROM PRI_TAA_HDR
                       WHERE TAA_NO = @[ctrt_no])
   AND TO_DATE(@[application_date],'YYYYMMDD') BETWEEN EFF_DT AND NVL(EXP_DT,TO_DATE('99991231','YYYYMMDD'))
   AND CFM_FLG = 'Y'
#end			]]></sql>
			<params>
				<param name="ctrt_no" type="12" value="" out="N"/>
				<param name="application_date" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
