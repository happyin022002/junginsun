<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCommonDBDAOGetMdmCurrencyInfoRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT CURR_CD AS VALUE0,
       CURR_NM AS VALUE1,
       CNT_CD AS VALUE3        
  FROM MDM_CURRENCY
 WHERE CURR_CD = @[value0]
   AND NVL(DELT_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="value0" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
