<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ModelManageDBDAOCopyModelshipAcctCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_MDL_CUST_CTRL
    (
       TRD_CD
     , COST_YRWK
     , VER_SEQ
     , CUST_CNT_CD
     , CUST_SEQ
     , DTL_SEQ
     , CUST_GRP_ID
     , SC_NO
     , RFA_NO
     , CUST_CTRL_CD
     , WK_MQC_QTY
     , ACCT_PIC_NM
     , DELT_FLG
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , PRE_SC_NO
     , PRE_RFA_NO
    )
SELECT TRD_CD
     , @[new_cost_yrwk]
     , @[new_ver_seq]
     , CUST_CNT_CD
     , CUST_SEQ
     , DTL_SEQ
     , CUST_GRP_ID
     , SC_NO
     , RFA_NO
     , CUST_CTRL_CD
     , WK_MQC_QTY
     , ACCT_PIC_NM
     , DELT_FLG
     , @[cre_usr_id]
     , SYSDATE
     , @[cre_usr_id]
     , SYSDATE
     , PRE_SC_NO
     , PRE_RFA_NO
FROM SPC_MDL_CUST_CTRL
WHERE TRD_CD = @[trade]
  AND COST_YRWK = @[cost_yrwk]
  AND VER_SEQ =  @[ver_seq] 			]]></sql>
			<params>
				<param name="new_cost_yrwk" type="12" value="" out="N"/>
				<param name="new_ver_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="cost_yrwk" type="12" value="" out="N"/>
				<param name="ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
