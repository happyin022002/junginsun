<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOSearchQtaEditIasSectorListRSQL">
			<desc><![CDATA[Qta Edit for IAS Sector을 조회 합니다.

* History
* 2014.06.20 이혜민 [CHM-201430168] IAS Sector Sales - Main, Sector 구분자 추가를 위한 화면 변경
* 2014.07.08 PEJ [CHM-201431035] QTA Edit for IAS Sector Sales의 BSA 중복 현상 로직 수정
* 2015.02.11 이혜민 VVD Adjustment 기준을 COST_MON으로 바꾸면서 화면 정렬을 SLS_YR로 수정
* 2015.06.15 김용습 [CHM-201536164] 주간 MAS Open에 따른 타모듈 프로그램 적용 요청
* 2016.02.05 최성민 [CHM-201639787] SQM Planning 도중 & Planning 완료 후 노선, P/F Group, Sector 추가 로직 Process 변경
]]></desc>
			<sql><![CDATA[
SELECT A1.QTA_RLSE_VER_NO
      ,A1.BSE_TP_CD
      ,A1.BSE_YR
      ,A1.BSE_QTR_CD
      ,(SELECT INTG_CD_VAL_DP_DESC AS NAME
        FROM COM_INTG_CD_DTL
        WHERE INTG_CD_ID  = 'CD00940'
        AND INTG_CD_VAL_CTNT = A1.OFC_VW_CD) OFC_VW_CD
      ,A1.TRD_CD
      ,A1.SUB_TRD_CD
      ,(SELECT INTG_CD_VAL_DP_DESC
		FROM COM_INTG_CD_DTL
		WHERE INTG_CD_ID = 'CD03218'
		AND INTG_CD_VAL_CTNT = A3.IAS_RGN_CD) IAS_RGN_CD
      ,A1.RLANE_CD
      ,A1.DIR_CD
      ,DECODE(A3.HUL_BND_CD,'BH','B/H','H/H') HUL_BND_CD
      ,A2.SLS_YRMON
      ,A2.BSE_MON
      ,A2.BSE_WK
      ,A1.VSL_CD||A1.SKD_VOY_NO||A1.SKD_DIR_CD VVD
      , A1.PF_GRP_CD
      ,A2.FNL_BSA_CAPA 
      ,A1.RHQ_CD
      ,A1.RGN_OFC_CD
      ,A1.POL_CD
      ,A1.POD_CD
      ,A1.VSL_CD
      ,A1.SKD_VOY_NO
      ,A1.SKD_DIR_CD
      ,A1.LOD_QTY
      ,A1.GRS_RPB_REV
      ,A1.PA_CM_UC_AMT
      ,A1.RA_CM_UC_AMT
      ,A1.SQM_CNG_TP_CD
	  ,DECODE(NVL(A4.SQM_MN_SCTR_FLG, 'N'), 'Y', 'Main', 'N', 'Sector') SQM_MN_SCTR_FLG
  FROM SQM_SCTR_CFM_QTA A1
      ,SQM_CFM_TGT_VVD A2
      ,MAS_LANE_RGST A3
      ,SQM_SCTR_PAIR_MGMT A4
--      , SQM_SCTR_PF_GRP PG
--      , VSK_VSL_SKD VS

 WHERE A1.BSE_TP_CD                  = 'Q'
   AND A1.BSE_YR                     = @[f_bse_yr]
   AND A1.BSE_QTR_CD                 = @[f_bse_qtr_cd]
   AND A1.OFC_VW_CD                  = @[f_ofc_vw_cd]
   AND SUBSTR(A1.QTA_RLSE_VER_NO,-2) = '02'
   AND A1.TRD_CD                     = 'IAS'  

#if (${f_sub_trd_cd} != '' && ${f_sub_trd_cd} != 'All')
   AND A1.SUB_TRD_CD                 = @[f_sub_trd_cd]
#end
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
   AND A1.RLANE_CD                   = @[f_rlane_cd]
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
   AND A1.DIR_CD                     = @[f_dir_cd]
#end
#if (${f_trd_dir} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
   AND A3.HUL_BND_CD                 = @[f_hul_bnd_cd]
#end
#if (${f_ias_rgn_cd} != '' && ${f_ias_rgn_cd} != 'All')
   AND A3.IAS_RGN_CD                 = @[f_ias_rgn_cd]
#end
#if (${f_rhq_cd} != '' && ${f_rhq_cd} != 'All')
   AND A1.RHQ_CD                     = @[f_rhq_cd]
#end
#if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
   AND A1.RGN_OFC_CD                 = @[f_rgn_ofc_cd]
#end
#if (${f_to_mon} != '' && ${f_to_mon} != 'All')
   AND A2.BSE_MON                    = @[f_to_mon]
#end
#if (${f_to_wk} != '' && ${f_to_wk} != 'All')
   AND A2.BSE_WK                     = @[f_to_wk]
#end
#if (${f_vsl_cd} != '' )
   AND A1.VSL_CD                     = @[f_vsl_cd]
#end
#if (${f_skd_voy_no} != '' )
   AND A1.SKD_VOY_NO                 = @[f_skd_voy_no]
#end
#if (${f_skd_dir_cd} != '' )
   AND A1.SKD_DIR_CD                 = @[f_skd_dir_cd]
#end
#if (${f_pol_cd} != '' && ${f_pol_cd} != 'All')
   AND A1.POL_CD                 = @[f_pol_cd]
#end
#if (${f_pod_cd} != '' && ${f_pod_cd} != 'All')
   AND A1.POD_CD                 = @[f_pod_cd]
#end
#if (${f_sqm_mn_sctr_flg} != '' && ${f_sqm_mn_sctr_flg} != 'All')
AND NVL(A4.SQM_MN_SCTR_FLG, 'N') = @[f_sqm_mn_sctr_flg]
#end

#if (${f_pf_grp_cd} != '' && ${f_pf_grp_cd} != 'All')
   AND A4.PF_GRP_CD                 = @[f_pf_grp_cd]
#end

   AND A1.QTA_RLSE_VER_NO            = A2.QTA_RLSE_VER_NO
   AND A1.BSE_TP_CD                  = A2.BSE_TP_CD
   AND A1.BSE_YR                     = A2.BSE_YR
   AND A1.BSE_QTR_CD                 = A2.BSE_QTR_CD
   AND A1.TRD_CD                     = A2.TRD_CD
   AND A1.RLANE_CD                   = A2.RLANE_CD
   AND A1.DIR_CD                     = A2.DIR_CD  
   AND A1.VSL_CD                     = A2.VSL_CD
   AND A1.SKD_VOY_NO                 = A2.SKD_VOY_NO
   AND A1.SKD_DIR_CD                 = A2.SKD_DIR_CD

   AND A1.TRD_CD                     = A3.TRD_CD
   AND A1.SUB_TRD_CD                 = A3.SUB_TRD_CD
   AND A1.RLANE_CD                   = A3.RLANE_CD
   AND A1.DIR_CD                     = A3.DIR_CD  

   AND A2.IOC_CD                     = A3.IOC_CD

   AND A1.BSE_TP_CD  				 = A4.BSE_TP_CD
   AND A1.BSE_YR     				 = A4.BSE_YR
   AND A1.BSE_QTR_CD 				 = A4.BSE_QTR_CD
   AND A1.RLANE_CD   				 = A4.RLANE_CD
   AND A1.DIR_CD     				 = A4.DIR_CD
   AND A1.PF_GRP_CD  				 = A4.PF_GRP_CD
   AND A1.POL_CD     				 = A4.POL_CD
   AND A1.POD_CD     				 = A4.POD_CD   

--   AND A1.BSE_TP_CD                  = PG.BSE_TP_CD(+)
--   AND A1.BSE_YR                     = PG.BSE_YR(+)
--   AND A1.BSE_QTR_CD                 = PG.BSE_QTR_CD(+)
--   AND A1.RLANE_CD                   = PG.RLANE_CD(+)    
--   AND A1.VSL_CD                     = VS.VSL_CD(+)
--   AND A1.SKD_VOY_NO                 = VS.SKD_VOY_NO(+)
--   AND A1.SKD_DIR_CD                 = VS.SKD_DIR_CD(+)
--   AND PG.PF_SVC_TP_CD               = VS.PF_SKD_TP_CD

 ORDER BY  A1.TRD_CD
          ,A1.SUB_TRD_CD
          ,A1.RLANE_CD
          ,NVL(SUBSTR(A2.SLS_YRMON,0,4),A1.BSE_YR)
          ,A2.BSE_MON
          ,A2.BSE_WK
          ,A1.VSL_CD||A1.SKD_VOY_NO||A1.SKD_DIR_CD
          ,A1.RHQ_CD
          ,A1.RGN_OFC_CD			]]></sql>
			<params>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="f_sub_trd_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_hul_bnd_cd" type="12" value="" out="N"/>
				<param name="f_ias_rgn_cd" type="12" value="" out="N"/>
				<param name="f_rhq_cd" type="12" value="" out="N"/>
				<param name="f_rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="f_to_mon" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
				<param name="f_skd_voy_no" type="12" value="" out="N"/>
				<param name="f_skd_dir_cd" type="12" value="" out="N"/>
				<param name="f_pol_cd" type="12" value="" out="N"/>
				<param name="f_pod_cd" type="12" value="" out="N"/>
				<param name="f_sqm_mn_sctr_flg" type="12" value="" out="N"/>
				<param name="f_pf_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
