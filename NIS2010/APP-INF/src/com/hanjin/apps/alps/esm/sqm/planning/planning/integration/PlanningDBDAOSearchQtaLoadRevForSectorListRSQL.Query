<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanningDBDAOSearchQtaLoadRevForSectorListRSQL">
			<desc><![CDATA[[QTA Set up for IAS Sector by Head Office] 데이터를 조회한다.
* History
* 2016.01.28 최성민 [CHM-201639851] (Basic Data Creation for IAS Secotr 화면의 Creation 로직 변경)]]></desc>
			<sql><![CDATA[
#if (${freezing_flg} == 'Y')
SELECT '*'||OFC_VW_NM||'.'||RLANE_CD||'.'||DIR_CD||'.'||HUL_BND_CD||'.'||PF_GRP_CD||'.'||RHQ_CD||'.'||RGN_OFC_CD||'.'||POL_CD||'.'||POD_CD 
FROM
(
#end
   SELECT M1.BSE_YR
        , M1.BSE_QTR_CD
        , M1.OFC_VW_NM
    #if (${excel_flg} != 'Y')
        , M1.OFC_VW_CD
    #end
        , M1.TRD_CD
        , M1.SUB_TRD_CD
        , M1.IAS_RGN_CD
        , M1.RLANE_CD
        , M1.DIR_CD
        , M1.HUL_BND_CD
        , M1.PF_GRP_CD
        , M1.FNL_BSA_CAPA
        , M1.RHQ_CD
        , M1.RGN_OFC_CD
        , M1.POL_CD
        , M1.POD_CD
        , M1.SQM_MN_SCTR_FLG
        , DECODE(M1.PRE_LOD_QTY,    0,S1.PRE_LOD_QTY,     M1.PRE_LOD_QTY)     AS PRE_LOD_QTY
        , DECODE(M1.PRE_GRS_RPB_REV,0,S1.PRE_GRS_RPB_REV, M1.PRE_GRS_RPB_REV) AS PRE_GRS_RPB_REV
    #if (${excel_flg} == 'Y')
        , NVL(DECODE(M1.PRE_LOD_QTY, 0,S1.PRE_LOD_QTY, M1.PRE_LOD_QTY), 0)* NVL(DECODE(M1.PRE_GRS_RPB_REV,0,S1.PRE_GRS_RPB_REV, M1.PRE_GRS_RPB_REV), 0)
    #end
        , M1.LOD_QTY
        , M1.GRS_RPB_REV
    #if (${excel_flg} == 'Y')
        , M1.LOD_QTY * M1.GRS_RPB_REV
    #end
        , M1.GID_LOD_QTY
        , M1.GID_GRS_RPB_REV
    #if (${excel_flg} == 'Y')
        , M1.GID_LOD_QTY * M1.GID_GRS_RPB_REV
    #end
    #if (${excel_flg} != 'Y')
        , M1.USE_FLG
    #end
     FROM (
    SELECT A2.BSE_YR
          ,A2.BSE_QTR_CD
          ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD00940' AND INTG_CD_VAL_CTNT = A2.OFC_VW_CD) AS OFC_VW_NM
          ,A2.OFC_VW_CD
          ,A2.TRD_CD
          ,A2.SUB_TRD_CD
          ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03218' AND INTG_CD_VAL_CTNT = A3.IAS_RGN_CD) AS IAS_RGN_CD
          ,A2.RLANE_CD
          ,A2.DIR_CD
          ,DECODE(A3.HUL_BND_CD,'BH','B/H','H/H') HUL_BND_CD
          ,A2.PF_GRP_CD OLD_PF_GRP_CD
          , MAX(A2.PF_GRP_CD) OVER (PARTITION BY A2.RLANE_CD, A2.DIR_CD, A2.FNL_BSA_CAPA, A2.RGN_OFC_CD, A2.POL_CD, A2.POD_CD) MAX_PF_GRP_CD -- 중복 체크용
          , WM_CONCAT(A2.PF_GRP_CD) OVER (PARTITION BY A2.RLANE_CD, A2.DIR_CD, A2.FNL_BSA_CAPA, A2.RGN_OFC_CD, A2.POL_CD, A2.POD_CD ORDER BY A2.PF_GRP_CD) PF_GRP_CD 
          ,A2.FNL_BSA_CAPA
          ,A2.RHQ_CD
          ,A2.RGN_OFC_CD
          ,A2.POL_CD
          ,A2.POD_CD
          ,DECODE(NVL(A5.SQM_MN_SCTR_FLG, 'N'), 'Y', 'Main', 'N', 'Sector') SQM_MN_SCTR_FLG
          , NVL(AA2.LOD_QTY, 0) PRE_LOD_QTY
          , NVL(AA2.GRS_RPB_REV, 0) PRE_GRS_RPB_REV
          ,A2.LOD_QTY
          ,A2.GRS_RPB_REV
          ,A2.GID_LOD_QTY
          ,A2.GID_GRS_RPB_REV
          ,DECODE(NVL(A4.PF_GRP_CD,'*'),'*','N','Y') USE_FLG 
          , A1.POL_CALL_SEQ
          , A1.POD_CALL_SEQ
          , A2.BSE_TP_CD
          -- BSA RANK 순서로 전분기 데이터를 가져오기 위함
          , ROW_NUMBER() OVER (PARTITION BY A2.BSE_TP_CD, A2.BSE_YR, A2.BSE_QTR_CD, A2.OFC_VW_CD, A2.RLANE_CD, A2.DIR_CD, A2.PF_GRP_CD, A2.RGN_OFC_CD, A2.POL_CD, A2.POD_CD ORDER BY A2.FNL_BSA_CAPA DESC) BSA_RANK
      FROM SQM_SCTR_LANE_OFC A1
          ,SQM_SCTR_LOD_REV A2
          ,SQM_SCTR_LOD_REV AA2
          ,MAS_LANE_RGST A3
          ,(
            SELECT DISTINCT BSE_TP_CD
                  ,BSE_YR
                  ,BSE_QTR_CD
                  ,RLANE_CD
                  ,PF_GRP_CD 
              FROM SQM_SCTR_CFM_QTA
             WHERE 1=1
               AND QTA_RLSE_VER_NO = SUBSTR(@[f_bse_yr],-2) || DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd]) || '02'
               AND BSE_TP_CD  = @[f_bse_tp_cd]
               AND BSE_YR     = @[f_bse_yr]
               AND BSE_QTR_CD = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
           ) A4
          ,SQM_SCTR_PAIR_MGMT A5
          , (
              SELECT DISTINCT TV.BSE_TP_CD, TV.BSE_YR, TV.BSE_QTR_CD, TV.RLANE_CD, TV.DIR_CD, PG.PF_GRP_CD 
                FROM SQM_QTA_TGT_VVD TV
                   , SQM_SCTR_PF_GRP PG 
               WHERE TV.BSE_TP_CD    = PG.BSE_TP_CD
                 AND TV.BSE_YR       = PG.BSE_YR
                 AND TV.BSE_QTR_CD   = PG.BSE_QTR_CD
                 AND TV.RLANE_CD     = PG.RLANE_CD
                 AND TV.PF_SVC_TP_CD = PG.PF_SVC_TP_CD
				UNION
              SELECT DISTINCT TV.BSE_TP_CD, TV.BSE_YR, TV.BSE_QTR_CD, TV.RLANE_CD, TV.DIR_CD, PG.PF_GRP_CD 
                FROM SQM_SCTR_ADD_TGT_VVD TV
                   , SQM_SCTR_PF_GRP PG 
               WHERE TV.BSE_TP_CD    = PG.BSE_TP_CD
                 AND TV.BSE_YR       = PG.BSE_YR
                 AND TV.BSE_QTR_CD   = PG.BSE_QTR_CD
                 AND TV.RLANE_CD     = PG.RLANE_CD
                 AND TV.PF_SVC_TP_CD = PG.PF_SVC_TP_CD
				
           ) A6 -- TARGET VVD.PF_SVC_TP_CD 에 존재하는 PF_GRP_CD만 거져오기 위함
     WHERE 1=1
       AND A1.BSE_TP_CD   = A2.BSE_TP_CD
       AND A1.BSE_YR      = A2.BSE_YR
       AND A1.BSE_QTR_CD  = A2.BSE_QTR_CD
       AND A1.OFC_VW_CD   = A2.OFC_VW_CD
       AND A1.RLANE_CD    = A2.RLANE_CD
       AND A1.DIR_CD      = A2.DIR_CD
       AND A1.PF_GRP_CD   = A2.PF_GRP_CD
       AND A1.RGN_OFC_CD  = A2.RGN_OFC_CD
       AND A1.POL_CD      = A2.POL_CD
       AND A1.POD_CD      = A2.POD_CD

       AND AA2.BSE_TP_CD(+) = A2.BSE_TP_CD
       AND AA2.BSE_YR(+)    = TO_CHAR(ADD_MONTHS(TO_DATE(A2.BSE_YR||DECODE(SUBSTR(A2.BSE_QTR_CD, 0, 1), '1', '01', '2', '04', '3', '07', '4', '10','1'),'YYYYMM'), -2), 'YYYY') -- 성능좋음       
       AND AA2.BSE_QTR_CD(+)= TO_CHAR(ADD_MONTHS(TO_DATE(A2.BSE_YR||DECODE(SUBSTR(A2.BSE_QTR_CD, 0, 1), '1', '01', '2', '04', '3', '07', '4', '10','1'),'YYYYMM'), -2), 'Q')||'Q'    
       AND AA2.OFC_VW_CD(+) = A2.OFC_VW_CD
       AND AA2.RLANE_CD(+)  = A2.RLANE_CD
       AND AA2.DIR_CD(+)    = A2.DIR_CD
       AND AA2.FNL_BSA_CAPA(+) = A2.FNL_BSA_CAPA
       AND AA2.PF_GRP_CD(+) = A2.PF_GRP_CD
       AND AA2.RGN_OFC_CD(+)= A2.RGN_OFC_CD
       AND AA2.POL_CD(+)    = A2.POL_CD
       AND AA2.POD_CD(+)    = A2.POD_CD

       AND A1.TRD_CD      = A3.TRD_CD
       AND A1.RLANE_CD    = A3.RLANE_CD
       AND A1.DIR_CD      = A3.DIR_CD

       AND A1.BSE_TP_CD   = A4.BSE_TP_CD(+)
       AND A1.BSE_YR      = A4.BSE_YR(+)
       AND A1.BSE_QTR_CD  = A4.BSE_QTR_CD(+)
       AND A1.RLANE_CD    = A4.RLANE_CD(+)
       AND A1.PF_GRP_CD   = A4.PF_GRP_CD(+)

       AND A1.BSE_TP_CD  = A5.BSE_TP_CD
       AND A1.BSE_YR     = A5.BSE_YR
       AND A1.BSE_QTR_CD = A5.BSE_QTR_CD
       AND A1.RLANE_CD   = A5.RLANE_CD
       AND A1.DIR_CD     = A5.DIR_CD
       AND A1.PF_GRP_CD  = A5.PF_GRP_CD
       AND A1.POL_CD     = A5.POL_CD
       AND A1.POD_CD     = A5.POD_CD
    
       AND A1.BSE_TP_CD     = A6.BSE_TP_CD
       AND A1.BSE_YR        = A6.BSE_YR
       AND A1.BSE_QTR_CD    = A6.BSE_QTR_CD
       AND A1.RLANE_CD      = A6.RLANE_CD
       AND A1.DIR_CD        = A6.DIR_CD
       AND A1.PF_GRP_CD     = A6.PF_GRP_CD

       AND A3.DELT_FLG    = 'N'
       AND A1.SQM_ACT_FLG = 'Y'
       AND A2.BSE_TP_CD   = @[f_bse_tp_cd]
       AND A2.BSE_YR      = @[f_bse_yr]
       AND A2.BSE_QTR_CD  = DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
    #if (${excel_flg} != 'Y')
       AND A2.OFC_VW_CD   = @[f_ofc_vw_cd]
    #end
    #if (${f_sub_trd_cd} != '' && ${f_sub_trd_cd} != 'All')
       AND A2.SUB_TRD_CD  = @[f_sub_trd_cd]
    #end
    #if (${f_ias_rgn_cd} != '' && ${f_ias_rgn_cd} != 'All')
       AND A3.IAS_RGN_CD  = @[f_ias_rgn_cd]
    #end
    #if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
       AND A2.RLANE_CD    = @[f_rlane_cd]
    #end
    #if (${f_click} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
       AND A2.DIR_CD      = @[f_dir_cd]
    #end
    #if (${f_click} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
       AND A3.HUL_BND_CD  = @[f_hul_bnd_cd]
    #end
    #if (${f_rhq_cd} != '' && ${f_rhq_cd} != 'All')
       AND A2.RHQ_CD      = @[f_rhq_cd]
    #end
    #if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
       AND A2.RGN_OFC_CD  = @[f_rgn_ofc_cd]
    #end
    #if (${f_pf_grp_cd} != '' && ${f_pf_grp_cd} != 'All')
       AND A2.PF_GRP_CD   = @[f_pf_grp_cd]
    #end
    #if (${f_fnal_bsa_capa} != '' && ${f_fnal_bsa_capa} != 'All')
       AND A2.FNL_BSA_CAPA= @[f_fnal_bsa_capa]
    #end
    #if (${f_pol_cd} != '' && ${f_pol_cd} != 'All')
       AND A2.POL_CD      = @[f_pol_cd]
    #end
    #if (${f_pod_cd} != '' && ${f_pod_cd} != 'All')
       AND A2.POD_CD      = @[f_pod_cd]
    #end
    #if (${f_sqm_mn_sctr_flg} != '' && ${f_sqm_mn_sctr_flg} != 'All')
       AND NVL(A5.SQM_MN_SCTR_FLG, 'N') = @[f_sqm_mn_sctr_flg]
    #end
       and rownum < 100000000 -- 조회 속도 향상을 위해 넣음. 넣으면 훨씬 빨라짐.

        ) M1
        , (
         SELECT BSE_TP_CD
              , @[f_bse_yr] BSE_YR
              , DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd]) BSE_QTR_CD
              , OFC_VW_CD
              , RLANE_CD
              , DIR_CD
              , PF_GRP_CD
              , RGN_OFC_CD
              , POL_CD
              , POD_CD
              , LOD_QTY AS PRE_LOD_QTY
              , GRS_RPB_REV AS PRE_GRS_RPB_REV
              , ROW_NUMBER() OVER (PARTITION BY BSE_TP_CD, BSE_YR, BSE_QTR_CD, OFC_VW_CD, RLANE_CD, DIR_CD, PF_GRP_CD, RGN_OFC_CD, POL_CD, POD_CD ORDER BY FNL_BSA_CAPA DESC) BSA_RANK
              , COUNT(*) OVER (PARTITION BY BSE_TP_CD, BSE_YR, BSE_QTR_CD, OFC_VW_CD, RLANE_CD, DIR_CD, PF_GRP_CD, RGN_OFC_CD, POL_CD, POD_CD ) MAX_BSA_RANK
           FROM SQM_SCTR_LOD_REV R1
          WHERE BSE_TP_CD = @[f_bse_tp_cd]
            AND BSE_YR    = TO_CHAR(ADD_MONTHS(TO_DATE(@[f_bse_yr]||DECODE(SUBSTR(DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd]), 0, 1), '1', '01', '2', '04', '3', '07', '4', '10','1'), 'YYYYMM'), -2), 'YYYY')
            AND BSE_QTR_CD= TO_CHAR(ADD_MONTHS(TO_DATE(@[f_bse_yr]||DECODE(SUBSTR(DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd]), 0, 1), '1', '01', '2','04', '3', '07', '4', '10', '1'), 'YYYYMM'), -2), 'Q')||'Q'
           #if (${excel_flg} != 'Y')
            AND OFC_VW_CD   = @[f_ofc_vw_cd]
           #end
           #if (${f_sub_trd_cd} != '' && ${f_sub_trd_cd} != 'All')
            AND SUB_TRD_CD  = @[f_sub_trd_cd]
           #end
           #if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
            AND RLANE_CD    = @[f_rlane_cd]
           #end
           #if (${f_click} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
            AND DIR_CD      = @[f_dir_cd]
           #end
           #if (${f_rhq_cd} != '' && ${f_rhq_cd} != 'All')
            AND RHQ_CD      = @[f_rhq_cd]
           #end
           #if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
            AND RGN_OFC_CD  = @[f_rgn_ofc_cd]
           #end
           #if (${f_pf_grp_cd} != '' && ${f_pf_grp_cd} != 'All')
            AND PF_GRP_CD   = @[f_pf_grp_cd]
           #end
           #if (${f_fnal_bsa_capa} != '' && ${f_fnal_bsa_capa} != 'All')
            AND FNL_BSA_CAPA= @[f_fnal_bsa_capa]
           #end
           #if (${f_pol_cd} != '' && ${f_pol_cd} != 'All')
            AND POL_CD      = @[f_pol_cd]
           #end
           #if (${f_pod_cd} != '' && ${f_pod_cd} != 'All')
            AND POD_CD      = @[f_pod_cd]
           #end

        UNION ALL
         -- FNL_BSA_CAPA기준으로 가장작은 숫자에 해당하는 RANK에 10개를 만들어준다. 지난 분기보다 현재분기에 FNL_BSA_CAPA 갯수가 더 많을 수 있으므로
         SELECT A.BSE_TP_CD
              , A.BSE_YR
              , A.BSE_QTR_CD
              , A.OFC_VW_CD
              , A.RLANE_CD
              , A.DIR_CD
              , A.PF_GRP_CD
              , A.RGN_OFC_CD
              , A.POL_CD
              , A.POD_CD
              , A.PRE_LOD_QTY
              , A.PRE_GRS_RPB_REV
              , B.BSA_RANK + A.MAX_BSA_RANK AS BSA_RANK
              , A.MAX_BSA_RANK
           FROM
                (SELECT BSE_TP_CD
                      , @[f_bse_yr] BSE_YR
                      , DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd]) BSE_QTR_CD
                      , OFC_VW_CD
                      , RLANE_CD
                      , DIR_CD
                      , PF_GRP_CD
                      , RGN_OFC_CD
                      , POL_CD
                      , POD_CD
                      , MAX(LOD_QTY) KEEP (DENSE_RANK LAST ORDER BY FNL_BSA_CAPA) AS PRE_LOD_QTY -- FNL_BSA_CAPA 기준으로 가장 큰숫자부터 RANK 정렬
                      , MAX(GRS_RPB_REV) KEEP (DENSE_RANK LAST ORDER BY FNL_BSA_CAPA) AS PRE_GRS_RPB_REV
                      , COUNT(*) AS MAX_BSA_RANK
                   FROM SQM_SCTR_LOD_REV R1
                  WHERE BSE_TP_CD = @[f_bse_tp_cd]
                    AND BSE_YR    = TO_CHAR(ADD_MONTHS(TO_DATE(@[f_bse_yr]||DECODE(SUBSTR(DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd]), 0, 1), '1', '01', '2', '04', '3', '07', '4', '10','1'), 'YYYYMM'), -2), 'YYYY')
                    AND BSE_QTR_CD= TO_CHAR(ADD_MONTHS(TO_DATE(@[f_bse_yr]||DECODE(SUBSTR(DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd]), 0, 1), '1', '01', '2','04', '3', '07', '4', '10', '1'), 'YYYYMM'), -2), 'Q')||'Q'
                   #if (${excel_flg} != 'Y')
                    AND OFC_VW_CD   = @[f_ofc_vw_cd]
                   #end
                   #if (${f_sub_trd_cd} != '' && ${f_sub_trd_cd} != 'All')
                    AND SUB_TRD_CD  = @[f_sub_trd_cd]
                   #end
                   #if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                    AND RLANE_CD    = @[f_rlane_cd]
                   #end
                   #if (${f_click} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                    AND DIR_CD      = @[f_dir_cd]
                   #end
                   #if (${f_rhq_cd} != '' && ${f_rhq_cd} != 'All')
                    AND RHQ_CD      = @[f_rhq_cd]
                   #end
                   #if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
                    AND RGN_OFC_CD  = @[f_rgn_ofc_cd]
                   #end
                   #if (${f_pf_grp_cd} != '' && ${f_pf_grp_cd} != 'All')
                    AND PF_GRP_CD   = @[f_pf_grp_cd]
                   #end
                   #if (${f_fnal_bsa_capa} != '' && ${f_fnal_bsa_capa} != 'All')
                    AND FNL_BSA_CAPA= @[f_fnal_bsa_capa]
                   #end
                   #if (${f_pol_cd} != '' && ${f_pol_cd} != 'All')
                    AND POL_CD      = @[f_pol_cd]
                   #end
                   #if (${f_pod_cd} != '' && ${f_pod_cd} != 'All')
                    AND POD_CD      = @[f_pod_cd]
                   #end
               GROUP BY BSE_TP_CD
                      , BSE_YR
                      , BSE_QTR_CD
                      , OFC_VW_CD
                      , RLANE_CD
                      , DIR_CD
                      , PF_GRP_CD
                      , RGN_OFC_CD
                      , POL_CD
                      , POD_CD
                ) A
              , (SELECT LEVEL AS BSA_RANK FROM DUAL CONNECT BY LEVEL < 10) B
        ) S1
    WHERE M1.OLD_PF_GRP_CD = M1.MAX_PF_GRP_CD
      AND S1.BSE_TP_CD(+)  = M1.BSE_TP_CD
      AND S1.BSE_YR(+)     = M1.BSE_YR
      AND S1.BSE_QTR_CD(+) = M1.BSE_QTR_CD
      AND S1.OFC_VW_CD(+)  = M1.OFC_VW_CD
      AND S1.RLANE_CD(+)   = M1.RLANE_CD
      AND S1.DIR_CD(+)     = M1.DIR_CD
      AND S1.PF_GRP_CD(+)  = M1.PF_GRP_CD
      AND S1.BSA_RANK(+)   = M1.BSA_RANK
      AND S1.RGN_OFC_CD(+) = M1.RGN_OFC_CD
      AND S1.POL_CD(+)     = M1.POL_CD
      AND S1.POD_CD(+)     = M1.POD_CD
        
 ORDER BY M1.BSE_YR
        , M1.BSE_QTR_CD
        , M1.OFC_VW_CD
        , M1.TRD_CD
        , M1.SUB_TRD_CD
        , M1.IAS_RGN_CD
        , M1.RLANE_CD
        , M1.DIR_CD
        , M1.HUL_BND_CD
        , M1.PF_GRP_CD
        , M1.FNL_BSA_CAPA
        , M1.RHQ_CD
        , M1.RGN_OFC_CD
        , M1.POL_CD
        , M1.POL_CALL_SEQ
        , M1.POD_CD
        , M1.POD_CALL_SEQ

#if (${freezing_flg} == 'Y')
)
WHERE LOD_QTY != 0
AND GRS_RPB_REV = 0
ORDER BY OFC_VW_NM DESC,RLANE_CD, DIR_CD, HUL_BND_CD, PF_GRP_CD, RHQ_CD, RGN_OFC_CD, POL_CD, POD_CD
#end

