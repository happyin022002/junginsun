<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOAllocationReprocessByTradRSQL">
			<desc><![CDATA[Compulsory Firm 에서 Reprocess 버튼 클릭시 동작, Trad로 SPC_SB_BKG_APLY_PROC 호출함.]]></desc>
			<sql><![CDATA[
CALL SPC_SB_BKG_APLY_PRC('W', null, null, @[rlane_cd], @[dir_cd], @[week], @[duration], @[cfm_usr_id])
/*
    in_mode       IN VARCHAR2, -- default=B, B:BKG단건, V:VVD, W:Week기준(wk, duration, rev lane, bound)
    in_bkg_no     IN VARCHAR2, 
    in_vvd        IN VARCHAR2,
    in_rlane      IN VARCHAR2,
    in_bound      IN VARCHAR2,
    in_wk         IN VARCHAR2,
    in_duration   IN VARCHAR2,
    in_user_id    IN VARCHAR2
*/			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="week" type="12" value="" out="N"/>
				<param name="duration" type="12" value="" out="N"/>
				<param name="cfm_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
