<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAOPriMotTrfMnUSQL">
			<desc><![CDATA[PRI_MOT_TRF_MN Table Update]]></desc>
			<sql><![CDATA[
UPDATE  PRI_MOT_TRF_MN
SET
        FILE_DT     = TO_DATE ( @[file_dt], 'YYYY-MM-DD' )
    ,   EFF_DT      = TO_DATE ( @[eff_dt],  'YYYY-MM-DD' )
    ,   UPD_USR_ID  = @[upd_usr_id]
    ,   UPD_DT      = SYSDATE
    ,   CFM_FLG     = DECODE ( @[cfm_flg], 'No', 'N', 'Yes', 'Y', @[cfm_flg] )
    ,   CFM_DT      = DECODE ( DECODE ( @[cfm_flg], 'No', 'N', 'Yes', 'Y', @[cfm_flg] ), 'Y', SYSDATE, NULL )
WHERE   SVC_SCP_CD  = @[svc_scp_cd]
AND     MOT_TRF_SEQ = @[mot_trf_seq]			]]></sql>
			<params>
				<param name="file_dt" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="mot_trf_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
