<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOModifyCodBookingListCSQL">
			<desc><![CDATA[COD BKG Inquiry에서 Audit한 결과를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_REV_AUD_RSLT(
	BKG_NO
,	BKG_REV_AUD_TP_CD
,	BKG_REV_AUD_SEQ
,	CNTR_TPSZ_CD
,	UMCH_RSN_RMK
,	RGN_GRP_AVC_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT

)VALUES (
	@[bkg_no]
,	'C'
,	 (
       SELECT NVL(MAX(BKG_REV_AUD_SEQ), 0) 
       FROM   BKG_REV_AUD_RSLT
       WHERE  BKG_NO = @[bkg_no]
       AND BKG_REV_AUD_TP_CD ='C'
      ) +1 
,	@[ctrt_cntr_tpsz_cd]
,	@[umch_rsn_rmk]
,	@[rgn_grp_avc_rmk]
,	@[usr_id]
,	sysdate
,	@[usr_id]
,	sysdate
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ctrt_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="umch_rsn_rmk" type="12" value="" out="N"/>
				<param name="rgn_grp_avc_rmk" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
