<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCmpnApprovalDBDAOSearchFFCmpnCSRDetailVVDCDCheckRSQL">
			<desc><![CDATA[SearchFFCmpnCSRDetailVVDCDCheck]]></desc>
			<sql><![CDATA[
/* GET VVD_CD */
SELECT 'CFDR'||SUBSTR(GL_DT,3,4)||'EE' AS VVD_CD
  FROM AP_INV_HDR 
 WHERE CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
