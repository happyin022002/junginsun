<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BangladeshManifestListDownloadDBDAOaddManifestIbDlListCSQL">
			<desc><![CDATA[방글라데시 Manifest Inbound DownLoad한 List 입력]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_BD_CNTR 
       (
        VSL_CD,             SKD_VOY_NO,             SKD_DIR_CD,         			BD_CSTMS_BND_CD,    POL_CD,
        POD_CD,             BL_LINE_NO,             BL_NO,              			BKG_CNTR_QTY,       PCK_DESC,
        MK_DESC,            CSTMS_DESC,             SAIL_DT,            			CNEE_LIC_NO,        CNEE_RMK,
        NTFY_LIC_NO,        NTFY_RMK,               CNTR_WGT,           			WGT_UT_CD,          NET_WGT,
        CNTR_NO,            MLT_CNTR_SEAL_NO_DESC,  CNTR_TPSZ_CD,       			CNTR_SZ_DESC,       CNTR_TP_DESC, 
        CNTR_HGT,           BD_CNTR_CGO_TP_CD,      IMO_DESC,           			IMDG_UN_DESC,       VAT_DESC,
        CMDT_DESC,          FDCK_DESC,              PRSB_CD,            			BIL_RMK,            BKG_CGO_TP_CD,
        VSL_NM,	            CRE_USR_ID,             CRE_DT,             			UPD_USR_ID,         UPD_DT
       )
VALUES (
        SUBSTR(@[vvd],1,4), SUBSTR(@[vvd],5,4),     SUBSTR(@[vvd],9,1), 			'I',                @[pol_cd],
        @[pod_cd],          @[line_no],             @[org_bl_no],       			@[qty],             @[description],
        @[marks],           @[goods_desc],          REPLACE(@[goods_date],'-',''),  @[cons_lice],       @[cons_nm],
        @[noty_lice],       @[noty_nm],             @[cntr_gross_wgt],  			@[cntr_ut_cd],      @[cntr_tare_wgt],
        @[cntr_no],         @[seal],                @[cntr_tz],         			@[cntr_size],            @[cntr_type],
        @[height],          @[status],              @[imco],            			@[un],              @[vat],
        @[cmdt_cd],         @[off_dock],            @[perish_cd],       			@[remarks],         @[bkg_cgo_tp_cd],
        @[vsl_nm],          @[cre_usr_id],          sysdate,            			@[upd_usr_id],      sysdate
       )			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="line_no" type="12" value="" out="N"/>
				<param name="org_bl_no" type="12" value="" out="N"/>
				<param name="qty" type="12" value="" out="N"/>
				<param name="description" type="12" value="" out="N"/>
				<param name="marks" type="12" value="" out="N"/>
				<param name="goods_desc" type="12" value="" out="N"/>
				<param name="goods_date" type="12" value="" out="N"/>
				<param name="cons_lice" type="12" value="" out="N"/>
				<param name="cons_nm" type="12" value="" out="N"/>
				<param name="noty_lice" type="12" value="" out="N"/>
				<param name="noty_nm" type="12" value="" out="N"/>
				<param name="cntr_gross_wgt" type="12" value="" out="N"/>
				<param name="cntr_ut_cd" type="12" value="" out="N"/>
				<param name="cntr_tare_wgt" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="seal" type="12" value="" out="N"/>
				<param name="cntr_tz" type="12" value="" out="N"/>
				<param name="cntr_size" type="12" value="" out="N"/>
				<param name="cntr_type" type="12" value="" out="N"/>
				<param name="height" type="12" value="" out="N"/>
				<param name="status" type="12" value="" out="N"/>
				<param name="imco" type="12" value="" out="N"/>
				<param name="un" type="12" value="" out="N"/>
				<param name="vat" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="off_dock" type="12" value="" out="N"/>
				<param name="perish_cd" type="12" value="" out="N"/>
				<param name="remarks" type="12" value="" out="N"/>
				<param name="bkg_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
