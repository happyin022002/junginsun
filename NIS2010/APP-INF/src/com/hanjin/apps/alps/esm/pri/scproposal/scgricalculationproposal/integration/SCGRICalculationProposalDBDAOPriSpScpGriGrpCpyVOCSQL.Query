<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGRICalculationProposalDBDAOPriSpScpGriGrpCpyVOCSQL">
			<desc><![CDATA[PriSpScpGriGrpVO Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_SP_SCP_GRI_GRP_CPY (
    USR_ID
,	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	GEN_SPCL_RT_TP_CD
,	GRI_GRP_SEQ
,	FLT_PCT_TP_CD
,	GRI_APPL_DIV_CD
,	GRI_APPL_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT
	@[cre_usr_id]
,	PROP_NO
,	AMDT_SEQ
,	SVC_SCP_CD
,	GEN_SPCL_RT_TP_CD
,	GRI_GRP_SEQ
,	FLT_PCT_TP_CD
,	GRI_APPL_DIV_CD
,	GRI_APPL_FLG
,	@[cre_usr_id]
,	sysdate
,	@[cre_usr_id]
,	sysdate
FROM PRI_SP_SCP_GRI_GRP
WHERE 
	PROP_NO = @[prop_no]
AND	AMDT_SEQ = @[amdt_seq]
AND	SVC_SCP_CD = @[svc_scp_cd]
AND	GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd] 
AND GRI_GRP_SEQ = @[gri_grp_seq]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
				<param name="gri_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
