<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriMnPrsCalcFlgOnAmendUSQL">
			<desc><![CDATA[Rate Amend/Cancel시 PRI_TRI_MN의 PRS Flag Update]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRI_MN SET 
	PRS_RT_CMPB_CALC_FLG = DECODE(@[prop_sts_cd], 'I', 'Y', 'N')
WHERE	TRI_PROP_NO = @[tri_prop_no]			]]></sql>
			<params>
				<param name="prop_sts_cd" type="12" value="" out="N"/>
				<param name="tri_prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
