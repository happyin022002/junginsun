<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOSearchFromUsdToEtcCurrencyRSQL">
			<desc><![CDATA[USD 금액을 다른 환율 금액으로 변경]]></desc>
			<sql><![CDATA[
SELECT USD_LOCL_XCH_RT * NVL(@[rate], 0) RATE
  FROM GL_MON_XCH_RT
 WHERE 1 = 1
       AND ACCT_XCH_RT_YRMON = (SELECT MAX(ACCT_XCH_RT_YRMON)
                                  FROM GL_MON_XCH_RT
                                 WHERE 1 = 1
                                       AND ACCT_XCH_RT_LVL = '1'
                                       AND CURR_CD = @[curr_cd])
       AND ACCT_XCH_RT_LVL = '1'
       AND CURR_CD = @[curr_cd]			]]></sql>
			<params>
				<param name="rate" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
