<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOaddSmsRcvListCSQL">
			<desc><![CDATA[BookingMasterMgtDBDAOaddSmsRcvListCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CLL_CNG_NTFY_SET 
(
SLAN_CD, DIR_CD, 
CRE_SEQ, 
OFC_CD, RCVR_USR_ID, RCVR_EML, 
PHN_NO, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT, 
RCVR_USR_NM, OFC_PHN_NO, OFC_FAX_NO,
LOCL_TS_IND_CD, PORT_CD, FILE_ATCH_FLG
)
VALUES 
(
@[slan_cd], @[dir_cd], 
(SELECT nvl(MAX(CRE_SEQ),0)+1 FROM BKG_CLL_CNG_NTFY_SET),
@[ofc_cd], @[rcvr_usr_id], @[rcvr_eml], 
@[phn_no], @[cre_usr_id], sysdate, @[upd_usr_id], sysdate,
@[rcvr_usr_nm], @[ofc_phn_no], @[ofc_fax_no],
@[locl_ts_ind_cd], @[port_cd], decode(@[file_atch_flg],'1','Y','N')
 )			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="rcvr_usr_id" type="12" value="" out="N"/>
				<param name="rcvr_eml" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rcvr_usr_nm" type="12" value="" out="N"/>
				<param name="ofc_phn_no" type="12" value="" out="N"/>
				<param name="ofc_fax_no" type="12" value="" out="N"/>
				<param name="locl_ts_ind_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="file_atch_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
