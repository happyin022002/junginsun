<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOMultiOwnDailyHireCSQL">
			<desc><![CDATA[MultiOwnDailyHire INSERT]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_OWN_VSL_DLY_HIR A
 USING (
        SELECT @[cost_yrmon]    AS COST_YRMON
              ,@[vsl_cd]        AS VSL_CD
              ,@[stnd_cost_cd]  AS STND_COST_CD
        FROM DUAL
       ) B
 ON   (
           A.COST_YRMON   = B.COST_YRMON
       AND A.VSL_CD       = B.VSL_CD
       AND A.STND_COST_CD = B.STND_COST_CD
      )
 WHEN MATCHED THEN
      UPDATE
         SET DHIR_AMT     = @[dhir_amt]
            ,OWN_VSL_RMK  = @[own_vsl_rmk]
            ,UPD_USR_ID   = @[upd_usr_id]
            ,UPD_DT       = SYSDATE
 WHEN NOT MATCHED THEN
      INSERT(
          COST_YRMON
         ,VSL_CD
         ,VSL_CLSS_CAPA
         ,STND_COST_CD
         ,DHIR_AMT
         ,OWN_VSL_RMK
         ,CRE_USR_ID
         ,CRE_DT
         ,UPD_USR_ID
         ,UPD_DT
      )VALUES(
          @[cost_yrmon]
         ,@[vsl_cd]
         ,@[vsl_clss_capa]
         ,@[stnd_cost_cd]
         ,@[dhir_amt]
         ,@[own_vsl_rmk]
         ,@[cre_usr_id]
         ,SYSDATE
         ,@[upd_usr_id]
         ,SYSDATE
      )			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="" out="Y"/>
				<param name="vsl_cd" type="12" value="" out="Y"/>
				<param name="stnd_cost_cd" type="12" value="" out="Y"/>
				<param name="dhir_amt" type="12" value="" out="Y"/>
				<param name="own_vsl_rmk" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="vsl_clss_capa" type="12" value="" out="Y"/>
				<param name="cre_usr_id" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
