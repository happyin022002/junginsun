<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingProcessMgtDBDAOBkgDocPerfOfcVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	BKG_OFC_CD
,   POR_CD
,	RGN_OFC_CD
,	GSO_OFC_CD
,	SUB_GRP_CTNT
,	TEAM_NM AS SUB_GSO_OFC_CD
,	USE_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	'' OFC_CD
,	'' CHK_OP
,	'' OPTION_TP
,	'' OFC_TY
,	'' CTRl_OFC_CD
,	'' SI_NTFC_EML
,	'' BKG_NTFC_EML
,	'' HNDL_OFC_CD
,	'' HNDL_OFC_SEQ
FROM BKG_DOC_PERF_OFC
#if (${chk_op} == '0' && ${ofc_cd} != '') 
WHERE	BKG_OFC_CD = @[ofc_cd]
#elseif (${chk_op} == '1' && ${ofc_cd} != '')  
WHERE	POR_CD = @[ofc_cd]
#elseif (${chk_op} == '2' && ${ofc_cd} != '') 
WHERE	 GSO_OFC_CD= @[ofc_cd]
#elseif (${chk_op} == '3' && ${ofc_cd} != '')  
WHERE	 RGN_OFC_CD= @[ofc_cd]
#end
			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
