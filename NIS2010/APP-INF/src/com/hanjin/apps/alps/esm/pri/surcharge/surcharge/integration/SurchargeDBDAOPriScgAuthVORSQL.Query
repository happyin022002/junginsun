<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOPriScgAuthVORSQL">
			<desc><![CDATA[Surcharge 승인 권한 조회]]></desc>
			<sql><![CDATA[
SELECT 
   'Y' AS APRO_AUTH
FROM PRI_AUTHORIZATION
WHERE	PRC_CTRT_TP_CD = 'C'
AND	TRD_CD = @[trd_cd]
AND	USR_ID = @[usr_id]
AND SYSDATE BETWEEN EFF_DT AND EXP_DT
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
