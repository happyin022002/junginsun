<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOmodifyPkupNtcDtlByBkgNoUSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
UPDATE BKG_PKUP_NTC_DTL 
SET 
	FAX_NO = @[fax_no]
,	FAX_TP_CD = DECODE(FAX_NO,@[fax_no],FAX_TP_CD,'M')
,	NTC_EML = @[ntc_eml]
,	EML_TP_CD = DECODE(NTC_EML,@[ntc_eml],EML_TP_CD,'M')
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	(BKG_NO, NTC_SEQ)
        IN 
        (SELECT B.BKG_NO, B.NTC_SEQ
           FROM BKG_PKUP_NTC A
               ,BKG_PKUP_NTC B
          WHERE A.BKG_NO              = @[bkg_no]
            AND A.NTC_SEQ             = @[ntc_seq]
            AND B.BKG_NO              = A.BKG_NO
            AND B.CNTR_NO             = A.CNTR_NO
            AND B.BKG_CUST_TP_CD      = A.BKG_CUST_TP_CD
            AND B.PKUP_NTC_SND_STS_CD = 'N'
        )
  AND	CUST_CNTC_TP_CD = @[cust_cntc_tp_cd]			]]></sql>
			<params>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="ntc_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ntc_seq" type="2" value="" out="N"/>
				<param name="cust_cntc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
