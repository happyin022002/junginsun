<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchBkgDgCgoRSQL">
			<desc><![CDATA[searchBkgDgCgo]]></desc>
			<sql><![CDATA[
SELECT 'Seq : '||DCGO_SEQ||', Un No : '||IMDG_UN_NO||', Class : '||IMDG_CLSS_CD DIFF_RMK
      , DCGO_SEQ
      , CNTR_NO
  FROM BKG_DG_CGO
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
