<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOsearchUsWhfSendExptTsBkgNoRSQL">
			<desc><![CDATA[searchUsWhfSendExptTsBkgNo]]></desc>
			<sql><![CDATA[
SELECT *
  FROM (
        SELECT B.BKG_NO
              ,ROW_NUMBER() OVER(PARTITION BY B.BKG_NO ORDER BY B.BKG_NO) AS RNUM
          FROM BKG_USA_WHF_CNTR A
              ,BKG_BOOKING B
         WHERE A.BL_NO = B.BL_NO
           AND A.VSL_CD = @[vsl_cd]
           AND A.SKD_VOY_NO = @[skd_voy_no]
           AND A.SKD_DIR_CD = @[skd_dir_cd]
           AND A.PORT_CD = @[port_cd]
           AND A.IO_BND_CD = @[io_bnd_cd]
           AND (A.USA_WHF_EXPT_FLG = 'Y' OR A.USA_WHF_TRSP_TP_CD = 'T')
       )
 WHERE RNUM = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
