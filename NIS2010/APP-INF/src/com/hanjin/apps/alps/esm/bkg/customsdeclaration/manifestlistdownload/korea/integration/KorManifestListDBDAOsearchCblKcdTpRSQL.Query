<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchCblKcdTpRSQL">
			<desc><![CDATA[CSTMS_BL_NO별 CSTMS_DECL_TP_CD을 다시 구한다.]]></desc>
			<sql><![CDATA[
SELECT CSTMS_DECL_TP_CD
  FROM BKG_CSTMS_KR_BL
 WHERE BKG_NO        = @[bkg_no]
   AND TRNS_SEQ      = @[trns_seq]
   AND DMST_PORT_CD  = @[dmst_port_cd]
   AND CSTMS_BL_NO   = @[c_bl_no]
   AND VSL_CD        = SUBSTR(@[vvd],1,4)
   AND SKD_VOY_NO    = SUBSTR(@[vvd],5,4)
   AND SKD_DIR_CD    = SUBSTR(@[vvd],9,1)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="trns_seq" type="12" value="" out="N"/>
				<param name="dmst_port_cd" type="12" value="" out="N"/>
				<param name="c_bl_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
