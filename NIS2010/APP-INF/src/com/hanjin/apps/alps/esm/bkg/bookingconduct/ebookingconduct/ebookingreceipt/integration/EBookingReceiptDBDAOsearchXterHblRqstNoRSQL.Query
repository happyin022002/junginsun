<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterHblRqstNoRSQL">
			<desc><![CDATA[EBookingReceiptDBDAOsearchXterHblRqstNoRSQL]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(CASE WHEN SUBSTR(XTER_RQST_NO,-2,1) ='H' AND REGEXP_LIKE(SUBSTR(XTER_RQST_NO,-1), '[0-9]')
       			THEN @[rqst_no]||'H'||TO_CHAR(SUBSTR(XTER_RQST_NO,-1)+1)
         		ELSE @[rqst_no]||'H1'
          	END),@[rqst_no]||'H1') XTER_RQST_NO
  FROM BKG_XTER_RQST_MST
 WHERE XTER_SNDR_ID = @[sender_id]
   AND XTER_RQST_NO LIKE @[rqst_no]||'H%'
   AND BL_NO_CTNT = @[rqst_no]
   AND XTER_BL_TP_CD ='H'			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="sender_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
