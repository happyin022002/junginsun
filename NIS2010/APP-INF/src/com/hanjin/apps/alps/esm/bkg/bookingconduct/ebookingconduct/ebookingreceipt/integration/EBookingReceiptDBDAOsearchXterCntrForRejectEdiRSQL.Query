<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchXterCntrForRejectEdiRSQL">
			<desc><![CDATA[searchXterCntrForRejectEdi]]></desc>
			<sql><![CDATA[
select 
    cntr.CNTR_NO	       a_cntr_no              
    , cntr.CNTR_TPSZ_CD	   a_cntr_tpsz            
    , seal.XTER_CNTR_SEAL_NO	   a_seal_no              
    , NULL	               a_cntr_rind            
    , NULL	               a_cntr_dind            
    , NULL	               a_cntr_aind            
    , NULL	               a_cntr_bind            
    , TRIM(TO_CHAR(cntr.cntr_wgt,'99999999.999'))	a_cntr_wgt_qty         
    , cntr.wgt_ut_cd       a_cntr_wgt_tp          
    , NULL	               a_cntr_temp            
    , NULL	               a_cntr_tun             
    , NULL	               a_cntr_temp_c          
    , NULL	               a_cntr_tun_c           
    , NULL	               a_cntr_rf_voltage      
    , NULL	               a_cntr_vent            
    , NULL	               a_cntr_humid           
    , NULL	               a_cntr_genset          
    , NULL	               a_cntr_rf_remark       
    , NULL	               a_cntr_rfdry_ind       
    , NULL	               a_cntr_ovf             
    , NULL	               a_cntr_ovr             
    , NULL	               a_cntr_ovh             
    , NULL	               a_cntr_ovlw            
    , NULL	               a_cntr_ovrw            
    , NULL	               a_cntr_ovwgt           
    , NULL	               a_cntr_void_slot       
    , NULL	               a_cntr_stwg_req        
    , NULL	               a_cntr_ttl_dim_len     
    , NULL	               a_cntr_ttl_dim_wdt     
    , NULL	               a_cntr_ttl_dim_hgt     
    , NULL	               a_cntr_trm_type        
    , NULL	               a_cntr_trm_weight      
    , NULL	               a_cntr_trm_haulage     
    , NULL	               a_cntr_trm_hmode       
    , NULL	               a_cntr_trm_pickup_cy   
    , NULL	               a_cntr_trm_return_cy   
    , NULL	               a_cntr_trm_instruction 
    , NULL	               a_cntr_trm_tran_dt     
    , NULL	               a_cntr_trm_tran_office 
    , NULL	               a_cntr_trm_tran_no     
    , NULL	               a_cntr_usr_id     
  from bkg_xter_cntr cntr, bkg_xter_cntr_seal_no seal
 where cntr.xter_sndr_id  = @[sender_id]
   and cntr.xter_rqst_no  = @[rqst_no]
   and cntr.xter_rqst_seq = @[rqst_seq]
   and cntr.cntr_seq      = 1
   and cntr.xter_sndr_id  = seal.xter_sndr_id   (+)
   and cntr.xter_rqst_no  = seal.xter_rqst_no   (+)
   and cntr.xter_rqst_seq = seal.xter_rqst_seq  (+)
   and cntr.cntr_no       = seal.cntr_no        (+)
   and cntr.cntr_seq      = seal.cntr_seq       (+)
   and 1                  = seal.cntr_seal_seq  (+)			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
