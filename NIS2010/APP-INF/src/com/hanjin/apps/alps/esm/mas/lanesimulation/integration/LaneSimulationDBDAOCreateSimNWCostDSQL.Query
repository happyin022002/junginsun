<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneSimulationDBDAOCreateSimNWCostDSQL">
			<desc><![CDATA[NWCost 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MAS_SIM_NTWK_COST
	   WHERE SIM_DT  = @[sim_dt]
	     AND SIM_NO  = @[sim_no]			]]></sql>
