<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOsearchGrpCmdtNmRSQL">
			<desc><![CDATA[Group Commodity Name 조회]]></desc>
			<sql><![CDATA[
SELECT 
	SCG_GRP_CMDT_DESC AS CMDT_NM
  FROM PRI_SCG_GRP_CMDT
 WHERE SVC_SCP_CD = 'TPW'
   AND CHG_CD = 'GRI'
   AND SCG_GRP_CMDT_CD = TRIM(TO_CHAR(@[cmdt_cd],'00')) 
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
