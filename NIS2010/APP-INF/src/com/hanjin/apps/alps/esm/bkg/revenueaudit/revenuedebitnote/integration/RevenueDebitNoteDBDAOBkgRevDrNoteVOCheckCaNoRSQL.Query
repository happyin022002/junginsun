<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOBkgRevDrNoteVOCheckCaNoRSQL">
			<desc><![CDATA[CA NO 가 존재 여부 조회]]></desc>
			<sql><![CDATA[
SELECT  COUNT(1)  CHK
FROM    (
        SELECT  'X'
        FROM    BKG_CORRECTION
        WHERE   BKG_NO  = @[bkg_no]
        AND     CORR_NO = @[bkg_corr_no]
        AND     RAT_FLG = 'Y'

        UNION ALL

        SELECT  'X'
        FROM    DUAL
        WHERE   @[bkg_corr_no] = 'NOBDR00001'
        )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_corr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
