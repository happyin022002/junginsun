<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAORsltPriRpAmdHstMnSpotVORSQL">
			<desc><![CDATA[2014.09.15 최성환  [CHM-201431899] Guideline RFA 생성 요청 ]]></desc>
			<sql><![CDATA[
SELECT HDR.RFA_NO
	  ,MN.PROP_NO
      ,MN.AMDT_SEQ
      ,'' CTRT_PTY_NM
      ,TO_CHAR(DUR.CTRT_EFF_DT, 'YYYY-MM-DD') CTRT_EFF_DT
      ,TO_CHAR(DUR.CTRT_EXP_DT, 'YYYY-MM-DD') CTRT_EXP_DT
      ,'Spot Guide' AS RFA_CTRT_TP_CD
FROM   PRI_RP_MN MN
      ,PRI_RP_HDR HDR
      ,PRI_RP_DUR DUR
WHERE  HDR.PROP_NO = MN.PROP_NO
AND    MN.PROP_NO = DUR.PROP_NO
AND    MN.AMDT_SEQ = DUR.AMDT_SEQ
AND    HDR.RFA_NO = @[rfa_no]
AND    MN.RFA_CTRT_TP_CD = 'G'
AND    MN.AMDT_SEQ = (SELECT /*+ INDEX_DESC(B XPKPRI_RP_MN)*/ AMDT_SEQ
                      FROM   PRI_RP_MN B
                      WHERE  PROP_NO =MN.PROP_NO
                      AND    PROP_STS_CD = 'A'
					  AND    ROWNUM = 1)			]]></sql>
			<params>
				<param name="rfa_no" type="12" value="ATL14A0013" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
