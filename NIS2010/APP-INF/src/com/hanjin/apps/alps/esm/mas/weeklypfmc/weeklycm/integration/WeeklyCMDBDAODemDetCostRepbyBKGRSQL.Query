<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAODemDetCostRepbyBKGRSQL">
			<desc><![CDATA[DEM/DET Cost Report by BKG 를 Retrieve(조회) 한다.]]></desc>
			<sql><![CDATA[
SELECT  POOL_CD
	   ,SCC_CD
	   ,BZC_RT
	   ,TAX_PCT||'%' AS TAX_PCT
	   ,RT_AMT
	   ,COST_YRMON
       ,POOL_CD AS POOL_CD_ORG
	   ,SCC_CD AS SCC_CD_ORG
       ,COST_YRMON AS COST_YRMON_ORG 
FROM MAS_CHSS_PDM_UPLD
#if (${f_yearmonth} != '') 
WHERE COST_YRMON LIKE @[f_yearmonth]||'%'
	#if (${f_poolno} != '')
	AND POOL_CD LIKE @[f_poolno]||'%'
		#if (${f_sccno} != '')
			AND SCC_CD LIKE @[f_sccno]||'%'
		#elseif (${f_sccno} == '')
		#end
	#elseif (${f_poolno} == '')
		#if (${f_sccno} != '')
			AND SCC_CD LIKE @[f_sccno]||'%'
		#elseif (${f_sccno} == '')
		#end
	#end
#elseif (${f_yearmonth} == '')     
	#if (${f_poolno} != '')
	WHERE POOL_CD LIKE @[f_poolno]||'%'
		#if (${f_sccno} != '')
			AND SCC_CD LIKE @[f_sccno]||'%'
		#elseif (${f_sccno} == '')
		#end
	#elseif (${f_poolno} == '')
		#if (${f_sccno} != '')
			WHERE SCC_CD LIKE @[f_sccno]||'%'
		#elseif (${f_sccno} == '')
		#end
	#end
#end			]]></sql>
			<params>
				<param name="f_yearmonth" type="12" value="" out="N"/>
				<param name="f_poolno" type="12" value="" out="N"/>
				<param name="f_sccno" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
