<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAORequestCheckForMatchBackVORSQL">
			<desc><![CDATA[Request 시 Match Back 조회 하지 않은 Scope를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   PROP_NO, AMDT_SEQ ,SVC_SCP_CD 
FROM (
          SELECT   MN.PROP_NO,MN.AMDT_SEQ ,MN.SVC_SCP_CD 
          FROM     PRI_RP_SCP_MN MN
          WHERE    MN.PROP_NO = @[prop_no]
      		  AND  MN.AMDT_SEQ = @[amdt_seq]
			  AND  MN.PRS_RT_MB_FLG = 'N'
			  AND  EXISTS (SELECT   1
                           FROM     PRI_RP_SCP_RT RT
                           WHERE    RT.PROP_NO        = MN.PROP_NO
                                AND RT.AMDT_SEQ       = MN.AMDT_SEQ 
                                AND RT.SVC_SCP_CD     = MN.SVC_SCP_CD
								AND N1ST_CMNC_AMDT_SEQ = MN.AMDT_SEQ 
								AND SRC_INFO_CD <> 'AD'
								AND PRC_PROG_STS_CD IN ('I','R')
                                AND ROWNUM = 1
              )
         
)
ORDER BY SVC_SCP_CD			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
