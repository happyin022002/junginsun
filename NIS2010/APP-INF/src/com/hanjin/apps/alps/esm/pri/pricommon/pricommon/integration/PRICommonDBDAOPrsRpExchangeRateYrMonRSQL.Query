<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOPrsRpExchangeRateYrMonRSQL">
			<desc><![CDATA[pri_rp_scp_mn prs_xch_rt_yrmon 을 조회한다. 조회조건 (prop_no , amdt_seq , svc_scp_cd )]]></desc>
			<sql><![CDATA[
select prs_xch_rt_yrmon as cd,
	 prs_xch_rt_yrmon as nm
from pri_rp_scp_mn 
where prop_no = @[etc1]
and amdt_seq = @[etc2]
and svc_scp_cd = @[etc3]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="etc3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
