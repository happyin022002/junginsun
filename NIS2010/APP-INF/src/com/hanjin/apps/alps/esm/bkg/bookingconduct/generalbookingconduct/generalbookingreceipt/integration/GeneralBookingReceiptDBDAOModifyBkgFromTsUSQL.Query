<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOModifyBkgFromTsUSQL">
			<desc><![CDATA[port skip update]]></desc>
			<sql><![CDATA[
UPDATE	BKG_BOOKING
SET		PORT_SKP_FLG = @[port_skp_flg]
WHERE	BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="port_skp_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
