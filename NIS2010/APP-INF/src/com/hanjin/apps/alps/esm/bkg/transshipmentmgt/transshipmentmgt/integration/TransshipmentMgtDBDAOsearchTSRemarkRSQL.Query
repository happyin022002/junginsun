<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransshipmentMgtDBDAOsearchTSRemarkRSQL">
			<desc><![CDATA[bkg_ts_rmk table을 조회한다.   ]]></desc>
			<sql><![CDATA[
select  bkg_no, ts_rmk
  from bkg_ts_rmk
 where bkg_no =@[bkg_no]
  and TS_RMK_DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
