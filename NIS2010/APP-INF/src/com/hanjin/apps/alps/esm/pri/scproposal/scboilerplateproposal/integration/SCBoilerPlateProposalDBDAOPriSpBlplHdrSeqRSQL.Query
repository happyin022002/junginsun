<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCBoilerPlateProposalDBDAOPriSpBlplHdrSeqRSQL">
			<desc><![CDATA[boiler plate header seq]]></desc>
			<sql><![CDATA[
SELECT MIN(BLPL_HDR_SEQ) BLPL_HDR_SEQ
FROM PRI_SG_BLPL_HDR
WHERE TO_DATE(@[hdr_eff_dt],'yyyy-MM-dd') BETWEEN EFF_DT AND EXP_DT
AND CFM_FLG = 'Y'
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="hdr_eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
