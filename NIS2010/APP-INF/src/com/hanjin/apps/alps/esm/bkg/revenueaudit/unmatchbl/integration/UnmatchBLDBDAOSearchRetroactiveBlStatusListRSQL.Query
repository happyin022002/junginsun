<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOSearchRetroactiveBlStatusListRSQL">
			<desc><![CDATA[소급적용하여 Rating한 BKG Summary Report 조회]]></desc>
			<sql><![CDATA[
WITH
BK AS(
SELECT  
#if (${date_type} == 'A')
        /*+LEADING(BKG_RATE, BKG_BOOKING)*/
#end
         (SELECT  OFC_CD 
         FROM MDM_ORGANIZATION 
         WHERE DELT_FLG = 'N' 
         AND OFC_TP_CD = 'HQ' 
#if(${ofc_tp_cd} == 'B')
         START WITH OFC_CD = B.BKG_OFC_CD
#else
         START WITH OFC_CD = B.CTRT_OFC_CD
#end
         CONNECT BY PRIOR PRNT_OFC_CD = OFC_CD) BKG_RHQ_CD,
        B.BKG_NO,
#if(${ofc_tp_cd} == 'B')
        B.BKG_OFC_CD,
#else
        B.CTRT_OFC_CD BKG_OFC_CD,
#end
        B.SVC_SCP_CD,
        B.POL_CD,
        R.BKG_CTRT_TP_CD,
        B.RFA_NO, (SELECT PROP_NO FROM PRI_RP_HDR WHERE RFA_NO = B.RFA_NO) RFA_PROP_NO,
        B.SC_NO, (SELECT PROP_NO FROM PRI_SP_HDR WHERE SC_NO = B.SC_NO) SC_PROP_NO,
        B.TAA_NO, (SELECT PROP_NO FROM PRI_TAA_HDR WHERE TAA_NO = B.TAA_NO) TAA_PROP_NO, 
        T.TRF_ITM_NO,
        R.RT_APLY_DT,
        NVL(T.PROP_NO,  (SELECT PROP_NO FROM BKG_AUTO_RT_HIS WHERE BKG_NO = B.BKG_NO AND CHG_CD = 'OFT' AND ROWNUM =1)) PROP_NO,
        NVL(T.AMDT_SEQ, (SELECT AMDT_SEQ FROM BKG_AUTO_RT_HIS WHERE BKG_NO = B.BKG_NO AND CHG_CD = 'OFT'AND ROWNUM =1)) AMDT_SEQ,
        T.GEN_SPCL_RT_TP_CD,
        NVL(T.CMDT_HDR_SEQ,R.PRC_CMDT_HDR_SEQ) CMDT_HDR_SEQ,
        NVL(T.ROUT_SEQ,R.PRC_ROUT_SEQ) ROUT_SEQ
FROM BKG_BOOKING B, BKG_RATE R, 
#if (${date_type} == 'L')
     VSK_VSL_PORT_SKD VSK, BKG_VVD VVD,
#end
     BKG_CHG_RT T,
     (SELECT  OFC_CD
      FROM  MDM_ORGANIZATION A
#if (${bkg_ofc_cd} != '')
      WHERE OFC_CD = @[bkg_ofc_cd]
#end
#if (${bkg_rhq_cd} != '')
      START WITH  A.OFC_CD    = @[bkg_rhq_cd]
#else
      START WITH  A.OFC_CD    IN ( SELECT OFC_CD FROM MDM_ORGANIZATION WHERE OFC_TP_CD = 'HQ' )
#end
      CONNECT BY  PRIOR A.OFC_CD  = A.PRNT_OFC_CD) O
WHERE 1=1 
#if (${date_type} == 'L')
AND VSK.VSL_CD = VVD.VSL_CD
AND VSK.SKD_VOY_NO = VVD.SKD_VOY_NO
AND VSK.SKD_DIR_cD = VVD.SKD_DIR_CD
AND VSK.VPS_PORT_CD = VVD.POL_CD
AND VSK.CLPT_IND_SEQ = VVD.POL_CLPT_IND_SEQ
AND VVD.VSL_PRE_PST_CD||VVD.VSL_SEQ = ( SELECT MIN(VSL_PRE_PST_CD||VSL_SEQ) FROM BKG_VVD VVD2 WHERE VVD2.BKG_NO = B.BKG_NO)
AND VSK.VPS_ETD_DT >= TO_DATE(@[fm_dt], 'YYYY-MM-DD')  
AND VSK.VPS_ETD_DT <= TO_DATE (@[to_dt] , 'YYYY-MM-DD') + 0.99999
AND VVD.BKG_NO = B.BKG_NO
#else
AND R.RT_APLY_DT BETWEEN TO_DATE(@[fm_dt],'YYYY-MM-DD') AND TO_DATE(@[to_dt],'YYYY-MM-DD') + 0.99999
#end
AND B.BKG_NO = R.BKG_NO
AND B.BKG_NO = T.BKG_NO
AND T.CHG_CD = 'OFT'
AND B.BKG_STS_CD = 'F'
AND B.BKG_CGO_TP_CD = 'F'
#if(${ofc_tp_cd} == 'B')
AND B.BKG_OFC_CD = O.OFC_CD
#else
AND B.CTRT_OFC_CD = O.OFC_CD
#end
),
RT AS(
SELECT  DISTINCT BK.BKG_RHQ_CD,
        BK.BKG_OFC_CD,
        BK.BKG_NO,
        BK.RFA_NO||BK.AMDT_SEQ CTRT_NO,
        TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT DAY_DIFF,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 1 AND 7 THEN BK.BKG_NO
             ELSE NULL
        END N1WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 1 AND 7 THEN BK.RFA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N1WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 8 AND 14 THEN BK.BKG_NO
             ELSE NULL
        END N2WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 8 AND 14 THEN BK.RFA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N2WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 15 AND 21 THEN BK.BKG_NO
             ELSE NULL
        END N3WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 15 AND 21 THEN BK.RFA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N3WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 22 AND 28 THEN BK.BKG_NO
             ELSE NULL
        END N4WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 22 AND 28 THEN BK.RFA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N4WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 29 AND 35 THEN BK.BKG_NO
             ELSE NULL
        END N5WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 29 AND 35 THEN BK.RFA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N5WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT >= 36 THEN BK.BKG_NO
             ELSE NULL
        END N6WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) - BK.RT_APLY_DT >= 36 THEN BK.RFA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N6WK_CTRT_NO
FROM BK, PRI_RP_MN RM, PRI_RP_SCP_MN RS, pri_rp_scp_rt PR
WHERE BK.BKG_CTRT_TP_CD = 'R'
#if (${bkg_ctrt_tp_cd} != '')
AND @[bkg_ctrt_tp_cd] = 'R'
#end 
AND BK.RFA_PROP_NO = RM.PROP_NO
AND BK.AMDT_SEQ = RM.AMDT_SEQ
AND RM.PROP_STS_CD = 'A'
AND RM.PROP_NO = RS.PROP_NO
AND RM.AMDT_SEQ = RS.AMDT_SEQ
AND RS.SVC_SCP_CD = BK.SVC_SCP_CD
AND BK.PROP_NO = PR.PROP_NO
AND BK.AMDT_SEQ = PR.AMDT_SEQ
AND BK.SVC_SCP_CD = PR.SVC_SCP_CD
AND BK.CMDT_HDR_SEQ = PR.CMDT_HDR_SEQ
AND BK.ROUT_SEQ = PR.ROUT_SEQ
AND TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',PR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(PR.ACPT_OFC_CD))) > BK.RT_APLY_DT
AND PR.RAT_UT_CD IN (SELECT CNTR_TPSZ_CD FROM BKG_QUANTITY WHERE BKG_NO = BK.BKG_NO)

UNION ALL

SELECT  DISTINCT BK.BKG_RHQ_CD,
        BK.BKG_OFC_CD,
        BK.BKG_NO,
        BK.SC_NO||BK.AMDT_SEQ CTRT_NO,
        TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT DAY_DIFF,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 1 AND 7 THEN BK.BKG_NO
             ELSE NULL
        END N1WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 1 AND 7 THEN BK.SC_NO||BK.AMDT_SEQ
             ELSE NULL
        END N1WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 8 AND 14 THEN BK.BKG_NO
             ELSE NULL
        END N2WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 8 AND 14 THEN BK.SC_NO||BK.AMDT_SEQ
             ELSE NULL
        END N2WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 15 AND 21 THEN BK.BKG_NO
             ELSE NULL
        END N3WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 15 AND 21 THEN BK.SC_NO||BK.AMDT_SEQ
             ELSE NULL
        END N3WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 22 AND 28 THEN BK.BKG_NO
             ELSE NULL
        END N4WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 22 AND 28 THEN BK.SC_NO||BK.AMDT_SEQ
             ELSE NULL
        END N4WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 29 AND 35 THEN BK.BKG_NO
             ELSE NULL
        END N5WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT BETWEEN 29 AND 35 THEN BK.SC_NO||BK.AMDT_SEQ
             ELSE NULL
        END N5WK_CTRT_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT >= 36 THEN BK.BKG_NO
             ELSE NULL
        END N6WK_BKG_NO,
        CASE WHEN TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) - BK.RT_APLY_DT >= 36 THEN BK.SC_NO||BK.AMDT_SEQ
             ELSE NULL
        END N6WK_CTRT_NO
FROM BK, PRI_SP_MN SM, PRI_SP_SCP_MN SS, PRI_SP_SCP_RT SR
WHERE BK.BKG_CTRT_TP_CD = 'S'
#if (${bkg_ctrt_tp_cd} != '')
AND @[bkg_ctrt_tp_cd] = 'S'
#end 
AND BK.SC_PROP_NO = SM.PROP_NO
AND BK.AMDT_SEQ = SM.AMDT_SEQ
AND SM.PROP_STS_CD = 'F'
AND SM.PROP_NO = SS.PROP_NO
AND SM.AMDT_SEQ = SS.AMDT_SEQ
AND SS.SVC_SCP_CD = BK.SVC_SCP_CD
AND BK.PROP_NO = SR.PROP_NO
AND BK.AMDT_SEQ = SR.AMDT_SEQ
AND BK.SVC_SCP_CD = SR.SVC_SCP_CD
AND BK.GEN_SPCL_RT_TP_CD = SR.GEN_SPCL_RT_TP_CD
AND BK.CMDT_HDR_SEQ = SR.CMDT_HDR_SEQ
AND BK.ROUT_SEQ = SR.ROUT_SEQ
AND TRUNC(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',SR.ACPT_DT,GLOBALDATE_PKG.GET_LOCCD_FNC(SR.ACPT_OFC_CD))) > BK.RT_APLY_DT
AND SR.RAT_UT_CD IN (SELECT CNTR_TPSZ_CD FROM BKG_QUANTITY WHERE BKG_NO = BK.BKG_NO)

UNION ALL

SELECT  DISTINCT BK.BKG_RHQ_CD, 
        BK.BKG_OFC_CD,
        BK.BKG_NO,
        BK.TAA_NO||BK.AMDT_SEQ CTRT_NO,
        TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT DAY_DIFF,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 1 AND 7 THEN BK.BKG_NO
             ELSE NULL
        END N1WK_BKG_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 1 AND 7 THEN BK.TAA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N1WK_CTRT_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 8 AND 14 THEN BK.BKG_NO
             ELSE NULL
        END N2WK_BKG_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 8 AND 14 THEN BK.TAA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N2WK_CTRT_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 15 AND 21 THEN BK.BKG_NO
             ELSE NULL
        END N3WK_BKG_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 15 AND 21 THEN BK.TAA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N3WK_CTRT_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 22 AND 28 THEN BK.BKG_NO
             ELSE NULL
        END N4WK_BKG_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 22 AND 28 THEN BK.TAA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N4WK_CTRT_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 29 AND 35 THEN BK.BKG_NO
             ELSE NULL
        END N5WK_BKG_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT BETWEEN 29 AND 35 THEN BK.TAA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N5WK_CTRT_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT >= 36 THEN BK.BKG_NO
             ELSE NULL
        END N6WK_BKG_NO,
        CASE WHEN TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) - BK.RT_APLY_DT >= 36 THEN BK.TAA_NO||BK.AMDT_SEQ
             ELSE NULL
        END N6WK_CTRT_NO
FROM BK, PRI_TAA_MN TM, PRI_TAA_TRI_LIST TL, PRI_TRI_MN TT, PRI_TRI_RT TR
WHERE BK.BKG_CTRT_TP_CD = 'T'
#if (${bkg_ctrt_tp_cd} != '')
AND @[bkg_ctrt_tp_cd] = 'T'
#end 
AND BK.TAA_PROP_NO = TM.TAA_PROP_NO
AND BK.AMDT_SEQ = TM.AMDT_SEQ
AND TM.CFM_FLG = 'Y'
AND TM.TAA_PROP_NO = TL.TAA_PROP_NO
AND TM.AMDT_SEQ = TL.AMDT_SEQ
AND TL.TRI_PROP_NO = TT.TRI_PROP_NO
AND TT.TRI_NO = BK.TRF_ITM_NO
AND TL.TRI_PROP_NO = TR.TRI_PROP_NO 
AND TR.PROP_STS_CD = 'F'
AND TRUNC(NVL(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',TM.CFM_DT,BK.POL_CD),TM.UPD_DT)) > BK.RT_APLY_DT
AND TR.RAT_UT_CD IN (SELECT CNTR_TPSZ_CD FROM BKG_QUANTITY WHERE BKG_NO = BK.BKG_NO)
)
SELECT BKG_RHQ_CD,
       BKG_OFC_CD,
       COUNT(DISTINCT N1WK_CTRT_NO) N1WK_CTRT_CNT,
       COUNT(DISTINCT N1WK_BKG_NO) N1WK_BKG_CNT,
       COUNT(DISTINCT N2WK_CTRT_NO) N2WK_CTRT_CNT,
       COUNT(DISTINCT N2WK_BKG_NO) N2WK_BKG_CNT,
       COUNT(DISTINCT N3WK_CTRT_NO) N3WK_CTRT_CNT,
       COUNT(DISTINCT N3WK_BKG_NO) N3WK_BKG_CNT,
       COUNT(DISTINCT N4WK_CTRT_NO) N4WK_CTRT_CNT,
       COUNT(DISTINCT N4WK_BKG_NO) N4WK_BKG_CNT,
       COUNT(DISTINCT N5WK_CTRT_NO) N5WK_CTRT_CNT,
       COUNT(DISTINCT N5WK_BKG_NO) N5WK_BKG_CNT,
       COUNT(DISTINCT N6WK_CTRT_NO) N6WK_CTRT_CNT,
       COUNT(DISTINCT N6WK_BKG_NO) N6WK_BKG_CNT,
       COUNT(DISTINCT BKG_NO) TTL_BKG_CNT,
       COUNT(DISTINCT CTRT_NO) TTL_CTRT_CNT
FROM RT
GROUP BY BKG_RHQ_CD, BKG_OFC_CD			]]></sql>
			<params>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_rhq_cd" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="bkg_ctrt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
