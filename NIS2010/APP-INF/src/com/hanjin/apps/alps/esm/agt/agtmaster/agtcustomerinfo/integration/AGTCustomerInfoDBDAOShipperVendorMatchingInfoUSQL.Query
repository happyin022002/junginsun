		<query name="AGTCustomerInfoDBDAOShipperVendorMatchingInfoUSQL">
			<desc><![CDATA[ESM_AGT_025 화면 수정]]></desc>
			<sql><![CDATA[
UPDATE AGT_CMPN_CUST_MTCH 
SET CUST_CNT_CD = @[cust_cnt_cd],
    CUST_SEQ    = @[cust_seq],
    VNDR_CNT_CD = @[vndr_cnt_cd],
    VNDR_SEQ    = @[vndr_seq],
    CUST_NM     = @[cust_nm],
    VNDR_NM     = @[vndr_nm],
    UPD_USR_ID  = @[upd_usr_id],
    UPD_DT      = SYSDATE
WHERE CUST_CNT_CD = @[cust_cnt_cd]
  AND CUST_SEQ    = @[cust_seq]
  AND VNDR_CNT_CD = @[vndr_cnt_cd2]
  AND VNDR_SEQ    = @[vndr_seq2]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="Y"/>
				<param name="cust_seq" type="12" value="" out="Y"/>
				<param name="vndr_cnt_cd" type="12" value="" out="Y"/>
				<param name="vndr_seq" type="12" value="" out="Y"/>
				<param name="cust_nm" type="12" value="" out="Y"/>
				<param name="vndr_nm" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="vndr_cnt_cd2" type="12" value="" out="Y"/>
				<param name="vndr_seq2" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
