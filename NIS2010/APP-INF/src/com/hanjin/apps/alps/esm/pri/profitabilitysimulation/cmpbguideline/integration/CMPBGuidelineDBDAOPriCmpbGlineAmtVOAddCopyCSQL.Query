<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMPBGuidelineDBDAOPriCmpbGlineAmtVOAddCopyCSQL">
			<desc><![CDATA[amt copy]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_CMPB_GLINE_AMT (
	SVC_SCP_CD
,	CRE_OFC_CD
,	GLINE_SEQ
,	PRS_CUST_TP_CD
,	BSE_SEQ
,	CMPB_SEQ
,	MQC_RNG_FM_QTY
,	MQC_RNG_TO_QTY
,	RAT_UT_CD
,	PRC_CGO_TP_CD
,	CURR_CD
,	CMPB_AMT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT
	@[svc_scp_cd]
,	@[cre_ofc_cd]
,	@[max_gline_seq]
,	@[prs_cust_tp_cd]
,	BSE_SEQ
,	CMPB_SEQ
,	MQC_RNG_FM_QTY
,	MQC_RNG_TO_QTY
,	RAT_UT_CD
,	PRC_CGO_TP_CD
,	CURR_CD
,	CMPB_AMT
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
FROM	PRI_CMPB_GLINE_AMT
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	CRE_OFC_CD = @[cre_ofc_cd_hidden]
AND	GLINE_SEQ = @[gline_seq]
AND	PRS_CUST_TP_CD = @[prs_cust_tp_cd_before]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="max_gline_seq" type="12" value="" out="N"/>
				<param name="prs_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd_hidden" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="prs_cust_tp_cd_before" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
