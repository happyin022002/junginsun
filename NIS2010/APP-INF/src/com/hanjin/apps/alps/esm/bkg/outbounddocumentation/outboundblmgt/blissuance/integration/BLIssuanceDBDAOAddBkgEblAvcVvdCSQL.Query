<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOAddBkgEblAvcVvdCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_EBL_AVC_VVD
	(BKG_NO
	,BL_NO
	,BKG_EBL_SEQ  
	,CNTR_SEQ
	,DOC_PARA_NO1
	,DOC_PARA_NO2
	,SKD_VOY_NO
	,SHP_CTNT1
	,SHP_CTNT2
	,SHP_CTNT3
	,IF_FLG
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT)
SELECT	B.BKG_NO BKG_NO
	,B.BL_NO BL_NO     
	,@[bkg_ebl_seq] BKG_EBL_SEQ
	,ROWNUM CNTR_SEQ
	,B.BKG_NO||LTRIM(TO_CHAR(@[bkg_ebl_seq], '0000')) DOC_PARA_NO1
	,B.BKG_NO||LTRIM(TO_CHAR(@[bkg_ebl_seq], '0000'))||LTRIM(TO_CHAR(ROWNUM, '0000')) DOC_PARA_NO2
	,'' SKD_VOY_NO
	,'' SHP_CTNT1 
	,'' SHP_CTNT2 
	,'' SHP_CTNT3 
	,'N' IF_FLG    
	,@[usr_id] CRE_USR_ID
	,SYSDATE CRE_DT    
	,@[usr_id] UPD_USR_ID
	,SYSDATE UPD_DT
  FROM	BKG_BOOKING B   
 WHERE  B.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_ebl_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
