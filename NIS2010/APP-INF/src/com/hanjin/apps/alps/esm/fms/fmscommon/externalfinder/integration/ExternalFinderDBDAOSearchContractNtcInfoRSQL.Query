<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDBDAOSearchContractNtcInfoRSQL">
			<desc><![CDATA[계약번호가 정상적인지 확인 (noitce에서 사용)]]></desc>
			<sql><![CDATA[
SELECT T.FLET_CTRT_NO  AGMT_NO
  FROM FMS_CONTRACT T
 WHERE 1=1 
   AND T.FLET_CTRT_NO = @[agmt_no]			]]></sql>
			<params>
				<param name="agmt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
