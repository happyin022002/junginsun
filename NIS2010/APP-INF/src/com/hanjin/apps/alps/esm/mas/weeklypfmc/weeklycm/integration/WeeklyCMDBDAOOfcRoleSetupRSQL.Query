<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOOfcRoleSetupRSQL">
			<desc><![CDATA[Office Role Setup 을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT OFC_CD 
      ,DELT_FLG 
      ,CRE_USR_ID AS CRE_USR_ID_SHEET
      ,TO_CHAR(CRE_DT, 'YYYY-MM-DD') AS CRE_DT 
      ,UPD_USR_ID AS UPD_USR_ID_SHEET
      ,TO_CHAR(UPD_DT, 'YYYY-MM-DD') AS UPD_DT
      ,OFC_CD AS OFC_CD_ORG
      ,CRE_USR_ID AS CRE_USR_ID_ORG
      ,UPD_USR_ID AS UPD_USR_ID_ORG
FROM MAS_OP_EXPT_OFC

#if (${f_ofc_cd} != '')
    WHERE OFC_CD LIKE @[f_ofc_cd]||'%'

#elseif (${f_ofc_cd} == '')

#end

ORDER BY OFC_CD			]]></sql>
			<params>
				<param name="f_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
