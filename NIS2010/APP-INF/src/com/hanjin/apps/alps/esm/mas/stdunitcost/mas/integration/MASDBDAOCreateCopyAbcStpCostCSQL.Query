<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MASDBDAOCreateCopyAbcStpCostCSQL">
			<desc><![CDATA[2012.08.27 이석준[CHM-201219844-01]   ABC(PA)/STP Cost(RA)화면에 Month Copy 기능 추가]]></desc>
			<sql><![CDATA[
#if(${table_name} =='MAS_SVC_TRNS_PRC')
INSERT INTO MAS_SVC_TRNS_PRC
(COST_YRMON,OFC_CD,OFC_ACT_CD,SVC_TRNS_PRC_CNT_CD,OFC_ACT_NM,DIV_MEAS_CD,
DIV_MEAS_NM,ACT_OFC_TTL_QTY,ACT_OFC_TTL_AMT,ACT_COST_UT_AMT,CNT_AVG_UC_AMT,
STP_MGN_RTO,SVC_TRNS_PRC_UT_AMT,SVC_TRNS_PRC_FLG,CRE_USR_ID,CRE_DT,UPD_USR_ID,UPD_DT)
SELECT 
    @[f_tar_mon] AS COST_YRMON,
    OFC_CD,
    OFC_ACT_CD,
    SVC_TRNS_PRC_CNT_CD,
    OFC_ACT_NM,
    DIV_MEAS_CD,
    DIV_MEAS_NM,
    ACT_OFC_TTL_QTY,
    ACT_OFC_TTL_AMT,
    ACT_COST_UT_AMT,
    CNT_AVG_UC_AMT,
    STP_MGN_RTO,
    SVC_TRNS_PRC_UT_AMT,
    SVC_TRNS_PRC_FLG,
    @[user_id],
    SYSDATE,
    @[user_id],
    SYSDATE
FROM  MAS_SVC_TRNS_PRC
WHERE COST_YRMON = @[f_src_mon]
#elseif (${table_name} =='MAS_SVC_TRNS_OFC_MAPG')
INSERT INTO MAS_SVC_TRNS_OFC_MAPG
(COST_YRMON,OFC_CD,OFC_ACT_CD,DIV_MEAS_CD,
 PFITCTR_FLG,SVC_TRNS_PRC_FLG,CRE_USR_ID,CRE_DT,UPD_USR_ID,UPD_DT)
SELECT 
    @[f_tar_mon] AS COST_YRMON,
    OFC_CD,
    OFC_ACT_CD,
    DIV_MEAS_CD,
    PFITCTR_FLG,
    SVC_TRNS_PRC_FLG,
    @[user_id],
    SYSDATE,
    @[user_id],
    SYSDATE
FROM  MAS_SVC_TRNS_OFC_MAPG
WHERE COST_YRMON = @[f_src_mon]
#elseif (${table_name} =='MAS_OFC_ROUT_MAPG')
INSERT INTO MAS_OFC_ROUT_MAPG
(COST_YRMON,OFC_CLSS_CD,SLS_ACT_CD,DIV_MEAS_CD,SLS_OFC_CLSS_NM,
 SLS_ACT_DESC,DIV_MEAS_NM,CRE_USR_ID,CRE_DT,UPD_USR_ID,UPD_DT)
SELECT 
    @[f_tar_mon] AS COST_YRMON,
    OFC_CLSS_CD,
    SLS_ACT_CD,
    DIV_MEAS_CD,
    SLS_OFC_CLSS_NM,
    SLS_ACT_DESC,
    DIV_MEAS_NM,
    @[user_id],
    SYSDATE,
    @[user_id],
    SYSDATE
FROM  MAS_OFC_ROUT_MAPG
WHERE COST_YRMON = @[f_src_mon]

#elseif (${table_name} =='MAS_SVC_TRNS_PRC_MGN')
INSERT INTO MAS_SVC_TRNS_PRC_MGN
(COST_YRMON,OFC_ACT_CD,STP_MGN_RTO,CRE_USR_ID,CRE_DT,UPD_USR_ID,UPD_DT)
SELECT 
    @[f_tar_mon] AS COST_YRMON,
    OFC_ACT_CD,
    STP_MGN_RTO,
    @[user_id],
    SYSDATE,
    @[user_id],
    SYSDATE
FROM  MAS_SVC_TRNS_PRC_MGN
WHERE COST_YRMON = @[f_src_mon]
#end			]]></sql>
			<params>
				<param name="f_tar_mon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_src_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
