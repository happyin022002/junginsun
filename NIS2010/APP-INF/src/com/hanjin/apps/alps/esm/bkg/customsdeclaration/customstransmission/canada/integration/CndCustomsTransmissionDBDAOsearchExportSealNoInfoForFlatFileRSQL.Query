<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndCustomsTransmissionDBDAOsearchExportSealNoInfoForFlatFileRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
#if (${mbl_no} == '') 
	SELECT CNTR_SEAL_NO AS SEAL_NO
	  FROM  BKG_CNTR_SEAL_NO
	 WHERE  BKG_NO = @[bkg_no]
       AND  CNTR_NO	= @[cntr_no]
       AND TRIM(CNTR_SEAL_NO) != '-'  /*제외*/
#else 
	SELECT DISTINCT S.CNTR_SEAL_NO AS SEAL_NO
	  FROM  BKG_CNTR_SEAL_NO S
	       ,BKG_CNTR_MF_DESC D
	 WHERE  S.BKG_NO = @[bkg_no]
	   AND  S.BKG_NO = D.BKG_NO
	   AND  S.CNTR_NO = D.CNTR_NO
	   AND  D.CNTR_MF_NO = @[bl_no]
       AND  S.CNTR_NO	= @[cntr_no]
       AND TRIM(S.CNTR_SEAL_NO) != '-'  /*제외*/
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
