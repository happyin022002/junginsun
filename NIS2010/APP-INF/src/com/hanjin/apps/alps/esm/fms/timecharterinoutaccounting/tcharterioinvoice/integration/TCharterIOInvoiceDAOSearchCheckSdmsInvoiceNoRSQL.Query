<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDAOSearchCheckSdmsInvoiceNoRSQL">
			<desc><![CDATA[TCharterIOInvoiceDAOSearchCheckSdmsInvoiceNoRSQL]]></desc>
			<sql><![CDATA[
SELECT TO_INV_NO 
  FROM FMS_INV_DTL
 WHERE FLET_ISS_TP_CD = 'CHT'
   AND TO_INV_NO = @[to_inv_no]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="to_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
