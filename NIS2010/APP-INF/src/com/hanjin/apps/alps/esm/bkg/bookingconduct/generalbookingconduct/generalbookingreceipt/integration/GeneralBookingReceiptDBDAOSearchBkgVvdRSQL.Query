,POL_YD_CD 
,POD_CD 
,POD_YD_CD 
,BKG_TRSP_MZD_CD 
,CNTR_LODG_FLG 
,REV_VVD_FLG 
,CRE_USR_ID 
,CRE_DT 
,UPD_USR_ID 
,UPD_DT 
,POD_CLPT_IND_SEQ
