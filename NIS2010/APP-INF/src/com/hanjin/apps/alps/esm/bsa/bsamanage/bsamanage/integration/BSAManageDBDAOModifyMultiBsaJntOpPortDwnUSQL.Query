<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOModifyMultiBsaJntOpPortDwnUSQL">
			<desc><![CDATA[ESM_BSA_0026 화면의 BsaJntOpPortDwn UPDATE]]></desc>
			<sql><![CDATA[
UPDATE BSA_JNT_OP_PORT_DWN 
SET PORT_CD        = @[port_cd]
   ,PORT_BSA_CAPA  = @[port_bsa_capa]
   ,UPD_USR_ID     = @[upd_usr_id]
   ,UPD_DT         = SYSDATE
WHERE BSA_SEQ      = @[bsa_seq]
  AND TRD_CD       = @[trd_cd]
  AND RLANE_CD     = @[rlane_cd]
  AND DIR_CD       = @[dir_cd]
  AND VOP_CD       = @[vop_cd]
  AND VSL_CAPA     = @[vsl_capa]
  AND BSA_OP_CD    = @[bsa_op_cd]
  AND BSA_OP_JB_CD = @[bsa_op_jb_cd]
  AND CRR_CD       = @[crr_cd]
  AND PORT_SEQ     = @[port_seq]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="port_bsa_capa" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bsa_seq" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vop_cd" type="12" value="" out="N"/>
				<param name="vsl_capa" type="12" value="" out="N"/>
				<param name="bsa_op_cd" type="12" value="" out="N"/>
				<param name="bsa_op_jb_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="port_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
