<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOAutoRatingRFACheckRSQL">
			<desc><![CDATA[AutoRatingRFACheck]]></desc>
			<sql><![CDATA[
SELECT NVL(
	(SELECT 'Y' 
		FROM BKG_BOOKING A 
		WHERE BKG_NO = @[input_text] 
			AND A.RFA_NO IS NOT NULL 
			AND BKG_CRE_DT < TO_DATE('20100401','YYYYMMDD')
	),'N') as OUTPUT_TEXT 
FROM DUAL
			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
