<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingProcessMgtDBDAOSearchCgoClzTmRSQL">
			<desc><![CDATA[cargo closing time setup SELECT]]></desc>
			<sql><![CDATA[
SELECT 	 POL_CD
		,LANE_CD
		,DIR_CD
		,CCT_HRS
		,EML_SND_FLG
		,MBL_SND_FLG
		,SHPR_NTC_FLG
		,BKG_PIC_NTC_FLG
		,SREP_NTC_FLG
		,OB_PIC_NTC_FLG
		,CNTC_EML
		,CNTC_MPHN_NO
		,CTRT_OFC_PHN_NO
		,AUTO_NTC_FLG
		,CRE_USR_ID
		,CRE_DT
		,UPD_USR_ID
		,UPD_DT
FROM 	BKG_CGO_CLZ_TM_STUP
WHERE	POL_CD	LIKE '%'||@[pol_cd]||'%'
AND		LANE_CD	LIKE '%'||@[lane_cd]||'%'			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
