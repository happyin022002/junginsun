<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOsearchFrontOfficeFlagRSQL">
			<desc><![CDATA[해당 BKG이 Front Office Type으로 접수된 적이 있는지 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(BL_FNT_OFC_FLG,'N') BL_FNT_OFC_FLG
  FROM BKG_SR_CRNT_RQST R
 WHERE R.BKG_NO = @[bkg_no]
   AND R.SR_AMD_SEQ = (SELECT MAX(SR_AMD_SEQ)
                         FROM BKG_SR_CRNT_RQST
                        WHERE BKG_NO = R.BKG_NO)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
