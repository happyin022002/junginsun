<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOCaIssueDateOutVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
#if (${cnt_cd} == 'CA' || ${cnt_cd} == 'US')
SELECT DISTINCT 
 D.BKG_NO BKG_NO
,Decode(substr(A.POD_CD,1,2),'US',USA_CSTMS_FILE_CD,'CA',CND_CSTMS_FILE_CD,' ') USA_CSTMS_FILE_CD
,A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD VVD
,B.BKG_NO
,B.BL_NO
,B.BL_TP_CD
,A.POL_CD
,A.POD_CD
,D.CORR_NO
,TO_CHAR(D.CORR_DT,'YYYY-MM-DD HH24:MI:SS') CORR_DT
,D.CORR_OFC_CD
,D.CA_RSN_CD 
,NVL((SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01632' AND INTG_CD_VAL_CTNT = D.CA_RSN_CD), D.CA_RSN_CD) CA_RSN_NM
,Decode(RAT_FLG,'N',EXPN_FLG) RAT_FLG
,NVL(RT_CORR_FLG,'N') 		AS KIND_A
,NVL(CHG_TERM_CORR_FLG,'N')	AS KIND_B
,NVL(RCVDE_TERM_CORR_FLG,'N') AS KIND_C
,NVL(ROUT_CORR_FLG,'N') 		AS KIND_D
,NVL(CUST_CORR_FLG,'N') 		AS KIND_E
,NVL(QTY_CORR_FLG,'N')		AS KIND_F
,NVL(MEAS_QTY_CORR_FLG,'N') 	AS KIND_G
,NVL(CMDT_CORR_FLG,'N') 		AS KIND_H
,NVL(TRNK_VSL_CORR_FLG,'N')  	AS KIND_I
,NVL(PRPST_VSL_CORR_FLG,'N') 	AS KIND_J
,NVL(CA_OTR_RSN_CORR_FLG,'N') AS KIND_K
,REPLACE(D.BKG_CORR_RMK, CHR(13) || CHR(10), ' ') BKG_CORR_RMK
,TO_CHAR(SKD.VPS_ETA_DT,'YYYY-MM-DD HH24:MI:SS') AS VPS_ETA_DT

   FROM BKG_CORRECTION D, BKG_BOOKING B, BKG_CSTMS_ADV_BL C, BKG_VVD A, VSK_VSL_PORT_SKD SKD    
  WHERE 1=1
#if (${from_dt} != '' && ${pk_tp} == 'date')
	AND D.CORR_DT >= TO_DATE(@[from_dt],'YYYY-MM-DD HH24:MI')      
    AND D.CORR_DT <= TO_DATE(@[to_dt],'YYYY-MM-DD HH24:MI')      
#end
    AND D.BKG_NO         = B.BKG_NO    
    AND (B.BKG_STS_CD       = 'F'    OR   B.BKG_STS_CD       = 'W'  )    
    AND B.BL_NO           > ' '    
    AND B.BKG_NO        = A.BKG_NO    
    AND B.BL_NO         = C.BL_NO    
    AND (D.CORR_NO <>'0000000001' AND D.CORR_NO <> 'TMP0000001')

    AND C.CNT_CD = DECODE(@[cnt_cd],'US','US','CA','CA',C.CNT_CD)
    #if (${vvd} != '' && ${pk_tp} == 'vvd')
       AND A.VSL_CD = SUBSTR(@[vvd], 0, 4)
       AND A.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
       AND A.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
    #end    
    #if (${pod_cd} != '')
	   AND A.POD_CD LIKE @[pod_cd] || '%'
	#end    
    #if (${del_cd} != '' && ${pk_tp} == 'vvd')
       AND B.DEL_CD LIKE @[del_cd] || '%'
    #end    
    #if (${bl_no} != '' && ${pk_tp} == 'bl')
       AND B.BL_NO = @[bl_no]
    #end     
    
    AND D.CORR_DT > (CASE WHEN C.CA_NO IS NOT NULL
                          THEN    
                              (SELECT F.CORR_DT    
                                FROM   BKG_CORRECTION F    
                                WHERE  F.BKG_NO = B.BKG_NO AND F.CORR_NO = C.CA_NO    
                                #if (${from_dt} != '' && ${pk_tp} == 'date')
                                   AND D.CORR_DT BETWEEN TO_DATE(@[from_dt],'YYYY-MM-DD HH24:MI') AND TO_DATE(@[to_dt],'YYYY-MM-DD HH24:MI')
                                #end                            
                                AND CORR_NO    <>  '0000000001'
                               ) 
                           ELSE    
                                TO_DATE('19000101', 'yyyymmdd') 
                          END)     
      
    AND (               D.ROUT_CORR_FLG        = 'Y' OR D.CUST_CORR_FLG       = 'Y' OR    
                        D.QTY_CORR_FLG         = 'Y' OR D.MEAS_QTY_CORR_FLG   = 'Y' OR    
                        D.CMDT_CORR_FLG        = 'Y' OR D.TRNK_VSL_CORR_FLG   = 'Y' OR    
                        D.PRPST_VSL_CORR_FLG   = 'Y' OR D.CA_OTR_RSN_CORR_FLG = 'Y' )
    AND SKD.VSL_CD = A.VSL_CD
	AND SKD.SKD_VOY_NO = A.SKD_VOY_NO
	AND SKD.SKD_DIR_CD = A.SKD_DIR_CD
	AND SKD.VPS_PORT_CD = A.POD_CD
	AND SKD.CLPT_IND_SEQ = A.POD_CLPT_IND_SEQ

    ORDER BY VVD                        


#else
SELECT BKG.BKG_NO
      ,BKG.USA_CSTMS_FILE_CD
      ,VVD.VSL_CD || VVD.SKD_VOY_NO || VVD.SKD_DIR_CD VVD
      ,BKG.BL_NO
      ,BKG.POL_CD
      ,BKG.POD_CD
      ,COR.CORR_NO
      ,TO_CHAR(COR.CORR_DT,'YYYY-MM-DD HH:MI:SS') CORR_DT
      ,COR.CORR_OFC_CD
      ,COR.CA_RSN_CD
      ,NVL((SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01632' AND INTG_CD_VAL_CTNT = COR.CA_RSN_CD), COR.CA_RSN_CD) CA_RSN_NM
      ,NVL(COR.RAT_FLG, COR.EXPN_FLG) RAT_FLG
      ,NVL(COR.RT_CORR_FLG,'N')              AS KIND_A
      ,NVL(COR.CHG_TERM_CORR_FLG,'N')	     AS KIND_B
      ,NVL(COR.RCVDE_TERM_CORR_FLG,'N')      AS KIND_C
      ,NVL(COR.ROUT_CORR_FLG,'N')            AS KIND_D
      ,NVL(COR.CUST_CORR_FLG,'N')            AS KIND_E
      ,NVL(COR.QTY_CORR_FLG,'N')             AS KIND_F
      ,NVL(COR.MEAS_QTY_CORR_FLG,'N')        AS KIND_G
      ,NVL(COR.CMDT_CORR_FLG,'N')            AS KIND_H
      ,NVL(COR.TRNK_VSL_CORR_FLG,'N')        AS KIND_I
      ,NVL(COR.PRPST_VSL_CORR_FLG,'N')       AS KIND_J
      ,NVL(COR.CA_OTR_RSN_CORR_FLG,'N')      AS KIND_K
      ,REPLACE(COR.BKG_CORR_RMK, CHR(13) || CHR(10), ' ') BKG_CORR_RMK
	  ,TO_CHAR(SKD.VPS_ETA_DT,'YYYY-MM-DD HH24:MI:SS') AS VPS_ETA_DT
  FROM BKG_VVD VVD
      ,BKG_BOOKING      BKG
      ,BKG_CORRECTION   COR
	  ,VSK_VSL_PORT_SKD SKD 

 WHERE VVD.BKG_NO = BKG.BKG_NO
   AND BKG.BKG_NO = COR.BKG_NO
   AND (COR.CORR_NO <>'0000000001' AND COR.CORR_NO <> 'TMP0000001')
   AND COR.CORR_CXL_FLG = 'N'
   AND SKD.VSL_CD = VVD.VSL_CD
   AND SKD.SKD_VOY_NO = VVD.SKD_VOY_NO
   AND SKD.SKD_DIR_CD = VVD.SKD_DIR_CD
   AND SKD.VPS_PORT_CD = VVD.POD_CD
   AND SKD.CLPT_IND_SEQ = VVD.POD_CLPT_IND_SEQ
/*
    AND (               COR.ROUT_CORR_FLG        = 'Y' OR COR.CUST_CORR_FLG       = 'Y' OR    
                        COR.QTY_CORR_FLG         = 'Y' OR COR.MEAS_QTY_CORR_FLG   = 'Y' OR    
                        COR.CMDT_CORR_FLG        = 'Y' OR COR.TRNK_VSL_CORR_FLG   = 'Y' OR    
                        COR.PRPST_VSL_CORR_FLG   = 'Y' OR COR.CA_OTR_RSN_CORR_FLG = 'Y' )
*/
#if (${vvd} != '' && ${pk_tp} == 'vvd')
           AND VVD.VSL_CD = SUBSTR(@[vvd], 0, 4)
           AND VVD.SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
           AND VVD.SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
#end

#if (${from_dt} != '' && ${pk_tp} == 'date')
           AND COR.CORR_DT BETWEEN TO_DATE(@[from_dt],'YYYY-MM-DD HH24:MI') AND TO_DATE(@[to_dt],'YYYY-MM-DD HH24:MI')
#end       
   
#if (${pod_cd} != '')
	#if (${pod_tp} == 'VVD') 
			AND VVD.POD_CD LIKE @[pod_cd] || '%'
					
	#else 
			AND BKG.POD_CD LIKE @[pod_cd] || '%'	
			AND BKG.POD_CD = VVD.POD_CD
	#end
#end  
#if (${del_cd} != '' && ${pk_tp} == 'vvd')
   AND BKG.DEL_CD LIKE @[del_cd] || '%'
#end
#if (${bl_no} != '' && ${pk_tp} == 'bl')
   AND BKG.BL_NO = @[bl_no]
#end
#end			]]></sql>
			<params>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
