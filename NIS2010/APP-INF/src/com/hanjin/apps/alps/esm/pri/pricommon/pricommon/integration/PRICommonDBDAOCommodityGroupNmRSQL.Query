<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOCommodityGroupNmRSQL">
			<desc><![CDATA[group commodity Name find]]></desc>
			<sql><![CDATA[
SELECT 
	PRC_GRP_CMDT_DESC NM
FROM PRI_SG_GRP_CMDT
WHERE	SVC_SCP_CD = @[etc1]
AND	GLINE_SEQ = @[etc2]
AND	PRC_CUST_TP_CD = @[etc3]
AND PRC_GRP_CMDT_CD = @[cd]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="etc3" type="12" value="" out="N"/>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
