<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOmanageIntAuthCSQL">
			<desc><![CDATA[manageIntAuth]]></desc>
			<sql><![CDATA[
#if (${buttonType} == 'btn_t11InternetAUTH')

	INSERT INTO BKG_INET_BL_PRN_AUTH
	(BKG_NO,
	INFO_SEQ,
	AUTH_DT,
	AUTH_OFC_CD,
	AUTH_USR_ID,
	SHPR_CNT_CD,
	SHPR_SEQ,
	CNEE_CNT_CD,
	CNEE_SEQ,
	NTFY_CNT_CD,
	NTFY_SEQ,
	FRT_FWRD_CNT_CD,
	FRT_FWRD_SEQ,
	ANTFY_CNT_CD,
	ANTFY_SEQ,
	VSL_CD,
	SKD_VOY_NO,
	SKD_DIR_CD,
	BL_OBRD_DT,
	BL_NO,
	CRE_USR_ID,
#if (${ser_no} != '') 
	OBL_INTER_SER_NO,
#end
	UPD_USR_ID )
	    SELECT BKG.BKG_NO,
	    (SELECT NVL( MAX(INFO_SEQ), 0) +1 FROM BKG_INET_BL_PRN_AUTH WHERE BKG_NO = @[bkg_no]),
	    SYSDATE,
	    @[upd_office],
	    @[upd_usr_id], 
	    SHPR.CUST_CNT_CD,
	    SHPR.CUST_SEQ,
	    CNEE.CUST_CNT_CD,
	    CNEE.CUST_SEQ,
	    NTFY.CUST_CNT_CD,
	    NTFY.CUST_SEQ,
	    FWRD.CUST_CNT_CD,
	    FWRD.CUST_SEQ,
	    ANTFY.CUST_CNT_CD,
	    ANTFY.CUST_SEQ,
	    BKG.VSL_CD,
	    BKG.SKD_VOY_NO,
	    BKG.SKD_DIR_CD,
	    DOC.BL_OBRD_DT,
	    BKG.BL_NO,
	    @[upd_usr_id],
#if (${ser_no} != '') 
		@[ser_no],
#end
	    @[upd_usr_id]
	    FROM BKG_BOOKING BKG,
	    BKG_BL_DOC DOC,
	    BKG_CUSTOMER SHPR,
	    BKG_CUSTOMER CNEE,
	    BKG_CUSTOMER NTFY,
	    BKG_CUSTOMER FWRD,
	    BKG_CUSTOMER ANTFY
	    WHERE BKG.BKG_NO = @[bkg_no]
	    AND BKG.BKG_NO = DOC.BKG_NO
	    AND BKG.BKG_NO = SHPR.BKG_NO 
	    AND SHPR.BKG_CUST_TP_CD = 'S'
	    AND BKG.BKG_NO = CNEE.BKG_NO(+)
	    AND CNEE.BKG_CUST_TP_CD(+) = 'C'
	    AND BKG.BKG_NO = NTFY.BKG_NO(+)
	    AND NTFY.BKG_CUST_TP_CD(+) = 'N'
	    AND BKG.BKG_NO = FWRD.BKG_NO(+)
	    AND FWRD.BKG_CUST_TP_CD(+) = 'F'
	    AND BKG.BKG_NO = ANTFY.BKG_NO(+)
	    AND ANTFY.BKG_CUST_TP_CD(+) = 'A'

#else 

	UPDATE BKG_INET_BL_PRN_AUTH
	SET 
		DELT_FLG = 'Y',
		DELT_USR_ID = @[upd_usr_id],
		DELT_OFC_CD = @[upd_office], 
		DELT_DT = SYSDATE
	WHERE 
		BKG_NO = @[bkg_no]
	AND DELT_FLG = 'N'
	--AND INFO_SEQ = (SELECT MAX(INFO_SEQ) FROM BKG_INET_BL_PRN_AUTH	WHERE BKG_NO = [bkg_no])

#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="upd_office" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
