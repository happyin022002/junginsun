<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchCaCgoPoLocSlanRSQL">
			<desc><![CDATA[CargoReleaseOrderDBDAOsearchCaCgoPoLocSlanRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT B.PO_NO AS PO_NO,
       D.LOC_CD AS LOC_CD,
       NVL(VSL_SLAN_NM, '') AS VSL_SLAN_NM
  FROM BKG_BOOKING      A,
       BKG_REF_DTL      B,
       MDM_VSL_SVC_LANE C,
       /*  (SELECT LOC_CD 
          FROM MDM_ORGANIZATION
         WHERE OFC_CD =(SELECT OFC_CD
                          FROM COM_USER
                         WHERE USR_ID = '')
       ) D */
       (SELECT LOC_CD
          FROM MDM_ORGANIZATION
         WHERE OFC_CD = @[ofc_cd]) D /* ACCOUNT의 Office Code 또는 타모듈에서의 Parameter 변수 처리 */
 WHERE A.BL_NO   = @[bl_no] /* 변수 처리 */
   AND A.BKG_NO  = B.BKG_NO(+)
   AND A.SLAN_CD = C.VSL_SLAN_CD
   AND ROWNUM    = 1			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
