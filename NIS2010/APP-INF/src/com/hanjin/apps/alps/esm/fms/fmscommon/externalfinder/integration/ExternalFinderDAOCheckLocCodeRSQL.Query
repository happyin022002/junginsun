<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExternalFinderDAOCheckLocCodeRSQL">
			<desc><![CDATA[Check Loc Code]]></desc>
			<sql><![CDATA[
select loc_cd dck_loc_cd 
from mdm_location
where loc_cd = @[loc_cd]
and delt_flg = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
