<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOCustSrepRSQL">
			<desc><![CDATA[GeneralBookingSearchDBDAOCustSrepVO]]></desc>
			<sql><![CDATA[
select  sls.ofc_cd,
        cust.srep_cd,
		SLS.SREP_NM,
        '1' sort
from    mdm_customer cust,
        mdm_sls_rep sls
where   cust.srep_cd     = sls.srep_cd
and     cust.cust_cnt_cd = @[cust_cnt_cd]
and     cust.cust_seq    = @[cust_seq]
and     cust.delt_flg    = 'N'
and     sls.srep_sts_cd  = 'N'
union all
select  sls.ofc_cd,
        cust.srep_cd,
		SLS.SREP_NM,
        '2' sort
from    bkg_cust_sls_rep cust,
        mdm_sls_rep sls,
        mdm_customer mdm_cust
where   cust.cust_cnt_cd  = @[cust_cnt_cd]
and     cust.cust_seq     = @[cust_seq]
and     cust.delt_flg     = 'N'
and 	cust.cust_cnt_cd  = mdm_cust.cust_cnt_cd
and 	cust.cust_seq     = mdm_cust.cust_seq
and 	cust.srep_cd     <> mdm_cust.srep_cd
and     cust.srep_cd      = sls.srep_cd
and     mdm_cust.delt_flg = 'N'
and     sls.srep_sts_cd   = 'N'
order by sort, srep_cd			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="KR" out="N"/>
				<param name="cust_seq" type="12" value="585" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
