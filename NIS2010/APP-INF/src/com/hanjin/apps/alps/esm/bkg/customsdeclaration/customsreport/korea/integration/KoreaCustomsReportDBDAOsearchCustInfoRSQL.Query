<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchCustInfoRSQL">
			<desc><![CDATA[Customer Type , NAME 조회]]></desc>
			<sql><![CDATA[
SELECT MAX(DECODE(BKG_CUST_TP_CD, 'C', replace(replace(NVL(CUST_NM, ' '), '"', '""'), CHR(13)||CHR(10), ' '), ' ')) CUST_C_NM
     , MAX(DECODE(BKG_CUST_TP_CD, 'N', replace(replace(NVL(CUST_NM, ' '), '"', '""'), CHR(13)||CHR(10), ' '), ' ')) CUST_N_NM
  FROM BKG_CUSTOMER
 WHERE BKG_NO = @[bkg_no]
   AND BKG_CUST_TP_CD in ('C','N')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
