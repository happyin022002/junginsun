<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAOGetLoclXchRtCommInfoRSQL">
			<desc><![CDATA[GetLoclXchRtCommInfo
fx_real_amt 를 환율 적용 하여 ACM_AGN_COMM.PAY_CRNT_AMT 금액을 만든다.]]></desc>
			<sql><![CDATA[
select 
	DECODE( @[pay_xch_rt],0,0,round ( @[fx_real_amt] / @[pay_xch_rt] ,13)) as usd_fx_comm,
	round ( @[fx_real_amt] * @[pay_xch_rt] ,13) as pay_fx_comm
from dual			]]></sql>
			<params>
				<param name="pay_xch_rt" type="12" value="" out="N"/>
				<param name="fx_real_amt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
