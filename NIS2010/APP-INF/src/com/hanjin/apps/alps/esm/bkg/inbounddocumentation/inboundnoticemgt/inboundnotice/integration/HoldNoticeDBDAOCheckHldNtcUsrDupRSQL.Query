<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOCheckHldNtcUsrDupRSQL">
			<desc><![CDATA[기 등록된 데이타 체크한다]]></desc>
			<sql><![CDATA[
SELECT 
	HLD_SEQ
FROM BKG_HLD_NTC_USR
WHERE	CSTMS_LOC_CD = @[cstms_loc_cd]
AND	   NTC_USR_ID    = @[ntc_usr_id]
AND	   NTC_ENBL_FLG  = 'Y'

#if(${cstms_dspo_cd} != '*')
AND	   CSTMS_DSPO_CD IN ('*',@[cstms_dspo_cd])
#end

#if(${ntc_mzd_cd} != 'M')
AND	   NTC_MZD_CD    IN ('M',@[ntc_mzd_cd])
#end

#if (${hld_seq} != '')
AND    HLD_SEQ != @[hld_seq]
#end			]]></sql>
			<params>
				<param name="cstms_loc_cd" type="12" value="" out="N"/>
				<param name="ntc_usr_id" type="12" value="" out="N"/>
				<param name="cstms_dspo_cd" type="12" value="" out="N"/>
				<param name="ntc_mzd_cd" type="12" value="" out="N"/>
				<param name="hld_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
