<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOsearchDoSplitNoRSQL">
			<desc><![CDATA[DoSplitNo를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(LPAD(MAX(DO_NO_SPLIT)+1,2,'0'),'01') AS DO_NO_SPLIT
FROM BKG_DO
WHERE BKG_NO=@[bkg_no]
  AND DO_NO=@[do_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="do_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
