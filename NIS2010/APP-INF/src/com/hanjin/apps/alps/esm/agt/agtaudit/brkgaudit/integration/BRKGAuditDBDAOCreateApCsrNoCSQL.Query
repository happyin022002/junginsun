<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BRKGAuditDBDAOCreateApCsrNoCSQL">
			<desc><![CDATA[ap_csr_no 테이블에 csr no 추가]]></desc>
			<sql><![CDATA[
INSERT INTO AP_CSR_NO(
	CSR_NO, 
	CRE_USR_ID, 
	CRE_DT)
VALUES(
	@[csr_no], 
	'BRKG/FAC', 
	SYSDATE)			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
