<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAOGetNewSimulationNoInfoRSQL">
			<desc><![CDATA[New Simulation No. 값을 구한다.]]></desc>
			<sql><![CDATA[
SELECT 'S' || TO_CHAR(SYSDATE, 'YYMMDD') || TO_CHAR(ACM_SIM_SEQ.NEXTVAL, 'FM0000') AS SIM_NO FROM DUAL			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
