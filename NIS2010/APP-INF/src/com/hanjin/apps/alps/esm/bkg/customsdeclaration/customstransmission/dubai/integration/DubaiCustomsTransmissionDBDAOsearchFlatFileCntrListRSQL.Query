<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DubaiCustomsTransmissionDBDAOsearchFlatFileCntrListRSQL">
			<desc><![CDATA[DubaiCntrInfoVO]]></desc>
			<sql><![CDATA[
SELECT A.BL_NO, 
       SUBSTR(A.CNTR_NO, 1, LENGTH(A.CNTR_NO)-1) AS CNTR_NO,
       SUBSTR(A.CNTR_NO, LENGTH(A.CNTR_NO), 1) AS CHECK_DIGIT,
       A.DU_PCK_TP_CD, 
       A.CNTR_TARE_WGT, 
       A.CNTR_SEAL_NO, 
       (SELECT SUBSTR(CNTR_SZ_DESC, 1, 2) 
        FROM MDM_CNTR_SZ 
        WHERE CNTR_SZ_CD = SUBSTR(B.CNTR_TPSZ_CD, 2, 1)
        AND DELT_FLG = 'N') SZ,
       (SELECT C.CNTR_TPSZ_ISO_CD 
        FROM MDM_CNTR_TP_SZ C 
        WHERE C.CNTR_TPSZ_CD = B.CNTR_TPSZ_CD
        AND DELT_FLG = 'N') CNTR_TPSZ_ISO_CD
  FROM BKG_CSTMS_DU_CNTR A, MST_CONTAINER B
 WHERE BL_NO = @[bl_no]
   AND POD_CD = @[pod_cd]   
   AND A.CNTR_NO = B.CNTR_NO			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
