<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CMPBGuidelineDBDAOLoadExcelCheckCountryRSQL">
			<desc><![CDATA[LoadExcelCheckCountry]]></desc>
			<sql><![CDATA[
SELECT  CNT_CD AS CD 
,		CNT_NM AS NM
FROM MDM_COUNTRY
WHERE CNT_CD= @[cd]
AND   DELT_FLG ='N'			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
