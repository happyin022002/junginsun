<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOsearchIFTSAIDateRSQL">
			<desc><![CDATA[IFTSAI전송을 위한 Data조회]]></desc>
			<sql><![CDATA[
SELECT DECODE(CLPT_SEQ, @[vps_call_seq], '9', '153') PORT_IND
         , VPS_PORT_CD LOC_CD
         , LOC_NM LOC_DESC
         , NVL(PLISM_YD_CD, ' ') YD_CD
         , TO_CHAR(VPS_ETA_DT, 'RRRRMMDDHH24MI') ETA
         , TO_CHAR(VPS_ETD_DT, 'RRRRMMDDHH24MI') ETD
         , TO_CHAR(VPS_ETB_DT, 'RRRRMMDDHH24MI') ETB
         , TO_CHAR(NVL(PRD_GET_CCT_FNC(Yd_Cd,Slan_Cd,SKD_DIR_CD, 'F', VPS_ETB_DT, VPS_ETD_DT, A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD  ) , VPS_ETB_DT -10/24), 'RRRRMMDDHH24MI') CCT
         , TO_CHAR(FT_DT, 'RRRRMMDD') FT
    FROM    VSK_VSL_PORT_SKD A, MDM_LOCATION B
    WHERE   VPS_PORT_CD      = B.LOC_CD  (+)
    AND     VSL_CD          = @[vsl_cd]
    AND     SKD_VOY_NO      = @[skd_voy_no]
    AND     SKD_DIR_CD      = @[skd_dir_cd]
    AND     CLPT_SEQ       >= @[vps_call_seq]
    AND     'S'            <> NVL(A.SKD_CNG_STS_CD, ' ')
    ORDER BY CLPT_SEQ			]]></sql>
			<params>
				<param name="vps_call_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
