		<query name="SpaceAllocationManageDBDAOSpcNshwRsltVOCSQL">
			<desc><![CDATA[2015.12.14 이혜민 [CHM-201539247] No-Show Summary 신규 컬럼 개발 요청(Fcast & Alloc)   ]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_NSHW_RSLT (
    RLANE_CD         ,
    DIR_CD           ,
    VSL_CD           ,
    SKD_VOY_NO       ,
    SKD_DIR_CD       ,
    SLS_OFC_CD       ,
    POL_YD_CD        ,
    ALOC_DDCT_BSE_CD ,
    FCAST_LOD_QTY    ,
    ALOC_LOD_QTY     ,
    BKG_LOD_QTY      ,
    REP_TRD_CD       ,
    REP_SUB_TRD_CD   ,
    TRD_CD           ,
    SUB_TRD_CD       ,
    IOC_CD           ,
    SLS_RHQ_CD       ,
    SLS_RGN_OFC_CD   ,
    OFC_KND_CD       ,
    SLS_AQ_CD        ,
    BSE_YRMON        ,
    BSE_WK           ,
    ORG_FCAST_LOD_QTY,
    ORG_ALOC_LOD_QTY ,
    CRE_USR_ID       ,
    CRE_DT           ,
    UPD_USR_ID       ,
    UPD_DT           ,
    MIN_LOD_QTY
) VALUES (
    @[rlane_cd]         ,
    @[dir_cd]           ,
    @[vsl_cd]           ,
    @[skd_voy_no]       ,
    @[skd_dir_cd]       ,
    @[sls_ofc_cd]       ,
    @[pol_yd_cd]        ,
    @[aloc_ddct_bse_cd] ,
    @[fcast_lod_qty]    ,
    @[aloc_lod_qty]     ,
    @[bkg_lod_qty]      ,
    @[rep_trd_cd]       ,
    @[rep_sub_trd_cd]   ,
    @[trd_cd]           ,
    @[sub_trd_cd]       ,
    @[ioc_cd]           ,
    @[sls_rhq_cd]       ,
    @[sls_rgn_ofc_cd]   ,
    @[ofc_knd_cd]       ,
    @[sls_aq_cd]        ,
    @[bse_yrmon]        ,
    @[bse_wk]           ,
    @[org_fcast_lod_qty],
    @[org_aloc_lod_qty] ,
    @[cre_usr_id]       ,
    SYSDATE             ,
    @[upd_usr_id]       ,
    SYSDATE             ,
    LEAST(NVL(@[fcast_lod_qty], 0), NVL(@[aloc_lod_qty], 0))
)			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="sls_ofc_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="aloc_ddct_bse_cd" type="12" value="" out="N"/>
				<param name="fcast_lod_qty" type="12" value="" out="N"/>
				<param name="aloc_lod_qty" type="12" value="" out="N"/>
				<param name="bkg_lod_qty" type="12" value="" out="N"/>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="rep_sub_trd_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
