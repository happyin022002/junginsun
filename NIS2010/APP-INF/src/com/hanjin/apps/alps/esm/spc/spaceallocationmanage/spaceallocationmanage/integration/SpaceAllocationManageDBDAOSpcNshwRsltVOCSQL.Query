				<param name="sls_rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="ofc_knd_cd" type="12" value="" out="N"/>
				<param name="sls_aq_cd" type="12" value="" out="N"/>
				<param name="bse_yrmon" type="12" value="" out="N"/>
				<param name="bse_wk" type="12" value="" out="N"/>
				<param name="org_fcast_lod_qty" type="12" value="" out="N"/>
				<param name="org_aloc_lod_qty" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
