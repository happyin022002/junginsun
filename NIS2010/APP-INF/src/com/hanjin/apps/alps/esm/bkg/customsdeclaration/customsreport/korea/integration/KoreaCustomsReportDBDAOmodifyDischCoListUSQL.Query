<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOmodifyDischCoListUSQL">
			<desc><![CDATA[Discharging Company List 수정]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_DCHG_CO
   SET CSTMS_DCHG_CO_NM = @[cstms_dchg_co_nm]
 WHERE PORT_CD = @[port_cd]
   AND CSTMS_DCHG_CO_ID = @[cstms_dchg_co_id]
			]]></sql>
			<params>
				<param name="cstms_dchg_co_nm" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="cstms_dchg_co_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
