<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOsearchBkgNoByRqstNoRSQL">
			<desc><![CDATA[searchBkgNoByRqstNo]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
FROM BKG_BOOKING
WHERE BKG_NO = @[bkg_no]
UNION
SELECT BKG_NO
FROM BKG_XTER_RQST_MST
WHERE BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
