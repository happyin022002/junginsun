<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Kor24ManifestListDBDAOsearchObTransTpExpWhfRSQL">
			<desc><![CDATA[Trans Type 조회하고 WHF/CTT Exception Check(Outbound)한다.]]></desc>
			<sql><![CDATA[
SELECT KOR.CSTMS_DECL_TP_CD TRANS_TYPE, DECODE(RATE.BKG_RT_WHF_EXPT_CD,'X','R') EXPT_KCD_TP
  FROM BKG_CSTMS_ADV_KR_BL KOR, BKG_RATE RATE
 WHERE KOR.BKG_NO = RATE.BKG_NO
   AND KOR.BKG_NO = @[bkg_no]
   AND KOR.CSTMS_DECL_TP_CD = @[kcd_tp]
   AND KOR.TRNS_SEQ = @[kt_seq]
   AND KOR.DMST_PORT_CD = @[kt_port]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="kcd_tp" type="12" value="" out="N"/>
				<param name="kt_seq" type="12" value="" out="N"/>
				<param name="kt_port" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
