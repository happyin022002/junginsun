<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCntrRtCustRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT 'PN' CUST_TP
, PPD_RCV_OFC_CD||PPD_PAYR_CNT_CD||PPD_PAYR_CUST_SEQ CUST_KEY
, PPD_RCV_OFC_CD OFC_CD
,PPD_PAYR_CNT_CD CNT_CD
,PPD_PAYR_CUST_SEQ CUST_SEQ
FROM BKG_RATE
WHERE BKG_NO = @[bkg_no]
UNION ALL
SELECT 'CN' THIRD_FLG
,CLT_OFC_CD||CLT_PAYR_CNT_CD||CLT_PAYR_CUST_SEQ
,CLT_OFC_CD
,CLT_PAYR_CNT_CD
,CLT_PAYR_CUST_SEQ
FROM BKG_RATE
WHERE BKG_NO = @[bkg_no]
UNION ALL
SELECT FRT_TERM_CD||'Y'
, N3PTY_RCV_OFC_CD||N3PTY_CUST_CNT_CD||N3PTY_CUST_SEQ
, N3PTY_RCV_OFC_CD
, N3PTY_CUST_CNT_CD
, N3PTY_CUST_SEQ
FROM(
SELECT FRT_TERM_CD
, N3PTY_RCV_OFC_CD
, N3PTY_CUST_CNT_CD
, N3PTY_CUST_SEQ
FROM BKG_CHG_RT
WHERE BKG_NO = @[bkg_no]
AND  FRT_INCL_XCLD_DIV_CD = 'N'
AND  N3PTY_RCV_OFC_CD IS NOT NULL
GROUP BY FRT_TERM_CD, N3PTY_RCV_OFC_CD, N3PTY_CUST_CNT_CD, N3PTY_CUST_SEQ
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
