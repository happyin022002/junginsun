<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPMasterDBDAOMultiHistLstFlgMasUSQL">
			<desc><![CDATA[MAS 테이블 동시 수정]]></desc>
			<sql><![CDATA[
UPDATE ${table_name} SET 
		LST_FLG = @[lst_flg]
 WHERE VSL_CD  = @[vsl_cd]
   AND VSL_SEQ ${operator} (SELECT MAX(VSL_SEQ) FROM MAS_VSL_RGST WHERE VSL_CD = @[vsl_cd])			]]></sql>
			<params>
				<param name="lst_flg" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
