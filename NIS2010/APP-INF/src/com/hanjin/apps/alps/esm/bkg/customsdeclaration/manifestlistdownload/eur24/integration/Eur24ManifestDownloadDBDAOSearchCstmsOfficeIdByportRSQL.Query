		<query name="Eur24ManifestDownloadDBDAOSearchCstmsOfficeIdByportRSQL">
			<desc><![CDATA[BKG_CSTMS_EUR_CD_STUP에서 port_cd로 TML_CD검색]]></desc>
			<sql><![CDATA[
SELECT 
  PORT_CD RVIS_N1ST_CLPT_CD,
  --SUBSTR(TML_CD, LENGTH(PORT_CD), 2) TML_CD,
  TML_CD,
  EUR_CSTMS_OFC_ID N1ST_PORT_OFC_CD_NEW
FROM BKG_CSTMS_EUR_CD_STUP
WHERE PORT_CD = @[port_cd]			]]></sql>
