<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchMdmVslCntrInfoRSQL">
			<desc><![CDATA[MDM VSL CNTR정보]]></desc>
			<sql><![CDATA[
SELECT MVC.CRR_CD
FROM MDM_VSL_CNTR MVC
WHERE 1=1
AND MVC.VSL_CD = @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
