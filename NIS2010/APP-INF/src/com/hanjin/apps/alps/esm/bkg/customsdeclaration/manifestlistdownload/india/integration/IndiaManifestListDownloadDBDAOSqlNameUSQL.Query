<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaManifestListDownloadDBDAOSqlNameUSQL">
			<desc><![CDATA[인도세관 테이블에 BL정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_IDA_BL SET 
	IDA_CSTMS_BL_TP_CD = @[ida_cstms_bl_tp_cd]
,	IDA_HBL_SEQ = @[ida_hbl_seq]
,	BL_CGO_DESC = @[bl_cgo_desc]
,	IDA_CSTMS_ITM_TP_CD = @[ida_cstms_itm_tp_cd]
,	IDA_LINE_NO = @[ida_line_no]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,	VSL_CD = @[vsl_cd]
,	SKD_VOY_NO = @[skd_voy_no]
,	SKD_DIR_CD = @[skd_dir_cd]
,	POD_CD = @[pod_cd]
,	BL_NO = @[bl_no]
,	NVOCC_REF_NO = @[nvocc_ref_no]
,	POL_CD = @[pol_cd]
,	DEL_CD = @[del_cd]
,	BL_DECL_TP_CD = @[bl_decl_tp_cd]
,	IDA_DEST_CD = @[ida_dest_cd]
,	IDA_CFS_ID = @[ida_cfs_id]
,	IAL_RGN_CD = @[ial_rgn_cd]
,	IDA_TRSP_CD = @[ida_trsp_cd]
,	CNEE_NM = @[cnee_nm]
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	POD_CD = @[pod_cd]
AND	BL_NO = @[bl_no]
AND	NVOCC_REF_NO = @[nvocc_ref_no]			]]></sql>
			<params>
				<param name="ida_cstms_bl_tp_cd" type="12" value="" out="N"/>
				<param name="ida_hbl_seq" type="2" value="" out="N"/>
				<param name="bl_cgo_desc" type="12" value="" out="N"/>
				<param name="ida_cstms_itm_tp_cd" type="12" value="" out="N"/>
				<param name="ida_line_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="nvocc_ref_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="bl_decl_tp_cd" type="12" value="" out="N"/>
				<param name="ida_dest_cd" type="12" value="" out="N"/>
				<param name="ida_cfs_id" type="12" value="" out="N"/>
				<param name="ial_rgn_cd" type="12" value="" out="N"/>
				<param name="ida_trsp_cd" type="12" value="" out="N"/>
				<param name="cnee_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
