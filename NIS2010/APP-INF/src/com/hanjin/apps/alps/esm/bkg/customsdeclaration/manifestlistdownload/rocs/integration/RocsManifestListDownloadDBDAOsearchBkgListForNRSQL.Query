<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RocsManifestListDownloadDBDAOsearchBkgListForNRSQL">
			<desc><![CDATA[ROCS(ROTTERDAM) 세관 신고용 Booking No List를 조회한다. (B/L Creation Indicator = 'N')]]></desc>
			<sql><![CDATA[
SELECT RBL.BKG_NO
FROM BKG_CSTMS_RTM_BL RBL
WHERE RBL.VSL_CALL_REF_NO = @[crn_number]
AND	(RBL.POL_CD LIKE @[pol_cd]||'%' or RBL.PRE_RLY_PORT_CD LIKE @[pol_cd]||'%')
#if ( ${vsl_cd} != '')
AND RBL.VSL_CD = @[vsl_cd]
AND RBL.SKD_VOY_NO = @[skd_voy_no]
AND RBL.SKD_DIR_CD = @[skd_dir_cd]
#end			]]></sql>
			<params>
				<param name="crn_number" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
