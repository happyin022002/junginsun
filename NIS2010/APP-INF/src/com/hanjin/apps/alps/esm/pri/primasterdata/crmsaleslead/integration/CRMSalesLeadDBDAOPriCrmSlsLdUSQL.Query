<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CRMSalesLeadDBDAOPriCrmSlsLdUSQL">
			<desc><![CDATA[CRM Sales Lead 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE PRI_CRM_SLS_LD SET 
 	SLS_LD_NM = @[sls_ld_nm]
,	SLS_LD_CUST_CNT_CD = @[sls_ld_cust_cnt_cd]
,	SLS_LD_CUST_SEQ = @[sls_ld_cust_seq]
,	SLS_LD_SREP_CD = @[sls_ld_srep_cd]
,	SLS_LD_ST_DT = TO_DATE(@[sls_ld_st_dt],'YYYYMMDD')
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	SLS_LD_NO = @[sls_ld_no]			]]></sql>
			<params>
				<param name="sls_ld_nm" type="12" value="" out="N"/>
				<param name="sls_ld_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="sls_ld_cust_seq" type="2" value="" out="N"/>
				<param name="sls_ld_srep_cd" type="12" value="" out="N"/>
				<param name="sls_ld_st_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="sls_ld_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
