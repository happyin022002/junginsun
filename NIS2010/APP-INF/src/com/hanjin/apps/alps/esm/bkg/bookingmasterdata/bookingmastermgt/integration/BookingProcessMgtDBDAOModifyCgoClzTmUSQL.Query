<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingProcessMgtDBDAOModifyCgoClzTmUSQL">
			<desc><![CDATA[cargo closing time setup UPDATE]]></desc>
			<sql><![CDATA[
UPDATE 	BKG_CGO_CLZ_TM_STUP
SET		POL_CD				= @[pol_cd]
		,LANE_CD			= @[lane_cd]	
		,DIR_CD				= @[dir_cd]
		,CCT_HRS			= SUBSTR(@[cct_hrs],1,1)||REPLACE(SUBSTR(@[cct_hrs],2),'-','')
		,EML_SND_FLG		= DECODE(@[eml_snd_flg],'1','Y','N')
		,MBL_SND_FLG		= DECODE(@[mbl_snd_flg],'1','Y','N')
		,SHPR_NTC_FLG		= DECODE(@[shpr_ntc_flg],'1','Y','N')
		,BKG_PIC_NTC_FLG	= DECODE(@[bkg_pic_ntc_flg],'1','Y','N')
		,SREP_NTC_FLG		= DECODE(@[srep_ntc_flg],'1','Y','N')
		,OB_PIC_NTC_FLG		= DECODE(@[ob_pic_ntc_flg],'1','Y','N')
		,CNTC_EML			= @[cntc_eml]
		,CNTC_MPHN_NO		= @[cntc_mphn_no]
		,CTRT_OFC_PHN_NO	= @[ctrt_ofc_phn_no]
		,AUTO_NTC_FLG		= DECODE(@[auto_ntc_flg],'1','Y','N')
		,UPD_USR_ID			= @[upd_usr_id]
		,UPD_DT				= SYSDATE
WHERE	POL_CD				= @[pol_cd]
AND		LANE_CD				= @[lane_cd]
AND     DIR_CD				= @[dir_cd]			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="cct_hrs" type="12" value="" out="N"/>
				<param name="eml_snd_flg" type="12" value="" out="N"/>
				<param name="mbl_snd_flg" type="12" value="" out="N"/>
				<param name="shpr_ntc_flg" type="12" value="" out="N"/>
				<param name="bkg_pic_ntc_flg" type="12" value="" out="N"/>
				<param name="srep_ntc_flg" type="12" value="" out="N"/>
				<param name="ob_pic_ntc_flg" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="cntc_mphn_no" type="12" value="" out="N"/>
				<param name="ctrt_ofc_phn_no" type="12" value="" out="N"/>
				<param name="auto_ntc_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
