<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingProcessMgtDBDAORemoveCgoClzTmDSQL">
			<desc><![CDATA[cargo closing time setup DELETE]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_CGO_CLZ_TM_STUP
WHERE	POL_CD				= @[pol_cd]
AND		LANE_CD				= @[lane_cd]
AND     DIR_CD 				= @[dir_cd]			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
