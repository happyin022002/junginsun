<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOCopyBasicDataRelationCSQL">
			<desc><![CDATA[최근 이전 분기의 데이터를 복사한다]]></desc>
			<sql><![CDATA[
INSERT INTO SQM_DAT_RLT (
       BSE_TP_CD
      ,BSE_YR
      ,BSE_QTR_CD
      ,OFC_VW_CD
      ,TRD_CD
      ,RLANE_CD
      ,CONV_DIR_CD
      ,RHQ_CD
      ,TEAM_CD
      ,OB_DIV_CD
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT @[f_bse_tp_cd]
      ,@[f_bse_yr]
      ,DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
      ,OFC_VW_CD
      ,TRD_CD
      ,RLANE_CD
      ,CONV_DIR_CD
      ,RHQ_CD
      ,TEAM_CD
      ,OB_DIV_CD
      ,@[cre_usr_id]
      ,SYSDATE
      ,@[cre_usr_id]
      ,SYSDATE
  FROM SQM_DAT_RLT
 WHERE BSE_YR||BSE_QTR_CD = ( SELECT /*+ INDEX_DESC(SQM_DAT_RLT XPKSQM_DAT_RLT) */
                                     BSE_YR || BSE_QTR_CD
                                FROM SQM_DAT_RLT
                               WHERE BSE_TP_CD = 'Q'
                                 AND BSE_YR || BSE_QTR_CD < @[f_bse_yr] || DECODE(@[f_bse_tp_cd], 'Y', '00', @[f_bse_qtr_cd])
                                 AND ROWNUM = 1 )			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
