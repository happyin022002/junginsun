<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOinsertCustAdvisoryNoticeRemarkCSQL">
			<desc><![CDATA[VVD 별 Customer Advisory Remark 정보를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CUST_AVC_NTC_RMK (
                   VSL_CD
                 , SKD_VOY_NO
                 , SKD_DIR_CD
                 , OFC_CD
                 , IMPT_NTC_RMK
                 , DELT_FLG
                 , CRE_USR_ID    
                 , CRE_DT
                 , UPD_USR_ID
                 , UPD_DT
                 , EML_SUBJ_CTNT
                 , SRC_DAT_TP_CD
                 , EML_SUBJ_CTNT_SEQ
                 , RMK_USE_FLG
                 , FILE_NM
                 , FILE_PATH_RMK
                 , FILE_SAV_ID
                 , FILE_DESC)           

VALUES( 
                SUBSTR(@[vvd], 1,4)
               , SUBSTR(@[vvd], 5,4)
               , SUBSTR(@[vvd], 9,1)
               , @[ofc_cd]          
               , @[impt_ntc_rmk]    
               , 'N'                
               , @[cre_usr_id]      
               , SYSDATE            
               , @[upd_usr_id]      
               , SYSDATE            
               , @[eml_subj_ctnt]   
               , 'E'                                   
               , @[eml_subj_ctnt_seq]
               , NVL(@[rmk_use_flg],'N')               
               , NULL                                          
               , @[file_path_rmk]      
               , NULL                                          
               , NVL(@[file_desc], 'N')                                          
)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="impt_ntc_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eml_subj_ctnt" type="12" value="" out="N"/>
				<param name="eml_subj_ctnt_seq" type="12" value="" out="N"/>
				<param name="rmk_use_flg" type="12" value="" out="N"/>
				<param name="file_path_rmk" type="12" value="" out="N"/>
				<param name="file_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
