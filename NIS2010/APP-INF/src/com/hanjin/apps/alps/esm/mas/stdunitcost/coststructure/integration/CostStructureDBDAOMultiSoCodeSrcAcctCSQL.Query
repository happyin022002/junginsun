<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAOMultiSoCodeSrcAcctCSQL">
			<desc><![CDATA[MAS_COST_SRC_ACCT 테이블등록 (Register Cost Items)
 2012.05.07 전윤주 [CHM-201217633] Hinterland Project 
                 - TRS cost table 생성 시 사용되는 계정 분리를 위해 Flag 추가
                  (inlnd_expn_use_flg, inlnd_tml_expn_calc_flg, ocn_fdr_expn_use_flg)]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_COST_SRC_ACCT A 
  USING (SELECT @[mas_cost_src_cd] AS MAS_COST_SRC_CD 
            ,@[cost_ass_bse_cd] AS COST_ASS_BSE_CD 
            ,@[stnd_cost_cd] AS STND_COST_CD 
            ,@[sgrp_cost_cd] AS SGRP_COST_CD 
            ,@[mas_cost_src_prt_cd] AS MAS_COST_SRC_PRT_CD 
            ,@[cost_src_sys_cd] AS COST_SRC_SYS_CD 
            ,@[cost_vol_cd] AS COST_VOL_CD 
            ,@[cost_vol_cd1] AS COST_VOL_CD1 
            ,@[full_mty_cd] AS FULL_MTY_CD 
            ,@[cost_src_mon] AS COST_SRC_MON 
            ,@[mas_cost_src_cd_nm] AS MAS_COST_SRC_CD_NM 
            ,@[cost_ut_amt_cd] AS COST_UT_AMT_CD 
            ,@[bkg_full_soc_cgo_flg] AS BKG_FULL_SOC_CGO_FLG 
            ,@[bkg_mcgo_flg] AS BKG_MCGO_FLG 
            ,@[bkg_rev_mcgo_flg] AS BKG_REV_MCGO_FLG 
            ,@[spcl_cgo_dg_flg] AS SPCL_CGO_DG_FLG 
            ,@[spcl_cgo_bb_flg] AS SPCL_CGO_BB_FLG 
            ,@[spcl_cgo_awk_flg] AS SPCL_CGO_AWK_FLG 
            ,@[spcl_cgo_rf_flg] AS SPCL_CGO_RF_FLG 
            ,@[inlnd_expn_use_flg] AS INLND_EXPN_USE_FLG 
            ,@[inlnd_tml_expn_calc_flg] AS INLND_TML_EXPN_CALC_FLG 
            ,@[ocn_fdr_expn_use_flg] AS OCN_FDR_EXPN_USE_FLG 
            ,@[user_id] AS CRE_USR_ID 
            ,SYSDATE AS CRE_DT 
            ,@[user_id] AS UPD_USR_ID 
            ,SYSDATE AS UPD_DT 
            ,@[delt_flg] AS DELT_FLG 
          FROM DUAL) B 
  ON (A.MAS_COST_SRC_CD = B.MAS_COST_SRC_CD) 
  WHEN NOT MATCHED THEN 
    INSERT (A.MAS_COST_SRC_CD
          ,A.COST_ASS_BSE_CD
          ,A.STND_COST_CD
          ,A.SGRP_COST_CD
          ,A.MAS_COST_SRC_PRT_CD 
          ,A.COST_SRC_SYS_CD
          ,A.COST_VOL_CD
          ,A.COST_VOL_CD1
          ,A.FULL_MTY_CD
          ,A.COST_SRC_MON
          ,A.MAS_COST_SRC_CD_NM 
          ,A.COST_UT_AMT_CD
          ,A.BKG_FULL_SOC_CGO_FLG
          ,A.BKG_MCGO_FLG
          ,A.BKG_REV_MCGO_FLG 
          ,A.SPCL_CGO_DG_FLG
          ,A.SPCL_CGO_BB_FLG
          ,A.SPCL_CGO_AWK_FLG
          ,A.SPCL_CGO_RF_FLG
          ,A.INLND_EXPN_USE_FLG
          ,A.INLND_TML_EXPN_CALC_FLG
          ,A.OCN_FDR_EXPN_USE_FLG
          ,A.CRE_USR_ID
          ,A.CRE_DT
          ,A.UPD_USR_ID
          ,A.UPD_DT
          ,A.DELT_FLG) 
    VALUES (B.MAS_COST_SRC_CD
          ,B.COST_ASS_BSE_CD
          ,B.STND_COST_CD
          ,B.SGRP_COST_CD
          ,B.MAS_COST_SRC_PRT_CD 
          ,B.COST_SRC_SYS_CD
          ,B.COST_VOL_CD
          ,B.COST_VOL_CD1
          ,B.FULL_MTY_CD
          ,B.COST_SRC_MON
          ,B.MAS_COST_SRC_CD_NM 
          ,B.COST_UT_AMT_CD
          ,B.BKG_FULL_SOC_CGO_FLG
          ,B.BKG_MCGO_FLG
          ,B.BKG_REV_MCGO_FLG 
          ,B.SPCL_CGO_DG_FLG
          ,B.SPCL_CGO_BB_FLG
          ,B.SPCL_CGO_AWK_FLG
          ,B.SPCL_CGO_RF_FLG 
          ,B.INLND_EXPN_USE_FLG
          ,B.INLND_TML_EXPN_CALC_FLG
          ,B.OCN_FDR_EXPN_USE_FLG
          ,B.CRE_USR_ID
          ,B.CRE_DT
          ,B.UPD_USR_ID
          ,B.UPD_DT
          ,B.DELT_FLG) 
  WHEN MATCHED THEN 
    UPDATE 
      SET A.COST_ASS_BSE_CD          = B.COST_ASS_BSE_CD
          ,A.STND_COST_CD            = B.STND_COST_CD
          ,A.SGRP_COST_CD            = B.SGRP_COST_CD 
          ,A.MAS_COST_SRC_PRT_CD     = B.MAS_COST_SRC_PRT_CD 
          ,A.COST_SRC_SYS_CD         = B.COST_SRC_SYS_CD
          ,A.COST_VOL_CD             = B.COST_VOL_CD
          ,A.COST_VOL_CD1            = B.COST_VOL_CD1 
          ,A.FULL_MTY_CD             = B.FULL_MTY_CD
          ,A.COST_SRC_MON            = B.COST_SRC_MON
          ,A.MAS_COST_SRC_CD_NM      = B.MAS_COST_SRC_CD_NM 
          ,A.COST_UT_AMT_CD          = B.COST_UT_AMT_CD          
          ,A.BKG_FULL_SOC_CGO_FLG    = B.BKG_FULL_SOC_CGO_FLG
          ,A.BKG_MCGO_FLG            = B.BKG_MCGO_FLG 
          ,A.BKG_REV_MCGO_FLG        = B.BKG_REV_MCGO_FLG
          ,A.SPCL_CGO_DG_FLG         = B.SPCL_CGO_DG_FLG 
          ,A.SPCL_CGO_BB_FLG         = B.SPCL_CGO_BB_FLG
          ,A.SPCL_CGO_AWK_FLG        = B.SPCL_CGO_AWK_FLG 
          ,A.SPCL_CGO_RF_FLG         = B.SPCL_CGO_RF_FLG
          ,A.INLND_EXPN_USE_FLG      = B.INLND_EXPN_USE_FLG
          ,A.INLND_TML_EXPN_CALC_FLG = B.INLND_TML_EXPN_CALC_FLG
          ,A.OCN_FDR_EXPN_USE_FLG    = B.OCN_FDR_EXPN_USE_FLG
          ,A.UPD_USR_ID              = B.UPD_USR_ID 
          ,A.UPD_DT                  = B.UPD_DT
          ,A.DELT_FLG                = B.DELT_FLG			]]></sql>
			<params>
				<param name="mas_cost_src_cd" type="12" value="" out="N"/>
				<param name="cost_ass_bse_cd" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
				<param name="sgrp_cost_cd" type="12" value="" out="N"/>
				<param name="mas_cost_src_prt_cd" type="12" value="" out="N"/>
				<param name="cost_src_sys_cd" type="12" value="" out="N"/>
				<param name="cost_vol_cd" type="12" value="" out="N"/>
				<param name="cost_vol_cd1" type="12" value="" out="N"/>
				<param name="full_mty_cd" type="12" value="" out="N"/>
				<param name="cost_src_mon" type="12" value="" out="N"/>
				<param name="mas_cost_src_cd_nm" type="12" value="" out="N"/>
				<param name="cost_ut_amt_cd" type="12" value="" out="N"/>
				<param name="bkg_full_soc_cgo_flg" type="12" value="" out="N"/>
				<param name="bkg_mcgo_flg" type="12" value="" out="N"/>
				<param name="bkg_rev_mcgo_flg" type="12" value="" out="N"/>
				<param name="spcl_cgo_dg_flg" type="12" value="" out="N"/>
				<param name="spcl_cgo_bb_flg" type="12" value="" out="N"/>
				<param name="spcl_cgo_awk_flg" type="12" value="" out="N"/>
				<param name="spcl_cgo_rf_flg" type="12" value="" out="N"/>
				<param name="inlnd_expn_use_flg" type="12" value="" out="N"/>
				<param name="inlnd_tml_expn_calc_flg" type="12" value="" out="N"/>
				<param name="ocn_fdr_expn_use_flg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
