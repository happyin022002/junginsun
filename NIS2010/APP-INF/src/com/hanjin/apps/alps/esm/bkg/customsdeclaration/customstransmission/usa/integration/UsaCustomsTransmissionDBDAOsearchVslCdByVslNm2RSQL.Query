<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchVslCdByVslNm2RSQL">
			<desc><![CDATA[dwkim, 미세관응답메세지 수신 outVO : VslNameVO]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
  FROM MDM_VSL_CNTR A
 WHERE (A.VSL_EDI_NM = REPLACE(TRIM(@[vsl_eng_nm]),' ','')
     OR A.VSL_ENG_NM = TRIM(@[vsl_eng_nm]))
   AND SUBSTR(A.VSL_CD, 1, 2) <> 'HP'
   AND DELT_FLG = 'N'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_eng_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
