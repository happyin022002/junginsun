<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAO021SusrLaneCSQL">
			<desc><![CDATA[CHM-201215649 2012.01.19 SHKIM
2012.02.16 김성훈 [CHM-201216142-01] 사용자별 Lane 정보 관리/조회시 Lane 항목 추가]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_USR_LANE_INFO
(USR_ID 
		,TRD_CD
		,SUB_TRD_CD
		,DIR_CD
		,USR_LANE_SEQ
		,RLANE_CD
		,CRE_USR_ID
		,CRE_DT
		,UPD_USR_ID
		,UPD_DT )
VALUES
(
	     @[usr_id]
		,@[trd_cd]
		,@[sub_trd_cd]
		,NVL(@[dir_cd], ' ')
		,@[usr_lane_seq]
		,@[rlane_cd]
		,@[usr_id]
		,SYSDATE
		,@[usr_id]
		,SYSDATE
)			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="usr_lane_seq" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
