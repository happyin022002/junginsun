<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOUpdateAllocQtaUSQL">
			<desc><![CDATA[[Allocation = QTA Setting]을을 [저장] 합니다.]]></desc>
			<sql><![CDATA[
UPDATE SQM_ALOC_QTA
   SET LOD_QTY       = @[lod_qty] 
      ,UPD_USR_ID    = @[upd_usr_id]
      ,UPD_DT        = SYSDATE 
 WHERE BSE_YR                     = @[bse_yr]
   AND BSE_MON                    = @[bse_mon]
   AND BSE_WK                     = @[bse_wk]
   AND TRD_CD                     = @[trd_cd]
   AND RLANE_CD                   = @[rlane_cd]
   AND DIR_CD                     = @[dir_cd]
   AND RHQ_CD                     = @[rhq_cd]
   AND RGN_OFC_CD                 = @[rgn_ofc_cd]
   AND VSL_CD                     = @[vsl_cd]
   AND SKD_VOY_NO                 = @[skd_voy_no]
   AND SKD_DIR_CD                 = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="lod_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_mon" type="12" value="" out="N"/>
				<param name="bse_wk" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
