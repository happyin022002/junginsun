<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CRMSalesLeadDBDAOPriCrmSlsLdCSQL">
			<desc><![CDATA[CRM Sales Lead 정보를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_CRM_SLS_LD (
	SLS_LD_NO
,	SLS_LD_NM
,	SLS_LD_CUST_CNT_CD
,	SLS_LD_CUST_SEQ
,	SLS_LD_SREP_CD
,	SLS_LD_ST_DT
,	SLS_LD_STS_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[sls_ld_no]
,	@[sls_ld_nm]
,	@[sls_ld_cust_cnt_cd]
,	@[sls_ld_cust_seq]
,	@[sls_ld_srep_cd]
,	TO_DATE(@[sls_ld_st_dt],'YYYYMMDD')
,	@[sls_ld_sts_cd]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="sls_ld_no" type="12" value="" out="N"/>
				<param name="sls_ld_nm" type="12" value="" out="N"/>
				<param name="sls_ld_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="sls_ld_cust_seq" type="2" value="" out="N"/>
				<param name="sls_ld_srep_cd" type="12" value="" out="N"/>
				<param name="sls_ld_st_dt" type="12" value="" out="N"/>
				<param name="sls_ld_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
