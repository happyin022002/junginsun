<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAOPropCpPriRpMnCSQL">
			<desc><![CDATA[Proposal Copy PRI_SP_MN Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_RP_MN (
    PROP_NO
,   AMDT_SEQ
,   EFF_DT
,   EXP_DT
,   CTRT_CUST_CNT_CD
,   CTRT_CUST_SEQ
,   PRC_CTRT_CUST_TP_CD
,   CTRT_CUST_VAL_SGM_CD
,   PROP_SREP_CD
,   PROP_OFC_CD
,   PROP_APRO_OFC_CD
,   PROP_STS_CD
,   RESPB_SREP_CD
,   RESPB_SLS_OFC_CD
,   CNTR_LOD_UT_CD
,   TGT_MVC_QTY
,   RFA_CTRT_TP_CD
,   CRE_USR_ID
,   CRE_DT
,   UPD_USR_ID
,   UPD_DT
#if (${IS_MASTER_COPY} == 'Y')
,   PROP_USR_ID
#end
)
SELECT @[new_prop_no] AS PROP_NO
     , 0 AS AMDT_SEQ
     , TO_DATE('99991231', 'YYYYMMDD') AS EFF_DT
     , TO_DATE('99991231', 'YYYYMMDD') AS EXP_DT
     , RP.CTRT_CUST_CNT_CD
     , RP.CTRT_CUST_SEQ
     , (SELECT MD.RVIS_CNTR_CUST_TP_CD
        FROM MDM_CUSTOMER MD
        WHERE MD.CUST_CNT_CD = RP.CTRT_CUST_CNT_CD
        AND   MD.CUST_SEQ = RP.CTRT_CUST_SEQ) AS PRC_CTRT_CUST_TP_CD
     , (SELECT MD.VBS_CLSS_CD
        FROM MDM_CUSTOMER MD
        WHERE MD.CUST_CNT_CD = RP.CTRT_CUST_CNT_CD
        AND   MD.CUST_SEQ = RP.CTRT_CUST_SEQ) AS CTRT_CUST_VAL_SGM_CD
     , @[srep_cd] AS PROP_SREP_CD
     , @[ofc_cd] AS PROP_OFC_CD
     , RP.PROP_APRO_OFC_CD
     , 'I' AS PROP_STS_CD
     , (SELECT MD.SREP_CD
        FROM MDM_CUSTOMER MD
        WHERE MD.CUST_CNT_CD = RP.CTRT_CUST_CNT_CD
        AND   MD.CUST_SEQ = RP.CTRT_CUST_SEQ) AS RESPB_SREP_CD
     , (SELECT MD.OFC_CD
        FROM MDM_CUSTOMER MD
        WHERE MD.CUST_CNT_CD = RP.CTRT_CUST_CNT_CD
        AND   MD.CUST_SEQ = RP.CTRT_CUST_SEQ) AS RESPB_SLS_OFC_CD
     , RP.CNTR_LOD_UT_CD
     , RP.TGT_MVC_QTY
#if (${IS_MASTER_COPY} == 'Y')
-------------------------    RFA 효율화를 위한 요청 (1차) (CHM-201640671)
     , @[rfa_ctrt_tp_cd] AS RFA_CTRT_TP_CD
-------------------------    RFA 효율화를 위한 요청 (1차) (CHM-201640671)
#else
     , RP.RFA_CTRT_TP_CD
#end
     , @[cre_usr_id] AS CRE_USR_ID
     , SYSDATE AS CRE_DT
     , @[upd_usr_id] AS UPD_USR_ID
     , SYSDATE AS UPD_DT
#if (${IS_MASTER_COPY} == 'Y') 
     -- Master RFA로 복사하는 경우 PROP_USR_ID도 계승한다.
     , DECODE(@[rfa_ctrt_tp_cd], 'M', PROP_USR_ID, '') AS PROP_USR_ID
#end
FROM PRI_RP_MN RP
WHERE RP.PROP_NO = @[prop_no]
AND   RP.AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="new_prop_no" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="rfa_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
