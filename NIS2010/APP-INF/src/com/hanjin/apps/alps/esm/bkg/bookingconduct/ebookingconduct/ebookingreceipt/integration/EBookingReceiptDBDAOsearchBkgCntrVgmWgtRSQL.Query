<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchBkgCntrVgmWgtRSQL">
			<desc><![CDATA[searchBkgCntrVgmWgt]]></desc>
			<sql><![CDATA[
select cntr_no, 
	case when decode(@[vgm_wgt], 0, vgm_wgt, @[vgm_wgt]) < @[cntr_wgt] then vgm_wgt 
	     when decode(@[vgm_wgt], 0, vgm_wgt, @[vgm_wgt]) = @[cntr_wgt] and (cntr_tpsz_cd = 'T2' or cntr_tpsz_cd = 'T4') then -1
else 0 end as vgm_wgt from bkg_container where vgm_wgt > 0
and bkg_no = @[bkg_no]
and cntr_no = @[cntr_no]			]]></sql>
			<params>
				<param name="vgm_wgt" type="12" value="" out="N"/>
				<param name="cntr_wgt" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
