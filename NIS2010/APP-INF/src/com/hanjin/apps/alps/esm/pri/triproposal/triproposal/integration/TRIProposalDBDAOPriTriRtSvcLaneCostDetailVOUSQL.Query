<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriRtSvcLaneCostDetailVOUSQL">
			<desc><![CDATA[ ]]></desc>
			<sql><![CDATA[
MERGE INTO PRI_TRI_RT A 
USING (
        SELECT A.TRI_PROP_NO, A.AMDT_SEQ
              ,DECODE(COST_ROUT_NO, '1', SUBSTR(N1ST_RLANE_CD, 1,3), '2', SUBSTR(N2ND_RLANE_CD, 1,3), '3', SUBSTR(N3RD_RLANE_CD, 1,3), '4', SUBSTR(N4TH_RLANE_CD, 1,3))
               AS SVC_LANE
         FROM  PRI_TRI_RT_USD_ROUT_CS A
              ,PRI_PRS_USD_ROUT_CS_INFO B
        WHERE  A.TRI_PROP_NO = @[tri_prop_no]
          AND  A.AMDT_SEQ = @[amdt_seq]
          AND  A.USD_ROUT_CS_SEL_FLG = 'Y'
          AND  A.ROUT_CS_SRC_DT = B.ROUT_CS_SRC_DT
          AND  A.ROUT_CS_NO = B.ROUT_CS_NO
    ) B
ON (
        A.TRI_PROP_NO = B.TRI_PROP_NO
   AND  A.AMDT_SEQ = B.AMDT_SEQ
   AND  A.PROP_STS_CD IN ( 'I', 'R' )
)
WHEN MATCHED THEN
UPDATE SET A.VSL_SLAN_CD = B.SVC_LANE			]]></sql>
			<params>
				<param name="tri_prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
