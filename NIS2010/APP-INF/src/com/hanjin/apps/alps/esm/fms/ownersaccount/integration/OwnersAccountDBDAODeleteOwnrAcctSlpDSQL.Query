<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAODeleteOwnrAcctSlpDSQL">
			<desc><![CDATA[OwnersAccount 전표의 최종승인 후 생성한 FMS_OWNR_ACCT_SLP 데이터 삭제 ]]></desc>
			<sql><![CDATA[
DELETE
  FROM FMS_OWNR_ACCT_SLP
 WHERE SLP_TP_CD||SLP_FUNC_CD||SLP_OFC_CD||SLP_ISS_DT||SLP_SER_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
