,	MEAS_UT_CD
,	ACT_WGT
,	WGT_UT_CD
,	ACT_WGT_PRN_FLG
,	HBL_TTL_KNT
,	SPCL_CGO_AUTH_KNT
,	BL_OBRD_TP_CD
,	BL_OBRD_DT
,	AUD_ERR_CTNT
,	IB_MF_CFM_FLG
,	BIS_SYS_FLG
,	ORG_CNT_NM
,	POR_CD
,	POR_NM
,	POL_CD
,	POL_NM
,	POD_CD
,	POD_NM
,	DEL_CD
,	DEL_NM
,	BL_MV_TP_NM
,	FNL_DEST_NM
,	VSL_NM
,	PRE_VSL_NM
,	BL_CVRD_TP_CD
,	MST_CVRD_BL_NO
