<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOInvoiceDBDAOModifySettleDateUSQL">
			<desc><![CDATA[Modify Settle Date]]></desc>
			<sql><![CDATA[
UPDATE FMS_CSUL_SLP
SET STL_DT = REPLACE(@[stl_dt], '-', '')
WHERE SLP_TP_CD||SLP_FUNC_CD||SLP_OFC_CD||SLP_ISS_DT||SLP_SER_NO||SLP_SEQ_NO = @[slp_no]			]]></sql>
			<params>
				<param name="stl_dt" type="12" value="" out="N"/>
				<param name="slp_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
