<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOSearchWhfBbCgoWgtInfoRSQL">
			<desc><![CDATA[BB CGO정보를 가져와서 WHF에 필요한 WGT정보 구하기]]></desc>
			<sql><![CDATA[
SELECT CEIL(SUM(VOL)) CBM_AMT
      , CEIL(SUM(TON_WGT)) TON_AMT
FROM (
      SELECT BKG_NO
            , BB_CGO_SEQ
            , DIM_LEN*DIM_WDT*DIM_HGT*0.883* POWER(0.01,3) AS VOL
            , GRS_WGT * 0.001 AS TON_WGT 
      FROM BKG_BB_CGO
      WHERE BKG_NO =@[bkg_no] 
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
