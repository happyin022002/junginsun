<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOsearchAutoHouseCheckRSQL">
			<desc><![CDATA[searchAutoHouseCheck]]></desc>
			<sql><![CDATA[
SELECT 'Y'
FROM BKG_CSTMS_ADV_RSLT A, BKG_CSTMS_ADV_BL B
WHERE A.CNT_CD = 'US'
AND DSPO_CD = '69'
AND CSTMS_FILE_TP_CD = 3
AND A.BL_NO = @[bl_no]
AND ARR_DT < TO_DATE( @[ir_dt] ,'RRMMDDHH24MISS')
AND A.CNT_CD = B.CNT_CD
AND A.BL_NO = B.BL_NO

			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="ir_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
