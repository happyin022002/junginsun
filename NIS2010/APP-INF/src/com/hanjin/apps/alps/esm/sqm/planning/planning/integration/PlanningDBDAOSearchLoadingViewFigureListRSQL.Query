<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanningDBDAOSearchLoadingViewFigureListRSQL">
			<desc><![CDATA[[Loading View Figure Copy]을 [조회] 합니다
]]></desc>
			<sql><![CDATA[
 SELECT A1.BSE_TP_CD
      , A1.BSE_YR
      , A1.BSE_QTR_CD
      , A1.OFC_VW_CD
      , A1.OB_DIV_CD
      , A1.QTA_STEP_CD
      , A1.QTA_VER_NO
      , A1.RHQ_CD
      , A1.RGN_OFC_CD
      , A1.TRD_CD
      , A1.RLANE_CD
      , A1.DIR_CD
      , A1.CONV_DIR_CD
      , A1.SUB_TRD_CD
      , A1.GID_LOD_POTN_RTO
      , A1.GID_REV_POTN_RTO
      , NVL(A2.LOD_POTN_RTO,0) AS LOD_POTN_RTO
      , NVL(A2.REV_POTN_RTO,0) AS REV_POTN_RTO
      , CASE WHEN A1.LOD_POTN_RTO = NVL(A2.LOD_POTN_RTO,0) AND A1.REV_POTN_RTO = NVL(A2.REV_POTN_RTO,0)  THEN 'N' ELSE 'Y' END AS MODI_FLG
      , A1.HUL_BND_CD
   FROM (
         SELECT PO.BSE_TP_CD
              , PO.BSE_YR
              , PO.BSE_QTR_CD
              , DECODE(PO.OFC_VW_CD, 'C', 'CONTRACT', 'L', 'LOADING') AS OFC_VW_CD
              , DECODE(RLT.OB_DIV_CD, 'N', 'N.OB', 'O/B')             AS OB_DIV_CD
              , PO.QTA_STEP_CD
              , PO.QTA_VER_NO
              , PO.RHQ_CD
              , PO.RGN_OFC_CD
              , PO.TRD_CD
              , PO.RLANE_CD
              , PO.DIR_CD
              , PO.CONV_DIR_CD
              , PO.SUB_TRD_CD
              , PO.GID_LOD_POTN_RTO
              , PO.GID_REV_POTN_RTO
              , PO.LOD_POTN_RTO
              , PO.REV_POTN_RTO
              , DECODE(MAS.HUL_BND_CD, 'HH', 'H/H', 'B/H') AS HUL_BND_CD
           FROM SQM_QTA_POTN_MGMT PO
              , SQM_QTA_LANE_OFC OFC
              , SQM_DAT_RLT RLT
              , MAS_LANE_RGST MAS
          WHERE PO.BSE_TP_CD                = @[f_bse_tp_cd]
            AND PO.BSE_YR                   = @[f_bse_yr]
#if (${f_bse_tp_cd} == 'Y')
            AND PO.BSE_QTR_CD               = '00'
#else
            AND PO.BSE_QTR_CD               = @[f_bse_qtr_cd]
#end
            AND PO.OFC_VW_CD                = @[f_ofc_vw_cd]
            AND PO.BSE_TP_CD                = OFC.BSE_TP_CD
            AND PO.BSE_YR                   = OFC.BSE_YR
            AND PO.BSE_QTR_CD               = OFC.BSE_QTR_CD
            AND PO.OFC_VW_CD                = OFC.OFC_VW_CD
            AND PO.TRD_CD                   = OFC.TRD_CD
            AND PO.RLANE_CD                 = OFC.RLANE_CD
            AND PO.DIR_CD                   = OFC.DIR_CD
            AND PO.RHQ_CD                   = OFC.RHQ_CD
            AND PO.RGN_OFC_CD               = OFC.RGN_OFC_CD
            AND PO.BSE_TP_CD                = RLT.BSE_TP_CD
            AND PO.BSE_YR                   = RLT.BSE_YR
            AND PO.BSE_QTR_CD               = RLT.BSE_QTR_CD
            AND PO.OFC_VW_CD                = RLT.OFC_VW_CD
            AND PO.TRD_CD                   = RLT.TRD_CD
            AND PO.RLANE_CD                 = DECODE(RLT.RLANE_CD, 'All', PO.RLANE_CD, RLT.RLANE_CD)
            AND PO.CONV_DIR_CD              = RLT.CONV_DIR_CD
            AND PO.RHQ_CD                   = RLT.RHQ_CD
            AND PO.TRD_CD                   = MAS.TRD_CD
            AND PO.RLANE_CD                 = MAS.RLANE_CD
            AND PO.DIR_CD                   = MAS.DIR_CD
            AND OFC.SQM_ACT_FLG             = 'Y'
            AND PO.RHQ_CD                   =
                (SELECT CASE WHEN @[ofc_cd] IN
                                        (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255'
                                        ) THEN PO.RHQ_CD ELSE DECODE(@[ofc_cd], 'SELCMI', PO.RHQ_CD, @[ofc_cd]) END TEAM_CD
                   FROM DUAL)

#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
            AND PO.TRD_CD       = @[f_trd_cd]
#end    
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
            AND PO.RLANE_CD     = @[f_rlane_cd]
#end  
#if (${f_click} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
            AND PO.CONV_DIR_CD  = @[f_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
            AND MAS.HUL_BND_CD  = @[f_trd_dir]
#end
#if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
            AND PO.RGN_OFC_CD   = @[f_rgn_ofc_cd]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
            AND RLT.OB_DIV_CD   = @[f_ob_div_cd]
#end
        ) A1
      , (SELECT PO.BSE_TP_CD
              , PO.BSE_YR
              , PO.BSE_QTR_CD
              , DECODE(PO.OFC_VW_CD, 'C', 'CONTRACT', 'L', 'LOADING') AS OFC_VW_CD
              , DECODE(RLT.OB_DIV_CD, 'N', 'N.OB', 'O/B')             AS OB_DIV_CD
              , PO.QTA_STEP_CD
              , PO.QTA_VER_NO
              , PO.RHQ_CD
              , PO.RGN_OFC_CD
              , PO.TRD_CD
              , PO.RLANE_CD
              , PO.DIR_CD
              , PO.CONV_DIR_CD
              , PO.SUB_TRD_CD
              , PO.GID_LOD_POTN_RTO
              , PO.GID_REV_POTN_RTO
              , PO.LOD_POTN_RTO
              , PO.REV_POTN_RTO
              , DECODE(MAS.HUL_BND_CD, 'HH', 'H/H', 'B/H') AS HUL_BND_CD
           FROM SQM_QTA_POTN_MGMT PO
              , SQM_QTA_LANE_OFC OFC
              , SQM_DAT_RLT RLT
              , MAS_LANE_RGST MAS
          WHERE 1                           =1
            AND PO.BSE_TP_CD                = @[f_bse_tp_cd]
            AND PO.BSE_YR                   = @[f_bse_yr]
#if (${f_bse_tp_cd} == 'Y')
            AND PO.BSE_QTR_CD               = '00'
#else
            AND PO.BSE_QTR_CD               = @[f_bse_qtr_cd]
#end
            AND PO.OFC_VW_CD                = 'L'
            AND PO.BSE_TP_CD                = OFC.BSE_TP_CD
            AND PO.BSE_YR                   = OFC.BSE_YR
            AND PO.BSE_QTR_CD               = OFC.BSE_QTR_CD
            AND PO.OFC_VW_CD                = OFC.OFC_VW_CD
            AND PO.TRD_CD                   = OFC.TRD_CD
            AND PO.RLANE_CD                 = OFC.RLANE_CD
            AND PO.DIR_CD                   = OFC.DIR_CD
            AND PO.RHQ_CD                   = OFC.RHQ_CD
            AND PO.RGN_OFC_CD               = OFC.RGN_OFC_CD
            AND PO.BSE_TP_CD                = RLT.BSE_TP_CD
            AND PO.BSE_YR                   = RLT.BSE_YR
            AND PO.BSE_QTR_CD               = RLT.BSE_QTR_CD
            AND PO.OFC_VW_CD                = RLT.OFC_VW_CD
            AND PO.TRD_CD                   = RLT.TRD_CD
            AND PO.RLANE_CD                 = DECODE(RLT.RLANE_CD, 'All', PO.RLANE_CD, RLT.RLANE_CD)
            AND PO.CONV_DIR_CD              = RLT.CONV_DIR_CD
            AND PO.RHQ_CD                   = RLT.RHQ_CD
            AND PO.TRD_CD                   = MAS.TRD_CD
            AND PO.RLANE_CD                 = MAS.RLANE_CD
            AND PO.DIR_CD                   = MAS.DIR_CD
            AND OFC.SQM_ACT_FLG             = 'Y'
            AND PO.RHQ_CD                   =
                (SELECT CASE WHEN @[ofc_cd] IN
                                        (SELECT INTG_CD_VAL_CTNT FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03255'
                                        ) THEN PO.RHQ_CD ELSE DECODE(@[ofc_cd], 'SELCMI', PO.RHQ_CD, @[ofc_cd]) END TEAM_CD
                   FROM DUAL)

#if (${f_trd_cd} != '' && ${f_trd_cd} != 'All')
            AND PO.TRD_CD       = @[f_trd_cd]
#end    
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
            AND PO.RLANE_CD     = @[f_rlane_cd]
#end  
#if (${f_click} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
            AND PO.CONV_DIR_CD  = @[f_dir_cd]
#end
#if (${f_click} == 'on' && ${f_trd_dir} != '' && ${f_trd_dir} != 'All')
            AND MAS.HUL_BND_CD  = @[f_trd_dir]
#end
#if (${f_rgn_ofc_cd} != '' && ${f_rgn_ofc_cd} != 'All')
            AND PO.RGN_OFC_CD   = @[f_rgn_ofc_cd]
#end
#if (${f_ob_div_cd} != '' && ${f_ob_div_cd} != 'All')
            AND RLT.OB_DIV_CD   = @[f_ob_div_cd]
#end
        ) A2
  WHERE A1.BSE_TP_CD   = A2.BSE_TP_CD(+)
    AND A1.BSE_YR      = A2.BSE_YR(+)
    AND A1.BSE_QTR_CD  = A2.BSE_QTR_CD(+)
    AND A1.QTA_VER_NO  = A2.QTA_VER_NO(+)
    AND A1.RHQ_CD      = A2.RHQ_CD(+)
    AND A1.RGN_OFC_CD  = A2.RGN_OFC_CD(+)
    AND A1.TRD_CD      = A2.TRD_CD(+)
    AND A1.RLANE_CD    = A2.RLANE_CD(+)
    AND A1.CONV_DIR_CD = A2.CONV_DIR_CD(+)
ORDER BY A1.TRD_CD
      , A1.SUB_TRD_CD
      , A1.RLANE_CD
      , A1.CONV_DIR_CD
      , A1.RHQ_CD
      , A1.RGN_OFC_CD

			]]></sql>
			<params>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="f_ofc_vw_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_trd_dir" type="12" value="" out="N"/>
				<param name="f_rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="f_ob_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
