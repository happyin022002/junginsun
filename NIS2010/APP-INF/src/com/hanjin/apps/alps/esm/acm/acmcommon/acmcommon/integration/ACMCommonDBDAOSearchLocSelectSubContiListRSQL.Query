<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMCommonDBDAOSearchLocSelectSubContiListRSQL">
			<desc><![CDATA[searchLocSelectSubContiList]]></desc>
			<sql><![CDATA[
    SELECT
           '0' AS CHECKBOX,
           SCONTI_CD,
           SCONTI_NM
      FROM MDM_SUBCONTINENT
     WHERE NVL (DELT_FLG, 'N') = 'N'
       AND CONTI_CD
        IN
         (
#if( '' != ${conti_cd} )
           ${conti_cd}
#else
           NULL
#end
         )
  ORDER BY SCONTI_CD
			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
