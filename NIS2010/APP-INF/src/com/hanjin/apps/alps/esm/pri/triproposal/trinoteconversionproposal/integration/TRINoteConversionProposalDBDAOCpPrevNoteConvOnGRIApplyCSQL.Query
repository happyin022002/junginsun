<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRINoteConversionProposalDBDAOCpPrevNoteConvOnGRIApplyCSQL">
			<desc><![CDATA[GRI Apply시 NoteConversion카피]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_TRI_NOTE_CONV
  (NOTE_CONV_MAPG_ID
  ,NOTE_CONV_SEQ
  ,NOTE_CONV_TP_CD
  ,TRF_PFX_CD
  ,TRF_NO
  ,TRI_PROP_NO
  ,AMDT_SEQ
  ,CHG_RULE_TP_CD
  ,NOTE_CONV_CHG_CD
  ,NOTE_CONV_RULE_CD
  ,EFF_DT
  ,EXP_DT
  ,RULE_APPL_CHG_TP_CD
  ,RT_APPL_TP_CD
  ,RT_OP_CD
  ,CURR_CD
  ,FRT_RT_AMT
  ,PAY_TERM_CD
  ,BKG_RAT_UT_CD
  ,BKG_PRC_CGO_TP_CD
  ,BKG_IMDG_CLSS_CD
  ,BKG_CMDT_TP_CD
  ,BKG_CMDT_DEF_CD
  ,BKG_USA_SVC_MOD_CD
  ,BKG_POR_TP_CD
  ,BKG_POR_DEF_CD
  ,BKG_POL_TP_CD
  ,BKG_POL_DEF_CD
  ,BKG_POD_TP_CD
  ,BKG_POD_DEF_CD
  ,BKG_DEL_TP_CD
  ,BKG_DEL_DEF_CD
  ,BKG_RCV_TERM_CD
  ,BKG_DE_TERM_CD
  ,BKG_MIN_CGO_WGT
  ,BKG_MAX_CGO_WGT
  ,BKG_SOC_FLG
  ,BKG_TS_PORT_TP_CD
  ,BKG_TS_PORT_DEF_CD
  ,BKG_DIR_CALL_FLG
  ,BKG_HNGR_BAR_TP_CD
  ,CONV_RAT_UT_CD
  ,CONV_PRC_CGO_TP_CD
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT)
  SELECT C.NOTE_CONV_MAPG_ID
        ,A.NOTE_CONV_SEQ
        ,A.NOTE_CONV_TP_CD
        ,A.TRF_PFX_CD
        ,A.TRF_NO
        ,A.TRI_PROP_NO
        ,C.AMDT_SEQ
        ,A.CHG_RULE_TP_CD
        ,A.NOTE_CONV_CHG_CD
        ,A.NOTE_CONV_RULE_CD
--        ,GREATEST(A.EFF_DT, C.EFF_DT)
--        ,LEAST(A.EXP_DT, C.EXP_DT)
        ,C.EFF_DT
        ,C.EXP_DT
        ,A.RULE_APPL_CHG_TP_CD
        ,A.RT_APPL_TP_CD
        ,A.RT_OP_CD
        ,A.CURR_CD
        ,A.FRT_RT_AMT
        ,A.PAY_TERM_CD
        ,A.BKG_RAT_UT_CD
        ,A.BKG_PRC_CGO_TP_CD
        ,A.BKG_IMDG_CLSS_CD
        ,A.BKG_CMDT_TP_CD
        ,A.BKG_CMDT_DEF_CD
        ,A.BKG_USA_SVC_MOD_CD
        ,A.BKG_POR_TP_CD
        ,A.BKG_POR_DEF_CD
        ,A.BKG_POL_TP_CD
        ,A.BKG_POL_DEF_CD
        ,A.BKG_POD_TP_CD
        ,A.BKG_POD_DEF_CD
        ,A.BKG_DEL_TP_CD
        ,A.BKG_DEL_DEF_CD
        ,A.BKG_RCV_TERM_CD
        ,A.BKG_DE_TERM_CD
        ,A.BKG_MIN_CGO_WGT
        ,A.BKG_MAX_CGO_WGT
        ,A.BKG_SOC_FLG
        ,A.BKG_TS_PORT_TP_CD
        ,A.BKG_TS_PORT_DEF_CD
        ,A.BKG_DIR_CALL_FLG
        ,A.BKG_HNGR_BAR_TP_CD
        ,A.CONV_RAT_UT_CD
        ,A.CONV_PRC_CGO_TP_CD
        ,@[cre_usr_id]
        ,SYSDATE
        ,@[cre_usr_id]
        ,SYSDATE
    FROM PRI_TRI_NOTE_CONV A
        ,PRI_TRI_RT B
        ,(SELECT A.TRI_PROP_NO, A.AMDT_SEQ, A.NOTE_CONV_MAPG_ID, A.EFF_DT, A.EXP_DT
            FROM PRI_TRI_MN S, PRI_TRI_RT A
           WHERE S.TRI_PROP_NO = A.TRI_PROP_NO
             AND S.TRF_PFX_CD = @[trf_pfx_cd]
             AND S.TRF_NO = @[trf_no]
             AND A.PROP_STS_CD = 'I'
             AND A.GRI_APPL_TP_CD = 'A') C
   WHERE A.NOTE_CONV_MAPG_ID = B.NOTE_CONV_MAPG_ID
     AND B.TRI_PROP_NO = C.TRI_PROP_NO
     AND B.AMDT_SEQ = C.AMDT_SEQ - 1
--     AND (A.EFF_DT BETWEEN C.EFF_DT AND C.EXP_DT OR A.EXP_DT BETWEEN C.EFF_DT AND B.EXP_DT OR
--         (A.EFF_DT <= C.EFF_DT AND A.EXP_DT > C.EXP_DT))			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
