<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOSearchErrorReportForSas111RSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT EDI_RCV_MSG_CTNT AS FLAT_FILE

  FROM BKG_CSTMS_ADV_JP_RCV_LOG

 WHERE BKG_NO = @[bl_no]
   AND JP_MSG_TP_ID = @[jp_msg_tp_id]
   AND TO_CHAR(RCV_DT, 'YYYYMMDDHH24MI') = (SELECT MAX(TO_CHAR(RCV_DT, 'YYYYMMDDHH24MI'))
                                              FROM BKG_CSTMS_ADV_JP_RCV_LOG
                                             WHERE BKG_NO = @[bl_no]
                                               AND JP_MSG_TP_ID = @[jp_msg_tp_id])			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="jp_msg_tp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
