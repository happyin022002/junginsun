<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JapanManifestListDownloadDBDAOmodifyJpcusBlStatusUSQL">
			<desc><![CDATA[modifyJpcusBlStatus]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_JP_BL SET 
	JP_BL_STS_CD = 'A'
WHERE	BL_NO = @[in_bl_no]
AND	BL_SPLIT_NO = NVL(@[in_bl_split_no],'  ')			]]></sql>
			<params>
				<param name="in_bl_no" type="12" value="" out="N"/>
				<param name="in_bl_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
