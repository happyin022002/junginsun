<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOCheckInvNoRSQL">
			<desc><![CDATA[Invoice No 중복 체크]]></desc>
			<sql><![CDATA[
SELECT 'DUP' CHK
  FROM FMS_CSUL_SLP S,
       FMS_CONSULTATION C
 WHERE 1 = 1
   AND C.SLP_TP_CD||C.SLP_FUNC_CD||C.SLP_OFC_CD||C.SLP_ISS_DT||C.SLP_SER_NO
        = S.SLP_TP_CD||S.SLP_FUNC_CD||S.SLP_OFC_CD||S.SLP_ISS_DT||S.SLP_SER_NO
   AND C.CXL_FLG = 'N'     
   AND S.VNDR_SEQ = @[vndr_seq]
   AND S.TO_INV_NO = @[to_inv_no]
   AND S.VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND S.ACCT_CD = '962111'
  -- AND S.PAIR_SLP_TP_CD IS NULL
#if(${csr_no} != '')
   AND C.SLP_TP_CD||C.SLP_FUNC_CD||C.SLP_OFC_CD||C.SLP_ISS_DT||C.SLP_SER_NO <> @[csr_no]
#end			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="to_inv_no" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
