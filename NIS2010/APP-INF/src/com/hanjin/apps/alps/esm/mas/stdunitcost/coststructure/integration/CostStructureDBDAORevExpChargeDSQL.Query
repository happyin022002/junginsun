<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostStructureDBDAORevExpChargeDSQL">
			<desc><![CDATA[Revenue Exception Charge 의 선택된 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM MAS_CHG_EXPT_LIST
 WHERE CHG_CD = @[chg_cd]
   AND CNT_CD = @[cnt_cd]
   AND DELT_FLG = @[delt_flg_org]			]]></sql>
			<params>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="delt_flg_org" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
