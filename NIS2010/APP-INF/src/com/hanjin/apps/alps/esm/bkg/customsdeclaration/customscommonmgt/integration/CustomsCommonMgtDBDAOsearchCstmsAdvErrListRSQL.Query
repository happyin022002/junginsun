<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomsCommonMgtDBDAOsearchCstmsAdvErrListRSQL">
			<desc><![CDATA[searchCstmsAdvErrList]]></desc>
			<sql><![CDATA[
SELECT CNT_CD,
       CSTMS_ERR_CD,
       ERR_CD_DESC,
       ERR_TP_CTNT
FROM BKG_CSTMS_ADV_ERR
WHERE CNT_CD = @[frm_cnt_cd]

#if (${frm_cstms_err_cd} != '')
    AND UPPER(CSTMS_ERR_CD) LIKE UPPER('%'||@[frm_cstms_err_cd]||'%')
#end
#if (${frm_err_cd_desc} != '')
    AND UPPER(ERR_CD_DESC) LIKE UPPER('%'||@[frm_err_cd_desc]||'%')
#end

ORDER BY CNT_CD, CSTMS_ERR_CD			]]></sql>
			<params>
				<param name="frm_cnt_cd" type="12" value="" out="N"/>
				<param name="frm_cstms_err_cd" type="12" value="" out="N"/>
				<param name="frm_err_cd_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
