<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOmodifyBkgKrWhfBlUSQL">
			<desc><![CDATA[U]]></desc>
			<sql><![CDATA[
UPDATE BKG_KR_WHF_BL                                                          
   SET WHF_POL_CD = DECODE(SUBSTR(@[whf_bnd_cd], 1, 1), 'O', @[pol_cd], NULL),
       WHF_POD_CD = DECODE(SUBSTR(@[whf_bnd_cd], 1, 1), 'I', @[pod_cd], NULL),
       WFG_EXPT_CD = @[wfg_expt_cd],                                          
       CUST_RGST_NO = @[cust_rgst_no],                                        
       WGT_QTY = @[wgt_qty] * 1000,                                           
       MEAS_QTY = @[meas_qty],                                                
       CSTMS_DECL_TP_CD = @[cstms_decl_tp_cd],                                
	   WHF_BL_STS_CD = DECODE(@[whf_bl_sts_cd],'D','D',WHF_BL_STS_CD),        
       RTON_WGT = @[rton_wgt],                                                
       WHF_AMT = @[whf_amt],                                                  
       CMDT_CD = @[cmdt_cd],                                                  
       UPD_USR_ID = @[upd_usr_id],                                            
       UPD_DT = SYSDATE                                                       
 WHERE VSL_CD     = @[vsl_cd]                                                 
   AND SKD_VOY_NO = @[skd_voy_no]                                             
   AND SKD_DIR_CD = @[skd_dir_cd]                                             
   AND WHF_BND_CD = @[whf_bnd_cd]                                             
   AND BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="whf_bnd_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="wfg_expt_cd" type="12" value="" out="N"/>
				<param name="cust_rgst_no" type="12" value="" out="N"/>
				<param name="wgt_qty" type="12" value="" out="N"/>
				<param name="meas_qty" type="12" value="" out="N"/>
				<param name="cstms_decl_tp_cd" type="12" value="" out="N"/>
				<param name="whf_bl_sts_cd" type="12" value="" out="N"/>
				<param name="rton_wgt" type="12" value="" out="N"/>
				<param name="whf_amt" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
