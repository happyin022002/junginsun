<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchKorDoAttorneyDtlRSQL">
			<desc><![CDATA[수임자의 사업자 번호와 위임자 사업자 번호로 기 등록여부를 확인한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT
  FROM BKG_DO_ATTY_DTL
 WHERE FM_ATTY_BIZ_NO = @[fm_atty_biz_no]
   AND TO_ATTY_BIZ_NO = @[to_atty_biz_no]			]]></sql>
			<params>
				<param name="fm_atty_biz_no" type="12" value="" out="N"/>
				<param name="to_atty_biz_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
