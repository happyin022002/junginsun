<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BDRCorrectionDBDAOModifyBkgCorrectionUSQL">
			<desc><![CDATA[BDRCorrectionDBDAOModifyBkgCorrectionUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_CORRECTION
   SET --2개 flag 추가확인 
       DOC_PERF_EXPT_CD    = @[doc_perf_expt_cd]
     , RT_CORR_FLG         = @[rt_corr_flg]
     , CHG_TERM_CORR_FLG   = @[chg_term_corr_flg]
     , RCVDE_TERM_CORR_FLG = @[rcvde_term_corr_flg]
     , ROUT_CORR_FLG       = @[rout_corr_flg]
     , CUST_CORR_FLG       = @[cust_corr_flg]
     , QTY_CORR_FLG        = @[qty_corr_flg]
     , MEAS_QTY_CORR_FLG   = @[meas_qty_corr_flg]
     , CMDT_CORR_FLG       = @[cmdt_corr_flg]
     , TRNK_VSL_CORR_FLG   = @[trnk_vsl_corr_flg]
     , PRPST_VSL_CORR_FLG  = @[prpst_vsl_corr_flg]
     , CORR_DT             = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,BKG_COM_USER_LOC_FNC(@[upd_usr_id]))
	 , CORR_GDT			   = GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,'GMT')
     , CORR_USR_ID         = @[corr_usr_id]
     , UPD_DT              = SYSDATE
     , UPD_USR_ID          = @[upd_usr_id]
	 , RAT_FLG			   = @[rat_flg]
	 , EXPN_FLG			   = @[expn_flg]
	 , CA_OTR_RSN_CORR_FLG = @[ca_otr_rsn_corr_flg]
	 , CA_RSN_CD           = NVL(@[ca_rsn_cd], CA_RSN_CD)
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'			]]></sql>
			<params>
				<param name="doc_perf_expt_cd" type="12" value="" out="N"/>
				<param name="rt_corr_flg" type="12" value="" out="N"/>
				<param name="chg_term_corr_flg" type="12" value="" out="N"/>
				<param name="rcvde_term_corr_flg" type="12" value="" out="N"/>
				<param name="rout_corr_flg" type="12" value="" out="N"/>
				<param name="cust_corr_flg" type="12" value="" out="N"/>
				<param name="qty_corr_flg" type="12" value="" out="N"/>
				<param name="meas_qty_corr_flg" type="12" value="" out="N"/>
				<param name="cmdt_corr_flg" type="12" value="" out="N"/>
				<param name="trnk_vsl_corr_flg" type="12" value="" out="N"/>
				<param name="prpst_vsl_corr_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="corr_usr_id" type="12" value="" out="N"/>
				<param name="rat_flg" type="12" value="" out="N"/>
				<param name="expn_flg" type="12" value="" out="N"/>
				<param name="ca_otr_rsn_corr_flg" type="12" value="" out="N"/>
				<param name="ca_rsn_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
