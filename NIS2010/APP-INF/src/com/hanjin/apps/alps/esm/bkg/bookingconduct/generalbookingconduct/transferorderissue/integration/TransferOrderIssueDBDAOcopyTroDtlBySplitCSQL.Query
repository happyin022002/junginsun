<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOcopyTroDtlBySplitCSQL">
			<desc><![CDATA[copyTroDtlBySplit]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_TRO_DTL
(
       BKG_NO
      ,IO_BND_CD
      ,RTN_TRO_FLG
      ,TRO_SEQ
      ,TRO_SUB_SEQ
      ,CNTR_TPSZ_CD
      ,TRO_QTY
      ,CNTR_NO
      ,DOR_ARR_DT
      ,PKUP_LOC_CD
      ,PKUP_YD_CD
      ,RTN_LOC_CD
      ,RTN_YD_CD
      ,CMDT_CD
      ,PCTL_NO
      ,CXL_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
(
SELECT 
       @[target_bkg_no]
      ,A.IO_BND_CD
      ,A.RTN_TRO_FLG
      ,A.TRO_SEQ
      ,A.TRO_SUB_SEQ
      ,A.CNTR_TPSZ_CD
      ,A.TRO_QTY
      ,A.CNTR_NO
      ,A.DOR_ARR_DT
      ,A.PKUP_LOC_CD
      ,A.PKUP_YD_CD
      ,A.RTN_LOC_CD
      ,A.RTN_YD_CD
      ,A.CMDT_CD
      ,A.PCTL_NO
      ,A.CXL_FLG
      ,@[usr_id]
      ,SYSDATE
      ,@[usr_id]
      ,SYSDATE
  FROM BKG_TRO_DTL A
      ,SCE_TRO_MAPG B
 WHERE A.BKG_NO = @[source_bkg_no]
   AND B.BKG_NO = @[target_bkg_no]
   AND A.IO_BND_CD = B.IO_BND_CD
   AND A.TRO_SEQ = B.TRO_SEQ
   AND A.TRO_SUB_SEQ = B.TRO_SUB_SEQ
   AND EXISTS (SELECT 'X'
                 FROM BKG_TRO C
                WHERE C.BKG_NO = A.BKG_NO
                  AND C.TRO_SEQ = A.TRO_SEQ
                  AND C.IO_BND_CD = A.IO_BND_CD
                  AND C.CFM_FLG = 'Y')
)			]]></sql>
			<params>
				<param name="target_bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="source_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
