<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAORsltCmdtListVORSQL">
			<desc><![CDATA[commodity 조회]]></desc>
			<sql><![CDATA[
SELECT
	CMDT_CD,
	CMDT_NM,
	REP_CMDT_CD
FROM MDM_COMMODITY
WHERE 	DELT_FLG = 'N'
#if (${cmdt_cd} != "")
AND CMDT_CD LIKE @[cmdt_cd] || '%'
#end
#if (${cmdt_nm} != "") 
AND CMDT_NM LIKE '%' || UPPER(@[cmdt_nm]) || '%'
#end
ORDER BY CMDT_CD, CMDT_NM			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="cmdt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
