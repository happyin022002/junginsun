<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchActVvdRSQL">
			<desc><![CDATA[UsaContainerManifestListVO]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(VSL_CD || SKD_VOY_NO || ACT_FILE_SKD_DIR_CD), 
           SUBSTR(@[vvd], 1, 8) || DECODE(SUBSTR(@[vvd], 9), 'E', 'W', 'W', 'E')) AS VVD
  FROM BKG_CSTMS_ADV_BL
 WHERE CNT_CD = 'US'
   AND VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9)
   AND CSTMS_POL_CD = @[pol]
   AND ACT_FILE_SKD_DIR_CD IS NOT NULL
			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
