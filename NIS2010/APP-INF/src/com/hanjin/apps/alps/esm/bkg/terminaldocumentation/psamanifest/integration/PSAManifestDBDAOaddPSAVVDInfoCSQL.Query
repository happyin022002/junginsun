<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOaddPSAVVDInfoCSQL">
			<desc><![CDATA[PSA VSL 정보 입력]]></desc>
			<sql><![CDATA[
INSERT
  INTO BKG_CSTMS_PSA_VVD 
     ( VSL_CD
     , SKD_VOY_NO
     , SKD_DIR_CD
     , PSA_VSL_NM
     , PSA_VOY_DIR_CD
     , CRE_DT
     , CRE_USR_ID
     , UPD_DT
     , UPD_USR_ID
     )
VALUES 
     ( @[vsl_cd]
     , @[skd_voy_no]
     , @[skd_dir_cd]
     , UPPER(TRIM(REPLACE(@[psa_vsl_nm], CHR(13)||CHR(10), '')))
     , UPPER(TRIM(@[psa_voy_dir_cd]))
     , SYSDATE
     , @[user_id]
     , SYSDATE
     , @[user_id]
     )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="psa_vsl_nm" type="12" value="" out="N"/>
				<param name="psa_voy_dir_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
