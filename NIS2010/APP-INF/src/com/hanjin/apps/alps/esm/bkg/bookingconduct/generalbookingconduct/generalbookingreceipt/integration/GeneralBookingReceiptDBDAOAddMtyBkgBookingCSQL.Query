<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOAddMtyBkgBookingCSQL">
			<desc><![CDATA[Empty Booking 저장]]></desc>
			<sql><![CDATA[
INSERT
INTO bkg_booking (
          bkg_no
        , slan_cd
        , vsl_cd
        , skd_voy_no
        , skd_dir_cd        
        , por_cd
        , pol_cd
        , pol_nod_cd
        , pod_cd
        , pod_nod_cd
        , del_cd
        , pre_rly_port_cd
        , pst_rly_port_cd
        , mty_pkup_yd_cd
        , bkg_ofc_cd
        , sls_rhq_cd
        , sls_rgn_ofc_cd
        , ob_sls_ofc_cd
        , doc_usr_id
,	BKG_CRE_DT
, 	BKG_CRE_GDT
        , bkg_sts_cd
        , bkg_cgo_tp_cd
        , rcv_term_cd
        , de_term_cd
        , rep_cmdt_cd
        , xter_rmk
        , inter_rmk
        , bl_no
        , bl_no_tp
        , mty_bkg_sts_cd
        , mty_cre_svr_cd
        , mty_split_aval_cd
        , cre_usr_id
        , cre_dt
        , upd_usr_id
	 , upd_dt
	,RC_FLG
	,AWK_CGO_FLG
	,BB_CGO_FLG
	,RD_CGO_FLG
	,HNGR_FLG
	,PRCT_FLG
	,SPCL_HIDE_FLG
	,SOC_FLG
	,EQ_SUBST_FLG
	,FD_GRD_FLG
	,FLEX_HGT_FLG
	,ALL_MTR_FLG
	,DBL_BKG_FLG
	,AP_BROG_FLG
	,CUST_TO_ORD_FLG
	,ALT_CUST_CFM_FLG
	,WT_RSN_SPCL_CGO_FLG
	,WT_RSN_HLD_FLG
	,MNL_BKG_NO_FLG
	,BL_ISS_BLCK_FLG
	,SI_FLG
	,XTER_RQST_AUTO_NTC_FLG
	,OVR_VOID_SLT_QTY	 
)VALUES (
          @[bkg_no]
        , (select vsl_slan_Cd from vsk_vsl_skd where vsl_cd = @[vsl_cd] and skd_voy_no = @[skd_voy_no] and skd_Dir_Cd = @[skd_dir_cd])--slan_cd
        , @[vsl_cd]
        , @[skd_voy_no]
        , @[skd_dir_cd]
        , substr(@[pol_yd_cd], 1, 5)--por_Cd
        , substr(@[pol_yd_cd], 1, 5)--pol_cd
        , @[pol_yd_cd]
        , substr(@[pod_yd_cd], 1, 5)--pod_cd
        , @[pod_yd_cd]
        , substr(@[pod_yd_cd], 1, 5)--del_cd
        , @[pre_rly_port_cd]
        , @[pst_rly_port_cd]
        , @[pkup_yd_cd]
        , @[bkg_ofc_cd]
        , @[sls_rhq_cd]--sls_rhq_cd
        , @[sls_rgn_ofc_cd]--sls_rgn_ofc_cd
        , @[ob_sls_cd]--ob_sls_cd
        , @[doc_usr_id]
        , GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,BKG_COM_USER_LOC_FNC(@[doc_usr_id]))
		, GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,'GMT')
        , 'F'--bkg_sts_cd
        , 'P'--bkg_cgo_tp_cd
        , 'Y'--rcv_term_cd
        , 'Y'--de_term_cd
        , '8609'--rep_cmdt_cd
        , 'Empty Container Repositionning Booking'--xter_rmk
        , @[bkg_rmk]--inter_rmk
        , @[bl_no]
        , '0'
        , @[mty_bkg_sts_cd]
        , @[bkg_cre_svr_cd]
        , @[mty_split_aval_cd]
        , @[cre_usr_id]
	 , SYSDATE
        , @[upd_usr_id]
	 , SYSDATE
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,'N'
	,0	 
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="ABC" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="pre_rly_port_cd" type="12" value="" out="N"/>
				<param name="pst_rly_port_cd" type="12" value="" out="N"/>
				<param name="pkup_yd_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="sls_rhq_cd" type="12" value="" out="N"/>
				<param name="sls_rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="ob_sls_cd" type="12" value="" out="N"/>
				<param name="doc_usr_id" type="12" value="" out="N"/>
				<param name="bkg_rmk" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="mty_bkg_sts_cd" type="12" value="" out="N"/>
				<param name="bkg_cre_svr_cd" type="12" value="" out="N"/>
				<param name="mty_split_aval_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="200505" out="N"/>
				<param name="upd_usr_id" type="12" value="3006606" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
