<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EServiceCompensationDBDAOPriCmpnEsvcVOUSQL">
			<desc><![CDATA[E-SVC Compensation Creation 데이터 일괄 수정]]></desc>
			<sql><![CDATA[
UPDATE PRI_CMPN_ESVC SET 
	SVC_SCP_CD = @[svc_scp_cd]
,	ORG_RGN_CD = @[org_rgn_cd]
,	DEST_RGN_CD = @[dest_rgn_cd]
,	PRC_CTRT_TP_CD =@[prc_ctrt_tp_cd]
,	SC_NO = @[sc_no]
,	RFA_NO = @[rfa_no]
,	CHG_CD = @[chg_cd]
,	CURR_CD = @[curr_cd]
,	FLT_PCT_TP_CD = DECODE(LENGTH(TRIM(@[curr_cd])),0,'P','F')
,	DC_AMT = NVL2(@[curr_cd],@[dc_amt],@[dc_per])
,	EFF_DT = TO_DATE(@[eff_dt],'YYYY-MM-DD')
,	EXP_DT = TO_DATE(@[exp_dt],'YYYY-MM-DD')
,	CMPN_RMK = @[cmpn_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND	CMPN_SEQ = @[cmpn_seq]			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="org_rgn_cd" type="12" value="" out="N"/>
				<param name="dest_rgn_cd" type="12" value="" out="N"/>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="dc_amt" type="6" value="" out="N"/>
				<param name="dc_per" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="cmpn_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cmpn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
