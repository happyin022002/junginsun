<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateQuotationDBDAOPriSqScgAdjVODSQL">
			<desc><![CDATA[      CM/OP 산출 이후 Conversion 대상이 되는 예외적용 Surcharge 값을 반영하여 Simulation하기 위한 정보 delete]]></desc>
			<sql><![CDATA[
DELETE FROM pri_sq_scg_adj
WHERE qttn_no = @[qttn_no]
    and qttn_ver_no = @[qttn_ver_no]
    and scg_adj_seq = @[scg_adj_seq]
    and gen_spcl_rt_tp_cd=@[gen_spcl_rt_tp_cd]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="scg_adj_seq" type="2" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
