<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOSearchMstCntrForMstRSQL">
			<desc><![CDATA[MstContainer 정보 수정을 위한 조회
2011.08.08 김용진 [CHM-201112434] Empty BKG Update 로직의 Validation 추가]]></desc>
			<sql><![CDATA[
select SYS_AREA_GRP_ID
    , CNMV_YR       
    , CNMV_ID_NO    
    , CNMV_SEQ      
    , CNMV_SPLIT_NO 
    , CNMV_CYC_NO   
    , to_char(CNMV_DT, 'YYYYMMDD HH24:MI') CNMV_DT
    , BKG_KNT       
    , CRNT_YD_CD    
    , DEST_YD_CD    
    , VSL_CD        
    , SKD_VOY_NO    
    , SKD_DIR_CD    
    , CNMV_STS_CD   
    , ACIAC_DIV_CD
	, PRE_STS_FLG  
from mst_container
where cntr_no = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
