<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAODemDetCostRepbyBKGDSQL">
			<desc><![CDATA[DEM/DET Cost Report by BKG 를 Save(저장 중 - delete) 한다.]]></desc>
			<sql><![CDATA[
DELETE FROM MAS_CHSS_PDM_UPLD
 WHERE POOL_CD = @[pool_cd]
   AND SCC_CD = @[scc_cd]
   AND BZC_RT = @[bzc_rt]
   AND RT_AMT = @[rt_amt]
   AND COST_YRMON = @[cost_yrmon] 			]]></sql>
			<params>
				<param name="pool_cd" type="12" value="" out="N"/>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="bzc_rt" type="12" value="" out="N"/>
				<param name="rt_amt" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
