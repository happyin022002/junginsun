<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOSearchCODBookingListRSQL">
			<desc><![CDATA[COD BKG List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DENSE_RANK() OVER(ORDER BY B.BKG_NO, B.BL_NO) SEQ,
       O.REGION RHQ_CD, B.BKG_OFC_CD, B.SVC_SCP_CD, B.BKG_NO, B.BL_NO, D.BDR_FLG, 
       TO_CHAR(R.RT_APLY_DT,'YYYYMMDD') RT_APLY_DT, TO_CHAR(V.VPS_ETD_DT,'YYYYMMDD') VPS_ETD_DT,
       DECODE(R.BKG_CTRT_TP_CD, 'S', B.SC_NO, 'R', B.RFA_NO, 'T', B.TAA_NO) CTRT_NO, 
       DECODE(N.RDN_NO,NULL,' ',N.RDN_NO) RDN_NO, ' ' SELF_AUDIT,
       N.RVIS_SEQ, C.COD_RQST_SEQ,
       C.OLD_POR_YD_CD, C.OLD_POL_YD_CD, C.OLD_POD_YD_CD, C.OLD_DEL_YD_CD, 
       C.NEW_POR_YD_CD, C.NEW_POL_YD_CD, C.NEW_POD_YD_CD, C.NEW_DEL_YD_CD,
       NVL(SUM(OFT.CHG_AMT),0) OFT_AMT, NVL(OFT.RAT_UT_CD,' ') OFT_RAT_UT_CD, NVL(OFT.CURR_CD,' ') OFT_CURR_CD,
       NVL(SUM(DVC.CHG_AMT),0) DVC_AMT, NVL(DVC.RAT_UT_CD,' ') DVC_RAT_UT_CD, NVL(DVC.CURR_CD,' ') DVC_CURR_CD,
       NVL(COD.CHG_CD,' ') COD_CHG_CD, NVL(SUM(COD.CHG_AMT),0) COD_AMT, NVL(COD.RAT_UT_CD,' ') COD_RAT_UT_CD, NVL(COD.CURR_CD,' ') COD_CURR_CD,
       COUNT (DISTINCT B.BKG_NO) OVER (PARTITION BY 1) BL_CNT,
       (SELECT INTG_CD_VAL_DESC
	    FROM COM_INTG_CD_DTL
		WHERE INTG_CD_ID = 'CD01716'
		AND INTG_CD_VAL_CTNT = BKG_CTRT_TP_CD) BKG_CTRT_TP_CD, 
       (SELECT INTG_CD_VAL_DESC
	    FROM COM_INTG_CD_DTL
		WHERE INTG_CD_ID = 'CD02153'
		AND INTG_CD_VAL_CTNT = COD_RQST_RSN_CD) COD_RQST_RSN_CD, 
       NVL((SELECT INTG_CD_VAL_DESC
	    FROM COM_INTG_CD_DTL
		WHERE INTG_CD_ID = 'CD01568'
		AND INTG_CD_VAL_CTNT = RDN_STS_CD),' ') RDN_STS_NM, 
       NVL((SELECT  UMCH_RSN_RMK  
        FROM BKG_REV_AUD_RSLT UB 
        WHERE UB.BKG_NO = B.BKG_NO 
        AND UB.BKG_REV_AUD_TP_CD='C' 
        ),' ') UMCH_RSN_RMK,
       NVL(( SELECT RGN_GRP_AVC_RMK 
         FROM BKG_REV_AUD_RSLT UB 
        WHERE UB.BKG_NO = B.BKG_NO 
         AND UB.BKG_REV_AUD_TP_CD='C'
        ),' ') RGN_GRP_AVC_RMK
FROM BKG_BOOKING B, BKG_RATE R, BKG_COD C, BKG_BL_DOC D, BKG_OFC_LVL_V O,
     BKG_VVD BV, VSK_VSL_PORT_SKD V, BKG_REV_DR_NOTE N, BKG_CHG_RT OFT, BKG_CHG_RT DVC, BKG_COD_COST COD
WHERE B.BKG_NO = C.BKG_NO
AND B.BKG_STS_CD <> 'X'
AND B.BKG_CGO_TP_CD = 'F'
AND R.BKG_NO = C.BKG_NO
#if (${date_type} == 'B')
AND B.BKG_CRE_DT BETWEEN TO_DATE(@[fm_dt],'YYYY-MM-DD') AND TO_DATE(@[to_dt],'YYYY-MM-DD') + 0.99999
#elseif (${date_type} == 'A')
AND R.RT_APLY_DT BETWEEN TO_DATE(@[fm_dt],'YYYY-MM-DD') AND TO_DATE(@[to_dt],'YYYY-MM-DD') + 0.99999
#elseif (${date_type} == 'E')
AND V.VPS_ETD_DT BETWEEN TO_DATE(@[fm_dt],'YYYY-MM-DD') AND TO_DATE(@[to_dt],'YYYY-MM-DD') + 0.99999
#end
AND C.COD_RQST_SEQ = (SELECT MAX (COD_RQST_SEQ) FROM BKG_COD WHERE BKG_NO = C.BKG_NO)
AND C.COD_STS_CD = 'F'
AND B.BKG_OFC_CD = O.OFC_CD
AND O.REGION = NVL(@[rhq_cd], O.REGION)
AND B.BKG_OFC_CD = NVL(@[bkg_ofc_cd], B.BKG_OFC_CD)
AND B.SVC_SCP_CD = NVL(@[svc_scp_cd], B.SVC_SCP_CD)
AND R.BKG_CTRT_TP_CD = NVL(@[bkg_ctrt_tp_cd], R.BKG_CTRT_TP_CD)
AND C.COD_RQST_RSN_CD = NVL(@[cod_rqst_rsn_cd], C.COD_RQST_RSN_CD)
AND D.BKG_NO = C.BKG_NO
AND B.BKG_NO = BV.BKG_NO
AND B.POL_CD = BV.POL_CD
AND BV.VSL_CD = V.VSL_CD
AND BV.SKD_VOY_NO = V.SKD_VOY_NO
AND BV.SKD_DIR_CD = V.SKD_DIR_CD
AND BV.POL_CD = V.VPS_PORT_CD
AND BV.POL_YD_CD = V.YD_CD
AND BV.POL_CLPT_IND_SEQ = V.CLPT_IND_SEQ
AND C.BKG_NO = N.BKG_NO(+)
AND (N.RVIS_SEQ = (SELECT MAX(RVIS_SEQ) FROM BKG_REV_DR_NOTE WHERE BKG_NO = C.BKG_NO)
    OR N.RVIS_SEQ IS NULL)
AND C.BKG_NO = OFT.BKG_NO(+)
AND OFT.CHG_CD(+) = 'OFT'
AND C.BKG_NO = DVC.BKG_NO(+)
AND DVC.CHG_CD(+) = 'DVC'
AND C.BKG_NO = COD.BKG_NO(+)
AND C.COD_RQST_SEQ = COD.COD_RQST_SEQ(+)
AND (
		OFT.RAT_UT_CD = COD.RAT_UT_CD
	OR	COD.RAT_UT_CD IS NULL
	OR  OFT.RAT_UT_CD  IN ( 'BX', 'BL' ) 
	OR  COD.RAT_UT_CD  IN ( 'BX', 'BL' ) 
	OR (	( SELECT A.CNTR_SZ_CD FROM PRI_RAT_UT A WHERE A.RAT_UT_CD = OFT.RAT_UT_CD )
		=	( SELECT A.CNTR_SZ_CD FROM PRI_RAT_UT A WHERE A.RAT_UT_CD = COD.RAT_UT_CD )
		)
	)
GROUP BY O.REGION, B.BKG_OFC_CD, B.SVC_SCP_CD, B.BKG_NO, B.BL_NO, D.BDR_FLG, R.RT_APLY_DT, V.VPS_ETD_DT,
       R.BKG_CTRT_TP_CD, DECODE(R.BKG_CTRT_TP_CD, 'S', B.SC_NO, 'R', B.RFA_NO, 'T', B.TAA_NO), 
       N.RDN_NO, N.RDN_STS_CD, C.COD_RQST_RSN_CD, C.COD_RQST_SEQ, N.RVIS_SEQ,
       C.OLD_POR_YD_CD, C.OLD_POL_YD_CD, C.OLD_POD_YD_CD, C.OLD_DEL_YD_CD, 
       C.NEW_POR_YD_CD, C.NEW_POL_YD_CD, C.NEW_POD_YD_CD, C.NEW_DEL_YD_CD,
       OFT.RAT_UT_CD, OFT.CURR_CD, DVC.RAT_UT_CD, DVC.CURR_CD, COD.RAT_UT_CD, COD.CURR_CD, COD.CHG_CD			]]></sql>
			<params>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="bkg_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="cod_rqst_rsn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
