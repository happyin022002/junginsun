		<query name="BookingMasterMgtDBDAOSearchWareHouseVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE bkg_warehouse SET 
	cnt_cd = @[cnt_cd]
,	wh_cd = @[wh_cd]
,	wh_nm = @[wh_nm]
,	wh_addr = @[wh_addr]
,	loc_cd = @[loc_cd]
,	cstms_cd = @[cstms_cd]
,	phn_no = @[phn_no]
,	fax_no = @[fax_no]
,	pson_nm = @[pson_nm]
,	diff_rmk = @[diff_rmk]
,	upd_usr_id = @[upd_usr_id]
,	upd_dt = sysdate
WHERE	cnt_cd = @[cnt_cd]
AND	wh_cd = @[wh_cd]
			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
