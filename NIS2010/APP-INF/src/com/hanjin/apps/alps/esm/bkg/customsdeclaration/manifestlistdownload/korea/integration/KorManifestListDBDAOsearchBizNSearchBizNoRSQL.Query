<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchBizNSearchBizNoRSQL">
			<desc><![CDATA[OutBound 의 BizNo구하기]]></desc>
			<sql><![CDATA[
SELECT REPLACE(WHF_SHPR_RGST_NO,'-','') BIZ_NO
  FROM BKG_RATE
 WHERE BKG_NO = @[bkg_no]
   AND BKG_RT_WHF_EXPT_CD = 'S'
   AND LENGTH(WHF_SHPR_RGST_NO) = 12			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
