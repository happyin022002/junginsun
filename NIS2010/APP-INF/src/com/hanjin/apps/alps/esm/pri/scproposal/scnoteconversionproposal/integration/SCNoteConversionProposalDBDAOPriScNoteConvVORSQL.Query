<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteConversionProposalDBDAOPriScNoteConvVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT    A.NOTE_CONV_MAPG_ID
		, A.NOTE_CONV_SEQ
		, A.NOTE_CONV_TP_CD
		, A.SVC_SCP_CD
		, A.PROP_NO
		, A.AMDT_SEQ
		, CASE WHEN A.CHG_RULE_TP_CD = 'C' THEN NOTE_CONV_CHG_CD
			   WHEN A.CHG_RULE_TP_CD = 'R' THEN NOTE_CONV_RULE_CD
			   END CHG_RULE_DEF_CD
		, A.CHG_RULE_TP_CD
		, A.NOTE_CONV_CHG_CD
		, A.NOTE_CONV_RULE_CD
		, TO_CHAR(A.EFF_DT,'YYYYMMDD') EFF_DT
		, TO_CHAR(A.EXP_DT,'YYYYMMDD') EXP_DT
		, A.RT_APPL_TP_CD
		, A.RT_OP_CD
		, A.CURR_CD
		, A.FRT_RT_AMT
		, A.PAY_TERM_CD
		, A.GEN_SPCL_RT_TP_CD
		, A.BKG_RAT_UT_CD
		, A.BKG_PRC_CGO_TP_CD
		, A.BKG_IMDG_CLSS_CD
		, A.BKG_CMDT_TP_CD
		, A.BKG_CMDT_DEF_CD
		, A.BKG_SCG_GRP_CMDT_CD
		, A.BKG_USA_SVC_MOD_CD
		, A.BKG_POR_TP_CD
		, DECODE(A.BKG_POR_TP_CD, 'T', SUBSTR(A.BKG_POR_DEF_CD, 3), A.BKG_POR_DEF_CD) BKG_POR_DEF_CD
		, A.BKG_POL_TP_CD
		, DECODE(A.BKG_POL_TP_CD, 'T', SUBSTR(A.BKG_POL_DEF_CD, 3), A.BKG_POL_DEF_CD) BKG_POL_DEF_CD
		, A.BKG_POD_TP_CD
		, DECODE(A.BKG_POD_TP_CD, 'T', SUBSTR(A.BKG_POD_DEF_CD, 3), A.BKG_POD_DEF_CD) BKG_POD_DEF_CD
		, A.BKG_DEL_TP_CD
		, DECODE(A.BKG_DEL_TP_CD, 'T', SUBSTR(A.BKG_DEL_DEF_CD, 3), A.BKG_DEL_DEF_CD) BKG_DEL_DEF_CD
		, A.BKG_ORG_TRSP_MOD_CD
		, A.BKG_DEST_TRSP_MOD_CD
		, A.BKG_RCV_TERM_CD
		, A.BKG_DE_TERM_CD
		, A.BKG_SLAN_CD
		, (A.BKG_VSL_CD||A.BKG_SKD_VOY_NO||A.BKG_SKD_DIR_CD) BKG_VVD_CD
		, A.BKG_VSL_CD
		, A.BKG_SKD_VOY_NO
		, A.BKG_SKD_DIR_CD
		, A.BKG_SOC_FLG
		, (A.BKG_ACT_CUST_CNT_CD||LPAD(A.BKG_ACT_CUST_SEQ,6,0)) BKG_ACT_CUST_DEF_CD
		, A.BKG_ACT_CUST_CNT_CD
		, A.BKG_ACT_CUST_SEQ
		, A.BKG_TS_PORT_TP_CD
		, A.BKG_TS_PORT_DEF_CD
		, A.BKG_DIR_CALL_FLG
		, A.BKG_MST_HBL_TP_CD
		, A.CONV_RAT_UT_CD
		, A.CONV_PRC_CGO_TP_CD
		, A.CONV_CMDT_TP_CD
		, A.CONV_CMDT_DEF_CD
		, A.CONV_ORG_LOC_TP_CD
		, DECODE(A.CONV_ORG_LOC_TP_CD, 'T', SUBSTR(A.CONV_ORG_LOC_DEF_CD, 3), A.CONV_ORG_LOC_DEF_CD) CONV_ORG_LOC_DEF_CD
		, A.CONV_ORG_VIA_LOC_TP_CD
		, DECODE(A.CONV_ORG_VIA_LOC_TP_CD, 'T', SUBSTR(A.CONV_ORG_VIA_LOC_DEF_CD, 3), A.CONV_ORG_VIA_LOC_DEF_CD) CONV_ORG_VIA_LOC_DEF_CD
		, A.CONV_DEST_VIA_LOC_TP_CD
		, DECODE(A.CONV_DEST_VIA_LOC_TP_CD, 'T', SUBSTR(A.CONV_DEST_VIA_LOC_DEF_CD, 3), A.CONV_DEST_VIA_LOC_DEF_CD) CONV_DEST_VIA_LOC_DEF_CD
		, A.CONV_DEST_LOC_TP_CD
		, DECODE(A.CONV_DEST_LOC_TP_CD, 'T', SUBSTR(A.CONV_DEST_LOC_DEF_CD, 3), A.CONV_DEST_LOC_DEF_CD) CONV_DEST_LOC_DEF_CD
		, A.CONV_PRC_RCV_TERM_CD
		, A.CONV_PRC_DE_TERM_CD
		, A.NOTE_HDR_SEQ
		, A.RULE_APPL_CHG_TP_CD
		, TO_CHAR(A.UPD_DT,'YYYYMMDD') UPD_DT
		, (SELECT USR_NM FROM COM_USER WHERE USR_ID = A.UPD_USR_ID) USR_NM

		, DECODE(A.BKG_POR_TP_CD, 'T', SUBSTR(A.BKG_POR_DEF_CD, 1, 2), '') BKG_POR_CNT_CD
		, DECODE(A.BKG_POL_TP_CD, 'T', SUBSTR(A.BKG_POL_DEF_CD, 1, 2), '') BKG_POL_CNT_CD
		, DECODE(A.BKG_POD_TP_CD, 'T', SUBSTR(A.BKG_POD_DEF_CD, 1, 2), '') BKG_POD_CNT_CD
		, DECODE(A.BKG_DEL_TP_CD, 'T', SUBSTR(A.BKG_DEL_DEF_CD, 1, 2), '') BKG_DEL_CNT_CD
		, DECODE(A.CONV_ORG_LOC_TP_CD, 'T', SUBSTR(A.CONV_ORG_LOC_DEF_CD, 1, 2), '') 			CONV_ORG_LOC_CNT_CD
		, DECODE(A.CONV_ORG_VIA_LOC_TP_CD, 'T', SUBSTR(A.CONV_ORG_VIA_LOC_DEF_CD, 1, 2), '') 	CONV_ORG_VIA_LOC_CNT_CD
		, DECODE(A.CONV_DEST_VIA_LOC_TP_CD, 'T', SUBSTR(A.CONV_DEST_VIA_LOC_DEF_CD, 1, 2), '') 	CONV_DEST_VIA_LOC_CNT_CD
		, DECODE(A.CONV_DEST_LOC_TP_CD, 'T', SUBSTR(A.CONV_DEST_LOC_DEF_CD, 1, 2), '') 			CONV_DEST_LOC_CNT_CD
		, A.BKG_IO_GA_CD
		, A.BKG_CNL_TZ_CD
		, A.BKG_ESVC_TP_CD
		, A.RULE_APPL_CHG_CD
        , A.RT_PATT_TP_CD
        , A.IGN_TRF_FLG
		, A.BKG_NO
	 FROM PRI_SC_NOTE_CONV A
	WHERE A.NOTE_CONV_MAPG_ID = @[note_conv_mapg_id]
	ORDER BY CHG_RULE_TP_CD			]]></sql>
			<params>
				<param name="note_conv_mapg_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
