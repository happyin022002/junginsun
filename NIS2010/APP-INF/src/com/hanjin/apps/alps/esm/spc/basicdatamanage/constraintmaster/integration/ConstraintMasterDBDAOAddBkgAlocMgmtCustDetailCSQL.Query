<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOAddBkgAlocMgmtCustDetailCSQL">
			<desc><![CDATA[Customer Deatil 정보 입력]]></desc>
			<sql><![CDATA[
INSERT INTO SPC_BKG_ALOC_MGMT_CUST_DTL

     ( BKG_ALOC_SEQ
     , BKG_CUST_TP_CD
     , CUST_CNT_CD
     , CUST_SEQ
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     )
VALUES
     ( @[bkg_aloc_seq]
     , @[bkg_cust_tp_cd]
     , SUBSTR(@[cust_cnt_cd], 1, 2)
     , TO_NUMBER(SUBSTR(@[cust_seq], 3, 6))
     , @[cre_usr_id]
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
     )			]]></sql>
			<params>
				<param name="bkg_aloc_seq" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
