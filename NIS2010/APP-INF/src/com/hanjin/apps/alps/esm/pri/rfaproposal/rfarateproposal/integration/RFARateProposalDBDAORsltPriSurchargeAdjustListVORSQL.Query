<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateProposalDBDAORsltPriSurchargeAdjustListVORSQL">
			<desc><![CDATA[PRS-Surcharge Adjust 조회   ]]></desc>
			<sql><![CDATA[
SELECT prop_no, 
    amdt_seq, 
    svc_scp_cd, 
    scg_adj_seq, 
    prc_cmdt_tp_cd, 
    prc_cmdt_def_cd, 
    org_loc_tp_cd, 
    org_loc_def_cd, 
    org_via_loc_tp_cd, 
    org_via_loc_def_cd, 
    dest_via_loc_tp_cd, 
    dest_via_loc_def_cd, 
    dest_loc_tp_cd, 
    dest_loc_def_cd, 
    prc_rcv_term_cd, 
    prc_de_term_cd, 
    bkg_rat_ut_cd, 
    prc_cgo_tp_cd, 
    chg_cd, 
    curr_cd, 
    adj_scg_amt, 
    adj_scg_usd_amt, 
    cre_usr_id, 
    cre_dt, 
    upd_usr_id, 
    upd_dt 
FROM pri_rp_scp_scg_adj
WHERE prop_no = @[prop_no]
    and amdt_seq = @[amdt_seq]
    and svc_scp_cd = @[svc_scp_cd]
ORDER BY prc_cmdt_def_cd,org_loc_def_cd,org_via_loc_def_cd,dest_via_loc_def_cd,dest_loc_def_cd			]]></sql>
			<params>
				<param name="prop_no" type="12" value="HAM090035" out="N"/>
				<param name="amdt_seq" type="2" value="0" out="N"/>
				<param name="svc_scp_cd" type="12" value="TPE" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
