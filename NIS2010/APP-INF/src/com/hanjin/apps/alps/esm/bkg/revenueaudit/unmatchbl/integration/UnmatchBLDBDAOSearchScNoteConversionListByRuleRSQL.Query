<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UnmatchBLDBDAOSearchScNoteConversionListByRuleRSQL">
			<desc><![CDATA[심사를 위해 특정 Rule Type (Conversion Table상 Code)을 포함하는 SC Note를 조회]]></desc>
			<sql><![CDATA[
#if (${note_conv_rule_cd} != 'NOR')
WITH SC_LIST AS (
    SELECT  DISTINCT 
            H.SC_NO
        ,   M.PROP_NO
        ,   M.AMDT_SEQ 
        ,   P.CTRT_PTY_NM
        ,   NC.NOTE_CONV_TP_CD
        ,   NC.NOTE_CONV_MAPG_ID
    FROM    PRI_SP_HDR H
        ,   PRI_SP_MN M
        ,   PRI_SP_CTRT_PTY P
        ,   PRI_SC_NOTE_CONV NC
    WHERE   M.PROP_NO = H.PROP_NO
    AND     M.AMDT_SEQ = (  SELECT  MAX ( X.AMDT_SEQ ) 
                            FROM    PRI_SP_MN X 
                            WHERE   X.PROP_NO = M.PROP_NO 
                            AND     SYSDATE BETWEEN X.EFF_DT AND X.EXP_DT
                            AND     X.PROP_STS_CD = 'F' )
    AND     M.PROP_NO = P.PROP_NO
    AND     M.AMDT_SEQ = P.AMDT_SEQ
    AND     P.PRC_CTRT_PTY_TP_CD = 'C'      
    AND     M.PROP_NO = NC.PROP_NO
    AND     M.AMDT_SEQ = NC.AMDT_SEQ
    AND     NC.EFF_DT <= TO_DATE(@[exp_dt], 'YYYY-MM-DD')      -- Effective Date (To)
    AND     NC.EXP_DT >= TO_DATE(@[eff_dt], 'YYYY-MM-DD')      -- Effective Date (From)
    AND     H.CRE_DT >= TO_DATE('20170101', 'YYYYMMDD')        -- Ignore Hanjin Shipping Contract
    AND     H.SC_NO = NVL(@[ctrt_no], H.SC_NO)
    AND     (NC.NOTE_CONV_RULE_CD = NVL(@[note_conv_rule_cd], NC.NOTE_CONV_RULE_CD) OR NC.NOTE_CONV_CHG_CD = NVL(@[note_conv_rule_cd], NC.NOTE_CONV_CHG_CD))
) 
SELECT CTRT_NO
    ,  BKG_CTRT_TP_CD
    ,  PROP_NO
    ,  AMDT_SEQ
    ,  CTRT_PTY_NM
    ,  SVC_SCP_CD
    ,  GEN_SPCL_RT_TP_CD AS RT_TP
    ,  NOTE_CONV_TP_CD
    ,  DP_SEQ
    ,  NOTE_CLSS_CD
    ,  CHG_CD
    ,  NOTE_CTNT
    ,  NOTE_CONV_SEQ
    ,  CHG_RULE_DEF_CD
    ,  EFF_DT
    ,  EXP_DT
    ,  RT_APPL_TP_CD
    ,  CURR_CD
    ,  RT_OP_CD
    ,  FRT_RT_AMT
    ,  BKG_RAT_UT_CD
    ,  BKG_PRC_CGO_TP_CD
    ,  BKG_IMDG_CLSS_CD
    ,  BKG_POR_DEF_CD
    ,  BKG_POL_DEF_CD
    ,  BKG_POD_DEF_CD
    ,  BKG_DEL_DEF_CD
    ,  BKG_CMDT_DEF_CD
    ,  BKG_SCG_GRP_CMDT_CD
    ,  BKG_ORG_TRSP_MOD_CD
    ,  BKG_DEST_TRSP_MOD_CD
    ,  BKG_RCV_TERM_CD
    ,  BKG_DE_TERM_CD
    ,  BKG_SLAN_CD
    ,  BKG_DIR_CALL_FLG
    ,  BKG_TS_PORT_DEF_CD
    ,  BKG_IO_GA_CD
    ,  BKG_CNL_TZ_CD
    ,  BKG_VVD_CD
    ,  BKG_ACT_CUST_DEF_CD
    ,  BKG_SOC_FLG
    ,  BKG_USA_SVC_MOD_CD
    ,  BKG_ESVC_TP_CD
    ,  BKG_MST_HBL_TP_CD
    ,  PAY_TERM_CD
    ,  RULE_APPL_CHG_TP_CD
    ,  RULE_APPL_CHG_CD
    ,  IGN_TRF_FLG
    ,  RT_PATT_TP_CD
    ,  CONV_RAT_UT_CD
    ,  CONV_PRC_CGO_TP_CD
    ,  CONV_CMDT_DEF_CD
    ,  CONV_ORG_LOC_DEF_CD
    ,  CONV_ORG_VIA_LOC_DEF_CD
    ,  CONV_DEST_VIA_LOC_DEF_CD
    ,  CONV_DEST_LOC_DEF_CD
    ,  CONV_PRC_RCV_TERM_CD
    ,  CONV_PRC_DE_TERM_CD
    ,  UPD_DT
    ,  USR_NM
    ,  COUNT(DISTINCT CTRT_NO) OVER (PARTITION BY 1) CTRT_CNT
-------------------------------------
    ,  '' NOTE_CONV_RULE_CD
-------------------------------------
FROM (

        SELECT  'Commodity' NOTE_CONV_TP_CD
            ,   M.SC_NO AS CTRT_NO
            ,   'S' AS BKG_CTRT_TP_CD
            ,   M.PROP_NO
            ,   M.AMDT_SEQ
            ,   M.CTRT_PTY_NM
            ,   CN.SVC_SCP_CD
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01705' AND C.INTG_CD_VAL_CTNT = CN.GEN_SPCL_RT_TP_CD ) GEN_SPCL_RT_TP_CD
            ,   TO_CHAR(CH.BLET_DP_SEQ) DP_SEQ
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01711' AND C.INTG_CD_VAL_CTNT = CN.NOTE_CLSS_CD ) NOTE_CLSS_CD
            ,   CN.CHG_CD
            ,   CN.NOTE_CTNT
            ,   NC.NOTE_CONV_SEQ
            ,   CASE WHEN NC.CHG_RULE_TP_CD = 'C' THEN NC.NOTE_CONV_CHG_CD
                     WHEN NC.CHG_RULE_TP_CD = 'R' THEN NC.NOTE_CONV_RULE_CD
                END CHG_RULE_DEF_CD
            ,   TO_CHAR(NC.EFF_DT, 'YYYY-MM-DD') EFF_DT
            ,   TO_CHAR(NC.EXP_DT, 'YYYY-MM-DD') EXP_DT
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01723' AND C.INTG_CD_VAL_CTNT = NC.RT_APPL_TP_CD ) RT_APPL_TP_CD
            ,   NC.CURR_CD
            ,   NC.RT_OP_CD
            ,   NC.FRT_RT_AMT
            ,   NC.BKG_RAT_UT_CD
            ,   NC.BKG_PRC_CGO_TP_CD
            ,   NC.BKG_IMDG_CLSS_CD
            ,   NC.BKG_POR_DEF_CD
            ,   NC.BKG_POL_DEF_CD
            ,   NC.BKG_POD_DEF_CD
            ,   NC.BKG_DEL_DEF_CD
            ,   NC.BKG_CMDT_DEF_CD
            ,   NC.BKG_SCG_GRP_CMDT_CD
            ,   NC.BKG_ORG_TRSP_MOD_CD
            ,   NC.BKG_DEST_TRSP_MOD_CD
            ,   NC.BKG_RCV_TERM_CD
            ,   NC.BKG_DE_TERM_CD
            ,   NC.BKG_SLAN_CD
            ,   NC.BKG_DIR_CALL_FLG
            ,   NC.BKG_TS_PORT_DEF_CD
            ,   NC.BKG_IO_GA_CD
            ,   NC.BKG_CNL_TZ_CD
            ,   NC.BKG_VSL_CD||NC.BKG_SKD_VOY_NO||NC.BKG_SKD_DIR_CD BKG_VVD_CD
            ,   NC.BKG_ACT_CUST_CNT_CD||LPAD(NC.BKG_ACT_CUST_SEQ,6,0) BKG_ACT_CUST_DEF_CD
            ,   NC.BKG_SOC_FLG
            ,   NC.BKG_USA_SVC_MOD_CD
            ,   NC.BKG_ESVC_TP_CD
            ,   NC.BKG_MST_HBL_TP_CD
            ,   NC.PAY_TERM_CD
            ,   NC.RULE_APPL_CHG_TP_CD
            ,   NC.RULE_APPL_CHG_CD
            ,   NC.IGN_TRF_FLG
            ,   NC.RT_PATT_TP_CD
            ,   NC.CONV_RAT_UT_CD
            ,   NC.CONV_PRC_CGO_TP_CD
            ,   NC.CONV_CMDT_DEF_CD
            ,   NC.CONV_ORG_LOC_DEF_CD
            ,   NC.CONV_ORG_VIA_LOC_DEF_CD
            ,   NC.CONV_DEST_VIA_LOC_DEF_CD
            ,   NC.CONV_DEST_LOC_DEF_CD
            ,   NC.CONV_PRC_RCV_TERM_CD
            ,   NC.CONV_PRC_DE_TERM_CD
            ,   TO_CHAR(NC.UPD_DT, 'YYYY-MM-DD') UPD_DT
            ,   C.USR_NM
        FROM    SC_LIST M
            ,   PRI_SC_NOTE_CONV NC
            ,   PRI_SP_SCP_RT_CNOTE CN
            ,   PRI_SP_SCP_RT_CMDT_HDR CH
            ,   COM_USER C
        WHERE   1 = 1
        AND     NC.PROP_NO = M.PROP_NO
        AND     NC.AMDT_SEQ = M.AMDT_SEQ
        AND     NC.NOTE_CONV_MAPG_ID = M.NOTE_CONV_MAPG_ID
        AND     NC.NOTE_CONV_TP_CD = M.NOTE_CONV_TP_CD 
        AND     NC.NOTE_CONV_TP_CD = 'C' 
        AND     NC.NOTE_CONV_MAPG_ID = CN.NOTE_CONV_MAPG_ID
        AND     CN.PROP_NO = CH.PROP_NO
        AND     CN.AMDT_SEQ = CH.AMDT_SEQ
        AND     CN.SVC_SCP_CD = CH.SVC_SCP_CD
        AND     CN.GEN_SPCL_RT_TP_CD = CH.GEN_SPCL_RT_TP_CD
        AND     CN.CMDT_HDR_SEQ = CH.CMDT_HDR_SEQ
        AND     CN.SRC_INFO_CD <> 'AD'
        AND     NC.UPD_USR_ID = C.USR_ID
        AND     CN.SVC_SCP_CD = NVL(@[svc_scp_cd], CN.SVC_SCP_CD)
        AND     NC.NOTE_CONV_TP_CD = NVL(@[note_conv_tp_cd], NC.NOTE_CONV_TP_CD)

        UNION ALL

        SELECT  'Route' NOTE_CONV_TP_CD
            ,   M.SC_NO AS CTRT_NO
            ,   'S' AS BKG_CTRT_TP_CD
            ,   M.PROP_NO
            ,   M.AMDT_SEQ
            ,   M.CTRT_PTY_NM
            ,   RN.SVC_SCP_CD
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01705' AND C.INTG_CD_VAL_CTNT = RN.GEN_SPCL_RT_TP_CD ) GEN_SPCL_RT_TP_CD
            ,   CH.BLET_DP_SEQ||'.'||RN.ROUT_SEQ DP_SEQ
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01711' AND C.INTG_CD_VAL_CTNT = RN.NOTE_CLSS_CD ) NOTE_CLSS_CD
            ,   RN.CHG_CD
            ,   RN.NOTE_CTNT
            ,   NC.NOTE_CONV_SEQ
            ,   CASE WHEN NC.CHG_RULE_TP_CD = 'C' THEN NC.NOTE_CONV_CHG_CD
                     WHEN NC.CHG_RULE_TP_CD = 'R' THEN NC.NOTE_CONV_RULE_CD
                END CHG_RULE_DEF_CD
            ,   TO_CHAR(NC.EFF_DT, 'YYYY-MM-DD') EFF_DT
            ,   TO_CHAR(NC.EXP_DT, 'YYYY-MM-DD') EXP_DT
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01723' AND C.INTG_CD_VAL_CTNT = NC.RT_APPL_TP_CD ) RT_APPL_TP_CD
            ,   NC.CURR_CD
            ,   NC.RT_OP_CD
            ,   NC.FRT_RT_AMT
            ,   NC.BKG_RAT_UT_CD
            ,   NC.BKG_PRC_CGO_TP_CD
            ,   NC.BKG_IMDG_CLSS_CD
            ,   NC.BKG_POR_DEF_CD
            ,   NC.BKG_POL_DEF_CD
            ,   NC.BKG_POD_DEF_CD
            ,   NC.BKG_DEL_DEF_CD
            ,   NC.BKG_CMDT_DEF_CD
            ,   NC.BKG_SCG_GRP_CMDT_CD
            ,   NC.BKG_ORG_TRSP_MOD_CD
            ,   NC.BKG_DEST_TRSP_MOD_CD
            ,   NC.BKG_RCV_TERM_CD
            ,   NC.BKG_DE_TERM_CD
            ,   NC.BKG_SLAN_CD
            ,   NC.BKG_DIR_CALL_FLG
            ,   NC.BKG_TS_PORT_DEF_CD
            ,   NC.BKG_IO_GA_CD
            ,   NC.BKG_CNL_TZ_CD
            ,   NC.BKG_VSL_CD||NC.BKG_SKD_VOY_NO||NC.BKG_SKD_DIR_CD BKG_VVD_CD
            ,   NC.BKG_ACT_CUST_CNT_CD||LPAD(NC.BKG_ACT_CUST_SEQ,6,0) BKG_ACT_CUST_DEF_CD
            ,   NC.BKG_SOC_FLG
            ,   NC.BKG_USA_SVC_MOD_CD
            ,   NC.BKG_ESVC_TP_CD
            ,   NC.BKG_MST_HBL_TP_CD
            ,   NC.PAY_TERM_CD
            ,   NC.RULE_APPL_CHG_TP_CD
            ,   NC.RULE_APPL_CHG_CD
            ,   NC.IGN_TRF_FLG
            ,   NC.RT_PATT_TP_CD
            ,   NC.CONV_RAT_UT_CD
            ,   NC.CONV_PRC_CGO_TP_CD
            ,   NC.CONV_CMDT_DEF_CD
            ,   NC.CONV_ORG_LOC_DEF_CD
            ,   NC.CONV_ORG_VIA_LOC_DEF_CD
            ,   NC.CONV_DEST_VIA_LOC_DEF_CD
            ,   NC.CONV_DEST_LOC_DEF_CD
            ,   NC.CONV_PRC_RCV_TERM_CD
            ,   NC.CONV_PRC_DE_TERM_CD
            ,   TO_CHAR(NC.UPD_DT, 'YYYY-MM-DD') UPD_DT
            ,   C.USR_NM
        FROM    SC_LIST M
            ,   PRI_SC_NOTE_CONV NC
            ,   PRI_SP_SCP_RT_CMDT_RNOTE RN
            ,   PRI_SP_SCP_RT_CMDT_HDR CH
            ,   COM_USER C
        WHERE   1 = 1
        AND     NC.PROP_NO = M.PROP_NO
        AND     NC.AMDT_SEQ = M.AMDT_SEQ
        AND     NC.NOTE_CONV_MAPG_ID = M.NOTE_CONV_MAPG_ID
        AND     NC.NOTE_CONV_TP_CD = M.NOTE_CONV_TP_CD 
        AND     NC.NOTE_CONV_TP_CD = 'R' 
        AND     NC.NOTE_CONV_MAPG_ID = RN.NOTE_CONV_MAPG_ID
        AND     RN.PROP_NO = CH.PROP_NO
        AND     RN.AMDT_SEQ = CH.AMDT_SEQ
        AND     RN.SVC_SCP_CD = CH.SVC_SCP_CD
        AND     RN.GEN_SPCL_RT_TP_CD = CH.GEN_SPCL_RT_TP_CD
        AND     RN.CMDT_HDR_SEQ = CH.CMDT_HDR_SEQ
        AND     RN.SRC_INFO_CD <> 'AD'
        AND     NC.UPD_USR_ID = C.USR_ID
        AND     RN.SVC_SCP_CD = NVL(@[svc_scp_cd], RN.SVC_SCP_CD)
        AND     NC.NOTE_CONV_TP_CD = NVL(@[note_conv_tp_cd], NC.NOTE_CONV_TP_CD)

        UNION ALL

        SELECT  'Special' NOTE_CONV_TP_CD
            ,   M.SC_NO AS CTRT_NO
            ,   'S' AS BKG_CTRT_TP_CD
            ,   M.PROP_NO
            ,   M.AMDT_SEQ
            ,   M.CTRT_PTY_NM
            ,   SD.SVC_SCP_CD
            ,   '' CTRT_PTY_NM
            ,   SH.DP_SEQ||'.'||SD.DP_SEQ  DP_SEQ
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01711' AND C.INTG_CD_VAL_CTNT = SH.NOTE_CLSS_CD ) NOTE_CLSS_CD
            ,   SD.CHG_CD
            ,   SD.NOTE_CTNT
            ,   NC.NOTE_CONV_SEQ
            ,   CASE WHEN NC.CHG_RULE_TP_CD = 'C' THEN NC.NOTE_CONV_CHG_CD
                     WHEN NC.CHG_RULE_TP_CD = 'R' THEN NC.NOTE_CONV_RULE_CD
                END CHG_RULE_DEF_CD
            ,   TO_CHAR(NC.EFF_DT, 'YYYY-MM-DD') EFF_DT
            ,   TO_CHAR(NC.EXP_DT, 'YYYY-MM-DD') EXP_DT
            ,   ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01723' AND C.INTG_CD_VAL_CTNT = NC.RT_APPL_TP_CD )  RT_APPL_TP_CD
            ,   NC.CURR_CD
            ,   NC.RT_OP_CD
            ,   NC.FRT_RT_AMT
            ,   NC.BKG_RAT_UT_CD
            ,   NC.BKG_PRC_CGO_TP_CD
            ,   NC.BKG_IMDG_CLSS_CD
            ,   NC.BKG_POR_DEF_CD
            ,   NC.BKG_POL_DEF_CD
            ,   NC.BKG_POD_DEF_CD
            ,   NC.BKG_DEL_DEF_CD
            ,   NC.BKG_CMDT_DEF_CD
            ,   NC.BKG_SCG_GRP_CMDT_CD
            ,   NC.BKG_ORG_TRSP_MOD_CD
            ,   NC.BKG_DEST_TRSP_MOD_CD
            ,   NC.BKG_RCV_TERM_CD
            ,   NC.BKG_DE_TERM_CD
            ,   NC.BKG_SLAN_CD
            ,   NC.BKG_DIR_CALL_FLG
            ,   NC.BKG_TS_PORT_DEF_CD
            ,   NC.BKG_IO_GA_CD
            ,   NC.BKG_CNL_TZ_CD
            ,   NC.BKG_VSL_CD||NC.BKG_SKD_VOY_NO||NC.BKG_SKD_DIR_CD BKG_VVD_CD
            ,   NC.BKG_ACT_CUST_CNT_CD||LPAD(NC.BKG_ACT_CUST_SEQ,6,0) BKG_ACT_CUST_DEF_CD
            ,   NC.BKG_SOC_FLG
            ,   NC.BKG_USA_SVC_MOD_CD
            ,   NC.BKG_ESVC_TP_CD
            ,   NC.BKG_MST_HBL_TP_CD
            ,   NC.PAY_TERM_CD
            ,   NC.RULE_APPL_CHG_TP_CD
            ,   NC.RULE_APPL_CHG_CD
            ,   NC.IGN_TRF_FLG
            ,   NC.RT_PATT_TP_CD
            ,   NC.CONV_RAT_UT_CD
            ,   NC.CONV_PRC_CGO_TP_CD
            ,   NC.CONV_CMDT_DEF_CD
            ,   NC.CONV_ORG_LOC_DEF_CD
            ,   NC.CONV_ORG_VIA_LOC_DEF_CD
            ,   NC.CONV_DEST_VIA_LOC_DEF_CD
            ,   NC.CONV_DEST_LOC_DEF_CD
            ,   NC.CONV_PRC_RCV_TERM_CD
            ,   NC.CONV_PRC_DE_TERM_CD
            ,   TO_CHAR(NC.UPD_DT, 'YYYY-MM-DD') UPD_DT
            ,   C.USR_NM
        FROM    SC_LIST M
            ,   PRI_SC_NOTE_CONV NC
            ,   PRI_SP_SCP_NOTE_CTNT SD
            ,   PRI_SP_SCP_NOTE SH
            ,   COM_USER C
        WHERE   1 = 1
        AND     NC.PROP_NO = M.PROP_NO
        AND     NC.AMDT_SEQ = M.AMDT_SEQ
        AND     NC.NOTE_CONV_MAPG_ID = M.NOTE_CONV_MAPG_ID
        AND     NC.NOTE_CONV_TP_CD = M.NOTE_CONV_TP_CD 
        AND     NC.NOTE_CONV_TP_CD = 'P' 
        AND     NC.NOTE_CONV_MAPG_ID = SD.NOTE_CONV_MAPG_ID
        AND     SD.PROP_NO = SH.PROP_NO
        AND     SD.AMDT_SEQ = SH.AMDT_SEQ
        AND     SD.SVC_SCP_CD = SH.SVC_SCP_CD
        AND     SD.NOTE_TP_CD = SH.NOTE_TP_CD
        AND     SD.NOTE_SEQ = SH.NOTE_SEQ
        AND     SD.SRC_INFO_CD <> 'AD'
        AND     NC.UPD_USR_ID = C.USR_ID
        AND     SD.SVC_SCP_CD = NVL(@[svc_scp_cd], SD.SVC_SCP_CD)
        AND     NC.NOTE_CONV_TP_CD = NVL(@[note_conv_tp_cd], NC.NOTE_CONV_TP_CD)
     )
ORDER BY CTRT_NO, PROP_NO, AMDT_SEQ, CTRT_PTY_NM, SVC_SCP_CD, GEN_SPCL_RT_TP_CD, NOTE_CONV_TP_CD, DP_SEQ
#else
  SELECT  SC_NO AS CTRT_NO
        , 'S' AS BKG_CTRT_TP_CD
        , AMDT_SEQ
        , CTRT_PTY_NM
        , SVC_SCP_CD
        , DP_SEQ
        , RT_TP
        , NOTE_CONV_TP_CD
        , NOTE_CTNT
        , TO_CHAR(EFF_DT, 'YYYY-MM-DD') AS EFF_DT
        , TO_CHAR(EXP_DT, 'YYYY-MM-DD') AS EXP_DT
        , TO_CHAR(UPD_DT, 'YYYY-MM-DD') AS UPD_DT
        , NOTE_CLSS_CD
        , CHG_CD
        , (SELECT USR_NM 
             FROM COM_USER 
            WHERE USR_ID = SC.UPD_USR_ID 
               AND USE_FLG = 'Y' ) AS USR_NM
        ,  COUNT(DISTINCT SC_NO) OVER (PARTITION BY 1) CTRT_CNT
  FROM (
        SELECT  H.SC_NO
              , M.AMDT_SEQ
              , P.CTRT_PTY_NM
              , S.SVC_SCP_CD
              , NT.DP_SEQ||'.'||CT.DP_SEQ  DP_SEQ
              ,'' AS RT_TP
              , 'Special' AS NOTE_CONV_TP_CD
              , ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01711' AND C.INTG_CD_VAL_CTNT = NT.NOTE_CLSS_CD ) NOTE_CLSS_CD
              , CT.NOTE_CTNT
              , CT.CHG_CD
              ,   (SELECT EFF_DT
                     FROM PRI_SP_SCP_MN 
                    WHERE PROP_NO = CT.PROP_NO 
                      AND AMDT_SEQ = CT.N1ST_CMNC_AMDT_SEQ 
                      AND SVC_SCP_CD = CT.SVC_SCP_CD) EFF_DT
             , S.EXP_DT
             , CT.UPD_DT
             , CT.UPD_USR_ID
        FROM    PRI_SP_HDR H
            ,   PRI_SP_MN M
            ,   PRI_SP_SCP_MN S
            ,   PRI_SP_SCP_NOTE_CTNT CT
            ,   PRI_SP_SCP_NOTE NT     
            ,   PRI_SP_CTRT_PTY P
        WHERE   M.PROP_NO = H.PROP_NO
        AND     M.AMDT_SEQ = (  SELECT  MAX ( X.AMDT_SEQ ) 
                                  FROM  PRI_SP_MN X 
                                 WHERE  X.PROP_NO = M.PROP_NO 
                                   AND  SYSDATE BETWEEN X.EFF_DT AND X.EXP_DT
                                   AND  X.PROP_STS_CD = 'F'  ) 
        AND     M.PROP_NO = P.PROP_NO
        AND     M.AMDT_SEQ = P.AMDT_SEQ
        AND     P.PRC_CTRT_PTY_TP_CD = 'C'     
        AND     M.PROP_NO = S.PROP_NO
        AND     M.AMDT_SEQ = S.AMDT_SEQ
        AND     S.SVC_SCP_CD = @[svc_scp_cd]
        AND     S.PROP_NO = CT.PROP_NO
        AND     S.AMDT_SEQ = CT.AMDT_SEQ
        AND     S.SVC_SCP_CD = CT.SVC_SCP_CD
        AND     CT.NOTE_TP_CD = 'P'
        AND     CT.PROP_NO = NT.PROP_NO
        AND     CT.AMDT_SEQ = NT.AMDT_SEQ
        AND     CT.SVC_SCP_CD = NT.SVC_SCP_CD
        AND     CT.NOTE_TP_CD = NT.NOTE_TP_CD
        AND     CT.NOTE_SEQ = NT.NOTE_SEQ
        AND     CT.SRC_INFO_CD <> 'AD'
        AND     H.CRE_DT >= TO_DATE('20170101', 'YYYYMMDD')       -- Ignore Hanjin Shipping Contract
        AND     M.EFF_DT <= TO_DATE(@[exp_dt], 'YYYY-MM-DD')      -- RFA MAIN에 대한 EFFECTIVE DATE (TO)
        AND     M.EXP_DT >= TO_DATE(@[eff_dt], 'YYYY-MM-DD')      -- RFA MAIN에 대한 EFFECTIVE DATE (FROM)
        AND     NOT EXISTS (SELECT 'Y'
                              FROM PRI_SC_NOTE_CONV
                             WHERE NOTE_CONV_MAPG_ID = CT.NOTE_CONV_MAPG_ID)
    #if (${ctrt_no} != '')
        AND     H.SC_NO = @[ctrt_no]
    #end
    #if (${note_conv_tp_cd} != '')
        AND     'P' = @[note_conv_tp_cd]
    #end



     UNION ALL
        SELECT  H.SC_NO
              , M.AMDT_SEQ
              , P.CTRT_PTY_NM
              , S.SVC_SCP_CD
              , TO_CHAR(NT.BLET_DP_SEQ)  DP_SEQ
              , ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01705' AND C.INTG_CD_VAL_CTNT = CT.GEN_SPCL_RT_TP_CD ) AS RT_TP
              , 'Commodity' AS NOTE_CONV_TP_CD
              , ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01711' AND C.INTG_CD_VAL_CTNT = CT.NOTE_CLSS_CD ) NOTE_CLSS_CD
              , CT.NOTE_CTNT
              , CT.CHG_CD
              ,   (SELECT EFF_DT
                     FROM PRI_SP_SCP_MN 
                    WHERE PROP_NO = CT.PROP_NO 
                      AND AMDT_SEQ = CT.N1ST_CMNC_AMDT_SEQ 
                      AND SVC_SCP_CD = CT.SVC_SCP_CD) EFF_DT
             , S.EXP_DT
             , CT.UPD_DT
             , CT.UPD_USR_ID
        FROM    PRI_SP_HDR H
            ,   PRI_SP_MN M
            ,   PRI_SP_SCP_MN S
            ,   PRI_SP_SCP_RT_CNOTE CT
            ,   PRI_SP_SCP_RT_CMDT_HDR NT
            ,   PRI_SP_CTRT_PTY P
        WHERE   M.PROP_NO = H.PROP_NO
        AND     M.AMDT_SEQ = (  SELECT  MAX ( X.AMDT_SEQ ) 
                                  FROM  PRI_SP_MN X 
                                 WHERE  X.PROP_NO = M.PROP_NO 
                                   AND  SYSDATE BETWEEN X.EFF_DT AND X.EXP_DT
                                   AND  X.PROP_STS_CD = 'F'  ) 
        AND     M.PROP_NO = P.PROP_NO
        AND     M.AMDT_SEQ = P.AMDT_SEQ
        AND     P.PRC_CTRT_PTY_TP_CD = 'C'      
        AND     M.PROP_NO = S.PROP_NO
        AND     M.AMDT_SEQ = S.AMDT_SEQ
        AND     S.SVC_SCP_CD = @[svc_scp_cd]
        AND     S.PROP_NO = CT.PROP_NO
        AND     S.AMDT_SEQ = CT.AMDT_SEQ
        AND     S.SVC_SCP_CD = CT.SVC_SCP_CD
        AND     CT.PROP_NO = NT.PROP_NO
        AND     CT.AMDT_SEQ = NT.AMDT_SEQ
        AND     CT.SVC_SCP_CD = NT.SVC_SCP_CD
        AND     CT.CMDT_HDR_SEQ = NT.CMDT_HDR_SEQ
        AND     CT.SRC_INFO_CD <> 'AD'
        AND     H.CRE_DT >= TO_DATE('20170101', 'YYYYMMDD')       -- Ignore Hanjin Shipping Contract
        AND     M.EFF_DT <= TO_DATE(@[exp_dt], 'YYYY-MM-DD')      -- RFA MAIN에 대한 EFFECTIVE DATE (TO)
        AND     M.EXP_DT >= TO_DATE(@[eff_dt], 'YYYY-MM-DD')      -- RFA MAIN에 대한 EFFECTIVE DATE (FROM)
        AND     NOT EXISTS (SELECT 'Y'
                              FROM PRI_SC_NOTE_CONV
                             WHERE NOTE_CONV_MAPG_ID = CT.NOTE_CONV_MAPG_ID)
    #if (${ctrt_no} != '')
        AND     H.SC_NO = @[ctrt_no]
    #end
    #if (${note_conv_tp_cd} != '')
        AND     'C' = @[note_conv_tp_cd]
    #end
     UNION ALL
        SELECT  H.SC_NO
              , M.AMDT_SEQ
              , P.CTRT_PTY_NM
              , S.SVC_SCP_CD
              , NT.BLET_DP_SEQ||'.'||CT.ROUT_SEQ DP_SEQ
              , ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01705' AND C.INTG_CD_VAL_CTNT = CT.GEN_SPCL_RT_TP_CD ) AS RT_TP
              , 'Route' AS NOTE_CONV_TP_CD
              , ( SELECT C.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL C WHERE C.INTG_CD_ID = 'CD01711' AND C.INTG_CD_VAL_CTNT = CT.NOTE_CLSS_CD ) NOTE_CLSS_CD
              , CT.NOTE_CTNT
              , CT.CHG_CD
              ,   (SELECT EFF_DT
                     FROM PRI_SP_SCP_MN 
                    WHERE PROP_NO = CT.PROP_NO 
                      AND AMDT_SEQ = CT.N1ST_CMNC_AMDT_SEQ 
                      AND SVC_SCP_CD = CT.SVC_SCP_CD) EFF_DT
             , S.EXP_DT
             , CT.UPD_DT
             , CT.UPD_USR_ID
        FROM    PRI_SP_HDR H
            ,   PRI_SP_MN M
            ,   PRI_SP_SCP_MN S
            ,   PRI_SP_SCP_RT_CMDT_RNOTE CT
            ,   PRI_SP_SCP_RT_CMDT_HDR NT
            ,   PRI_SP_CTRT_PTY P
        WHERE   M.PROP_NO = H.PROP_NO
        AND     M.AMDT_SEQ = (  SELECT  MAX ( X.AMDT_SEQ ) 
                                  FROM  PRI_SP_MN X 
                                 WHERE  X.PROP_NO = M.PROP_NO 
                                   AND  SYSDATE BETWEEN X.EFF_DT AND X.EXP_DT
                                   AND  X.PROP_STS_CD = 'F'  ) 
        AND     M.PROP_NO = P.PROP_NO
        AND     M.AMDT_SEQ = P.AMDT_SEQ
        AND     P.PRC_CTRT_PTY_TP_CD = 'C'     
        AND     M.PROP_NO = S.PROP_NO
        AND     M.AMDT_SEQ = S.AMDT_SEQ
        AND     S.SVC_SCP_CD = @[svc_scp_cd]
        AND     S.PROP_NO = CT.PROP_NO
        AND     S.AMDT_SEQ = CT.AMDT_SEQ
        AND     S.SVC_SCP_CD = CT.SVC_SCP_CD
        AND     CT.PROP_NO = NT.PROP_NO
        AND     CT.AMDT_SEQ = NT.AMDT_SEQ
        AND     CT.SVC_SCP_CD = NT.SVC_SCP_CD
        AND     CT.CMDT_HDR_SEQ = NT.CMDT_HDR_SEQ
        AND     CT.SRC_INFO_CD <> 'AD'
        AND     H.CRE_DT >= TO_DATE('20170101', 'YYYYMMDD')       -- Ignore Hanjin Shipping Contract
        AND     M.EFF_DT <= TO_DATE(@[exp_dt], 'YYYY-MM-DD')      -- RFA MAIN에 대한 EFFECTIVE DATE (TO)
        AND     M.EXP_DT >= TO_DATE(@[eff_dt], 'YYYY-MM-DD')      -- RFA MAIN에 대한 EFFECTIVE DATE (FROM)
        AND     NOT EXISTS (SELECT 'Y'
                              FROM PRI_SC_NOTE_CONV
                             WHERE NOTE_CONV_MAPG_ID = CT.NOTE_CONV_MAPG_ID)
    #if (${ctrt_no} != '')
        AND     H.SC_NO = @[ctrt_no]
    #end
    #if (${note_conv_tp_cd} != '')
        AND     'R' = @[note_conv_tp_cd]
    #end
                         
    ) SC
WHERE EFF_DT <= TO_DATE(@[exp_dt], 'YYYY-MM-DD')      -- NOTE DESC에 대한 EFFECTIVE DATE (TO)
  AND EXP_DT >= TO_DATE(@[eff_dt], 'YYYY-MM-DD')      -- NOTE DESC에 대한 EFFECTIVE DATE (FROM)
ORDER BY SC_NO, AMDT_SEQ, CTRT_PTY_NM, SVC_SCP_CD, RT_TP, NOTE_CONV_TP_CD, DP_SEQ
#end			]]></sql>
			<params>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="note_conv_rule_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="note_conv_tp_cd" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
