<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SriLankaManifestListDownloadDBDAOsearchVesselVpsRSQL">
			<desc><![CDATA[Vessel Port Schedula]]></desc>
			<sql><![CDATA[
SELECT 	TO_CHAR(VPS_ETA_DT,'YYYY-MM-DD') VPS_ETA_DT, 
		TO_CHAR(VPS_ETA_DT,'HH24:MI:SS') VPS_ETA_DT_TIME, 
#if ( ${io_bnd_cd} == 'I') 
   	   @[port_cd] arrival_port_cd,
#else
		@[port_cd] depature_port,
#end 
		'SM LINE' shipping_agent, 
		'SM LINE' local_agent
FROM   VSK_VSL_PORT_SKD
WHERE  VSL_CD        =  @[vsl_cd]
AND    SKD_VOY_NO    = @[skd_voy_no]
AND    SKD_DIR_CD    = @[skd_dir_cd]
AND    VPS_PORT_CD    = @[port_cd]
AND    NVL(SKD_CNG_STS_CD,' ') <> 'S'
AND    CLPT_SEQ  = (SELECT  MAX(CLPT_SEQ)
			FROM    VSK_VSL_PORT_SKD
			WHERE   VSL_CD        =  @[vsl_cd]
			AND     SKD_VOY_NO    =  @[skd_voy_no]
			AND     SKD_DIR_CD    = @[skd_dir_cd]
			AND     VPS_PORT_CD    = @[port_cd]
			AND     NVL(SKD_CNG_STS_CD,' ') <> 'S')			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
