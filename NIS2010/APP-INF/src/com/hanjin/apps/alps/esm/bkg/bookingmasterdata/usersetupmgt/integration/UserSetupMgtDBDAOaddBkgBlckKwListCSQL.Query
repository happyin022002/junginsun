<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOaddBkgBlckKwListCSQL">
			<desc><![CDATA[bkg_blck_kw_list 테이블에 데이터 insert]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_BLCK_KW_LIST (
	BLCK_KW_TP_CD
,	BLCK_KW_TP_SEQ
,	BLCK_KW_NM
,	BLCK_KW_CTNT
,	BLCK_KW_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
    @[blck_kw_tp_cd]
,	NVL((select max(BLCK_KW_TP_SEQ) from BKG_BLCK_KW_LIST WHERE BLCK_KW_TP_CD = @[blck_kw_tp_cd] ),0)+1
,	@[blck_kw_nm]
,	@[blck_kw_ctnt]
,	@[blck_kw_rmk]
,	@[cre_usr_id]
,	sysdate
,	@[upd_usr_id]
,	sysdate
)			]]></sql>
			<params>
				<param name="blck_kw_tp_cd" type="12" value="" out="N"/>
				<param name="blck_kw_nm" type="12" value="" out="N"/>
				<param name="blck_kw_ctnt" type="12" value="" out="N"/>
				<param name="blck_kw_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
