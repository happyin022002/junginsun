<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOaddCllMainForDownloadCSQL">
			<desc><![CDATA[addCllMainForDownload]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CSTMS_TML_CLL
				(VSL_CD,
				SKD_VOY_NO,
				SKD_DIR_CD,
				PORT_CD,
				ETA_DT,
				ETD_DT,
				BKG_NO,
				CNTR_NO,
				BL_NO,
				BL_TP_CD,
				CRE_DT,
				UPD_DT,
				SND_DT,
				POR_CD,
				POL_CD,
				POD_CD,
				DEL_CD,
				SHPR_CNT_CD,
				SHPR_SEQ,
				SHPR_NM1,
				SHPR_NM2,
				SHPR_NM3,
				SHPR_NM4,
				SHPR_NM5,
				CNEE_CNT_CD,
				CNEE_SEQ,
				CNEE_NM1,
				CNEE_NM2,
				CNEE_NM3,
				CNEE_NM4,
				CNEE_NM5,
				NTFY_CNT_CD,
				NTFY_SEQ,
				NTFY_NM1,
				NTFY_NM2,
				NTFY_NM3,
				NTFY_NM4,
				NTFY_NM5,
				ANTFY_CNT_CD,
				ANTFY_SEQ,
				ANTFY_NM1,
				ANTFY_NM2,
				ANTFY_NM3,
				ANTFY_NM4,
				ANTFY_NM5,
				FF_CNT_CD,
				FF_CUST_SEQ,
				FF_NM1,
				FF_NM2,
				FF_NM3,
				FF_NM4,
				FF_NM5,
				CNTR_TPSZ_CD,
				SOC_FLG,
				FULL_MTY_CD,
				RCV_TERM_CD,
				DE_TERM_CD,
				PCK_TP_CD,
				PCK_QTY,
				WGT_TP_CD,
				CNTR_WGT,
				GRS_WGT_UT_CD,
				GRS_CNTR_WGT,
				CNTR_MEAS_UT_CD,
				MEAS_QTY,
				OVR_FWRD_LEN,
				OVR_BKWD_LEN,
				OVR_HGT,
				OVR_PORT_LEN,
				OVR_SD_LEN,
				OVR_WGT_UT_CD,
				OVR_CNTR_WGT,
				FDO_TEMP,
				CDO_TEMP,
				CNTR_VENT_RTO,
				CNTR_SEAL_NO,
				TS_CGO_CD,
				TARE_CNTR_WGT,
				TS_VVD_ID,
				TS_POL_CD,
				TS_POD_CD,
				CMDT_CD,
				CMDT_DESC,
				REP_CMDT_CD,
				REP_CMDT_DESC,
				CNTR_LODG_NO,
				TRNK_VVD_ID,
				CFM_FLG,
				TEU_CNTR_QTY,
				FEU_CNTR_QTY,
				RC_FLG,
				DCGO_FLG,
				AWK_CGO_FLG,
				BB_CGO_FLG,
				RD_CGO_FLG,
				LIST_RMK,
				BL_RMK,
				CRE_USR_ID,
				UPD_USR_ID,
				RC_SEQ,
				AWK_CGO_SEQ,
				VGM_WGT
				#if (${pol_split_no} != '')
        		,CLPT_IND_SEQ 
				#end
				,VGM_VRFY_SIG_CTNT
                ,VGM_MZD_TP_CD
				)
			SELECT	V.VSL_CD,
					V.SKD_VOY_NO,
					V.SKD_DIR_CD,
					V.POL_CD,
					NVL(TO_DATE(@[vps_eta_dt],'YYYYMMDDHH24MI'),NULL),
					NVL(TO_DATE(@[vps_etd_dt],'YYYYMMDDHH24MI'),NULL),
					B.BKG_NO,
					NVL(C.CNTR_NO,@[tbx_seq]),
					NVL(B.BL_NO,' '),NVL(B.BL_TP_CD,' '),
					SYSDATE,
					SYSDATE,
					NULL,
					NVL(B.POR_CD,' '),
					NVL(B.POL_CD,' '),
					NVL(B.POD_CD,' '),
					NVL(B.DEL_CD,' '),
					NVL(BCS.CUST_CNT_CD,' '),
					BCS.CUST_SEQ,
					SCE_TOKEN_NL_FNC(BCS.CUST_NM,1),
					SCE_TOKEN_NL_FNC(BCS.CUST_NM,2),
					SCE_TOKEN_NL_FNC(BCS.CUST_ADDR,1),
					SCE_TOKEN_NL_FNC(BCS.CUST_ADDR,2),
					SCE_TOKEN_NL_FNC(BCS.CUST_ADDR,3),
					NVL(BCC.CUST_CNT_CD,' '),
					BCC.CUST_SEQ,
					SCE_TOKEN_NL_FNC(BCC.CUST_NM,1),
					SCE_TOKEN_NL_FNC(BCC.CUST_NM,2),
					SCE_TOKEN_NL_FNC(BCC.CUST_ADDR,1),
					SCE_TOKEN_NL_FNC(BCC.CUST_ADDR,2),
					SCE_TOKEN_NL_FNC(BCC.CUST_ADDR,3),
					NVL(BCN.CUST_CNT_CD,' '),
					BCN.CUST_SEQ,
					SCE_TOKEN_NL_FNC(BCN.CUST_NM,1),
					SCE_TOKEN_NL_FNC(BCN.CUST_NM,2),
					SCE_TOKEN_NL_FNC(BCN.CUST_ADDR,1),
					SCE_TOKEN_NL_FNC(BCN.CUST_ADDR,2),
					SCE_TOKEN_NL_FNC(BCN.CUST_ADDR,3),
					NVL(BCA.CUST_CNT_CD,' '),
					BCA.CUST_SEQ,
					SCE_TOKEN_NL_FNC(BCA.CUST_NM,1),
					SCE_TOKEN_NL_FNC(BCA.CUST_NM,2),
					SCE_TOKEN_NL_FNC(BCA.CUST_NM,3),
					SCE_TOKEN_NL_FNC(BCA.CUST_NM,4),
					SCE_TOKEN_NL_FNC(BCA.CUST_NM,5),
					NVL(BCF.CUST_CNT_CD,' '),
					BCF.CUST_SEQ,
					SCE_TOKEN_NL_FNC(BCF.CUST_NM,1),
					SCE_TOKEN_NL_FNC(BCF.CUST_NM,2),
					SCE_TOKEN_NL_FNC(BCF.CUST_NM,3),
					SCE_TOKEN_NL_FNC(BCF.CUST_NM,4),
					SCE_TOKEN_NL_FNC(BCF.CUST_NM,5),
					@[cntr_tpsz_cd],
					NVL(C.SOC_FLG,' '),
					DECODE(B.BKG_CGO_TP_CD,'F','F','M'),
					NVL(C.RCV_TERM_CD,' '),NVL(C.DE_TERM_CD,' '),
					NVL(C.PCK_TP_CD,' '),
					NVL(C.PCK_QTY, 0),
					DECODE(C.WGT_UT_CD, NULL, DOC.WGT_UT_CD, C.WGT_UT_CD),
					DECODE(NVL(C.CNTR_WGT,0), 0, ROUND(DOC.ACT_WGT / @[in_qty], 0), ROUND(C.CNTR_WGT,0)),
					DECODE(C.WGT_UT_CD, NULL, DOC.WGT_UT_CD, C.WGT_UT_CD),
					
					ROUND(
					(DECODE(NVL(C.CNTR_WGT,0),0,ROUND(nvl(DOC.ACT_WGT,0) * decode(substr(NVL(C.CNTR_TPSZ_CD,@[cntr_tpsz_cd]),2,1),'2',1,2) / NVL((SELECT SUM(DECODE(SUBSTR(CNTR_TPSZ_CD,2,1),'2',1,2)) FROM BKG_CONTAINER BC WHERE BC.BKG_NO = C.BKG_NO),1)),C.CNTR_WGT)
					+ DECODE(NVL(DECODE(NVL(KKK.SPEC_TARE_WGT, 0), 0, DECODE(NVL(KKK.MDM_CNTR_TPSZ_TARE_WGT, 0), 0, DECODE(KKK.O_CNTR_TPSZ_CD, 'T2', 3600, 'T4', 5200, 0), KKK.MDM_CNTR_TPSZ_TARE_WGT), KKK.SPEC_TARE_WGT  ),0),0,(SELECT CNTR_TPSZ_TARE_WGT FROM MDM_CNTR_TP_SZ WHERE CNTR_TPSZ_CD = @[cntr_tpsz_cd]), NVL(DECODE(NVL(KKK.SPEC_TARE_WGT, 0), 0, DECODE(NVL(KKK.MDM_CNTR_TPSZ_TARE_WGT, 0), 0, DECODE(KKK.O_CNTR_TPSZ_CD, 'T2', 3600, 'T4', 5200, 0), KKK.MDM_CNTR_TPSZ_TARE_WGT), KKK.SPEC_TARE_WGT  ),0))) * DECODE(NVL(C.WGT_UT_CD, ' '), 'LBS',0.4536,1),2),

					DECODE(C.MEAS_UT_CD, NULL, DOC.MEAS_UT_CD, C.MEAS_UT_CD),
					DECODE(C.MEAS_QTY,NULL,ROUND(DOC.MEAS_QTY / @[in_qty], 0),C.MEAS_QTY),
					NVL(A.OVR_FWRD_LEN,NULL),
					NVL(A.OVR_BKWD_LEN,NULL),
					NVL(A.OVR_HGT,NULL),
					NVL(A.OVR_LF_LEN,NULL),
					NVL(A.OVR_RT_LEN,NULL),
					NVL(A.WGT_UT_CD,NULL),
					NVL(A.GRS_WGT,0),
					NVL(R.FDO_TEMP,NULL),
					NVL(R.CDO_TEMP,NULL),
					NVL(R.VENT_RTO,NULL),
					NVL(( SELECT CNTR_SEAL_NO --NVL(MIN(CNTR_SEAL_NO),' ') 
					   FROM BKG_CNTR_SEAL_NO
					   WHERE BKG_NO = C.BKG_NO
					   AND CNTR_NO = C.CNTR_NO 
                       AND CNTR_SEAL_SEQ = 1),' ') AS CNTR_SEAL_NO,
					DECODE(B.POL_CD,V.POL_CD,'L','T'),

					DECODE(NVL(DECODE(NVL(KKK.SPEC_TARE_WGT, 0), 0, DECODE(NVL(KKK.MDM_CNTR_TPSZ_TARE_WGT, 0), 0, DECODE(KKK.O_CNTR_TPSZ_CD, 'T2', 3600, 'T4', 5200, 0), KKK.MDM_CNTR_TPSZ_TARE_WGT), KKK.SPEC_TARE_WGT  ),0),0,(SELECT CNTR_TPSZ_TARE_WGT FROM MDM_CNTR_TP_SZ WHERE CNTR_TPSZ_CD = @[cntr_tpsz_cd]), NVL(DECODE(NVL(KKK.SPEC_TARE_WGT, 0), 0, DECODE(NVL(KKK.MDM_CNTR_TPSZ_TARE_WGT, 0), 0, DECODE(KKK.O_CNTR_TPSZ_CD, 'T2', 3600, 'T4', 5200, 0), KKK.MDM_CNTR_TPSZ_TARE_WGT), KKK.SPEC_TARE_WGT  ),0)),

					T.VSL_CD||T.SKD_VOY_NO||T.SKD_DIR_CD,
					T.POL_CD,
					T.POD_CD,
					O.CMDT_CD,
					O.CMDT_NM,
					P.REP_CMDT_CD,
					P.REP_CMDT_NM,
					@[tbn_seq],
					B.VSL_CD||B.SKD_VOY_NO||B.SKD_DIR_CD,
					DECODE(C.CNTR_CFM_FLG,NULL,'N','Y','Y',C.MF_CFM_FLG),
					NVL(@[in_teu],0),
					NVL(@[in_feu],0),
					NVL(C.RC_FLG,'N'),
					NVL(C.DCGO_FLG,'N'),
					NVL(C.AWK_CGO_FLG,'N'),
					NVL(C.BB_CGO_FLG,'N'),
					NVL(C.RD_CGO_FLG,'N'),
					substr(Translate(NVL(R.DIFF_RMK,' '),chr(13)||chr(10),' '),1,50),
        			substr(Translate(NVL(B.XTER_RMK,' '),chr(13)||chr(10),' '),1,200),
					@[in_usr_id],
					@[in_usr_id],
					R.RC_SEQ,
					A.AWK_CGO_SEQ,
					DECODE(NVL(C.VGM_WGT_UT_CD, 0), 'LBS', ROUND (NVL(C.VGM_WGT, 0)*0.4536, 3), NVL(C.VGM_WGT, 0)) VGM_WGT
					#if (${pol_split_no} != '')
					, V.POL_CLPT_IND_SEQ -- Add. 2015.02.09. CHM-201533845
					#end
					,C.VGM_VRFY_SIG_CTNT
                	,C.VGM_MZD_TP_CD
			 FROM	BKG_BOOKING B,
					BKG_BL_DOC DOC,
					MDM_COMMODITY O,
					MDM_REP_CMDT P,
					BKG_VVD V,
					BKG_VVD T,
					BKG_CONTAINER C,
					BKG_RF_CGO R,
					BKG_AWK_CGO A,
					BKG_CUSTOMER BCS,
					BKG_CUSTOMER BCC,
					BKG_CUSTOMER BCN,
					BKG_CUSTOMER BCA,
					BKG_CUSTOMER BCF,
					( SELECT NVL(SPEC.TARE_WGT,0) SPEC_TARE_WGT, NVL(MDM.CNTR_TPSZ_TARE_WGT,0) MDM_CNTR_TPSZ_TARE_WGT,
					         NVL(O.CNTR_TPSZ_CD,'') O_CNTR_TPSZ_CD, O.CNTR_NO CNTR_NO, @[bkg_no] BKG_NO
					FROM 
        			MST_CONTAINER    O,
        			MST_CNTR_SPEC    SPEC,
        			MDM_CNTR_TP_SZ   MDM
					  WHERE 1=1
            			 AND    O.CNTR_NO          = @[cntr_no]
            			 AND    O.CNTR_SPEC_NO     = SPEC.CNTR_SPEC_NO(+)
            			 AND    O.CNTR_TPSZ_CD     = MDM.CNTR_TPSZ_CD
            			 AND    ROWNUM = 1 ) KKK
			 WHERE	B.BKG_NO	= @[bkg_no]
			 AND	B.BKG_NO	= DOC.BKG_NO
			 AND	C.CNTR_NO(+)	= @[cntr_no]
			 AND	B.BKG_NO	= C.BKG_NO(+)
			 AND	B.BKG_NO	= V.BKG_NO(+)
			 AND	@[in_pol_cd]	= V.POL_CD(+)
			 #if (${pol_split_no} != '')
			 AND    @[pol_split_no] = V.POL_CLPT_IND_SEQ(+) -- Add. 2015.02.09. CHM-201533845
			 #end
			 AND	B.BKG_NO	= T.BKG_NO(+)
			 AND	@[in_pol_cd]	= T.POD_CD(+)
			 AND	B.CMDT_CD	= O.CMDT_CD(+)
			 AND	B.REP_CMDT_CD	= P.REP_CMDT_CD(+)
			 AND	C.BKG_NO	= R.BKG_NO(+)
			 AND	C.CNTR_NO	= R.CNTR_NO(+)
			 AND	C.BKG_NO	= A.BKG_NO(+)
			 AND	C.CNTR_NO	= A.CNTR_NO(+)
			 AND	B.BKG_NO	= BCS.BKG_NO
			 AND	BCS.BKG_CUST_TP_CD = 'S'
			 AND	B.BKG_NO	= BCC.BKG_NO
			 AND	BCC.BKG_CUST_TP_CD = 'C'
			 AND	B.BKG_NO	= BCN.BKG_NO
			 AND	BCN.BKG_CUST_TP_CD = 'N'
			 AND	B.BKG_NO	= BCA.BKG_NO(+)
			 AND	BCA.BKG_CUST_TP_CD (+) = 'A'
			 AND	B.BKG_NO	= BCF.BKG_NO(+)
			 AND	BCF.BKG_CUST_TP_CD (+) = 'F'
			 AND    KKK.CNTR_NO(+) = @[cntr_no]
			 AND    KKK.BKG_NO(+) = B.BKG_NO
			 AND	ROWNUM = 1			]]></sql>
			<params>
				<param name="vps_eta_dt" type="12" value="" out="N"/>
				<param name="vps_etd_dt" type="12" value="" out="N"/>
				<param name="tbx_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="in_qty" type="12" value="" out="N"/>
				<param name="tbn_seq" type="12" value="" out="N"/>
				<param name="in_teu" type="12" value="" out="N"/>
				<param name="in_feu" type="12" value="" out="N"/>
				<param name="in_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="pol_split_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
