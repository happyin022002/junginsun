<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOsearchRmkTemplateRSQL">
			<desc><![CDATA[searchRmkTemplate]]></desc>
			<sql><![CDATA[
SELECT
	USR_ID,
	TMPLT_TP_CD,
	TMPLT_SEQ,
	TMPLT_HDR_NM,
	TMPLT_CTNT
FROM BKG_USR_TMPLT
WHERE	USR_ID = @[usr_id]
AND TMPLT_TP_CD IN ('I','X')
			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
