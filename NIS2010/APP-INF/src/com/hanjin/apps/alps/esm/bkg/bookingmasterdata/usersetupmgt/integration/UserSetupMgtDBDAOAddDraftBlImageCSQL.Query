    SUBSTR(@[cust_code],1,2),
    SUBSTR(@[cust_code],3,6),
    @[bl_img_file_tp_cd],
    DECODE(@[rat_flg],1,'Y','N'),
    @[upd_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="cust_code" type="12" value="" out="N"/>
				<param name="bl_img_file_tp_cd" type="12" value="" out="N"/>
				<param name="rat_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
