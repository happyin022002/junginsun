			<desc><![CDATA[UserSetupMgtDBDAOAddDraftBlImageCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_CUST_BL_IMG_STUP (
    CUST_CNT_CD,
    CUST_SEQ,
    BL_IMG_FILE_TP_CD,
    RAT_FLG,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT    
)
