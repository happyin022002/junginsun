<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAOSearchEU24CountryListRSQL">
			<desc><![CDATA[Eur24ManifestDownloadDBDAOSearchEU24CountryListRSQL]]></desc>
			<sql><![CDATA[
/* Eu24CountryList VO */
SELECT CNT_CD
     ,CSTMS_DIV_ID
     ,CSTMS_DIV_ID_SEQ
     ,ATTR_CTNT1 AS CNT_CD
     ,ATTR_CTNT2
     ,ATTR_CTNT3
     ,ATTR_CTNT4
     ,ATTR_CTNT5
  FROM BKG_CSTMS_CD_CONV_CTNT
 WHERE DELT_FLG = 'N'
   AND CNT_CD = 'EU'
   AND CSTMS_DIV_ID='EU_MEMBER_CNT'

#if (${cnt_cd} != '') 
   AND ATTR_CTNT1 = @[cnt_cd]
#end
			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
