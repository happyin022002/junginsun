<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltPriMotTrfMnNewSeqRSQL">
			<desc><![CDATA[PRI_MOT_TRF_MN 의 Service Scope 별 신규 Sequence 생성]]></desc>
			<sql><![CDATA[
SELECT  NVL (   (   SELECT /*+ INDEX_DESC(A XPKPRI_MOT_TRF_MN) */
                       	    MOT_TRF_SEQ
                    FROM	PRI_MOT_TRF_MN A
                    WHERE	A.SVC_SCP_CD = @[svc_scp_cd]
                    AND		ROWNUM = 1 ), 0 ) + 1 AS NEW_MOT_TRF_SEQ
FROM    DUAL			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
