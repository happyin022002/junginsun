<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAONetworkCostExceptionListCdNmRSQL">
			<desc><![CDATA[Network Cost Exception List 의 Cost Name 을 구한다.]]></desc>
			<sql><![CDATA[
SELECT STND_COST_CD AS CODE 
      ,STND_COST_NM AS F_STND_COST_NM
FROM   MAS_STND_ACCT 
WHERE MGRP_COST_CD IN ('OV','OF','VF') -- 2015.03.18 VF 추가
AND   NVL(DELT_FLG,'N') = 'N'
AND   STND_COST_CD != '54600000' 
AND   STND_COST_CD = @[f_stnd_cost_cd]
AND   SUBSTR(STND_COST_CD,-1) != '9' -- 2015.03.18 추가
ORDER BY  ACCT_DP_SEQ			]]></sql>
			<params>
				<param name="f_stnd_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
