<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOContractDAOFmsHireUSQL">
			<desc><![CDATA[TCharterIOContractDAOFmsHireUSQL]]></desc>
			<sql><![CDATA[
update fms_hire set 
	flet_ctrt_no = @[flet_ctrt_no],
	eff_dt = TO_DATE(@[eff_dt],'YYYYMMDDHH24MI'),
	exp_dt = TO_DATE(@[exp_dt],'YYYYMMDDHH24MI'),
	hir_rt_n1st_amt = @[hir_rt_n1st_amt],
	hir_curr_n1st_cd = @[hir_curr_n1st_cd],
	hir_rt_n2nd_amt = @[hir_rt_n2nd_amt],
	hir_curr_n2nd_cd = @[hir_curr_n2nd_cd],
	upd_usr_id = @[upd_usr_id],
	upd_dt = sysdate
where flet_ctrt_no = @[flet_ctrt_no]
  and eff_dt = decode(@[ori_eff_dt],null,TO_DATE(@[eff_dt],'YYYYMMDDHH24MI'),TO_DATE(@[ori_eff_dt],'YYYYMMDDHH24MI'))
  and exp_dt = decode(@[ori_exp_dt],null,TO_DATE(@[exp_dt],'YYYYMMDDHH24MI'),TO_DATE(@[ori_exp_dt],'YYYYMMDDHH24MI'))			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="hir_rt_n1st_amt" type="12" value="" out="N"/>
				<param name="hir_curr_n1st_cd" type="12" value="" out="N"/>
				<param name="hir_rt_n2nd_amt" type="12" value="" out="N"/>
				<param name="hir_curr_n2nd_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ori_eff_dt" type="12" value="" out="N"/>
				<param name="ori_exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
