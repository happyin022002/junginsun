<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOLocalCurrCdRSQL">
			<desc><![CDATA[Local Currency 조회]]></desc>
			<sql><![CDATA[
SELECT M.AR_CURR_CD
  FROM MDM_ORGANIZATION M
 WHERE 1 = 1
   AND M.OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
