<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OPMasterDBDAOSearchHistVSLInfoListRSQL">
			<desc><![CDATA[2016.03.03 Create Lane Table, Create Vessel Table history 자동 관리]]></desc>
			<sql><![CDATA[
#if (${date_check_flag} == 'Y')
SELECT VSL_CD, VSL_APLY_FM_DT, VSL_APLY_TO_DT, NEXT_FROM
FROM
(
#end
SELECT
	IBFLAG
    ,A.VSL_SEQ                                 AS VSL_SEQ
    ,A.VSL_CD                                  AS VSL_CD
    ,A.VSL_TP_CD                               AS VSL_TP_CD
    ,A.VSL_OSHP_CD                             AS VSL_OSHP_CD
    ,A.VOP_CD                                  AS VOP_CD
    ,A.VVD_CD                                  AS VVD_CD
    ,TO_CHAR(A.VSL_APLY_FM_DT , 'YYYY-MM-DD')  AS VSL_APLY_FM_DT
    ,TO_CHAR(A.VSL_APLY_TO_DT, 'YYYY-MM-DD')   AS VSL_APLY_TO_DT
#if (${date_check_flag} == 'Y')
    ,(SELECT TO_CHAR(AA.VSL_APLY_FM_DT, 'YYYY-MM-DD')
        FROM MAS_VSL_RGST AA
        WHERE AA.VSL_CD = A.VSL_CD
            AND AA.VSL_SEQ = A.VSL_SEQ + 1) AS NEXT_FROM
#end
    ,A.VSL_CLSS_CAPA                           AS VSL_CLSS_CAPA
    ,A.PORT_CLSS_CAPA                          AS PORT_CLSS_CAPA
    ,A.VSL_DZND_CAPA                           AS VSL_DZND_CAPA
    ,A.STND_LDB_CAPA                           AS STND_LDB_CAPA
    ,A.CRR_CD
    ,A.BSA_VSL_FLG
    ,A.VSL_RGST_CNT_CD
    #foreach(${keys} IN ${keyList})
    ,NVL(SUM(DECODE(A.SUB_TRD_CD, '$keys', A.SUB_TRD_CAPA)),0) AS c$keys
    #end
    ,A.CRE_USR_ID							   AS CRE_USR_ID
	,A.TRD_CHK_FLG								-- HIDDEN
	,A.VSL_PRC									-- HIDDEN 
	,A.VSL_PRC_RTO								-- HIDDEN 
	,TO_CHAR(A.VSL_RETN_FM_DT , 'YYYY-MM-DD')	-- HIDDEN
	,TO_CHAR(A.VSL_RETN_TO_DT , 'YYYY-MM-DD')	-- HIDDEN
	--,A.BSA_VSL_FLG								-- HIDDEN 
    ,A.UPD_USR_ID                              AS UPD_USR_ID
    ,A.UPD_DT                                  AS UPD_DT
  FROM (SELECT
			 DECODE(B.VSL_CD,A.VSL_CD,'R','U') AS IBFLAG
			,A.VSL_CD
			,A.VSL_TP_CD
			,A.VSL_OSHP_CD
			,A.VOP_CD
			,A.PORT_CLSS_CAPA
			,A.STND_LDB_CAPA
			,A.VSL_CLSS_CAPA
			,A.VSL_DZND_CAPA
			,A.VSL_RETN_FM_DT
			,A.VSL_RETN_TO_DT
			,B.SUB_TRD_CD
			,B.SUB_TRD_CAPA
			,DECODE(B.CRE_USR_ID,'BATCH',B.CRE_USR_ID,'') CRE_USR_ID
            ,A.UPD_USR_ID
            ,TO_CHAR(A.UPD_DT, 'YYYY/MM/DD HH:MI') UPD_DT
			,A.VSL_SEQ
			,A.VVD_CD
			,A.VSL_APLY_FM_DT
			,A.VSL_APLY_TO_DT
			,A.CRR_CD
			,A.BSA_VSL_FLG
			,A.VSL_RGST_CNT_CD
			,A.TRD_CHK_FLG			-- HIDDEN
			,A.VSL_PRC				-- HIDDEN 
			,A.VSL_PRC_RTO			-- HIDDEN 
         FROM MAS_VSL_RGST A
            , MAS_VSL_SUB_TRD_CAPA B
        WHERE 1 = 1
          AND A.VSL_CD    = B.VSL_CD(+)
          AND A.VSL_SEQ   = B.VSL_SEQ(+)
          AND A.VSL_TP_CD = 'C'
          AND A.VSL_CD    = @[f_vsl_cd]) A
 GROUP BY
	 IBFLAG     
    ,A.VSL_SEQ                                 
    ,A.VSL_CD                                  
    ,A.VSL_TP_CD                               
    ,A.VSL_OSHP_CD                             
    ,A.VOP_CD                                  
    ,A.VVD_CD                                  
    ,TO_CHAR(A.VSL_APLY_FM_DT , 'YYYY-MM-DD')  
    ,TO_CHAR(A.VSL_APLY_TO_DT, 'YYYY-MM-DD')   
    ,A.VSL_CLSS_CAPA                           
    ,A.PORT_CLSS_CAPA                          
    ,A.VSL_DZND_CAPA                           
    ,A.STND_LDB_CAPA                           
    ,A.CRE_USR_ID
    ,A.UPD_USR_ID
    ,A.UPD_DT
    ,A.CRR_CD
    ,A.BSA_VSL_FLG
    ,A.VSL_RGST_CNT_CD
	,A.TRD_CHK_FLG			-- HIDDEN
	,A.VSL_PRC				-- HIDDEN 
	,A.VSL_PRC_RTO			-- HIDDEN 
	,A.VSL_RETN_FM_DT		-- HIDDEN 
	,A.VSL_RETN_TO_DT		-- HIDDEN
 ORDER BY A.VSL_SEQ
#if (${date_check_flag} == 'Y')
)
WHERE NEXT_FROM IS NOT NULL
AND ( 
    SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 1, 4) < SUBSTR(REPLACE(VSL_APLY_FM_DT,'-',''), 1, 4)
        OR SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 1, 4) > SUBSTR(REPLACE(NEXT_FROM,'-',''), 1, 4)
        OR (SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 1, 4) = SUBSTR(REPLACE(VSL_APLY_FM_DT,'-',''), 1, 4) AND SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 5, 2) < SUBSTR(REPLACE(VSL_APLY_FM_DT,'-',''), 5, 2))
        OR (SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 1, 4) = SUBSTR(REPLACE(NEXT_FROM,'-',''), 1, 4) AND SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 5, 2) > SUBSTR(REPLACE(NEXT_FROM,'-',''), 5, 2))
        OR (SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 1, 4) = SUBSTR(REPLACE(VSL_APLY_FM_DT,'-',''), 1, 4) AND SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 5, 2) = SUBSTR(REPLACE(VSL_APLY_FM_DT,'-',''), 5, 2) AND SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 7, 2) < SUBSTR(REPLACE(VSL_APLY_FM_DT,'-',''), 7, 2))
        OR (SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 1, 4) = SUBSTR(REPLACE(NEXT_FROM,'-',''), 1, 4) AND SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 5, 2) = SUBSTR(REPLACE(NEXT_FROM,'-',''), 5, 2) AND SUBSTR(REPLACE(VSL_APLY_TO_DT,'-',''), 7, 2) > SUBSTR(REPLACE(NEXT_FROM,'-',''), 7, 2))
    )
#end			]]></sql>
			<params>
				<param name="f_vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
