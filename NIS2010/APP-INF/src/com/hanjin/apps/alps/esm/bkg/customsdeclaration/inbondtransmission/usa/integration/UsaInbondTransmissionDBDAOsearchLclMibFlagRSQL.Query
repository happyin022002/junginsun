<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInbondTransmissionDBDAOsearchLclMibFlagRSQL">
			<desc><![CDATA[dwkim,  Partial Check쿼리]]></desc>
			<sql><![CDATA[
SELECT A.bl_no
FROM BKG_CSTMS_ADV_BL A, BKG_CSTMS_ADV_IBD A1, BKG_CSTMS_ADV_CNTR B, BKG_CSTMS_ADV_CNTR C, BKG_CSTMS_ADV_BL D, BKG_CSTMS_ADV_IBD D1, BKG_CGO_RLSE CR
WHERE A.CNT_CD = 'US' 
   AND A.vsl_cd        = SUBSTR(@[vvd],1,4)
   AND A.skd_voy_no   = SUBSTR(@[vvd],5,4)
   AND A.skd_dir_cd   = SUBSTR(@[vvd],9,1)
   AND A.POD_CD       = @[pod]
   AND A.HUB_LOC_CD   = @[hub]
   AND A.DEL_CD       = @[del]
   AND A.CNT_CD = A1.CNT_CD
   AND A.BL_NO = A1.BL_NO
   AND A.MF_STS_CD     = 'A'
   AND B.CNT_CD         = A.CNT_CD
   AND B.bl_no         = A.bl_no
   AND A.MF_NO IS NULL

   AND D.vsl_cd       = SUBSTR(@[vvd],1,4)
   AND D.skd_voy_no   = SUBSTR(@[vvd],5,4)
   AND D.skd_dir_cd   = SUBSTR(@[vvd],9,1)
   AND D.POD_CD       = @[pod]
   AND D.HUB_LOC_CD   = @[hub]
   AND D.DEL_CD       = @[del]
   AND D.CNT_CD = D1.CNT_CD
   AND D.BL_NO = D1.BL_NO
   AND D.MF_STS_CD     = 'A'
   AND D.CNT_CD         = C.CNT_CD
   AND D.bl_no         = C.bl_no
   AND D.MF_NO IS NULL

   AND C.cnt_cd        = B.cnt_cd
   AND C.cntr_no       = B.cntr_no

   AND D.bl_no <> A.bl_no

   AND A.bl_no = CR.bl_no(+)
   AND A.BL_NO = NVl(@[bl_no], A.BL_NO)
   AND (CR.CSTMS_CLR_CD <> 'Y' or CR.CSTMS_CLR_CD is null)

   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
				<param name="hub" type="12" value="" out="N"/>
				<param name="del" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
