<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsWharfageDecMgtDBDAOmodifyBkgUsaWhfEmlUSQL">
			<desc><![CDATA[modifyBkgUsaWhfEml]]></desc>
			<sql><![CDATA[
UPDATE BKG_USA_WHF_EML 
   SET BRTH_DESC = @[brth_desc]
,	   BIL_RCV_PTY_NM = @[bil_rcv_pty_nm]
,	   UPD_USR_ID = @[upd_usr_id]
,	   UPD_DT = SYSDATE
WHERE	PORT_CD = @[port_cd]
AND	PORT_SEQ = @[port_seq]
AND	YD_CD = @[yd_cd]			]]></sql>
			<params>
				<param name="brth_desc" type="12" value="" out="N"/>
				<param name="bil_rcv_pty_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="port_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
