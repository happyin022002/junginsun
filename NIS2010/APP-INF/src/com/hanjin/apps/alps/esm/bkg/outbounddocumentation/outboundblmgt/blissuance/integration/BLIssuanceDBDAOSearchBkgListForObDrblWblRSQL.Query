<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOSearchBkgListForObDrblWblRSQL">
			<desc><![CDATA[SearchBkgListForObDrblWbl]]></desc>
			<sql><![CDATA[
SELECT   BKG_NO
        ,BKG_STS_CD
        ,BL_NO
        ,SHPR_CD
        ,SHPR_NM
        ,SHORT_SHPR_NM
        ,FF_CD
        ,VVD
        ,POL_CD
        ,POD_CD
        ,POR_CD
        ,FAX_NO
        ,EML
        ,CNTC_PSON_NM
        ,FAX_RESULT
        ,FAX_DATE
        ,FAX_REASON
        ,EML_RESULT
        ,EML_DATE
        ,EML_REASON
        ,FRT_ALL_FLG
        ,FRT_CLT_FLG
        ,FRT_PPD_FLG
        ,FRT_CHG_FLG
        ,FRT_ARR_FLG
        ,REMARK
        ,QTY_BKG
        ,QTY_CNTR
        ,CASE WHEN QTY_BKG = QTY_CNTR THEN 'Y' ELSE 'N' END QTY_FLG
        ,NTC_KND_CD 
FROM
(
        SELECT
        #if ((''==${vsl_cd} || ''==${skd_voy_no} || ''==${skd_dir_cd}) && (${bkg_no} == '' && ${bl_no} == '') && ('' !=${bl_obrd_dt_from} && '' !=${bl_obrd_dt_to}))
               /*+ INDEX(DOC XAK2BKG_BL_DOC) */
        #end
               BKG.BKG_NO,
               BKG.BKG_STS_CD,
               BKG.BL_NO || BKG.BL_TP_CD AS BL_NO,
               CST.CUST_CNT_CD || LPAD (CST.CUST_SEQ, 6, 0) AS SHPR_CD,
               CST.CUST_NM AS SHPR_NM,
               SUBSTRB (TRANSLATE (CST.CUST_NM, CHR (13) || CHR (10), ' '), 1, 20) AS SHORT_SHPR_NM,
               NVL2(FFC.CUST_CNT_CD,FFC.CUST_CNT_CD||LPAD(FFC.CUST_SEQ,6,0),NULL) AS FF_CD,
               BKG.VSL_CD || BKG.SKD_VOY_NO || BKG.SKD_DIR_CD AS VVD,
               BKG.POL_CD,
               BKG.POD_CD,
               BKG.POR_CD,
               NVL(FAX.NTC_FAX_NO, CTT.CNTC_PSON_FAX_NO) AS FAX_NO,
               NVL(NVL(EML.NTC_EML, CTT.CNTC_PSON_EML),(SELECT CNTC_PSON_EML FROM BKG_CNTC_PSON WHERE BKG_NO = BKG.BKG_NO AND 'BK' = BKG_CNTC_PSON_TP_CD AND ROWNUM = 1)) AS EML,
               CTT.CNTC_PSON_NM,
               (SELECT INTG_CD_VAL_DP_DESC
                  FROM COM_INTG_CD_DTL
                 WHERE 'CD02396' = INTG_CD_ID
                   AND INTG_CD_VAL_CTNT =
                       NVL2(FSI.FAX_PROC_STS_CD,
                       DECODE(FSI.FAX_PROC_STS_CD, 1, 2, 2, 2, 3, 2, 4, 2, 5, 3, 6, 4, 1),
                       NVL2(FAX.BKG_NTC_SND_RSLT_CD,
                       DECODE(FAX.BKG_NTC_SND_RSLT_CD, 4, 2, 5, 3, 6, 4, 1), 1))) AS FAX_RESULT,
               NVL(FSI.XPT_DT, NVL(FSI.UPD_DT, FAX.SND_RQST_DT)) AS FAX_DATE,
               NVL(FSI.XPT_ERR_MSG, FSI.XPT_ERR_DTL_MSG) AS FAX_REASON,
               (SELECT INTG_CD_VAL_DP_DESC
                  FROM COM_INTG_CD_DTL
                 WHERE 'CD02396' = INTG_CD_ID
                   AND INTG_CD_VAL_CTNT =
                       NVL2(ESI.EML_PROC_STS_CD, DECODE(ESI.EML_PROC_STS_CD, 1, 2, 2, 2, 3, 3, 4, 4, 1),
                       NVL2(EML.BKG_NTC_SND_RSLT_CD, DECODE(EML.BKG_NTC_SND_RSLT_CD, 3, 3, 4, 4, 1), 1))) AS EML_RESULT,
               NVL(ESI.EML_DT, EML.SND_RQST_DT) AS EML_DATE,
               ESI.EML_ERR_MSG AS EML_REASON,
               NVL(FAX.FRT_ALL_FLG, EML.FRT_ALL_FLG) AS FRT_ALL_FLG,
               NVL(FAX.FRT_CLT_FLG, EML.FRT_CLT_FLG) AS FRT_CLT_FLG,
               NVL(FAX.FRT_PPD_FLG, EML.FRT_PPD_FLG) AS FRT_PPD_FLG,
               NVL(FAX.FRT_CHG_FLG, EML.FRT_CHG_FLG) AS FRT_CHG_FLG,
               NVL(FAX.FRT_ARR_FLG, EML.FRT_ARR_FLG) AS FRT_ARR_FLG,
               BKG_DRAFT_REMARK_FNC(BKG.BKG_NO, '6', 'N', NULL) AS REMARK,
               BKG_JOIN_FNC( CURSOR(SELECT CNTR_TPSZ_CD||'-'||ltrim(TO_CHAR(NVL(OP_CNTR_QTY, 0),'99990.99'))
                                                                                                     FROM BKG_QUANTITY
                                                                                                     WHERE BKG_NO = BKG.BKG_NO                                                     
                                                                                                     AND   CNTR_TPSZ_CD IS NOT NULL
                                                                                                     ORDER BY CNTR_TPSZ_CD)
                   ) QTY_BKG,
               BKG_JOIN_FNC( CURSOR(SELECT  CNTR_TPSZ_CD||'-'||ltrim(TO_CHAR(NVL(sum(CNTR_VOL_QTY), 0),'99990.99'))
                                                                                                     FROM    BKG_CONTAINER
                                                                                                     WHERE BKG_NO = BKG.BKG_NO
                                                                                                     AND   CNTR_TPSZ_CD IS NOT NULL
                                                                                                     GROUP BY CNTR_TPSZ_CD
                                                                                                     ORDER BY CNTR_TPSZ_CD)
                   ) QTY_CNTR,
        #if ('W'==${bl_tp_cd})
               NVL (NVL (FAX.NTC_KND_CD, EML.NTC_KND_CD),'WB') AS NTC_KND_CD
        #else
               NVL(NVL(FAX.NTC_KND_CD, EML.NTC_KND_CD), 'BL') AS NTC_KND_CD
        #end
          FROM BKG_BOOKING BKG
               #if (''!=${vsl_cd} || ''!=${skd_voy_no} || ''!=${skd_dir_cd})
               ,BKG_VVD VVD
               #end
               ,BKG_CUSTOMER CST
               ,BKG_CUSTOMER FFC
               ,BKG_CNTC_PSON CTT
               ,BKG_BL_ISS ISS
               ,BKG_BL_DOC DOC
               #if ('All'!=${bkg_cust_tp_cd} || ''!=${cust_cnt_cd} || ''!=${cust_seq} || ''!=${cust_nm})
               ,BKG_CUSTOMER CST2
               #end
               ,BKG_NTC_HIS FAX
               ,BKG_NTC_HIS EML
               ,COM_FAX_SND_INFO FSI
               ,COM_EML_SND_INFO ESI
        #if ('W'!=${bl_tp_cd})
               
        #end
         WHERE BKG.BKG_NO = CST.BKG_NO
           AND 'S' = CST.BKG_CUST_TP_CD
           AND BKG.BKG_NO = FFC.BKG_NO(+)
           AND 'F' = FFC.BKG_CUST_TP_CD(+)
           AND BKG.BKG_NO = CTT.BKG_NO(+)
           AND 'SI' = CTT.BKG_CNTC_PSON_TP_CD(+)
           AND BKG.BKG_NO = ISS.BKG_NO(+)
           AND BKG.BKG_NO = DOC.BKG_NO(+)
           AND BKG.BKG_STS_CD NOT IN ('A', 'X')
           AND BKG.BKG_CGO_TP_CD <> 'P'
        #if (''!=${vsl_cd} || ''!=${skd_voy_no} || ''!=${skd_dir_cd})
           AND BKG.BKG_NO = VVD.BKG_NO
           #if (''!=${vsl_cd})
           AND @[vsl_cd] = VVD.VSL_CD  /*VVD*/
           #end
           #if (''!=${skd_voy_no})
           AND @[skd_voy_no] = VVD.SKD_VOY_NO  /*VVD*/
           #end
           #if (''!=${skd_dir_cd})
           AND @[skd_dir_cd] = VVD.SKD_DIR_CD  /*VVD*/
           #end
        #end
        #if ('All'!=${bkg_cust_tp_cd} || ''!=${cust_cnt_cd} || ''!=${cust_seq} || ''!=${cust_nm})
           AND CST2.BKG_NO = BKG.BKG_NO
           #if ('All'!=${bkg_cust_tp_cd})
           AND @[bkg_cust_tp_cd] = CST2.BKG_CUST_TP_CD  /*BKG_CUST_TP_CD*/
           #end
           #if (''!=${cust_cnt_cd})
           AND @[cust_cnt_cd] = CST2.CUST_CNT_CD  /*CUST_CNT_CD*/
           #end
           #if (''!=${cust_seq})
           AND CST2.CUST_SEQ = TO_NUMBER(@[cust_seq])  /*CUST_SEQ*/
           #end
           #if (''!=${cust_nm})
           AND UPPER(CST2.CUST_NM) LIKE UPPER(@[cust_nm])||'%'  /*CUST_NM*/
           #end
        #end
           AND BKG.BKG_NO = FAX.BKG_NO(+)
           AND 'F' = FAX.NTC_VIA_CD(+)
           AND FAX.SND_ID = FSI.FAX_SND_NO(+)
           AND BKG.BKG_NO = EML.BKG_NO(+)
           AND 'M' = EML.NTC_VIA_CD(+)
           AND EML.SND_ID = ESI.EML_SND_NO(+)
        #if ('W'==${bl_tp_cd})
           AND @[bl_tp_cd] = BKG.BL_TP_CD  /*BL_TP_CD*/
           AND 'WB' = FAX.NTC_KND_CD(+)
           AND NVL(FAX.HIS_SEQ,1) =
               NVL((SELECT MAX(HIS_SEQ) FROM BKG_NTC_HIS
                     WHERE BKG_NO = FAX.BKG_NO
                       AND 'F' = NTC_VIA_CD
                       AND 'WB' = NTC_KND_CD),1)
           AND 'WB' = EML.NTC_KND_CD(+)
           AND NVL(EML.HIS_SEQ,1) =
               NVL((SELECT MAX(HIS_SEQ) FROM BKG_NTC_HIS
                     WHERE BKG_NO = EML.BKG_NO
                       AND 'M' = NTC_VIA_CD
                       AND 'WB' = NTC_KND_CD),1)
        #else
           
           AND 'BL' = FAX.NTC_KND_CD(+)
           AND NVL(FAX.HIS_SEQ, 1) =
               NVL((SELECT MAX(HIS_SEQ) FROM BKG_NTC_HIS
                     WHERE BKG_NO = FAX.BKG_NO
                       AND 'F' = NTC_VIA_CD
                       AND 'BL' = NTC_KND_CD), 1)
           AND 'BL' = EML.NTC_KND_CD(+)
           AND NVL(EML.HIS_SEQ, 1) =
               NVL((SELECT MAX(HIS_SEQ) FROM BKG_NTC_HIS
                     WHERE BKG_NO = EML.BKG_NO
                       AND 'M' = NTC_VIA_CD
                       AND 'BL' = NTC_KND_CD), 1)
           
           
        #end
           #if (''!=${pol_cd})
           AND BKG.POL_CD LIKE @[pol_cd]||'%'  /*POL_CD*/
           #end
           #if (''!=${pol_yd_cd})
           AND SUBSTR(BKG.POL_NOD_CD,-2) LIKE @[pol_yd_cd]||'%'  /*pol_yd_cd*/
           #end
           #if (''!=${pod_cd})
           AND BKG.POD_CD LIKE @[pod_cd]||'%'  /*POD_CD*/
           #end
           #if (''!=${bkg_ofc_cd})
           AND UPPER(BKG.BKG_OFC_CD) LIKE UPPER(@[bkg_ofc_cd])||'%'  /*BKG_OFC_CD*/
           #end
           #if (''!=${doc_usr_id})
           AND UPPER(BKG.DOC_USR_ID) LIKE UPPER(@[doc_usr_id])||'%'  /*DOC_USR_ID*/
           #end
           #if ('All'!=${bkg_sts_cd})
           AND @[bkg_sts_cd] = BKG.BKG_STS_CD  /*BKG_STS_CD*/
           #end
           #if (''!=${obl_iss_ofc_cd})
           AND UPPER(ISS.OBL_ISS_OFC_CD) LIKE UPPER(@[obl_iss_ofc_cd])||'%'  /*OBL_ISS_OFC_CD*/
           #end
           #if (''!=${obl_iss_usr_id})
           AND UPPER(ISS.OBL_ISS_USR_ID) LIKE UPPER(@[obl_iss_usr_id])||'%'  /*OBL_ISS_USR_ID*/
           #end
           #if (''!=${ob_sls_ofc_cd})
           AND UPPER(BKG.OB_SLS_OFC_CD) LIKE UPPER(@[ob_sls_ofc_cd])||'%'  /*OB_SLS_OFC_CD*/
           #end
           #if (''!=${ob_srep_cd})
           AND UPPER(BKG.OB_SREP_CD) LIKE UPPER(@[ob_srep_cd])||'%'  /*OB_SREP_CD*/
           #end
           #if (''!=${bkg_no})
           AND BKG.BKG_NO IN ( ${bkg_no} )  /*BKG_NO*/
           #end
           #if (''!=${bl_no})
           AND BKG.BL_NO IN ( ${bl_no} )  /*BL_NO*/
           #end
           #if (''!=${sc_no})
           AND @[sc_no] = BKG.SC_NO  /*SC_NO*/
           #end
           #if (''!=${rfa_no})
           AND @[rfa_no] = BKG.RFA_NO  /*RFA_NO*/
           #end
           #if (''!=${bl_obrd_dt_from} && ''!=${bl_obrd_dt_to})
           AND DOC.BL_OBRD_DT BETWEEN TO_DATE(@[bl_obrd_dt_from],'RRRRMMDD') AND TO_DATE(@[bl_obrd_dt_to],'RRRRMMDD')  /*BL_OBRD_DT*/
           #elseif (''!=${obl_iss_dt_from} && ''!=${obl_iss_dt_to})
           AND ISS.OBL_ISS_DT BETWEEN TO_DATE(@[obl_iss_dt_from],'RRRRMMDD') AND TO_DATE(@[obl_iss_dt_to],'RRRRMMDD')  /*OBL_ISS_DT*/
           #end
           #if ('All'!=${fax_proc_sts_cd})
           AND NVL2(FSI.FAX_PROC_STS_CD,
               DECODE(FSI.FAX_PROC_STS_CD,1,2,2,2,3,2,4,2,5,3,6,4,1),
               NVL2(FAX.BKG_NTC_SND_RSLT_CD,
               DECODE(FAX.BKG_NTC_SND_RSLT_CD,4,2,5,3,6,4,1),1)) = @[fax_proc_sts_cd]
           #end
           #if ('All'!=${eml_proc_sts_cd})
           AND NVL2(ESI.EML_PROC_STS_CD,
               DECODE(ESI.EML_PROC_STS_CD,1,2,2,2,3,3,4,4,1),
               NVL2(EML.BKG_NTC_SND_RSLT_CD,
               DECODE(EML.BKG_NTC_SND_RSLT_CD,3,3,4,4,1),1)) = @[eml_proc_sts_cd]
           #end
           #if('Y' == ${no_sr_flg})
           AND BKG.XTER_SI_CD IS NULL
           #end
)
WHERE 1=1
#if('Y' == ${un_pickup_flg})
AND (CASE WHEN QTY_BKG = QTY_CNTR THEN 'Y' ELSE 'N' END) = 'N'           
#end
ORDER BY BKG_NO

			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="bl_tp_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="doc_usr_id" type="12" value="" out="N"/>
				<param name="bkg_sts_cd" type="12" value="" out="N"/>
				<param name="obl_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="obl_iss_usr_id" type="12" value="" out="N"/>
				<param name="ob_sls_ofc_cd" type="12" value="" out="N"/>
				<param name="ob_srep_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="bl_obrd_dt_from" type="12" value="" out="N"/>
				<param name="bl_obrd_dt_to" type="12" value="" out="N"/>
				<param name="obl_iss_dt_from" type="12" value="" out="N"/>
				<param name="obl_iss_dt_to" type="12" value="" out="N"/>
				<param name="fax_proc_sts_cd" type="12" value="" out="N"/>
				<param name="eml_proc_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
