<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOsearchTmnl301TroMainInfoRSQL">
			<desc><![CDATA[searchTmnl301TroMainInfo]]></desc>
			<sql><![CDATA[
SELECT BKG_NO,
	   TRO_SEQ,
'{TRO_OUT'																								|| CHR(10)
	|| 'TRO_SEQ:'				|| LPAD(TRO_SEQ, 2, '0')	        									|| CHR(10)
	|| 'TRO_TEL:'				|| NVL(ACT_SHPR_PHN_NO, ' ')			  	    						|| CHR(10)
	|| 'TRO_FAX:'				|| NVL(CNTC_FAX_NO, ' ')									            || CHR(10)
	|| 'TRO_MOBILE:'			|| NVL(CNTC_MPHN_NO, ' ')				            					|| CHR(10)
	|| 'TRO_ACUST_NM:'  		|| SUBSTR(NVL(ACT_SHPR_NM, ' '), 1, 50)			 						|| CHR(10)
	|| 'TRO_PERSON:'			|| SUBSTR(NVL(CNTC_PSON_NM, ' '), 1, 20)	  	                        || CHR(10)
	|| 'TRO_OWN_TRK:'   		|| OWNR_TRK_FLG															|| CHR(10)
	|| 'TRO_REMARK:'    		|| NVL(TRANSLATE(DIFF_RMK, CHR(10)||CHR(13), ' '), ' ')					|| CHR(10)
|| '}TRO_OUT'		    		|| CHR(10) TRO_MAIN_INFO
  FROM BKG_TRO
 WHERE BKG_NO 				= @[bkg_no]
   AND TRO_SEQ				= @[tro_seq]
   AND IO_BND_CD      		= 'O'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
