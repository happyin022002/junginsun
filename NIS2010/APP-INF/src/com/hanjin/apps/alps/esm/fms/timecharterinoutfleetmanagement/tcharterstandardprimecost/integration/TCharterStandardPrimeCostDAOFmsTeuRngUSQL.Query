<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterStandardPrimeCostDAOFmsTeuRngUSQL">
			<desc><![CDATA[TEU Range Target Update]]></desc>
			<sql><![CDATA[
update fms_teu_rng set 
	rng_fm_qty = @[rng_fm_qty],
	rng_to_qty = @[rng_to_qty],
	hir_aply_flg = @[hir_aply_flg],
	mkt_rt_aply_flg = @[mkt_rt_aply_flg],
	upd_usr_id = @[upd_usr_id]
where	rng_yr = @[rng_yr]
and	rng_seq = @[rng_seq]			]]></sql>
			<params>
				<param name="rng_fm_qty" type="2" value="" out="N"/>
				<param name="rng_to_qty" type="2" value="" out="N"/>
				<param name="hir_aply_flg" type="12" value="" out="N"/>
				<param name="mkt_rt_aply_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="91" value="" out="N"/>
				<param name="rng_yr" type="12" value="" out="N"/>
				<param name="rng_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
