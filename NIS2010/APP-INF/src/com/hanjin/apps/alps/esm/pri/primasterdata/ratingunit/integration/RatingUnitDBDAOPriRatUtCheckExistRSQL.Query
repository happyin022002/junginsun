<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RatingUnitDBDAOPriRatUtCheckExistRSQL">
			<desc><![CDATA[rating unit cd 가 존재하는 지 체크한다.]]></desc>
			<sql><![CDATA[
SELECT  RAT_UT_CD AS CD  
FROM PRI_RAT_UT
WHERE RAT_UT_CD = @[cd]			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
