		<query name="GeneralBookingReceiptDBDAOCreateCustCACSQL">
			<desc><![CDATA[GeneralBookingReceiptDBDAOCreateCustCACSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_CUSTOMER (
    BKG_NO 
#else
INSERT INTO BKG_CUST_HIS (
    BKG_NO 
    , CORR_NO 
#end
