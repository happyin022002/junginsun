    , CUST_TP_CD 
    , REF_NO 
    , ADDR_PRN_FLG 
    , VAL_NM 
    , VAL_FAX_NO 
    , VAL_CD 
    , VAL_USR_ID 
    , VAL_OFC_CD 
    , VAL_DT 
    , MTCH_FLG 
    , AN_SND_FLG 
    , CHG_DP_FLG 
    , ORG_CUST_CNT_CD 
    , ORG_CUST_SEQ 
    , IB_CUST_NM 
    , IB_CUST_ADDR 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
    , CUST_CD_UPD_DT 
	, EUR_CSTMS_ST_NM
	, EORI_NO
	, END_USR_NM
   )
