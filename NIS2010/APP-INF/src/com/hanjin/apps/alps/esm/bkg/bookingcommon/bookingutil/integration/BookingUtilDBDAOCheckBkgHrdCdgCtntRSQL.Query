<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOCheckBkgHrdCdgCtntRSQL">
			<desc><![CDATA[BookingUtilDBDAOCheckBkgHrdCdgCtnt]]></desc>
			<sql><![CDATA[
SELECT HRD_CDG_ID, HRD_CDG_ID_SEQ, 
       ATTR_CTNT1, ATTR_CTNT2, ATTR_CTNT3, ATTR_CTNT4, ATTR_CTNT5, 
       ATTR_CTNT6, ATTR_CTNT7, ATTR_CTNT8, ATTR_CTNT9, ATTR_CTNT10
  FROM BKG_HRD_CDG_CTNT
 WHERE HRD_CDG_ID = @[hrd_cdg_id]
   AND ATTR_CTNT1 = @[attr_ctnt1]

   #if (${attr_ctnt2} != '') 
   AND ATTR_CTNT2 = @[attr_ctnt2]
   #end

   #if (${attr_ctnt3} != '') 
   AND ATTR_CTNT3 = @[attr_ctnt3]
   #end			]]></sql>
			<params>
				<param name="hrd_cdg_id" type="12" value="" out="N"/>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
				<param name="attr_ctnt3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
