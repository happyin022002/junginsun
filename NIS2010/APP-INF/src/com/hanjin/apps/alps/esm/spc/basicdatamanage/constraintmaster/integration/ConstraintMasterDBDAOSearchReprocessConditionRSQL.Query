<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOSearchReprocessConditionRSQL">
			<desc><![CDATA[Reprocess 버튼 클릭 시 수행전 현재 같은 조건으로 Reprocess되고 있는 백엔드잡이 있는지 체크합니다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM SPC_SB_BKG_PROC_STS
WHERE 1=1
#if(${f_level} != '' )
AND SPC_CTRL_OFC_LVL_CD = @[f_level]
#end
#if (${f_sls_ofc_cd} != '')
AND SLS_OFC_CD          = @[f_sls_ofc_cd]
#end
#if (${f_ofc_cd} != '')
AND OFC_CD              = @[f_ofc_cd]
#end
#if (${f_vvd_cd} != '')
AND VVD_CD              = @[f_vvd_cd]
#else
#if (${f_trd_cd} != '')
AND TRD_CD              = @[f_trd_cd]
#end
#if (${f_sub_trd_cd} != '')
AND SUB_TRD_CD          = @[f_sub_trd_cd]
#end
#if (${f_rlane_cd} != '')
AND RLANE_CD            = @[f_rlane_cd]
#end
#if (${f_dir_cd} != '')
AND DIR_CD              = @[f_dir_cd]
#end
#if (${f_year}||${f_week} != '')
AND YRWK                = @[f_year]||@[f_week]
#end
#if (${f_duration} != '')
AND DUR_WKS             = @[f_duration]
#end
#end			]]></sql>
			<params>
				<param name="f_level" type="12" value="" out="N"/>
				<param name="f_sls_ofc_cd" type="12" value="" out="N"/>
				<param name="f_ofc_cd" type="12" value="" out="N"/>
				<param name="f_vvd_cd" type="12" value="" out="N"/>
				<param name="f_trd_cd" type="12" value="" out="N"/>
				<param name="f_sub_trd_cd" type="12" value="" out="N"/>
				<param name="f_rlane_cd" type="12" value="" out="N"/>
				<param name="f_dir_cd" type="12" value="" out="N"/>
				<param name="f_year" type="12" value="" out="N"/>
				<param name="f_week" type="12" value="" out="N"/>
				<param name="f_duration" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
