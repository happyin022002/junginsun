<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOModifyApprovalStepUpdate3USQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
UPDATE AP_INV_HDR SET
     ACT_XCH_RT = (SELECT T.LOCL_KRW_XCH_RT 
              		 FROM GL_MON_XCH_RT T, AP_INV_HDR A 
              		 WHERE T.ACCT_XCH_RT_LVL   = 1 
              		 AND T.CURR_CD           = A.CSR_CURR_CD
              		 AND T.ACCT_XCH_RT_YRMON = SUBSTR(A.GL_DT,1,6)
                     AND A.CSR_NO = @[csr_no]
                    )   
WHERE
	CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
