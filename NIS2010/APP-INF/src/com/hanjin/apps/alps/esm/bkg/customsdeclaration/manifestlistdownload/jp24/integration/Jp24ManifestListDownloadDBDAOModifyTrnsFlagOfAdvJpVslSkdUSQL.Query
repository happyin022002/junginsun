<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOModifyTrnsFlagOfAdvJpVslSkdUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_JP_VSL_SKD

   SET
#if (${send_div} == '1')
       -- 1503화면 : 1 / BATCH_ETD : 2 / BATCH_ATD : 3  */
       MF_SND_FLG = 'Y',
       FNL_EDI_SND_FLG = 'Y',
#elseif (${send_div} == '2')
       -- BATCH_ETD : 2
       MF_SND_FLG = 'Y',
#elseif (${send_div} == '3')
       -- BATCH_ATD : 
       FNL_EDI_SND_FLG = 'Y',
#end
       ETB_DT = TO_DATE(@[etd_date]||' '||@[etd_time], 'YYYY-MM-DD HH24:MI'),
       UPD_USR_ID = @[usr_id],
       UPD_DT = SYSDATE

 WHERE VSL_CD = SUBSTR(@[vvd], 1, 4)
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)
   AND POL_CD = @[pol_cd]
			]]></sql>
			<params>
				<param name="etd_date" type="12" value="" out="N"/>
				<param name="etd_time" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
