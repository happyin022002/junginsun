<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOModifyCustCntcCodeUSQL">
			<desc><![CDATA[Update cust contact point]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUST_CNTC_PNT
SET
   CUST_EML          = @[cust_eml]
,  CUST_IP           = @[cust_ip]
,  CUST_URL          = @[cust_url]
,  INTL_PHN_NO       = @[intl_phn_no]
,  PHN_NO            = @[phn_no]
,  INTL_FAX_NO       = @[intl_fax_no]
,  FAX_NO            = @[fax_no]
,  EAI_EVNT_DT		 = CASE WHEN NVL(CUST_EML,'0') <> NVL(@[cust_eml],'0') OR NVL(CUST_URL,'0') <> NVL(@[cust_url],'0') OR NVL(INTL_PHN_NO,'0') <> NVL(@[intl_phn_no],'0') 
								 OR NVL(PHN_NO,'0') <> NVL(@[phn_no],'0') OR NVL(INTL_FAX_NO,'0') <> NVL(@[intl_fax_no],'0') OR NVL(FAX_NO,'0') <> NVL(@[fax_no],'0')
                            THEN SYSDATE
							ELSE EAI_EVNT_DT
                       END
WHERE  CUST_CNT_CD    = @[cust_cnt_cd]
AND CUST_SEQ          = @[cust_seq]
AND CUST_CNTC_PNT_SEQ = 1			]]></sql>
			<params>
				<param name="cust_eml" type="12" value="" out="N"/>
				<param name="cust_ip" type="12" value="" out="N"/>
				<param name="cust_url" type="12" value="" out="N"/>
				<param name="intl_phn_no" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="intl_fax_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
