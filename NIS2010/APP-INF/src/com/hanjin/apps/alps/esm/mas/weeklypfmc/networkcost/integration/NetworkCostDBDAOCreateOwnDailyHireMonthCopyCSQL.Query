<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOCreateOwnDailyHireMonthCopyCSQL">
			<desc><![CDATA[2013.05.08 성미영 [CHM-201324182] AVG hire by Own VSL (PA) 전월 COPY 기능 추가
2015.08.24 손진환 [CHM-201537399] AVG-hire by Own-VSL (PA) Month Copy 기능 추가 생성 요청]]></desc>
			<sql><![CDATA[
INSERT
  INTO MAS_OWN_VSL_DLY_HIR ( 
       COST_YRMON,
	   COST_WK,
       VSL_CD,
       VSL_CLSS_CAPA,
       STND_COST_CD,
       DHIR_AMT,
       EFF_FM_YRMON,
       EFF_TO_YRMON,
       VSL_AMT,
       COM_DTRB_AMT,
       AVG_TEU_AMT,
       DHIR_ADD_AMT,
       DHIR_BFR_AMT,
       COM_TTL_AMT,
       VSL_AMT_RT,
       TTL_DYS,
       TTL_TEU_QTY,
       OWN_VSL_RMK,
       CRE_USR_ID,
       CRE_DT,
       UPD_USR_ID,
       UPD_DT 
  )
  SELECT @[f_tar_mon] COST_YRMON,
	   COST_WK,
       VSL_CD,
       VSL_CLSS_CAPA,
       STND_COST_CD,
       DHIR_AMT,
       EFF_FM_YRMON,
       EFF_TO_YRMON,
       VSL_AMT,
       COM_DTRB_AMT,
       AVG_TEU_AMT,
       DHIR_ADD_AMT,
       DHIR_BFR_AMT,
       COM_TTL_AMT,
       VSL_AMT_RT,
       TTL_DYS,
       TTL_TEU_QTY,
       OWN_VSL_RMK,
       @[user_id] CRE_USR_ID,
       SYSDATE CRE_DT,
       @[user_id] UPD_USR_ID,
       SYSDATE UPD_DT
  FROM MAS_OWN_VSL_DLY_HIR
 WHERE COST_YRMON = @[f_src_mon]			]]></sql>
			<params>
				<param name="f_tar_mon" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="f_src_mon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
