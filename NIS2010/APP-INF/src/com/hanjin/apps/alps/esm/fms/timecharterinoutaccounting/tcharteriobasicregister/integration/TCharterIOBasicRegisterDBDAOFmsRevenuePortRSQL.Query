<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOBasicRegisterDBDAOFmsRevenuePortRSQL">
			<desc><![CDATA[Revenue Port Management Select]]></desc>
			<sql><![CDATA[
SELECT RLANE_CD,
	   SKD_DIR_CD,
	   REV_DIR_CD,
	   SLAN_CD,
	   FLET_IOC_CD,
	   ST_PORT_CD,
	   FNL_PORT_CD,
	   CRE_USR_ID,
	   CRE_DT,
	   UPD_USR_ID,
	   UPD_DT
  FROM FMS_BSE_PORT 
#if (${slan_cd} != "")                                                                               
WHERE SLAN_CD = @[slan_cd]                                                                   
#end
#if (${rlane_cd} != "")                                                                               
WHERE RLANE_CD = @[rlane_cd]
#end
ORDER BY RLANE_CD			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
