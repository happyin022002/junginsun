,	CSR_DESC
,	EFF_DT
,	EVID_TP_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[slp_tp_cd]
,	@[slp_func_cd]
,	@[slp_ofc_cd]
,	SUBSTR(@[slp_iss_dt],3)
,	@[slp_ser_no]
,	@[flet_ctrt_no]
,	@[csr_curr_cd]
,	0
,	@[cre_usr_id]
,	@[csr_desc]
,	@[eff_dt]
