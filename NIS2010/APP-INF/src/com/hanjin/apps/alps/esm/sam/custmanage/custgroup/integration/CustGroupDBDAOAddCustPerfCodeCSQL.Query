<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustGroupDBDAOAddCustPerfCodeCSQL">
			<desc><![CDATA[Create Group Customer code]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CUST_PERF_GRP(
	 CUST_GRP_ID
,	 CUST_GRP_NM
,    OFC_CD
,    SREP_CD
,    VBS_CLSS_CD
,    NBS_CLSS_CD1
,    NBS_CLSS_CD2
,    NBS_CLSS_CD3
,    CRE_USR_ID
,    CRE_DT
,    UPD_USR_ID
,    UPD_DT
,    DELT_FLG
,	 PRMRY_CUST_CNT_CD
,    PRMRY_CUST_SEQ
)VALUES (
	@[cust_grp_id]
,   @[cust_grp_nm]
,	@[ofc_cd]
,	@[srep_cd]
,	@[vbs_clss_cd]
,	@[nbs_clss_cd1]
,	@[nbs_clss_cd2]
,	@[nbs_clss_cd3]
,	@[cre_usr_id]
,	sysdate
,	@[upd_usr_id]
,	sysdate
,	NVL(@[delt_flg],'N')
,   @[cust_cnt_cd]
,   @[cust_seq]
)			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
				<param name="cust_grp_nm" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="vbs_clss_cd" type="12" value="" out="N"/>
				<param name="nbs_clss_cd1" type="12" value="" out="N"/>
				<param name="nbs_clss_cd2" type="12" value="" out="N"/>
				<param name="nbs_clss_cd3" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
