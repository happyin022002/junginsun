<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IndiaManifestListDownloadDBDAOmodifyFormSettingUSQL">
			<desc><![CDATA[Ofc code 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_IDA_PRN_FOM SET 
	OFC_CD = @[ofc_cd]
,	HDR_CTNT = @[hdr_ctnt]
,	FTR_CTNT = @[ftr_ctnt]
,	DECL_ADDR = @[decl_addr]
,	BOD_CTNT = @[bod_ctnt]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	OFC_CD = @[ofc_cd]
			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="hdr_ctnt" type="12" value="" out="N"/>
				<param name="ftr_ctnt" type="12" value="" out="N"/>
				<param name="decl_addr" type="12" value="" out="N"/>
				<param name="bod_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
