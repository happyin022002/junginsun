<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOSearchBkgRjEmlRSQL">
			<desc><![CDATA[Special Cargo Reject건 자동이메일전송기능]]></desc>
			<sql><![CDATA[
SELECT MAX(REV1.USR_NM) USR_NM,
	    MAX(REV1.USR_EML) USR_EML,
	    MAX(SP.RQST_USR_ID) RQST_USR_ID,
	    MAX(REV2.USR_EML) RQST_USR_EML   
FROM BKG_BOOKING BK,
	     COM_USER REV1,
	     COM_USER REV2
-- checking spcl_cgo_tp
#if (${spcl_cgo_tp} == 'A')
	     ,BKG_AWK_CGO SP
#elseif (${spcl_cgo_tp} == 'B')
		 ,BKG_BB_CGO SP
#elseif (${spcl_cgo_tp} == 'D')
		 ,BKG_DG_CGO SP
#elseif (${spcl_cgo_tp} == 'R')
		 ,BKG_RF_CGO SP

#end

WHERE BK.BKG_NO = @[bkg_no]
	   AND UPPER(BK.DOC_USR_ID) = UPPER(REV1.USR_ID(+))
	   AND BK.BKG_NO = SP.BKG_NO
	   AND SP.RQST_USR_ID = REV2.USR_ID(+)
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
