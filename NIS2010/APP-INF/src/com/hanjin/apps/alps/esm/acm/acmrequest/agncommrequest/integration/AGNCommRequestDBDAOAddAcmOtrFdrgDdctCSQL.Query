<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGNCommRequestDBDAOAddAcmOtrFdrgDdctCSQL">
			<desc><![CDATA[AcmOtrFdrgDdct
2016.08.05 김상현 [CHM-201642499] ALPS > ACM VIP Agreement Creation 상 SC/RFA No. 추가 요청]]></desc>
			<sql><![CDATA[
INSERT INTO ACM_AGN_COMM_TRSP
SELECT *
FROM (SELECT @[bkg_no] AS BKG_NO,
        @[agn_cd] AS AGN_CD,
        @[io_bnd_cd] AS IO_BND_CD,
        @[ac_tp_cd] AS AC_TP_CD,
        @[max_ac_seq] AS AC_SEQ,
        ((SELECT NVL(MAX(AC_TRSP_SEQ), 0)
          FROM ACM_AGN_COMM_TRSP
          WHERE BKG_NO = @[bkg_no]
            AND AGN_CD = @[agn_cd]
            AND IO_BND_CD = @[io_bnd_cd]
            AND AC_TP_CD = @[ac_tp_cd]
            AND AC_SEQ = @[max_ac_seq]
         )
         +
         ROW_NUMBER() OVER(ORDER BY LOC.POL_CD)
        ) AS AC_TRSP_SEQ,
        'E' AS TRSP_MOD_CD,
        'Own' || '-' || QTY.CNTR_TPSZ_CD AS TRSP_DDCT_CD,
        LOC.POL_CD FM_LOC_CD,
        LOC.POD_CD TO_LOC_CD,
        (QTY.OP_CNTR_QTY * NVL((SELECT DDCT_AMT
                                FROM ACM_OTR_FDRG_DDCT DDCT,
                                  ACM_AGN_SET_CNTR_GRP GRP
                                WHERE DDCT.POL_CD = LOC.POL_CD
                                  AND DDCT.POD_CD = LOC.POD_CD
                                  AND GRP.CNTR_TPSZ_GRP_NM IN ('20', '40')
                                  AND GRP.CNTR_TPSZ_CD = QTY.CNTR_TPSZ_CD
                                  AND GRP.CNTR_TPSZ_GRP_NM = DDCT.CNTR_TPSZ_CD
                                  AND @[sa_dt] BETWEEN DDCT.FM_EFF_DT AND DDCT.TO_EFF_DT
                                  AND NVL(DDCT.DELT_FLG, 'N') = 'N'
                               ),
                               (SELECT DDCT_AMT
                                FROM ACM_OTR_FDRG_DDCT DDCT,
                                  ACM_AGN_SET_CNTR_GRP GRP
                                WHERE UPPER(DDCT.POL_CD) = 'OTHER'
                                  AND UPPER(DDCT.POD_CD) = 'OTHER'
                                  AND GRP.CNTR_TPSZ_GRP_NM IN ('20', '40')
                                  AND GRP.CNTR_TPSZ_CD = QTY.CNTR_TPSZ_CD
                                  AND GRP.CNTR_TPSZ_GRP_NM = DDCT.CNTR_TPSZ_CD
                                  AND @[sa_dt] BETWEEN DDCT.FM_EFF_DT AND DDCT.TO_EFF_DT
                                  AND NVL(DDCT.DELT_FLG, 'N') = 'N'
                               )
                           )
        ) AS TRSP_DDCT_AMT,
        '1' AS TRSP_LVL,
        @[usr_id] AS UPD_ID,
        SYSDATE UPD_DT,
        @[usr_id] AS CRE_ID,
        SYSDATE CRE_DT
      FROM BKG_QUANTITY QTY,
        (SELECT BND, POL_CD, POD_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, SLAN_CD
         FROM (SELECT 'O' BND, SUBSTR(POL_CD, 1, 5) POL_CD, SUBSTR(POD_CD, 1, 5) POD_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, SLAN_CD
               FROM BKG_VVD
               WHERE BKG_NO = @[bkg_no] AND VSL_PRE_PST_CD = 'S'
               UNION ALL
               SELECT 'I' BND, SUBSTR(POL_CD, 1, 5) POL_CD, SUBSTR(POD_CD, 1, 5) POD_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, SLAN_CD
               FROM BKG_VVD
               WHERE BKG_NO = @[bkg_no] AND VSL_PRE_PST_CD = 'U'
              )
         GROUP BY BND, POL_CD, POD_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, SLAN_CD
        ) LOC
      WHERE QTY.BKG_NO = @[bkg_no]
        AND LOC.BND IN (DECODE(@[fdrg_ddct_org_flg], 'Y', 'O', 'X'), DECODE(@[fdrg_ddct_dest_flg], 'Y', 'I', 'X'))
        AND (SELECT COUNT(*) CNT
             FROM MAS_LANE_RGST
             WHERE DIR_CD = LOC.SKD_DIR_CD
               AND SLAN_CD = LOC.SLAN_CD
               AND LOD_SPL_CNG_FLG = 'Y'
               AND DELT_FLG = 'N'
            ) > 0
        AND QTY.CNTR_TPSZ_CD IN (SELECT CNTR_TPSZ_CD FROM ACM_AGN_SET_CNTR_GRP WHERE CNTR_TPSZ_GRP_NM IN ('20', '40'))
        AND (SELECT COUNT(*) CNT
             FROM ACM_AGN_COMM_TRSP
             WHERE BKG_NO = @[bkg_no]
               AND AGN_CD = @[agn_cd]
               AND IO_BND_CD = @[io_bnd_cd]
               AND AC_TP_CD = @[ac_tp_cd]
               AND AC_SEQ = @[max_ac_seq]
               AND TRSP_MOD_CD = 'F'
               AND FM_LOC_CD = LOC.POL_CD
               AND TO_LOC_CD = LOC.POD_CD
          ) = 0
        AND EXISTS(SELECT 1
                   FROM ACM_OTR_FDRG_DDCT DDCT,
                     ACM_AGN_SET_CNTR_GRP GRP
                   WHERE (DDCT.POL_CD = LOC.POL_CD OR UPPER(DDCT.POL_CD) = 'OTHER')
                     AND (DDCT.POD_CD = LOC.POD_CD OR UPPER(DDCT.POD_CD) = 'OTHER')
                     AND GRP.CNTR_TPSZ_GRP_NM IN ('20', '40')
                     AND GRP.CNTR_TPSZ_CD = QTY.CNTR_TPSZ_CD
                     AND GRP.CNTR_TPSZ_GRP_NM = DDCT.CNTR_TPSZ_CD
                     AND @[sa_dt] BETWEEN DDCT.FM_EFF_DT AND DDCT.TO_EFF_DT
					 AND NVL(DDCT.DELT_FLG, 'N') = 'N'
            )
  )
WHERE TRSP_DDCT_AMT IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="ac_tp_cd" type="12" value="" out="N"/>
				<param name="max_ac_seq" type="12" value="" out="N"/>
				<param name="sa_dt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="fdrg_ddct_org_flg" type="12" value="" out="N"/>
				<param name="fdrg_ddct_dest_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
