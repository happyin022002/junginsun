<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOMultiLaneDetailUSQL">
			<desc><![CDATA[Lane Detail Update]]></desc>
			<sql><![CDATA[
UPDATE COA_LANE_VSL_FREQ SET 
	FREQ_NO		 = @[freq_no]
,	UPD_USR_ID	 = @[upd_usr_id]
,	UPD_DT		 = SYSDATE
WHERE	TRD_CD	 		= @[trd_cd]
 AND	RLANE_CD 		= @[rlane_cd]
 AND	COST_USE_TP_CD 	= @[cost_use_tp_cd]
 AND	DIR_CD		 	= @[dir_cd]			]]></sql>
			<params>
				<param name="freq_no" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="cost_use_tp_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
