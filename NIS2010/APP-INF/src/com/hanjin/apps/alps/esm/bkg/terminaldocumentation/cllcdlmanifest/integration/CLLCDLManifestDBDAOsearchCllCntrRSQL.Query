<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOsearchCllCntrRSQL">
			<desc><![CDATA[searchCllCntr

2011.1.20 경종윤 
  - [CHM-201108213] CLL/CDL EDI 기능에 Multi Seal 지원 로직 추가
  - [CHM-201108406] COPRAR EDI issue 관련 보완 요청 - Venterm Terminal (CAVANM2)]]></desc>
			<sql><![CDATA[
SELECT	'{CNTR_INFO'||CHR(10)||
	'CNTRNBR:'||NVL(M.CNTR_NO,' ')||CHR(10)||
	'CNTRNBR_OLD:'||(	SELECT OLD_CNTR 
						FROM	(
									SELECT  PRE_CTNT.CNTR_NO OLD_CNTR, 
        									CRNT_CTNT.CNTR_NO CRNT_CTNT
				 					FROM (       
											SELECT 	ROWNUM RN, SUBSTR(PRE_CTNT,1,11) CNTR_NO 
											FROM 	BKG_HIS_MST MST, BKG_HIS_DTL DTL
	      									WHERE 	MST.BKG_NO = DTL.BKG_NO
      										AND   	MST.HIS_SEQ = DTL.HIS_SEQ
      										AND   	MST.BKG_NO = @[bkg_no]
      										AND   	DTL.HIS_CATE_NM LIKE 'Container%'
      										AND   	MST.BKG_HIS_ISS_UI_ID NOT IN ('ESM_BKG_0079_07','SYSTEM')
      										AND   	DTL.PRE_CTNT IS NOT NULL
      										AND   	SUBSTR(PRE_CTNT,1,11) NOT IN (	SELECT 	CNTR_NO
 																					FROM 	BKG_CONTAINER
	 																				WHERE 	BKG_NO = @[bkg_no])) PRE_CTNT,
								(	SELECT 	ROWNUM RN, CNTR_NO
 									FROM 	BKG_CONTAINER
 									WHERE 	BKG_NO = @[bkg_no]) CRNT_CTNT
								WHERE 	CRNT_CTNT.RN = PRE_CTNT.RN(+))
						WHERE 	CRNT_CTNT = @[cntr_no]
						AND		ROWNUM = 1)||CHR(10)||
	'ALPS_EQID:'||(SELECT MAX(DOC_PROC_SEQ) 
					 FROM BKG_DOC_PROC_SKD
					WHERE BKG_DOC_PROC_TP_CD = 'CNTATC'
					  AND BKG_NO = @[bkg_no]
					  AND DIFF_RMK = @[cntr_no] )||CHR(10)||
    'PUNIT:'||NVL(M.PCK_TP_CD,' ')||CHR(10)||
	'PKG:'||NVL(M.PCK_QTY,0)||CHR(10)||
	'CNTRWGT:'||DECODE(NVL(M.WGT_TP_CD,' '),
					   'LBS', ROUND(NVL(M.CNTR_WGT,0)*0.4536,0),
					   NVL(M.CNTR_WGT,0))||CHR(10)||
	'CNTRGWGT:'||CHR(10)||
	'CNTR_WGT_UNIT:'||'KGS'||CHR(10)||

    'CNTR_TOTAL_WGT:'|| (
                            SELECT 
									NVL(
										SUM(
                                        	DECODE(NVL(WGT_TP_CD,' ')
                                				,'LBS', ROUND(NVL(CNTR_WGT,0)*0.4536,0)
                                				,NVL(CNTR_WGT,0)
                                		      )
                                			) 
										, DECODE(NVL(M.WGT_TP_CD,' '),
					   						'LBS', ROUND(NVL(M.CNTR_WGT,0)*0.4536,0),
					   							NVL(M.CNTR_WGT,0))
										) CNTR_TOTAL_WGT
							FROM BKG_CSTMS_TML_CLL
							WHERE 1=1
							AND VSL_CD 			= SUBSTR(@[in_vvd_cd],1,4)
							AND SKD_VOY_NO 		= SUBSTR(@[in_vvd_cd],5,4)
							AND SKD_DIR_CD 		= SUBSTR(@[in_vvd_cd],9,1)
							AND PORT_CD 		= @[in_pol_cd]
							#if (${in_pol_split_no} != '')
							AND NVL(CLPT_IND_SEQ, '1') = @[in_pol_split_no]
							#end
							AND CNTR_NO 		= @[cntr_no]
							AND CRE_USR_ID 		= @[cre_usr_id]
							AND  TEU_CNTR_QTY + FEU_CNTR_QTY < 1
                        ) ||CHR(10)||
        
    'CNTR_TOTAL_WGT_UNIT:'||'KGS'||CHR(10)||
	'CNTRTRW:'||NVL(TARE_CNTR_WGT,0)||CHR(10)||
	'CNTRTYPE:'||NVL(M.CNTR_TPSZ_CD,' ')||CHR(10)||
	'VGM_WGTQTY:'||NVL(M.VGM_WGT,0)||CHR(10)||
    'VGM_WGTCD:'||'KGS'||CHR(10)||
    'VGM_METHOD:'||NVL(M.VGM_MZD_TP_CD, ' ')||CHR(10)||
    'VGM_SIGNATURE:'||NVL(M.VGM_VRFY_SIG_CTNT, ' ')||CHR(10)||
	'SEALNBR:'||NVL(M.CNTR_SEAL_NO,' ')||CHR(10)||
	'FM_IND:'||NVL(M.FULL_MTY_CD,' ')||CHR(10)||
	'RF_IND:'||NVL(M.RC_FLG,' ')||CHR(10)||
	'DG_IND:'||NVL(M.DCGO_FLG,' ')||CHR(10)||
	'AK_IND:'||NVL(M.AWK_CGO_FLG,' ')||CHR(10)||
	'BK_IND:'||NVL(M.BB_CGO_FLG,' ')||CHR(10)||
	'TEMP:'||DECODE(M.FDO_TEMP,NULL,M.CDO_TEMP,M.FDO_TEMP)||CHR(10)||
	'TUNIT:'||DECODE(M.FDO_TEMP,NULL,'C','F')||CHR(10)||
	'VENT:'	|| CASE WHEN M.CNTR_VENT_RTO = 0 THEN 'C'
					WHEN M.CNTR_VENT_RTO > 0 AND M.CNTR_VENT_RTO < 35 THEN 'Q'
					WHEN M.CNTR_VENT_RTO >= 35 AND M.CNTR_VENT_RTO < 65 THEN 'H'
					WHEN M.CNTR_VENT_RTO >= 65 AND M.CNTR_VENT_RTO < 100 THEN 'T'
					WHEN M.CNTR_VENT_RTO = 100 THEN 'O' END		|| CHR(10)||
	'VENT_NUM:' || (SELECT DECODE(NVL(VENT_RTO, 0), 0, '', VENT_RTO) FROM BKG_RF_CGO WHERE BKG_NO = M.BKG_NO AND RC_SEQ = M.RC_SEQ) ||CHR(10)||
	'VENT_CMH:' || (SELECT DECODE(NVL(CBM_PER_HR_QTY, 0), 0, '', CBM_PER_HR_QTY) FROM BKG_RF_CGO WHERE BKG_NO = M.BKG_NO AND RC_SEQ = M.RC_SEQ) ||CHR(10)||
	'HUMID:'    ||  NVL((SELECT HUMID_NO FROM BKG_RF_CGO WHERE BKG_NO = M.BKG_NO AND RC_SEQ = M.RC_SEQ),0) ||CHR(10)||
    'MEASURE:'||NVL(M.MEAS_QTY,0)||CHR(10)||
	'MEASURE_UNIT:'||NVL(M.CNTR_MEAS_UT_CD,' ')||CHR(10)||
	'RDTYPE:'||NVL(M.RCV_TERM_CD,' ')||NVL(M.DE_TERM_CD,' ')||CHR(10)||
	'CMDT_DESC:'||@[cmdt_desc]||CHR(10)||
	'CMDTCD:'||@[cmdt_cd]||CHR(10)||
	'RF_REMARK:'||replace(NVL(M.LIST_RMK,' '),chr(13)||chr(10),' ')||CHR(10)||
	'RFDRY_IND:'||NVL(M.RD_CGO_FLG,' ')||CHR(10)||
	'OVF:'||NVL(M.OVR_FWRD_LEN,0)||CHR(10)||
	'OVR:'||NVL(M.OVR_BKWD_LEN,0)||CHR(10)||
	'OVH:'||NVL(M.OVR_HGT,0)||CHR(10)||
	'OVLW:'||NVL(M.OVR_PORT_LEN,0)||CHR(10)||
	'OVRW:'||NVL(M.OVR_SD_LEN,0)||CHR(10)||
	'OVWGT:'||NVL(M.OVR_CNTR_WGT,0)||CHR(10)||
	'OVWGT_UNIT:'||NVL(M.OVR_WGT_UT_CD,' ')||CHR(10)||
	'VOID_SLOT:'||NVL(( SELECT A.OVR_VOID_SLT_QTY
                          FROM BKG_AWK_CGO A 
                         WHERE A.BKG_NO = M.BKG_NO
                           AND NVL(A.CNTR_NO,TO_CHAR(A.AWK_CGO_SEQ)) = CASE WHEN NVL(A.CNTR_NO,'N') = 'N' THEN TO_CHAR(NVL(M.AWK_CGO_SEQ,0))
                                                                       ELSE M.CNTR_NO END ),0)||CHR(10)||
	'STWG_REQ:'||REPLACE(REPLACE(NVL(( SELECT A.STWG_RQST_DESC
                          FROM BKG_AWK_CGO A 
                         WHERE A.BKG_NO = M.BKG_NO
                           AND NVL(A.CNTR_NO,TO_CHAR(A.AWK_CGO_SEQ)) = CASE WHEN NVL(A.CNTR_NO,'N') = 'N' THEN TO_CHAR(NVL(M.AWK_CGO_SEQ,0))
                                                                       ELSE M.CNTR_NO END ),' '),CHR(10),''),CHR(13),'')||CHR(10)||
	'SOCIND:'||NVL(M.SOC_FLG,' ')||CHR(10)||
	'HAULAGE:'||' '||CHR(10)||
	'BKWGT:'||NVL(BB.GRS_WGT,0)||CHR(10)||
	'BKWGTU:'||NVL(BB.WGT_UT_CD,' ')||CHR(10)||
	'BKW:'||NVL(BB.DIM_WDT,0)||CHR(10)||
	'BKH:'||NVL(BB.DIM_HGT,0)||CHR(10)||
	'BKL:'||NVL(BB.DIM_LEN,0)||CHR(10)||
	'CNTROWN:'||NVL(O.OWNR_CO_CD,' ')||CHR(10)||
	'CNTRTRM:'||NVL(O.LSTM_CD,' ')||CHR(10) CNTR_INFO,

	BKG_JOIN_FNC(CURSOR(
        (
            SELECT CNTR_SEAL_NO 
              FROM BKG_CNTR_SEAL_NO
             WHERE BKG_NO = M.BKG_NO
              AND CNTR_NO = M.CNTR_NO
              AND CNTR_SEAL_SEQ > 0
         )
     ),'@') CNTR_SEAL_NO,

	M.CNTR_NO,
	'' IN_VVD_CD,
	'' IN_POL_CD,
	'' BKG_NO,
	'' CRE_USR_ID,
	'' BL_NO
FROM	BKG_CSTMS_TML_CLL M, BKG_BB_CGO BB, MST_CONTAINER O
WHERE	M.VSL_CD		= SUBSTR(@[in_vvd_cd],1,4)
AND	M.SKD_VOY_NO		= SUBSTR(@[in_vvd_cd],5,4)
AND	M.SKD_DIR_CD		= SUBSTR(@[in_vvd_cd],9,1)
AND	M.PORT_CD		= @[in_pol_cd]
#if (${in_pol_split_no} != '')
AND NVL(M.CLPT_IND_SEQ, '1')  = @[in_pol_split_no]
#end
AND	M.BKG_NO		= @[bkg_no]
AND	M.CNTR_NO		= @[cntr_no]
AND	M.BKG_NO        = BB.BKG_NO(+)
AND	M.CNTR_NO       = O.CNTR_NO(+)
AND M.CRE_USR_ID = @[cre_usr_id]
AND	rownum = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="in_vvd_cd" type="12" value="" out="N"/>
				<param name="in_pol_cd" type="12" value="" out="N"/>
				<param name="in_pol_split_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
