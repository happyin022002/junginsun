<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TransferOrderIssueDBDAOcopyTroSpclCgoSeqBySeqCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
insert into bkg_tro_spcl_cgo_seq(BKG_NO 
,IO_BND_CD 
,RTN_TRO_FLG 
,TRO_SEQ 
,TRO_SPCL_CGO_SEQ 
,SPCL_CGO_CD 
,SPCL_CGO_SEQ 
,CRE_USR_ID 
,CRE_DT 
,UPD_USR_ID 
,UPD_DT
)
select @[targetBkg] BKG_NO 
 ,IO_BND_CD 
,RTN_TRO_FLG 
,TRO_SEQ 
,TRO_SPCL_CGO_SEQ 
,SPCL_CGO_CD 
,SPCL_CGO_SEQ 
,@[usr_id] CRE_USR_ID 
,sysdate CRE_DT 
,@[usr_id] UPD_USR_ID 
,sysdate UPD_DT
from  bkg_tro_spcl_cgo_seq
where bkg_no =  @[bkg_no] 
and tro_seq = @[tro_seq]
and tro_spcl_cgo_seq = @[tro_sub_seq]			]]></sql>
			<params>
				<param name="targetBkg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="tro_seq" type="2" value="" out="N"/>
				<param name="tro_sub_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
