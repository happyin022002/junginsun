<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyUserAuthSetupUSQL">
			<desc><![CDATA[갱신]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_COM_USR_AUTH
   SET CSTMS_AUTH_FLG =
           CASE WHEN @[bl_vvd] = 1 AND PROC_ID = 'BLI' AND ACT_ID = 'VVD' THEN 'Y'
                WHEN @[bl_pod] = 1 AND PROC_ID = 'BLI' AND ACT_ID = 'POD' THEN 'Y'
                WHEN @[bl_del] = 1 AND PROC_ID = 'BLI' AND ACT_ID = 'DEL' THEN 'Y'
                WHEN @[bl_hub] = 1 AND PROC_ID = 'BLI' AND ACT_ID = 'HUB' THEN 'Y'
                WHEN @[bl_mib] = 1 AND PROC_ID = 'BLI' AND ACT_ID = 'MIB' THEN 'Y'
                WHEN @[bl_fpo] = 1 AND PROC_ID = 'BLI' AND ACT_ID = 'FPO' THEN 'Y'
                WHEN @[bl_ptt] = 1 AND PROC_ID = 'BLI' AND ACT_ID = 'PTT' THEN 'Y'
                WHEN @[mi_hub] = 1 AND PROC_ID = 'MIB' AND ACT_ID = 'HUB' THEN 'Y'
                WHEN @[of_mit] = 1 AND PROC_ID = 'OFM' AND ACT_ID = 'MIT' THEN 'Y'
                WHEN @[of_his] = 1 AND PROC_ID = 'OFM' AND ACT_ID = 'HIS' THEN 'Y'
           ELSE 'N' END
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE
 WHERE CNT_CD = 'US'
   AND USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="bl_vvd" type="12" value="" out="N"/>
				<param name="bl_pod" type="12" value="" out="N"/>
				<param name="bl_del" type="12" value="" out="N"/>
				<param name="bl_hub" type="12" value="" out="N"/>
				<param name="bl_mib" type="12" value="" out="N"/>
				<param name="bl_fpo" type="12" value="" out="N"/>
				<param name="bl_ptt" type="12" value="" out="N"/>
				<param name="mi_hub" type="12" value="" out="N"/>
				<param name="of_mit" type="12" value="" out="N"/>
				<param name="of_his" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
