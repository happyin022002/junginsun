<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchSubPsaMaxSerialNoRSQL">
			<desc><![CDATA[PSA RELEASE ORDER Table의 MAX SUB PSA SERIAL NUMBER +1 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  NVL( MAX( SUB_PSA_SER_NO ), 0 ) + 1 SUB_PSA_SER_NO
FROM   BKG_CSTMS_PSA_RLSE_ORD
WHERE  BKG_NO      =   @[bkg_no]
AND    BKG_SEQ     =   @[bkg_seq]
AND    PSA_SER_NO  =   @[psa_ser_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="psa_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
