<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGroupCommodityGuidelineDBDAOMaxGrpLocSeqRSQL">
			<desc><![CDATA[GRP_CMDT_SEQ의 max+1 값]]></desc>
			<sql><![CDATA[
SELECT 
       NVL((SELECT /*+ INDEX_DESC(a XPKPRI_SG_GRP_CMDT) */
                   grp_cmdt_seq
              FROM pri_sg_grp_cmdt a
             WHERE svc_scp_cd = @[svc_scp_cd]
               AND gline_seq = @[gline_seq]
			   AND prc_cust_tp_cd = @[prc_cust_tp_cd]
               AND rownum = 1), 0) + 1 AS next_seq
FROM DUAL			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="gline_seq" type="12" value="" out="N"/>
				<param name="prc_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
