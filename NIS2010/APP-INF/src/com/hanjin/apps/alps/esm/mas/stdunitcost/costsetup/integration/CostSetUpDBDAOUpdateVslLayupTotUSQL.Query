<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostSetUpDBDAOUpdateVslLayupTotUSQL">
			<desc><![CDATA[MAS_MNL_COST_STUP 테이블에 변경된 Vessel Charter / Layup  Expense 값으로 변경]]></desc>
			<sql><![CDATA[
UPDATE MAS_MNL_COST_STUP
SET OTR_EXPN_AMT = @[tot_sum]
	,UPD_USR_ID = @[usr_id]
	,UPD_DT = SYSDATE
WHERE COST_YRMON = (SELECT COST_YR||EQ_WK FROM MAS_WK_PRD WHERE COST_YR = SUBSTR(REPLACE(@[cost_yrmon],'-',''),1,4) AND COST_WK = SUBSTR(REPLACE(@[cost_yrmon],'-',''),5,2))
    AND COST_WK = SUBSTR(REPLACE(@[cost_yrmon],'-',''),5,2)
	AND RLANE_CD = @[rlane_cd]			]]></sql>
			<params>
				<param name="tot_sum" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
