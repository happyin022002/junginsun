<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOPriScgAproRt2VOUSQL">
			<desc><![CDATA[Approve 정보를 Rqst 테이블에 업데이트한다]]></desc>
			<sql><![CDATA[
UPDATE PRI_SCG_RQST_RT
   SET SCG_RQST_PROC_CD = @[scg_rqst_proc_cd]
	  ,UPD_USR_ID = @[upd_usr_id]
	  ,UPD_DT = sysdate
	  ,APRO_USR_ID = (SELECT USR_NM FROM COM_USER WHERE USR_ID = @[apro_usr_id])
	  ,APRO_DT = sysdate
	  ,DELT_FLG = NVL(@[delt_flg], 'N')
   WHERE SCG_SEQ = @[scg_seq]
   AND   SVC_SCP_CD = @[svc_scp_cd]
   AND   SCG_RQST_SEQ = @[scg_rqst_seq]
   AND   CHG_CD = @[chg_cd]			]]></sql>
			<params>
				<param name="scg_rqst_proc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="apro_usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="scg_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="scg_rqst_seq" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
