<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOseacrhManualBdrUserCheckRSQL">
			<desc><![CDATA[BDR권한 여부를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL((SELECT  ATTR_CTNT2 || ','|| ATTR_CTNT3 as OUTPUT_TEXT
FROM BKG_HRD_CDG_CTNT
where hrd_cdg_id ='BDR_MAN_USER'
AND ATTR_CTNT1 = @[input_text] 
AND ATTR_CTNT4 = 'N'),'0,0') AS OUTPUT_TEXT 
FROM DUAL			]]></sql>
			<params>
				<param name="input_text" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
