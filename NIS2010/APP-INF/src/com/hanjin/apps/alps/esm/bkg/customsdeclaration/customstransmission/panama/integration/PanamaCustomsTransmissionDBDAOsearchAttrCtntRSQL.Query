<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PanamaCustomsTransmissionDBDAOsearchAttrCtntRSQL">
			<desc><![CDATA[searchAttrCtnt]]></desc>
			<sql><![CDATA[
SELECT 
	ATTR_CTNT2 
FROM 
	BKG_CSTMS_CD_CONV_CTNT
WHERE CNT_CD = 'PA'
AND CSTMS_DIV_ID = 'PANAMA_MCT_CD'
AND ATTR_CTNT1 = @[hamotrpcd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="hamotrpcd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
