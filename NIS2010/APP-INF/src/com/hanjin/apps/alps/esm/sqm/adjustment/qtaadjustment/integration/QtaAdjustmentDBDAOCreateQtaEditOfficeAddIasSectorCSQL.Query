<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOCreateQtaEditOfficeAddIasSectorCSQL">
			<desc><![CDATA[Qta Edit Office Add for IAS Sector을 생성합니다.

*History
2014.06.26 이혜민 PAIR, Office는 같은데 POL_CALL_SEQ, POD_CALL_SEQ 가 달라 중복 데이터 insert 수정
2014.07.04 이혜민 SQM_SCTR_LANE_OFC 내 SQM_ACT_FLG='Y' 추가
2016.02.05 최성민 [CHM-201639787] SQM Planning 도중 & Planning 완료 후 노선, P/F Group, Sector 추가 로직 Process 변경
2016.04.11 최성민 [CHM-201640884] Sector Office Add 로직 수정
2016.04.22 CHM-201641278 [SQM] IAS Trade 판매목표 수립 Portion 연결 시스템 수정 요청 CSR]]></desc>
			<sql><![CDATA[
MERGE INTO SQM_SCTR_CFM_QTA A1 USING 
(
 SELECT DISTINCT A2.QTA_RLSE_VER_NO
      , A1.BSE_TP_CD
      , A1.BSE_YR
      , A1.BSE_QTR_CD
      , A1.OFC_VW_CD
      , A1.RLANE_CD
      , A1.DIR_CD
      , A2.VSL_CD
      , A2.SKD_VOY_NO
      , A2.SKD_DIR_CD
      , A1.RGN_OFC_CD
      , A1.POL_CD
      , A1.POD_CD
        -- Adjustment by VVD for IAS 화면의 VVD 별 PF GRP 를 가져오고 없으면 TARGET VVD FIX 테이블에서 참조한다.
      , NVL(MAX(A4.PF_GRP_CD) OVER (PARTITION BY A4.RLANE_CD, A4.VSL_CD, A4.SKD_VOY_NO, A4.SKD_DIR_CD), QT.PF_GRP_CD) AS PF_GRP_CD
      , A1.TRD_CD
      , A1.SUB_TRD_CD
      , A1.RHQ_CD
      , NVL((SELECT N3RD_PRNT_OFC_CD FROM SQM_ORGANIZATION_V WHERE OFC_CD = A1.RGN_OFC_CD), '') AS AQ_CD
      , A2.FNL_BSA_CAPA
--      , A2.LOD_QTY
      ,NVL((SELECT NVL(PLANNING.LOD_QTY, 0) FROM SQM_SCTR_LOD_REV PLANNING
        WHERE PLANNING.BSE_TP_CD = A1.BSE_TP_CD
        AND PLANNING.BSE_YR = A1.BSE_YR
        AND PLANNING.BSE_QTR_CD = A1.BSE_QTR_CD
        AND PLANNING.OFC_VW_CD = A1.OFC_VW_CD
        AND PLANNING.RLANE_CD = A1.RLANE_CD
        AND PLANNING.DIR_CD = A1.DIR_CD
        AND PLANNING.FNL_BSA_CAPA = A2.FNL_BSA_CAPA
        AND PLANNING.PF_GRP_CD = NVL((SELECT MAX(CFM.PF_GRP_CD) FROM SQM_SCTR_CFM_QTA CFM
                                        WHERE CFM.RLANE_CD = A1.RLANE_CD
                                        AND CFM.VSL_CD = A2.VSL_CD
                                        AND CFM.SKD_VOY_NO = A2.SKD_VOY_NO
                                        AND CFM.SKD_DIR_CD = A2.SKD_DIR_CD), QT.PF_GRP_CD)
        AND PLANNING.RGN_OFC_CD = A1.RGN_OFC_CD
        AND PLANNING.POL_CD = A1.POL_CD
        AND PLANNING.POD_CD = A1.POD_CD
        AND ROWNUM < 2), 0) AS LOD_QTY
--      , A2.GRS_REV AS GRS_RPB_REV
      ,NVL((SELECT NVL(PLANNING.GRS_RPB_REV, 0) FROM SQM_SCTR_LOD_REV PLANNING
        WHERE PLANNING.BSE_TP_CD = A1.BSE_TP_CD
        AND PLANNING.BSE_YR = A1.BSE_YR
        AND PLANNING.BSE_QTR_CD = A1.BSE_QTR_CD
        AND PLANNING.OFC_VW_CD = A1.OFC_VW_CD
        AND PLANNING.RLANE_CD = A1.RLANE_CD
        AND PLANNING.DIR_CD = A1.DIR_CD
        AND PLANNING.FNL_BSA_CAPA = A2.FNL_BSA_CAPA
        AND PLANNING.PF_GRP_CD = NVL((SELECT MAX(CFM.PF_GRP_CD) FROM SQM_SCTR_CFM_QTA CFM
                                        WHERE CFM.RLANE_CD = A1.RLANE_CD
                                        AND CFM.VSL_CD = A2.VSL_CD
                                        AND CFM.SKD_VOY_NO = A2.SKD_VOY_NO
                                        AND CFM.SKD_DIR_CD = A2.SKD_DIR_CD), QT.PF_GRP_CD)
        AND PLANNING.RGN_OFC_CD = A1.RGN_OFC_CD
        AND PLANNING.POL_CD = A1.POL_CD
        AND PLANNING.POD_CD = A1.POD_CD
        AND ROWNUM < 2), 0) AS GRS_RPB_REV
      , A3.PA_CM_UC_AMT
      , A3.RA_CM_UC_AMT
      , MIN(A1.POL_CALL_SEQ) OVER (PARTITION BY A1.BSE_TP_CD, A1.BSE_YR, A1.BSE_QTR_CD, A1.OFC_VW_CD, A1.RLANE_CD, A1.DIR_CD, A1.RGN_OFC_CD, A1.POL_CD, A1.POD_CD, A1.TRD_CD, A1.SUB_TRD_CD) POL_CALL_SEQ
      , MAX(A1.POD_CALL_SEQ) OVER (PARTITION BY A1.BSE_TP_CD, A1.BSE_YR, A1.BSE_QTR_CD, A1.OFC_VW_CD, A1.RLANE_CD, A1.DIR_CD, A1.RGN_OFC_CD, A1.POL_CD, A1.POD_CD, A1.TRD_CD, A1.SUB_TRD_CD) POD_CALL_SEQ
      , 'I'       AS SQM_CNG_TP_CD
      , @[cre_usr_id] AS CRE_USR_ID
      , SYSDATE   AS CRE_DT
      , @[upd_usr_id] AS UPD_USR_ID
      , SYSDATE   AS UPD_DT
   FROM SQM_SCTR_LANE_OFC A1
      , SQM_CFM_TGT_VVD A2
      , SQM_SCTR_PAIR_COST A3
      , SQM_SCTR_CFM_QTA A4
      , (SELECT QT.BSE_TP_CD
              , QT.BSE_YR
              , QT.BSE_QTR_CD
              , QT.TRD_CD
              , QT.RLANE_CD
              , QT.DIR_CD
              , QT.VSL_CD
              , QT.SKD_VOY_NO
              , QT.SKD_DIR_CD
              , PG.PF_GRP_CD
           FROM SQM_SCTR_PF_GRP PG
              , SQM_QTA_TGT_VVD QT
          WHERE PG.BSE_TP_CD    = QT.BSE_TP_CD
            AND PG.BSE_YR       = QT.BSE_YR
            AND PG.BSE_QTR_CD   = QT.BSE_QTR_CD
            AND PG.RLANE_CD     = QT.RLANE_CD
            AND PG.PF_SVC_TP_CD = QT.PF_SVC_TP_CD
            AND PG.RLANE_CD     = QT.RLANE_CD  
          UNION            
         SELECT QT.BSE_TP_CD
              , QT.BSE_YR
              , QT.BSE_QTR_CD
              , QT.TRD_CD
              , QT.RLANE_CD
              , QT.DIR_CD
              , QT.VSL_CD
              , QT.SKD_VOY_NO
              , QT.SKD_DIR_CD
              , PG.PF_GRP_CD
           FROM SQM_SCTR_PF_GRP PG
              , SQM_SCTR_ADD_TGT_VVD QT
          WHERE PG.BSE_TP_CD    = QT.BSE_TP_CD
            AND PG.BSE_YR       = QT.BSE_YR
            AND PG.BSE_QTR_CD   = QT.BSE_QTR_CD
            AND PG.RLANE_CD     = QT.RLANE_CD
            AND PG.PF_GRP_CD    = QT.PF_GRP_CD
            AND PG.RLANE_CD     = QT.RLANE_CD
        ) QT
  WHERE 1              =1
    AND A1.SQM_ACT_FLG = 'Y'
    AND A1.BSE_TP_CD   = @[bse_tp_cd]
    AND A1.BSE_YR      = @[bse_yr]
    AND A1.BSE_QTR_CD  = @[bse_qtr_cd]
    AND A1.OFC_VW_CD   = SUBSTR(@[ofc_vw_cd], 0 ,1)
    AND A1.SUB_TRD_CD  = @[sub_trd_cd]
    AND A1.RLANE_CD    = @[rlane_cd]
    AND A1.DIR_CD      = @[dir_cd]
    AND A1.POL_CD      = @[pol_cd]
    AND A1.POD_CD      = @[pod_cd]
    AND A1.RHQ_CD      = @[rhq_cd]
    AND A1.RGN_OFC_CD  = @[rgn_ofc_cd]

    AND SUBSTR(A2.QTA_RLSE_VER_NO, -2) = '02'
    AND A1.BSE_TP_CD       = A2.BSE_TP_CD
    AND A1.BSE_YR          = A2.BSE_YR
    AND A1.BSE_QTR_CD      = A2.BSE_QTR_CD
    AND A1.SUB_TRD_CD      = A2.SUB_TRD_CD
    AND A1.RLANE_CD        = A2.RLANE_CD
    AND A1.DIR_CD          = A2.DIR_CD
    AND A1.BSE_TP_CD       = A3.BSE_TP_CD
    AND A1.BSE_YR          = A3.BSE_YR
    AND A1.BSE_QTR_CD      = A3.BSE_QTR_CD
    AND A1.SUB_TRD_CD      = A3.SUB_TRD_CD
    AND A1.RLANE_CD        = A3.RLANE_CD
    AND A1.DIR_CD          = A3.DIR_CD
    AND A1.POL_CD          = A3.POL_CD
    AND A1.POD_CD          = A3.POD_CD
    AND A2.QTA_RLSE_VER_NO = A4.QTA_RLSE_VER_NO(+)
    AND A2.BSE_TP_CD       = A4.BSE_TP_CD(+)
    AND A2.BSE_YR          = A4.BSE_YR(+)
    AND A2.BSE_QTR_CD      = A4.BSE_QTR_CD(+)
    AND A2.TRD_CD          = A4.TRD_CD(+)
    AND A2.RLANE_CD        = A4.RLANE_CD(+)
    AND A2.DIR_CD          = A4.DIR_CD(+)
    AND A2.VSL_CD          = A4.VSL_CD(+)
    AND A2.SKD_VOY_NO      = A4.SKD_VOY_NO(+)
    AND A2.SKD_DIR_CD      = A4.SKD_DIR_CD(+)
    AND A2.BSE_TP_CD       = QT.BSE_TP_CD(+)
    AND A2.BSE_YR          = QT.BSE_YR(+)
    AND A2.BSE_QTR_CD      = QT.BSE_QTR_CD(+)
    AND A2.TRD_CD          = QT.TRD_CD(+)
    AND A2.RLANE_CD        = QT.RLANE_CD(+)
    AND A2.DIR_CD          = QT.DIR_CD(+)
    AND A2.VSL_CD          = QT.VSL_CD(+)
    AND A2.SKD_VOY_NO      = QT.SKD_VOY_NO(+)
    AND A2.SKD_DIR_CD      = QT.SKD_DIR_CD(+)
    AND (A4.PF_GRP_CD = @[pf_grp_cd] OR QT.PF_GRP_CD = @[pf_grp_cd])

) A2
ON (    A1.QTA_RLSE_VER_NO = A2.QTA_RLSE_VER_NO
	AND A1.BSE_TP_CD       = A2.BSE_TP_CD
	AND A1.BSE_YR	       = A2.BSE_YR
	AND A1.BSE_QTR_CD      = A2.BSE_QTR_CD
	AND A1.OFC_VW_CD       = A2.OFC_VW_CD
	AND A1.RLANE_CD	       = A2.RLANE_CD
	AND A1.DIR_CD	       = A2.DIR_CD
	AND A1.VSL_CD	       = A2.VSL_CD
	AND A1.SKD_VOY_NO      = A2.SKD_VOY_NO
	AND A1.SKD_DIR_CD      = A2.SKD_DIR_CD
	AND A1.RGN_OFC_CD      = A2.RGN_OFC_CD
	AND A1.POL_CD	       = A2.POL_CD
	AND A1.POD_CD	       = A2.POD_CD )

WHEN NOT MATCHED THEN
	INSERT (
	  A1.QTA_RLSE_VER_NO
	, A1.BSE_TP_CD
	, A1.BSE_YR
	, A1.BSE_QTR_CD
	, A1.OFC_VW_CD
	, A1.RLANE_CD
	, A1.DIR_CD
	, A1.VSL_CD
	, A1.SKD_VOY_NO
	, A1.SKD_DIR_CD
	, A1.RGN_OFC_CD
	, A1.POL_CD
	, A1.POD_CD
	, A1.PF_GRP_CD
	, A1.TRD_CD
	, A1.SUB_TRD_CD
	, A1.RHQ_CD
	, A1.AQ_CD
	, A1.FNL_BSA_CAPA
	, A1.LOD_QTY
	, A1.GRS_RPB_REV
	, A1.PA_CM_UC_AMT
	, A1.RA_CM_UC_AMT
	, A1.POL_CALL_SEQ
	, A1.POD_CALL_SEQ
	, A1.SQM_CNG_TP_CD
	, A1.CRE_USR_ID
	, A1.CRE_DT
	, A1.UPD_USR_ID
	, A1.UPD_DT )
   VALUES (
	  A2.QTA_RLSE_VER_NO
	, A2.BSE_TP_CD
	, A2.BSE_YR
	, A2.BSE_QTR_CD
	, A2.OFC_VW_CD
	, A2.RLANE_CD
	, A2.DIR_CD
	, A2.VSL_CD
	, A2.SKD_VOY_NO
	, A2.SKD_DIR_CD
	, A2.RGN_OFC_CD
	, A2.POL_CD
	, A2.POD_CD
	, A2.PF_GRP_CD
	, A2.TRD_CD
	, A2.SUB_TRD_CD
	, A2.RHQ_CD
	, A2.AQ_CD
	, A2.FNL_BSA_CAPA
	, A2.LOD_QTY
	, A2.GRS_RPB_REV
	, A2.PA_CM_UC_AMT
	, A2.RA_CM_UC_AMT
	, A2.POL_CALL_SEQ
	, A2.POD_CALL_SEQ
	, A2.SQM_CNG_TP_CD
	, A2.CRE_USR_ID
	, A2.CRE_DT
	, A2.UPD_USR_ID
	, A2.UPD_DT )			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bse_tp_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="bse_qtr_cd" type="12" value="" out="N"/>
				<param name="ofc_vw_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="pf_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
