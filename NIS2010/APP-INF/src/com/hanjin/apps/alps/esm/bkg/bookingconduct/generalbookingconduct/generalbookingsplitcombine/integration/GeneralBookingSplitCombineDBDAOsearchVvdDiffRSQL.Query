<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSplitCombineDBDAOsearchVvdDiffRSQL">
			<desc><![CDATA[전체 VVD 및 Location, Yard 비교 (Pre/Trunk/Post 순서대로 점검)]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(TGT.BKG_NO),COUNT(SRC.BKG_NO),'Y','N') AS DIFF
  FROM BKG_VVD TGT
      ,BKG_VVD SRC
WHERE TGT.BKG_NO = @[target_bkg]
   AND SRC.BKG_NO(+) = @[source_bkg]
   AND TGT.VSL_PRE_PST_CD = SRC.VSL_PRE_PST_CD(+)
   AND TGT.VSL_SEQ = SRC.VSL_SEQ(+)
   AND NVL(TGT.VSL_CD,'X') = NVL(SRC.VSL_CD(+),'X')
   AND NVL(TGT.SKD_VOY_NO,'X') = NVL(SRC.SKD_VOY_NO(+),'X')
   AND NVL(TGT.SKD_DIR_CD,'X') = NVL(SRC.SKD_DIR_CD(+),'X')
   AND TGT.POL_CD = SRC.POL_CD(+)
   AND TGT.POL_YD_CD = SRC.POL_YD_CD(+)
   AND TGT.POD_CD = SRC.POD_CD(+)
   AND TGT.POD_YD_CD = SRC.POD_YD_CD(+)
			]]></sql>
			<params>
				<param name="target_bkg" type="12" value="" out="N"/>
				<param name="source_bkg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
