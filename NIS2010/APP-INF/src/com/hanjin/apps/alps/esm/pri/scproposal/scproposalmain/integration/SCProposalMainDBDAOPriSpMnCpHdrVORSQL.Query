<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCProposalMainDBDAOPriSpMnCpHdrVORSQL">
			<desc><![CDATA[s/c copy 의 Boiler plate, Affilate 의 record 건수]]></desc>
			<sql><![CDATA[
SELECT (
        SELECT COUNT(*)
        FROM  PRI_SP_AFIL
        WHERE PROP_NO  = @[prop_no]
        AND   AMDT_SEQ = @[amdt_seq]
        AND ROWNUM = 1
        ) AFIL_CNT
       ,(
        SELECT COUNT(*)
        FROM  PRI_SP_BLPL
        WHERE PROP_NO  = @[prop_no]
        AND   AMDT_SEQ = @[amdt_seq]
        AND ROWNUM = 1
       ) BLPL_CNT
FROM DUAL       			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
