<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSASpecialManifestDBDAOsearchRcvLogSeqRSQL">
			<desc><![CDATA[수신정보 키값(RCV_LOG_SEQ) 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(RCV_LOG_SEQ), 0) + 1
FROM BKG_CSTMS_PSA_DG_RCV_ERR
WHERE PSA_EDI_MSG_TP_ID = @[msg_tp_id]			]]></sql>
			<params>
				<param name="msg_tp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
