<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintItemDBDAOMultiConstraintItemUSQL">
			<desc><![CDATA[SAQ_MDL_CNST  정보 UPDATE]]></desc>
			<sql><![CDATA[
UPDATE SAQ_MDL_CNST
   SET MDL_CNST_NM   = @[mdl_cnst_nm],
       RALOC_USE_FLG = DECODE(@[raloc_use_flg], '1', 'Y', 'N'),
       UPD_USR_ID    = @[upd_usr_id],
       UPD_DT        = SYSDATE
 WHERE MDL_CNST_CD   = @[mdl_cnst_cd]			]]></sql>
			<params>
				<param name="mdl_cnst_nm" type="12" value="" out="N"/>
				<param name="raloc_use_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mdl_cnst_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
