<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltSearchMOFLaneListVOUSQL">
			<desc><![CDATA[Korea MOF Filing (Base Table) - Scope & Location UPDATE]]></desc>
			<sql><![CDATA[
UPDATE PRI_MOF_LANE_MAPG
   SET LOC_CD = @[loc_cd]
     , MOF_LANE_CD = @[mof_lane_cd]
     , MOF_LOC_ID = @[mof_loc_id]
     , MOF_LOC_NM = @[mof_loc_nm]
     , FILE_USE_ONY_FLG = DECODE(NVL(@[file_use_ony_flg],'0'),'0','N','1','Y')
     , DELT_FLG = DECODE(NVL(@[delt_flg],'0'),'0','N','1','Y')
     , UPD_OFC_CD = @[upd_ofc_cd]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT = SYSDATE
 WHERE ORG_DEST_TP_CD = @[org_dest_tp_cd]
   AND MAPG_SEQ = @[mapg_seq]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="mof_lane_cd" type="12" value="" out="N"/>
				<param name="mof_loc_id" type="12" value="" out="N"/>
				<param name="mof_loc_nm" type="12" value="" out="N"/>
				<param name="file_use_ony_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="mapg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
