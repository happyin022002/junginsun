<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOInsertVslLayupTotCopyCSQL">
			<desc><![CDATA[COA_MNL_COST_STUP 테이블에 total값 입력]]></desc>
			<sql><![CDATA[
INSERT INTO COA_MNL_COST_STUP 
(
    COST_YRMON
    ,COST_WK
    ,TRD_CD
    ,RLANE_CD
    ,IOC_CD
    ,DIR_CD
    ,SUB_TRD_CD
    ,OTR_EXPN_AMT
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
    ,MTY_INV_AMT
    ,APLY_ADJ_PL_FLG
)
VALUES
(
    (SELECT COST_YR||EQ_WK FROM COA_WK_PRD WHERE COST_YR = SUBSTR(REPLACE(@[f_tar_week],'-',''),1,4) AND COST_WK = SUBSTR(REPLACE(@[f_tar_week],'-',''),5,2))
    ,SUBSTR(REPLACE(@[f_tar_week],'-',''),5,2)
    ,'COM'
    ,@[rlane_cd]
    ,'O'
    ,'M'
    ,'OT'
    ,@[tot_sum]
    ,@[usr_id]
    ,SYSDATE
    ,@[usr_id]
    ,SYSDATE
    ,NULL
    ,NULL    
)			]]></sql>
			<params>
				<param name="f_tar_week" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="tot_sum" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
