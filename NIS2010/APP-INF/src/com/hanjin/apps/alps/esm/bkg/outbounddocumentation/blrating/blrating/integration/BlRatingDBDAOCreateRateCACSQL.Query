<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOCreateRateCACSQL">
			<desc><![CDATA[BlRatingDBDAOCreateRateCACSQL]]></desc>
			<sql><![CDATA[
#if (${copy_type_cd} == 'BKG')
INSERT INTO BKG_RATE (
    BKG_NO 
#else
INSERT INTO BKG_RT_HIS (
    BKG_NO 
    , CORR_NO 
#end
    , RT_BL_TP_CD 
    , FRT_TERM_CD 
    , BKG_CTRT_TP_CD 
    , PPD_RCV_OFC_CD 
    , PPD_PAYR_CNT_CD 
    , PPD_PAYR_CUST_SEQ 
    , CLT_OFC_CD 
    , CLT_PAYR_CNT_CD 
    , CLT_PAYR_CUST_SEQ 
    , REV_DIV_CD 
    , BKG_RT_WHF_EXPT_CD 
    , WHF_SHPR_RGST_NO 
    , RT_APLY_DT 
    , CGO_RCV_DT 
    , DIFF_RMK 
    , RT_INTER_RMK
    , DOC_INTER_RMK
    , AUD_STS_CD
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
    , PRC_RT_MTCH_PATT_CD 
    , PRC_GEN_SPCL_RT_TP_CD 
    , PRC_CMDT_HDR_SEQ 
    , PRC_ROUT_SEQ  
