    , DHF_LOC_CD
    , DDC_CURR_CD
    , DHF_CURR_CD
    , GRP_RAT_RSLT_CD
    , GRP_RAT_FAIL_RSN_CD
    , GRP_RAT_CHK_FLG
    , MST_RFA_ROUT_ID
    , IDA_IHC_AUD_STS_CD
    , IDA_THC_AUD_STS_CD
    , IDA_AUD_BAT_DT
   )
#if (${copy_type_cd} == 'BKG')
SELECT BKG_NO 
#elseif (${copy_type_cd} == 'TEMP')
SELECT BKG_NO 
        , 'TMP0000001' CORR_NO 
#else
SELECT BKG_NO 
        , @[ca_no] CORR_NO 
#end
        , RT_BL_TP_CD 
        , FRT_TERM_CD 
        , BKG_CTRT_TP_CD 
        , PPD_RCV_OFC_CD 
        , PPD_PAYR_CNT_CD 
        , PPD_PAYR_CUST_SEQ 
        , CLT_OFC_CD 
        , CLT_PAYR_CNT_CD 
        , CLT_PAYR_CUST_SEQ 
        , REV_DIV_CD 
        , BKG_RT_WHF_EXPT_CD 
        , WHF_SHPR_RGST_NO 
        , RT_APLY_DT 
        , CGO_RCV_DT 
        , DIFF_RMK 
        , RT_INTER_RMK
        , DOC_INTER_RMK
        , AUD_STS_CD 
        , CRE_USR_ID 
        , CRE_DT 
        , UPD_USR_ID 
        , sysdate
        , PRC_RT_MTCH_PATT_CD 
        , PRC_GEN_SPCL_RT_TP_CD 
        , PRC_CMDT_HDR_SEQ 
        , PRC_ROUT_SEQ 
        , N1ST_REV_AUD_DT
        , LST_AUD_BAT_DT
        , CALC_CTRT_TP_CD 
        , OFT_MSS_FLG
        , OFT_CHK_USR_ID
        , OFT_CHK_DT
        , RT_CHK_RSLT_CD
        , RT_CHK_DT
        , DHF_LOC_CD
        , DDC_CURR_CD
        , DHF_CURR_CD
        , GRP_RAT_RSLT_CD
        , GRP_RAT_FAIL_RSN_CD
        , GRP_RAT_CHK_FLG
        , MST_RFA_ROUT_ID
        , IDA_IHC_AUD_STS_CD
        , IDA_THC_AUD_STS_CD
        , IDA_AUD_BAT_DT
#if (${copy_type_cd} == 'BKG')
  FROM BKG_RT_HIS
 WHERE BKG_NO  = @[bkg_no]
   AND CORR_NO = 'TMP0000001'
#else
  FROM BKG_RATE
 WHERE BKG_NO = @[bkg_no]
#end			]]></sql>
			<params>
				<param name="ca_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
