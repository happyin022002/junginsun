<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PRICommonDBDAOCmpbCommodityGroupNmRSQL">
			<desc><![CDATA[CmpbCommodityGroupNm]]></desc>
			<sql><![CDATA[
SELECT PRC_GRP_CMDT_DESC AS NM
  FROM PRI_CMPB_GRP_CMDT
 WHERE SVC_SCP_CD = @[etc1]
   AND CRE_OFC_CD = @[etc2]
   AND GLINE_SEQ = @[etc3]
   AND PRC_GRP_CMDT_CD = @[cd]			]]></sql>
			<params>
				<param name="etc1" type="12" value="" out="N"/>
				<param name="etc2" type="12" value="" out="N"/>
				<param name="etc3" type="12" value="" out="N"/>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
