<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnersAccountDBDAOOwnersAccountCsulSlpCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO FMS_CSUL_SLP (
	SLP_TP_CD
,	SLP_FUNC_CD
,	SLP_OFC_CD
,	SLP_ISS_DT
,	SLP_SER_NO	-- 5
,	SLP_SEQ_NO
,	ACCT_CD
,	CTR_CD
,	SLP_LOC_CD
,	CSR_CURR_CD	-- 10
,	CSR_AMT
,	CSR_DESC
,	VNDR_SEQ
,	TRNS_CURR_CD
,	TRNS_AMT	-- 15
,	STL_FLG
,	INV_SEQ
,   FLET_SRC_TP_CD
,	VSL_CD
,	SKD_VOY_NO	-- 20
,	SKD_DIR_CD
,	REV_DIR_CD
,	ORG_SLP_TP_CD
,	ORG_SLP_FUNC_CD
,	ORG_SLP_OFC_CD	-- 25
,	ORG_ISS_DT
,	ORG_SLP_SER_NO
,	ORG_SLP_SEQ_NO
,	LSG_GR_NO
,	TO_INV_NO	-- 30
,	VVD_EFF_DT
,	VVD_EXP_DT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID	-- 35
,	UPD_DT
,	ACCT_ITM_SEQ
,	OA_LOC_CD
,	OA_INV_DT
,	ATCH_FILE_OA_LNK_ID	-- 40
,	PAIR_SLP_TP_CD
,	PAIR_SLP_FUNC_CD
,	PAIR_SLP_OFC_CD
,	PAIR_SLP_ISS_DT
,	PAIR_SLP_SER_NO	-- 45
,	PAIR_SLP_SEQ_NO
) SELECT  
	@[slp_tp_cd]
,	@[slp_func_cd]
,	@[slp_ofc_cd]
,   TO_CHAR(SYSDATE, 'YYMMDD')
,	@[slp_ser_no]	-- 5
,	NVL(@[slp_seq_no],    (SELECT LPAD(NVL(TO_NUMBER(MAX(SLP_SEQ_NO)),0)+1,4,'0') 
						   FROM 	FMS_CSUL_SLP
						   WHERE	SLP_TP_CD	= @[slp_tp_cd]
					       AND	SLP_FUNC_CD = @[slp_func_cd]
						   AND	SLP_OFC_CD  = @[slp_ofc_cd]
					       AND	SLP_ISS_DT  = TO_CHAR(SYSDATE, 'YYMMDD')
						   AND	SLP_SER_NO  = @[slp_ser_no]
						   AND  SLP_SEQ_NO < 1000
						  )
		)
,	@[acct_cd]
,	(SELECT M.AP_CTR_CD
	   FROM MDM_ORGANIZATION M
	  WHERE M.OFC_CD = @[slp_ofc_cd])
,	(SELECT N.LOC_CD
   	   FROM MDM_ORGANIZATION N
  	  WHERE N.OFC_CD = @[slp_ofc_cd])
,	@[csr_curr_cd]	-- 10
,	@[csr_amt]
,	@[csr_desc]
,	@[vndr_seq]
,	'USD'
,	@[trns_amt]	-- 15
,	'N'
,	@[inv_seq]
,   '04'
,	SUBSTR(@[vvd_cd],1,4)
,	SUBSTR(@[vvd_cd],5,4)	-- 20
,	SUBSTR(@[vvd_cd],9,1)
,	SUBSTR(@[vvd_cd],10,1)
,	@[org_slp_tp_cd]
,	@[org_slp_func_cd]
,	@[org_slp_ofc_cd]	-- 25
,	@[org_iss_dt]
,	@[org_slp_ser_no]
,	@[org_slp_seq_no]
,	@[lsg_gr_no]
,	@[to_inv_no]	-- 30
,	TO_DATE(REPLACE(@[vvd_eff_dt],'-',''),'yyyymmdd')
,	TO_DATE(REPLACE(@[vvd_exp_dt],'-',''),'yyyymmdd')
,	@[cre_usr_id]
,	SYSDATE
,	@[cre_usr_id]	-- 35
,	SYSDATE
,	@[acct_itm_seq]
,	@[oa_loc_cd]
,	@[oa_inv_dt]	-- 39
,	@[slp_tp_cd] || @[slp_func_cd] || @[slp_ofc_cd] || TO_CHAR(SYSDATE, 'YYMMDD') || @[slp_ser_no] ||@[slp_seq_no]
,	@[pair_slp_tp_cd]
,	@[pair_slp_func_cd]
,	@[pair_slp_ofc_cd]
,	@[pair_slp_iss_dt]
,	@[pair_slp_ser_no]	-- 45
,	@[pair_slp_seq_no]
FROM DUAL			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="" out="N"/>
				<param name="slp_func_cd" type="12" value="" out="N"/>
				<param name="slp_ofc_cd" type="12" value="" out="N"/>
				<param name="slp_ser_no" type="12" value="" out="N"/>
				<param name="slp_seq_no" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="csr_curr_cd" type="12" value="" out="N"/>
				<param name="csr_amt" type="12" value="" out="N"/>
				<param name="csr_desc" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="trns_amt" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="org_slp_tp_cd" type="12" value="" out="N"/>
				<param name="org_slp_func_cd" type="12" value="" out="N"/>
				<param name="org_slp_ofc_cd" type="12" value="" out="N"/>
				<param name="org_iss_dt" type="12" value="" out="N"/>
				<param name="org_slp_ser_no" type="12" value="" out="N"/>
				<param name="org_slp_seq_no" type="12" value="" out="N"/>
				<param name="lsg_gr_no" type="12" value="" out="N"/>
				<param name="to_inv_no" type="12" value="" out="N"/>
				<param name="vvd_eff_dt" type="12" value="" out="N"/>
				<param name="vvd_exp_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="acct_itm_seq" type="12" value="" out="N"/>
				<param name="oa_loc_cd" type="12" value="" out="N"/>
				<param name="oa_inv_dt" type="12" value="" out="N"/>
				<param name="pair_slp_tp_cd" type="12" value="" out="N"/>
				<param name="pair_slp_func_cd" type="12" value="" out="N"/>
				<param name="pair_slp_ofc_cd" type="12" value="" out="N"/>
				<param name="pair_slp_iss_dt" type="12" value="" out="N"/>
				<param name="pair_slp_ser_no" type="12" value="" out="N"/>
				<param name="pair_slp_seq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
