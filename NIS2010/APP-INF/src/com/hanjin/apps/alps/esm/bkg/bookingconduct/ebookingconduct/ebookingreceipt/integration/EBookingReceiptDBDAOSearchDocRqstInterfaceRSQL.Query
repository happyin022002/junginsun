<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchDocRqstInterfaceRSQL">
			<desc><![CDATA[external request 처리를 위해 external rqst의 Booking 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT M.BKG_NO,
       NVL(M.XTER_BL_TP_CD, I.RQST_BL_TP_CD) RQST_BL_TP_CD,
       NVL(NVL(M.INCL_RT_BL_KNT, I.OBL_RT_INCL_KNT), 0) OBL_RT_INCL_KNT,
       NVL(NVL(M.XCLD_RT_BL_KNT, I.OBL_RT_XCLD_KNT), 0) OBL_RT_XCLD_KNT,
       NVL(I.OBL_TTL_KNT, 0) OBL_TTL_KNT,
       NVL(NVL(M.NON_NEGO_RT_INCL_KNT, I.NON_NEGO_RT_INCL_KNT), 0) NON_NEGO_RT_INCL_KNT,
       NVL(NVL(M.NON_NEGO_RT_XCLD_KNT, I.NON_NEGO_RT_XCLD_KNT), 0) NON_NEGO_RT_XCLD_KNT,
       NVL(I.CPY_TTL_KNT, 0) CPY_TTL_KNT,
       NVL(M.BL_ISS_LOC_CD, I.RQST_ISS_PLC_NM) RQST_ISS_PLC_NM,
       TO_CHAR(NVL(M.BL_ISS_DT, RQST_ISS_DT),'YYYY-MM-DD') RQST_ISS_DT,
       I.BL_DE_TO_CD,
       I.BL_DE_MZD_CD,
       I.BL_DOC_RQST_RMK,
       '' LOC_NM,
       I.WBL_RT_TP_CD,
       I.WBL_EML
FROM BKG_XTER_RQST_MST M, BKG_BL_ISS I
WHERE M.BKG_NO = I.BKG_NO(+)
AND M.XTER_SNDR_ID = @[xter_sndr_id]
AND M.XTER_RQST_NO = @[xter_rqst_no]
AND M.XTER_RQST_SEQ = @[xter_rqst_seq]			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
