<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOPriScgGrpCmdtVORSQL">
			<desc><![CDATA[PRI_SCG_GRP_CMDT 테이블 조회]]></desc>
			<sql><![CDATA[
SELECT SCG_GRP_CMDT_SEQ AS SEQ
     , SCG_GRP_CMDT_CD AS CD
     , SCG_GRP_CMDT_DESC AS NM
  FROM PRI_SCG_GRP_CMDT
 WHERE SVC_SCP_CD = @[svc_scp_cd]
   AND CHG_CD = @[chg_cd]
   AND DELT_FLG = 'N'
ORBER BY SCG_GRP_CMDT_CD ASC			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
