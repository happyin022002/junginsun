<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOmodifyKorCllRemarkUSQL">
			<desc><![CDATA[modifyKorCllRemark]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_TML_KR_CLL_RMK SET 
	DIFF_RMK = @[diff_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	CNTR_LIST_NO = @[cntr_list_no]
AND	KR_TML_RMK_TP_ID = @[kr_tml_rmk_tp_id]
AND	KR_TML_RMK_SEQ = '1'			]]></sql>
			<params>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_list_no" type="12" value="" out="N"/>
				<param name="kr_tml_rmk_tp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
