<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRealCustomerProposalDBDAORemoveAllRealCustomerDSQL">
			<desc><![CDATA[Proposal No. AMD NO.에 해당하는 Real Customer를 모두 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM PRI_SP_REAL_CUST
 WHERE PROP_NO = @[prop_no]
 AND AMDT_SEQ = @[amdt_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
