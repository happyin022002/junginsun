<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HoldNoticeDBDAOmodifyTpbInfoUSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
UPDATE BKG_HLD_N3RD_PTY_BIL_CNTR SET 
	N3PTY_BIL_NO = @[n3pty_bil_no]
,	INCUR_CHG_AMT = @[incur_chg_amt]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	BKG_NO = @[bkg_no]
AND	NTC_SEQ = @[ntc_seq]
AND	CNTR_NO = @[cntr_no]
			]]></sql>
			<params>
				<param name="n3pty_bil_no" type="12" value="" out="N"/>
				<param name="incur_chg_amt" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ntc_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
