<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FFCommCalculationDBDAOSearchFFDataChekRSQL">
			<desc><![CDATA[FFCommCalculationDBDAOSearchFFDataChekRSQL]]></desc>
			<sql><![CDATA[
SELECT '*' FF 
  FROM ACM_FF_CMPN
 WHERE BKG_NO = @[bkg_no]
	AND FF_CMPN_SEQ = @[ff_cmpn_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ff_cmpn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
