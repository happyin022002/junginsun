<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateQuotationDBDAORsltPriSqRtCmdtVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	A.QTTN_NO
,	A.QTTN_VER_NO
,	A.GEN_SPCL_RT_TP_CD
,	A.CMDT_HDR_SEQ
,	A.CMDT_SEQ
,	A.PRC_CMDT_TP_CD
,	A.PRC_CMDT_DEF_CD
,	A.SRC_INFO_CD
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,	A.UPD_DT
,	DECODE(A.PRC_CMDT_TP_CD,
          'G',                                            
          (SELECT PRC_GRP_CMDT_DESC                       --GROUP COMMODITY
             FROM PRI_SQ_GRP_CMDT
            WHERE QTTN_NO = A.QTTN_NO
              AND   QTTN_VER_NO = A.QTTN_VER_NO
              AND   PRC_GRP_CMDT_CD = A.PRC_CMDT_DEF_CD
              AND   ROWNUM = 1),
          'R',
          (SELECT REP_CMDT_NM                             --REP COMMODITY
             FROM MDM_REP_CMDT
            WHERE REP_CMDT_CD = A.PRC_CMDT_DEF_CD
              AND ROWNUM = 1),
          'C',
          (SELECT CMDT_NM                                  --COMMODITY
             FROM MDM_COMMODITY
            WHERE CMDT_CD = A.PRC_CMDT_DEF_CD
              AND ROWNUM = 1)) AS PRC_CMDT_DEF_NM
FROM PRI_SQ_RT_CMDT A
WHERE	A.QTTN_NO = @[qttn_no]
AND		A.QTTN_VER_NO = @[qttn_ver_no]
AND		A.GEN_SPCL_RT_TP_CD = @[gen_spcl_rt_tp_cd]
ORDER BY DECODE(A.PRC_CMDT_TP_CD, 'G','1','R','2','C','3'),  A.PRC_CMDT_DEF_CD
			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="gen_spcl_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
