<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteProposalDBDAOPriSpScpNoteCtntChgTpVOUSQL">
			<desc><![CDATA[charge type code를 업데이트한다.]]></desc>
			<sql><![CDATA[
#if (${note_conv_tp_cd} == 'P') 
	UPDATE PRI_SP_SCP_NOTE_CTNT SET
		   NOTE_CHG_TP_CD 	= @[note_chg_tp_cd]
		 , UPD_USR_ID		= @[upd_usr_id]
		 , UPD_DT			= sysdate
	 WHERE NOTE_CONV_MAPG_ID = @[note_conv_mapg_id]

#elseif (${note_conv_tp_cd} == 'R') 
	UPDATE PRI_SP_SCP_RT_CMDT_RNOTE SET
		   NOTE_CHG_TP_CD 	= @[note_chg_tp_cd]
		 , UPD_USR_ID		= @[upd_usr_id]
		 , UPD_DT			= sysdate
	 WHERE NOTE_CONV_MAPG_ID = @[note_conv_mapg_id]

#elseif (${note_conv_tp_cd} == 'C') 
	UPDATE PRI_SP_SCP_RT_CNOTE SET
		   NOTE_CHG_TP_CD 	= @[note_chg_tp_cd]
		 , UPD_USR_ID		= @[upd_usr_id]
		 , UPD_DT			= sysdate
	 WHERE NOTE_CONV_MAPG_ID = @[note_conv_mapg_id]

#end			]]></sql>
			<params>
				<param name="note_chg_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="note_conv_mapg_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
