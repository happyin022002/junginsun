<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AGTCommonDBDAOSearchAGTCommRptDefaultItemRSQL">
			<desc><![CDATA[searchAGTCommRptDefaultItem]]></desc>
			<sql><![CDATA[
SELECT 
	RPT_ITM_DESC, 
	LOWER(RPT_ITM_COL_NM) AS RPT_ITM_COL_NM
FROM AGT_RPT_ITM_INFO_DTL
WHERE CRE_USR_ID = @[userId]
    AND SLCT_ITM_FOM_SEQ = @[seq]
ORDER BY AC_RPT_ITM_CD			]]></sql>
			<params>
				<param name="userId" type="12" value="" out="Y"/>
				<param name="seq" type="2" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
