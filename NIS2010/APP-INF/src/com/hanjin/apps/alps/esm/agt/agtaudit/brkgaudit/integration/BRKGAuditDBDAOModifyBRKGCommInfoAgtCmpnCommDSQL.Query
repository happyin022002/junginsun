<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BRKGAuditDBDAOModifyBRKGCommInfoAgtCmpnCommDSQL">
			<desc><![CDATA[ModifyBRKGCommInfoAgtCmpnComm]]></desc>
			<sql><![CDATA[
DELETE
      FROM AGT_CMPN_COMM
     WHERE BKG_NO   = @[bkg_no]
       AND CMPN_SEQ = @[cmpn_seq]
	   AND COMM_PROC_STS_CD != 'IF'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="Y"/>
				<param name="cmpn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
