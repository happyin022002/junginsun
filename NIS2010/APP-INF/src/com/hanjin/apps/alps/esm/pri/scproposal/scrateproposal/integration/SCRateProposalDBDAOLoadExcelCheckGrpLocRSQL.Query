<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRateProposalDBDAOLoadExcelCheckGrpLocRSQL">
			<desc><![CDATA[Group Location Check]]></desc>
			<sql><![CDATA[
--SELECT PRC_GRP_LOC_CD AS CD
--      ,PRC_GRP_LOC_DESC AS NM
--  FROM PRI_SP_SCP_GRP_LOC
-- WHERE PROP_NO = 
--   AND AMDT_SEQ = 
--   AND SVC_SCP_CD = 
--   AND PRC_GRP_LOC_CD = 
SELECT A.PRC_GRP_LOC_CD AS CD
      ,A.PRC_GRP_LOC_DESC AS NM
  FROM PRI_SP_SCP_GRP_LOC A
 WHERE PROP_NO = @[prop_no]
   AND AMDT_SEQ = @[amdt_seq]
   AND SVC_SCP_CD = @[svc_scp_cd]
   AND PRC_GRP_LOC_CD = @[cd]
   AND EXISTS (SELECT 'OK'
          FROM PRI_SP_SCP_GRP_LOC_DTL S
         WHERE S.PROP_NO = A.PROP_NO
           AND S.AMDT_SEQ = A.AMDT_SEQ
           AND S.SVC_SCP_CD = A.SVC_SCP_CD
           AND S.GRP_LOC_SEQ = A.GRP_LOC_SEQ
           AND S.SRC_INFO_CD <> 'AD')			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
