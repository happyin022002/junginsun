<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchMndTsSyFlgRSQL">
			<desc><![CDATA[MndTsSyFlg 조회한다]]></desc>
			<sql><![CDATA[
SELECT 'Y' as OUTPUT_TEXT  
FROM BKG_VVD 
WHERE BKG_NO = @[bkg_no] 
AND POD_CD LIKE 'SY%' 
AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
