<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorCustomsTransmissionDBDAOaddDiscBodySendDtlLogCSQL">
			<desc><![CDATA[LOG 관리를 위한 작업(DETAIL) => 하선신고서의 Body Flat File에 대한 (56번 조회 데이터) 전송결과 저장]]></desc>
			<sql><![CDATA[
INSERT 
  INTO BKG_CSTMS_KR_SND_LOG_DTL
     ( MSG_LOG_TP_ID
     , SND_DT
     , OFC_CD
     , FLT_FILE_REF_NO
     , MF_SND_SEQ
     , LOG_DTL_SEQ
     , EDI_SND_MSG
     , CRE_USR_ID
     , UPD_USR_ID
     )
SELECT '5ID'
     , TO_DATE(@[snd_dt], 'YYYYMMDD HH24:MI:SS')
     , @[ofc_cd]
     , @[flt_file_ref_no]
     , 1
     , NVL(MAX(LOG_DTL_SEQ), 0) + 1 
     , @[edi_snd_msg]
     , @[user_id]
     , @[user_id]
  FROM BKG_CSTMS_KR_SND_LOG_DTL
 WHERE MSG_LOG_TP_ID  =   '5ID'
   AND SND_DT = TO_DATE(@[snd_dt], 'YYYYMMDD HH24:MI:SS')
   AND OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="edi_snd_msg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
