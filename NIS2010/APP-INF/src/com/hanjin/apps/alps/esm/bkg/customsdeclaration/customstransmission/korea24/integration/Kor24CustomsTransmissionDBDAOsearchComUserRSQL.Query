<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Kor24CustomsTransmissionDBDAOsearchComUserRSQL">
			<desc><![CDATA[User ID 조회 (IMFMOD의 'SENDER'에 쓰임 )]]></desc>
			<sql><![CDATA[
SELECT  USR_NM
FROM    COM_USER
WHERE   USR_ID = @[user_id]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
