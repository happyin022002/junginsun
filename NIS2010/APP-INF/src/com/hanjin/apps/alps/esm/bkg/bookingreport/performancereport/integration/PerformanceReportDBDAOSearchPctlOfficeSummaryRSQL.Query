<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchPctlOfficeSummaryRSQL">
			<desc><![CDATA[Port Closing Inquiry[ESM_BKG_0914]
PCT (Port Closing Time) 현황 조회 화면 
3.Office List summary]]></desc>
			<sql><![CDATA[
#if (${dt_type} == 'PCT')
--------------------------------------------------------------------------------
    SELECT /*+ ALL_ROWS */
           TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 0, 1)))) S_BL_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 1, 0)))) O_BL_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 0, DECODE(BC.CUST_CNT_CD, NULL, 0, 1))))) S_CNEE_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', DECODE(BC.CUST_CNT_CD, NULL, 0, 1), 0)))) O_CNEE_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 0, DECODE(NC.CUST_CNT_CD, NULL, 0, 1))))) S_NTFY_TTL
          ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', DECODE(NC.CUST_CNT_CD, NULL, 0, 1), 0)))) O_NTFY_TTL
      FROM BKG_BOOKING BK
          ,BKG_VVD VVD
          ,BKG_CUSTOMER BC
          ,BKG_CUSTOMER NC
          ,VSK_VSL_PORT_SKD VPS
          ,(SELECT ROWNUM
                  ,REGION
                  ,GSO
                  ,OFC_CD
              FROM BKG_OFC_LVL_V
            ) OLA
    #if (${rev_status} != '')
          ,(
            SELECT S.*, DECODE(NVL(REV_LANE, 'N'), 'N', 'N', 'Y') REV_STATUS
              FROM MDM_VSL_SVC_LANE S
                  ,(
                    SELECT DISTINCT SUBSTR(RLANE_CD, 1, 3) REV_LANE
                      FROM MDM_DTL_REV_LANE
                     WHERE DELT_FLG = 'N'
                    ) R
              WHERE S.VSL_SLAN_CD = R.REV_LANE(+)
            ) REV
    #end
     WHERE 1 = 1
       AND OLA.REGION IS NOT NULL
       AND BK.BKG_OFC_CD = OLA.OFC_CD
       AND BK.BKG_STS_CD <> 'X'
       AND BC.BKG_NO = BK.BKG_NO
       AND BC.BKG_CUST_TP_CD = 'C'
       AND NC.BKG_NO = BK.BKG_NO
       AND NC.BKG_CUST_TP_CD = 'N'
       AND VVD.BKG_NO = BK.BKG_NO
       AND VPS.VSL_CD = VVD.VSL_CD
       AND VPS.SKD_VOY_NO = VVD.SKD_VOY_NO
       AND VPS.SKD_DIR_CD = VVD.SKD_DIR_CD
       AND VPS.VPS_PORT_CD = VVD.POL_CD
       AND VPS.CLPT_IND_SEQ = 1
       AND VVD.VSL_PRE_PST_CD || VVD.VSL_SEQ = (SELECT /*+ INDEX(VVD XPKBKG_VVD) */
                                                       BV.VSL_PRE_PST_CD || BV.VSL_SEQ
                                                  FROM BKG_VVD BV
                                                 WHERE BV.BKG_NO = VVD.BKG_NO
                                                   AND ROWNUM =1)


    #if (${rhq_cd} != '')
       AND OLA.REGION = @[rhq_cd]
    #end
    #if (${gso} != '')
      AND OLA.GSO = @[gso]
    #end
    #if (${bkg_ofc_cd} != '')
      AND OLA.OFC_CD = @[bkg_ofc_cd]
    #end
    #if (${bkg_sts_cd} != '')
       AND BK.BKG_STS_CD = @[bkg_sts_cd]
    #end
    #if (${bkg_rt_sts_cd} != '' && ${bkg_rt_sts_cd} != 'A')
       AND BKG_RATE_RESULT_FNC(BK.BKG_NO) = @[bkg_rt_sts_cd]
    #end
    #if (${rev_status} != '')
       AND NVL(REV.REV_STATUS, 'N') = @[rev_status]
    #end
    #if (${bkg_staff} != '')
       AND BK.DOC_USR_ID LIKE @[bkg_staff] || '%%'
    #end
    #if (${fr_dt} != '')
       AND BK.PORT_CLZ_DT   >= TO_DATE(@[fr_dt], 'YYYY-MM-DD')
    #end
    #if (${to_dt} != '')
       AND BK.PORT_CLZ_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
    #end
    #if (${pol_cd} != '')
       AND BK.POL_CD = @[pol_cd]
    #end
    #if (${vvd_cd} != '')
       AND BK.VSL_CD =  SUBSTR(@[vvd_cd],1,4)
       AND BK.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
       AND BK.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
    #end
    #if (${rev_status} != '')
       AND VVD.SLAN_CD = REV.VSL_SLAN_CD(+)
    #end
    #if (${rev_status} != '')
       AND VVD.SLAN_CD = REV.VSL_SLAN_CD(+)
    #end
    #if (${slan_cd} != 'All')
       AND BK.SLAN_CD = @[slan_cd]
    #end
    #if (${cntr_cfm} == 'Y')
       AND EXISTS (
                   SELECT 'X' 
                     FROM BKG_DOC_PROC_SKD
                    WHERE BKG_NO = BK.BKG_NO
                      AND BKG_DOC_PROC_TP_CD IN ('CNTCFM', 'CNTRLS') 
                      AND DOC_PERF_DELT_FLG = 'N'
                  )
    #elseif (${cntr_cfm} == 'N')
       AND NOT EXISTS (
                   SELECT 'X' 
                     FROM BKG_DOC_PROC_SKD
                    WHERE BKG_NO = BK.BKG_NO
                      AND BKG_DOC_PROC_TP_CD IN ('CNTCFM', 'CNTRLS') 
                      AND DOC_PERF_DELT_FLG = 'N'
                  )
    #end


--------------------------------------------------------------------------------

#elseif (${dt_type} =='ETD') 
--2.Office List
--2-2.ETD
--------------------------------------------------------------------------------
SELECT /*+ leading(vps) use_nl(bc sc nc bmo vps) */  /* @@ 힌트적용  2014.11.24 튜닝 */
      TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 0, 1)))) S_BL_TTL
     ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 1, 0)))) O_BL_TTL
     ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 0, DECODE(BC.CUST_CNT_CD, NULL, 0, 1))))) S_CNEE_TTL
     ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', DECODE(BC.CUST_CNT_CD, NULL, 0, 1), 0)))) O_CNEE_TTL
     ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', 0, DECODE(NC.CUST_CNT_CD, NULL, 0, 1))))) S_NTFY_TTL
     ,TRIM(TO_CHAR(SUM(DECODE(BK.CUST_TO_ORD_FLG, 'Y', DECODE(NC.CUST_CNT_CD, NULL, 0, 1), 0)))) O_NTFY_TTL
FROM   BKG_BOOKING BK
     ,BKG_CUSTOMER BC
     ,BKG_CUSTOMER SC
     ,BKG_CUSTOMER NC
     ,BKG_BL_DOC BMO
     ,BKG_VVD         VVD
     ,VSK_VSL_PORT_SKD VPS
     ,(SELECT /*+ USE_NL(VPS VVD VAP) */  VPS.VSL_CD,VPS.SKD_VOY_NO,VPS.SKD_DIR_CD,VAP.ACT_ATD_INP_DT,VPS.SLAN_CD,VPS.VPS_PORT_CD
        FROM   VSK_VSL_PORT_SKD  VPS
        ,      BKG_VVD           VVD
        ,      VSK_ACT_PORT_SKD VAP
        WHERE  1=1
        AND    VPS.VPS_PORT_CD   =   VVD.POL_CD
        AND    VPS.VSL_CD       =   VVD.VSL_CD
        AND    VPS.SKD_VOY_NO   =   VVD.SKD_VOY_NO
        AND    VPS.SKD_DIR_CD   =   VVD.SKD_DIR_CD
        AND    VPS.VSL_CD 		 = 	 VAP.VSL_CD
        AND    VPS.SKD_VOY_NO   =   VAP.SKD_VOY_NO
        AND    VPS.SKD_DIR_CD   =   VAP.SKD_DIR_CD
        AND    VPS.VPS_PORT_CD  =   VAP.VPS_PORT_CD
        AND    VPS.CLPT_IND_SEQ =   VAP.CLPT_IND_SEQ
        AND    VPS.CLPT_IND_SEQ =    VVD.POL_CLPT_IND_SEQ
#if (${fr_dt} != '')                 
	AND    VPS.VPS_ETD_DT >= TO_DATE(@[fr_dt], 'YYYY-MM-DD') 
#end
#if (${to_dt} != '') 
        AND    VPS.VPS_ETD_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999 
#end      
        GROUP BY VPS.VSL_CD,VPS.SKD_VOY_NO,VPS.SKD_DIR_CD,VAP.ACT_ATD_INP_DT,VPS.SLAN_CD,VPS.VPS_PORT_CD
        ) VAS
 ,(SELECT ROWNUM
      ,REGION
      ,GSO
      ,OFC_CD
      FROM   BKG_OFC_LVL_V) OLA
#if (${rev_status} != '') 
	  ,(SELECT S.*, DECODE(NVL(REV_LANE, 'N'), 'N', 'N', 'Y') REV_STATUS 
                FROM   MDM_VSL_SVC_LANE S
                      ,(SELECT DISTINCT SUBSTR(RLANE_CD, 1, 3) REV_LANE
                        FROM   MDM_DTL_REV_LANE
                        WHERE  DELT_FLG = 'N') R
                WHERE  S.VSL_SLAN_CD = R.REV_LANE(+)) REV
#end
WHERE  BK.BKG_OFC_CD = OLA.OFC_CD

AND    OLA.REGION IS NOT NULL 
#if (${rhq_cd} != '') 
AND    OLA.REGION = @[rhq_cd]
#end
#if (${gso} != '') 
AND    OLA.GSO = @[gso]
#end
#if (${bkg_ofc_cd} != '') 
AND    OLA.OFC_CD = @[bkg_ofc_cd]
#end

AND    BK.BKG_STS_CD <> 'X'
#if (${bkg_sts_cd} != '') 
AND    BK.BKG_STS_CD = @[bkg_sts_cd]
#end

#if (${bkg_rt_sts_cd} != '' && ${bkg_rt_sts_cd} != 'A') 
AND    BKG_RATE_RESULT_FNC(BK.BKG_NO) = @[bkg_rt_sts_cd]
#end

#if (${rev_status} != '') 
AND    NVL(REV.REV_STATUS, 'N') = @[rev_status]
#end

#if (${bkg_staff} != '') 
AND    BK.DOC_USR_ID LIKE @[bkg_staff] || '%%' 
#end

#if (${pol_cd} != '') 
AND    BK.POL_CD = @[pol_cd]
#end
AND    BK.BKG_NO = BMO.BKG_NO 
#if (${rev_status} != '') 
AND    VPS.SLAN_CD = REV.VSL_SLAN_CD 
#end
AND    BC.BKG_NO(+) = BK.BKG_NO 
AND    BC.BKG_CUST_TP_CD = 'C' 
AND    SC.BKG_NO = BK.BKG_NO 
AND    SC.BKG_CUST_TP_CD(+) = 'S' 
AND    NC.BKG_NO = BK.BKG_NO 
AND    NC.BKG_CUST_TP_CD(+) = 'N' 

AND     BK.POL_CD           =  VVD.POL_CD
AND     BK.BKG_NO           =   VVD.BKG_NO
#if (${fr_dt} != '')                 
AND    VPS.VPS_ETD_DT >= TO_DATE(@[fr_dt], 'YYYY-MM-DD') 
#end
#if (${to_dt} != '') 
AND    VPS.VPS_ETD_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999 
#end

AND     VPS.VSL_CD          =   VVD.VSL_CD
AND     VPS.SKD_VOY_NO      =   VVD.SKD_VOY_NO
AND     VPS.SKD_DIR_CD      =   VVD.SKD_DIR_CD
AND     VPS.CLPT_IND_SEQ    =   VVD.POL_CLPT_IND_SEQ
AND	VPS.VPS_PORT_CD     =   VVD.POL_CD
AND     VPS.VSL_CD          =   VAS.VSL_CD(+)
AND     VPS.SKD_VOY_NO      =   VAS.SKD_VOY_NO(+)
AND     VPS.SKD_DIR_CD      =   VAS.SKD_DIR_CD(+)
AND     VPS.VPS_PORT_CD     =   VAS.VPS_PORT_CD(+)
AND     BK.BKG_CGO_TP_CD <> 'P'

#if (${vvd_cd} != '')
AND   BK.VSL_CD =  SUBSTR(@[vvd_cd],1,4)
AND   BK.SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
AND   BK.SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
#end

#if (${slan_cd} != 'All')
AND BK.SLAN_CD = @[slan_cd]
#end

ORDER BY OLA.REGION, OLA.GSO, OLA.OFC_CD
--------------------------------------------------------------------------------
#end			]]></sql>
			<params>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="gso" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_sts_cd" type="12" value="" out="N"/>
				<param name="bkg_rt_sts_cd" type="12" value="" out="N"/>
				<param name="rev_status" type="12" value="" out="N"/>
				<param name="bkg_staff" type="12" value="" out="N"/>
				<param name="fr_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
