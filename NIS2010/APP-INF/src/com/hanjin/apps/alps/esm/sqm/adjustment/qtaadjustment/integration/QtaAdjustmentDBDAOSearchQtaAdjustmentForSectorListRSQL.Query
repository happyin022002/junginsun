<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QtaAdjustmentDBDAOSearchQtaAdjustmentForSectorListRSQL">
			<desc><![CDATA[QTA Adjustment by VVD For Sector 정보를 조회합니다.

2016.02.25 최성민 [CHM-201640193] QTA Adjustment bu VVD와 QTA Adjustment by VVD for IAS Sector 에서 Target VVD 바라보는 것을 잠시 끊을 수 있도록 하는 로직 수정]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(@[f_bse_yr],-2) ||@[f_bse_qtr_cd]||'02' AS QTA_RLSE_VER_NO 
      ,A2.TRD_CD
      ,A2.RLANE_CD
      ,A2.SUB_TRD_CD
      ,A2.DIR_CD
      ,DECODE(A1.HUL_BND_CD,'BH','B/H','H/H') HUL_BND_CD
      ,A2.IAS_RGN_CD
      ,A2.IOC_CD
      ,BSE_MON
      ,DECODE(BSE_MON,'','',@[f_bse_yr]||BSE_MON) BSE_YRMON
      ,@[f_bse_yr]||@[f_bse_qtr_cd] AS REVENUE_QUARTER
      ,BSE_WK
      ,SLS_YR
      ,SLS_YRMON
      ,VVD
      ,PF_GRP_CD  
      ,PF_SVC_TP_CD
      ,FNL_BSA_CAPA
      ,LOD_QTY
      ,GRS_REV
      ,MAS_BSE_MON
 	  ,DECODE(MAS_BSE_MON,'','',@[f_bse_yr]||MAS_BSE_MON) MAS_BSE_YRMON
      ,MAS_BSE_WK
      ,MAS_COST_YR
      ,MAS_COST_YRMON
      ,MAS_VVD
      ,MAS_PF_GRP_CD  
      ,MAS_PF_SVC_TP_CD
      ,MAS_FNL_BSA_CAPA
      ,FLAG
--Sales YRMON 만 바뀌었을 경우 Flag 'Y' 표시. 후에 SQM_CFM_TGT_VVD_ADJ에 안넣으려고 
       ,CASE
              WHEN FLAG = 'U' AND FNL_BSA_CAPA <> MAS_FNL_BSA_CAPA THEN ''
              WHEN FLAG = 'U' AND BSE_MON      <> MAS_BSE_MON      THEN ''
              WHEN FLAG = 'U' AND BSE_WK       <> MAS_BSE_WK       THEN ''
              WHEN FLAG = 'U' AND SLS_YRMON    <> MAS_COST_YRMON    THEN 'Y'
                                                                   ELSE ''
         END AS STS_FLAG
      ,BSA_ZR_FLG AS F_CLICK
      ,@[f_bse_tp_cd] AS BSE_TP_CD
      ,@[f_bse_yr] AS BSE_YR
      ,@[f_bse_qtr_cd] AS BSE_QTR_CD
      ,DECODE((SELECT MAS_MON_VVD.BSA_ZR_FLG 
            FROM MAS_MON_VVD 
            WHERE MAS_MON_VVD.TRD_CD = A2.TRD_CD
                AND MAS_MON_VVD.RLANE_CD = A2.RLANE_CD
                AND MAS_MON_VVD.SUB_TRD_CD = A2.SUB_TRD_CD
                AND MAS_MON_VVD.DIR_CD = A2.DIR_CD
                AND MAS_MON_VVD.IOC_CD = A2.IOC_CD
                AND MAS_MON_VVD.COST_YRMON = NVL(MAS_COST_YRMON,A2.SLS_YRMON) 
                AND MAS_MON_VVD.VSL_CD||MAS_MON_VVD.SKD_VOY_NO||MAS_MON_VVD.DIR_CD = NVL(MAS_VVD,VVD)), 'Y', 'Y', 'N', '', '') AS BSA_ZR_FLG
  FROM MAS_LANE_RGST A1, 
        (
         SELECT O_TRD_CD           AS TRD_CD
               ,O_RLANE_CD         AS RLANE_CD
               ,O_SUB_TRD_CD       AS SUB_TRD_CD
               ,O_DIR_CD           AS DIR_CD
               ,O_IOC_CD           AS IOC_CD
               ,IAS_RGN_CD     
               ,O_VVD
               ,LOD_QTY
               ,GRS_REV
               ,BSA_ZR_FLG
               ,MAX(BSE_YR)        AS BSE_YR
               ,MAX(BSE_MON)       AS BSE_MON
               ,MAX(BSE_WK)        AS BSE_WK
               ,MAX(SLS_YR)        AS SLS_YR
               ,MAX(SLS_YRMON)     AS SLS_YRMON  
               ,MAX(VVD)           AS VVD
               ,MAX(PF_GRP_CD)     AS PF_GRP_CD  
               ,MAX(PF_SVC_TP_CD)  AS PF_SVC_TP_CD
               ,MAX(FNL_BSA_CAPA)  AS FNL_BSA_CAPA
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE MAS_COST_YR            END) AS MAS_COST_YR
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE MAS_COST_YRMON         END) AS MAS_COST_YRMON
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE MAS_MON                END) AS MAS_BSE_MON
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE COST_WK                END) AS MAS_BSE_WK
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE MAS_VVD                END) AS MAS_VVD
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE MAS_PF_GRP_CD          END) AS MAS_PF_GRP_CD
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE MAS_PF_SVC_TP_CD       END) AS MAS_PF_SVC_TP_CD
               ,MAX(CASE WHEN NVL(SQM_INFO, '*') = NVL(MAS_INFO, '*') OR @[f_dis_mas] = 'Y' THEN '' ELSE FNL_HJS_BSA_CAPA || '' END) AS MAS_FNL_BSA_CAPA
               ,CASE WHEN @[f_dis_mas] = 'Y' THEN 'R'
                     WHEN MAX(SQM_INFO) = MAX(MAS_INFO) THEN 'R'
                     WHEN NVL(MAX(SQM_INFO), '*') = '*' THEN 'I'
                     WHEN NVL(MAX(MAS_INFO), '*') = '*' THEN 'D'
                                                        ELSE 'U'
                END AS FLAG
               ,MAX(SQM_INFO) AS SQM_INFO
               ,MAX(MAS_INFO) AS MAS_INFO
           FROM (
                 SELECT B1.TRD_CD
                       ,B1.DIR_CD
                       ,B1.SUB_TRD_CD
                       ,B1.RLANE_CD
                       ,B1.BSE_YR
                       ,B1.BSE_MON
                       ,B1.BSE_WK
                       ,B1.SLS_YRMON
                       ,SUBSTR(B1.SLS_YRMON,0,4) AS SLS_YR
                       ,NVL(B1.IAS_RGN_CD, B2.IAS_RGN_CD) AS IAS_RGN_CD
                       ,B1.VSL_CD || B1.SKD_VOY_NO || B1.DIR_CD AS VVD
                       ,B1.PF_GRP_CD
                       ,B1.PF_SVC_TP_CD
                       ,B1.FNL_BSA_CAPA
                       ,SUBSTR(B2.YRMON, -2) AS MAS_MON
                       ,B2.COST_WK
                       ,B2.COST_YRMON AS MAS_COST_YRMON
                       ,SUBSTR(B2.COST_YRMON,0,4) AS MAS_COST_YR
                       ,B2.VSL_CD || B2.SKD_VOY_NO || B2.DIR_CD AS MAS_VVD
                       ,B2.MAS_PF_GRP_CD
                       ,B2.MAS_PF_SVC_TP_CD
                       ,B2.FNL_HJS_BSA_CAPA
                       ,NVL(B2.BSA_ZR_FLG,'N') AS BSA_ZR_FLG
                       ,NVL(B1.LOD_QTY,0) AS LOD_QTY
                       ,NVL(B1.GRS_REV,0) AS GRS_REV
                       ,B1.TRD_CD || B1.SUB_TRD_CD || B1.RLANE_CD || B1.DIR_CD || B1.VSL_CD || B1.SKD_VOY_NO || B1.DIR_CD || B1.BSE_MON || B1.BSE_WK || B1.FNL_BSA_CAPA || B1.SLS_YRMON||B1.PF_GRP_CD AS SQM_INFO
                       ,B2.TRD_CD || B2.SUB_TRD_CD || B2.RLANE_CD || B2.DIR_CD || B2.VSL_CD || B2.SKD_VOY_NO || B2.DIR_CD || SUBSTR(B2.YRMON,-2) || B2.COST_WK || B2.FNL_HJS_BSA_CAPA || B2.COST_YRMON||DECODE(B2.VSL_CD, NULL, NULL, NVL(B2.MAS_PF_GRP_CD, B1.PF_GRP_CD)) AS MAS_INFO
                       ,NVL(B1.TRD_CD         , B2.TRD_CD)                AS O_TRD_CD
                       ,NVL(B1.RLANE_CD       , B2.RLANE_CD)              AS O_RLANE_CD
                       ,NVL(B1.SUB_TRD_CD     , B2.SUB_TRD_CD)            AS O_SUB_TRD_CD
                       ,NVL(B1.IOC_CD         , B2.IOC_CD)                AS O_IOC_CD
                       ,NVL(B1.DIR_CD         , B2.DIR_CD)                AS O_DIR_CD
                       ,NVL(B1.BSE_MON        , SUBSTR(B2.YRMON, -2))     AS O_BSE_MON
                       ,NVL(B1.BSE_WK         , B2.COST_WK)               AS O_BSE_WK
                       ,NVL(B1.VSL_CD || B1.SKD_VOY_NO || B1.DIR_CD, B2.VSL_CD || B2.SKD_VOY_NO || B2.DIR_CD) AS O_VVD
                   FROM (
                          SELECT A1.TRD_CD
                                ,A1.DIR_CD
                                ,A1.SUB_TRD_CD
                                ,A1.RLANE_CD
                                ,A1.BSE_YR
                                ,A1.BSE_MON
                                ,A1.BSE_WK
                                ,A1.SLS_YRMON
                                ,A1.VSL_CD
                                ,A1.SKD_VOY_NO
                                ,A1.FNL_BSA_CAPA
                                ,A1.IOC_CD
                              --  ,MAX(NVL((SELECT PF_GRP_CD  
                             --       FROM SQM_SCTR_PF_GRP 
                             ---      WHERE BSE_TP_CD = QT.BSE_TP_CD
                             --        AND BSE_YR = QT.BSE_YR
                             --        AND BSE_QTR_CD = QT.BSE_QTR_CD
                              --       AND RLANE_CD = QT.RLANE_CD
                              ---       AND PF_SVC_TP_CD = QT.PF_SVC_TP_CD ),A4.PF_GRP_CD)) AS PF_GRP_CD
                                ,MAX(NVL(A4.PF_GRP_CD,(SELECT PF_GRP_CD  
                                    FROM SQM_SCTR_PF_GRP 
                                   WHERE BSE_TP_CD = QT.BSE_TP_CD
                                     AND BSE_YR = QT.BSE_YR
                                     AND BSE_QTR_CD = QT.BSE_QTR_CD
                                     AND RLANE_CD = QT.RLANE_CD
                                     AND PF_SVC_TP_CD = QT.PF_SVC_TP_CD ))) AS PF_GRP_CD
                                ,QT.PF_SVC_TP_CD
                                ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03218' AND INTG_CD_VAL_CTNT = A3.IAS_RGN_CD) AS  IAS_RGN_CD
                                ,SUM(A4.LOD_QTY) AS LOD_QTY
                                ,SUM(A4.LOD_QTY * A4.GRS_RPB_REV) AS GRS_REV
                            FROM SQM_CFM_TGT_VVD A1
                                ,MAS_LANE_RGST A3
                                ,SQM_SCTR_CFM_QTA A4
                                ,SQM_QTA_TGT_VVD QT
                           WHERE 1=1
                             AND A1.TRD_CD          = A3.TRD_CD
                             AND A1.RLANE_CD        = A3.RLANE_CD
                             AND A1.DIR_CD          = A3.DIR_CD
							 AND A1.IOC_CD 			= A3.IOC_CD

                             AND A1.QTA_RLSE_VER_NO = A4.QTA_RLSE_VER_NO
                             AND A1.BSE_TP_CD       = A4.BSE_TP_CD
                             AND A1.BSE_YR          = A4.BSE_YR
                             AND A1.BSE_QTR_CD      = A4.BSE_QTR_CD
                             AND A1.TRD_CD          = A4.TRD_CD
                             AND A1.RLANE_CD        = A4.RLANE_CD
                             AND A1.DIR_CD          = A4.DIR_CD
                             AND A1.VSL_CD          = A4.VSL_CD
                             AND A1.SKD_VOY_NO      = A4.SKD_VOY_NO
                             AND A1.SKD_DIR_CD      = A4.SKD_DIR_CD

                             AND A1.BSE_TP_CD       = QT.BSE_TP_CD(+)
                             AND A1.BSE_YR          = QT.BSE_YR(+)
                             AND A1.BSE_QTR_CD      = QT.BSE_QTR_CD(+)
                             AND A1.TRD_CD          = QT.TRD_CD(+) 
                             AND A1.RLANE_CD        = QT.RLANE_CD(+) 
                             AND A1.DIR_CD          = QT.DIR_CD(+) 
                             AND A1.VSL_CD          = QT.VSL_CD(+) 
                             AND A1.SKD_VOY_NO      = QT.SKD_VOY_NO(+) 
                             AND A1.SKD_DIR_CD      = QT.SKD_DIR_CD(+) 

                             AND A4.OFC_VW_CD(+)    = 'L'
                             AND A1.BSE_TP_CD       = @[f_bse_tp_cd]
                             AND A1.BSE_YR          = @[f_bse_yr]
                             AND A1.BSE_QTR_CD      = @[f_bse_qtr_cd]
                             AND A1.QTA_RLSE_VER_NO = SUBSTR(@[f_bse_yr],-2) ||@[f_bse_qtr_cd]||'02'
                             AND A1.BSE_YR || A1.BSE_QTR_CD >= '20143Q'
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                             AND A1.RLANE_CD        IN (${f_rlane_cd})
#end
#if (${f_sub_trd_cd} != '' && ${f_sub_trd_cd} != 'All')
                             AND A1.SUB_TRD_CD      = @[f_sub_trd_cd]
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                             AND A1.DIR_CD          = @[f_dir_cd]
#end
#if (${f_ias_rgn_cd} != '' && ${f_ias_rgn_cd} != 'All')
                             AND A3.IAS_RGN_CD      = @[f_ias_rgn_cd]
#end
    	                     AND EXISTS (
	                                        SELECT DISTINCT 1
	                                          FROM SQM_SCTR_PAIR_MGMT S1
	                                         WHERE 1=1
	                                           AND S1.BSE_TP_CD  = A1.BSE_TP_CD
	                                           AND S1.BSE_YR     = A1.BSE_YR
	                                           AND S1.BSE_QTR_CD = A1.BSE_QTR_CD
	                                           AND S1.TRD_CD     = A1.TRD_CD
	                                           AND S1.RLANE_CD   = A1.RLANE_CD
	                                           AND S1.DIR_CD     = A1.DIR_CD
	                                           AND S1.SQM_ACT_FLG = 'Y'
                                         )
                           GROUP BY
                                 A1.TRD_CD
                                ,A1.DIR_CD
                                ,A1.SUB_TRD_CD
                                ,A1.RLANE_CD
                                ,A1.BSE_YR
                                ,A1.BSE_MON
                                ,A1.BSE_WK
                                ,A1.SLS_YRMON
                                ,A1.VSL_CD
                                ,A1.SKD_VOY_NO
                                ,A1.FNL_BSA_CAPA
                                ,A1.IOC_CD
                                ,A3.IAS_RGN_CD
                                ,QT.PF_SVC_TP_CD
                        ) B1
#if (${f_dis_mas} == 'Y')
                        LEFT OUTER JOIN
#else
                        FULL OUTER JOIN
#end
                        (
                          SELECT A1.TRD_CD
                                ,A1.SUB_TRD_CD
                                ,A1.RLANE_CD
                                ,A1.DIR_CD
                                ,A1.VSL_CD
                                ,A1.SKD_VOY_NO
                                ,A1.COST_YRMON
                                ,A1.COST_YRMON AS YRMON
                                ,A1.COST_WK
                                ,A1.IOC_CD
                                ,A1.BSA_ZR_FLG
                                ,NVL(A2.FNL_HJS_BSA_CAPA, 0) AS FNL_HJS_BSA_CAPA
                                ,(SELECT PF_GRP_CD 
                                    FROM SQM_SCTR_PF_GRP 
                                   WHERE BSE_TP_CD   = A3.BSE_TP_CD
                                     AND BSE_YR      = A3.BSE_YR
                                     AND BSE_QTR_CD  = A3.BSE_QTR_CD
                                     AND RLANE_CD    = A3.RLANE_CD
                                     AND PF_SVC_TP_CD = VS.PF_SKD_TP_CD) AS MAS_PF_GRP_CD
                                ,VS.PF_SKD_TP_CD AS MAS_PF_SVC_TP_CD
                                ,(SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD03218' AND INTG_CD_VAL_CTNT = A3.IAS_RGN_CD) AS  IAS_RGN_CD
                            FROM MAS_MON_VVD       A1
                                ,BSA_VVD_MST       A2
                                ,VSK_VSL_SKD       VS
                                ,(
                                   SELECT DISTINCT
                                          S2.BSE_TP_CD
                                         ,S2.BSE_YR
                                         ,S2.BSE_QTR_CD
                                         ,S2.TRD_CD
                                         ,S2.RLANE_CD
                                         ,S2.DIR_CD
                                         ,S2.SUB_TRD_CD
                                         ,S4.IAS_RGN_CD
                                     FROM SQM_SCTR_PAIR_MGMT S2
                                         ,MAS_LANE_RGST S4
                                    WHERE 1=1
                                      AND S2.TRD_CD         = S4.TRD_CD
                                      AND S2.RLANE_CD       = S4.RLANE_CD
                                      AND S2.DIR_CD         = S4.DIR_CD
                                      AND S2.BSE_TP_CD      = @[f_bse_tp_cd]
                                      AND S2.BSE_YR         = @[f_bse_yr]
                                      AND S2.BSE_QTR_CD     = @[f_bse_qtr_cd]
                                      AND S2.BSE_YR || S2.BSE_QTR_CD >= '20143Q'
                                      AND S2.SQM_ACT_FLG    = 'Y'
#if (${f_rlane_cd} != '' && ${f_rlane_cd} != 'All')
                                      AND S2.RLANE_CD       IN (${f_rlane_cd})
#end
#if (${f_sub_trd_cd} != '' && ${f_sub_trd_cd} != 'All')
                                      AND S2.SUB_TRD_CD     = @[f_sub_trd_cd]
#end
#if (${f_trd_dir} != 'on' && ${f_dir_cd} != '' && ${f_dir_cd} != 'All')
                                      AND S2.DIR_CD         = @[f_dir_cd]
#end
#if (${f_ias_rgn_cd} != '' && ${f_ias_rgn_cd} != 'All')
                                      AND S4.IAS_RGN_CD     = @[f_ias_rgn_cd]
#end

                                  ) A3
                           WHERE A1.TRD_CD         = A2.TRD_CD     (+)
                             AND A1.RLANE_CD       = A2.RLANE_CD   (+)
                             AND A1.VSL_CD         = A2.VSL_CD     (+)
                             AND A1.SKD_VOY_NO     = A2.SKD_VOY_NO (+)
                             AND A1.DIR_CD         = A2.SKD_DIR_CD (+)
                             AND A1.IOC_CD         = A2.IOC_CD     (+)
                             
                             AND A1.VSL_CD         = VS.VSL_CD(+)
                             AND A1.SKD_VOY_NO     = VS.SKD_VOY_NO(+)
                             AND A1.DIR_CD         = VS.SKD_DIR_CD(+)

                             AND A1.TRD_CD         = A3.TRD_CD
                             AND A1.RLANE_CD       = A3.RLANE_CD
                             AND A1.DIR_CD         = A3.DIR_CD
                             AND A1.SUB_TRD_CD     = A3.SUB_TRD_CD
                             AND A1.DELT_FLG       = 'N'
                             AND A1.IOC_CD         = DECODE(A1.RLANE_CD, 'RBCCO', 'I', A1.IOC_CD) -- RBCCO 노선은 IOC_CD = 'I' 인것만
							-- 2014년 : Week / 2015년~ : COST_YRMON (COA 이행 데이터 기준이 다름)
                             AND CASE WHEN '2014' = A3.BSE_YR THEN SUBSTR(A1.COST_YRMON, 1, 4)||A1.COST_WK ELSE A1.COST_YRMON END 
                                 BETWEEN CASE WHEN '2014' = A3.BSE_YR THEN @[f_bse_yr]||@[f_fm_wk] ELSE @[f_bse_yr]||DECODE(@[f_bse_qtr_cd],'1Q','01','2Q','04','3Q','07','4Q','10') END 
                                 	 AND CASE WHEN '2014' = A3.BSE_YR THEN @[f_bse_yr]||@[f_to_wk] ELSE @[f_bse_yr]||DECODE(@[f_bse_qtr_cd],'1Q','03','2Q','06','3Q','09','4Q','12') END 
                             AND NOT EXISTS ( SELECT 1
                                                FROM SQM_QTA_TGT_VVD S1
                                               WHERE S1.BSE_TP_CD  = @[f_bse_tp_cd]
                                                 AND S1.BSE_YR     = @[f_bse_yr]
                                                 AND S1.BSE_QTR_CD = @[f_bse_qtr_cd]
                                                 AND S1.DELT_FLG   = 'Y'
                                                 AND A1.TRD_CD     = S1.TRD_CD
                                                 AND A1.RLANE_CD   = S1.RLANE_CD
                                                 AND A1.VSL_CD     = S1.VSL_CD
                                                 AND A1.SKD_VOY_NO = S1.SKD_VOY_NO
                                                 AND A1.DIR_CD     = S1.DIR_CD
                                            )
                        ) B2
                     ON B1.TRD_CD          = B2.TRD_CD
                    AND B1.SUB_TRD_CD      = B2.SUB_TRD_CD
                    AND B1.DIR_CD          = B2.DIR_CD
                    AND B1.RLANE_CD        = B2.RLANE_CD
                    AND B1.IOC_CD          = B2.IOC_CD
                    AND B1.VSL_CD          = B2.VSL_CD
                    AND B1.SKD_VOY_NO      = B2.SKD_VOY_NO
                )
       GROUP BY O_TRD_CD
               ,O_RLANE_CD
               ,O_SUB_TRD_CD
               ,O_DIR_CD
               ,O_IOC_CD
               ,IAS_RGN_CD
               ,O_VVD
               ,LOD_QTY
               ,GRS_REV
               ,BSA_ZR_FLG
       ) A2
WHERE A1.TRD_CD         = A2.TRD_CD
AND A1.SUB_TRD_CD       = A2.SUB_TRD_CD
AND A1.RLANE_CD         = A2.RLANE_CD
AND A1.DIR_CD           = A2.DIR_CD
AND A1.IOC_CD           = A2.IOC_CD
#if (${f_trd_dir} == 'on' && ${f_hul_bnd_cd} != '' && ${f_hul_bnd_cd} != 'All')
AND A1.HUL_BND_CD       = @[f_hul_bnd_cd]
#end
ORDER BY A2.TRD_CD
        ,A2.RLANE_CD
        ,A2.DIR_CD
        ,NVL(SLS_YR,NVL(MAS_COST_YR, BSE_YR))
        ,NVL(MAS_BSE_MON, BSE_MON)
        ,NVL(MAS_BSE_WK, BSE_WK)

/*
History
2014.06.19 박은주 확정데이터에 존재하지 않을경우 VVD가 누락되는경우 발생
2014.07.10 이혜민 target vvd엔 데이터 존재하나 cfm_qta에 없을경우 데이터 누락되어 cfm_qta에 아우터조인걸어줌
2015.02.06 이혜민 [CHM-201533807] COA 기준데이터를 2013년 : SLS_YRMON / 2014년 : Week / 2015년~ : COST_YRMON 로 수정 / 조회시 기준은 SLS_YR기준으로
