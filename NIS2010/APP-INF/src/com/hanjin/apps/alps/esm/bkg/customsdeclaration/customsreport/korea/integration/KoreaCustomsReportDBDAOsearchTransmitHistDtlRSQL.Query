<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KoreaCustomsReportDBDAOsearchTransmitHistDtlRSQL">
			<desc><![CDATA[Korea에서 입/출항 선박 신고 적하목록 전송 문서의 상세내역 조회]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(EDI_SND_MSG, 1, length(EDI_SND_MSG) -1) EDI_SND_MSG
  FROM BKG_CSTMS_KR_SND_LOG_DTL
 WHERE MSG_LOG_TP_ID  =   @[msg_log_tp_id]
   AND SND_DT = TO_DATE(@[snd_dt], 'YYYYMMDD hh24:mi:ss')
   AND OFC_CD = @[ofc_cd]
   AND MF_SND_SEQ = TO_NUMBER(@[mf_snd_seq])
ORDER BY LOG_DTL_SEQ			]]></sql>
			<params>
				<param name="msg_log_tp_id" type="12" value="" out="N"/>
				<param name="snd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="mf_snd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
