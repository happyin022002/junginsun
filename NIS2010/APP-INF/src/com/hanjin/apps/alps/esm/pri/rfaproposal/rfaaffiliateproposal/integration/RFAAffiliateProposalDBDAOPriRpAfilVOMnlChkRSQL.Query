<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAAffiliateProposalDBDAOPriRpAfilVOMnlChkRSQL">
			<desc><![CDATA[조회 결과가 0 이면 MANNUAL 입력 불가,1이상일 경우 MANUAL INPUT 가능]]></desc>
			<sql><![CDATA[
SELECT 
	DECODE(COUNT(*),0,'N','Y') MNL_INP_FLG
FROM PRI_RP_SCP_MN
WHERE	PROP_NO = @[prop_no]
AND	AMDT_SEQ = @[amdt_seq]
AND	SVC_SCP_CD NOT IN ('TAW','TAE','ASE','ASW')			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
