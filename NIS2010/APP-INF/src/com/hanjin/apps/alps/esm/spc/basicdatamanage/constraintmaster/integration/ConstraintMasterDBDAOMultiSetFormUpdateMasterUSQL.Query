<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintMasterDBDAOMultiSetFormUpdateMasterUSQL">
			<desc><![CDATA[SPC_RPT_FOM 컬럼 rpt_fom_desc 수정]]></desc>
			<sql><![CDATA[
UPDATE 
SPC_RPT_FOM
SET   upd_usr_id=@[upd_usr_id]
	,rpt_fom_nm =@[rpt_fom_nm]
	,rpt_fom_desc =@[rpt_fom_desc]
WHERE pgm_no=@[pgm_no]
AND cre_usr_id= @[cre_usr_id]
AND rpt_fom_no= @[rpt_fom_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rpt_fom_nm" type="12" value="" out="N"/>
				<param name="rpt_fom_desc" type="12" value="" out="N"/>
				<param name="pgm_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="rpt_fom_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
