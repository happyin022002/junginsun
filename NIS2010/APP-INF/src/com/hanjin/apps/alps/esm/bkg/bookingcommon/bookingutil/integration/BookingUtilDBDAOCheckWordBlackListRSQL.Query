<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOCheckWordBlackListRSQL">
			<desc><![CDATA[WORD_BLACK_LIST 에 있는 단어를 포함하는 지 CHECK]]></desc>
			<sql><![CDATA[
SELECT BLCK_KW_NM
   FROM BKG_BLCK_KW_LIST
  WHERE BLCK_KW_TP_CD = 'YEL'
    AND BLCK_KW_CTNT = @[word_knd]
    AND ' '||REPLACE(REPLACE(REPLACE(UPPER(@[word]), CHR(13)||CHR(10),' '),CHR(13),' '),CHR(10),' ')||' ' 
        LIKE '% '||UPPER(BLCK_KW_NM)||' %'    
    AND ROWNUM = 1			]]></sql>
			<params>
				<param name="word_knd" type="12" value="" out="N"/>
				<param name="word" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
