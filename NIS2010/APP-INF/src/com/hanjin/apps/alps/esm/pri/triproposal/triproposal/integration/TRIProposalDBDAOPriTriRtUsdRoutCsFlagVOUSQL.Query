<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriRtUsdRoutCsFlagVOUSQL">
			<desc><![CDATA[PRI_TRI_RT_USD_ROUT_CS의 USD_ROUT_CS_SEL_FLG값을 갱신]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRI_RT_USD_ROUT_CS
SET USD_ROUT_CS_SEL_FLG = @[usd_rout_cs_sel_flg]
 WHERE  TRI_PROP_NO = @[tri_prop_no]   
   AND  AMDT_SEQ       = @[amdt_seq]
   AND  ROUT_CS_NO     = @[rout_cs_no] 
   AND  ROUT_CS_SRC_DT = @[rout_cs_src_dt]			]]></sql>
			<params>
				<param name="usd_rout_cs_sel_flg" type="12" value="" out="N"/>
				<param name="tri_prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="2" value="" out="N"/>
				<param name="rout_cs_no" type="12" value="" out="N"/>
				<param name="rout_cs_src_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
