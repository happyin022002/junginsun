<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOsearchComUserInfoRSQL">
			<desc><![CDATA[user id로 사용자정보 검색]]></desc>
			<sql><![CDATA[
SELECT USR_ID
      ,USR_NM
      ,OFC_CD
      ,USR_EML 
      ,DFLT_EML
  FROM COM_USER
 WHERE USR_ID = @[usr_id] 
   AND USE_FLG = 'Y'			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
