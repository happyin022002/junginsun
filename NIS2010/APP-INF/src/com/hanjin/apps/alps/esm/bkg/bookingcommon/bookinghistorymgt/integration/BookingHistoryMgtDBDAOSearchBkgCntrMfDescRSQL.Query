<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchBkgCntrMfDescRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchBkgCntrMfDescRSQL]]></desc>
			<sql><![CDATA[
SELECT A1.BKG_NO
, A1.CNTR_MF_SEQ
, A1.CNTR_NO
, A1.PCK_QTY
, A1.PCK_TP_CD
, A1.CNTR_MF_WGT
, A1.WGT_UT_CD
, A1.MEAS_QTY
, A1.MEAS_UT_CD
, A1.DCGO_FLG
, A1.BB_CGO_FLG
, A1.AWK_CGO_FLG
, A1.RC_FLG
, A1.RD_CGO_FLG
, A1.HNGR_FLG
, A1.CNTR_MF_MK_DESC
, A1.CNTR_MF_GDS_DESC
, A1.HBL_SEQ
, A1.HAMO_TRF_CD
, NVL( BKG_JOIN_FNC (CURSOR( SELECT CMDT.NCM_NO
                               FROM BKG_CNTR_MF_DESC_DTL CMDT
                              WHERE CMDT.BKG_NO =@[bkg_no]
                                AND   CMDT.CNTR_NO = A1.CNTR_NO
								AND   CMDT.CNTR_MF_SEQ = A1.CNTR_MF_SEQ )
                     ),A1.NCM_NO) AS NCM_NO                           
, A1.PO_NO
, A1.CNTR_MF_NO
, A1.CSTMS_DECL_NO
, A1.CRE_USR_ID
, TO_CHAR(A1.CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
, A1.UPD_USR_ID
, TO_CHAR(A1.UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
, A1.CMDT_HS_CD
, A1.WPM_TRT_CD
  FROM BKG_CNTR_MF_DESC A1
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
