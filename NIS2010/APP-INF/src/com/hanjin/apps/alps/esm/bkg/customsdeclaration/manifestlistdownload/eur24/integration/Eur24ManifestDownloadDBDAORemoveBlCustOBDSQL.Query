<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAORemoveBlCustOBDSQL">
			<desc><![CDATA[BKG_CSTMS_EUR_CUST 정보 삭제]]></desc>
			<sql><![CDATA[
BEGIN
#if (${s_ibflag} == 'D')
DELETE BKG_CSTMS_EUR_IO_CUST
WHERE VSL_CD||SKD_VOY_NO||SKD_DIR_CD = @[vvd]
  AND BL_NO = @[bl_no]
  AND CSTMS_PORT_CD = @[cstms_port_cd]
  AND CUST_CNT_CD = @[cust_cnt_cd]
  AND CUST_SEQ = @[cust_seq]
  AND BND_TP_CD = 'O'
  AND BKG_CUST_TP_CD = 'S';
#end

#if (${f_ibflag} == 'D')
DELETE BKG_CSTMS_EUR_IO_CUST
WHERE VSL_CD||SKD_VOY_NO||SKD_DIR_CD = @[vvd]
  AND BL_NO = @[bl_no]
  AND CSTMS_PORT_CD = @[cstms_port_cd]
  AND CUST_CNT_CD = @[cust_cnt_cd]
  AND CUST_SEQ = @[cust_seq]
  AND BND_TP_CD = 'O'
  AND BKG_CUST_TP_CD = 'F';
#end

#if (${c_ibflag} == 'D')
DELETE BKG_CSTMS_EUR_IO_CUST
WHERE VSL_CD||SKD_VOY_NO||SKD_DIR_CD = @[vvd]
  AND BL_NO = @[bl_no]
  AND CSTMS_PORT_CD = @[cstms_port_cd]
  AND CUST_CNT_CD = @[cust_cnt_cd]
  AND CUST_SEQ = @[cust_seq]
  AND BND_TP_CD = 'O'
  AND BKG_CUST_TP_CD = 'C';
#end

#if (${n_ibflag} == 'D')
DELETE BKG_CSTMS_EUR_IO_CUST
WHERE VSL_CD||SKD_VOY_NO||SKD_DIR_CD = @[vvd]
  AND BL_NO = @[bl_no]
  AND CSTMS_PORT_CD = @[cstms_port_cd]
  AND CUST_CNT_CD = @[cust_cnt_cd]
  AND CUST_SEQ = @[cust_seq]
  AND BND_TP_CD = 'O'
  AND BKG_CUST_TP_CD = 'N';
#end
 EXCEPTION WHEN VALUE_ERROR THEN ROLLBACK;
 WHEN OTHERS THEN ROLLBACK;
 COMMIT;
 END;			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
