<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PSAManifestDBDAOsearchBkgMtyPkupYdCdForPsaRSQL">
			<desc><![CDATA[EDI 추가 전송을 위한 BKG MTY_PKUP_YD_CD 조회]]></desc>
			<sql><![CDATA[
SELECT BKG.MTY_PKUP_YD_CD 
  FROM BKG_BOOKING BKG 
 WHERE BKG.BKG_NO = @[bkg_no]
   AND BKG.MTY_PKUP_YD_CD IN ('SGSINAO', 'SGSINAC')			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
