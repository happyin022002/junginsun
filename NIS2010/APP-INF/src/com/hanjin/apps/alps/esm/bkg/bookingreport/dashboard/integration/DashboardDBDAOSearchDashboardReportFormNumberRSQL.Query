<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DashboardDBDAOSearchDashboardReportFormNumberRSQL">
			<desc><![CDATA[1213화면에서 아이템들을 저장하기전, report number를 가져온다]]></desc>
			<sql><![CDATA[
SELECT RPT_FOM_NO 
FROM BKG_DBD_RPT_FOM 
WHERE CRE_USR_ID = @[usr_id]
AND RPT_FOM_NM = @[rpt_fom_nm]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="rpt_fom_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
