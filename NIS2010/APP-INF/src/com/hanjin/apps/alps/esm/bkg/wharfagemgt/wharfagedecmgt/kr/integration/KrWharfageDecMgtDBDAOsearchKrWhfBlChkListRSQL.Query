<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOsearchKrWhfBlChkListRSQL">
			<desc><![CDATA[S]]></desc>
			<sql><![CDATA[
SELECT DISTINCT T.CSTMS_DECL_TP_CD, T.BL_NO
FROM BKG_CSTMS_KR_BL T,
     (
      SELECT T.BKG_NO, T.CSTMS_DECL_TP_CD, MAX(T.TRNS_SEQ) SEQ
			FROM BKG_CSTMS_KR_BL T, MDM_LOCATION LOC,
			   (
			     SELECT DISTINCT A.VSL_CD,   A.SKD_VOY_NO,  A.SKD_DIR_CD
						FROM BKG_CSTMS_KR_VVD_SMRY A,
						    (
						     SELECT 
									MRN_NO,    
									MRN_CHK_NO,  
									VSL_CD,   
									SKD_VOY_NO,
									SKD_DIR_CD, 
									IO_BND_CD,            
									PORT_CD,            
									OB_DECL_TP_CD,          
									MAX(VVD_SEQ) SEQ
									FROM BKG_CSTMS_KR_VVD_SMRY
									WHERE 1=1  
									AND MRN_NO     = SUBSTR(@[mrn_no],1,10)
									AND MRN_CHK_NO = SUBSTR(@[mrn_no],11,1)
									AND IO_BND_CD  = SUBSTR(@[io_bnd_cd],1,1)
									AND PORT_CD    = @[port_cd]
									GROUP BY MRN_NO,    
									MRN_CHK_NO,  
									VSL_CD,   
									SKD_VOY_NO,
									SKD_DIR_CD, 
									IO_BND_CD,            
									PORT_CD,            
									OB_DECL_TP_CD
						    ) B
						WHERE 1=1
						AND A.MRN_NO     	= SUBSTR(@[mrn_no],1,10)
						AND A.MRN_CHK_NO    = SUBSTR(@[mrn_no],11,1)
						AND A.IO_BND_CD     = SUBSTR(@[io_bnd_cd],1,1)
						AND A.PORT_CD       = @[port_cd]
						AND A.MRN_NO        = B.MRN_NO
						AND A.MRN_CHK_NO    = B.MRN_CHK_NO
						AND A.VSL_CD        = B.VSL_CD
						AND A.SKD_VOY_NO    = B.SKD_VOY_NO
						AND A.SKD_DIR_CD    = B.SKD_DIR_CD
						AND A.IO_BND_CD     = B.IO_BND_CD
						AND A.PORT_CD       = B.PORT_CD
						AND A.OB_DECL_TP_CD = B.OB_DECL_TP_CD
						AND A.VVD_SEQ       = B.SEQ
			   ) C
			WHERE NVL(T.DELT_USR_ID,'N') = 'N'
			AND DECODE(@[io_bnd_cd],'II',T.TS_POD_CD,T.TS_POL_CD)  = LOC.LOC_CD
			AND T.CSTMS_DECL_TP_CD            IN (DECODE(@[io_bnd_cd],'OO','E','I'),DECODE(@[io_bnd_cd],'OO','R','T'))
			AND T.VSL_CD             = C.vsl_cd
			AND T.SKD_VOY_NO         = C.skd_voy_no
			AND T.SKD_DIR_CD         = C.skd_dir_cd
			GROUP BY T.BKG_NO, T.CSTMS_DECL_TP_CD
     ) U
WHERE DECODE(@[io_bnd_cd],'II', T.TS_POD_CD, T.TS_POL_CD) = @[port_cd]
 AND NVL(T.DELT_USR_ID, 'N') = 'N'
 AND T.CSTMS_DECL_TP_CD IN (DECODE(@[io_bnd_cd],'IN','I','II','I','IT','T','OO','E','OT','R'),DECODE(@[io_bnd_cd],'IN','T','II','I','IT','T','OO','E','OT','R'))
 AND T.BKG_NO             = U.BKG_NO
 AND T.CSTMS_DECL_TP_CD   = U.CSTMS_DECL_TP_CD
 AND T.TRNS_SEQ           = U.SEQ
 
MINUS
 
SELECT 
DISTINCT WHF_BND_CD,
BL_NO
FROM BKG_KR_WHF_BL
WHERE 1=1 
AND VSL_CD      = SUBSTR( @[vvd],1,4 )
AND SKD_VOY_NO  = SUBSTR( @[vvd],5,4 )
AND SKD_DIR_CD  = SUBSTR( @[vvd],9,1 )
AND WHF_BND_CD IN (DECODE(@[io_bnd_cd],'IN','I','II','I','IT','T','OO','E','OT','R'),DECODE(@[io_bnd_cd],'IN','T','II','I','IT','T','OO','E','OT','R'))
AND DECODE(@[io_bnd_cd], 'II', WHF_POD_CD, WHF_POL_CD) = @[port_cd] 
AND WHF_BL_STS_CD   != 'D'			]]></sql>
			<params>
				<param name="mrn_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
