<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScqAwkwardDBDAOsearchPriScqAwkCntrTpszRSQL">
			<desc><![CDATA[MDM_CNTR_TP_SZ]]></desc>
			<sql><![CDATA[
SELECT CNTR_TPSZ_CD CD
     , CNTR_TPSZ_CD NM
  FROM MDM_CNTR_TP_SZ
 WHERE DELT_FLG = 'N'
   AND CNTR_TP_CD IN('A','F','O','P','S','C')
   AND CNTR_SZ_CD IN('2','4','5')
   AND CNTR_TPSZ_CD <> 'A2'
 ORDER BY CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
