<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ArrivalNoticeDBDAOmodifyArrNtcFormUSQL">
			<desc><![CDATA[Arrival Notice Form을 update 한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_ARR_NTC_WD 
SET AN_TP_CD =@[an_tp_cd]
 , OFC_CD = @[ofc_cd]
 , POD_CD = NVL(@[pod_cd],'*')
 , CHN_AGN_CD = NVL(@[chn_agn_cd],'*')
 , LOCL_LANG_FLG = @[locl_lang_flg]
 , ARR_PRV_FOM_CD = @[arr_prv_fom_cd]
 , ECLZ_BL_CPY_FLG = @[eclz_bl_cpy_flg]
 , UPD_USR_ID = @[cre_usr_id]
 , UPD_DT = SYSDATE
WHERE AN_SEQ = @[an_seq]			]]></sql>
			<params>
				<param name="an_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="chn_agn_cd" type="12" value="" out="N"/>
				<param name="locl_lang_flg" type="12" value="" out="N"/>
				<param name="arr_prv_fom_cd" type="12" value="" out="N"/>
				<param name="eclz_bl_cpy_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="an_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
