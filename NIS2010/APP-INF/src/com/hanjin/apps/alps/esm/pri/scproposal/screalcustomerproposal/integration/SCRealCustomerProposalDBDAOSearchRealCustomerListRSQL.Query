<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCRealCustomerProposalDBDAOSearchRealCustomerListRSQL">
			<desc><![CDATA[Real Customer를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT PRI.REAL_CUST_SEQ
      ,PRI.CUST_CNT_CD
      ,TO_CHAR(PRI.CUST_SEQ,'FM000000') AS CUST_SEQ
      ,PRI.CUST_VAL_SGM_CD
      ,PRI.PRC_CTRT_CUST_TP_CD
      ,PRI.CUST_SREP_CD
      ,PRI.CUST_SLS_OFC_CD
      ,PRI.REP_CUST_FLG
      ,PRI.PROP_NO
      ,PRI.AMDT_SEQ
      ,CUST.CUST_LGL_ENG_NM REAL_CUST_NM
      ,REP.SREP_NM REAL_CUST_SREP_NM
      ,VAL.INTG_CD_VAL_DP_DESC REAL_CUST_VAL_SGM
	  ,PRI.CUST_LOC_CD
FROM PRI_SP_REAL_CUST PRI
    ,MDM_SLS_REP REP
    ,MDM_CUSTOMER CUST
    ,COM_INTG_CD_DTL VAL
WHERE 1=1
AND PROP_NO = @[prop_no]
AND AMDT_SEQ = @[amdt_seq]
AND CUST.CUST_CNT_CD = PRI.CUST_CNT_CD
AND CUST.CUST_SEQ = PRI.CUST_SEQ
AND REP.SREP_CD = PRI.CUST_SREP_CD
AND VAL.INTG_CD_ID(+) = 'CD00698'
AND VAL.INTG_CD_VAL_CTNT(+) = PRI.CUST_VAL_SGM_CD			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
