<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAOChkPriMotTrfMnEffDtExistRSQL">
			<desc><![CDATA[PRI_MOT_TRF_MN 의 Service Scope 별 중복 Effective Date 건 존재 여부 체크]]></desc>
			<sql><![CDATA[
SELECT	MOT_TRF_SEQ
FROM	PRI_MOT_TRF_MN
WHERE	SVC_SCP_CD = @[svc_scp_cd]
AND		EFF_DT = TO_DATE ( @[eff_dt], 'YYYY-MM-DD' )			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
