<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOsearchYearComboRSQL">
			<desc><![CDATA[BookingUtilDBDAOsearchYearComboRSQL]]></desc>
			<sql><![CDATA[
/* Year 콤보의 목록을 가져온다 */
  SELECT T_YEAR - CPY_NO CODE
    FROM (
           SELECT 2015 AS F_YEAR
                 ,TO_CHAR(SYSDATE, 'YYYY') + 1 AS T_YEAR
             FROM DUAL
         ) Y
        ,COM_CPY_NO C
   WHERE C.CPY_NO <= T_YEAR - F_YEAR
ORDER BY C.CPY_NO			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
