<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOsearchRejectEdiHeaderRSQL">
			<desc><![CDATA[searchRejectEdiHeader]]></desc>
			<sql><![CDATA[
select attr_ctnt2 rcv_id, ATTR_CTNT3 sndr_id, ATTR_CTNT4 flat_file_type
  from (
    select 1 rnk1, attr_ctnt2, ATTR_CTNT3, ATTR_CTNT4
      from bkg_hrd_cdg_ctnt 
     where HRD_CDG_ID = 'REJECT_HOST_TP_ID'
       and ATTR_CTNT2 = @[rcv_id]
    union   
    select 2 rnk1, attr_ctnt2, ATTR_CTNT3, ATTR_CTNT4
      from bkg_hrd_cdg_ctnt 
     where HRD_CDG_ID = 'REJECT_HOST_TP_ID'
       and ATTR_CTNT1 like substr(@[rcv_id], 1, 4)||'%'
    union   
    select 3 rnk1, attr_ctnt2, ATTR_CTNT3, ATTR_CTNT4
      from bkg_hrd_cdg_ctnt 
     where HRD_CDG_ID = 'REJECT_HOST_TP_ID'
       and ATTR_CTNT1 = 'IKEA.EBCCNS1'
    )   
 where rownum = 1			]]></sql>
			<params>
				<param name="rcv_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
