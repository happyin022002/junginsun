<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOAddCustChkPntAttachCSQL">
			<desc><![CDATA[Attachment 를 등록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_ATCH_FILE 
(   ATCH_FILE_LNK_ID,
    ATCH_FILE_LNK_SEQ,
    FILE_SAV_ID,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
) VALUES
(
    @[atch_file_lnk_id],
    (SELECT NVL(MAX(ATCH_FILE_LNK_SEQ),0) + 1
     FROM BKG_ATCH_FILE
     WHERE ATCH_FILE_LNK_ID = @[atch_file_lnk_id]),
    @[file_sav_id],
    @[cre_usr_id],
    SYSDATE,
    @[upd_usr_id],
    SYSDATE    
)			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
