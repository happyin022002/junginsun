<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOModifyRestrictCmdtUSQL">
			<desc><![CDATA[BKG_IMP_RSTR_CMDT update]]></desc>
			<sql><![CDATA[
UPDATE BKG_IMP_RSTR_CMDT
SET		TS_FLG		  = DECODE(NVL(@[ts_flg], 0), 0, 'N', 'Y'),
		FROB_FLG	  = DECODE(NVL(@[frob_flg], 0), 0, 'N', 'Y'),
		RSTR_CMDT_NM  = @[rstr_cmdt_nm],
		PROHI_CMDT_NM = @[prohi_cmdt_nm],
		EFF_DT        = TO_DATE(@[eff_dt], 'YYYY-MM-DD HH24:MI:SS'),
		EXP_DT		  = TO_DATE(@[exp_dt], 'YYYY-MM-DD HH24:MI:SS'),
		WEB_SITE_URL  = @[web_site_url],
		INTER_RMK	  = @[inter_rmk],
        UPD_USR_ID = @[upd_usr_id],
        UPD_DT = SYSDATE,
	    RSTR_CMDT_GRP_NM = @[rstr_cmdt_grp_nm]
WHERE RGN_OFC_CD  = @[rgn_ofc_cd]
AND   CNT_CD	  = @[cnt_cd]
AND   LOC_CD      = DECODE(@[loc_cd],'ALL','*****',@[loc_cd])
AND   DP_SEQ      = @[dp_seq]			]]></sql>
			<params>
				<param name="ts_flg" type="12" value="" out="N"/>
				<param name="frob_flg" type="12" value="" out="N"/>
				<param name="rstr_cmdt_nm" type="12" value="" out="N"/>
				<param name="prohi_cmdt_nm" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="web_site_url" type="12" value="" out="N"/>
				<param name="inter_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rstr_cmdt_grp_nm" type="12" value="" out="N"/>
				<param name="rgn_ofc_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="dp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
