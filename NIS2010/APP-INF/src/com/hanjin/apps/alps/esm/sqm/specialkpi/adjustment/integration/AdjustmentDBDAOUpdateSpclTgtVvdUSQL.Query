<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentDBDAOUpdateSpclTgtVvdUSQL">
			<desc><![CDATA[[SKD Adjustment by VVD] 에서 확정데이터를 [수정] 합니다

* 2015.06.15 김용습 [CHM-201536164] 주간 MAS Open에 따른 타모듈 프로그램 적용 요청]]></desc>
			<sql><![CDATA[
UPDATE SQM_SPCL_TGT_VVD
   SET BSE_MON      = @[mas_bse_mon]
      ,BSE_WK       = @[mas_bse_wk]
      ,FNL_BSA_CAPA = @[mas_fnl_bsa_capa]
      ,UPD_USR_ID   = @[usr_id]
      ,UPD_DT       = SYSDATE
 WHERE BSE_TP_CD  = @[f_bse_tp_cd]
   AND BSE_YR     = @[f_bse_yr]
   AND BSE_QTR_CD = @[f_bse_qtr_cd]
   AND TRD_CD     = @[trd_cd]
   AND RLANE_CD   = @[rlane_cd]
   AND DIR_CD     = @[dir_cd]
   AND VSL_CD || SKD_VOY_NO || SKD_DIR_CD = @[vvd]			]]></sql>
			<params>
				<param name="mas_bse_mon" type="12" value="" out="N"/>
				<param name="mas_bse_wk" type="12" value="" out="N"/>
				<param name="mas_fnl_bsa_capa" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="f_bse_tp_cd" type="12" value="" out="N"/>
				<param name="f_bse_yr" type="12" value="" out="N"/>
				<param name="f_bse_qtr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
