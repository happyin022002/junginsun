<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOaddBlAtchCSQL">
			<desc><![CDATA[BLIssuanceDBDAOaddBlAtchCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_N3RD_PTY_BL_FILE_STO 
(
   BKG_NO
  ,POL_CD
  ,N3PTY_OFC_CD
  ,PAYR_CUST_CNT_CD
  ,PAYR_CUST_SEQ
  ,OBL_ISS_OFC_CD
  ,N3PTY_BL_CHG_TTL_AMT
  ,FRT_TERM_CD
  ,FILE_SEQ
  ,FILE_NM
  ,FILE_PATH_RMK
  ,FILE_SAV_ID
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
) 
VALUES 
(
  @[bkg_no]
 ,@[pol_cd]
 ,@[n3pty_ofc_cd]
 ,@[payr_cust_cnt_cd]
 ,@[payr_cust_seq]
 ,@[obl_iss_ofc_cd]
 ,@[n3pty_bl_chg_ttl_amt]
 ,@[frt_term_cd]
 ,(
	SELECT NVL(MAX(FILE_SEQ),0)+1
	FROM BKG_N3RD_PTY_BL_FILE_STO
	WHERE BKG_NO = @[bkg_no]
      AND POL_CD = @[pol_cd]
      AND N3PTY_OFC_CD = @[n3pty_ofc_cd]
      AND PAYR_CUST_CNT_CD = @[payr_cust_cnt_cd]
      AND PAYR_CUST_SEQ = @[payr_cust_seq]
      AND OBL_ISS_OFC_CD = @[obl_iss_ofc_cd]
      AND N3PTY_BL_CHG_TTL_AMT = @[n3pty_bl_chg_ttl_amt]
      AND FRT_TERM_CD = @[frt_term_cd]
  )
  ,@[file_nm]
  ,@[file_path_rmk]
  ,@[file_sav_id]
  ,@[cre_usr_id]
  ,sysdate
  ,@[upd_usr_id]
  ,sysdate
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="n3pty_ofc_cd" type="12" value="" out="N"/>
				<param name="payr_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="payr_cust_seq" type="12" value="" out="N"/>
				<param name="obl_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="n3pty_bl_chg_ttl_amt" type="12" value="" out="N"/>
				<param name="frt_term_cd" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_path_rmk" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
