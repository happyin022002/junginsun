<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchRgnOfficeCdRSQL">
			<desc><![CDATA[Region-ESM_BKG_0226]]></desc>
			<sql><![CDATA[
SELECT OFC.OFC_N3RD_LVL_CD REGION_CD
,     MO.OFC_ENG_NM REGION_NM
FROM DMT_OFC_LVL_V OFC
,    MDM_ORGANIZATION MO 
WHERE OFC.OFC_LVL = '3'
AND OFC.OFC_KND_CD = '2'
AND MO.OFC_CD =  OFC.OFC_N3RD_LVL_CD
GROUP BY OFC.OFC_N3RD_LVL_CD,MO.OFC_ENG_NM			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
