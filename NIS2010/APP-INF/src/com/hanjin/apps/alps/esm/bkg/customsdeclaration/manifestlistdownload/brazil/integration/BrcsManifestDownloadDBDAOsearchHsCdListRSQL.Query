<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BrcsManifestDownloadDBDAOsearchHsCdListRSQL">
			<desc><![CDATA[브라질 NCM 코드 조회]]></desc>
			<sql><![CDATA[
SELECT  CNT_CD
      , MF_CMDT_CD BRZ_CMDT_CD
      , DECODE(ACT_FLG, 'N', 'Y', 'N') ACT_FLG
      , CMDT_DESC
      , CMDT_CATE_CTNT
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , TO_CHAR(UPD_DT, 'YYYY-MM-DD') UPD_DT
	  , ( SELECT OFC_CD FROM COM_USER WHERE USR_ID = A.UPD_USR_ID AND USE_FLG = 'Y' ) OFFICE
FROM    BKG_CSTMS_CMDT A
WHERE   CNT_CD = 'BR'

#if (${brz_cmdt_cd} != '')

	#if (${page_gubun} != 'popup')
AND	    MF_CMDT_CD LIKE @[brz_cmdt_cd] || '%'
	#else
AND	    MF_CMDT_CD = @[brz_cmdt_cd]
	#end

#else

    #if (${page_gubun} == 'popup')
AND	    1 = 0
    #end

#end

#if (${cmdt_desc} != '')
AND	    UPPER(cmdt_desc) LIKE '%' || UPPER(@[cmdt_desc]) || '%'
#end

#if (${del_check} != 'on')
AND     ACT_FLG = 'Y'
#end			]]></sql>
			<params>
				<param name="brz_cmdt_cd" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
