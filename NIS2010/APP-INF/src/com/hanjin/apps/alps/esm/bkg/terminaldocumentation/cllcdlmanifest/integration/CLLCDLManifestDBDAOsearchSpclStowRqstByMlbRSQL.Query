        							'CAVAN', DECODE(SUBSTR(NVL(BK.BLCK_STWG_CD,'LOC'), 3, 1),'1','ONE','2','TWO','3','THR','4','FOR','5','FIV','T','TRS', 'LOC'),'') IN ('LOC','ONE','TWO','THR','FOR','FIV','HOT','TRS')
GROUP BY NVL(LO.UN_LOC_CD,BV.POD_CD), BK.STWG_CD, BQ.CNTR_TPSZ_CD, OP_CNTR_QTY, VPS.CLPT_SEQ, BK.BKG_NO

