      DECODE(CLL.STWG_CD, 'OLBL', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) OLBL_20,
      DECODE(CLL.STWG_CD, 'OLBL', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) OLBL_40,
      DECODE(CLL.STWG_CD, 'OLBL', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0,  1), 0) OLBL_40H,
