      DECODE(CLL.STWG_CD, 'AF', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) AF_40,
      DECODE(CLL.STWG_CD, 'AF', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0,  '7', 0, 1), 0) AF_40H,
      DECODE(CLL.STWG_CD, 'AF', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) AF_45,
