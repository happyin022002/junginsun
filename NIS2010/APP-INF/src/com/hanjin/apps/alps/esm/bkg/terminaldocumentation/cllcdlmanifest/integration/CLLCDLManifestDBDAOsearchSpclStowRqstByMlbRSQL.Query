      DECODE(CLL.STWG_CD, 'AL', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0,  1), 0) AL_40H,
      DECODE(CLL.STWG_CD, 'AL', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) AL_45,
