      DECODE(CLL.STWG_CD, 'BC', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) BC_40,
      DECODE(CLL.STWG_CD, 'BC', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0,  1), 0) BC_40H,
      DECODE(CLL.STWG_CD, 'BC', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) BC_45,
      DECODE(CLL.STWG_CD, 'MUPG', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 1, 0), 0) MUPG_20,
      DECODE(CLL.STWG_CD, 'MUPG', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '4', 1, 0), 0) MUPG_40,
      DECODE(CLL.STWG_CD, 'MUPG', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0,  1), 0) MUPG_40H,
      DECODE(CLL.STWG_CD, 'MUPG', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) MUPG_45,
