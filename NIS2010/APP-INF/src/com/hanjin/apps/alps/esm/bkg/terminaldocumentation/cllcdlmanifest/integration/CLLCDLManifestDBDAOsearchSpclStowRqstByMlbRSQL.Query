      DECODE(CLL.STWG_CD, 'OBSS', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '2', 0, '4', 0, '7', 0,  1), 0) OBSS_40H,
      DECODE(CLL.STWG_CD, 'OBSS', DECODE(SUBSTR(BQ.CNTR_TPSZ_CD, 2, 1), '7', 1, 0), 0) OBSS_45,
