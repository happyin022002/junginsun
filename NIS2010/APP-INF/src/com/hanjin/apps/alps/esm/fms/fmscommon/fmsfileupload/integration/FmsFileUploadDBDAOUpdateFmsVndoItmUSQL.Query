<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FmsFileUploadDBDAOUpdateFmsVndoItmUSQL">
			<desc><![CDATA[FMS VNOR ITM 수정]]></desc>
			<sql><![CDATA[
#if(${vnor_itm_seq} != '')
    UPDATE FMS_VNOR_ITM F
    SET F.ATCH_FILE_FLET_LNK_ID = @[atch_file_lnk_id]
    WHERE 1=1
    AND F.VSL_CD = @[vsl_cd]
    AND F.VNOR_SEQ = @[vnor_seq]
    AND F.VNOR_ITM_SEQ = @[vnor_itm_seq]
#else
    UPDATE FMS_BUNKER F
    SET  F.ATCH_FILE_FLET_LNK_ID = @[atch_file_lnk_id]
        ,F.UPD_USR_ID = @[upd_usr_id]
        ,F.UPD_DT = SYSDATE
    WHERE 1=1
    AND F.FLET_CTRT_NO = @[vsl_cd]
    AND F.BNK_SEQ = @[vnor_seq]
#end			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vnor_seq" type="12" value="" out="N"/>
				<param name="vnor_itm_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
