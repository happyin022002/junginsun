<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOSearchHistNewBkgQtyDtlRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOSearchHistNewBkgQtyDtlRSQL]]></desc>
			<sql><![CDATA[
SELECT HIS_CATE_NM
     , PRE_CTNT
     , CRNT_CTNT
  FROM 
    (
        SELECT 'Volume Detail' HIS_CATE_NM
                , '' PRE_CTNT
                , NOW.CNTR_TPSZ_CD||
                  '/'||NOW.OP_CNTR_QTY||
                  '/'||NOW.RCV_TERM_CD||
                  '/'||NOW.DE_TERM_CD||
                  '/D:'||NOW.DCGO_FLG||
                  '/R:'||NOW.RC_FLG||
                  '/A:'||NOW.AWK_CGO_FLG||
                  '/B:'||NOW.BB_CGO_FLG||
                  '/'||NOW.EQ_SUBST_CNTR_TPSZ_CD CRNT_CTNT           
#if (${ca_flg} == 'Y')
          FROM BKG_QTY_DTL_HIS NOW
         WHERE NOW.BKG_NO  = @[bkg_no]
	       AND NOW.CORR_NO = 'TMP0000001' 
#else
          FROM BKG_QTY_DTL NOW
         WHERE NOW.BKG_NO = @[bkg_no]
#end

#if ($field_list.size() > 0)
           AND (BKG_NO, CNTR_TPSZ_CD, SUBST_SEQ) NOT IN (
           #foreach($field_id in ${field_list}) 
               #if($velocityCount < $field_list.size()) $field_id,  #else $field_id  #end
           #end
           )
#end 		
		UNION ALL
        SELECT 'Hanger Vol' HIS_CATE_NM
                , '' PRE_CTNT
                , NOW.CNTR_TPSZ_CD||
                  '/'||NOW.OP_CNTR_QTY||
                  '/Merchant:'||NOW.MER_HNGR_FLG||
                  '/Carrier:'||
                  '(s:'||NOW.CRR_HNGR_SGL_BAR_USE_FLG||
                  '/d:'||NOW.CRR_HNGR_DBL_BAR_USE_FLG||
                  '/t:'||NOW.CRR_HNGR_TPL_BAR_USE_FLG||')' CRNT_CTNT
#if (${ca_flg} == 'Y')
          FROM BKG_QTY_DTL_HIS NOW
         WHERE NOW.BKG_NO      (+) = @[bkg_no]
	       AND NOW.CORR_NO     (+) = 'TMP0000001' 
#else
          FROM BKG_QTY_DTL NOW
         WHERE NOW.BKG_NO      (+) = @[bkg_no]
#end
		   AND (NOW.CRR_HNGR_FLG = 'Y' OR NOW.MER_HNGR_FLG = 'Y' OR NOW.CRR_HNGR_SGL_BAR_USE_FLG = 'Y' OR NOW.CRR_HNGR_DBL_BAR_USE_FLG = 'Y' OR NOW.CRR_HNGR_TPL_BAR_USE_FLG = 'Y')

#if ($field_list.size() > 0)
           AND (BKG_NO, CNTR_TPSZ_CD, SUBST_SEQ) NOT IN (
           #foreach($field_id in ${field_list}) 
               #if($velocityCount < $field_list.size()) $field_id,  #else $field_id  #end
           #end
           )
#end 		
    )
 WHERE CRNT_CTNT IS NOT NULL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="TYO329840200" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
