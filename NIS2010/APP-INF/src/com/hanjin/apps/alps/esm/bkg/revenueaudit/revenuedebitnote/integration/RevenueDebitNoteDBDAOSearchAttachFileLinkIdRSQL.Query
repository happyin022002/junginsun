<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RevenueDebitNoteDBDAOSearchAttachFileLinkIdRSQL">
			<desc><![CDATA[Attach File Link ID를 조회한다]]></desc>
			<sql><![CDATA[
SELECT ATCH_FILE_LNK_ID
FROM (
        SELECT ATCH_FILE_LNK_ID
        FROM BKG_REV_DR_NOTE
        WHERE RDN_NO = @[rdn_no]
        AND RVIS_SEQ = @[rvis_seq]
        AND ATCH_FILE_LNK_ID IS NOT NULL
        UNION ALL
        SELECT  TO_CHAR(SYSDATE,'YYMM')|| LPAD(NVL(SUBSTR(MAX(ATCH_FILE_LNK_ID),5,10), 0) + 1, 6, '0') ATCH_FILE_LNK_ID
        FROM    BKG_ATCH_FILE
        WHERE   ATCH_FILE_LNK_ID  LIKE TO_CHAR(SYSDATE, 'YYMM') || '%'
      )
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="rdn_no" type="12" value="" out="N"/>
				<param name="rvis_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
