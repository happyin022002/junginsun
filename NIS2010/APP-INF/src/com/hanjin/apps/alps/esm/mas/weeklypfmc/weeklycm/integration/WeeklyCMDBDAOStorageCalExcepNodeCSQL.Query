<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WeeklyCMDBDAOStorageCalExcepNodeCSQL">
			<desc><![CDATA[Storage Calculation Exception Node 를 Save(저장 중 - insert) 한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MAS_STO_EXP_NOD (
	NOD_CD,
	CTRL_OFC_CD,
	OB_STO_FLG,
	OB_STO_NOD_TP_CD,
	IB_STO_FLG,
	IB_STO_NOD_TP_CD,
	EXP_CUST_CD_CTNT,
	STO_EXP_NOD_RMK,
	MAS_UC_FLG,
    DELT_FLG,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
) VALUES( 
	@[nod_cd],
	@[ctrl_ofc_cd],
	@[ob_sto_flg],
	@[ob_nod_tp],
	@[ib_sto_flg],
	@[ib_nod_tp],
	@[exp_cust_cd],
	@[rmk],
	@[mas_uc_flg],	
    @[delt_flg],	
	@[cre_usr_id],
	SYSDATE,
    @[upd_usr_id],
	SYSDATE
)			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="ob_sto_flg" type="12" value="" out="N"/>
				<param name="ob_nod_tp" type="12" value="" out="N"/>
				<param name="ib_sto_flg" type="12" value="" out="N"/>
				<param name="ib_nod_tp" type="12" value="" out="N"/>
				<param name="exp_cust_cd" type="12" value="" out="N"/>
				<param name="rmk" type="12" value="" out="N"/>
				<param name="mas_uc_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
