<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ACMSimulationDBDAOSearchActualAgreementListRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT AGN_AGMT_NO,
       AGMT_FM_DT_CD,
       TO_CHAR(TO_DATE(AGMT_FM_DT, 'YYYYMMDD'), 'YYYY-MM-DD') AS AGMT_FM_DT,
       AGMT_TO_DT_CD,
       TO_CHAR(TO_DATE(AGMT_TO_DT, 'YYYYMMDD'), 'YYYY-MM-DD') AS AGMT_TO_DT,
       DELT_FLG,
       TO_CHAR(UPD_DT, 'YYYY-MM-DD') AS UPD_DT
  FROM ACM_AGN_AGMT_MST
 WHERE AGN_CD = @[agn_cd]
			]]></sql>
			<params>
				<param name="agn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
