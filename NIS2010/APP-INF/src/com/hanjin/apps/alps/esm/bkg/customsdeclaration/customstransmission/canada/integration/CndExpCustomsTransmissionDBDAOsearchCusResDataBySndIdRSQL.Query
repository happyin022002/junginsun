<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CndExpCustomsTransmissionDBDAOsearchCusResDataBySndIdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
       ,SKD_VOY_NO
       ,SKD_DIR_CD
       ,POL_CD
       ,POD_CD
FROM BKG_CSTMS_ADV_STWG_SND_LOG
WHERE 1=1
    AND SND_PROC_ID = 'STW_CA'
    AND STWG_SND_ID =@[crr_bat_no]			]]></sql>
			<params>
				<param name="crr_bat_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
