<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOSearchSalesTeamListRSQL">
			<desc><![CDATA[Sales Rep의 Office Team을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT OFC_TEAM_CD 
FROM MDM_SLS_REP
WHERE OFC_CD = @[ofc_cd]
AND SREP_STS_CD = 'N'
AND OFC_TEAM_CD IS NOT NULL
ORDER BY OFC_TEAM_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
