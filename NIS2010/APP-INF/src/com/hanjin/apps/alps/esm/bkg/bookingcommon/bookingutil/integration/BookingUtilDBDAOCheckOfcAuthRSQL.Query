<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingUtilDBDAOCheckOfcAuthRSQL">
			<desc><![CDATA[입력한 부서코드가 권한이 있는지 확인]]></desc>
			<sql><![CDATA[
SELECT 'Y' SUB_FLG
FROM(SELECT OFC_CD 
       FROM BKG_OFC_LVL_V
      WHERE @[ofc_cd] IN (OFC_CD ,OFC_N3RD_LVL_CD, OFC_N4TH_LVL_CD, OFC_N5TH_LVL_CD,OFC_N6TH_LVL_CD, OFC_N7TH_LVL_CD)
     )
WHERE OFC_CD = @[bkg_ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
