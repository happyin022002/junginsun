<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChinaManifestListDownloadDBDAOsearchPodRoutDescRSQL">
			<desc><![CDATA[searchPodRoutDesc]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(MAX(SYS_CONNECT_BY_PATH (POD_CD,';')),2)||';' AS POD_ROUT_DESC
FROM  (
    SELECT 	BKG_NO, POD_CD,
    		ROW_NUMBER() OVER(PARTITION BY BKG_NO ORDER BY BKG_NO, VSL_PRE_PST_CD, VSL_SEQ) RNUM
    FROM   	BKG_VVD
    WHERE  	1=1
    AND    	BKG_NO = @[bkg_no]
	AND		VSL_PRE_PST_CD||VSL_SEQ >= ( SELECT VSL_PRE_PST_CD||VSL_SEQ FROM BKG_VVD
                                          WHERE BKG_NO 				= @[bkg_no]
                                          AND 	POD_CD 				= @[bkg_pod_cd]

                                          AND 	VSL_CD 				= @[vsl_cd]
                                          AND 	SKD_VOY_NO			= @[skd_voy_no]
                                          AND 	SKD_DIR_CD			= @[skd_dir_cd])
)
GROUP BY BKG_NO
START WITH RNUM = 1
CONNECT BY PRIOR RNUM = RNUM - 1 AND PRIOR BKG_NO = BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_pod_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
