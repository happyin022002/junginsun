<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAProposalMainDBDAORsltMQCEstimateVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT MN.PROP_NO
    ,MN.AMDT_SEQ
    ,MN.SVC_SCP_CD
    ,MN.prs_prop_scp_mqc_qty
    ,MN.PROP_SCP_STS_CD
    ,PRS_RT_CMPB_CALC_FLG AS CALC_FLG
FROM   PRI_RP_SCP_MN MN
WHERE 
  MN.PROP_NO = @[frm_prop_no]
AND MN.AMDT_SEQ = @[frm_amdt_seq]
ORDER BY MN.CRE_DT, MN.SVC_SCP_CD			]]></sql>
			<params>
				<param name="frm_prop_no" type="12" value="" out="N"/>
				<param name="frm_amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
