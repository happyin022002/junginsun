<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EBookingReceiptDBDAOSearchXterSIPoOtherNoBkgRSQL">
			<desc><![CDATA[alps의 export/import licens no를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT MST.BKG_NO
      ,'' REF_SEQ
      ,A.BL_NO
      ,'BKPO' BKG_REF_TP_CD
      ,NVL(MST.PO_NO,CUST_REF_NO_CTNT) CUST_REF_NO_CTNT
      ,'I' IBFLAG
 FROM BKG_XTER_RQST_MST MST
     ,BKG_BOOKING A
     ,BKG_REFERENCE B
WHERE XTER_SNDR_ID = @[xter_sndr_id]
  AND XTER_RQST_NO = @[xter_rqst_no]
  AND XTER_RQST_SEQ= @[xter_rqst_seq]
  AND MST.BKG_NO = A.BKG_NO(+)
  AND MST.BKG_NO = B.BKG_NO(+)
  AND B.BKG_REF_TP_CD(+) ='BKPO'			]]></sql>
			<params>
				<param name="xter_sndr_id" type="12" value="" out="N"/>
				<param name="xter_rqst_no" type="12" value="" out="N"/>
				<param name="xter_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
