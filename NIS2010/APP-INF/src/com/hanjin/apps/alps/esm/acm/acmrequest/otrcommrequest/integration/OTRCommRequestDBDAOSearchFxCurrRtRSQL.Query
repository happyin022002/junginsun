<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OTRCommRequestDBDAOSearchFxCurrRtRSQL">
			<desc><![CDATA[SearchFxCurrRt]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(M.FX_CURR_RT),0) AS PAY_XCH_RT 
FROM ACM_OFC_INFO I, MDM_ORGANIZATION M
WHERE 1=1
AND I.AGN_CD = @[agn_cd]
AND REPLACE(@[aply_dt],'-', '') BETWEEN I.AGN_FM_DT AND I.AGN_TO_DT
AND I.AGN_CD = M.OFC_CD
AND I.XCH_RT_DIV_LVL = 4
AND M.BIL_CURR_CD = @[curr_cd]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="agn_cd" type="12" value="" out="N"/>
				<param name="aply_dt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
