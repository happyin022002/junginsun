<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOModifyBkgSrFaxUrgencyUSQL">
			<desc><![CDATA[PerformanceReportDBDAOModifyBkgSrFaxUrgencyUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_SR_FAX
SET SR_URG_CD =@[sr_urg_cd],
SR_AMD_TP_CD =@[sr_amd_tp_cd],
BKG_NO =@[bkg_no],
BKG_NO_MTCH_STS_CD ='S',
RCV_OFC_CD = (SELECT A.BKG_OFC_CD 
			    FROM BKG_BOOKING A  
               WHERE A.BKG_NO = @[bkg_no]),
UPD_USR_ID =@[usr_id],
UPD_DT = SYSDATE
WHERE SR_NO = @[sr_no]
AND FAX_LOG_REF_NO =@[fax_log_ref_no]
AND SR_KND_CD =@[sr_knd_cd]			]]></sql>
			<params>
				<param name="sr_urg_cd" type="12" value="" out="N"/>
				<param name="sr_amd_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="sr_no" type="12" value="" out="N"/>
				<param name="fax_log_ref_no" type="12" value="" out="N"/>
				<param name="sr_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
