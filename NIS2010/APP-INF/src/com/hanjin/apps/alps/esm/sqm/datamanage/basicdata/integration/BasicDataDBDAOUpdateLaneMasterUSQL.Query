<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BasicDataDBDAOUpdateLaneMasterUSQL">
			<desc><![CDATA[[Lane Master]을 [업데이트] 합니다.
2014.01.10 박은주 [CHM-201328244] IAS Sector Sales 판매시스템 개발(IAS_SCTR_FLG 추가)]]></desc>
			<sql><![CDATA[
UPDATE SQM_QTA_LANE_MGMT 
   SET SQM_ACT_FLG  = DECODE(@[sqm_act_flg],1,'Y','N') 
      ,IAS_SCTR_FLG = DECODE(@[ias_sctr_flg],1,'Y',0,'N',NULL) 
      ,LANE_DIR_CD  = @[lane_dir_cd]
      ,UPD_USR_ID   = @[upd_usr_id]
      ,UPD_DT       = SYSDATE 
 WHERE TRD_CD     = @[trd_cd]
   AND RLANE_CD   = @[rlane_cd]
   AND SUB_TRD_CD = @[sub_trd_cd]			]]></sql>
			<params>
				<param name="sqm_act_flg" type="12" value="" out="N"/>
				<param name="ias_sctr_flg" type="12" value="" out="N"/>
				<param name="lane_dir_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
