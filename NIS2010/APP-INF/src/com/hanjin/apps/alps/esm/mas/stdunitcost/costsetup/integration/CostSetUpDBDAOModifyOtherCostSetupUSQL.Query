<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostSetUpDBDAOModifyOtherCostSetupUSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_MNL_COST_STUP A
 USING (
         SELECT SUBSTR('${cost_yrwk}',1,4) cost_yr,
                SUBSTR('${cost_yrwk}',5,2) cost_wk,
                @[trd_cd] TRD_CD,
                @[rlane_cd] RLANE_CD,
                (SELECT COST_YR||EQ_WK FROM MAS_WK_PRD WHERE COST_YR = SUBSTR('${cost_yrwk}',1,4) AND COST_WK  = SUBSTR('${cost_yrwk}',5,2)) AS COST_YRMON
           FROM DUAL) B
   ON (    A.COST_YRMON LIKE B.COST_YR||'%'
       AND A.COST_WK = B.COST_WK
       AND A.TRD_CD  = B.TRD_CD
       AND A.RLANE_CD = B.RLANE_CD)
WHEN MATCHED THEN
    UPDATE 
        SET OTR_EXPN_AMT = @[otr_expn_amt],
            UPD_USR_ID   = '${usr_id}',
            UPD_DT = SYSDATE
WHEN NOT MATCHED THEN 
    INSERT 
    ( COST_YRMON,COST_WK,TRD_CD,RLANE_CD,IOC_CD,DIR_CD,SUB_TRD_CD,OTR_EXPN_AMT,
      CRE_USR_ID,CRE_DT,UPD_USR_ID,UPD_DT)
   VALUES (B.COST_YRMON,
           SUBSTR('${cost_yrwk}',5,2),
           @[trd_cd],
           @[rlane_cd],
           'O',
           'M',
           'OT',
           @[otr_expn_amt],
           '${usr_id}',
           sysdate,
           '${usr_id}',
           sysdate
	)
			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="otr_expn_amt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
