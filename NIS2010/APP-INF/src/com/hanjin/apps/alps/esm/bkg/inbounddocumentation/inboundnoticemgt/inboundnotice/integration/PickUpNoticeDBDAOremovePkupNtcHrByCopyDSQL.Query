<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PickUpNoticeDBDAOremovePkupNtcHrByCopyDSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
DELETE FROM BKG_PKUP_NTC_HR
WHERE	PKUP_NTC_SEQ = (SELECT PKUP_NTC_SEQ
                        FROM   BKG_PKUP_NTC_STUP
                        WHERE  PKUP_NTC_SND_TP_CD = 'A'
                        AND    OFC_CD             = @[ofc_cd]
                        AND    DEL_CD             = DECODE(nvl(@[del_cd],'ALL'),'ALL','*',@[del_cd]))			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
