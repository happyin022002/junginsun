<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaCustomsTransmissionDBDAOmodifyCntrRsltUSQL">
			<desc><![CDATA[modifyCntrRslt]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_CNTR_RSLT
   SET CSTMS_CLR_CD = @[cstms_clr_cd]
      ,PRE_CSTMS_CLR_CD = CSTMS_CLR_CD
      ,PRT_BL_NO = @[prt_bl_no]
      ,CSTMS_RMK1 = @[cstms_rmk1]
WHERE CNT_CD = 'US'
  AND BL_NO = @[bl_no]
  AND CSTMS_SEQ = (SELECT MAX(CSTMS_SEQ) 
                     FROM BKG_CSTMS_ADV_CNTR_RSLT 
                    WHERE CNT_CD = 'US'
                      AND BL_NO = @[bl_no]
                      AND CNTR_NO LIKE TRIM(@[cntr_no]) || '%'
                  )
  AND CNTR_NO LIKE TRIM(@[cntr_no]) || '%'			]]></sql>
			<params>
				<param name="cstms_clr_cd" type="12" value="" out="N"/>
				<param name="prt_bl_no" type="12" value="" out="N"/>
				<param name="cstms_rmk1" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
