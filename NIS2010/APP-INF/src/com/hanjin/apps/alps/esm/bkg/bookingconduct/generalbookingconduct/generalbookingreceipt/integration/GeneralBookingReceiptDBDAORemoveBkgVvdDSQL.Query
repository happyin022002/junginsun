<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAORemoveBkgVvdDSQL">
			<desc><![CDATA[VVD 삭제]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')
DELETE FROM BKG_VVD_HIS
#else
DELETE FROM BKG_VVD
#end
WHERE	BKG_NO = @[bkg_no]
#if (${ca_flg}== 'Y')
AND   CORR_NO = 'TMP0000001'
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
