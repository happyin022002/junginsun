<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Eur24ManifestDownloadDBDAORemoveBlCntrDSQL">
			<desc><![CDATA[RemoveBlCntr]]></desc>
			<sql><![CDATA[
DELETE
FROM BKG_CSTMS_EUR_CNTR
WHERE VSL_CD||SKD_VOY_NO||SKD_DIR_CD = @[vvd]
  AND BL_NO = @[bl_no]
  AND CSTMS_PORT_CD = @[cstms_port_cd]
  AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cstms_port_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
