<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationCMDBDAOModifyCrdDtUSQL">
			<desc><![CDATA[ModifyCrdDt]]></desc>
			<sql><![CDATA[
#if (${ca_flg} == 'Y') 
UPDATE BKG_CNTR_HIS 
SET    CGO_RCV_DT       = TO_DATE(@[cgo_rcv_dt], 'YYYYMMDDHH24MI')
,      UPD_USR_ID       = @[upd_usr_id]
,      UPD_DT           = sysdate        
WHERE  BKG_NO = @[bkg_no]
AND    CORR_NO = 'TMP0000001'
AND	   CNTR_NO = @[cntr_no]
#else 
UPDATE BKG_CONTAINER SET
     CGO_RCV_DT       = TO_DATE(@[cgo_rcv_dt], 'YYYYMMDDHH24MI')
,    UPD_USR_ID       = @[upd_usr_id]
,    UPD_DT           = sysdate        
WHERE	BKG_NO = @[bkg_no]
AND	CNTR_NO = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="cgo_rcv_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
