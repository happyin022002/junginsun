<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOSearchActualCustomerRSQL">
			<desc><![CDATA[GeneralBookingSearchDBDAOSearchActualCustomerRSQL]]></desc>
			<sql><![CDATA[
#if (${bkg_upld_flg} == 'N')

WITH 
SCP AS(
SELECT A.SVC_SCP_CD
FROM MDM_SVC_SCP_LMT A,
     MDM_SVC_SCP_LMT B, 
     MDM_SVC_SCP C
WHERE A.SVC_SCP_CD = B.SVC_SCP_CD
AND A.SVC_SCP_CD = C.SVC_SCP_CD
AND C.DELT_FLG = 'N'
AND A.ORG_DEST_CD = 'O'
AND A.DELT_FLG = 'N'
AND A.SVC_SCP_IND_FLG ='Y'
AND A.RGN_CD IN (SELECT RGN_CD FROM MDM_LOCATION WHERE LOC_CD = @[por_cd])
AND B.ORG_DEST_CD = 'D'
AND B.DELT_FLG = 'N'
AND B.SVC_SCP_IND_FLG ='Y'
AND B.RGN_CD IN (SELECT RGN_CD  FROM MDM_LOCATION WHERE LOC_CD = @[del_cd])
),
CTRT AS(
SELECT DISTINCT 'R' CTRT_TP, RM.PROP_NO, RM.AMDT_SEQ, RS.SVC_SCP_CD,
        TO_CHAR(RS.EFF_DT, 'YYYY-MM-DD')            AS FROM_DT  ,
                TO_CHAR(RS.EXP_DT, 'YYYY-MM-DD')            AS TO_DT
FROM  PRI_RP_MN RM, PRI_RP_SCP_MN RS, PRI_RP_HDR RH
WHERE RH.RFA_NO = @[rfa_no]
AND RM.PROP_NO = RH.PROP_NO
AND RM.PROP_NO = RS.PROP_NO
AND RM.AMDT_SEQ = RS.AMDT_SEQ
AND RM.EFF_DT <= NVL(TO_DATE(@[app_dt], 'YYYY-MM-DD'), sysdate)
AND RM.EXP_DT >= NVL(TO_DATE(@[app_dt], 'YYYY-MM-DD'), sysdate)
AND RM.PROP_STS_CD = 'A'
AND RS.SVC_SCP_CD IN (SELECT SVC_SCP_CD FROM SCP)
UNION ALL
SELECT DISTINCT 'S' CTRT_TP, SM.PROP_NO, SM.AMDT_SEQ, SS.SVC_SCP_CD,
        TO_CHAR(SS.EFF_DT, 'YYYY-MM-DD')            AS FROM_DT  ,
         TO_CHAR(SS.EXP_DT, 'YYYY-MM-DD')            AS TO_DT
FROM PRI_SP_HDR SH, PRI_SP_MN SM, PRI_SP_SCP_MN SS
WHERE SH.SC_NO = @[sc_no]
AND SH.PROP_NO = SM.PROP_NO
AND SM.PROP_NO = SS.PROP_NO
AND SM.AMDT_SEQ = SS.AMDT_SEQ
AND SM.PROP_STS_CD  = 'F'
AND SM.EFF_DT <= NVL(TO_DATE(@[app_dt], 'YYYY-MM-DD'), sysdate)
AND SM.EXP_DT >= NVL(TO_DATE(@[app_dt], 'YYYY-MM-DD'), sysdate)
AND SS.SVC_SCP_CD IN (SELECT SVC_SCP_CD FROM SCP)
),
CUST AS 
(
-- RFA
SELECT CUST.CUST_CNT_CD, CUST.CUST_SEQ, CTRT.FROM_DT, CTRT.TO_DT
FROM CTRT, PRI_RP_SCP_RT_CMDT RP_CMDT, MDM_COMMODITY REP_CMDT, PRI_RP_SCP_RT_ACT_CUST CUST
WHERE RP_CMDT.PRC_CMDT_DEF_CD = REP_CMDT.REP_CMDT_CD
AND RP_CMDT.PRC_CMDT_TP_CD  = 'R' --REP COMMODITY
AND RP_CMDT.PROP_NO         = CTRT.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CTRT.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = CTRT.SVC_SCP_CD
AND CTRT.CTRT_TP            = 'R'
AND REP_CMDT.CMDT_CD        = @[cmdt_cd]
AND RP_CMDT.PROP_NO         = CUST.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CUST.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = CUST.SVC_SCP_CD
AND RP_CMDT.CMDT_HDR_SEQ    = CUST.CMDT_HDR_SEQ
UNION
SELECT CUST.CUST_CNT_CD, CUST.CUST_SEQ, CTRT.FROM_DT, CTRT.TO_DT
FROM CTRT, PRI_RP_SCP_RT_CMDT RP_CMDT, PRI_RP_SCP_RT_ACT_CUST CUST
WHERE RP_CMDT.PRC_CMDT_TP_CD= 'C' --COMMODITY
AND RP_CMDT.PROP_NO         = CTRT.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CTRT.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = CTRT.SVC_SCP_CD
AND CTRT.CTRT_TP            = 'R'
AND RP_CMDT.PRC_CMDT_DEF_CD = @[cmdt_cd]
AND RP_CMDT.PRC_CMDT_TP_CD  = 'C' --COMMODITY
AND RP_CMDT.PROP_NO         = CUST.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CUST.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = CUST.SVC_SCP_CD
AND RP_CMDT.CMDT_HDR_SEQ    = CUST.CMDT_HDR_SEQ
UNION
SELECT CUST.CUST_CNT_CD, CUST.CUST_SEQ, CTRT.FROM_DT, CTRT.TO_DT
FROM CTRT, PRI_RP_SCP_RT_CMDT RP_CMDT, PRI_RP_SCP_GRP_CMDT_DTL GRP_CMDT, 
     MDM_COMMODITY REP_CMDT, PRI_RP_SCP_RT_ACT_CUST CUST
WHERE RP_CMDT.PROP_NO       = GRP_CMDT.PROP_NO
AND RP_CMDT.AMDT_SEQ        = GRP_CMDT.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = GRP_CMDT.SVC_SCP_CD
AND RP_CMDT.PRC_CMDT_TP_CD  = 'G' --GROUP COMMODITY
AND GRP_CMDT.PRC_CMDT_TP_CD = 'R' --REP COMMODITY
AND GRP_CMDT.PRC_CMDT_DEF_CD= REP_CMDT.REP_CMDT_CD
AND RP_CMDT.PROP_NO         = CTRT.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CTRT.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD = CTRT.SVC_SCP_CD
AND CTRT.CTRT_TP = 'R'
AND REP_CMDT.CMDT_CD        = @[cmdt_cd]
AND RP_CMDT.PROP_NO         = CUST.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CUST.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = CUST.SVC_SCP_CD
AND RP_CMDT.CMDT_HDR_SEQ    = CUST.CMDT_HDR_SEQ
UNION
SELECT CUST.CUST_CNT_CD, CUST.CUST_SEQ, CTRT.FROM_DT, CTRT.TO_DT
FROM CTRT, PRI_RP_SCP_RT_CMDT RP_CMDT, PRI_RP_SCP_GRP_CMDT_DTL GRP_CMDT, PRI_RP_SCP_RT_ACT_CUST CUST
WHERE RP_CMDT.PROP_NO       = GRP_CMDT.PROP_NO
AND RP_CMDT.AMDT_SEQ        = GRP_CMDT.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = GRP_CMDT.SVC_SCP_CD
AND GRP_CMDT.PRC_CMDT_DEF_CD= @[cmdt_cd]
AND RP_CMDT.PRC_CMDT_TP_CD  = 'G' --GROUP COMMODITY
AND GRP_CMDT.PRC_CMDT_TP_CD = 'C' --COMMODITY
AND RP_CMDT.PROP_NO         = CTRT.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CTRT.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = CTRT.SVC_SCP_CD
AND CTRT.CTRT_TP = 'R'
AND RP_CMDT.PROP_NO         = CUST.PROP_NO
AND RP_CMDT.AMDT_SEQ        = CUST.AMDT_SEQ
AND RP_CMDT.SVC_SCP_CD      = CUST.SVC_SCP_CD
AND RP_CMDT.CMDT_HDR_SEQ    = CUST.CMDT_HDR_SEQ

UNION ALL
-- SC
SELECT CUST.CUST_CNT_CD, CUST.CUST_SEQ, CTRT.FROM_DT, CTRT.TO_DT
FROM CTRT, PRI_SP_SCP_MN MN, PRI_SP_SCP_GRP_CMDT CMDT,  
     PRI_SP_SCP_GRP_CMDT_DTL DTL,  PRI_SP_SCP_RT_CMDT RTCM, PRI_SP_SCP_RT_ACT_CUST CUST
WHERE 1=1
AND CMDT.PROP_NO = CTRT.PROP_NO
AND CMDT.AMDT_SEQ = CTRT.AMDT_SEQ
AND CMDT.PROP_NO = MN.PROP_NO
AND CMDT.AMDT_SEQ = MN.AMDT_SEQ
AND CMDT.SVC_SCP_CD = MN.SVC_SCP_CD
AND CMDT.PROP_NO = DTL.PROP_NO
AND CMDT.AMDT_SEQ = DTL.AMDT_SEQ
AND CMDT.SVC_SCP_CD = DTL.SVC_SCP_CD
AND CMDT.GRP_CMDT_SEQ = DTL.GRP_CMDT_SEQ
AND DTL.PRC_CMDT_TP_CD = 'C'
AND DTL.PRC_CMDT_DEF_CD = @[cmdt_cd]
AND CTRT.CTRT_TP = 'S'
AND CMDT.PROP_NO = RTCM.PROP_NO
AND CMDT.AMDT_SEQ = RTCM.AMDT_SEQ
AND CMDT.SVC_SCP_CD = RTCM.SVC_SCP_CD
AND CMDT.PRC_GRP_CMDT_CD = RTCM.PRC_CMDT_DEF_CD
AND RTCM.PROP_NO = CUST.PROP_NO
AND RTCM.AMDT_SEQ = CUST.AMDT_SEQ
AND RTCM.SVC_SCP_CD = CUST.SVC_SCP_CD
AND RTCM.GEN_SPCL_RT_TP_CD = CUST.GEN_SPCL_RT_TP_CD
AND RTCM.CMDT_HDR_SEQ = CUST.CMDT_HDR_SEQ
UNION ALL
SELECT  CUST.CUST_CNT_CD, CUST.CUST_SEQ, CTRT.FROM_DT, CTRT.TO_DT
FROM CTRT, PRI_SP_SCP_RT_CMDT A, PRI_SP_SCP_RT_ACT_CUST CUST
WHERE 1=1
AND A.PROP_NO = CTRT.PROP_NO
AND A.AMDT_SEQ = CTRT.AMDT_SEQ
AND A.PRC_CMDT_TP_CD ='C'
AND A.PRC_CMDT_DEF_CD = @[cmdt_cd]
AND CTRT.CTRT_TP = 'S'
AND A.PROP_NO = CUST.PROP_NO
AND A.AMDT_SEQ = CUST.AMDT_SEQ
AND A.SVC_SCP_CD = CUST.SVC_SCP_CD
AND A.GEN_SPCL_RT_TP_CD = CUST.GEN_SPCL_RT_TP_CD
AND A.CMDT_HDR_SEQ = CUST.CMDT_HDR_SEQ
)
SELECT MDM.CUST_CNT_CD, MDM.CUST_SEQ, MDM.CUST_CNT_CD||MDM.CUST_SEQ AS CODE, MDM.CUST_LGL_ENG_NM, CUST.FROM_DT, CUST.TO_DT
FROM CUST, MDM_CUSTOMER MDM 
WHERE CUST.CUST_CNT_CD = MDM.CUST_CNT_CD
AND CUST.CUST_SEQ = MDM.CUST_SEQ


#else
SELECT CUST_CNT_CD,CUST_SEQ,CODE,CUST_LGL_ENG_NM,FROM_DT,TO_DT
FROM   (
        SELECT  AC.CUST_CNT_CD || AC.CUST_SEQ AS CODE     ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')            AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')            AS TO_DT    ,
                AC.CUST_CNT_CD  ,
                AC.CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = AC.CUST_CNT_CD AND A.CUST_SEQ = AC.CUST_SEQ )  CUST_LGL_ENG_NM,
                RANK() OVER (PARTITION BY SM.SVC_SCP_CD ORDER BY DECODE(BK.SVC_SCP_CD, SM.SVC_SCP_CD, 1, 2)) AS RN
        FROM    PRI_SP_HDR    HD  ,
                PRI_SP_MN     MN  ,
                PRI_SP_SCP_MN SM  ,
                PRI_SP_SCP_RT_ACT_CUST  AC  ,
                BKG_BOOKING   BK
        WHERE   1 = 1
        AND     HD.SC_NO        = @[sc_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='F'        /*상수*/ 
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        AND     SM.SVC_SCP_CD   IN ( BK.SVC_SCP_CD, DECODE(BK.SVC_SCP_CD, 'ACE', 'TPE', 'MXE', 'TPE') )
        AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT
        AND     AC.PROP_NO      = SM.PROP_NO
        AND     AC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     AC.SVC_SCP_CD   = SM.SVC_SCP_CD
        AND     BK.BKG_NO       = @[bkg_no]
        UNION
        SELECT  NC.BKG_ACT_CUST_CNT_CD || NC.BKG_ACT_CUST_SEQ AS CODE ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')  AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')  AS TO_DT    ,
                NC.BKG_ACT_CUST_CNT_CD            AS CUST_CNT_CD  ,
                NC.BKG_ACT_CUST_SEQ               AS CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = NC.BKG_ACT_CUST_CNT_CD AND A.CUST_SEQ = NC.BKG_ACT_CUST_SEQ )  CUST_LGL_ENG_NM,
                RANK() OVER (PARTITION BY SM.SVC_SCP_CD ORDER BY DECODE(BK.SVC_SCP_CD, SM.SVC_SCP_CD, 1, 2)) AS RN
        FROM    PRI_SP_HDR        HD  ,
                PRI_SP_MN         MN  ,
                PRI_SP_SCP_MN     SM  ,
                PRI_SC_NOTE_CONV  NC  ,
                BKG_BOOKING       BK
        WHERE   1 = 1
        AND     HD.SC_NO        = @[sc_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='F'        /*상수*/
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        AND     SM.SVC_SCP_CD   IN ( BK.SVC_SCP_CD, DECODE(BK.SVC_SCP_CD, 'ACE', 'TPE', 'MXE', 'TPE') )
        AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT
        AND     NC.PROP_NO      = SM.PROP_NO
        AND     NC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     NC.SVC_SCP_CD   = SM.SVC_SCP_CD
        AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN NC.EFF_DT AND NC.EXP_DT
        AND     NC.BKG_ACT_CUST_CNT_CD IS NOT NULL
        AND     BK.BKG_NO       = @[bkg_no]
       )
 WHERE RN = 1  
/*CUSTOMER Name*/
#if (${cust_lgl_eng_nm} != '') 
AND UPPER(CUST_LGL_ENG_NM) LIKE '%'||UPPER(@[cust_lgl_eng_nm])||'%'
#end
 
#if (${sc_no} == '') 
AND 'Y' = 'N'/*SWITCH*/
#end

UNION ALL

SELECT CUST_CNT_CD,CUST_SEQ,CODE,CUST_LGL_ENG_NM,FROM_DT,TO_DT
FROM   (
        SELECT  AC.CUST_CNT_CD || AC.CUST_SEQ AS CODE     ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')            AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')            AS TO_DT    ,
                AC.CUST_CNT_CD  ,
                AC.CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = AC.CUST_CNT_CD AND A.CUST_SEQ = AC.CUST_SEQ )  CUST_LGL_ENG_NM
        FROM    PRI_RP_HDR    HD  ,
                PRI_RP_MN     MN  ,
                PRI_RP_SCP_MN SM  ,
                PRI_RP_SCP_RT_ACT_CUST  AC  ,
                BKG_BOOKING   BK
        WHERE   1 = 1
        AND     HD.RFA_NO        = @[rfa_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='A'        /*상수*/
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        AND     SM.SVC_SCP_CD   = BK.SVC_SCP_CD
        AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT
        AND     AC.PROP_NO      = SM.PROP_NO
        AND     AC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     AC.SVC_SCP_CD   = SM.SVC_SCP_CD
        AND     BK.BKG_NO       = @[bkg_no]
        UNION
        SELECT  NC.BKG_ACT_CUST_CNT_CD || NC.BKG_ACT_CUST_SEQ AS CODE ,
                TO_CHAR(SM.EFF_DT, 'YYYY-MM-DD')  AS FROM_DT  ,
                TO_CHAR(SM.EXP_DT, 'YYYY-MM-DD')  AS TO_DT    ,
                NC.BKG_ACT_CUST_CNT_CD            AS CUST_CNT_CD  ,
                NC.BKG_ACT_CUST_SEQ               AS CUST_SEQ     ,
                ( SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER A WHERE A.CUST_CNT_CD = NC.BKG_ACT_CUST_CNT_CD AND A.CUST_SEQ = NC.BKG_ACT_CUST_SEQ )  CUST_LGL_ENG_NM
        FROM    PRI_RP_HDR        HD  ,
                PRI_RP_MN         MN  ,
                PRI_RP_SCP_MN     SM  ,
                PRI_SC_NOTE_CONV  NC  ,
                BKG_BOOKING       BK
        WHERE   1 = 1
        AND     HD.RFA_NO        = @[rfa_no]
        AND     MN.PROP_NO      = HD.PROP_NO
        AND     MN.PROP_STS_CD  ='A'        /*상수*/
        AND     SM.PROP_NO      = MN.PROP_NO
        AND     SM.AMDT_SEQ     = MN.AMDT_SEQ
        AND     SM.SVC_SCP_CD   = BK.SVC_SCP_CD
        AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN SM.EFF_DT AND SM.EXP_DT
        AND     NC.PROP_NO      = SM.PROP_NO
        AND     NC.AMDT_SEQ     = SM.AMDT_SEQ
        AND     NC.SVC_SCP_CD   = SM.SVC_SCP_CD
        AND     TO_DATE(@[app_dt], 'YYYY-MM-DD') BETWEEN NC.EFF_DT AND NC.EXP_DT
        AND     NC.BKG_ACT_CUST_CNT_CD IS NOT NULL
        AND     BK.BKG_NO       = @[bkg_no]
) A
WHERE  1=1
/*CUSTOMER Name*/
#if (${cust_lgl_eng_nm} != '') 
AND UPPER(CUST_LGL_ENG_NM) LIKE '%'||UPPER(@[cust_lgl_eng_nm])||'%'
#end

#if (${rfa_no} == '') 
AND 'Y' = 'N'/*SWITCH*/
#end

#end			]]></sql>
			<params>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="app_dt" type="12" value="2011-10-22" out="N"/>
				<param name="sc_no" type="12" value="AEN337337" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="SZP1F9822100" out="N"/>
				<param name="cust_lgl_eng_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
