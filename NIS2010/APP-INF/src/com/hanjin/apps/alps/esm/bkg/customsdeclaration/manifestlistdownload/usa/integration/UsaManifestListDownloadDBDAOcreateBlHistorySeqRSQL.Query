<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOcreateBlHistorySeqRSQL">
			<desc><![CDATA[createBlHistorySeq]]></desc>
			<sql><![CDATA[
SELECT  /*+ index_desc(A XPKBKG_CSTMS_IBD_HIS) */    
        NVL(MAX(HIS_SEQ), 0)+1 AS HIS_SEQ
FROM    BKG_CSTMS_IBD_HIS A
WHERE   1=1
AND	    CNT_CD = @[cnt_cd]  
AND     BL_NO = @[bl_no] 			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
