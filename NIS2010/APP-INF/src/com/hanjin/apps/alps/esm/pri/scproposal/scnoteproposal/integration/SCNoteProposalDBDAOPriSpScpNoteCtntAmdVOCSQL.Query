<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCNoteProposalDBDAOPriSpScpNoteCtntAmdVOCSQL">
			<desc><![CDATA[SCNoteProposalDBDAOPriSpScpNoteCtntAmdVOCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO pri_sp_scp_note_ctnt(
    PROP_NO,
    AMDT_SEQ,
    SVC_SCP_CD,
    NOTE_TP_CD,
    NOTE_SEQ,
    NOTE_CTNT_SEQ,
    CHG_CD,
    NOTE_CTNT,
    NOTE_CONV_FLG,
    DP_SEQ,
    NOTE_CONV_MAPG_ID,
    NOTE_CHG_TP_CD,
    PRC_PROG_STS_CD,
    SRC_INFO_CD,
    ACPT_USR_ID,
    ACPT_OFC_CD,
    ACPT_DT,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT,
	N1ST_CMNC_AMDT_SEQ
)
SELECT
    prop_no             ,
    amdt_seq+1          ,
    svc_scp_cd          ,
    note_tp_cd          ,
    note_seq            ,
    note_ctnt_seq       ,
    chg_cd              ,
    note_ctnt           ,
    note_conv_flg       ,
    dp_seq              ,
    DECODE(NOTE_CONV_MAPG_ID,'','', SYS_GUID())   ,
    note_chg_tp_cd      ,
    prc_prog_sts_cd     ,
    src_info_cd         ,
    ACPT_USR_ID		,
    ACPT_OFC_CD		,
    ACPT_DT			,
    @[cre_usr_id]       ,
    SYSDATE             ,
    @[upd_usr_id]       ,
    SYSDATE			,
	N1ST_CMNC_AMDT_SEQ
FROM 
    pri_sp_scp_note_ctnt
WHERE
    prop_no     = @[prop_no]
AND amdt_seq    = @[amdt_seq]
AND src_info_cd <> 'AD'			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
