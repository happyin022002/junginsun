<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpacecontrolinquiryDBDAOSearchTsPlanGuideDetailListRSQL">
			<desc><![CDATA[[T/S Plan & guide detail list]을 [조회]합니다.]]></desc>
			<sql><![CDATA[
SELECT
        PLN_SEQ
      , REP_TRD_CD
      , REP_SUB_TRD_CD
      , RLANE_CD
      , VVD_CD
      , IRR_PORT_CD
      , IRR_YD_CD
      , DIR_CD
      , IOC_CD
      , DECODE(CNTR_FULL_FLG, 'Y', 1, 0) CNTR_FULL_FLG
      , DECODE(CNTR_MTY_FLG, 'Y', 1, 0) CNTR_MTY_FLG
      , MLT_CRR_LIST_CTNT
      , MLT_POL_LIST_CTNT
      , TO_CHAR(N1ST_PORT_ETD_DT, 'YYYY-MM-DD') AS N1ST_PORT_ETD_DT
      , N1ST_RLANE_CD
      , N1ST_VVD_CD
      , TO_CHAR(N1ST_PORT_ETB_DT, 'YYYY-MM-DD') AS N1ST_PORT_ETB_DT
      , N1ST_POD_CD
      , N1ST_POD_YD_CD
      , N2ND_POL_CD
      , N2ND_POL_YD_CD
      , TO_CHAR(N2ND_PORT_ETD_DT, 'YYYY-MM-DD') AS N2ND_PORT_ETD_DT
      , N2ND_RLANE_CD
      , N2ND_VVD_CD
      , TO_CHAR(N2ND_PORT_ETB_DT, 'YYYY-MM-DD') AS N2ND_PORT_ETB_DT
      , N2ND_POD_CD
      , N2ND_POD_YD_CD
      , N3RD_POL_CD
      , N3RD_POL_YD_CD
      , TO_CHAR(N3RD_PORT_ETD_DT, 'YYYY-MM-DD') AS N3RD_PORT_ETD_DT
      , N3RD_RLANE_CD
      , N3RD_VVD_CD
      , TO_CHAR(N3RD_PORT_ETB_DT, 'YYYY-MM-DD') AS N3RD_PORT_ETB_DT
      , N3RD_POD_CD
      , N3RD_POD_YD_CD
      , N4TH_POL_CD
      , N4TH_POL_YD_CD
      , TO_CHAR(N4TH_PORT_ETD_DT, 'YYYY-MM-DD') AS N4TH_PORT_ETD_DT
      , N4TH_RLANE_CD
      , N4TH_VVD_CD
      , TO_CHAR(N4TH_PORT_ETB_DT, 'YYYY-MM-DD') AS N4TH_PORT_ETB_DT
      , N4TH_POD_CD
      , N4TH_POD_YD_CD
      , N5TH_POL_CD
      , N5TH_POL_YD_CD
      , TO_CHAR(N5TH_PORT_ETD_DT, 'YYYY-MM-DD') AS N5TH_PORT_ETD_DT
      , N5TH_RLANE_CD
      , N5TH_VVD_CD
      , TO_CHAR(N5TH_PORT_ETB_DT, 'YYYY-MM-DD') AS N5TH_PORT_ETB_DT
      , MLT_POD_LIST_CTNT
      , TS_PLN_CFM_STS_CD
      , TS_RMK
      , DECODE((SELECT COUNT(*) 
         FROM SPC_TS_PLN_GID_FILE_STO STO
        WHERE STO.REP_TRD_CD        = DTL.REP_TRD_CD
          AND STO.REP_SUB_TRD_CD    = DTL.REP_SUB_TRD_CD
          AND STO.RLANE_CD          = DTL.RLANE_CD
          AND STO.VVD_CD            = DTL.VVD_CD
          AND STO.IRR_PORT_CD       = DTL.IRR_PORT_CD
          AND STO.IRR_YD_CD         = DTL.IRR_YD_CD
          AND STO.PLN_SEQ           = DTL.PLN_SEQ
       ),0,'N','Y') AS PLN_ATCH
      , UPD_USR_ID
      , TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MM') AS UPD_DT 
FROM  SPC_TS_PLN_GID_DTL DTL
WHERE REP_TRD_CD     = @[rep_trd_cd]
AND   REP_SUB_TRD_CD = @[rep_sub_trd_cd]
AND   RLANE_CD       = @[rlane_cd]
AND   VVD_CD         = @[vvd_cd]
AND   IRR_PORT_CD    = @[irr_port_cd]
AND   IRR_YD_CD      = @[irr_yd_cd]
#if (${ts_pln_cfm_sts_cd} != 'All') 
AND   TS_PLN_CFM_STS_CD = @[ts_pln_cfm_sts_cd]
#end			]]></sql>
			<params>
				<param name="rep_trd_cd" type="12" value="" out="N"/>
				<param name="rep_sub_trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="irr_port_cd" type="12" value="" out="N"/>
				<param name="irr_yd_cd" type="12" value="" out="N"/>
				<param name="ts_pln_cfm_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
