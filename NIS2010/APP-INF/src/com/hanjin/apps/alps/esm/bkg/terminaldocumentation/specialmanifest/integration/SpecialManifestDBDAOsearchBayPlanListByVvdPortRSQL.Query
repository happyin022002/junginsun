<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialManifestDBDAOsearchBayPlanListByVvdPortRSQL">
			<desc><![CDATA[EDI로 수신한 Bay Plan 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
     ETA_DT
    ,ETD_DT
    ,BAY_PLN_ID
    ,TO_CHAR(RCV_DT, 'YYYY-MM-DD HH24:MI') RCV_DT
    ,VSL_CD VSL
    ,VSL_CD || VSL_VOY_DIR_NO TRNK_VVD_ID
    ,EUR_DG_DEP_PORT_CD POL_CD
    ,EUR_DG_NXT_PORT_CD POD_CD
    ,UPD_USR_ID
FROM BKG_CSTMS_EUR_DG_BAY_PLN
ORDER BY RCV_DT DESC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
