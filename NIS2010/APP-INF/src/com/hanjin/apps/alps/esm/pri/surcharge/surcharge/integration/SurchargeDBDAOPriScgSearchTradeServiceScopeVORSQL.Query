<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SurchargeDBDAOPriScgSearchTradeServiceScopeVORSQL">
			<desc><![CDATA[Trade 별 Service Scope 검색]]></desc>
			<sql><![CDATA[
SELECT A.SVC_SCP_CD AS CD ,
	   B.SVC_SCP_NM AS NM
  FROM PRI_SCG_TRD_SVC_SCP_MAPG A , MDM_SVC_SCP B
 WHERE A.SVC_SCP_CD = B.SVC_SCP_CD
   AND TRD_CD = @[trd_cd]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
