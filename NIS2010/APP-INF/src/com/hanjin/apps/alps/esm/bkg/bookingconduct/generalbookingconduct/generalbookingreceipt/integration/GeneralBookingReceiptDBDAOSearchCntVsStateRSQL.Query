<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOSearchCntVsStateRSQL">
			<desc><![CDATA[입력된 country code와 state code로 mdm_location을 확인]]></desc>
			<sql><![CDATA[
select count(1) cnt 
from mdm_state
where cnt_cd = @[cnt_cd]
and ste_cd = @[ste_cd]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
