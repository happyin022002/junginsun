<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Jp24ManifestListDownloadDBDAOGetAdvJpVslByCallsignRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_SS(SKD XPKBKG_CSTMS_ADV_JP_VSL_SKD) */
       SKD.VSL_CD,
       SKD.JO_CD1 AS RLX_DIV,
       (SELECT A.POL_CD
          FROM BKG_CSTMS_ADV_JP_SND_LOG A
         WHERE A.JP_SND_LOG_ID = 'ATD'
           AND A.VSL_CD = SKD.VSL_CD
           AND A.SKD_VOY_NO = @[skd_voy_no]
           AND A.SKD_DIR_CD = @[skd_dir_cd]
           AND A.SND_DT = (SELECT MAX(SND_DT)
                             FROM BKG_CSTMS_ADV_JP_SND_LOG SND
                            WHERE SND.JP_SND_LOG_ID = 'ATD'
                              AND SND.VSL_CD = SKD.VSL_CD
                              AND SND.SKD_VOY_NO = @[skd_voy_no]
                              AND SND.SKD_DIR_CD = @[skd_dir_cd])) AS POL_CD

  FROM BKG_CSTMS_ADV_JP_VSL_SKD SKD
 
 WHERE SKD.CALL_SGN_NO = @[call_sgn_no]
   AND SKD.SKD_VOY_NO = @[skd_voy_no]
   AND SKD.SKD_DIR_CD = @[skd_dir_cd]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="call_sgn_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
