<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VIPDeductAgreementDBDAOModifyVIPAgreementUSQL">
			<desc><![CDATA[VIP deduct agreement 수정
2016.08.05 김상현 [CHM-201642499] ALPS > ACM VIP Agreement Creation 상 SC/RFA No. 추가 요청]]></desc>
			<sql><![CDATA[
UPDATE ACM_VIP_AGMT
SET TRD_CD = @[trd_cd],
  DIR_CD = @[dir_cd],
  HUL_BND_CD = @[hul_bnd_cd],
  SUB_TRD_CD = @[sub_trd_cd],
  POR_GRP_TP_CD = @[por_grp_tp_cd],
  POR_ROUT_CD = @[por_rout_cd],
  POL_GRP_TP_CD = @[pol_grp_tp_cd],
  POL_ROUT_CD = @[pol_rout_cd],
  POD_GRP_TP_CD = @[pod_grp_tp_cd],
  POD_ROUT_CD = @[pod_rout_cd],
  DEL_GRP_TP_CD = @[del_grp_tp_cd],
  DEL_ROUT_CD = @[del_rout_cd],
  VIP_BKG_RT = @[vip_bkg_rt],
  SC_NO = @[sc_no],
  RFA_NO = @[rfa_no],
  TAA_NO = @[taa_no],
  FM_EFF_DT = @[fm_eff_dt],
  TO_EFF_DT = @[to_eff_dt],
  UPD_USR_ID = @[upd_usr_id],
  UPD_DT = SYSDATE
WHERE CUST_GRP_ID = @[cust_grp_id]
  AND AGMT_SEQ = @[agmt_seq]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="hul_bnd_cd" type="12" value="" out="N"/>
				<param name="sub_trd_cd" type="12" value="" out="N"/>
				<param name="por_grp_tp_cd" type="12" value="" out="N"/>
				<param name="por_rout_cd" type="12" value="" out="N"/>
				<param name="pol_grp_tp_cd" type="12" value="" out="N"/>
				<param name="pol_rout_cd" type="12" value="" out="N"/>
				<param name="pod_grp_tp_cd" type="12" value="" out="N"/>
				<param name="pod_rout_cd" type="12" value="" out="N"/>
				<param name="del_grp_tp_cd" type="12" value="" out="N"/>
				<param name="del_rout_cd" type="12" value="" out="N"/>
				<param name="vip_bkg_rt" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="taa_no" type="12" value="" out="N"/>
				<param name="fm_eff_dt" type="12" value="" out="N"/>
				<param name="to_eff_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cust_grp_id" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
