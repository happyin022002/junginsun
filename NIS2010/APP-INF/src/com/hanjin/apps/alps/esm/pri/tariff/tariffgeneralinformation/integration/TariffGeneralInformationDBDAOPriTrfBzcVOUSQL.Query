<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffGeneralInformationDBDAOPriTrfBzcVOUSQL">
			<desc><![CDATA[Tariff Basic 정보를 Update 한다.]]></desc>
			<sql><![CDATA[
UPDATE  PRI_TRF_BZC
   SET  EFF_DT            = TO_DATE(@[eff_dt], 'YYYYMMDD')
       ,EXP_DT            = TO_DATE(@[exp_dt], 'YYYYMMDD')
       ,TRF_BZC_TP_CD     = @[trf_bzc_tp_cd]    
       ,TRF_BZC_WGT       = @[trf_bzc_wgt]      
       ,TRF_BZC_WGT_UT_CD = @[trf_bzc_wgt_ut_cd]
       ,TRF_BZC_VOL_QTY   = @[trf_bzc_vol_qty]  
       ,TRF_BZC_VOL_UT_CD = @[trf_bzc_vol_ut_cd]
       ,CURR_CD           = @[curr_cd]          
       ,PUB_CNTC_PSON_NM  = @[pub_cntc_pson_nm] 
       ,PUB_OFC_ADDR      = @[pub_ofc_addr]     
       ,PUB_OFC_CTY_NM    = @[pub_ofc_cty_nm]   
       ,PUB_OFC_STE_CD    = @[pub_ofc_ste_cd]   
       ,PUB_OFC_ZIP_CD    = @[pub_ofc_zip_cd]   
       ,PUB_OFC_CNT_NM    = @[pub_ofc_cnt_nm]   
       ,PUB_OFC_PHN_NO    = @[pub_ofc_phn_no]   
       ,PUB_OFC_FAX_NO    = @[pub_ofc_fax_no]      
       ,APRO_OFC_CD       = @[apro_ofc_cd]       
       ,UPD_USR_ID        = @[upd_usr_id]       
       ,UPD_DT            = SYSDATE
 WHERE  1 = 1
   AND  TRF_PFX_CD = @[trf_pfx_cd]
   AND  TRF_NO     = @[trf_no]
   AND  AMDT_SEQ   = @[amdt_seq]			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="trf_bzc_tp_cd" type="12" value="" out="N"/>
				<param name="trf_bzc_wgt" type="12" value="" out="N"/>
				<param name="trf_bzc_wgt_ut_cd" type="12" value="" out="N"/>
				<param name="trf_bzc_vol_qty" type="12" value="" out="N"/>
				<param name="trf_bzc_vol_ut_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="pub_cntc_pson_nm" type="12" value="" out="N"/>
				<param name="pub_ofc_addr" type="12" value="" out="N"/>
				<param name="pub_ofc_cty_nm" type="12" value="" out="N"/>
				<param name="pub_ofc_ste_cd" type="12" value="" out="N"/>
				<param name="pub_ofc_zip_cd" type="12" value="" out="N"/>
				<param name="pub_ofc_cnt_nm" type="12" value="" out="N"/>
				<param name="pub_ofc_phn_no" type="12" value="" out="N"/>
				<param name="pub_ofc_fax_no" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
