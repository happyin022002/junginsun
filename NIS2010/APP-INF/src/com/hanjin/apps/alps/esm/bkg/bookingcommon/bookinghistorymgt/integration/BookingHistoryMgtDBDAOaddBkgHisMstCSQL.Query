<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOaddBkgHisMstCSQL">
			<desc><![CDATA[Booking History Master 를 기록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO BKG_HIS_MST ( 
	BKG_NO
,	HIS_SEQ
,	BKG_HIS_ISS_UI_ID
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,   CORR_NO
, 	EVNT_DT
, 	CRE_OFC_CD
) VALUES( 
	@[bkg_no]
,	@[his_seq]
,	@[bkg_his_iss_ui_id]
,	NVL(@[cre_usr_id], 'SYSTEM')
,	SYSDATE
,	NVL(@[upd_usr_id], 'SYSTEM')
,	SYSDATE
,   @[corr_no] 
, 	DECODE(@[cre_usr_id],'WEB',GLOBALDATE_PKG.TIME_LOCAL_FNC(@[por_cd]),
                         'HOMEPAGE', GLOBALDATE_PKG.TIME_LOCAL_FNC((SELECT POR_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no])),
                         'BATCH', GLOBALDATE_PKG.TIME_LOCAL_FNC((SELECT POR_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no])),
						 'VGM MVMT', GLOBALDATE_PKG.TIME_LOCAL_FNC((SELECT POR_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no])),
						 'VGM TRS', GLOBALDATE_PKG.TIME_LOCAL_FNC((SELECT POR_CD FROM BKG_BOOKING WHERE BKG_NO = @[bkg_no])), 
                         GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,BKG_COM_USER_LOC_FNC(@[cre_usr_id])))
,   (SELECT LST_LGIN_OFC_CD FROM COM_USER WHERE UPPER(USR_ID) = UPPER(@[cre_usr_id]) AND USE_FLG = 'Y' AND ROWNUM = 1)
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="his_seq" type="2" value="" out="N"/>
				<param name="bkg_his_iss_ui_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="corr_no" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
