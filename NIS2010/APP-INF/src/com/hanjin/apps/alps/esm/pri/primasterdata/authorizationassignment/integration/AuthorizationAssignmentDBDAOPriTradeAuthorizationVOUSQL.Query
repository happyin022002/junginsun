<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AuthorizationAssignmentDBDAOPriTradeAuthorizationVOUSQL">
			<desc><![CDATA[Trade Authorization 생성]]></desc>
			<sql><![CDATA[
UPDATE PRI_AUTHORIZATION
SET EFF_DT = TO_DATE (@[eff_dt], 'YYYYMMDD')
  , EXP_DT = TO_DATE ('99991231', 'YYYYMMDD')
  , UPD_USR_ID = @[cre_usr_id]
  , UPD_DT = SYSDATE
WHERE PRC_CTRT_TP_CD = @[prc_ctrt_tp_cd] 
AND   TRD_CD = @[trd_cd] 
AND   USR_ID =  @[usr_id]
			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="prc_ctrt_tp_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
