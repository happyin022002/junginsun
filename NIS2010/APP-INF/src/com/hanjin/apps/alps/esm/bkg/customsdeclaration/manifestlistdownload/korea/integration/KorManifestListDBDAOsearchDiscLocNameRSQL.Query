<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KorManifestListDBDAOsearchDiscLocNameRSQL">
			<desc><![CDATA[Discharging Location Name 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(LOC_NM,' ') LOC_NM
  FROM BKG_DCHG_LOC
 WHERE OTR_DCHG_CD = @[otr_dchg_cd]
   AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="otr_dchg_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
