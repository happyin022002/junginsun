<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CODCorrectionDBDAOmodifyCodRsoUSQL">
			<desc><![CDATA[bkg_no, cod_rqst_seq에 맞는 bkg_cod에 rgn_code를 update한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_COD SET 
	RGN_CD = @[rgn_cd]
,	UPD_USR_ID = @[usr_id]
,	UPD_DT = sysdate
WHERE	BKG_NO = @[bkg_no]
AND	COD_RQST_SEQ = @[cod_rqst_seq]			]]></sql>
			<params>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cod_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
