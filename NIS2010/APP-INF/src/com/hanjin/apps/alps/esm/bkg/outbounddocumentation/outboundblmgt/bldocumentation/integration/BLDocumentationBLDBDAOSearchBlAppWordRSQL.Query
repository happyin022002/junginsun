<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLDocumentationBLDBDAOSearchBlAppWordRSQL">
			<desc><![CDATA[BL에 특정 문구를 삽입하기 위해
POD_CD, SLAN_CD, SKD_DIR)CD, SVC_SCP_CD를 조회한다]]></desc>
			<sql><![CDATA[
SELECT BK.POD_CD
	 , BK.SLAN_CD
	 , BK.SKD_DIR_CD
	 , BK.SVC_SCP_CD
  FROM BKG_BOOKING  BK
 WHERE 1=1 
   AND BK.BKG_NO =@[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
