<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AusCustomsTransmissionDBDAOsearchContainerDescRSQL">
			<desc><![CDATA[호주세관 및 항만청으로 전송할 Manifest Container Description 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   
		'' d_cmdt,
		NVL(A.PCK_TP_CD,'') d_punit,
		NVL(A.PCK_QTY,0) d_pkg,
		DECODE(NVL(A.WGT_UT_CD,''),'LBS',ROUND(NVL(A.CNTR_MF_WGT,0)*0.4536,3),NVL(A.CNTR_MF_WGT,0)) d_wgt,
		DECODE(NVL(A.MEAS_UT_CD,''),'CBF',ROUND(NVL(A.MEAS_QTY,0)*0.0283,3),NVL(A.MEAS_QTY,0)) d_meas,
		TRANSLATE(NVL(B.CSTMS_DESC,' '),CHR(13)||CHR(10),' ') d_desc,
		DECODE(A.CNTR_MF_MK_DESC,NULL,'',
		REPLACE(A.CNTR_MF_MK_DESC,CHR(13)||CHR(10),CHR(10)))  d_mark
FROM  BKG_CNTR_MF_DESC A, BKG_BL_DOC B
WHERE	A.BKG_NO        = @[bkg_no] 
AND	A.BKG_NO        = B.BKG_NO 
AND	A.CNTR_NO       = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
