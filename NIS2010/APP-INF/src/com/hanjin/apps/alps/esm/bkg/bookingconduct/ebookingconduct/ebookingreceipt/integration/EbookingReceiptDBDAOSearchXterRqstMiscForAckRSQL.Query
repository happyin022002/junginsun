<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EbookingReceiptDBDAOSearchXterRqstMiscForAckRSQL">
			<desc><![CDATA[auto confirm edi 중 misc 정보를 조회한다.]]></desc>
			<sql><![CDATA[
select  'IB_PLANT_CD:'          ||FCTRY_CTNT	     ||CHR(10)||
        'IB_PLANT_NM:'          ||FCTRY_NM			 ||CHR(10)||
        'IB_DIV_CD:'            ||DIV_CTNT		     ||CHR(10)||
        'IB_DIV_NM:'            ||DIV_NM			 ||CHR(10)||
        'DOC_CRDATE_GMT:'       ||TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL',sysdate,'GMT' ),'RRRRMMDDHH24MISS')||CHR(10)||
        'IB_CCT_DT:'            ||CLZ_DT		     ||CHR(10)||
        'IB_BIZ_TP:'            ||BZTP_CTNT		     ||CHR(10)||
        'IB_BIZ_NM:'            ||BIZ_NM		     ||CHR(10)||
        'IB_TS_TP:'             ||TS_TP_CTNT		 ||CHR(10)||
        'IB_TS_NM:'             ||TS_NM		         ||CHR(10)||
        'IBM_LINE_CHG_WGT:'     ||LINE_CHG_WGT	     ||CHR(10)||
        'IBM_LINE_CHG_WGT_CD:'  ||LINE_CHG_WGT_UT_CD ||CHR(10)||
        'IBM_EDN_CHG_QTY:'      ||GDS_CHG_PCK_QTY	 ||CHR(10)||
        'IBM_EDN_CHG_QTY_CD:'   ||GDS_CHG_PCK_TP_CTNT||CHR(10)||
        'IBM_EDN_CHG_WGT:'      ||GDS_CHG_WGT	     ||CHR(10)||
        'IBM_EDN_CHG_WGT_CD:'   ||GDS_CHG_WGT_UT_CD	 ||CHR(10)||
        'IBM_SUMCHG_WGT_QTY:'   ||DECODE(TTL_CHG_WGT,0,'',TTL_CHG_WGT)||CHR(10)||
        'IBM_SUMCHG_WGT_CD:'    ||TTL_CHG_WGT_UT_CD	 ||CHR(10)||
        'IBM_PKG_QTY:'          ||PCK_QTY		     ||CHR(10)||
        'IBM_PKG_QTY_CD:'       ||PCK_TP_CTNT		 ||CHR(10)||
        'IBM_VOL_QTY:'          ||VOL_QTY	      	 ||CHR(10)||
        'IBM_VOL_QTY_CD:'       ||VOL_TP_CTNT		 ||CHR(10) FLAT_FILE
  from BKG_XTER_RQST_MISC
 where XTER_SNDR_ID  = @[sender_id]
   and xter_rqst_no  = @[rqst_no]
   and xter_rqst_seq = @[rqst_seq]			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
