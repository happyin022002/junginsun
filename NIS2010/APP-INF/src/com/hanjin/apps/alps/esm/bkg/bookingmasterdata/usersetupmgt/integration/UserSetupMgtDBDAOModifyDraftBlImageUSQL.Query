<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserSetupMgtDBDAOModifyDraftBlImageUSQL">
			<desc><![CDATA[UserSetupMgtDBDAOModifyDraftBlImageUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_CUST_BL_IMG_STUP 
SET RAT_FLG = DECODE(@[rat_flg],1,'Y','N'),
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT = SYSDATE
WHERE CUST_CNT_CD = SUBSTR(@[cust_code],1,2) 
    AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_code],3,6))
    AND BL_IMG_FILE_TP_CD = @[bl_img_file_tp_cd]			]]></sql>
			<params>
				<param name="rat_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cust_code" type="12" value="" out="N"/>
				<param name="bl_img_file_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
