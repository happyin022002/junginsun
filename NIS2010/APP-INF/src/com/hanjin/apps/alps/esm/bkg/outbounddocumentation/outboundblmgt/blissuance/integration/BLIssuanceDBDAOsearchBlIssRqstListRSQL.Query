<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchBlIssRqstListRSQL">
			<desc><![CDATA[BL Issue 관련 Data를 조회 한다]]></desc>
			<sql><![CDATA[
SELECT WO_CCD2.CRE_DT
       , XTER_RQST_NO
       , XTER_RQST_SEQ
       , BKG_NO
       , BII_BL_NO                                                                                                       
       , BII_VSL_NM                                                                     
       , BII_VVD                     
       , BL_ON_DT                                                   
       , CUST_CNT_CD 
       , CUST_SEQ        
       , BL_ISS_RQST_DT
       , RQST_RCT_LOC_CD
       , BL_NO
       , BL_NO_TP 
       , DECODE(NVL(BL_ISS_ACT_DT,''),'',BL_ISS_RJCT_DT,BL_ISS_ACT_DT) BL_ISS_ACT_DT
       , CCD2.INTG_CD_VAL_DESC RQST_BL_TP_CD
       , WO_CCD2.INTG_CD_VAL_DESC BL_ISS_STS_CD
       , BL_ISS_RMK
       , RQST_CO_NM 
       , RQST_USR_EML
       , RQST_ATND_NM
       , RQST_PHN_NO
       , BL_RQST_RMK
       , ACT_SHPR_NM
       , ACT_SHPR_RGST_NO
       , TAX_INV_RCVR_CO_NM
       , TAX_INV_RCVR_RGST_NO
       , TAX_INV_RCVR_ATND_NM
       , TAX_INV_RCVR_PHN_NO
       , REMIT_CO_NM
       , REMIT_KND_CD
       , DELT_ID
       , OBL_ISS_FLG
       , VSL_CD
       , SKD_VOY_NO
       , SKD_DIR_CD
       , VVD
       , POL_CD
       , RN
       , VPS_ETD_DT
	   , DECODE(NVL(BL_ISS_RQST_CD,'W'),'W','Web','M','Mobile','Web') BL_ISS_RQST_CD
FROM (
    SELECT CCD1.INTG_CD_VAL_DESC
         , WO_CCD1.*
    FROM (
        SELECT SUB.* 
             , TO_CHAR(SKD.VPS_ETD_DT, 'YYYY-MM-DD HH24:MI') VPS_ETD_DT
        FROM (
                      SELECT EBL.CRE_DT
                           , EBL.XTER_RQST_NO
                           , EBL.XTER_RQST_SEQ
                           , B.BKG_NO
                           , B.BL_NO BII_BL_NO                                                                                                       
                           , MDM.VSL_ENG_NM BII_VSL_NM                                                                     
                           , V.SKD_VOY_NO||V.SKD_DIR_CD BII_VVD                     
                           , TO_CHAR(BBD.BL_OBRD_DT, 'YYYY-MM-DD') BL_ON_DT                                                   
                           , C.CUST_CNT_CD 
                           , C.CUST_SEQ        
                           , TO_CHAR(EBL.BL_ISS_RQST_DT, 'YYYY-MM-DD HH24:MI') BL_ISS_RQST_DT
                           , EBL.RQST_RCT_LOC_CD
                           , B.BL_NO
                           , B.BL_NO_TP 
                           , TO_CHAR(EBL.BL_ISS_ACT_DT, 'YYYY-MM-DD HH24:MI') BL_ISS_ACT_DT
                           , TO_CHAR(EBL.BL_ISS_RJCT_DT, 'YYYY-MM-DD HH24:MI') BL_ISS_RJCT_DT
                           , EBL.RQST_BL_TP_CD
                           , DECODE(B.BL_TP_CD,'W','A',EBL.BL_ISS_STS_CD) BL_ISS_STS_CD
                           , EBL.BL_ISS_RMK
                           , EBL.RQST_CO_NM 
                           , EBL.RQST_USR_EML
                           , EBL.RQST_ATND_NM
                           , EBL.RQST_PHN_NO
                           , EBL.BL_RQST_RMK
                           , EBL.ACT_SHPR_NM
                           , EBL.ACT_SHPR_RGST_NO
                           , EBL.TAX_INV_RCVR_CO_NM
                           , EBL.TAX_INV_RCVR_RGST_NO
                           , EBL.TAX_INV_RCVR_ATND_NM
                           , EBL.TAX_INV_RCVR_PHN_NO
                           , EBL.REMIT_CO_NM
                           , EBL.REMIT_KND_CD
                           , DECODE(EBL.DELT_FLG, 'Y',EBL.UPD_USR_ID,NULL) DELT_ID
                           , ISS.OBL_ISS_FLG
                           , V.VSL_CD
                           , V.SKD_VOY_NO
                           , V.SKD_DIR_CD
                           , V.VSL_CD||V.SKD_VOY_NO||V.SKD_DIR_CD VVD
                           , V.POL_CD
                           , DENSE_RANK() OVER (PARTITION BY V.BKG_NO ORDER BY V.VSL_PRE_PST_CD||V.VSL_SEQ) RN
						   , EBL.BL_ISS_RQST_CD
                      FROM  BKG_BOOKING B                                                                                
                           , BKG_CUSTOMER C 
                           , BKG_BL_DOC BBD 
                           , BKG_VVD V 
                           , MDM_VSL_CNTR MDM 
                           , BKG_BL_ISS ISS
                           , BKG_BL_ISS_RQST EBL
                           #if (${ofc_cd} != '') 
                           , COM_USER USR
                           #end
                      WHERE B.BKG_NO = C.BKG_NO
                      AND B.BKG_NO = BBD.BKG_NO
                      AND B.BKG_NO = V.BKG_NO
                      AND B.VSL_CD = MDM.VSL_CD
                      #if (${ofc_cd} != '') 
                      AND EBL.BL_ISS_USR_ID = USR.USR_ID
                      #end
                      AND B.BKG_NO = ISS.BKG_NO(+)
                      AND B.BL_NO = EBL.BL_NO(+)
                      AND B.BKG_STS_CD <> 'X'
                
                      #if (${rqst_from_dt} != '') 
                      AND EBL.BL_ISS_RQST_DT >= TO_DATE(@[rqst_from_dt], 'YYYY-MM-DD')
                      #end
                      #if (${rqst_to_dt} != '') 
                      AND EBL.BL_ISS_RQST_DT < TO_DATE(@[rqst_to_dt], 'YYYY-MM-DD') + 1
                      #end
                      #if (${bl_no} != '') 
                      AND EBL.BL_NO  = @[bl_no]
                      #end
                      #if (${rqst_rct_loc_cd} != '') 
                      AND EBL.RQST_RCT_LOC_CD = @[rqst_rct_loc_cd]
                      #end
                      #if (${rqst_bl_tp_cd} != 'All' && ${rqst_bl_tp_cd} != '') 
                      AND EBL.RQST_BL_TP_CD = @[rqst_bl_tp_cd]
                      #end
                      #if (${bl_iss_rqst_cd} != 'All' && ${bl_iss_rqst_cd} != '') 
                      AND EBL.BL_ISS_RQST_CD = @[bl_iss_rqst_cd]
                      #end
                      #if (${vvd} != '') 
                      AND EBL.VSL_CD = SUBSTR(@[vvd],1,4)
                      AND EBL.SKD_VOY_NO = SUBSTR(@[vvd],5,4)
                      AND EBL.SKD_DIR_CD = SUBSTR(@[vvd],9,1)
                      #end
                      #if (${bl_iss_sts_cd} != 'All' && ${bl_iss_sts_cd} != '') 
                      AND DECODE(B.BL_TP_CD,'W','A',EBL.BL_ISS_STS_CD) LIKE @[bl_iss_sts_cd]||'%'
                      #end
                      #if (${rqst_co_nm} != '') 
                      AND EBL.RQST_CO_NM LIKE '%'||@[rqst_co_nm]||'%'
                      #end
                      #if (${delt_flg} != '') 
                      AND EBL.DELT_FLG = @[delt_flg]
                      #end
                      #if (${pol_cd} != '') 
                      AND B.POL_CD = @[pol_cd]
                      #end
                      #if (${ofc_cd} != '') 
                      AND USR.OFC_CD = @[ofc_cd]
                      #end
                      AND C.BKG_CUST_TP_CD = 'S'
                  ) SUB
                    , VSK_VSL_PORT_SKD SKD
                  WHERE SUB.VSL_CD = SKD.VSL_CD
                    AND SUB.SKD_VOY_NO = SKD.SKD_VOY_NO
                    AND SUB.SKD_DIR_CD = SKD.SKD_DIR_CD
                    AND SUB.POL_CD = SKD.VPS_PORT_CD
                    AND SKD.CLPT_IND_SEQ = 1
                    AND SUB.RN = 1
        ) WO_CCD1
        , COM_INTG_CD_DTL CCD1
        WHERE CCD1.INTG_CD_ID = 'CD02808' 
        AND WO_CCD1.BL_ISS_STS_CD = CCD1.INTG_CD_VAL_CTNT
    ) WO_CCD2
    , COM_INTG_CD_DTL CCD2
WHERE CCD2.INTG_CD_ID = 'CD02807' AND
WO_CCD2.RQST_BL_TP_CD = CCD2.INTG_CD_VAL_CTNT
ORDER BY CRE_DT ASC			]]></sql>
			<params>
				<param name="rqst_from_dt" type="12" value="" out="N"/>
				<param name="rqst_to_dt" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="rqst_rct_loc_cd" type="12" value="" out="N"/>
				<param name="rqst_bl_tp_cd" type="12" value="" out="N"/>
				<param name="bl_iss_rqst_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="bl_iss_sts_cd" type="12" value="" out="N"/>
				<param name="rqst_co_nm" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
