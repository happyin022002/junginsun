<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOsearchOrgPartyListRSQL">
			<desc><![CDATA[SNP/Broker Nomination  조회 :BKG_CSTMS_ADV_ORZ_PTY]]></desc>
			<sql><![CDATA[
SELECT
  A.CUST_CNT_CD,
  A.CUST_SEQ,
  A.CUST_CNT_CD||A.CUST_SEQ CUST_CD,
  (SELECT M.CUST_LGL_ENG_NM FROM MDM_CUSTOMER M
   WHERE M.CUST_CNT_CD = A.CUST_CNT_CD
   AND   M.CUST_SEQ = A.CUST_SEQ )CUST_NM,
  A.SC_NO,
  A.POD_CD,
  A.POD_YD_NO,
  A.DEL_CD,
  A.DEL_YD_NO, 
  A.CSTMS_PTY_SEQ,
  A.CSTMS_PTY_TP_CD, 
  A.CSTMS_PTY_NM,
  A.CSTMS_PTY_ID, 
  A.DELT_FLG,
  A.CRE_OFC_CD,
  A.UPD_OFC_CD,
  A.CRE_USR_ID ,
  A.CRE_DT,
  A.UPD_USR_ID,
  A.UPD_DT
FROM BKG_CSTMS_ADV_ORZ_PTY A
WHERE 1=1 
AND A.DELT_FLG = 'N'
#if (${cust_cd} != '') 
AND  A.CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
#end
#if (${cust_cd} != '') 
AND  A.CUST_SEQ = SUBSTR(@[cust_cd],3,6)
#end
#if (${sc_no} != '') 
AND  A.SC_NO = @[sc_no]
#end
#if (${pod_cd} != '') 
AND DECODE( A.POD_CD,'ALL',@[pod_cd] , A.POD_CD) = @[pod_cd]
#end
#if (${del_cd} != '') 
AND DECODE( A.DEL_CD,'ALL',@[del_cd] , A.DEL_CD ) = @[del_cd]
#end
#if (${cre_ofc_cd} != '') 
AND  A.CRE_OFC_CD = @[cre_ofc_cd]
#end
ORDER BY A.CUST_CNT_CD, 
         A.CUST_SEQ,
         A.POD_CD, A.POD_YD_NO,
         A.DEL_CD, A.DEL_YD_NO,
         A.CSTMS_PTY_SEQ			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
