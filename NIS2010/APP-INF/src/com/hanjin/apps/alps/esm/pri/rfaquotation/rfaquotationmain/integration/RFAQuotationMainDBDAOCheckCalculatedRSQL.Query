<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAQuotationMainDBDAOCheckCalculatedRSQL">
			<desc><![CDATA[cal 했는지 체크]]></desc>
			<sql><![CDATA[
SELECT  COUNT(A1.QTTN_NO) AS CNT_CAL       
FROM    (       
        SELECT A.QTTN_NO AS QTTN_NO
        FROM   PRI_RQ_MN A
        ,      PRI_RQ_RT B
        WHERE  A.QTTN_NO = B.QTTN_NO
        AND    A.QTTN_VER_NO = B.QTTN_VER_NO
        AND    A.PRS_RT_CMPB_CALC_FLG = 'Y'
        AND    A.QTTN_NO = @[qttn_no] 
        AND    A.QTTN_VER_NO = @[qttn_ver_no] 
        ) A1			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
