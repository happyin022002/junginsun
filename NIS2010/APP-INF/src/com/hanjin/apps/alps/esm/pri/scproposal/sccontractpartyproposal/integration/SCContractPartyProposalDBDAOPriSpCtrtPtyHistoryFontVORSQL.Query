<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCContractPartyProposalDBDAOPriSpCtrtPtyHistoryFontVORSQL">
			<desc><![CDATA[Contract Party History Option font]]></desc>
			<sql><![CDATA[
SELECT PRC_CTRT_PTY_TP_CD CD
	  ,AMDT_FLG
	  ,ACPT_FLG
	,  CASE ACT_CNT
	   		WHEN 'Y' THEN '3'
			ELSE
				CASE AMDT_FLG||AMD_CNT
					WHEN 'YY' THEN DECODE(@[amdt_seq],'0','1','2')
                    ELSE	
						CASE AMD_CNT WHEN 'N' THEN '0'
                    	ELSE
                        	'1'
                    	END
				END
	  END ETC1
FROM   PRI_SP_AMDT_SMRY C
    , (SELECT '04' PROP_TERM_TP_CD
	  		  ,PRC_CTRT_PTY_TP_CD
             ,DECODE(N1ST_CMNC_AMDT_SEQ,DECODE((SELECT LGCY_IF_FLG FROM PRI_SP_MN TMP WHERE TMP.PROP_NO = @[prop_no] AND TMP.AMDT_SEQ = @[amdt_seq]), 'Y', N1ST_CMNC_AMDT_SEQ, @[amdt_seq]),'Y','N') AMD_CNT
			 ,DECODE(N1ST_CMNC_AMDT_SEQ,AMDT_SEQ,DECODE(PRC_PROG_STS_CD,'A','Y','N'),'N') ACT_CNT
       FROM   PRI_SP_CTRT_PTY
       WHERE  PROP_NO = @[prop_no]
       AND    AMDT_SEQ = @[amdt_seq]   
	   GROUP BY PRC_CTRT_PTY_TP_CD ,N1ST_CMNC_AMDT_SEQ,AMDT_SEQ,PRC_PROG_STS_CD                                                  
) D
WHERE  PROP_NO = @[prop_no]
AND    AMDT_SEQ = @[amdt_seq]
AND    C.PROP_TERM_TP_CD = D.PROP_TERM_TP_CD			]]></sql>
			<params>
				<param name="amdt_seq" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
