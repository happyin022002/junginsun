<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingHistoryMgtDBDAOsearchCstmDescChgHisRSQL">
			<desc><![CDATA[BookingHistoryMgtDBDAOsearchCstmDescChgHisRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_DESC_CNG_ITM_CD
       ,BKG_NO
  FROM 
   (
    SELECT '4' BKG_DESC_CNG_ITM_CD
            , TRIM(@[cstms_desc])     PRE_CTNT
            , TRIM(NOW_BL.CSTMS_DESC) CRNT_CTNT
            ,@[bkg_no] BKG_NO
#if (${ca_flg} == 'Y')
      FROM BKG_BL_DOC_HIS NOW_BL
     WHERE @[bkg_no]  = NOW_BL.BKG_NO    
	   AND NOW_BL.CORR_NO = 'TMP0000001' 
#else
      FROM BKG_BL_DOC NOW_BL
     WHERE @[bkg_no] = NOW_BL.BKG_NO    
#end
       )
 WHERE NVL(TRIM(PRE_CTNT), ' ') <> NVL(TRIM(CRNT_CTNT), ' ')			]]></sql>
			<params>
				<param name="cstms_desc" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
