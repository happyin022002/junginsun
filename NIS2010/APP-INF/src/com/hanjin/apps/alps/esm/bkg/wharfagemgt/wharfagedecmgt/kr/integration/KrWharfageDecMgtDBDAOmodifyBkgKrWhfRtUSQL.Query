<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KrWharfageDecMgtDBDAOmodifyBkgKrWhfRtUSQL">
			<desc><![CDATA[U]]></desc>
			<sql><![CDATA[
UPDATE BKG_KR_WHF_RT
   SET PORT_OFC_CD         = @[port_ofc_cd]
       ,CMDT_CD            = @[cmdt_cd]
       ,BL_CUST_NM         = @[bl_cust_nm]
       ,TAX_CUST_NM        = @[tax_cust_nm]
       ,KR_WHF_EXPT_CD     = @[kr_whf_expt_cd]
       ,DECL_RMK           = @[decl_rmk]
       ,TAX_TEU_QTY        = @[tax_teu_qty]
       ,TAX_FEU_QTY        = @[tax_feu_qty]
       ,TAX_45FT_QTY       = @[tax_45ft_qty]
       ,CNTR_TEU_QTY       = @[cntr_teu_qty]
       ,CNTR_FEU_QTY       = @[cntr_feu_qty]
       ,CNTR_45FT_QTY      = @[cntr_45ft_qty]
       ,CNTR_TPSZ_TEU_QTY  = @[cntr_tpsz_teu_qty]
       ,CNTR_TPSZ_FEU_QTY  = @[cntr_tpsz_feu_qty]
       ,CNTR_TPSZ_45FT_QTY = @[cntr_tpsz_45ft_qty]
       ,EXPT_TEU_QTY       = @[expt_teu_qty]
       ,EXPT_FEU_QTY       = @[expt_feu_qty]
       ,EXPT_45FT_QTY      = @[expt_45ft_qty]
       ,BLK_TEU_QTY        = @[blk_teu_qty]
       ,BLK_FEU_QTY        = @[blk_feu_qty]
       ,BLK_45FT_QTY       = @[blk_45ft_qty]
       ,BB_CGO_WGT         = @[bb_cgo_wgt]
       ,BLK_MEAS_QTY       = @[blk_meas_qty]
       ,KR_CSTMS_FRT_TP_CD = @[kr_cstms_frt_tp_cd]
       ,PORT_CD            = @[port_cd]
       ,UPD_USR_ID         = @[upd_usr_id]
       ,UPD_DT       = SYSDATE
 WHERE VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]
   AND WHF_BND_CD = @[whf_bnd_cd]
   AND BL_NO      = @[bl_no]
   AND CHG_RT_SEQ = (SELECT MAX(CHG_RT_SEQ) AS CHG_RT_SEQ
                     FROM BKG_KR_WHF_RT                  
                     WHERE 1=1                           
                     AND VSL_CD     = @[vsl_cd]          
                     AND SKD_VOY_NO = @[skd_voy_no]
                     AND SKD_DIR_CD = @[skd_dir_cd]              
                     AND WHF_BND_CD = @[whf_bnd_cd]
                     AND BL_NO      = @[bl_no])			]]></sql>
			<params>
				<param name="port_ofc_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="bl_cust_nm" type="12" value="" out="N"/>
				<param name="tax_cust_nm" type="12" value="" out="N"/>
				<param name="kr_whf_expt_cd" type="12" value="" out="N"/>
				<param name="decl_rmk" type="12" value="" out="N"/>
				<param name="tax_teu_qty" type="12" value="" out="N"/>
				<param name="tax_feu_qty" type="12" value="" out="N"/>
				<param name="tax_45ft_qty" type="12" value="" out="N"/>
				<param name="cntr_teu_qty" type="12" value="" out="N"/>
				<param name="cntr_feu_qty" type="12" value="" out="N"/>
				<param name="cntr_45ft_qty" type="12" value="" out="N"/>
				<param name="cntr_tpsz_teu_qty" type="12" value="" out="N"/>
				<param name="cntr_tpsz_feu_qty" type="12" value="" out="N"/>
				<param name="cntr_tpsz_45ft_qty" type="12" value="" out="N"/>
				<param name="expt_teu_qty" type="12" value="" out="N"/>
				<param name="expt_feu_qty" type="12" value="" out="N"/>
				<param name="expt_45ft_qty" type="12" value="" out="N"/>
				<param name="blk_teu_qty" type="12" value="" out="N"/>
				<param name="blk_feu_qty" type="12" value="" out="N"/>
				<param name="blk_45ft_qty" type="12" value="" out="N"/>
				<param name="bb_cgo_wgt" type="12" value="" out="N"/>
				<param name="blk_meas_qty" type="12" value="" out="N"/>
				<param name="kr_cstms_frt_tp_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="whf_bnd_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
