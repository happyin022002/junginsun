<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoReleaseOrderDBDAOSearchDoInfoRSQL">
			<desc><![CDATA[DO No에 해당하는 Bkg No 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_NO
     , RLSE_SEQ  
  FROM BKG_DO
 WHERE DO_NO        = SUBSTR(@[do_no],1,10)      
   AND NVL(DO_NO_SPLIT, '00') = NVL(SUBSTR(@[do_no],11,2), '00')			]]></sql>
			<params>
				<param name="do_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
