<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaManifestListDownloadDBDAOmodifyAdvancedCustomerByBlUSQL">
			<desc><![CDATA[modifyAdvancedCustomerByBl]]></desc>
			<sql><![CDATA[
UPDATE BKG_CSTMS_ADV_CUST
SET	CUST_CNT_CD = @[cust_cnt_cd]
,	CUST_NM = @[cust_nm]
,	CUST_SEQ = @[cust_seq]
,	CUST_ADDR = @[cust_addr]
,	UPD_USR_ID = @[upd_usr_id]
WHERE  1=1
AND	   CNT_CD = @[cnt_cd]  
AND    BL_NO = @[bl_no] 
AND    BKG_CUST_TP_CD = @[bkg_cust_tp_cd]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_addr" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
