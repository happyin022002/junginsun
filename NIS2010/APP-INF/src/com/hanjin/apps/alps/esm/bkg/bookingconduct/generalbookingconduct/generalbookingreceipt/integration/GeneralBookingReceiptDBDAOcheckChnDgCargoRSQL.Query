<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOcheckChnDgCargoRSQL">
			<desc><![CDATA[중국 상해 I/B DG화물 관련 MSDS 경고팝업 추가에 따른 SQL문.]]></desc>
			<sql><![CDATA[
SELECT DECODE( SUM(CNT), 0, 'N', 'Y') AS valFlg
FROM (
    SELECT COUNT('X') CNT
    FROM BKG_BOOKING
    WHERE BKG_NO = @[bkg_no]
      AND ( SUBSTR(POR_CD, 1, 2) <> 'CN'
          or POD_CD ='CNSHA' )
    UNION ALL
    SELECT COUNT('X')
    FROM BKG_VVD
    WHERE BKG_NO = @[bkg_no]
      AND POD_CD ='CNSHA' )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
