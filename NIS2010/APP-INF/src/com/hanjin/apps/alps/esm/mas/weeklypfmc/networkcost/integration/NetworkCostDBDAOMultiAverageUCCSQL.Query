<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkCostDBDAOMultiAverageUCCSQL">
			<desc><![CDATA[Average U/C(OP fixed/variable cost, SPC CHT Rev/Charterage)]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_OP_AVG_UT_COST A 
  USING (SELECT @[cost_yrmon] COST_YRMON 
            ,@[trd_cd] TRD_CD 
            ,@[rlane_cd] RLANE_CD             
            ,@[stnd_cost_cd] STND_COST_CD 
            ,@[op_uc_amt] OP_UC_AMT                        
            ,@[cre_usr_id] CRE_USR_ID 
            ,SYSDATE CRE_DT 
            ,@[cre_usr_id] UPD_USR_ID
            ,SYSDATE UPD_DT 
			,'A' COST_USE_TP_CD
			,'E' DIR_CD
          FROM DUAL) B 
  ON (A.COST_YRMON = B.COST_YRMON 
  	AND A.TRD_CD = B.TRD_CD
  	AND A.RLANE_CD = B.RLANE_CD
    AND A.STND_COST_CD = B.STND_COST_CD
    AND A.COST_USE_TP_CD = B.COST_USE_TP_CD
    AND A.DIR_CD = B.DIR_CD )  
  WHEN MATCHED THEN 
    UPDATE 
      SET A.OP_UC_AMT = B.OP_UC_AMT
        , A.UPD_USR_ID = B.UPD_USR_ID
        , A.UPD_DT = B.UPD_DT
                
  WHEN NOT MATCHED THEN 
    INSERT (A.COST_YRMON
          , A.TRD_CD
          , A.RLANE_CD
          , A.STND_COST_CD
          , A.OP_UC_AMT
          , A.CRE_USR_ID
          , A.CRE_DT
          , A.UPD_USR_ID
          , A.UPD_DT
		  , A.COST_USE_TP_CD
		  , A.DIR_CD ) 
    VALUES (B.COST_YRMON
          , B.TRD_CD
          , B.RLANE_CD
          , B.STND_COST_CD
          , B.OP_UC_AMT
          , B.CRE_USR_ID
          , B.CRE_DT
          , B.UPD_USR_ID
          , B.UPD_DT
		  , B.COST_USE_TP_CD
		  , B.DIR_CD )			]]></sql>
			<params>
				<param name="cost_yrmon" type="12" value="201112" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="stnd_cost_cd" type="12" value="" out="N"/>
				<param name="op_uc_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
