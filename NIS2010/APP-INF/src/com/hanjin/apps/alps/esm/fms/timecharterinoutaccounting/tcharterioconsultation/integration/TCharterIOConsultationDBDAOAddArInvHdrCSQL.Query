'' AS AGMT_DOC_CFM_CD,
'' AS AGMT_FILE_CFM_CD,
'' AS AGMT_EVID_CFM_FNL_CD,
'' AS AP_FILE_DIV_CD,
@[cre_usr_id] 	AS CRE_USR_ID,
SYSDATE 		AS CRE_DT
FROM DUAL			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
