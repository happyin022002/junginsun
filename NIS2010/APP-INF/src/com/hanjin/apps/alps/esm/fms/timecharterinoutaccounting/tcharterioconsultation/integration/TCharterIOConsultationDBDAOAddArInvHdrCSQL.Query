@[csr_no] AS CSR_NO,
'' AS ACCT_XCH_RT_YRMON,
'' AS CSR_USD_AMT,
'Y' AS RQST_APRO_STEP_FLG,
'' AS COM_AP_CSR_APRO_HIS_SEQ,
'GW' AS CSR_APRO_TP_CD,
SYSDATE AS CSR_APRO_STEP_ASGN_RQST_DT,
'' AS CSR_APRO_STEP_ASGN_DT,
'' AS CSR_APRO_CMPL_DT,
