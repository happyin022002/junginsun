GW_AGMT_DOC_CFM_CD,
AGMT_DOC_CFM_CD,
AGMT_FILE_CFM_CD,
AGMT_EVID_CFM_FNL_CD,
AP_FILE_DIV_CD,
CRE_USR_ID,
CRE_DT
