<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOsearchDblEdiChgByTraxRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT N3RD_OFC
,      CLT_OFC_CD
,      PPD_RCV_OFC_CD
FROM   (SELECT MAX(NVL(N3PTY_RCV_OFC_CD, ' ')) N3RD_OFC
        FROM   BKG_CHG_RT
        WHERE  BKG_NO = @[bkg_no]
           AND ( (N3PTY_CUST_CNT_CD = 'CN'
              AND N3PTY_CUST_SEQ = 44619
              AND (N3PTY_RCV_OFC_CD = 'SHARC'
                OR N3PTY_RCV_OFC_CD = 'SHAHQ'))
             OR (N3PTY_CUST_CNT_CD = 'SG'
             AND N3PTY_RCV_OFC_CD = 'SINSC'
             AND (N3PTY_CUST_SEQ = DECODE(@[edi_receive_id],'TRAX.HPENTAPAC',16932,260)
               OR N3PTY_CUST_SEQ IN (11029,17434))))
           AND ROWNUM = 1) B1, 
       (SELECT MAX(CLT_OFC_CD) CLT_OFC_CD, MAX(PPD_RCV_OFC_CD) PPD_RCV_OFC_CD
        FROM   BKG_RATE
        WHERE  BKG_NO = @[bkg_no]
           AND ((CLT_PAYR_CUST_SEQ = 'CN' AND CLT_PAYR_CUST_SEQ = 44619)
             OR (CLT_PAYR_CNT_CD = 'SG' AND CLT_PAYR_CUST_SEQ IN (DECODE(@[edi_receive_id],'TRAX.HPENTAPAC',16932,260), 11029,17434)
             AND CLT_OFC_CD IN ('SHAHQ', 'SHARC', 'SINSC'))
             OR (PPD_PAYR_CNT_CD = 'SG'
             AND PPD_RCV_OFC_CD = 'SINSC'
             AND PPD_PAYR_CUST_SEQ IN (DECODE(@[edi_receive_id],'TRAX.HPENTAPAC',16932,260), 11029,17434)))
           AND ROWNUM = 1) B2			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="edi_receive_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
