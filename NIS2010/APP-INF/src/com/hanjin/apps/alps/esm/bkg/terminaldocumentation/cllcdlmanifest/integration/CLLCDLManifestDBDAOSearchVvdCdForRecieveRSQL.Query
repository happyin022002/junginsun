<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLLCDLManifestDBDAOSearchVvdCdForRecieveRSQL">
			<desc><![CDATA[SearchVvdCdForRecieve]]></desc>
			<sql><![CDATA[
SELECT 
	VSL_CD IN_VSLCD,
	SKD_VOY_NO IN_VSLVOY,
	SKD_DIR_CD IN_VSLDIR
FROM	BKG_CSTMS_TML_CLL
WHERE	VSL_CD = @[in_vvdcll]
AND	SKD_VOY_NO||SKD_DIR_CD LIKE '%'||RTRIM(@[in_vvd])||'%'
AND	ROWNUM = 1 			]]></sql>
			<params>
				<param name="in_vvdcll" type="12" value="" out="N"/>
				<param name="in_vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
