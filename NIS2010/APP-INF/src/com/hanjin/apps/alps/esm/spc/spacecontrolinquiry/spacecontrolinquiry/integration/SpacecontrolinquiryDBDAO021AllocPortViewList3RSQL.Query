                   , CTRL_ECC_FLG, CTRL_LOC_FLG, CTRL_USA_SVC_MOD_FLG, CTRL_ACCT_FLG, CTRL_DEST_LVL_CD, USA_BKG_MOD_CD, DEST_LOC_CD
                   , ACCOUNT_CD
