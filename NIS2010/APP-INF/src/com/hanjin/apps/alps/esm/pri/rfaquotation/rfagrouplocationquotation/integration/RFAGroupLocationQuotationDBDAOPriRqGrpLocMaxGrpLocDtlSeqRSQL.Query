<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAGroupLocationQuotationDBDAOPriRqGrpLocMaxGrpLocDtlSeqRSQL">
			<desc><![CDATA[Max GrpLocDtl Seq]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(MAX(GRP_LOC_DTL_SEQ)+1,1) AS MAX_GRP_LOC_DTL_SEQ
FROM PRI_RQ_GRP_LOC_DTL
WHERE	QTTN_NO = @[qttn_no]
AND	QTTN_VER_NO = @[qttn_ver_no]
AND	GRP_LOC_SEQ = @[grp_loc_seq]			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="grp_loc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
