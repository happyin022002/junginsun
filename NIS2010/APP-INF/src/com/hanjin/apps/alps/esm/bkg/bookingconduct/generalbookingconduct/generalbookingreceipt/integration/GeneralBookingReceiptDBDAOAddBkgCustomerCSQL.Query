<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingReceiptDBDAOAddBkgCustomerCSQL">
			<desc><![CDATA[Booking Customer 정보 저장]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')
INSERT INTO BKG_CUST_HIS (
#else
INSERT INTO BKG_CUSTOMER (
#end
	BKG_NO
#if (${ca_flg}== 'Y')
,   CORR_NO
#end
,	BKG_CUST_TP_CD
,	CUST_CNT_CD
,	CUST_SEQ
,	CUST_NM
,	CUST_ADDR
,	CUST_CTY_NM
,	CUST_STE_CD
,	CSTMS_DECL_CNT_CD
,	CUST_ZIP_ID
,	CUST_FAX_NO
,	CUST_EML
,	CUST_TP_CD
,	REF_NO
,	ADDR_PRN_FLG
,	VAL_NM
,	VAL_FAX_NO
,	VAL_CD
,	VAL_USR_ID
,	VAL_OFC_CD
,	VAL_DT
,	MTCH_FLG
,	AN_SND_FLG
,	CHG_DP_FLG
,	IB_CUST_NM
,	IB_CUST_ADDR
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
, 	EUR_CSTMS_ST_NM
, 	EORI_NO
,   END_USR_NM
) VALUES( 
	@[bkg_no]
#if (${ca_flg}== 'Y')
,   'TMP0000001'
#end
,	@[bkg_cust_tp_cd]
,	@[cust_cnt_cd]
,	@[cust_seq]
,	NVL(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(REGEXP_REPLACE(@[cust_nm], chr(13)||chr(10), '▤') , chr(10),''), chr(13), '▤'), chr(9), ' '), '▤' ,chr(13)||chr(10))
        ,(SELECT CUST_LGL_ENG_NM FROM MDM_CUSTOMER WHERE CUST_CNT_CD = @[cust_cnt_cd] AND CUST_SEQ = @[cust_seq]))
,	(SELECT REPLACE(bzet_addr, '@*', CHR(13)||CHR(10))                
	   FROM MDM_CUST_ADDR
      WHERE CUST_CNT_CD   = @[cust_cnt_cd]
        AND CUST_SEQ      = @[cust_seq]
        AND PRMRY_CHK_FLG = 'Y'
        AND ROWNUM        = 1)
,	(SELECT CTY_NM                    
	   FROM MDM_CUST_ADDR
      WHERE CUST_CNT_CD   = @[cust_cnt_cd]
        AND CUST_SEQ      = @[cust_seq]
        AND PRMRY_CHK_FLG = 'Y'
        AND ROWNUM        = 1)
,	(SELECT STE_CD                    
	   FROM MDM_CUST_ADDR
      WHERE CUST_CNT_CD   = @[cust_cnt_cd]
        AND CUST_SEQ      = @[cust_seq]
        AND PRMRY_CHK_FLG = 'Y'
        AND ROWNUM        = 1)
,	@[cstms_decl_cnt_cd]
,	(SELECT ZIP_CD                    
	   FROM MDM_CUST_ADDR
      WHERE CUST_CNT_CD   = @[cust_cnt_cd]
        AND CUST_SEQ      = @[cust_seq]
        AND PRMRY_CHK_FLG = 'Y'
        AND ROWNUM        = 1)
,	@[cust_fax_no]
,	(SELECT CNTC_EML                   
	   FROM MDM_CUST_ADDR
      WHERE CUST_CNT_CD   = @[cust_cnt_cd]
        AND CUST_SEQ      = @[cust_seq]
        AND PRMRY_CHK_FLG = 'Y'
        AND ROWNUM        = 1)
,	@[cust_tp_cd]
,	@[ref_no]
,	NVL(@[addr_prn_flg],'Y')
,	@[val_nm]
,	@[val_fax_no]
,	@[val_cd]
,	@[val_usr_id]
,	@[val_ofc_cd]
,	TO_DATE(@[val_dt],'YYYY-MM-DD')
,	NVL(@[mtch_flg]  , 'N')
,	NVL(@[an_snd_flg], 'N')
,	NVL(@[chg_dp_flg], 'N')
,	@[ib_cust_nm]
,	@[ib_cust_addr]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
, 	@[eur_cstms_st_nm]
, 	@[eori_no]
,   @[end_usr_nm]
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="cust_nm" type="12" value="" out="N"/>
				<param name="cstms_decl_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_fax_no" type="12" value="" out="N"/>
				<param name="cust_tp_cd" type="12" value="" out="N"/>
				<param name="ref_no" type="12" value="" out="N"/>
				<param name="addr_prn_flg" type="12" value="" out="N"/>
				<param name="val_nm" type="12" value="" out="N"/>
				<param name="val_fax_no" type="12" value="" out="N"/>
				<param name="val_cd" type="12" value="" out="N"/>
				<param name="val_usr_id" type="12" value="" out="N"/>
				<param name="val_ofc_cd" type="12" value="" out="N"/>
				<param name="val_dt" type="12" value="" out="N"/>
				<param name="mtch_flg" type="12" value="" out="N"/>
				<param name="an_snd_flg" type="12" value="" out="N"/>
				<param name="chg_dp_flg" type="12" value="" out="N"/>
				<param name="ib_cust_nm" type="12" value="" out="N"/>
				<param name="ib_cust_addr" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eur_cstms_st_nm" type="12" value="" out="N"/>
				<param name="eori_no" type="12" value="" out="N"/>
				<param name="end_usr_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
