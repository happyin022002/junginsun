<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MotFilingLocationPropertyDBDAOPriMotFileLocPptVOCSQL">
			<desc><![CDATA[PriMotFileLocPpt Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRI_MOT_FILE_LOC_PPT
( 
    ORG_DEST_TP_CD,
    LOC_CD,
    MOT_FILE_LOC_CD,
    MOT_FILE_LOC_NM,
    MOT_FILE_LANE_CD,
    FILE_USE_ONY_FLG,
    DELT_FLG,
    SVC_SCP_CD,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
)
VALUES
(
    @[org_dest_tp_cd],
    @[loc_cd],
    @[mot_file_loc_cd],
    @[mot_file_loc_nm],
    @[mot_file_lane_cd],
    DECODE(NVL(@[file_use_ony_flg],'0'),'0','N','1','Y'),
    DECODE(NVL(@[delt_flg],'0'),'0','N','1','Y'),
    @[svc_scp_cd],
    @[cre_usr_id],
    SYSDATE,
    @[cre_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="org_dest_tp_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="mot_file_loc_cd" type="12" value="" out="N"/>
				<param name="mot_file_loc_nm" type="12" value="" out="N"/>
				<param name="mot_file_lane_cd" type="12" value="" out="N"/>
				<param name="file_use_ony_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
