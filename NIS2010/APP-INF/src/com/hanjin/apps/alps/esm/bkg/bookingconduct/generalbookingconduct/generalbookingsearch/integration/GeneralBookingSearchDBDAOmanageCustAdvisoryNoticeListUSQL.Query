<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralBookingSearchDBDAOmanageCustAdvisoryNoticeListUSQL">
			<desc><![CDATA[Advisory Notice 대상 B/L의 연락처 정보를 갱신한다.]]></desc>
			<sql><![CDATA[
UPDATE BKG_CUST_AVC_NTC_DTL 
SET FAX_NO     = @[fax_no],
    NTC_EML    = @[ntc_eml],
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT     = sysdate
WHERE SRC_DAT_TP_CD  = @[src_dat_tp_cd]
AND   BL_NO          = @[bl_no]
AND   BKG_CUST_TP_CD = @[bkg_cust_tp_cd]
			]]></sql>
			<params>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="ntc_eml" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="src_dat_tp_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
