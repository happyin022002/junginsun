<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TCharterIOContractDAOSearchFileCertificationListRSQL">
			<desc><![CDATA[TCharterIOContractDAOSearchFileCertificationListRSQL]]></desc>
			<sql><![CDATA[
SELECT FILE_SEQ,    
	   FLET_FILE_TP_CD,
	   FILE_NM,      
	   FILE_DESC,      
	   '0' FILE_DOWNLOAD,
	   FILE_SAV_ID,
	   EFF_DT,
	   EXP_DT,
	   UPD_DT
  FROM (
		SELECT /*+ INDEX(FC XPKFMS_CHTR_PTY_FILE) */ 
			   FILE_SEQ,
			   FLET_FILE_TP_CD,
			   FILE_NM,
			   FILE_DESC,
			   '0' FILE_DOWNLOAD,
			   FILE_SAV_ID,
			   TO_CHAR(EFF_DT,'YYYYMMDD') EFF_DT,
			   TO_CHAR(EXP_DT,'YYYYMMDD') EXP_DT,
			   TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI' ) UPD_DT
		  FROM FMS_CHTR_PTY_FILE FC
		 WHERE FLET_CTRT_NO = @[flet_ctrt_no]
		   AND FLET_FILE_TP_CD = @[flet_file_tp_cd]
	   )			]]></sql>
			<params>
				<param name="flet_ctrt_no" type="12" value="" out="N"/>
				<param name="flet_file_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
