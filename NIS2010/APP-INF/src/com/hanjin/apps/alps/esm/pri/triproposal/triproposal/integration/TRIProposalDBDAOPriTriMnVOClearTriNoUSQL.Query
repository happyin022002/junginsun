<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TRIProposalDBDAOPriTriMnVOClearTriNoUSQL">
			<desc><![CDATA[PRI_TRI_MN TRI No. Clear]]></desc>
			<sql><![CDATA[
UPDATE PRI_TRI_MN
   SET TRI_NO = DECODE((SELECT EML_SND_DT FROM PRI_TRI_RT WHERE TRI_PROP_NO = @[tri_prop_no] AND AMDT_SEQ = @[amdt_seq]), NULL, NULL, TRI_NO)
WHERE TRI_PROP_NO = @[tri_prop_no]			]]></sql>
			<params>
				<param name="tri_prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
