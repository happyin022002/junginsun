<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="NetworkDistributionDBDAOSearchAgrdNtwkAllocByAgmtRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT B.COST_YRMON, B.COST_WK AS FM_COST_WK
     , A.FM_TRD_CD, A.FM_RLANE_CD, A.FM_IOC_CD
     , A.FM_VSL_CD, A.FM_SKD_VOY_NO, A.FM_DIR_CD
     , A.FM_VSL_CD || A.FM_SKD_VOY_NO || A.FM_DIR_CD AS FM_VVD
     , A.TS_UC_AMT, A.SML_SLS_AMT, A.CHT_OUT_AMT
     , B.COST_WK AS TO_COST_WK
     , A.TO_TRD_CD, A.TO_RLANE_CD, A.TO_IOC_CD
     , A.TO_VSL_CD, A.TO_SKD_VOY_NO, A.TO_DIR_CD     
     , A.TO_VSL_CD || A.TO_SKD_VOY_NO || A.TO_DIR_CD AS TO_VVD
     , A.LOCL_TS_STS_CD
     , DECODE(A.BZC_ALOC_TP_CD, 'R', 'Y', 'N') AS RAT_FLG
     , DECODE(A.BZC_ALOC_TP_CD, 'F', 'Y', 'N') AS FX_AMT_FLG
     , A.OVR_USD_ALOC_CHG_FLG
     , A.BSA, DECODE(A.BSA,0,0,A.AGRD_TEU / A.BSA) * 100 AS RTO, A.AGRD_TEU, A.TS_TEU, A.OVR_TEU
     , A.OVR_USD_ALOC_CHG_RTO, A.OVR_SLT_PRC, A.BZC_ALOC_FX_AMT
     , A.AGRD_EXPN_AMT, A.OVR_USD_AMT, A.FX_EXPN_AMT
	 , A.CRE_USR_ID,A.UPD_USR_ID
  FROM MAS_ALOC_AGMT_EXPN A, MAS_MON_VVD B
 WHERE A.TO_TRD_CD       = B.TRD_CD    
   AND A.TO_RLANE_CD     = B.RLANE_CD  
   AND A.TO_IOC_CD       = B.IOC_CD    
   AND A.TO_VSL_CD       = B.VSL_CD    
   AND A.TO_SKD_VOY_NO   = B.SKD_VOY_NO
   AND A.TO_DIR_CD       = B.DIR_CD
   AND B.DELT_FLG <> 'Y'
#if (${f_chkprd} == 'M')
	AND B.COST_YRMON = @[f_year] || @[f_mon]  
#end
#if (${f_chkprd} == 'W')
	AND B.COST_YRMON LIKE @[f_year]  || '%'  
   	AND B.COST_WK BETWEEN @[f_fm_wk] AND @[f_to_wk]
#end
 
 ORDER BY B.COST_YRMON, B.COST_WK
        , A.FM_TRD_CD, A.FM_RLANE_CD, A.FM_IOC_CD, A.FM_VSL_CD, A.FM_SKD_VOY_NO, A.FM_DIR_CD
        , A.LOCL_TS_STS_CD 
        , A.TO_TRD_CD, A.TO_RLANE_CD, A.TO_IOC_CD, A.TO_VSL_CD, A.TO_SKD_VOY_NO, A.TO_DIR_CD			]]></sql>
			<params>
				<param name="f_year" type="12" value="" out="N"/>
				<param name="f_mon" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
