<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManilaManifestListDownloadDBDAOsearchCheckVvdRSQL">
			<desc><![CDATA[입력된 VVD 등록여부 체크]]></desc>
			<sql><![CDATA[
SELECT 'x'  OK_FLAG
       FROM  dual
       WHERE EXISTS ( SELECT 'y'
                      FROM  VSK_VSL_PORT_SKD
                      WHERE VSL_CD        = @[vsl_cd]
                          AND SKD_VOY_NO =  @[skd_voy_no]
                          AND SKD_DIR_CD    = @[skd_dir_cd])			]]></sql>
			<params>
				<param name="vslcd" type="12" value="" out="N"/>
				<param name="skdvoyno" type="12" value="" out="N"/>
				<param name="skddircd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
