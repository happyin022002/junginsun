<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BlRatingDBDAOSearchGroupRatingChargeRSQL">
			<desc><![CDATA[Temp테이블에 저장된 Rating 결과, 기존 Manual/Interface Charge 조회]]></desc>
			<sql><![CDATA[
SELECT BKG_NO,          FRT_TERM_CD,          TRF_ITM_NO,           CGO_CATE_CD,
      IMDG_CLSS_CD,     CHG_CD,               CURR_CD,              RAT_UT_CD,  
      RAT_AS_QTY,       CHG_UT_AMT,           CHG_AMT,              RCV_TERM_CD,
      DE_TERM_CD,       PRN_HDN_FLG,          'A' AUTO_RAT_CD,      FRT_INCL_XCLD_DIV_CD INCL_OFT_FLG, 
      0 APLY_XCH_RTO,   NOTE_RT_SEQ,          PROP_NO,              AMDT_SEQ,
      SVC_SCP_CD,       GEN_SPCL_RT_TP_CD,    CMDT_HDR_SEQ,         ROUT_SEQ,
      FX_RT_FLG,        @[usr_id] CRE_USR_ID, @[usr_id] UPD_USR_ID,
      DECODE(SOC_FLG,'Y','Y','') SOC_FLG
FROM BKG_REV_AUD_CHG_TMP T
WHERE CHG_CD NOT IN (SELECT CHG_CD
                     FROM BKG_CHG_RT R
                     WHERE R.BKG_NO = T.BKG_NO
                     AND R.CHG_CD = T.CHG_CD
                     AND R.CHG_CD IN ('OIH','DIH')
                     AND R.AUTO_RAT_CD = 'I'
                     AND ROWNUM = 1
                     )
UNION ALL
SELECT BKG_NO,          FRT_TERM_CD,          TRF_ITM_NO,           CGO_CATE_CD,
       IMDG_CLSS_CD,    CHG_CD,               CURR_CD,              RAT_UT_CD,  
       RAT_AS_QTY,      CHG_UT_AMT,           CHG_AMT,              RCV_TERM_CD, 
       DE_TERM_CD,      PRN_HDN_FLG,          AUTO_RAT_CD,          FRT_INCL_XCLD_DIV_CD INCL_OFT_FLG, 
       APLY_XCH_RTO,    NOTE_RT_SEQ,          PROP_NO,              AMDT_SEQ,
       SVC_SCP_CD,      GEN_SPCL_RT_TP_CD,    CMDT_HDR_SEQ,         ROUT_SEQ,
       FX_RT_FLG,       @[usr_id] CRE_USR_ID, @[usr_id] UPD_USR_ID,
       SOC_FLG
FROM BKG_CHG_RT T
WHERE BKG_NO = @[bkg_no]
AND AUTO_RAT_CD IN ('I','M')			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
