<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFARateQuotationDBDAORsltPriRqRtDuplicateCheckRSQL">
			<desc><![CDATA[DuplicateCheck]]></desc>
			<sql><![CDATA[
WITH T AS
 (SELECT A.QTTN_NO,
         A.QTTN_VER_NO,
         A.CMDT_HDR_SEQ,
         A.ROUT_SEQ,
         A.RT_SEQ,
         B.PRC_CMDT_DEF_CD,
         REPLACE(B.PRC_CMDT_DEF_NM, '|||', ' / ' || CHR(13)) AS PRC_CMDT_DEF_NM,
         D.ROUT_PNT_LOC_DEF_CD AS ORG_ROUT_PNT_LOC_DEF_CD,
         REPLACE(D.ROUT_PNT_LOC_DEF_NM, '|||', CHR(13)) AS ORG_ROUT_PNT_LOC_DEF_NM,
         NVL(E.ROUT_VIA_PORT_DEF_CD, '||') AS ORG_ROUT_VIA_PORT_DEF_CD,
         REPLACE(E.ROUT_VIA_PORT_DEF_NM, '|||', CHR(13)) AS ORG_ROUT_VIA_PORT_DEF_NM,
         NVL(F.ROUT_VIA_PORT_DEF_CD, '||') AS DEST_ROUT_VIA_PORT_DEF_CD,
         REPLACE(F.ROUT_VIA_PORT_DEF_NM, '|||', CHR(13)) AS DEST_ROUT_VIA_PORT_DEF_NM,
         G.ROUT_PNT_LOC_DEF_CD AS DEST_ROUT_PNT_LOC_DEF_CD,
         REPLACE(G.ROUT_PNT_LOC_DEF_NM, '|||', CHR(13)) AS DEST_ROUT_PNT_LOC_DEF_NM,
         A.RAT_UT_CD,
         A.PRC_CGO_TP_CD,
         A.CURR_CD,
         A.QTTN_RT_AMT
    FROM (SELECT X1.QTTN_NO,
                 X1.QTTN_VER_NO,
                 X1.CMDT_HDR_SEQ,
                 X1.ROUT_SEQ,
                 X1.RT_SEQ,
                 X1.RAT_UT_CD,
                 X1.PRC_CGO_TP_CD,
                 X1.CURR_CD,
                 X1.QTTN_RT_AMT
            FROM PRI_RQ_RT          X1,
                 PRI_RQ_RT_CMDT_HDR X2
           WHERE X1.QTTN_NO = @[qttn_no]
                 AND X1.QTTN_VER_NO = @[qttn_ver_no]
                 AND X1.QTTN_NO = X2.QTTN_NO
                 AND X1.QTTN_VER_NO = X2.QTTN_VER_NO
                 AND X1.CMDT_HDR_SEQ = X2.CMDT_HDR_SEQ
                 AND X2.FIC_RT_TP_CD = NVL(@[fic_rt_tp_cd], 'G')) A,
         (SELECT QTTN_NO,
                 QTTN_VER_NO,
                 CMDT_HDR_SEQ,
                 MAX(SYS_CONNECT_BY_PATH(PRC_CMDT_DEF_CD, '||')) AS PRC_CMDT_DEF_CD,
                 SUBSTR(MAX(SYS_CONNECT_BY_PATH(PRC_CMDT_DEF_NM, '|||')), 4) AS PRC_CMDT_DEF_NM
            FROM (SELECT A.QTTN_NO,
                         A.QTTN_VER_NO,
                         A.CMDT_HDR_SEQ,
                         A.PRC_CMDT_DEF_CD,
                         DECODE(A.PRC_CMDT_TP_CD, 'G', (SELECT PRC_GRP_CMDT_DESC
                                    FROM PRI_RQ_GRP_CMDT
                                   WHERE QTTN_NO = A.QTTN_NO
                                         AND QTTN_VER_NO = A.QTTN_VER_NO
                                         AND PRC_GRP_CMDT_CD = A.PRC_CMDT_DEF_CD
                                         AND ROWNUM = 1), 'R', (SELECT REP_CMDT_NM
                                    FROM MDM_REP_CMDT
                                   WHERE REP_CMDT_CD = A.PRC_CMDT_DEF_CD
                                         AND ROWNUM = 1), 'C', (SELECT CMDT_NM
                                    FROM MDM_COMMODITY
                                   WHERE CMDT_CD = A.PRC_CMDT_DEF_CD
                                         AND ROWNUM = 1)) AS PRC_CMDT_DEF_NM,
                         ROW_NUMBER() OVER(PARTITION BY A.QTTN_NO, A.QTTN_VER_NO, A.CMDT_HDR_SEQ ORDER BY DECODE(A.PRC_CMDT_TP_CD, 'G', '1', 'R', '2', 'C', '3'), A.PRC_CMDT_DEF_CD, A.QTTN_NO, A.QTTN_VER_NO, A.CMDT_HDR_SEQ, A.CMDT_SEQ) AS RN
                    FROM PRI_RQ_RT_CMDT     A,
                         PRI_RQ_RT_CMDT_HDR X
                   WHERE A.QTTN_NO = @[qttn_no]
                         AND A.QTTN_VER_NO = @[qttn_ver_no]
                         AND A.QTTN_NO = X.QTTN_NO
                         AND A.QTTN_VER_NO = X.QTTN_VER_NO
                         AND A.CMDT_HDR_SEQ = X.CMDT_HDR_SEQ
                         AND NVL(X.FIC_RT_TP_CD, 'G') = NVL(@[fic_rt_tp_cd], 'G'))
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                     AND PRIOR RN = RN - 1
           GROUP BY QTTN_NO,
                    QTTN_VER_NO,
                    CMDT_HDR_SEQ) B,
         (SELECT QTTN_NO,
                 QTTN_VER_NO,
                 CMDT_HDR_SEQ,
                 ROUT_SEQ,
                 MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD || '|' || RCV_DE_TERM_CD, '||')) AS ROUT_PNT_LOC_DEF_CD,
                 SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_NM || NVL2(RCV_DE_TERM_NM, '(' || RCV_DE_TERM_NM || ')', ''), '|||')), 4) AS ROUT_PNT_LOC_DEF_NM
            FROM (SELECT QTTN_NO,
                         QTTN_VER_NO,
                         CMDT_HDR_SEQ,
                         ROUT_SEQ,
                         ORG_DEST_TP_CD,
                         ROUT_PNT_LOC_DEF_CD,
                         RCV_DE_TERM_CD,
                         DECODE(ROUT_PNT_LOC_TP_CD, 'G', (SELECT PRC_GRP_LOC_DESC
                                    FROM PRI_RQ_GRP_LOC
                                   WHERE QTTN_NO = A.QTTN_NO
                                         AND QTTN_VER_NO = A.QTTN_VER_NO
                                         AND PRC_GRP_LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                         AND ROWNUM = 1), 'L', (SELECT LOC_NM
                                    FROM MDM_LOCATION
                                   WHERE LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                         AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM,
                         (SELECT INTG_CD_VAL_DP_DESC
                            FROM COM_INTG_CD_DTL
                           WHERE INTG_CD_ID = 'CD02138'
                                 AND INTG_CD_VAL_CTNT = A.RCV_DE_TERM_CD
                                 AND ROWNUM = 1) AS RCV_DE_TERM_NM,
                         ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD, QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) AS RN
                    FROM PRI_RQ_RT_ROUT_PNT A
                   WHERE QTTN_NO = @[qttn_no]
                         AND QTTN_VER_NO = @[qttn_ver_no]
                         AND ORG_DEST_TP_CD = 'O')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                     AND PRIOR ROUT_SEQ = ROUT_SEQ
                     AND PRIOR RN = RN - 1
           GROUP BY QTTN_NO,
                    QTTN_VER_NO,
                    CMDT_HDR_SEQ,
                    ROUT_SEQ,
                    ORG_DEST_TP_CD) D,
         (SELECT QTTN_NO,
                 QTTN_VER_NO,
                 CMDT_HDR_SEQ,
                 ROUT_SEQ,
                 MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '||')) AS ROUT_VIA_PORT_DEF_CD,
                 SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '|||')), 4) AS ROUT_VIA_PORT_DEF_NM
            FROM (SELECT QTTN_NO,
                         QTTN_VER_NO,
                         CMDT_HDR_SEQ,
                         ROUT_SEQ,
                         ORG_DEST_TP_CD,
                         ROUT_VIA_PORT_DEF_CD,
                         ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD, QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) AS RN
                    FROM PRI_RQ_RT_ROUT_VIA A
                   WHERE QTTN_NO = @[qttn_no]
                         AND QTTN_VER_NO = @[qttn_ver_no]
                         AND ORG_DEST_TP_CD = 'O')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                     AND PRIOR ROUT_SEQ = ROUT_SEQ
                     AND PRIOR RN = RN - 1
           GROUP BY QTTN_NO,
                    QTTN_VER_NO,
                    CMDT_HDR_SEQ,
                    ROUT_SEQ,
                    ORG_DEST_TP_CD) E,
         (SELECT QTTN_NO,
                 QTTN_VER_NO,
                 CMDT_HDR_SEQ,
                 ROUT_SEQ,
                 MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '||')) AS ROUT_VIA_PORT_DEF_CD,
                 SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_VIA_PORT_DEF_CD, '|||')), 4) AS ROUT_VIA_PORT_DEF_NM
            FROM (SELECT QTTN_NO,
                         QTTN_VER_NO,
                         CMDT_HDR_SEQ,
                         ROUT_SEQ,
                         ORG_DEST_TP_CD,
                         ROUT_VIA_PORT_DEF_CD,
                         ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY DECODE(ROUT_VIA_PORT_TP_CD, 'G', '1', 'L', '2'), ROUT_VIA_PORT_DEF_CD, QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) AS RN
                    FROM PRI_RQ_RT_ROUT_VIA A
                   WHERE QTTN_NO = @[qttn_no]
                         AND QTTN_VER_NO = @[qttn_ver_no]
                         AND ORG_DEST_TP_CD = 'D')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                     AND PRIOR ROUT_SEQ = ROUT_SEQ
                     AND PRIOR RN = RN - 1
           GROUP BY QTTN_NO,
                    QTTN_VER_NO,
                    CMDT_HDR_SEQ,
                    ROUT_SEQ,
                    ORG_DEST_TP_CD) F,
         (SELECT QTTN_NO,
                 QTTN_VER_NO,
                 CMDT_HDR_SEQ,
                 ROUT_SEQ,
                 MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_CD || '|' || RCV_DE_TERM_CD, '||')) AS ROUT_PNT_LOC_DEF_CD,
                 SUBSTR(MAX(SYS_CONNECT_BY_PATH(ROUT_PNT_LOC_DEF_NM || NVL2(RCV_DE_TERM_NM, '(' || RCV_DE_TERM_NM || ')', ''), '|||')), 4) AS ROUT_PNT_LOC_DEF_NM
            FROM (SELECT QTTN_NO,
                         QTTN_VER_NO,
                         CMDT_HDR_SEQ,
                         ROUT_SEQ,
                         ORG_DEST_TP_CD,
                         ROUT_PNT_LOC_DEF_CD,
                         RCV_DE_TERM_CD,
                         DECODE(ROUT_PNT_LOC_TP_CD, 'G', (SELECT PRC_GRP_LOC_DESC
                                    FROM PRI_RQ_GRP_LOC
                                   WHERE QTTN_NO = A.QTTN_NO
                                         AND QTTN_VER_NO = A.QTTN_VER_NO
                                         AND PRC_GRP_LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                         AND ROWNUM = 1), 'L', (SELECT LOC_NM
                                    FROM MDM_LOCATION
                                   WHERE LOC_CD = A.ROUT_PNT_LOC_DEF_CD
                                         AND ROWNUM = 1)) AS ROUT_PNT_LOC_DEF_NM,
                         (SELECT INTG_CD_VAL_DP_DESC
                            FROM COM_INTG_CD_DTL
                           WHERE INTG_CD_ID = 'CD02138'
                                 AND INTG_CD_VAL_CTNT = A.RCV_DE_TERM_CD
                                 AND ROWNUM = 1) AS RCV_DE_TERM_NM,
                         ROW_NUMBER() OVER(PARTITION BY QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD ORDER BY DECODE(ROUT_PNT_LOC_TP_CD, 'G', '1', 'L', '2'), ROUT_PNT_LOC_DEF_CD, QTTN_NO, QTTN_VER_NO, CMDT_HDR_SEQ, ROUT_SEQ, ORG_DEST_TP_CD) AS RN
                    FROM PRI_RQ_RT_ROUT_PNT A
                   WHERE QTTN_NO = @[qttn_no]
                         AND QTTN_VER_NO = @[qttn_ver_no]
                         AND ORG_DEST_TP_CD = 'D')
           START WITH RN = 1
          CONNECT BY PRIOR CMDT_HDR_SEQ = CMDT_HDR_SEQ
                     AND PRIOR ROUT_SEQ = ROUT_SEQ
                     AND PRIOR RN = RN - 1
           GROUP BY QTTN_NO,
                    QTTN_VER_NO,
                    CMDT_HDR_SEQ,
                    ROUT_SEQ,
                    ORG_DEST_TP_CD) G
   WHERE A.QTTN_NO = B.QTTN_NO(+)
         AND A.QTTN_VER_NO = B.QTTN_VER_NO(+)
         AND A.CMDT_HDR_SEQ = B.CMDT_HDR_SEQ(+)
         AND A.QTTN_NO = D.QTTN_NO(+)
         AND A.QTTN_VER_NO = D.QTTN_VER_NO(+)
         AND A.CMDT_HDR_SEQ = D.CMDT_HDR_SEQ(+)
         AND A.ROUT_SEQ = D.ROUT_SEQ(+)
         AND A.QTTN_NO = E.QTTN_NO(+)
         AND A.QTTN_VER_NO = E.QTTN_VER_NO(+)
         AND A.CMDT_HDR_SEQ = E.CMDT_HDR_SEQ(+)
         AND A.ROUT_SEQ = E.ROUT_SEQ(+)
         AND A.QTTN_NO = F.QTTN_NO(+)
         AND A.QTTN_VER_NO = F.QTTN_VER_NO(+)
         AND A.CMDT_HDR_SEQ = F.CMDT_HDR_SEQ(+)
         AND A.ROUT_SEQ = F.ROUT_SEQ(+)
         AND A.QTTN_NO = G.QTTN_NO(+)
         AND A.QTTN_VER_NO = G.QTTN_VER_NO(+)
         AND A.CMDT_HDR_SEQ = G.CMDT_HDR_SEQ(+)
         AND A.ROUT_SEQ = G.ROUT_SEQ(+))
SELECT A.QTTN_NO,
       A.QTTN_VER_NO,
       A.CMDT_HDR_SEQ,
       A.ROUT_SEQ,
       A.RT_SEQ,
       A.PRC_CMDT_DEF_NM,
       A.ORG_ROUT_PNT_LOC_DEF_NM,
       A.ORG_ROUT_VIA_PORT_DEF_NM,
       A.DEST_ROUT_VIA_PORT_DEF_NM,
       A.DEST_ROUT_PNT_LOC_DEF_NM,
       A.RAT_UT_CD,
       A.PRC_CGO_TP_CD,
       A.CURR_CD,
       A.QTTN_RT_AMT,
       DENSE_RANK() OVER(PARTITION BY NULL ORDER BY A.QTTN_NO, A.QTTN_VER_NO, A.PRC_CMDT_DEF_CD, A.ORG_ROUT_PNT_LOC_DEF_CD, A.ORG_ROUT_VIA_PORT_DEF_CD, A.DEST_ROUT_VIA_PORT_DEF_CD, A.DEST_ROUT_PNT_LOC_DEF_CD, A.RAT_UT_CD, A.PRC_CGO_TP_CD) AS SEQ,
       'Show ' || CHR(64 + ROW_NUMBER() OVER(PARTITION BY A.QTTN_NO, A.QTTN_VER_NO, A.PRC_CMDT_DEF_CD, A.ORG_ROUT_PNT_LOC_DEF_CD, A.ORG_ROUT_VIA_PORT_DEF_CD, A.DEST_ROUT_VIA_PORT_DEF_CD, A.DEST_ROUT_PNT_LOC_DEF_CD, A.RAT_UT_CD, A.PRC_CGO_TP_CD
                            ORDER BY
                            A.RT_SEQ)) AS VIEW_TEXT
  FROM T A,
       (SELECT QTTN_NO,
               QTTN_VER_NO,
               PRC_CMDT_DEF_CD,
               ORG_ROUT_PNT_LOC_DEF_CD,
               ORG_ROUT_VIA_PORT_DEF_CD,
               DEST_ROUT_VIA_PORT_DEF_CD,
               DEST_ROUT_PNT_LOC_DEF_CD,
               RAT_UT_CD,
               PRC_CGO_TP_CD
          FROM T
         GROUP BY QTTN_NO,
                  QTTN_VER_NO,
                  PRC_CMDT_DEF_CD,
                  ORG_ROUT_PNT_LOC_DEF_CD,
                  ORG_ROUT_VIA_PORT_DEF_CD,
                  DEST_ROUT_VIA_PORT_DEF_CD,
                  DEST_ROUT_PNT_LOC_DEF_CD,
                  RAT_UT_CD,
                  PRC_CGO_TP_CD
        HAVING COUNT(*) > 1) B
 WHERE A.QTTN_NO = B.QTTN_NO
       AND A.QTTN_VER_NO = B.QTTN_VER_NO
       AND A.PRC_CMDT_DEF_CD = B.PRC_CMDT_DEF_CD
       AND A.ORG_ROUT_PNT_LOC_DEF_CD = B.ORG_ROUT_PNT_LOC_DEF_CD
       AND A.ORG_ROUT_VIA_PORT_DEF_CD = B.ORG_ROUT_VIA_PORT_DEF_CD
       AND A.DEST_ROUT_VIA_PORT_DEF_CD = B.DEST_ROUT_VIA_PORT_DEF_CD
       AND A.DEST_ROUT_PNT_LOC_DEF_CD = B.DEST_ROUT_PNT_LOC_DEF_CD
       AND A.RAT_UT_CD = B.RAT_UT_CD
       AND A.PRC_CGO_TP_CD = B.PRC_CGO_TP_CD
			]]></sql>
			<params>
				<param name="qttn_no" type="12" value="" out="N"/>
				<param name="qttn_ver_no" type="12" value="" out="N"/>
				<param name="fic_rt_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
