<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoReceiptDBDAOEBookingBkgAwkCgoUSQL">
			<desc><![CDATA[EBookingBkgAwkCgo]]></desc>
			<sql><![CDATA[
#if (${ca_flg}== 'Y')

UPDATE BKG_AWK_CGO_HIS SET 
 	CNTR_NO               = @[cntr_no]
,	CNTR_TPSZ_CD          = @[cntr_tpsz_cd]
,	CMDT_CD               = @[cmdt_cd]
,	TTL_DIM_LEN           = @[ttl_dim_len]
,	TTL_DIM_WDT           = @[ttl_dim_wdt]
,	TTL_DIM_HGT           = @[ttl_dim_hgt]
,	GRS_WGT               = @[grs_wgt]
,	WGT_UT_CD             = @[wgt_ut_cd]
,	PCK_QTY               = @[pck_qty]
,	PCK_TP_CD             = @[pck_tp_cd]
,	NET_WGT               = @[net_wgt]
,	STWG_RQST_DESC        = REPLACE(REPLACE(REPLACE(@[stwg_rqst_desc], CHR(13)||CHR(10), ' '), CHR(13), ' '), CHR(10), ' ')
,	UPD_USR_ID 			  = @[upd_usr_id]
,	UPD_DT 				  = SYSDATE
,	OVR_FWRD_LEN		  = @[ovr_fwrd_len]
,	OVR_BKWD_LEN		  = @[ovr_bkwd_len]
,	OVR_HGT				  = @[ovr_hgt]
,	OVR_LF_LEN			  = @[ovr_lf_len]
,	OVR_RT_LEN			  = @[ovr_rt_len]
,	OVR_VOID_SLT_QTY	  = @[ovr_void_slt_qty]	
,	IN_GA_FLG			  = @[in_ga_flg]
WHERE	BKG_NO = @[bkg_no]
AND	AWK_CGO_SEQ = @[awk_cgo_seq]
AND NVL(SPCL_CGO_APRO_CD,' ') NOT IN ('R', 'Y', 'N')
AND	CORR_NO = 'TMP0000001'

#else

UPDATE BKG_AWK_CGO SET 
 	CNTR_NO               = @[cntr_no]
,	CNTR_TPSZ_CD          = @[cntr_tpsz_cd]
,	CMDT_CD               = @[cmdt_cd]
,	TTL_DIM_LEN           = @[ttl_dim_len]
,	TTL_DIM_WDT           = @[ttl_dim_wdt]
,	TTL_DIM_HGT           = @[ttl_dim_hgt]
,	GRS_WGT               = @[grs_wgt]
,	WGT_UT_CD             = @[wgt_ut_cd]
,	PCK_QTY               = @[pck_qty]
,	PCK_TP_CD             = @[pck_tp_cd]
,	NET_WGT               = @[net_wgt]
,	STWG_RQST_DESC        = REPLACE(REPLACE(REPLACE(@[stwg_rqst_desc], CHR(13)||CHR(10), ' '), CHR(13), ' '), CHR(10), ' ')
,	UPD_USR_ID 			  = @[upd_usr_id]
,	UPD_DT 				  = SYSDATE
,	OVR_FWRD_LEN		  = @[ovr_fwrd_len]
,	OVR_BKWD_LEN		  = @[ovr_bkwd_len]
,	OVR_HGT				  = @[ovr_hgt]
,	OVR_LF_LEN			  = @[ovr_lf_len]
,	OVR_RT_LEN			  = @[ovr_rt_len]
,	OVR_VOID_SLT_QTY	  = @[ovr_void_slt_qty]	
,	IN_GA_FLG			  = @[in_ga_flg]
WHERE	BKG_NO = @[bkg_no]
AND NVL(SPCL_CGO_APRO_CD,' ') NOT IN ('R', 'Y', 'N')
AND	AWK_CGO_SEQ = @[awk_cgo_seq]

#end			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="ttl_dim_len" type="12" value="" out="N"/>
				<param name="ttl_dim_wdt" type="12" value="" out="N"/>
				<param name="ttl_dim_hgt" type="12" value="" out="N"/>
				<param name="grs_wgt" type="6" value="" out="N"/>
				<param name="wgt_ut_cd" type="12" value="" out="N"/>
				<param name="pck_qty" type="12" value="" out="N"/>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="net_wgt" type="6" value="" out="N"/>
				<param name="stwg_rqst_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ovr_fwrd_len" type="12" value="" out="N"/>
				<param name="ovr_bkwd_len" type="12" value="" out="N"/>
				<param name="ovr_hgt" type="12" value="" out="N"/>
				<param name="ovr_lf_len" type="12" value="" out="N"/>
				<param name="ovr_rt_len" type="12" value="" out="N"/>
				<param name="ovr_void_slt_qty" type="12" value="" out="N"/>
				<param name="in_ga_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="awk_cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
