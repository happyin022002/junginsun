<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOBkgDpcsQueueSetSearchWhereListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT BKG_JOIN_FNC( CURSOR(
SELECT DECODE(ROWNUM,1,'AND (', 'OR ') || '(1=1 ' || 
  DECODE(ORG_CNT_CD,        NULL,' ', ' AND ''' || ORG_CNT_CD || ''' IN (POR.CNT_CD, POL.CNT_CD)') || 
  DECODE(POL_CD,            NULL,' ', ' AND B.POL_CD = ''' || POL_CD || '''') ||
    
  DECODE(DEL_CONTI_CD,      NULL,' ', ' AND NVL(DEL.CONTI_CD,POD.CONTI_CD) ' ||
                                        ' IN (''' || DECODE(DEL_CONTI_CD,  'E', 'F'',''E', DEL_CONTI_CD) || ''')') ||
  DECODE(HNDL_OFC_CD,       NULL,' ', ' AND XTER.HNDL_OFC_CD = ''' || HNDL_OFC_CD || '''') ||   

  DECODE(CUST_CNT_CD,       NULL,' ', ' AND DECODE('''||SRCH.BKG_CUST_TP_CD||''', ''F'', F.BKG_CUST_TP_CD, ''C'', C.BKG_CUST_TP_CD, S.BKG_CUST_TP_CD) = '''||SRCH.CUST_CNT_CD||'''')||
  DECODE(CUST_SEQ, 0, ' ',  NULL,' ', ' AND DECODE('''||SRCH.BKG_CUST_TP_CD||''', ''F'', F.CUST_SEQ, ''C'', C.CUST_SEQ, S.CUST_SEQ) = '''||SRCH.CUST_SEQ||'''')||
  DECODE(CUST_NM,       	NULL,' ', ' AND DECODE('''||SRCH.BKG_CUST_TP_CD||''', ''F'', F.CUST_NM,  ''C'', C.CUST_NM,  S.CUST_NM)  LIKE '''||SRCH.CUST_NM||'%''')||
  DECODE(SLAN_CD,           NULL,' ', ' AND VVD.SLAN_CD = ''' || SLAN_CD || '''') || 
  DECODE(SREP_CD,           NULL,' ', ' AND NVL(B.OB_SREP_CD, XTER.SREP_CD)  = ''' || SREP_CD ||'''')||
  ')' CUST_NM
 
FROM BKG_XTER_SRCH_SET SRCH
WHERE USR_ID = @[usr_id]
AND SET_SLCT_FLG = 'Y'
AND DOC_TP_CD = 'D'

)
,'\\n'
) FROM DUAL			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
