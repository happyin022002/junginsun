<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingMasterMgtDBDAOSearchDPSCUserGroupUSQL">
			<desc><![CDATA[DPCS - S/R 업무처리 담당자 Group 정보를  갱신한다]]></desc>
			<sql><![CDATA[
UPDATE BKG_DPCS_USR_GRP SET 
	USR_ID = @[usr_id]
,	DPCS_WRK_GRP_CD = @[dpcs_wrk_grp_cd]
,	DPCS_PSN_CD = @[dpcs_psn_cd]
,	DPCS_WRK_PRT_CD = @[dpcs_wrk_prt_cd]
,	DPCS_WRK_SVR_CD = @[dpcs_wrk_svr_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = TO_DATE(@[upd_dt],'YYYY-MM-DD')  --SYSDATE
,	CRE_DT = TO_DATE(@[cre_dt],'YYYY-MM-DD') 

WHERE	USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="dpcs_wrk_grp_cd" type="12" value="" out="N"/>
				<param name="dpcs_psn_cd" type="12" value="" out="N"/>
				<param name="dpcs_wrk_prt_cd" type="12" value="" out="N"/>
				<param name="dpcs_wrk_svr_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
