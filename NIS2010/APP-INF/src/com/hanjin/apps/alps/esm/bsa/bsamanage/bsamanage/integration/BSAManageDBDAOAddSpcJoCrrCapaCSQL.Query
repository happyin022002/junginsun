<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BSAManageDBDAOAddSpcJoCrrCapaCSQL">
			<desc><![CDATA[AddSpcJoCrrCapa Insert]]></desc>
			<sql><![CDATA[
INSERT INTO BSA_JNT_OP_CRR_CAPA (
        BSA_SEQ, 
        TRD_CD, 
        RLANE_CD, 
        DIR_CD, 
        VOP_CD, 
        VSL_CAPA,
        BSA_OP_CD, 
        BSA_OP_JB_CD, 
        CRR_CD,
        CRR_BSA_CAPA, 
        SPC_CTRL_SLT_CAPA,
        CRE_USR_ID, 
        CRE_DT, 
        UPD_USR_ID, 
        UPD_DT)
 SELECT
        A.BSA_SEQ, 
        A.TRD_CD, 
        A.RLANE_CD, 
        A.DIR_CD, 
        A.VOP_CD, 
        A.VSL_CAPA,
        B.BSA_OP_CD, 
        LPAD(C.NUM+5,3,'0') BSA_OP_JB_CD, 
        B.CRR_CD,
        0 CRR_BSA_CAPA, 
        0 SPC_CTRL_SLT_CAPA,
        @[cre_usr_id], 
        SYSDATE, 
        @[upd_usr_id], 
        SYSDATE
 FROM   BSA_JNT_OP_BZC  A,
       (
        SELECT DISTINCT 
               BSA_OP_CD, 
               CRR_CD
         FROM BSA_CRR_RGST
         WHERE BSA_OP_CD    = @[bsa_op_cd]   /* Joint Operating */
           AND BSA_OP_JB_CD IN ('001','002','003','004','005')
           AND NVL(APLY_FLG,'N') = 'Y'
         ORDER BY crr_cd
        ) B,
        (SELECT NUM 
         FROM(
		          SELECT ROWNUM NUM 
		          FROM COM_CPY_NO 
		          WHERE ROWNUM<= 17)
		    WHERE num NOT IN (15,16)
        -- SELECT ROWNUM NUM FROM COM_CPY_NO WHERE ROWNUM<= 14 BSA_OP_CD BETWEEN '006' AND '019'
        ) C
 WHERE  A.BSA_SEQ   = @[bsa_seq]
 AND    A.TRD_CD    = @[trd_cd]
 AND    A.RLANE_CD  = @[rlane_cd]
 AND    A.DIR_CD    = @[dir_cd]
 AND    A.VOP_CD    = @[vop_cd]
 AND    A.VSL_CAPA  = @[vsl_capa]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bsa_op_cd" type="12" value="" out="Y"/>
				<param name="bsa_seq" type="12" value="" out="Y"/>
				<param name="trd_cd" type="12" value="" out="Y"/>
				<param name="rlane_cd" type="12" value="" out="Y"/>
				<param name="dir_cd" type="12" value="" out="Y"/>
				<param name="vop_cd" type="12" value="" out="Y"/>
				<param name="vsl_capa" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
