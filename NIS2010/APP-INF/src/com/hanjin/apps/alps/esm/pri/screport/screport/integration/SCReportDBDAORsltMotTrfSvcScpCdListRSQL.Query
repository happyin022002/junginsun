<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCReportDBDAORsltMotTrfSvcScpCdListRSQL">
			<desc><![CDATA[MOT Tariff 화면에서 사용되는 Service Scope List 가져오기
PRI_MOT_FILE_LOC_PPT 에 등록되어있는 Service Scope 만을 가져온다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
       A.SVC_SCP_CD AS CD
	  ,A.SVC_SCP_NM AS NM
FROM   MDM_SVC_SCP A
	,  PRI_MOT_FILE_LOC_PPT B
WHERE  A.DELT_FLG = 'N'
AND    B.ORG_DEST_TP_CD = 'D'
AND	   A.SVC_SCP_CD = B.SVC_SCP_CD
ORDER BY A.SVC_SCP_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
