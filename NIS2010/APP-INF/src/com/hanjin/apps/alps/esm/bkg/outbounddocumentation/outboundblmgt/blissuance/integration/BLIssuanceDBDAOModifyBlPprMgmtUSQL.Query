<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BLIssuanceDBDAOModifyBlPprMgmtUSQL">
			<desc><![CDATA[BLIssuanceDBDAOModifyBlPprMgmtUSQL]]></desc>
			<sql><![CDATA[
UPDATE BKG_CUST_BL_PPR_MGMT 
   SET CUST_DTRB_KNT = @[cust_dtrb_knt]
      ,FM_SER_NO = @[cust_fm_ser_no]
      ,TO_SER_NO = @[cust_to_ser_no]
 WHERE DTRB_YR = @[dtrb_yr]
   AND RHQ_CD = @[rhq_cd]
   AND OFC_CD = @[ofc_cd]
   AND CUST_CNT_CD = @[cust_cnt_cd]
   AND CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_dtrb_knt" type="12" value="" out="N"/>
				<param name="cust_fm_ser_no" type="12" value="" out="N"/>
				<param name="cust_to_ser_no" type="12" value="" out="N"/>
				<param name="dtrb_yr" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
