<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgBbCgoVOCSQL">
			<desc><![CDATA[BKG_BB_CGO에서 SCG_BB_CGO으로 DATA 복제]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_BB_CGO (
	BKG_NO
,	SPCL_CGO_APRO_RQST_SEQ
,	BB_CGO_SEQ
,	RCV_TERM_CD
,	DE_TERM_CD
,	PCK_QTY
,	PCK_TP_CD
,	GRS_WGT
,	NET_WGT
,	WGT_UT_CD
,	DIM_LEN
,	DIM_WDT
,	DIM_HGT
,	CGO_WGT
,	STWG_INSTR_DESC
,	BB_DCGO_SEQ
,	CMDT_CD
,	SLNG_PNT_FLG
,	GRAV_CTR_DESC
,	PCK_DTL_DESC
,	CGO_LODG_MZD_CD
,	SCR_DNG_CTNT
,	SPCL_RQST_DESC
,	DIFF_RMK
,	RQST_DT
,	RQST_GDT
,	RQST_USR_ID
,	SPCL_CGO_APRO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) 
SELECT 
	BKG_NO
#if (${spcl_cgo_apro_rqst_seq} != '') 
,	@[spcl_cgo_apro_rqst_seq]
#else
,	(	SELECT MAX(SPCL_CGO_APRO_RQST_SEQ) FROM SCG_APRO_RQST WHERE BKG_NO = @[bkg_no] AND SPCL_CGO_CATE_CD = 'BB'	)
#end
,	BB_CGO_SEQ
,	RCV_TERM_CD
,	DE_TERM_CD
,	PCK_QTY
,	PCK_TP_CD
,	GRS_WGT
,	NET_WGT
,	WGT_UT_CD
,	DIM_LEN
,	DIM_WDT
,	DIM_HGT
,	CGO_WGT
,	STWG_INSTR_DESC
,	BB_DCGO_SEQ
,	CMDT_CD
,	SLNG_PNT_FLG
,	GRAV_CTR_DESC
,	PCK_DTL_DESC
,	CGO_LODG_MZD_CD
,	SCR_DNG_CTNT
,	SPCL_RQST_DESC
,	DIFF_RMK
,	RQST_DT
,	RQST_GDT
,	RQST_USR_ID
,	SPCL_CGO_APRO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_BB_CGO
WHERE BKG_NO = @[bkg_no]
#if (${spcl_cgo_apro_rqst_seq} != '' || ${cgo_seq} == '') 
AND SPCL_CGO_APRO_CD not in ('C','D')
#end
#if (${cgo_seq} != '') 
AND BB_CGO_SEQ = @[cgo_seq]
#end			]]></sql>
			<params>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
