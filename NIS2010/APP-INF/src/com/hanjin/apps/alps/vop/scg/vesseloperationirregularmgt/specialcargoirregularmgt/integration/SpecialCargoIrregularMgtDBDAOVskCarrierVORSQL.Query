<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoIrregularMgtDBDAOVskCarrierVORSQL">
			<desc><![CDATA[SPCL CGO Irregular List의 Carrier Combo 조회]]></desc>
			<sql><![CDATA[
SELECT VCR.CRR_CD
     , VCR.CRR_NM
  FROM SCG_IRREGULAR SIR

#if (${crr_cd} == 'vsl_opr_tp_cd') 
     , MDM_CARRIER VCR
WHERE SIR.VSL_CRR_CD = VCR.CRR_CD

#elseif (${crr_cd} == 'cgo_opr_cd') 
     , MDM_CARRIER VCR
WHERE SIR.CGO_OPR_CD = VCR.CRR_CD
#end

 GROUP BY
       VCR.CRR_CD
     , VCR.CRR_NM			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
