<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgCdVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	CD_TBL_ID
,	VAL_CD
,	CD_NM
,	ATTR_CTNT1
,	ATTR_CTNT2
,	ATTR_CTNT3
,	ATTR_CTNT4
,	ATTR_CTNT5
,	ATTR_CTNT6
,	ATTR_CTNT7
,	ATTR_CTNT8
,	ATTR_CTNT9
,	ATTR_CTNT10
,	ATTR_VAL1
,	ATTR_VAL2
,	ATTR_VAL3
,	ATTR_VAL4
,	ATTR_VAL5
,	ATTR_VAL6
,	ATTR_VAL7
,	ATTR_VAL8
,	ATTR_VAL9
,	ATTR_VAL10
,	ATTR_DT1
,	ATTR_DT2
,	ATTR_DT3
,	ATTR_DT4
,	ATTR_DT5
,	DELT_FLG
FROM SCG_CD
WHERE	CD_TBL_ID = @[cd_tbl_id]
#if (${delt_flg} != '') 
AND DELT_FLG = @[delt_flg]
#else
AND DELT_FLG = 'N'
#end
#if (${val_cd} != '') 
AND	VAL_CD like @[val_cd]||'%'
#end
#if (${cd_nm} != '') 
AND	UPPER(CD_NM) like UPPER(@[cd_nm])||'%'
#end
#if (${attr_ctnt1} != '') 
AND	ATTR_CTNT1 = @[attr_ctnt1]
#end
#if (${attr_ctnt2} != '') 
AND	ATTR_CTNT2 = @[attr_ctnt2]
#end			]]></sql>
			<params>
				<param name="cd_tbl_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="val_cd" type="12" value="" out="N"/>
				<param name="cd_nm" type="12" value="" out="N"/>
				<param name="attr_ctnt1" type="12" value="" out="N"/>
				<param name="attr_ctnt2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
