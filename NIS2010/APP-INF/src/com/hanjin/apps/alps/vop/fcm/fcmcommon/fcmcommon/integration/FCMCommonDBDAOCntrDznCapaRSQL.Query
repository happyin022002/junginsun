<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FCMCommonDBDAOCntrDznCapaRSQL">
			<desc><![CDATA[MDM에서 Vessel(Container) Design Capacity 목록을 조회한다.]]></desc>
			<sql><![CDATA[
WITH TMP AS (
    SELECT
    T1.CNTR_DZN_CAPA
    FROM MDM_VSL_CNTR T1
    WHERE 1=1
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG='N'
    AND T1.FDR_DIV_CD='T'
    AND T1.CRR_CD='SML'
    AND NVL(T1.CNTR_DZN_CAPA, 0)!=0
    GROUP BY T1.CNTR_DZN_CAPA
)
SELECT MAX(CNTR_DZN_CAPA) CNTR_DZN_CAPA FROM (
    SELECT SYS_CONNECT_BY_PATH(CNTR_DZN_CAPA, '|') CNTR_DZN_CAPA
    FROM (SELECT CNTR_DZN_CAPA,
                 ROW_NUMBER() OVER(ORDER BY CNTR_DZN_CAPA) RNUM FROM TMP)
    WHERE LEVEL = RNUM
    START WITH RNUM=1
    CONNECT BY PRIOR RNUM=RNUM-1
)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
