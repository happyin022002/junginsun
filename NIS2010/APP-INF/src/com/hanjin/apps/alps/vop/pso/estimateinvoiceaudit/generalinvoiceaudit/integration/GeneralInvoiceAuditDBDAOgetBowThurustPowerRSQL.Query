<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetBowThurustPowerRSQL">
			<desc><![CDATA[2014.07.18 이윤정 CHM-201430973 [PSO] New Object (Bow Thrust Power)]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
,BWTHST_BHP_PWR AS HP
,BWTHST_BHP_PWR * 0.7355 AS KW
FROM MDM_VSL_CNTR
WHERE 1=1
AND VSL_CD = SUBSTR(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
