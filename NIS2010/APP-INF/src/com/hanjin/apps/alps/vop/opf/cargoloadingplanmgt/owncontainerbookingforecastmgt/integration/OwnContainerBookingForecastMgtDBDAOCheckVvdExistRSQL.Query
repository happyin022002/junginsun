<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOCheckVvdExistRSQL">
			<desc><![CDATA[vvd 존재 여부]]></desc>
			<sql><![CDATA[
SELECT DECODE(X.CNT,0,'N','Y') AS FLG
  FROM (  SELECT NVL(COUNT(1),0)  AS CNT
            FROM VSK_VSL_PORT_SKD A
           WHERE A.VSL_CD        = @[vsl_cd]
             AND A.SKD_VOY_NO    = @[skd_voy_no]
             AND A.SKD_DIR_CD    = @[skd_dir_cd]
             AND A.TURN_PORT_IND_CD IN ('Y','N') ) X			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
