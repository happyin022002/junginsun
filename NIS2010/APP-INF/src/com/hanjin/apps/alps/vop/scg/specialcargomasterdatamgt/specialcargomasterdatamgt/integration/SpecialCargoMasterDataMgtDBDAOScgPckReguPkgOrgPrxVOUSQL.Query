<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgPckReguPkgOrgPrxVOUSQL">
			<desc><![CDATA[데이터를 UPDATE 를 한다.]]></desc>
			<sql><![CDATA[
UPDATE SCG_PCK_REGU_ORG_PRX
SET  IMDG_UN_NO    = @[imdg_un_no]  
    ,ORG_PRX_DESC  = @[org_prx_desc]
    ,IMDG_PCK_CD        = @[imdg_pck_cd]      
    ,MAX_CAPA_QTY      = @[max_capa_qty]    
    ,IMDG_CTRL_TEMP     = @[imdg_ctrl_temp]   
    ,IMDG_EMER_TEMP     = @[imdg_emer_temp]   
    ,DELT_FLG       = @[delt_flg]     
    ,UPD_USR_ID    = @[upd_usr_id]  
    ,UPD_DT        = SYSDATE        
WHERE 	IMDG_PCK_INSTR_CD     = @[imdg_pck_instr_cd] 
AND   	IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]
AND     REGU_DP_NO    = @[regu_dp_no]     
AND     SUB_SEQ        = @[sub_seq]			]]></sql>
			<params>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="org_prx_desc" type="12" value="" out="N"/>
				<param name="imdg_pck_cd" type="12" value="" out="N"/>
				<param name="max_capa_qty" type="12" value="" out="N"/>
				<param name="imdg_ctrl_temp" type="12" value="" out="N"/>
				<param name="imdg_emer_temp" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
				<param name="regu_dp_no" type="12" value="" out="N"/>
				<param name="sub_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
