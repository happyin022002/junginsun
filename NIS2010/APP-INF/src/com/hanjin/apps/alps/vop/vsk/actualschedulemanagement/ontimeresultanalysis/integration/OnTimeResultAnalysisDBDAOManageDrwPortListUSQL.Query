<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnTimeResultAnalysisDBDAOManageDrwPortListUSQL">
			<desc><![CDATA[Drewry Report의 Target Port List를 업데이트 한다.]]></desc>
			<sql><![CDATA[
MERGE INTO VSK_DRW_TGT_PORT DRW
    USING
    (
        SELECT  @[port_cd]                PORT_CD
        FROM    DUAL
    )   SRC
    ON
    (
                DRW.PORT_CD = SRC.PORT_CD	
    )
WHEN MATCHED THEN 
UPDATE
SET       
                  DELT_FLG          = 'Y'
                , UPD_USR_ID        = @[upd_usr_id]
                , UPD_DT            = SYSDATE
                
WHEN NOT MATCHED THEN
INSERT (
                  PORT_CD
                , DELT_FLG
                , CRE_USR_ID
                , CRE_DT
                , UPD_USR_ID
                , UPD_DT
       )
VALUES (
                  @[port_cd]
	            , @[cre_usr_id] 
	            , SYSDATE 
	            , @[upd_usr_id]
	            , SYSDATE
	   )			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
