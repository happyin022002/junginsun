<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselInformationMgtDBDAOmodifyMdmVslCntrUSQL">
			<desc><![CDATA[Particular 2 에 신규 추가한 Tier 정보를 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE  MDM_VSL_CNTR
SET     MN_ENG_TR_KNT   = @[mn_eng_tr_knt]
WHERE   1 = 1
AND     VSL_CD  = @[vsl_cd]			]]></sql>
			<params>
				<param name="mn_eng_tr_knt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
