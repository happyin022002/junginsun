<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOSearchVslClsListRSQL">
			<desc><![CDATA[SearchVslClsList]]></desc>
			<sql><![CDATA[
SELECT	DISTINCT CNTR_VSL_CLSS_CAPA
FROM	MDM_VSL_CNTR M
WHERE	CNTR_VSL_CLSS_CAPA IS NOT NULL
AND	CNTR_VSL_CLSS_CAPA > 0
AND	DELT_FLG		= 'N'
ORDER BY CNTR_VSL_CLSS_CAPA			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
