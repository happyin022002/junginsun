<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SimulationDBDAOMaxSimnoRSQL">
			<desc><![CDATA[sim_no max 값을 구함]]></desc>
			<sql><![CDATA[
SELECT MAX(BNK_CSM_SIM_NO) SIMNO
FROM FCM_BNK_CSM_PF_SIM 
WHERE 1=1
AND SUBSTR(BNK_CSM_SIM_NO,0,6) = TO_CHAR(SYSDATE,'YYYYMM')			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
