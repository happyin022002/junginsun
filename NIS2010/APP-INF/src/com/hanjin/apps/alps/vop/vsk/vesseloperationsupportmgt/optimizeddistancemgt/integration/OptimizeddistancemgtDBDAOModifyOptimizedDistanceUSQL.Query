<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OptimizeddistancemgtDBDAOModifyOptimizedDistanceUSQL">
			<desc><![CDATA[User가 Input한 Optimized Distance를 저장합니다.]]></desc>
			<sql><![CDATA[
UPDATE VSK_PORT_OPMZ_DIST
  SET
	  FM_YD_GRP_ID = @[sheet_fm_yd_grp_cd]
	, TO_YD_CD 	   = @[sheet_to_port_cd]
    , TO_YD_GRP_ID = @[sheet_to_yd_grp_cd]  
	, OPMZ_DIST    = @[opmz_dist]
	, RNG_MIN_DIST = @[rng_min_dist]
    , RNG_MAX_DIST = @[rng_max_dist]
    , UPD_USR_ID   = @[usr_id]
    , UPD_DT       = sysdate
	, FILE_NM	   = @[file_nm]
    , FILE_SAV_ID  = @[file_sav_id]
   
  WHERE 1=1
    AND FM_YD_CD = @[sheet_fm_port_cd]	
	AND FM_YD_GRP_ID = @[pre_sheet_fm_yd_grp_cd]
    AND TO_YD_CD = @[sheet_to_port_cd] 
	AND TO_YD_GRP_ID = @[pre_sheet_to_yd_grp_cd]			]]></sql>
			<params>
				<param name="sheet_fm_yd_grp_cd" type="12" value="" out="N"/>
				<param name="sheet_to_port_cd" type="12" value="" out="N"/>
				<param name="sheet_to_yd_grp_cd" type="12" value="" out="N"/>
				<param name="opmz_dist" type="12" value="" out="N"/>
				<param name="rng_min_dist" type="12" value="" out="N"/>
				<param name="rng_max_dist" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="sheet_fm_port_cd" type="12" value="" out="N"/>
				<param name="pre_sheet_fm_yd_grp_cd" type="12" value="" out="N"/>
				<param name="pre_sheet_to_yd_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
