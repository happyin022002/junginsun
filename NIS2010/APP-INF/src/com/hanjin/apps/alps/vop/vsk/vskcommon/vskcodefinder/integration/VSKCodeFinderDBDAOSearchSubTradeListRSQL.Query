		<query name="VSKCodeFinderDBDAOSearchSubTradeListRSQL">
			<desc><![CDATA[Sub Trade 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT SUB_TRD_CD||':'||SUB_TRD_NM SUB_TRD_CD
FROM MDM_SUB_TRD
WHERE DELT_FLG = 'N'
AND SUB_TRD_CD <> 'IP'
ORDER BY SUB_TRD_CD			]]></sql>
			<params>
			</params>
		</query>
