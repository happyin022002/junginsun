<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgRgnShpOprPortVOUSQL">
			<desc><![CDATA[Loading Prot for RSO]]></desc>
			<sql><![CDATA[
UPDATE scg_rgn_shp_opr_port SET 
	loc_cd = @[loc_cd]
,	rgn_shp_opr_cd = @[rgn_shp_opr_cd]
,	delt_flg = @[delt_flg]
,	upd_usr_id = @[upd_usr_id]
,	upd_dt = sysdate
WHERE	loc_cd = @[key_loc_cd]
AND	rgn_shp_opr_cd = @[key_rgn_shp_opr_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="key_loc_cd" type="12" value="" out="N"/>
				<param name="key_rgn_shp_opr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
