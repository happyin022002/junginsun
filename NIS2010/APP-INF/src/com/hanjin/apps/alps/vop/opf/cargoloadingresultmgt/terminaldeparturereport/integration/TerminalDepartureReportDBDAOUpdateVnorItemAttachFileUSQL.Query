<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOUpdateVnorItemAttachFileUSQL">
			<desc><![CDATA[Attach File Upload 시 VNOR 의 Item을 갱신합니다.]]></desc>
			<sql><![CDATA[
UPDATE OPF_VNOR_ITM
SET ATCH_FILE_LNK_ID = @[atch_file_lnk_id]
	,ATCH_FILE_KNT = (SELECT COUNT(*) FROM OPF_ATCH_FILE WHERE ATCH_FILE_LNK_ID = @[atch_file_lnk_id])
WHERE VSL_CD = @[vsl_cd]
AND VNOR_SEQ = @[vnor_seq]
AND VNOR_ITM_SEQ = @[vnor_itm_seq]			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vnor_seq" type="12" value="" out="N"/>
				<param name="vnor_itm_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
