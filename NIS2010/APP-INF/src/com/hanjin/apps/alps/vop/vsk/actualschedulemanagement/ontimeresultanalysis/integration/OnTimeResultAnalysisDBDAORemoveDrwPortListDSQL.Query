<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnTimeResultAnalysisDBDAORemoveDrwPortListDSQL">
			<desc><![CDATA[Drewry Report Port List 삭제 한다.]]></desc>
			<sql><![CDATA[
MERGE INTO VSK_DRW_TGT_PORT DRW
    USING
    (
        SELECT    @[port_cd]      PORT_CD      
        FROM    DUAL
    )   SRC
    ON
    (
                    DRW.PORT_CD = SRC.PORT_CD
    )
                
WHEN MATCHED THEN 
UPDATE
SET       
                  DELT_FLG          = 'Y'
                , UPD_USR_ID        = @[upd_usr_id]
                , UPD_DT            = SYSDATE			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
