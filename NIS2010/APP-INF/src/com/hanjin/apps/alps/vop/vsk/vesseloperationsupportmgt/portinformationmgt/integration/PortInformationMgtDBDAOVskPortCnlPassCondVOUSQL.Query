			<sql><![CDATA[
UPDATE VSK_PORT_CNL_PASS_COND SET 
	SVC_SCP_BND_CD = @[svc_scp_bnd_cd]
,   CNL_TZ_SEQ_CD = @[cnl_tz_seq_cd]
,	SCG_EXPT_LMT_HRMNT = @[scg_expt_lmt_hrmnt]
,	SCG_FM_LMT_HRMNT = @[scg_fm_lmt_hrmnt]
,	SCG_TO_LMT_HRMNT = @[scg_to_lmt_hrmnt]
,	LMT_TM_SCG_RTO = @[lmt_tm_scg_rto]
,	CNL_RMK = @[cnl_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	LOC_CD = @[loc_cd]
AND	PORT_SEQ = @[port_seq]			]]></sql>
			<params>
				<param name="svc_scp_bnd_cd" type="12" value="" out="N"/>
				<param name="cnl_tz_seq_cd" type="12" value="" out="N"/>
				<param name="scg_expt_lmt_hrmnt" type="12" value="" out="N"/>
				<param name="scg_fm_lmt_hrmnt" type="12" value="" out="N"/>
				<param name="scg_to_lmt_hrmnt" type="12" value="" out="N"/>
				<param name="lmt_tm_scg_rto" type="6" value="" out="N"/>
				<param name="cnl_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="port_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
