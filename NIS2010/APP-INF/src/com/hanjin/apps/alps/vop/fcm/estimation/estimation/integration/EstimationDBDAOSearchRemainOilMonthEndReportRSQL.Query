<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationDBDAOSearchRemainOilMonthEndReportRSQL">
			<desc><![CDATA[EstimationDBDAOSearchRemainOilMonthEndReportRSQL]]></desc>
			<sql><![CDATA[
/**
-- FcmRmnOilMonEndRptRVO
SELECT
'' REV_YRMON,
'' VSL_CD,
'' SKD_VOY_NO,
'' SKD_DIR_CD,
'' VSL_SLAN_CD,
'' FOIL_RMN_WGT,
'' DOIL_RMN_WGT,
'' CLND_OIL_RMN_WGT,
'' SYS_OIL_RMN_WGT,
'' GNR_OIL_RMN_WGT,
'' LOW_SULP_DOIL_RMN_WGT,
'' TRBN_GNR_OIL_RMN_WGT,
'' LOW_SULP_FOIL_RMN_WGT,
'' RPT_DOC_NO,
'' RPT_ISS_DT,
'' RPT_USR_ID,
'' RPT_USR_NM,
'' TEAM_DESC,
'' RPT_RMK,
'' CRE_USR_ID,
'' CRE_DT,
'' UPD_USR_ID,
'' UPD_DT
FROM FCM_RMN_OIL_MON_END_RPT
**/
SELECT REV_YRMON
	   ,VSL_CD
	   ,SKD_VOY_NO
	   ,SKD_DIR_CD
	   ,VSL_SLAN_CD
	   ,FOIL_RMN_WGT
	   ,DOIL_RMN_WGT
	   ,CLND_OIL_RMN_WGT
	   ,SYS_OIL_RMN_WGT
	   ,GNR_OIL_RMN_WGT
	   ,LOW_SULP_DOIL_RMN_WGT
	   ,TRBN_GNR_OIL_RMN_WGT
	   ,LOW_SULP_FOIL_RMN_WGT
	   ,RPT_DOC_NO
	   ,RPT_ISS_DT
	   ,RPT_USR_ID
	   ,RPT_USR_NM
	   ,TEAM_DESC
	   ,RPT_RMK
	   ,CRE_USR_ID
	   ,CRE_DT
	   ,UPD_USR_ID
	   ,UPD_DT
FROM    FCM_RMN_OIL_MON_END_RPT
WHERE   REV_YRMON=REPLACE(@[rev_yrmon], '-', '')
#if (${vsl_cd} != '') 
AND     VSL_CD = @[vsl_cd]
#end
#if (${skd_voy_no} != '') 
AND     SKD_VOY_NO = @[skd_voy_no]
#end
#if (${skd_dir_cd} != '') 
AND     SKD_DIR_CD = @[skd_dir_cd]
#end
#if (${rev_dir_cd} != '') 
AND     REV_DIR_CD = @[rev_dir_cd]
#end
ORDER BY REV_YRMON, VSL_CD, SKD_VOY_NO, SKD_DIR_CD			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
