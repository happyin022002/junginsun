<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOModifyVskVslPortSkdByAutoUpdateUSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
UPDATE 		VSK_VSL_PORT_SKD	PS
SET			PS.VPS_ETA_DT 		= TO_DATE(@[vps_eta_dt], 'YYYYMMDDHH24MI')
		,	PS.VPS_ETB_DT 		= TO_DATE(@[vps_etb_dt], 'YYYYMMDDHH24MI')
		,	PS.VPS_ETD_DT 		= TO_DATE(@[vps_etd_dt], 'YYYYMMDDHH24MI')
		,	PS.SEA_BUF_HRS 		= @[sea_buf_hrs]
		,	PS.PORT_BUF_HRS 	= @[port_buf_hrs]
		,	PS.UPD_USR_ID 		= @[upd_usr_id]
		,	PS.UPD_DT			= CASE 	WHEN PS.AUTO_SKD_CNG_FLG	= 'Y' THEN PS.UPD_DT
										WHEN PS.VSL_CD||PS.SKD_VOY_NO||PS.SKD_DIR_CD = @[org_upd_tgt_vvd] AND PS.VPS_PORT_CD = @[org_upd_tgt_vps_port_cd] AND PS.CLPT_IND_SEQ = @[org_upd_tgt_clpt_ind_seq] THEN SYSDATE
										ELSE PS.UPD_DT
								  END
		----,	UPD_DT 			= SYSDATE
WHERE		PS.VSL_CD 			= @[vsl_cd]
AND			PS.SKD_VOY_NO 		= @[skd_voy_no]
AND			PS.SKD_DIR_CD 		= @[skd_dir_cd]
AND			PS.VPS_PORT_CD 		= @[vps_port_cd]
AND			PS.CLPT_IND_SEQ 	= @[clpt_ind_seq]			]]></sql>
			<params>
				<param name="vps_eta_dt" type="12" value="" out="N"/>
				<param name="vps_etb_dt" type="12" value="" out="N"/>
				<param name="vps_etd_dt" type="12" value="" out="N"/>
				<param name="sea_buf_hrs" type="12" value="" out="N"/>
				<param name="port_buf_hrs" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="org_upd_tgt_vvd" type="12" value="" out="N"/>
				<param name="org_upd_tgt_vps_port_cd" type="12" value="" out="N"/>
				<param name="org_upd_tgt_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
