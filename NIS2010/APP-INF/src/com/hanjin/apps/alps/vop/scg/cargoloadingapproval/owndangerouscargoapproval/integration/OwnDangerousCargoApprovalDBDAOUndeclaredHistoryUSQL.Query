		<query name="OwnDangerousCargoApprovalDBDAOUndeclaredHistoryUSQL">
			<desc><![CDATA[OwnDangerousCargoApprovalDBDAOUndeclaredHistory]]></desc>
			<sql><![CDATA[
UPDATE 	SCG_NON_DG_CGO_UDECL_HIS 
SET	  ON_BRD_FLG            = NVL(@[on_brd_flg]        , ON_BRD_FLG           ) 
	, CMDT_CTNT             = NVL(@[cmdt_ctnt]         , CMDT_CTNT            ) 
	, RSLT_RMK1             = NVL(@[rslt_rmk1]         , RSLT_RMK1            ) 
	, RSLT_RMK2             = NVL(@[rslt_rmk2]         , RSLT_RMK2            ) 
	, FILE_SAV_ID           = NVL(@[file_sav_id]       , FILE_SAV_ID          ) 
	, FILE_NM               = NVL(@[file_nm]           , FILE_NM              ) 
	, UPD_USR_ID            = @[upd_usr_id]
	, UPD_DT                = SYSDATE
WHERE BKG_NO = @[bkg_no]
--AND RQST_DT = [rqst_dt]			]]></sql>
			<params>
				<param name="on_brd_flg" type="12" value="" out="N"/>
				<param name="cmdt_ctnt" type="12" value="" out="N"/>
				<param name="rslt_rmk1" type="12" value="" out="N"/>
				<param name="rslt_rmk2" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
