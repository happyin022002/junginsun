<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOModifyPackingInstructionRegulationPKGMethodRefListUSQL">
			<desc><![CDATA[Packing Instruction Creation]]></desc>
			<sql><![CDATA[
UPDATE SCG_PCK_REF     
   SET  REF_DESC = @[ref_desc]
, DELT_FLG       = @[delt_flg]
, UPD_USR_ID     = @[upd_usr_id]
, UPD_DT         = SYSDATE
FROM 
WHERE 1=1      
 AND PCK_REF_CD      = @[pck_ref_cd]
 AND REF_DIV_NO      = @[ref_div_no] 
 AND IMDG_PCK_INSTR_CD      = @[imdg_pck_instr_cd]
 AND IMDG_PCK_INSTR_SEQ  = @[imdg_pck_instr_seq]			]]></sql>
			<params>
				<param name="ref_desc" type="12" value="testtest" out="N"/>
				<param name="delt_flg" type="12" value="N" out="N"/>
				<param name="upd_usr_id" type="12" value="test" out="N"/>
				<param name="pck_ref_cd" type="12" value="PKG" out="N"/>
				<param name="ref_div_no" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_cd" type="12" value="P002" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
