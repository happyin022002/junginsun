<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOSearchPsoTariffRSQL">
			<desc><![CDATA[SearchPsoTariff]]></desc>
			<sql><![CDATA[
SELECT PORT_TRF_NO 
   FROM PSO_TARIFF
  WHERE CHG_XPR_NO  = @[chg_xpr_no]
   			]]></sql>
			<params>
				<param name="chg_xpr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
