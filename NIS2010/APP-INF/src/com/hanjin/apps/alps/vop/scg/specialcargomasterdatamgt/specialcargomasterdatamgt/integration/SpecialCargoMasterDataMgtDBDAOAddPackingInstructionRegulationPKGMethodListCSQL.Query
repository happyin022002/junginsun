<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOAddPackingInstructionRegulationPKGMethodListCSQL">
			<desc><![CDATA[PackingInstruction Creation]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_PCK_REGU_MZD
( IMDG_PCK_INSTR_CD
, IMDG_PCK_INSTR_SEQ
, REGU_DP_NO
, SUB_SEQ
, IMDG_PCK_MZD_CD
, CMB_PCK_MAX_WGT 
, LQD_MAX_WGT
, PCK_REF_CD
, REF_DIV_NO
, DELT_FLG
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
) 
SELECT @[imdg_pck_instr_cd]
    , @[imdg_pck_instr_seq]
    , @[regu_dp_no]
    , NVL(MAX(SUB_SEQ), 0) + 1
    , @[imdg_pck_mzd_cd]
    , @[max_mass]
    , @[max_ctnt]
    , @[pck_ref_cd] 
    , @[ref_div_no] 
    , 'N'
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
FROM SCG_PCK_REGU_MZD     
WHERE 1=1      
 AND IMDG_PCK_INSTR_CD     = @[imdg_pck_instr_cd]    
 AND IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]     
 AND REGU_DP_NO    = @[regu_dp_no]			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="P001" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="1" out="N"/>
				<param name="regu_dp_no" type="12" value="1" out="N"/>
				<param name="imdg_pck_mzd_cd" type="12" value="OP2" out="N"/>
				<param name="max_mass" type="12" value="0.7" out="N"/>
				<param name="max_ctnt" type="12" value="0.7" out="N"/>
				<param name="pck_ref_cd" type="12" value="" out="N"/>
				<param name="ref_div_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="test" out="N"/>
				<param name="upd_usr_id" type="12" value="test" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
