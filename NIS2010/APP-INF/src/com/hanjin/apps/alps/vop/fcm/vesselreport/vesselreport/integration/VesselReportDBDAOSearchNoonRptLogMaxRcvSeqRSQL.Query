<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselReportDBDAOSearchNoonRptLogMaxRcvSeqRSQL">
			<desc><![CDATA[Search max sequence of noon report log for update.
=====================================================]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(SYSDATE, 'YYYYMMDD') RCV_DT
     , NVL(MAX(RCV_SEQ), 0)+1 RCV_SEQ
FROM   FCM_NOON_RPT_LOG
WHERE  RCV_DT = TO_CHAR(SYSDATE, 'YYYYMMDD')			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
