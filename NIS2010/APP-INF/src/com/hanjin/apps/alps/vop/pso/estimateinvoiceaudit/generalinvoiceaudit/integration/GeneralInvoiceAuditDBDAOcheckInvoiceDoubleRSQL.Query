<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOcheckInvoiceDoubleRSQL">
			<desc><![CDATA[중복된 invoice 에 대해 상세한 정보를 alert로 띄워주기 위함]]></desc>
			<sql><![CDATA[
SELECT  'CSR NO : ' || T1.CSR_NO ||'*'|| 'INVOICE NO: '  ||T1.INV_NO ||'*'||
      'ISSUE DATE : '  || TO_CHAR(T1.INV_ISS_DT ,'YYYY/MM/DD') AS CSR_MSG
  FROM AP_PAY_INV T1
 WHERE T1.INV_NO    = @[inv_no]
   AND T1.VNDR_SEQ =  @[vndr_seq]
   AND T1.DELT_FLG =  'N'			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
