<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOModifyPackingInstructionPortableTankUSQL">
			<desc><![CDATA[ModifyPackingInstructionPortableTank]]></desc>
			<sql><![CDATA[
UPDATE SCG_PCK_PTB_TNK
   SET IMDG_UN_NO          = @[imdg_un_no]
      ,MIN_TST_PRSS     = @[min_tst_prss]
      ,MIN_SHL_THCK_CTNT        = @[min_shl_thck_ctnt]
      ,BTM_OPN_PROVI_CTNT    = @[btm_opn_provi_ctnt]
      ,PRS_RLF_PROVI_CTNT    = @[prs_rlf_provi_ctnt]
      ,SBST_DESC        = @[sbst_desc]
      ,SBST_DESC_REF_NO = @[sbst_desc_ref_no]
      ,FILL_DGR_CTNT            = @[fill_dgr_ctnt]
      ,IMDG_CTRL_TEMP           = @[imdg_ctrl_temp]
      ,CNTR_TEMP_REF_NO    = @[cntr_temp_ref_no]
      ,IMDG_EMER_TEMP           = @[imdg_emer_temp]
      ,EMER_TEMP_REF_NO    = @[emer_temp_ref_no]
      ,MAX_ALW_WRK_SML_PRSS = @[max_alw_wrk_sml_prss]
      ,MAX_ALW_WRK_BARE_PRSS = @[max_alw_wrk_bare_prss]
      ,MAX_ALW_WRK_SUN_SHLD_PRSS = @[max_alw_wrk_sun_shld_prss]
      ,MAX_ALW_WRK_INSLT_PRSS = @[max_alw_wrk_inslt_prss]
      ,OPN_BLW_LQD_LVL_CD  = @[opn_blw_lqd_lvl_cd]
      ,MAX_FILL_DNST_CTNT       = @[max_fill_dnst_ctnt]
      ,PCK_REF_CD              = @[pck_ref_cd]
      ,REF_DIV_NO              = @[ref_div_no]
      ,DELT_FLG            = @[delt_flg]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE
 WHERE PTBL_TNK_INSTR_CD     = @[ptbl_tnk_instr_cd]
   AND SUB_SEQ                  = @[sub_seq]			]]></sql>
			<params>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="min_tst_prss" type="12" value="" out="N"/>
				<param name="min_shl_thck_ctnt" type="12" value="" out="N"/>
				<param name="btm_opn_provi_ctnt" type="12" value="" out="N"/>
				<param name="prs_rlf_provi_ctnt" type="12" value="" out="N"/>
				<param name="sbst_desc" type="12" value="" out="N"/>
				<param name="sbst_desc_ref_no" type="12" value="" out="N"/>
				<param name="fill_dgr_ctnt" type="12" value="" out="N"/>
				<param name="imdg_ctrl_temp" type="12" value="" out="N"/>
				<param name="cntr_temp_ref_no" type="12" value="" out="N"/>
				<param name="imdg_emer_temp" type="12" value="" out="N"/>
				<param name="emer_temp_ref_no" type="12" value="" out="N"/>
				<param name="max_alw_wrk_sml_prss" type="12" value="" out="N"/>
				<param name="max_alw_wrk_bare_prss" type="12" value="" out="N"/>
				<param name="max_alw_wrk_sun_shld_prss" type="12" value="" out="N"/>
				<param name="max_alw_wrk_inslt_prss" type="12" value="" out="N"/>
				<param name="opn_blw_lqd_lvl_cd" type="12" value="" out="N"/>
				<param name="max_fill_dnst_ctnt" type="12" value="" out="N"/>
				<param name="pck_ref_cd" type="12" value="" out="N"/>
				<param name="ref_div_no" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ptbl_tnk_instr_cd" type="12" value="" out="N"/>
				<param name="sub_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
