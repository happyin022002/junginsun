<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchOfficeObjectListARSQL">
			<desc><![CDATA[object 조회]]></desc>
			<sql><![CDATA[

SELECT /*DISTINCT*/
       B.PSO_OBJ_CD             PSO_OBJ_CD
      ,C1.INTG_CD_VAL_DP_DESC   PSO_OBJ_CD_DSP
      ,B.PSO_MEAS_UT_CD         PSO_MEAS_UT_CD
      ,C2.INTG_CD_VAL_DP_DESC   PSO_MEAS_UT_CD_DSP
      ,B.OBJ_LIST_NO            OBJ_LIST_NO
FROM   PSO_OBJ_LIST         B
      ,COM_INTG_CD_DTL      C1
      ,COM_INTG_CD_DTL      C2
WHERE  B.PSO_OBJ_CD = C1.INTG_CD_VAL_CTNT
AND    B.PSO_MEAS_UT_CD = C2.INTG_CD_VAL_CTNT
AND    C1.INTG_CD_ID = 'CD01846'
AND    C2.INTG_CD_ID = 'CD01848'

#if(${types} != '0206')
AND    PSO_MEAS_UT_CD != 12  -- 12 FLAG 이 아닌것
#end
ORDER  BY C1.INTG_CD_VAL_DP_DESC
		 ,C2.INTG_CD_VAL_DP_DESC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
