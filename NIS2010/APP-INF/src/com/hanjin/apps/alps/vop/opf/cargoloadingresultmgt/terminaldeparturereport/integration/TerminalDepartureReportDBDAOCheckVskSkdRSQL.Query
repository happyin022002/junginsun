<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOCheckVskSkdRSQL">
			<desc><![CDATA[유효한 Vessel Port Schedule  인지 체크한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y') IS_OK
FROM VSK_VSL_PORT_SKD
WHERE VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = SUBSTR(@[skd_voy_no], 1, 4)
AND SKD_DIR_CD = SUBSTR(@[skd_voy_no], 5, 6)
-- From Port Check 할 경우
#if (${vnor_fm_port_cd} != '')
AND VPS_PORT_CD = @[vnor_fm_port_cd]
#end
-- To Port Check 할 경우
#if (${vnor_to_port_cd} != '')
AND VPS_PORT_CD = @[vnor_to_port_cd]
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="vnor_fm_port_cd" type="12" value="" out="N"/>
				<param name="vnor_to_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
