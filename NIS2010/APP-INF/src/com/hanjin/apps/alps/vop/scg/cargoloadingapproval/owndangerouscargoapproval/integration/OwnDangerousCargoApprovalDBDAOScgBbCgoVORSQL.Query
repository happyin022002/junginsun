<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgBbCgoVORSQL">
			<desc><![CDATA[SCG_BB_CGO의 내용을 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT 
	BKG_NO
,	SPCL_CGO_APRO_RQST_SEQ
,	BB_CGO_SEQ
,	RCV_TERM_CD
,	DE_TERM_CD
,	PCK_QTY
,	PCK_TP_CD
,	GRS_WGT
,	NET_WGT
,	WGT_UT_CD
,	DIM_LEN
,	DIM_WDT
,	DIM_HGT
,	CGO_WGT
,	STWG_INSTR_DESC
,	BB_DCGO_SEQ
,	CMDT_CD
,	SLNG_PNT_FLG
,	GRAV_CTR_DESC
,	PCK_DTL_DESC
,	CGO_LODG_MZD_CD
,	SCR_DNG_CTNT
,	SPCL_RQST_DESC
,	DIFF_RMK
,	RQST_DT
,	RQST_USR_ID
,	SPCL_CGO_APRO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_BB_CGO
WHERE	BKG_NO = @[bkg_no]
#if (${spcl_cgo_apro_rqst_seq} != '') 
AND	SPCL_CGO_APRO_RQST_SEQ = @[spcl_cgo_apro_rqst_seq]
#else
AND	SPCL_CGO_APRO_RQST_SEQ = (	SELECT MAX(SPCL_CGO_APRO_RQST_SEQ) FROM SCG_APRO_RQST WHERE SPCL_CGO_CATE_CD = 'BB' AND BKG_NO = @[bkg_no]	)
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
