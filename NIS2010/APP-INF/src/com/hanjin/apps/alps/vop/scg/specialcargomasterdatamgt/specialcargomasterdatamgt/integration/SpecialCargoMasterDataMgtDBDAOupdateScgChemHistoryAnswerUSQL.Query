<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOupdateScgChemHistoryAnswerUSQL">
			<desc><![CDATA[chemical history update]]></desc>
			<sql><![CDATA[
UPDATE scg_chem_his SET   chem_opin_knd_cd    = @[chem_opin_knd_cd]
                        , cmt_ctnt            = @[cmt_ctnt]
                        , imdg_spcl_provi_no  = @[imdg_spcl_provi_no]
                        , re_msg_id           = @[upd_usr_id]
                        , re_msg_dt           = SYSDATE
                        , upd_usr_id          = @[upd_usr_id]
                        , upd_dt              = SYSDATE
                   WHERE chem_seq             = @[chem_seq]			]]></sql>
			<params>
				<param name="chem_opin_knd_cd" type="12" value="" out="N"/>
				<param name="cmt_ctnt" type="12" value="" out="N"/>
				<param name="imdg_spcl_provi_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="chem_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
