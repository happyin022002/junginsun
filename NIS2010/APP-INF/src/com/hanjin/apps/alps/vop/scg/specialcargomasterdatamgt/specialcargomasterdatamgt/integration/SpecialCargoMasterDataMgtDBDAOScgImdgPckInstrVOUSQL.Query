<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgPckInstrVOUSQL">
			<desc><![CDATA[Packing Instructions/Provisions (Creation)  수정   ]]></desc>
			<sql><![CDATA[
UPDATE SCG_IMDG_PCK_INSTR SET 
	IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
,	FILE_SAV_ID = @[file_sav_id]
,	FILE_NM = @[file_nm]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,   PCK_DESC  = @[pck_desc]
WHERE IMDG_PCK_INSTR_CD  = @[old_imdg_pck_instr_cd]
  AND IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pck_desc" type="12" value="" out="N"/>
				<param name="old_imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
