<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerLinesDangerousCargoApprovalDBDAOPreRestrictionPIRegulatoryRSQL">
			<desc><![CDATA[PreRestrictionPIRegulatory]]></desc>
			<sql><![CDATA[
WITH VLD_CHK_TBL AS (
#if ($vld_chk.size() > 0)
	#foreach($key IN ${vld_chk})
		#if($velocityCount < $vld_chk.size()) 
     SELECT '$key' AS VLD_CHK
       FROM DUAL
      UNION ALL
		#else
     SELECT '$key' AS VLD_CHK
       FROM DUAL
		#end
    #end
#end
)
SELECT 'Limited Q'||''''||'TY' DIV_NM
      ,CASE WHEN IMDG_LMT_QTY > 0 THEN IMDG_LMT_QTY||' '||IMDG_LMT_QTY_MEAS_UT_CD 
            ELSE '0'
       END REGU_VAL
      ,'Max Weight' MAX_WGT_NM
      ,'N/A' MAX_WGT_RSLT
      ,'PKG Type' PKG_TP_NM
      ,'N/A' PKG_TP_RSLT
  FROM SCG_IMDG_UN_NO S
 WHERE S.IMDG_UN_NO = @[imdg_un_no]
   AND S.IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
UNION ALL
SELECT 'Excepted Q'||''''||'TY' DIV_NM
      ,IMDG_EXPT_QTY_CD REGU_VAL
      ,'Max Weight' MAX_WGT_NM
      ,'N/A' MAX_WGT_RSLT
      ,'PKG Type' PKG_TP_NM
      ,'N/A' PKG_TP_RSLT
  FROM SCG_IMDG_UN_NO S
 WHERE S.IMDG_UN_NO = @[imdg_un_no]
   AND S.IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
UNION ALL
SELECT 'Packing Instruction' DIV_NM
      ,(SELECT N1ST_IMDG_PCK_INSTR_CD||NVL2(N2ND_IMDG_PCK_INSTR_CD, ', '||N2ND_IMDG_PCK_INSTR_CD, NULL)
       ||NVL2(N3RD_IMDG_PCK_INSTR_CD, ', '||N3RD_IMDG_PCK_INSTR_CD, NULL)
       ||NVL2(N1ST_IMDG_IBC_INSTR_CD, ', '||N1ST_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N2ND_IMDG_IBC_INSTR_CD, ', '||N2ND_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N3RD_IMDG_IBC_INSTR_CD, ', '||N3RD_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N4TH_IMDG_IBC_INSTR_CD, ', '||N4TH_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N5TH_IMDG_IBC_INSTR_CD, ', '||N5TH_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N1ST_IMDG_PCK_PROVI_CD, '('||N1ST_IMDG_PCK_PROVI_CD
       ||NVL2(N2ND_IMDG_PCK_PROVI_CD, ', '||N2ND_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N3RD_IMDG_PCK_PROVI_CD, ', '||N3RD_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N4TH_IMDG_PCK_PROVI_CD, ', '||N4TH_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N5TH_IMDG_PCK_PROVI_CD, ', '||N5TH_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N1ST_IMDG_IBC_PROVI_CD, ', '||N1ST_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N2ND_IMDG_IBC_PROVI_CD, ', '||N2ND_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N3RD_IMDG_IBC_PROVI_CD, ', '||N3RD_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N4TH_IMDG_IBC_PROVI_CD, ', '||N4TH_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N5TH_IMDG_IBC_PROVI_CD, ', '||N5TH_IMDG_IBC_PROVI_CD, NULL)||')', NULL)
  FROM SCG_IMDG_UN_NO S
 WHERE S.IMDG_UN_NO = @[imdg_un_no]
   AND S.IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
) REGU_VAL
      ,'Max Weight' MAX_WGT_NM
      ,(SELECT CASE WHEN C_CNT > 0 THEN 'N/A'
                    WHEN W_CNT > 0 THEN 'Invalid'
               ELSE 'Valid' END
          FROM (SELECT COUNT(CASE WHEN SUBSTR(VLD_CHK, 0, 1) = 'C' THEN 1 END) C_CNT
                      ,COUNT(CASE WHEN SUBSTR(VLD_CHK, 0, 1) = 'W' THEN 1 END) W_CNT
                  FROM VLD_CHK_TBL
               )
       ) MAX_WGT_RSLT
      ,'PKG Type' PKG_TP_NM
      ,(SELECT CASE WHEN C_CNT > 0 THEN 'N/A'
                    WHEN P_CNT > 0 THEN 'Invalid'
               ELSE 'Valid' END
          FROM (SELECT COUNT(CASE WHEN SUBSTR(VLD_CHK, 0, 1) = 'C' THEN 1 END) C_CNT
                      ,COUNT(CASE WHEN SUBSTR(VLD_CHK, 0, 1) = 'P' THEN 1 END) P_CNT
                  FROM VLD_CHK_TBL
               )
       ) PKG_TP_RSLT
  FROM DUAL			]]></sql>
			<params>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
