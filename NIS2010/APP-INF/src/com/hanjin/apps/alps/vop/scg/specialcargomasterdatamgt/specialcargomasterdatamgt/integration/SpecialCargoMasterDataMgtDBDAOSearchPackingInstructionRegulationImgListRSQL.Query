<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchPackingInstructionRegulationImgListRSQL">
			<desc><![CDATA[Image File List 조회]]></desc>
			<sql><![CDATA[
SELECT B.IMDG_PCK_INSTR_CD,
    B.IMDG_PCK_INSTR_SEQ,
    B.REGU_DP_NO,
    B.FILE_NM,
    TO_CHAR(B.UPD_DT, 'YYYY/MM/DD HH24:MI:SS') UPD_DT,
	B.SUB_SEQ
FROM 
SCG_PCK_REGU_IMG B,
SCG_PCK_REGU A
WHERE 1 = 1
AND A.IMDG_PCK_INSTR_CD = B.IMDG_PCK_INSTR_CD
AND A.IMDG_PCK_INSTR_SEQ = B.IMDG_PCK_INSTR_SEQ
AND A.REGU_DP_NO = B.REGU_DP_NO
#if (${imdg_pck_instr_cd} != '') 
AND   B.IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
#end
#if (${imdg_pck_instr_seq} != '') 
AND   B.IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]
#end
#if (${regu_dp_no} != '') 
AND   B.REGU_DP_NO = @[regu_dp_no]
#end
AND B.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
				<param name="regu_dp_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
