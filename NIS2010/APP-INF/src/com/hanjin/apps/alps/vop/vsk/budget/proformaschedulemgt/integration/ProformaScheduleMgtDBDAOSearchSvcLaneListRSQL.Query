<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOSearchSvcLaneListRSQL">
			<desc><![CDATA[등록된 Proforma SKD 정보를 조회한다.

* History
* 2011.02.22 진마리아 CHM-201108456-01 사업계획용 스케줄 정보 노출 대응을 위한 시스템 업데이트건]]></desc>
			<sql><![CDATA[
SELECT 
	VSL_SLAN_CD,
	VSL_SLAN_NM,
	VSL_SVC_TP_CD,
	VSL_TP_CD,
	ST_EFF_DT,
	END_EFF_DT,
	VSL_SLAN_SKD_TP_CD,
	OFC_CD,
	CO_CD,
	FDR_DIV_CD,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT,
	DELT_FLG,
	EAI_EVNT_DT,
	CNL_AGN_VNDR_SEQ,
	VSKD_FLET_GRP_CD,
	SPCL_CGO_RQST_TGT_LANE_FLG,
	TML_PROD_RPT_FLG,
	PNDLM_FLG
FROM MDM_VSL_SVC_LANE
where 1 = 1
AND VSL_SLAN_CD LIKE UPPER(@[vsl_slan_cd]) ||'%'
AND DELT_FLG = 'N'
AND VSL_TP_CD = 'C' /*컨테이너선*/
ORDER BY VSL_SLAN_CD			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
