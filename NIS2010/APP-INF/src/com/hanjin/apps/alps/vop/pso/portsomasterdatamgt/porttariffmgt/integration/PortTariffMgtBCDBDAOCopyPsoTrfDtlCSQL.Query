<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOCopyPsoTrfDtlCSQL">
			<desc><![CDATA[CopyPsoTrfDtl]]></desc>
			<sql><![CDATA[
INSERT INTO PSO_TRF_DTL
   ( PORT_TRF_NO,
     TRF_SEQ,
     FM_VAL,
     TO_VAL,
     TRF_RT_AMT,
     COND_NO,
     CRE_USR_ID,
     CRE_DT,
     UPD_USR_ID,
     UPD_DT)
   SELECT @[new_pso_trf_no],
          TRF_SEQ,
          FM_VAL,
          TO_VAL,
          TRF_RT_AMT,
          COND_NO,
          @[cre_usr_id],
          SYSDATE,
          @[cre_usr_id],
          SYSDATE  
    FROM PSO_TRF_DTL
   WHERE PORT_TRF_NO =  @[pso_trf_no]			]]></sql>
			<params>
				<param name="new_pso_trf_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="pso_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
