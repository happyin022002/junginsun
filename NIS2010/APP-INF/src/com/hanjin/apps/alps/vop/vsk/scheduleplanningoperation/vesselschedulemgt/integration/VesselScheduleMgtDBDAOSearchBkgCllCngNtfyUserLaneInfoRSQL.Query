<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchBkgCllCngNtfyUserLaneInfoRSQL">
			<desc><![CDATA[BKG_CLL_CNG_NTFY_SET 테이블에서 LANE에 대한 User Information 추출]]></desc>
			<sql><![CDATA[
SELECT    X.SLAN_CD
       ,  X.DIR_CD  
       ,  X.OFC_CD    
       ,  X.RCVR_USR_ID
       ,  (SELECT C.USR_NM FROM COM_USER C WHERE C.USR_ID = X.RCVR_USR_ID)	AS USR_NM
       ,  X.RCVR_EML
       ,  X.OFC_PHN_NO
       ,  X.OFC_FAX_NO
FROM      BKG_CLL_CNG_NTFY_SET   X
WHERE     1 = 1
AND	      X.OFC_CD			     IN ('PUSBO','KANKS','INCKS')
AND	      X.PORT_CD              = @[vps_port_cd]
AND       X.SLAN_CD              = @[slan_cd]  
AND       X.DIR_CD               = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="CAX" out="N"/>
				<param name="skd_dir_cd" type="12" value="E" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
