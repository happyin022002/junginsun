<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGExternalFinderDBDAOSearchVVDVORSQL">
			<desc><![CDATA[VVD 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
     vs.vsl_cd
   , vs.skd_voy_no
   , vs.skd_dir_cd
   , mc.vsl_eng_nm
   , vs.vsl_slan_cd
   , ml.vsl_slan_nm
   , mc.crr_cd vsl_opr_tp_cd
   , vc.crr_nm vsl_opr_tp_nm
FROM vsk_vsl_skd vs
   , mdm_vsl_cntr mc
   , mdm_carrier vc
   , mdm_vsl_svc_lane ml
WHERE vs.vsl_cd = @[vsl_cd]
AND	vs.skd_voy_no = @[skd_voy_no]
AND	vs.skd_dir_cd = @[skd_dir_cd]
AND vs.vsl_cd = mc.vsl_cd
AND mc.crr_cd = vc.crr_cd
AND	vs.vsl_slan_cd = ml.vsl_slan_cd			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
