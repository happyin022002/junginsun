<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalInformationMgtDBDAOVskPortGntrCrnSpclCgoHndlInfoAttachFileRSQL">
			<desc><![CDATA[G/Crane 작업시 Special Cargo Handling Information에 대한 첨부파일 조회]]></desc>
			<sql><![CDATA[
SELECT    	X.LOC_CD			AS PORT_CD
       	,  	X.ATCH_FILE_SEQ
       	,  	X.FILE_NM
       	,  	X.FILE_SAV_ID
       	,  	X.CRE_USR_ID
       	,  	X.CRE_DT
       	,  	X.UPD_USR_ID
       	,  	X.UPD_DT
		,	(SELECT COUNT(XX.ATCH_FILE_SEQ) FROM VSK_PORT_GNTR_HNDL_FILE XX WHERE XX.LOC_CD = @[port_cd])	AS ATCH_FILE_KNT
FROM      	VSK_PORT_GNTR_HNDL_FILE   X
WHERE     	X.LOC_CD                  = @[port_cd]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
