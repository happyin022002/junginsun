<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOInsertVnorAttachFileCSQL">
			<desc><![CDATA[VNOR 의 File Attached 건을 생성합니다.]]></desc>
			<sql><![CDATA[
INSERT INTO OPF_ATCH_FILE (
	ATCH_FILE_LNK_ID,
	ATCH_FILE_LNK_SEQ,
	FILE_SAV_ID,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
) VALUES (
	@[atch_file_lnk_id],
	@[atch_file_lnk_seq],
	@[file_sav_id],
	@[cre_usr_id],
	SYSDATE,
	@[cre_usr_id],
	SYSDATE
)			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="atch_file_lnk_seq" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
