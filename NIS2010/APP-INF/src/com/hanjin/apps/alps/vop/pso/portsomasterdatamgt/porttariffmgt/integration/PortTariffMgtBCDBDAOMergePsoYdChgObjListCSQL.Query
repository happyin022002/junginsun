<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOMergePsoYdChgObjListCSQL">
			<desc><![CDATA[PSO_YD_CHG_OBJ_LIST <merge>]]></desc>
			<sql><![CDATA[
MERGE INTO PSO_YD_CHG_OBJ_LIST A
USING DUAL ON (A.YD_CHG_NO 			= @[yd_chg_no]
               AND A.YD_CHG_VER_SEQ = @[yd_chg_ver_seq]
               AND A.OBJ_LIST_NO 	= @[obj_list_no])
WHEN MATCHED
THEN UPDATE SET 
            A.DFLT_CTNT  = @[dflt_ctnt]
           ,A.DFLT_VAL   = @[dflt_val] 
           ,A.DFLT_FLG   = @[dflt_flg]
           ,A.UPD_USR_ID = @[cre_usr_id]
           ,A.UPD_DT     = SYSDATE
     WHERE  1=1
     AND    A.YD_CHG_NO = @[yd_chg_no]
     AND    A.YD_CHG_VER_SEQ = @[yd_chg_ver_seq]
     AND    A.OBJ_LIST_NO = @[obj_list_no]
WHEN NOT MATCHED
THEN INSERT (A.YD_CHG_NO
            ,A.YD_CHG_VER_SEQ
            ,A.OBJ_LIST_NO
            ,A.DFLT_CTNT
            ,A.DFLT_VAL
            ,A.DFLT_FLG
            ,A.CRE_USR_ID
            ,A.CRE_DT
            ,A.UPD_USR_ID
            ,A.UPD_DT)
     VALUES (@[yd_chg_no]     
            ,@[yd_chg_ver_seq]
            ,@[obj_list_no]   
            ,@[dflt_ctnt]     
            ,@[dflt_val]      
            ,@[dflt_flg]      
            ,@[cre_usr_id]    
            ,SYSDATE        
            ,@[cre_usr_id]    
            ,SYSDATE        
     )			]]></sql>
			<params>
				<param name="yd_chg_no" type="12" value="" out="N"/>
				<param name="yd_chg_ver_seq" type="12" value="" out="N"/>
				<param name="obj_list_no" type="12" value="" out="N"/>
				<param name="dflt_ctnt" type="12" value="" out="N"/>
				<param name="dflt_val" type="12" value="" out="N"/>
				<param name="dflt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
