<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualScheduleMgtDBDAOSearchHjsVvdByReceviedHjsVoyDirAllRSQL">
			<desc><![CDATA[수신된 SML Voyage, SML Directon, Call Sign No, Yard Code를 이용한 SML에 맵핑되는 모든 VVD를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  VSL_CD, SKD_VOY_NO, SKD_DIR_CD, YD_CD
FROM    VSK_VSL_PORT_SKD
WHERE   VSL_CD      = @[edi_vsl_nm]
AND     SKD_VOY_NO  = @[edi_skd_voy_no]
AND     SKD_DIR_CD  = @[edi_skd_dir_nm]
AND     VPS_PORT_CD = @[vps_port_cd]
AND     'S'        != NVL(SKD_CNG_STS_CD, ' ')
UNION
SELECT VSL_CD, SKD_VOY_NO, SKD_DIR_CD, YD_CD
FROM   VSK_VSL_PORT_SKD
WHERE  VSL_CD  IN
(
    SELECT  VSL_CD
    FROM    MDM_VSL_CNTR
    WHERE   CALL_SGN_NO = @[call_sgn_no]
    AND     DELT_FLG    = 'N'
)
AND    SKD_VOY_NO   = @[edi_skd_voy_no]
AND    SKD_DIR_CD   = @[edi_skd_dir_nm]
AND    VPS_PORT_CD  = @[vps_port_cd]
AND    'S'         != NVL(SKD_CNG_STS_CD, ' ')
UNION
SELECT  VSL_CD, SKD_VOY_NO, SKD_DIR_CD, YD_CD
FROM    VSK_VSL_PORT_SKD
WHERE   VSL_CD IN
(
    SELECT  VSL_CD
    FROM    MDM_VSL_CNTR
    WHERE   LLOYD_NO      = @[lloyd_no]
    AND     DELT_FLG      = 'N'
)
AND    SKD_VOY_NO  = @[edi_skd_voy_no]
AND    SKD_DIR_CD  = @[edi_skd_dir_nm]
AND    VPS_PORT_CD = @[vps_port_cd]
AND    'S'        != NVL(SKD_CNG_STS_CD, ' ')
GROUP BY VSL_CD, SKD_VOY_NO, SKD_DIR_CD, YD_CD			]]></sql>
			<params>
				<param name="edi_vsl_nm" type="12" value="" out="N"/>
				<param name="edi_skd_voy_no" type="12" value="" out="N"/>
				<param name="edi_skd_dir_nm" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="call_sgn_no" type="12" value="" out="N"/>
				<param name="lloyd_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
