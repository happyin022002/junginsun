<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchPfSkdDetailRSQL">
			<desc><![CDATA[Vessel Lane Code, P/F Service Type Code가 일치하는 Proforma 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
	T1.VSL_SLAN_CD
,	T1.PF_SVC_TP_CD
,	T1.SLAN_STND_FLG
,	T1.SVC_DUR_DYS
,	T1.STND_SVC_SPD
,	T1.BRTH_ITVAL_DYS
,	T1.MML_USD_FLG
,	T1.SIM_DT
,	T1.SIM_NO
,	T1.N1ST_VSL_CLSS_CD
,	T1.N1ST_VSL_CLSS_KNT
,	T1.N2ND_VSL_CLSS_CD
,	T1.N2ND_VSL_CLSS_KNT
,	T1.N3RD_VSL_CLSS_CD
,	T1.N3RD_VSL_CLSS_KNT
,	T1.CLPT_KNT
,	T1.TTL_DIST
,	T1.MAX_SPD
,	T1.AVG_SPD
,	T1.DELT_FLG
,	T1.PF_SKD_RMK
,	T2.PORT_CD
,	T2.SKD_DIR_CD
,	T2.CLPT_SEQ
,	T2.PORT_ROTN_SEQ
,	T2.YD_CD
,	T2.CALL_YD_IND_SEQ
,	T2.TURN_PORT_FLG
,	T2.TURN_PORT_IND_CD
,	T2.ETB_DY_CD
,	T2.ETB_DY_NO
,	T2.ETB_TM_HRMNT
,	T2.ETD_DY_CD
,	T2.ETD_DY_NO
,	T2.ETD_TM_HRMNT
,	T2.LNK_DIST
,	T2.LNK_SPD
,	T2.TZTM_HRS
,	T2.SEA_BUF_HRS
,	T2.SEA_BUF_SPD
,	T2.MNVR_IN_HRS
,	T2.MNVR_OUT_HRS
,	T2.IB_IPCGO_QTY
,	T2.IB_OCN_CGO_QTY
,	T2.OB_IPCGO_QTY
,	T2.OB_OCN_CGO_QTY
,	T2.TML_PROD_QTY
,	T2.CRN_KNT
,	T2.ACT_WRK_HRS
,	T2.PORT_BUF_HRS
,	T2.CRE_USR_ID
,	T2.CRE_DT
,	T2.UPD_USR_ID
,	T2.UPD_DT
FROM VSK_PF_SKD T1, VSK_PF_SKD_DTL T2
WHERE 1=1
AND T1.VSL_SLAN_CD=@[vsl_slan_cd]
AND T1.PF_SVC_TP_CD=@[pf_svc_tp_cd]
AND T1.VSL_SLAN_CD=T2.VSL_SLAN_CD
AND T1.PF_SVC_TP_CD=T2.PF_SVC_TP_CD
ORDER BY PORT_ROTN_SEQ			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="pf_svc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
