<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOSearchVnorSummaryOtItmRmkRSQL">
			<desc><![CDATA[VNOR Summary Other Loss Item Remark 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT VNOR_ITM_RMK
FROM OPF_VNOR_ITM
WHERE VSL_CD = @[vsl_cd]
AND VNOR_SEQ = @[vnor_seq]
AND VNOR_ITM_CD <> 'OH'
ORDER BY VNOR_ITM_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vnor_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
