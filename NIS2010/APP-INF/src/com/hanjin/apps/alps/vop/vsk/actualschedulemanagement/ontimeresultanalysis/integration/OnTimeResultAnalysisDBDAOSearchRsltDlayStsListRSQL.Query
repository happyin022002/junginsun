<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnTimeResultAnalysisDBDAOSearchRsltDlayStsListRSQL">
			<desc><![CDATA[SearchRsltDlayStsList]]></desc>
			<sql><![CDATA[
SELECT  DECODE(T22.GRP, 'ZZZZZZZ', 'TOTAL', T22.GRP) AS LANE, DECODE(NO, 1, 'CNT', 2, 'HRS') AS CNT_HRS
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 01, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS SBW
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 02, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS SMT
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 03, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS SVD
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 04, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS SDA
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 05, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WAD
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 06, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WPG
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 07, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WPC
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 08, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WPV
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 09, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WMT
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 10, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WCA
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 11, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WIO
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 12, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS WNH
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 13, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS OTH
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 14, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BTT
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 15, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BLS
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 16, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BDA
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 17, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BNS
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 18, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BCW
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 19, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BCM
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 20, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BRB
        ,MAX(DECODE(INTG_CD_VAL_DP_SEQ, 21, DECODE(NO, 1, TO_CHAR(DELAY_CNT, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_CNT / TTL_DLY_CNT) * 100, 1), '990.0') || '%' , 2, TO_CHAR(DELAY_TM, '999,999') ||CHR(10)|| TO_CHAR(ROUND((DELAY_TM / TTL_DLY_TM) * 100, 1), '990.0') || '%' ))) AS BSP
        ,MAX(DECODE(NO, 1, TO_CHAR(TTL_DLY_CNT, '999,999') ||CHR(10)|| '100' || '%'  , 2, TO_CHAR(TTL_DLY_TM, '999,999') ||CHR(10)||'100' || '%' )) AS TTL
        ,MAX(DECODE(NO, 1, 'ARR', 2, 'DEP', 3, 'TTL')) AS ARR_DEP
        ,MAX(DECODE(NO, 1, TO_CHAR(ARR, '999,999'), 2, TO_CHAR(DEP, '999,999'), 3, TO_CHAR(ARR + DEP, '999,999') )) AS CALL_CNT
        ,MAX(DECODE(NO, 1, TO_CHAR(EXC_BER, '999,999')           ||CHR(10)|| TO_CHAR(ROUND(DECODE(ARR, 0, 0, (EXC_BER / ARR) * 100), 1)                        , '990.0') || '%',
                        2, TO_CHAR(EXC_DEP, '999,999')           ||CHR(10)|| TO_CHAR(DECODE(DEP, 0, 0, ROUND((EXC_DEP / DEP) * 100, 1))                        , '990.0') || '%', 
                        3, TO_CHAR(EXC_BER + EXC_DEP, '999,999') ||CHR(10)|| TO_CHAR(ROUND(DECODE((ARR + DEP), 0, 0, (EXC_BER + EXC_DEP)/(ARR + DEP) * 100, 1)), '990.0') || '%')) AS ONTM_CNT
FROM    (
        SELECT  1
                , DECODE(GROUPING(GRP), 0, GRP, 1,'ZZZZZZZ')     AS GRP
                , INTG_CD_VAL_CTNT
                , INTG_CD_VAL_DP_SEQ
                , SUM(DELAY_CNT)                                 AS DELAY_CNT
                , SUM(DELAY_TM )                                 AS DELAY_TM
                , SUM(SUM(DELAY_CNT)) OVER (PARTITION BY GRP)    AS TTL_DLY_CNT /*GRP별 DELAY COUNT SUM */
                , SUM(SUM(DELAY_TM )) OVER (PARTITION BY GRP)    AS TTL_DLY_TM  /*GRP별 DELAY TIME SUM  */
        FROM    (
                SELECT  RSN_CD1 || RSN_CD2 || RSN_CD3 ||RSN_CD4  AS RSN_CD
                        #if (${grp_flg} == 'P')
                        , T1.VPS_PORT_CD                         AS GRP
                        #end
                        #if (${grp_flg} == 'V')
                        , T1.VSL_CD                              AS GRP
                        #end
                        #if (${grp_flg} == 'L')
                        , T2.VSL_SLAN_CD                         AS GRP
                        #end
                        , SUM(DELAY_CNT)                         AS DELAY_CNT
                        , SUM(NVL(DLAY_HRS1, 0) + NVL(DLAY_HRS2, 0) + NVL(DLAY_HRS3, 0) + NVL(DLAY_HRS4, 0)) AS DELAY_TM
                FROM    (
                        SELECT  VSL_CD
                                , SKD_VOY_NO
                                , SKD_DIR_CD
                                , VPS_PORT_CD
                                , COUNT(*)                              AS DELAY_CNT
                                , DECODE(SEQ, 1, ARR_DLAY_RSN_CD1    )  AS RSN_CD1
                                , NVL(SUM(DECODE(SEQ, 1, ARR_DLAY_HRS1   )),0) AS DLAY_HRS1
                                , DECODE(SEQ, 2, ARR_DLAY_RSN_CD2    )  AS RSN_CD2
                                , NVL(SUM(DECODE(SEQ, 2, ARR_DLAY_HRS2   )),0) AS DLAY_HRS2
                                , DECODE(SEQ, 3, DEP_DLAY_RSN_CD1    )  AS RSN_CD3
                                , NVL(SUM(DECODE(SEQ, 3, DEP_DLAY_HRS1   )),0) AS DLAY_HRS3
                                , DECODE(SEQ, 4, DEP_DLAY_RSN_CD2    )  AS RSN_CD4
                                , NVL(SUM(DECODE(SEQ, 4, DEP_DLAY_HRS2   )),0) AS DLAY_HRS4
                        FROM    VSK_VSL_SKD_RSLT,
                                (
                                SELECT 1 SEQ FROM DUAL UNION ALL
                                SELECT 2     FROM DUAL UNION ALL
                                SELECT 3     FROM DUAL UNION ALL
                                SELECT 4     FROM DUAL
                                )
                        WHERE   ACT_INP_YRMON  >= TO_CHAR(TO_DATE(@[act_inp_fm_dt], 'YYYYMM'), 'YYYYMM')
                        AND     ACT_INP_YRMON  <= TO_CHAR(TO_DATE(@[act_inp_to_dt], 'YYYYMM'), 'YYYYMM')
                        AND     VSL_CD         LIKE @[vsl_cd]|| '%'
                        AND     VPS_PORT_CD    LIKE @[vps_port_cd]|| '%'
                        GROUP BY SEQ
                                  , VSL_CD
                                  , SKD_VOY_NO
                                  , SKD_DIR_CD
                                  , VPS_PORT_CD
                                  , ARR_DLAY_RSN_CD1
                                  , ARR_DLAY_RSN_CD2
                                  , DEP_DLAY_RSN_CD1
                                  , DEP_DLAY_RSN_CD2
                        ) T1, VSK_VSL_SKD T2, MDM_VSL_CNTR T3
                WHERE   T1.VSL_CD      = T2.VSL_CD
                AND     T1.SKD_VOY_NO  = T2.SKD_VOY_NO
                AND     T1.SKD_DIR_CD  = T2.SKD_DIR_CD
                AND     T1.VSL_CD      = T3.VSL_CD
                AND    ((DECODE(@[lane_grp], 'I', T2.VSL_SLAN_CD) = NVL(@[vsl_slan_cd], T2.VSL_SLAN_CD))  OR  (DECODE(@[lane_grp], 'G', T2.VSL_SLAN_CD) IN  (SELECT VSL_SLAN_CD FROM VSK_USR_LANE_GRP WHERE USR_ID = @[usr_id] AND LANE_GRP_NM = @[lane_grp_nm])))
                AND     T3.CRR_CD      LIKE @[crr_cd]|| '%'
                AND     RSN_CD1 || RSN_CD2 || RSN_CD3 ||RSN_CD4 IS NOT NULL
                GROUP BY RSN_CD1
                         , RSN_CD2
                         , RSN_CD3
                         , RSN_CD4
                         #if (${grp_flg} == 'P')
                         , T1.VPS_PORT_CD
                         #end
                         #if (${grp_flg} == 'V')
                         , T1.VSL_CD
                         #end
                         #if (${grp_flg} == 'L')
                         , T2.VSL_SLAN_CD
                         #end
                ) T11,
                COM_INTG_CD_DTL T12
        WHERE   T11.RSN_CD      = T12.INTG_CD_VAL_CTNT
        AND     T12.INTG_CD_ID  = 'CD01830'
        GROUP BY 1
                 , ROLLUP(GRP)
                 , INTG_CD_VAL_CTNT
                 , INTG_CD_VAL_DP_SEQ
        ) T21,
        (
        SELECT  1
                #if (${grp_flg} == 'P')
                , DECODE(DECODE(GROUPING(T1.VSL_CD||VPS_PORT_CD), 0, T1.VSL_CD||VPS_PORT_CD, 1, 'ZZZZZZZ'), 'ZZZZZZZ', 'ZZZZZZZ', 
                          SUBSTR(DECODE(GROUPING(T1.VSL_CD||VPS_PORT_CD), 0, T1.VSL_CD||VPS_PORT_CD, 1, 'ZZZZZZZ'), 5)) AS GRP
                #end
                #if (${grp_flg} == 'V')
                , DECODE(GROUPING(T1.VSL_CD  ), 0, T1.VSL_CD  , 1,'ZZZZZZZ') AS GRP
                #end
                #if (${grp_flg} == 'L')
                , DECODE(DECODE(GROUPING(T1.VSL_CD||VSL_SLAN_CD), 0, T1.VSL_CD||VSL_SLAN_CD, 1, 'ZZZZZZZ'), 'ZZZZZZZ', 'ZZZZZZZ', 
                          SUBSTR(DECODE(GROUPING(T1.VSL_CD||VSL_SLAN_CD), 0, T1.VSL_CD||VSL_SLAN_CD, 1, 'ZZZZZZZ'), 5)) AS GRP
                #end               
                , SUM(ARR) AS ARR
                , SUM(DEP) AS DEP
                , SUM(EXC_BER) AS EXC_BER
                , SUM(EXC_DEP) AS EXC_DEP
        FROM    (
                SELECT  1
                        , VSL_CD
                        , SKD_VOY_NO
                        , SKD_DIR_CD
                        , VPS_PORT_CD
                        , SUM(DECODE(VSKD_RSLT_XCLD_CD, 'A', 0, 'S', 0, 1)) AS ARR
                        , SUM(DECODE(VSKD_RSLT_XCLD_CD, 'D', 0, 'S', 0, 1)) AS DEP
                        , SUM(DECODE(VSKD_RSLT_XCLD_CD, 'A', 0, 'S', 0, DECODE(SIGN(0 - DECODE(@[ie_flg], 'I', INCL_BRTH_DLAY_HRS, XCLD_BRTH_DLAY_HRS)), -1, 0, 1))) AS EXC_BER
                        , SUM(DECODE(VSKD_RSLT_XCLD_CD, 'D', 0, 'S', 0, DECODE(SIGN(0 - DECODE(@[ie_flg], 'I', INCL_DEP_DLAY_HRS , XCLD_DEP_DLAY_HRS )), -1, 0, 1))) AS EXC_DEP
                FROM    VSK_VSL_SKD_RSLT
                WHERE   1               = 1
                AND     ACT_INP_YRMON  >= TO_CHAR(TO_DATE(@[act_inp_fm_dt], 'YYYYMM'), 'YYYYMM')
                AND     ACT_INP_YRMON  <= TO_CHAR(TO_DATE(@[act_inp_to_dt], 'YYYYMM'), 'YYYYMM')
                AND     VSL_CD         LIKE @[vsl_cd]|| '%'
                AND     VPS_PORT_CD    LIKE @[vps_port_cd]|| '%'
                GROUP BY VSL_CD
                         , SKD_VOY_NO
                         , SKD_DIR_CD
                         , VPS_PORT_CD
                ) T1,      
                VSK_VSL_SKD T2, MDM_VSL_CNTR T3
        WHERE   T1.VSL_CD      = T2.VSL_CD
        AND     T1.SKD_VOY_NO  = T2.SKD_VOY_NO
        AND     T1.SKD_DIR_CD  = T2.SKD_DIR_CD
        AND     T2.VSL_CD      = T3.VSL_CD
        AND    ((DECODE(@[lane_grp], 'I', T2.VSL_SLAN_CD) = NVL(@[vsl_slan_cd], T2.VSL_SLAN_CD))  OR  (DECODE(@[lane_grp], 'G', T2.VSL_SLAN_CD) IN  (SELECT VSL_SLAN_CD FROM VSK_USR_LANE_GRP WHERE USR_ID = @[usr_id] AND LANE_GRP_NM = @[lane_grp_nm])))
        AND     NVL(T2.ACT_CRR_CD, T3.CRR_CD)     LIKE @[crr_cd] || '%'
        #if (${grp_flg} == 'P')
        GROUP BY ROLLUP (T1.VSL_CD||VPS_PORT_CD)
        #end
        #if (${grp_flg} == 'V')
        GROUP BY ROLLUP (T1.VSL_CD)
        #end
        #if (${grp_flg} == 'L')
        GROUP BY ROLLUP (T1.VSL_CD||VSL_SLAN_CD)
        #end
        ) T22,
        (SELECT 1 NO FROM DUAL UNION ALL SELECT 2 FROM DUAL UNION ALL SELECT 3 FROM DUAL)
WHERE   T21.GRP (+) = T22.GRP
GROUP BY T22.GRP, NO
ORDER BY T22.GRP, NO			]]></sql>
			<params>
				<param name="act_inp_fm_dt" type="12" value="" out="N"/>
				<param name="act_inp_to_dt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="lane_grp" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="lane_grp_nm" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="ie_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
