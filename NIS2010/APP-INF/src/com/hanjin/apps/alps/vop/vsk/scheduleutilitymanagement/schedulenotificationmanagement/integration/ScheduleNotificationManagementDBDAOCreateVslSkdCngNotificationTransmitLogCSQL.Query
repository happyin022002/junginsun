<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScheduleNotificationManagementDBDAOCreateVslSkdCngNotificationTransmitLogCSQL">
			<desc><![CDATA[Vessel Schedule Change Notification 개인별설정에 따른 전송대상데이터 VSK_VSL_SKD_NTFC_TRSM_LOG 파일에 저장]]></desc>
			<sql><![CDATA[
INSERT   INTO VSK_VSL_SKD_NTFC_TRSM_LOG
              (
                 VSL_CD
              ,  SKD_VOY_NO
              ,  SKD_DIR_CD
              ,  VPS_PORT_CD
              ,  CLPT_IND_SEQ
              ,  USR_ID
              ,  NTFC_TRSM_HIS_SEQ
              --------------------------------------------
              ,  VVD_HIS_SEQ
              ,  HIS_DTL_SEQ
              ,  VSL_SLAN_CD
              ,  TRSM_MZD_CD
              ,  NTFC_TRSM_TP_CD
              ,  PF_ETA_DT
              ,  PF_ETB_DT
              ,  PF_ETD_DT
              ,  VPS_ETA_DT
              ,  VPS_ETB_DT
              ,  VPS_ETD_DT
              ,  PORT_SKP_TP_CD
              ,  BFR_YD_CD
              ,  CRNT_YD_CD
              ,  PAIR_RVS_PORT_CD
              ,  PAIR_RVS_CLPT_IND_SEQ
              --------------------------------------------
			  ,  RCVR_EML
			  ,  SNDR_EML
			  ,  EML_SND_NO
			  ,  TRSM_DT
              -------------------------------------------- 
              ,	 CRE_USR_ID
              ,	 CRE_DT
              ,	 UPD_USR_ID
              ,	 UPD_DT
              )
VALUES       (
			     @[vsl_cd] 							/* PK.VSL_CD				*/           
              ,  @[skd_voy_no]                      /* PK.SKD_VOY_NO            */
              ,  @[skd_dir_cd]                      /* PK.SKD_DIR_CD            */
              ,  @[vps_port_cd]                     /* PK.VPS_PORT_CD           */
              ,  @[clpt_ind_seq]                    /* PK.CLPT_IND_SEQ          */
              ,  @[usr_id]                          /* PK.USR_ID                */
              ,  VSK_NTFC_TRSM_HIS_SEQ.NEXTVAL      /* PK.NTFC_TRSM_HIS_SEQ     */
              --------------------------------------------------------------------
              ,  @[vvd_his_seq]						/* VVD_HIS_SEQ				*/
              ,  @[his_dtl_seq]                     /* HIS_DTL_SEQ              */
              ,  @[vsl_slan_cd]                     /* VSL_SLAN_CD              */
              ,  'EML'                              /* TRSM_MZD_CD              */
              ,  @[ntfc_trsm_tp_cd]
              ,  TO_DATE(@[pf_eta_dt]	,'YYYY-MM-DD HH24:MI')
              ,  TO_DATE(@[pf_etb_dt]	,'YYYY-MM-DD HH24:MI')
              ,  TO_DATE(@[pf_etd_dt]	,'YYYY-MM-DD HH24:MI')
              ,  TO_DATE(@[vps_eta_dt]	,'YYYY-MM-DD HH24:MI')
              ,  TO_DATE(@[vps_etb_dt]	,'YYYY-MM-DD HH24:MI')
              ,  TO_DATE(@[vps_etd_dt]	,'YYYY-MM-DD HH24:MI')
              ,  @[port_skp_tp_cd]
              ,  @[bfr_yd_cd]
              ,  @[crnt_yd_cd]
              ,  @[pair_rvs_port_cd]
              ,  @[pair_rvs_clpt_ind_seq]
              --------------------------------------------
			  ,  @[rcvr_eml]
			  ,  @[sndr_eml]
			  ,  @[eml_snd_no]
			  ,  SYSDATE							/* TRSM_DT  				*/
              -------------------------------------------- 
              ,  SUBSTR(NVL(@[his_cre_usr_id],'NO_ACCOUNT_USER_ID'),1,20)	/* CRE_USR_ID */
              ,  SYSDATE                            /* CRE_DT                   */
              ,  SUBSTR(NVL(@[his_cre_usr_id],'NO_ACCOUNT_USER_ID'),1,20)	/* UPD_USR_ID */
              ,  SYSDATE                            /* UPD_DT                   */
                  
              )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="vvd_his_seq" type="12" value="" out="N"/>
				<param name="his_dtl_seq" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="ntfc_trsm_tp_cd" type="12" value="" out="N"/>
				<param name="pf_eta_dt" type="12" value="" out="N"/>
				<param name="pf_etb_dt" type="12" value="" out="N"/>
				<param name="pf_etd_dt" type="12" value="" out="N"/>
				<param name="vps_eta_dt" type="12" value="" out="N"/>
				<param name="vps_etb_dt" type="12" value="" out="N"/>
				<param name="vps_etd_dt" type="12" value="" out="N"/>
				<param name="port_skp_tp_cd" type="12" value="" out="N"/>
				<param name="bfr_yd_cd" type="12" value="" out="N"/>
				<param name="crnt_yd_cd" type="12" value="" out="N"/>
				<param name="pair_rvs_port_cd" type="12" value="" out="N"/>
				<param name="pair_rvs_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="rcvr_eml" type="12" value="" out="N"/>
				<param name="sndr_eml" type="12" value="" out="N"/>
				<param name="eml_snd_no" type="12" value="" out="N"/>
				<param name="his_cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
