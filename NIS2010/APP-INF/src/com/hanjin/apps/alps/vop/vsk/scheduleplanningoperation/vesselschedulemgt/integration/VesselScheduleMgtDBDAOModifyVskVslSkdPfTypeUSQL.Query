<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOModifyVskVslSkdPfTypeUSQL">
			<desc><![CDATA[VVD의 P/F Type 정보를 수정합니다.]]></desc>
			<sql><![CDATA[
UPDATE VSK_VSL_SKD
SET PF_SKD_TP_CD = DECODE(@[pf_skd_tp_cd], '0', '', @[pf_skd_tp_cd]),
ACT_CRR_CD = @[act_crr_cd],
UPD_USR_ID = @[upd_usr_id],
UPD_DT = SYSDATE
WHERE 1=1
AND VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="pf_skd_tp_cd" type="12" value="" out="N"/>
				<param name="act_crr_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
