<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOCheckOfficeCodeRSQL">
			<desc><![CDATA[유효한 Office Code 인지 체크한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y') IS_OK
FROM MDM_ORGANIZATION
WHERE OFC_CD = @[office_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="office_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
