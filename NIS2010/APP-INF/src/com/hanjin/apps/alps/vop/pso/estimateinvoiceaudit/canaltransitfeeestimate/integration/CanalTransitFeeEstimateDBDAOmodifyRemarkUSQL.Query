<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanalTransitFeeEstimateDBDAOmodifyRemarkUSQL">
			<desc><![CDATA[modifyRemark]]></desc>
			<sql><![CDATA[
update pso_cnl_tz_fee_dtl
set  diff_rmk = @[diff_rmk]
where 
pso_bztp_cd = '5'
and VSL_CD = substr(@[vvd], 1, 4)
AND SKD_VOY_NO = substr(@[vvd], 5, 4)
AND SKD_DIR_CD = substr(@[vvd], 9)
and yd_cd = @[yd_cd]
and call_seq = @[call_seq]
and lgs_cost_cd = @[lgs_cost_cd]			]]></sql>
			<params>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="call_seq" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
