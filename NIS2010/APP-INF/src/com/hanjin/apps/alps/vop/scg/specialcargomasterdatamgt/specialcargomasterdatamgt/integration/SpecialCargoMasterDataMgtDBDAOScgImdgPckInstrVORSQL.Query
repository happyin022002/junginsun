<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgPckInstrVORSQL">
			<desc><![CDATA[Packing Instructions/Provisions (Creation) 의 조회]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_PCK_INSTR_CD
,	IMDG_PCK_INSTR_SEQ
,	FILE_SAV_ID
,	FILE_NM
,	CRE_USR_ID
,	TO_CHAR(CRE_DT,'YYYYMMDD') AS CRE_DT
,	UPD_USR_ID
,	TO_CHAR(UPD_DT,'YYYYMMDD') AS UPD_DT
,	IMDG_PCK_INSTR_CD AS OLD_IMDG_PCK_INSTR_CD
,   '' FILE_SET_YN
,   IMDG_PCK_INSTR_SUB_SEQ
,   PCK_DESC
FROM SCG_IMDG_PCK_INSTR
WHERE 1=1	
#if (${imdg_pck_instr_cd} != '') 
AND IMDG_PCK_INSTR_CD LIKE @[imdg_pck_instr_cd]||'%'
#end
#if (${imdg_pck_instr_seq} != '') 
AND IMDG_PCK_INSTR_SEQ  = TO_NUMBER(@[imdg_pck_instr_seq])
#else
--AND IMDG_PCK_INSTR_SEQ  = 2
AND IMDG_PCK_INSTR_SEQ  = (SELECT MAX(N1ST_IMDG_PCK_INSTR_SEQ) FROM SCG_IMDG_UN_NO)
#end
ORDER BY IMDG_PCK_INSTR_CD			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
