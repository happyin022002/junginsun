<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOCBFIFSummaryPodUSQL">
			<desc><![CDATA[OwnContainerBookingForecastMgtDBDAOCBFIFSummaryPod]]></desc>
			<sql><![CDATA[
UPDATE OPF_CGO_BKG_FCAST_SMRY
SET    BKG_20FT_QTY = @[bkg_20ft_qty]
       , BKG_40FT_QTY = @[bkg_40ft_qty]
       , BKG_40FT_HC_QTY = @[bkg_40ft_hc_qty]
       , BKG_45FT_HC_QTY = @[bkg_45ft_hc_qty]
	   , UPD_USR_ID = @[upd_usr_id]
	   , UPD_DT = SYSDATE
 WHERE VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]
   AND YD_CD = @[yd_cd]
   AND POL_CLPT_IND_SEQ = @[pol_clpt_ind_seq]
   AND CRR_CD     = @[crr_cd]
   AND POD_CD = @[pod_cd]
   AND BLCK_STWG_CD = @[blck_stwg_cd]			]]></sql>
			<params>
				<param name="bkg_20ft_qty" type="12" value="" out="N"/>
				<param name="bkg_40ft_qty" type="12" value="" out="N"/>
				<param name="bkg_40ft_hc_qty" type="12" value="" out="N"/>
				<param name="bkg_45ft_hc_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="blck_stwg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
