<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselPositionPollingManagementDBDAOSelectPositionPollingHeaderRSQL">
			<desc><![CDATA[SelectPositionPollingHeader]]></desc>
			<sql><![CDATA[
SELECT    X.RCV_DT
        , X.DLY_RCV_SEQ
        , X.RCV_CTNT
        , X.CRE_USR_ID
        , X.CRE_DT
        , X.UPD_USR_ID
        , X.UPD_DT
        , X.FILE_NM 
FROM    VSK_VSL_PSN_PLNG_HDR X
WHERE	RCV_DT = @[rcv_dt]
AND		DLY_RCV_SEQ = @[dly_rcv_seq]			]]></sql>
			<params>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="dly_rcv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
