<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgRgnShpOprCdRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT    A.LOC_CD,A.RGN_SHP_OPR_CD,A.DELT_FLG,A.CRE_USR_ID
          ,A.CRE_DT,A.UPD_USR_ID,A.UPD_DT
  FROM    SCG_RGN_SHP_OPR_PORT A
 WHERE    A.LOC_CD = @[loc_cd]  
   AND    A.RGN_SHP_OPR_CD = @[rgn_shp_opr_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
