<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOCBFIFCgoGrsWgtUSQL">
			<desc><![CDATA[OwnContainerBookingForecastMgtDBDAOCBFIFCgoGrsWgt]]></desc>
			<sql><![CDATA[
MERGE INTO OPF_CGO_BKG_FCAST_WGT_SMRY K USING DUAL
ON( K.VSL_CD        = @[vsl_cd]
   AND K.SKD_VOY_NO = @[skd_voy_no]
   AND K.SKD_DIR_CD = @[skd_dir_cd]
   AND K.YD_CD||POL_CLPT_IND_SEQ = @[yd_cd]
   AND K.CRR_CD     = @[crr_cd]    
) 
WHEN MATCHED THEN
    UPDATE
       SET CGO_GRS_WGT  = @[cgo_grs_wgt]
	   , CRE_USR_ID     = @[upd_usr_id]
	   , CRE_DT         = SYSDATE
	   , UPD_USR_ID     = @[upd_usr_id]
	   , UPD_DT         = SYSDATE
WHEN NOT MATCHED THEN
    INSERT(VSL_CD
         , SKD_VOY_NO
         , SKD_DIR_CD
         , YD_CD
         , POL_CLPT_IND_SEQ
         , CRR_CD
         , CGO_GRS_WGT
         , CRE_USR_ID
         , CRE_DT
         , UPD_USR_ID
         , UPD_DT
    )
    VALUES
    (      @[vsl_cd]
         , @[skd_voy_no]
         , @[skd_dir_cd]
         , @[pol_cd]
         , @[pol_clpt_ind_seq]
         , @[crr_cd]
         , @[cgo_grs_wgt]
         , @[cre_usr_id]                                                               
         , SYSDATE
         , @[upd_usr_id]
         , SYSDATE
    )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="cgo_grs_wgt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
