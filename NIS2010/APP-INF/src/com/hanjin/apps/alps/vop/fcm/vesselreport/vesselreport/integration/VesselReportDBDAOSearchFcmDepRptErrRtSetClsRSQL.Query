<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselReportDBDAOSearchFcmDepRptErrRtSetClsRSQL">
			<desc><![CDATA[FCM DEPARTURE REPORT ERROR RATE SETTING 조회 쿼리 - FcmDepRptErrClsVO 형태]]></desc>
			<sql><![CDATA[
SELECT VSL_CD ,
       (
       	SELECT CNTR_DZN_CAPA
        FROM MDM_VSL_CNTR
        WHERE VSL_CD = T1.VSL_CD
        AND ROWNUM = 1 
       ) AS CNTR_DZN_CAPA,
       NVGT_ML_DIST_RT AS NVGT_ML_DIST_CTNT ,
       ENG_ML_DIST_RT AS ENG_ML_DIST_CTNT ,
       MNVR_IN_ML_DIST_RT AS MNVR_IN_ML_DIST_CTNT ,
       MNVR_OUT_ML_DIST_RT AS MNVR_OUT_ML_DIST_CTNT ,
       AVG_SPD_RT AS AVG_SPD_CTNT ,
       AVG_RPM_PWR_RT AS AVG_RPM_PWR_CTNT ,
       PRLR_PCH_RT AS AVG_PRLR_PCH_VAL ,
       SAIL_TM_RT AS SAIL_TM_HRS ,
       ARR_FOIL_RT AS ARR_FOIL_CTNT ,
       ARR_LOW_SULP_FOIL_RT AS ARR_LOW_SULP_FOIL_CTNT ,
       ARR_DOIL_RT AS ARR_DOIL_CTNT ,
       ARR_LOW_SULP_DOIL_RT AS ARR_LOW_SULP_DOIL_CTNT ,
       DEP_FOIL_RT AS DEP_FOIL_CTNT ,
       DEP_LOW_SULP_FOIL_RT AS DEP_LOW_SULP_FOIL_CTNT ,
       DEP_DOIL_RT AS DEP_DOIL_CTNT ,
       DEP_LOW_SULP_DOIL_RT AS DEP_LOW_SULP_DOIL_CTNT ,
       SEA_STMNG_MN_ENG_RT AS SEA_STMNG_MN_ENG_TTL_QTY ,
       PORT_TTL_RT AS AVG_PORT_TTL_QTY ,
       PORT_TTL_HR_RT_RT AS AVG_PORT_TTL_HR_QTY ,
       SB_ENG_DT_RT AS SB_ENG_DT ,
       PLT_IN_DT_RT AS PLT_IN_DT ,
       BFR_BRTH_ANK_DRP_DT_RT AS BFR_BRTH_ANK_DRP_DT ,
       BFR_BRTH_ANK_OFF_DT_RT AS BFR_BRTH_ANK_OFF_DT ,
       VPS_ETB_DT_RT AS VPS_ETB_DT ,
       CGO_WRK_ST_DT_RT AS CGO_WRK_ST_DT ,
       CGO_WRK_END_DT_RT AS CGO_WRK_END_DT ,
       VPS_ETD_DT_RT AS VPS_ETD_DT ,
       AFT_UNBRTH_ANK_DRP_DT_RT AS AFT_UNBRTH_ANK_DRP_DT ,
       AFT_UNBRTH_ANK_OFF_DT_RT AS AFT_UNBRTH_ANK_OFF_DT ,
       PLT_OUT_DT_RT AS PLT_OUT_DT ,
       RUP_DT_RT AS RUP_DT ,
       FCNTR_OBRD_TEU_RT AS FCNTR_OBRD_TEU_CTNT ,
       MCNTR_OBRD_TEU_RT AS MCNTR_OBRD_TEU_CTNT ,
       TTL_CNTR_OBRD_TEU_RT AS TTL_CNTR_OBRD_TEU_CTNT ,
       DEP_CGO_RT AS DEP_CGO_CTNT ,
       RF_CNTR_DCHG_KNT_RT AS RF_CNTR_DCHG_KNT_CTNT ,
       RF_CNTR_LOD_KNT_RT AS RF_CNTR_LOD_KNT_CTNT ,
       RF_CNTR_OBRD_KNT_RT AS RF_CNTR_OBRD_KNT_CTNT ,
       CRE_USR_ID ,
       CRE_DT ,
       UPD_USR_ID ,
       UPD_DT
  FROM FCM_DEP_RPT_ERR_RT_SET T1 WHERE VSL_CD = @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
