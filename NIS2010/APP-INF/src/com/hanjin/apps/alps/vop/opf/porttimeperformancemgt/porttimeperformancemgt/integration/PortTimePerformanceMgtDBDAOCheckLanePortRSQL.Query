<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTimePerformanceMgtDBDAOCheckLanePortRSQL">
			<desc><![CDATA[Lane, Port Validation
History------------------------------------------------------------------------------------
2012.02.20 김민아 [CHM-201215901-01] Port Time Reduction project 개발 (2차)
2015.08.17 김기원 CHM-201537021  조직코드 변경]]></desc>
			<sql><![CDATA[
SELECT  MAX(DECODE(GUBUN, 'LOC', ITEM)) AS VPS_PORT_CD
       ,MAX(DECODE(GUBUN, 'LAN', ITEM)) AS SLAN_CD
  FROM  (
        SELECT  'LOC' AS GUBUN
               ,DECODE(               
                         ( CASE WHEN NVL(ML.DELT_FLG,'N') <> 'N' OR ML.CALL_PORT_FLG <> 'Y' THEN ''
                                ELSE
                                CASE WHEN ML.CONTI_CD  IN ('E','F') AND ML.LOC_CD NOT IN('EGAIS','ZADUR','RUVVO')         THEN 'HAMRU'
                                     WHEN ML.CONTI_CD  = 'M'                                                              THEN 'NYCRA'
                                     WHEN ML.CONTI_CD = 'A' AND ML.SCONTI_CD = 'AF' THEN DECODE(ML.CNT_CD,'KR','SELIB','JP','TYOIB','SHARC')
                                     WHEN (ML.CONTI_CD = 'A' AND ML.SCONTI_CD <> 'AF') OR ML.LOC_CD IN ('EGAIS','ZADUR')  THEN 'SINRS'
									 WHEN ML.CONTI_CD  IN ('E') AND ML.LOC_CD = 'RUVVO' THEN 'VVOIA'
                                     ELSE ''
                                END
                           END)
                   , NULL, NULL, LOC_CD) AS ITEM
          FROM  MDM_LOCATION ML
         WHERE  1 = 1
           AND  CALL_PORT_FLG = 'Y'
           AND  DELT_FLG = 'N'
           AND  LOC_CD = UPPER(@[vps_port_cd])
        UNION
        SELECT  'LAN' AS GUBUN
               ,VSL_SLAN_CD AS ITEM
          FROM  MDM_VSL_SVC_LANE
         WHERE  1 = 1
           AND  DELT_FLG = 'N'
           AND  VSL_SLAN_CD = UPPER(@[slan_cd])
        )			]]></sql>
			<params>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
