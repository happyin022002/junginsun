<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOCreateVslSkdChangeHistoryCSQL">
			<desc><![CDATA[Vessel Schedule 최초생성시 이력테이블에 관련데이터 INSERT]]></desc>
			<sql><![CDATA[
INSERT INTO VSK_VSL_SKD_CNG_HIS  H
            (		H.VSL_CD
				,  	H.SKD_VOY_NO
				,  	H.SKD_DIR_CD
				,	H.VVD_HIS_SEQ
				---------------------------------------------------------------------
				,	H.VSKD_CNG_TP_CD
				,  	H.VSL_SLAN_CD
				,  	H.SKD_STS_CD
				,  	H.RUSE_PROHI_FLG
				,  	H.SKD_STS_MNL_FLG
				,  	H.SKD_VOY_TP_CD
				,  	H.SKD_USD_IND_CD
				,  	H.PF_SKD_TP_CD
				,  	H.ST_PORT_CD
				,  	H.N1ST_PORT_BRTH_DT
				,  	H.PSDO_VVD_CD
				,  	H.CO_CD
				,  	H.SKD_RMK
				,	H.DLS_EDI_SND_TGT_FLG
				,	H.ACT_CRR_CD

				,  	H.CRE_USR_ID
				,  	H.CRE_DT
				,	H.UPD_USR_ID
				,	H.UPD_DT
				
				,	H.HIS_CRE_USR_ID
				,	H.HIS_CRE_DT

				,	H.HIS_DELT_PROHI_FLG

				,	H.SKD_HIS_DESC
            )
	SELECT      	VS.VSL_CD
            	,  	VS.SKD_VOY_NO
            	,  	VS.SKD_DIR_CD
				,	@[vvd_his_seq]
				---------------------------------------------------------------------
				,	@[vskd_cng_tp_cd]
            	,  	VS.VSL_SLAN_CD
            	,  	VS.SKD_STS_CD
            	,  	VS.RUSE_PROHI_FLG
            	,  	VS.SKD_STS_MNL_FLG
            	,  	VS.SKD_VOY_TP_CD
            	,  	VS.SKD_USD_IND_CD
            	,  	VS.PF_SKD_TP_CD
            	,  	VS.ST_PORT_CD
            	,  	VS.N1ST_PORT_BRTH_DT
            	,  	VS.PSDO_VVD_CD
            	,  	VS.CO_CD
            	,	VS.SKD_RMK
            	,	VS.DLS_EDI_SND_TGT_FLG
            	,	VS.ACT_CRR_CD

            	,	VS.CRE_USR_ID
            	,	VS.CRE_DT
            	,	VS.UPD_USR_ID
            	,	VS.UPD_DT

				,	SUBSTR(@[his_cre_usr_id],1,20)
				,	SYSDATE

				,	'N'					AS HIS_DELT_PROHI_FLG

				,	SUBSTR(@[skd_his_desc],1,500)	/* SKD_HIS_DESC			*/

	FROM        	VSK_VSL_SKD          VS
	WHERE       	1 = 1
	AND         	VS.VSL_CD            = @[vsl_cd]
	AND         	VS.SKD_VOY_NO        = @[skd_voy_no]
	AND         	VS.SKD_DIR_CD        = @[skd_dir_cd]
			]]></sql>
			<params>
				<param name="vvd_his_seq" type="12" value="" out="N"/>
				<param name="vskd_cng_tp_cd" type="12" value="" out="N"/>
				<param name="his_cre_usr_id" type="12" value="" out="N"/>
				<param name="skd_his_desc" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
