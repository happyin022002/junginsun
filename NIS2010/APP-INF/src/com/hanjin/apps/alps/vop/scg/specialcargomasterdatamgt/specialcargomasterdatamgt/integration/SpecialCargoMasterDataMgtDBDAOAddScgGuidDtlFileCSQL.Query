<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOAddScgGuidDtlFileCSQL">
			<desc><![CDATA[AddScgGuidDtlFile]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_GUID_DTL_FILE 
     (SPCL_CGO_GUID_CD
    ,SPCL_CGO_GUID_SEQ
    ,SPCL_CGO_GUID_ATCH_FILE_SEQ
    ,FILE_NM
    ,FILE_SAV_ID
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT)
VALUES(
      @[spcl_cgo_guid_cd]
    , @[spcl_cgo_guid_seq]
    , (SELECT 
    	NVL(MAX(SPCL_CGO_GUID_ATCH_FILE_SEQ),0)+1 SPCL_CGO_GUID_ATCH_FILE_SEQ
        FROM SCG_GUID_DTL_FILE
        WHERE
        SPCL_CGO_GUID_CD    = @[spcl_cgo_guid_cd]
        AND SPCL_CGO_GUID_SEQ   = @[spcl_cgo_guid_seq])
    , @[file_nm]    
    , @[file_sav_id]
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE)			]]></sql>
			<params>
				<param name="spcl_cgo_guid_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_guid_seq" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
