<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchYardNameRSQL">
			<desc><![CDATA[Yard Code 정보로 Yard Name을 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT YD_NM
FROM MDM_YARD
WHERE YD_CD = @[yd_cd]			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
