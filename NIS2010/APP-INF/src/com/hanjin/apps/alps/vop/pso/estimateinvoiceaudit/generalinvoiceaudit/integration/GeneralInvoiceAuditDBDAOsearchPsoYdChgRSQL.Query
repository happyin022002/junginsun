<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOsearchPsoYdChgRSQL">
			<desc><![CDATA[Search tariff information.
=============================================================================
2012.03.14 CHM-201216319-01 진마리아 쿼리 정리 정돈]]></desc>
			<sql><![CDATA[
SELECT YD_CHG_NO
      ,YD_CHG_VER_SEQ
      ,CURR_CD
FROM   PSO_YD_CHG
WHERE  1=1
AND    LGS_COST_CD = @[lgs_cost_cd]
AND    YD_CD       = @[yd_cd]
AND    VNDR_SEQ    = @[vndr_seq]
AND    LST_FLG     = 'Y'
AND    TO_DATE(@[iss_dt], 'YYYY-MM-DD') BETWEEN EFF_DT AND EXP_DT			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="iss_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
