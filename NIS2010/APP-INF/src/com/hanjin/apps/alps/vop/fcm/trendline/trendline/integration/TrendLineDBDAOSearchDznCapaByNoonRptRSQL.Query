<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrendLineDBDAOSearchDznCapaByNoonRptRSQL">
			<desc><![CDATA[해당 기간의 Noon Rpt에 존재하는 Vesse의 Capa을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT M.CNTR_DZN_CAPA
FROM FCM_NOON_RPT T1, MDM_VSL_CNTR M
WHERE T1.VSL_CD = M.VSL_CD
#if (${trnd_line_fm_dt} != '' && ${trnd_line_to_dt} != '') 
AND T1.NOON_RPT_DT BETWEEN TO_DATE(@[trnd_line_fm_dt], 'YYYY-MM-DD') AND TO_DATE(@[trnd_line_to_dt], 'YYYY-MM-DD')+0.99999
#end
#if (${vsl_clss_sub_cd} != '')
AND T1.VSL_CD IN (SELECT VSL_CD FROM FCM_VSL_CNTR_RGST WHERE VSL_CLSS_SUB_CD=@[vsl_clss_sub_cd] AND NVL(TRND_LINE_USE_FLG,'X')<>'N')
#end
#if (${vsl_slan_cd} != '' && ${vsl_slan_cd} != 'A')
AND T1.VSL_SLAN_CD = @[vsl_slan_cd]
#end 
AND T1.VSL_CD IN (SELECT R.VSL_CD FROM FCM_VSL_CNTR_RGST R WHERE T1.VSL_CD = R.VSL_CD AND NVL(R.TRND_LINE_USE_FLG,' ')<>'N')
AND T1.MN_FOIL_CSM_QTY != '0'
AND T1.MN_FOIL_CSM_QTY IS NOT NULL
AND TO_CHAR(TO_NUMBER(SUBSTR(T1.SAIL_HRMNT, 1, 2))+(TO_NUMBER(SUBSTR(T1.SAIL_HRMNT, 3, 2))/60)) IN ('23','24','25')
AND ((T1.SLP_RT > -16) AND (T1.SLP_RT < 25))
AND TO_NUMBER(NVL(T1.WND_SCL_NO,0))<=7
AND TO_NUMBER(NVL(T1.SEA_STE_NO,0))<=7
AND T1.ENG_ML_DIST <> '0'
AND CNTR_DZN_CAPA IS NOT NULL
ORDER BY M.CNTR_DZN_CAPA			]]></sql>
			<params>
				<param name="trnd_line_fm_dt" type="12" value="" out="N"/>
				<param name="trnd_line_to_dt" type="12" value="" out="N"/>
				<param name="vsl_clss_sub_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
