<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselPositionPollingManagementDBDAOUpdatePositionPollingDetailListUSQL">
			<desc><![CDATA[UpdatePositionPollingDetailList]]></desc>
			<sql><![CDATA[
UPDATE   VSK_VSL_PSN_PLNG_DTL  
SET      SKD_VOY_NO             = @[skd_voy_no]
       , SKD_DIR_CD             = @[skd_dir_cd]
       , PLNG_GEN_DIFF_HRS      = @[plng_gen_diff_hrs]
       , VSL_PRE_LAT            = @[vsl_pre_lat]
       , VSL_PRE_LON            = @[vsl_pre_lon]
       , VSL_PRE_SPD            = @[vsl_pre_spd]
       , VSL_PRE_PROG_DIR_CTNT  = @[vsl_pre_prog_dir_ctnt]
	   , VSL_PLNG_DIST			= @[vsl_plng_dist]
WHERE   RCV_DT          = @[rcv_dt]
AND     DLY_RCV_SEQ     = @[dly_rcv_seq]
AND     IF_RCV_SEQ      = @[if_rcv_seq]			]]></sql>
			<params>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="plng_gen_diff_hrs" type="12" value="" out="N"/>
				<param name="vsl_pre_lat" type="12" value="" out="N"/>
				<param name="vsl_pre_lon" type="12" value="" out="N"/>
				<param name="vsl_pre_spd" type="12" value="" out="N"/>
				<param name="vsl_pre_prog_dir_ctnt" type="12" value="" out="N"/>
				<param name="vsl_plng_dist" type="12" value="" out="N"/>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="dly_rcv_seq" type="12" value="" out="N"/>
				<param name="if_rcv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
