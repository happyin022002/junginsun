<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalInformationMgtDBDAOVskPortGntrCrnSpclCgoHndlRmkCSQL">
			<desc><![CDATA[G/Crane에 대한 Special Cargo Handling Remark 정보 저장처리한다.]]></desc>
			<sql><![CDATA[
MERGE INTO  VSK_PORT_GNTR_HNDL_INFO X
USING
            (SELECT  SUBSTR(@[yd_cd],1,5)	AS LOC_CD
             FROM                   		DUAL
            ) Y
ON          (X.LOC_CD                		= Y.LOC_CD)
WHEN NOT MATCHED THEN
INSERT     (LOC_CD, SPCL_CGO_HNDL_RMK, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
VALUES     (
               SUBSTR(@[yd_cd],1,5)
             , @[spcl_cgo_hndl_rmk]
             , @[upd_usr_id]
             , SYSDATE
             , @[upd_usr_id]
             , SYSDATE
           ) 

WHEN MATCHED THEN               
UPDATE       
SET        	SPCL_CGO_HNDL_RMK	= @[spcl_cgo_hndl_rmk]
       	,  	UPD_USR_ID			= @[upd_usr_id]
		,	UPD_DT				= SYSDATE	    			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_hndl_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
