		<query name="SetupDBDAOFcmVslCntrRgstVORSQL">
			<desc><![CDATA[Vessel Registration 정보를 조회한다.

History
2012.04.04 진마리아 CHM-201216636-01 [FCM] ALPS 모델 및 DB 구조 불일치 복구
2012.04.19 진마리아 CHM-201217192-01 Vessel Registration내 선박 추가 로직 변경 요청건
2013.01.02 진마리아 CHM-201322241-01 FCM 사용 하지않는 소스 및 화면 정리]]></desc>
			<sql><![CDATA[
SELECT *
FROM 
(

SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T2.OWNR_NM
    ,T2.VSL_BLD_DT
    ,T2.VSL_DZND_CAPA
    ,T2.BSE_14TON_VSL_CAPA
    ,T2.FM_DT
    ,T2.TO_DT
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_VSL_CLSS_CAPA
    ,T1.VSL_CLSS_SUB_CD     
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_TP_DESC
    ,T1.MN_ENG_RPM_PWR
    ,T1.MN_ENG_BHP_PWR
    ,T1.TBCGR_NO
    ,T1.TBCGR_MKR_NM
    ,T1.TBCGR_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.TBCGR_COFF_FLG
    ,T1.SPR_AUX_BLW_MKR_NM
    ,T1.SPR_AUX_BLW_TP_DESC
    ,T1.SPR_AUX_BLW_NO
    ,T1.FOIL_ADTV_CD
    ,T1.HL_PNT_CD
    ,T1.GNR_CSM_WGT
    ,T1.FOIL_TNK_CBM_CAPA
    ,T1.FOIL_TNK_MT_CAPA
    ,T1.FOIL_STL_SVC_TNK_MT_CAPA
    ,T1.OP_MAX_SPD
    ,T1.OP_MIN_SPD
    ,T1.OP_GNR_SPD
    ,T1.TRND_LINE_USE_FLG
    ,T1.MNVR_CSM_WGT
    ,T1.CRE_DT
    ,T1.CRE_USR_ID
    ,T1.UPD_DT
    ,T1.UPD_USR_ID
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.VSL_CLSS_SUB_CD
    ,T2.MN_ENG_MKR_NM
    ,T2.MN_ENG_TP_DESC
    ,T2.MN_ENG_RPM_PWR
    ,T2.MN_ENG_BHP_PWR
    ,T2.TBCGR_NO
    ,T2.TBCGR_MKR_NM
    ,T2.TBCGR_TP_DESC
    ,T2.TBCGR_RPM_PWR
    ,T2.TBCGR_COFF_FLG
    ,T2.SPR_AUX_BLW_MKR_NM
    ,T2.SPR_AUX_BLW_TP_DESC
    ,T2.SPR_AUX_BLW_NO
    ,T2.FOIL_ADTV_CD
    ,T2.HL_PNT_CD
    ,T2.GNR_CSM_WGT
    ,T2.FOIL_TNK_CBM_CAPA
    ,T2.FOIL_TNK_MT_CAPA
    ,T2.FOIL_STL_SVC_TNK_MT_CAPA
    ,T2.OP_MAX_SPD
    ,T2.OP_MIN_SPD
    ,T2.OP_GNR_SPD
    ,T2.TRND_LINE_USE_FLG
    ,T2.MNVR_CSM_WGT
    ,T2.CRE_DT
    ,T2.CRE_USR_ID
    ,T2.UPD_DT
    ,T2.UPD_USR_ID
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD=T2.VSL_CD(+)
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG='N'
    AND T1.FDR_DIV_CD='T'
    AND T1.CRR_CD='SML'
    ORDER BY T1.VSL_CD
)T1
, (
    SELECT
    T1.VSL_CD
    ,NVL((
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_VENDOR B
    WHERE  A.OWNR_SEQ = B.FLET_MGMT_OWNR_VNDR_SEQ
    AND T1.VNDR_SEQ=B.VNDR_SEQ(+)), (
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_CUSTOMER B
    WHERE  A.OWNR_SEQ = B.FLET_MGMT_OWNR_CUST_SEQ
    AND T1.CUST_CNT_CD = B.CUST_CNT_CD(+)
    AND T1.CUST_SEQ = B.CUST_SEQ(+)
    )) OWNR_NM
    ,TO_CHAR(TO_DATE(VSL_BLD_DT, 'YYYYMMDD'), 'YYYY-MM-DD') VSL_BLD_DT
    ,VSL_DZND_CAPA
    ,BSE_14TON_VSL_CAPA
    ,TO_CHAR(EFF_DT, 'YYYY-MM-DD') FM_DT
    ,TO_CHAR(EXP_DT, 'YYYY-MM-DD') TO_DT
    FROM (
        SELECT
        ROW_NUMBER() OVER(PARTITION BY VSL_CD ORDER BY EXP_DT DESC) SEQ
        ,T1.*
        FROM FMS_CONTRACT T1
        WHERE 1=1
        AND T1.FLET_CTRT_TP_CD!='TO'
        AND T1.EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD')
    ) T1
    WHERE T1.SEQ=1
)T2
WHERE 1=1
AND T1.VSL_CD=T2.VSL_CD
#if (${vsl_cd} != '') 
AND T1.VSL_CD LIKE '%'||@[vsl_cd]||'%'
#end

UNION ALL
SELECT T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,'' OWNR_NM
    ,'' VSL_BLD_DT
    ,TO_NUMBER('') VSL_DZND_CAPA
    ,TO_NUMBER('') BSE_14TON_VSL_CAPA
    ,'' FM_DT
    ,'' TO_DT
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_VSL_CLSS_CAPA
    ,T1.VSL_CLSS_SUB_CD     
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_TP_DESC
    ,T1.MN_ENG_RPM_PWR
    ,T1.MN_ENG_BHP_PWR
    ,T1.TBCGR_NO
    ,T1.TBCGR_MKR_NM
    ,T1.TBCGR_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.TBCGR_COFF_FLG
    ,T1.SPR_AUX_BLW_MKR_NM
    ,T1.SPR_AUX_BLW_TP_DESC
    ,T1.SPR_AUX_BLW_NO
    ,T1.FOIL_ADTV_CD
    ,T1.HL_PNT_CD
    ,T1.GNR_CSM_WGT
    ,T1.FOIL_TNK_CBM_CAPA
    ,T1.FOIL_TNK_MT_CAPA
    ,T1.FOIL_STL_SVC_TNK_MT_CAPA
    ,T1.OP_MAX_SPD
    ,T1.OP_MIN_SPD
    ,T1.OP_GNR_SPD
    ,T1.TRND_LINE_USE_FLG
    ,T1.MNVR_CSM_WGT
    ,T1.CRE_DT
    ,T1.CRE_USR_ID
    ,T1.UPD_DT
    ,T1.UPD_USR_ID
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.VSL_CLSS_SUB_CD
    ,T2.MN_ENG_MKR_NM
    ,T2.MN_ENG_TP_DESC
    ,T2.MN_ENG_RPM_PWR
    ,T2.MN_ENG_BHP_PWR
    ,T2.TBCGR_NO
    ,T2.TBCGR_MKR_NM
    ,T2.TBCGR_TP_DESC
    ,T2.TBCGR_RPM_PWR
    ,T2.TBCGR_COFF_FLG
    ,T2.SPR_AUX_BLW_MKR_NM
    ,T2.SPR_AUX_BLW_TP_DESC
    ,T2.SPR_AUX_BLW_NO
    ,T2.FOIL_ADTV_CD
    ,T2.HL_PNT_CD
    ,T2.GNR_CSM_WGT
    ,T2.FOIL_TNK_CBM_CAPA
    ,T2.FOIL_TNK_MT_CAPA
    ,T2.FOIL_STL_SVC_TNK_MT_CAPA
    ,T2.OP_MAX_SPD
    ,T2.OP_MIN_SPD
    ,T2.OP_GNR_SPD
    ,T2.TRND_LINE_USE_FLG
    ,T2.MNVR_CSM_WGT
    ,T2.CRE_DT
    ,T2.CRE_USR_ID
    ,T2.UPD_DT
    ,T2.UPD_USR_ID
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD=T2.VSL_CD
    AND T2.VSL_CD NOT IN (SELECT VSL_CD FROM FMS_CONTRACT WHERE FLET_CTRT_TP_CD!='TO' AND EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD'))
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG='N'
    AND T1.FDR_DIV_CD='T'
    AND T1.CRR_CD='SML'
    ORDER BY T1.VSL_CD
)T1
WHERE 1=1
#if (${vsl_cd} != '') 
AND T1.VSL_CD LIKE '%'||@[vsl_cd]||'%'
#end

--FOR CRR CD IS NOT SML
UNION ALL
SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T2.OWNR_NM
    ,T2.VSL_BLD_DT
    ,T2.VSL_DZND_CAPA
    ,T2.BSE_14TON_VSL_CAPA
    ,T2.FM_DT
    ,T2.TO_DT
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_VSL_CLSS_CAPA
    ,T1.VSL_CLSS_SUB_CD     
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_TP_DESC
    ,T1.MN_ENG_RPM_PWR
    ,T1.MN_ENG_BHP_PWR
    ,T1.TBCGR_NO
    ,T1.TBCGR_MKR_NM
    ,T1.TBCGR_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.TBCGR_COFF_FLG
    ,T1.SPR_AUX_BLW_MKR_NM
    ,T1.SPR_AUX_BLW_TP_DESC
    ,T1.SPR_AUX_BLW_NO
    ,T1.FOIL_ADTV_CD
    ,T1.HL_PNT_CD
    ,T1.GNR_CSM_WGT
    ,T1.FOIL_TNK_CBM_CAPA
    ,T1.FOIL_TNK_MT_CAPA
    ,T1.FOIL_STL_SVC_TNK_MT_CAPA
    ,T1.OP_MAX_SPD
    ,T1.OP_MIN_SPD
    ,T1.OP_GNR_SPD
    ,T1.TRND_LINE_USE_FLG
    ,T1.MNVR_CSM_WGT
    ,T1.CRE_DT
    ,T1.CRE_USR_ID
    ,T1.UPD_DT
    ,T1.UPD_USR_ID
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.VSL_CLSS_SUB_CD
    ,T2.MN_ENG_MKR_NM
    ,T2.MN_ENG_TP_DESC
    ,T2.MN_ENG_RPM_PWR
    ,T2.MN_ENG_BHP_PWR
    ,T2.TBCGR_NO
    ,T2.TBCGR_MKR_NM
    ,T2.TBCGR_TP_DESC
    ,T2.TBCGR_RPM_PWR
    ,T2.TBCGR_COFF_FLG
    ,T2.SPR_AUX_BLW_MKR_NM
    ,T2.SPR_AUX_BLW_TP_DESC
    ,T2.SPR_AUX_BLW_NO
    ,T2.FOIL_ADTV_CD
    ,T2.HL_PNT_CD
    ,T2.GNR_CSM_WGT
    ,T2.FOIL_TNK_CBM_CAPA
    ,T2.FOIL_TNK_MT_CAPA
    ,T2.FOIL_STL_SVC_TNK_MT_CAPA
    ,T2.OP_MAX_SPD
    ,T2.OP_MIN_SPD
    ,T2.OP_GNR_SPD
    ,T2.TRND_LINE_USE_FLG
    ,T2.MNVR_CSM_WGT
    ,T2.CRE_DT
    ,T2.CRE_USR_ID
    ,T2.UPD_DT
    ,T2.UPD_USR_ID
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD=T2.VSL_CD --NOT OUTER JOIN
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG='N'
    AND T1.FDR_DIV_CD='T'
    AND T1.CRR_CD<>'SML'
    ORDER BY T1.VSL_CD
)T1
, (
    SELECT
    T1.VSL_CD
    ,NVL((
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_VENDOR B
    WHERE  A.OWNR_SEQ = B.FLET_MGMT_OWNR_VNDR_SEQ
    AND T1.VNDR_SEQ=B.VNDR_SEQ(+)), (
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_CUSTOMER B
    WHERE  A.OWNR_SEQ = B.FLET_MGMT_OWNR_CUST_SEQ
    AND T1.CUST_CNT_CD = B.CUST_CNT_CD(+)
    AND T1.CUST_SEQ = B.CUST_SEQ(+)
    )) OWNR_NM
    ,TO_CHAR(TO_DATE(VSL_BLD_DT, 'YYYYMMDD'), 'YYYY-MM-DD') VSL_BLD_DT
    ,VSL_DZND_CAPA
    ,BSE_14TON_VSL_CAPA
    ,TO_CHAR(EFF_DT, 'YYYY-MM-DD') FM_DT
    ,TO_CHAR(EXP_DT, 'YYYY-MM-DD') TO_DT
    FROM (
        SELECT
        ROW_NUMBER() OVER(PARTITION BY VSL_CD ORDER BY EXP_DT DESC) SEQ
        ,T1.*
        FROM FMS_CONTRACT T1
        WHERE 1=1
        AND T1.FLET_CTRT_TP_CD!='TO'
        AND T1.EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD')
    ) T1
    WHERE T1.SEQ=1
)T2
WHERE 1=1
AND T1.VSL_CD=T2.VSL_CD
#if (${vsl_cd} != '') 
AND T1.VSL_CD LIKE '%'||@[vsl_cd]||'%'
#end

UNION ALL
SELECT T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,'' OWNR_NM
    ,'' VSL_BLD_DT
    ,TO_NUMBER('') VSL_DZND_CAPA
    ,TO_NUMBER('') BSE_14TON_VSL_CAPA
    ,'' FM_DT
    ,'' TO_DT
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_VSL_CLSS_CAPA
    ,T1.VSL_CLSS_SUB_CD     
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_TP_DESC
    ,T1.MN_ENG_RPM_PWR
    ,T1.MN_ENG_BHP_PWR
    ,T1.TBCGR_NO
    ,T1.TBCGR_MKR_NM
    ,T1.TBCGR_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.TBCGR_COFF_FLG
    ,T1.SPR_AUX_BLW_MKR_NM
    ,T1.SPR_AUX_BLW_TP_DESC
    ,T1.SPR_AUX_BLW_NO
    ,T1.FOIL_ADTV_CD
    ,T1.HL_PNT_CD
    ,T1.GNR_CSM_WGT
    ,T1.FOIL_TNK_CBM_CAPA
    ,T1.FOIL_TNK_MT_CAPA
    ,T1.FOIL_STL_SVC_TNK_MT_CAPA
    ,T1.OP_MAX_SPD
    ,T1.OP_MIN_SPD
    ,T1.OP_GNR_SPD
    ,T1.TRND_LINE_USE_FLG
    ,T1.MNVR_CSM_WGT
    ,T1.CRE_DT
    ,T1.CRE_USR_ID
    ,T1.UPD_DT
    ,T1.UPD_USR_ID
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.DWT_WGT
    ,T1.GRS_RGST_TONG_WGT
    ,T1.NET_RGST_TONG_WGT
    ,T1.LLOYD_NO
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.VSL_CLSS_SUB_CD
    ,T2.MN_ENG_MKR_NM
    ,T2.MN_ENG_TP_DESC
    ,T2.MN_ENG_RPM_PWR
    ,T2.MN_ENG_BHP_PWR
    ,T2.TBCGR_NO
    ,T2.TBCGR_MKR_NM
    ,T2.TBCGR_TP_DESC
    ,T2.TBCGR_RPM_PWR
    ,T2.TBCGR_COFF_FLG
    ,T2.SPR_AUX_BLW_MKR_NM
    ,T2.SPR_AUX_BLW_TP_DESC
    ,T2.SPR_AUX_BLW_NO
    ,T2.FOIL_ADTV_CD
    ,T2.HL_PNT_CD
    ,T2.GNR_CSM_WGT
    ,T2.FOIL_TNK_CBM_CAPA
    ,T2.FOIL_TNK_MT_CAPA
    ,T2.FOIL_STL_SVC_TNK_MT_CAPA
    ,T2.OP_MAX_SPD
    ,T2.OP_MIN_SPD
    ,T2.OP_GNR_SPD
    ,T2.TRND_LINE_USE_FLG
    ,T2.MNVR_CSM_WGT
    ,T2.CRE_DT
    ,T2.CRE_USR_ID
    ,T2.UPD_DT
    ,T2.UPD_USR_ID
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD=T2.VSL_CD
    AND T2.VSL_CD NOT IN (SELECT VSL_CD FROM FMS_CONTRACT WHERE FLET_CTRT_TP_CD!='TO' AND EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD'))
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG='N'
    AND T1.FDR_DIV_CD='T'
    AND T1.CRR_CD<>'SML'
    ORDER BY T1.VSL_CD
)T1
WHERE 1=1
#if (${vsl_cd} != '') 
AND T1.VSL_CD LIKE '%'||@[vsl_cd]||'%'
#end

)
ORDER BY VSL_CD			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
