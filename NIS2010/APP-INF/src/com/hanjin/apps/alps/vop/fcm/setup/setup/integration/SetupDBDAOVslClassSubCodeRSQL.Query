<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SetupDBDAOVslClassSubCodeRSQL">
			<desc><![CDATA[SetupDBDAOVslClassSubCodeRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT T2.VSL_CLSS_SUB_CD FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
WHERE 1=1
AND T1.VSL_CD=T2.VSL_CD
AND T1.CNTR_DZN_CAPA=@[cntr_vsl_clss_capa]
ORDER BY T2.VSL_CLSS_SUB_CD			]]></sql>
			<params>
				<param name="cntr_vsl_clss_capa" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
