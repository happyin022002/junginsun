<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchScgChemHistoryFileSeqRSQL">
			<desc><![CDATA[file seq구하기]]></desc>
			<sql><![CDATA[
select nvl(max(atch_file_seq)+1,1) AS atch_file_seq
  from scg_chem_his_file
 where chem_seq         = @[chem_seq]
   and atch_file_div_cd = @[atch_file_div_cd]			]]></sql>
			<params>
				<param name="chem_seq" type="12" value="" out="N"/>
				<param name="atch_file_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
