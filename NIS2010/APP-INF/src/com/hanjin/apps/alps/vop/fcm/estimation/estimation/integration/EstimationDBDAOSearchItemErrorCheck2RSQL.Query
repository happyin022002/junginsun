<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationDBDAOSearchItemErrorCheck2RSQL">
			<desc><![CDATA[Search item error check No. 2]]></desc>
			<sql><![CDATA[
SELECT MAX( CASE WHEN T1.CSM_OIL_TP_CD='HFO' THEN
                SIGN(T1.VOY_END_RMN_WGT-T2.FOIL_RMN_WGT)
            WHEN T1.CSM_OIL_TP_CD='MDO' THEN
                SIGN(T1.VOY_END_RMN_WGT-T2.DOIL_RMN_WGT)
            END) ERR_CHK
FROM FCM_ESTM_MON_CSM_IF T1, FCM_RMN_OIL_MON_END_RPT T2
WHERE T1.BSE_YRMON = T2.REV_YRMON
AND T1.VSL_CD = T2.VSL_CD
AND T1.SKD_VOY_NO = T2.SKD_VOY_NO
AND T1.SKD_DIR_CD = T2.SKD_DIR_CD
AND T1.BSE_YRMON  = @[bse_yrmon]
AND T1.VSL_CD     = @[vsl_cd]
AND T1.SKD_VOY_NO = @[skd_voy_no]
AND T1.SKD_DIR_CD = @[skd_dir_cd]
AND T1.REV_DIR_CD = @[rev_dir_cd]			]]></sql>
			<params>
				<param name="bse_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
