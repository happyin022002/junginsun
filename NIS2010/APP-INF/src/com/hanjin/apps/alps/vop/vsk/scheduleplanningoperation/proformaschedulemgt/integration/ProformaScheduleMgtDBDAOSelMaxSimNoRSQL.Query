<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOSelMaxSimNoRSQL">
			<desc><![CDATA[Simulation No를 가져온다. 입력된 날짜 + Max Sequence No]]></desc>
			<sql><![CDATA[
SELECT LPAD(NVL(MAX(TO_NUMBER(SIM_NO)),0)+1,3,'0') AS SIM_NO
FROM VSK_SIM_INFO
WHERE SIM_DT = TO_CHAR( sysdate ,'yyyymmdd' )  			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
