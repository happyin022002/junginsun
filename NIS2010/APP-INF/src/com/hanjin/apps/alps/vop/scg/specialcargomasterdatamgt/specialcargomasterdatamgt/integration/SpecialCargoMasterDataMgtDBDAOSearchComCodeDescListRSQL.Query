<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchComCodeDescListRSQL">
			<desc><![CDATA[MAKE COMBO]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_CTNT AS CD
      ,INTG_CD_VAL_DP_DESC AS NM
FROM   COM_INTG_CD_DTL
WHERE INTG_CD_ID = @[cd]
AND   APLY_ST_DT  <= TO_CHAR(SYSDATE, 'YYYYMMDD')
AND	  APLY_END_DT >= TO_CHAR(SYSDATE, 'YYYYMMDD')
ORDER BY INTG_CD_VAL_DP_SEQ ASC			]]></sql>
			<params>
				<param name="cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
