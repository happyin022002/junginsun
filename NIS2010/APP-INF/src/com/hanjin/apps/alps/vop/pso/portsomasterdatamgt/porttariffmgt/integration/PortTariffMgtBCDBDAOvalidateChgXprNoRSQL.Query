<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOvalidateChgXprNoRSQL">
			<desc><![CDATA[[CHM-201534260] BASE tariff등록시 object rate 는 반드시 포함되어야 함.]]></desc>
			<sql><![CDATA[
SELECT X.CHK
  FROM (
SELECT CASE WHEN INSTR(B.FOML_SYS_DESC, @[exp_cnd] ) <> 0 THEN 'Y' 
            ELSE 'N' END  AS CHK
 FROM PSO_CHG_XPR_DTL A,  PSO_FORMULA B
WHERE A.FOML_NO    = B.FOML_NO
  AND A.CHG_XPR_NO =  @[chg_xpr_no]
  ORDER BY CHK ASC ) X
  WHERE ROWNUM    = 1			]]></sql>
			<params>
				<param name="exp_cnd" type="12" value="" out="N"/>
				<param name="chg_xpr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
