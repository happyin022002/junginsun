<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAOModifyRdrUtilizeUSQL">
			<desc><![CDATA[RDR Utilize 수정  Utilize에서 저장시
Ticket ID : CHM-201007765-01
개발자 : 박희동(2010-12-20)
수정내용 : UPD_SYS_CD 컬럼추가...N으로 setting]]></desc>
			<sql><![CDATA[
UPDATE RDR_UTILIZE
  SET  SLOT_QTY     = @[slot_qty]
,      WEIGHT       = @[total_wgt]
,      UPDATE_USER  = @[update_user]
,      UPDATE_TIME  = SYSDATE
,      UPD_SYS_CD   = 'N'
WHERE
      VSL_CD =   @[vsl_cd]
  AND VOY_NO =   @[voy_no]
  AND DIR_CD =   @[dir_cd]
  AND REGION =   @[region]
  AND OPR_CD =   @[opr_cd]
  AND TYPE   =   @[type]
  AND TYPE IN ('F','E','A')			]]></sql>
			<params>
				<param name="slot_qty" type="12" value="" out="N"/>
				<param name="total_wgt" type="12" value="" out="N"/>
				<param name="update_user" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
				<param name="type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
