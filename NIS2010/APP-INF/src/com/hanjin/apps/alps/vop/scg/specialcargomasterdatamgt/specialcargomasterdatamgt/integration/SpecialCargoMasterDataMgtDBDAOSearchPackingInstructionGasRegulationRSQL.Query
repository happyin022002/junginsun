<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchPackingInstructionGasRegulationRSQL">
			<desc><![CDATA[SearchPackingInstructionGasRegulation]]></desc>
			<sql><![CDATA[
SELECT G.IMDG_PCK_INSTR_CD
      ,G.IMDG_PCK_INSTR_SEQ
      ,G.GAS_TP_CD
      ,G.IMDG_UN_NO
      ,G.LC50_VAL
      ,G.CLND_CHK_FLG
      ,G.TUB_CHK_FLG
      ,G.PRSS_DRM_CHK_FLG
      ,G.CLND_BDL_CHK_FLG
      ,G.MEGC_CHK_FLG
      ,G.TST_PRD_YR
      ,G.TST_PRSS
      ,G.MAX_WRK_PRSS
      ,G.GAS_FILL_RTO
      ,G.GAS_SPCL_PCK_PROVI_N1ST_CTNT
      ,G.GAS_SPCL_PCK_PROVI_N2ND_CTNT
      ,G.GAS_SPCL_PCK_PROVI_N3RD_CTNT
      ,G.GAS_SPCL_PCK_PROVI_N4TH_CTNT
      ,G.PCK_REF_CD
      ,G.REF_DIV_NO 
      ,U.PRP_SHP_NM
      ,G.DELT_FLG
  FROM SCG_PCK_GAS_REGU G
      ,(SELECT IMDG_UN_NO
             ,RANK() OVER (PARTITION BY IMDG_UN_NO
                           ORDER BY IMDG_UN_NO_SEQ DESC) R
             ,PRP_SHP_NM
         FROM SCG_IMDG_UN_NO
       ) U
 WHERE G.IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
   AND G.IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]
   AND G.IMDG_UN_NO = U.IMDG_UN_NO
   AND U.R = 1
   AND G.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
