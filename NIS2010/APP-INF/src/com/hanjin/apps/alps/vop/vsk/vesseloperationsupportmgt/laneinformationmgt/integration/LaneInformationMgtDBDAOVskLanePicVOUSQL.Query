<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneInformationMgtDBDAOVskLanePicVOUSQL">
			<desc><![CDATA[Lane PIC 정보를 수정 합니다.
===============================================
History
2011.04.28 진마리아 [CHM-201110229-01] Lane informtion내 PIC의 Vessel 칼럼을 Carrier로 변경 요청건]]></desc>
			<sql><![CDATA[
UPDATE VSK_LANE_PIC SET 
	VSL_SLAN_NM = @[vsl_slan_nm]
,	LANE_MNG_USR_DESC = @[lane_mng_usr_desc]
,   RGN_SHP_OPR_CD = @[rgn_shp_opr_cd]
,	USR_NM = @[usr_nm]
,	JB_DESC = @[jb_desc]
,	PIC_VSL_DESC = @[pic_vsl_desc]
,	PIC_PHN_NO = @[pic_phn_no]
,	MPHN_NO = @[mphn_no]
,	FAX_NO = @[fax_no]
,	LANE_PIC_USR_EML = @[lane_pic_usr_eml]
,	PIC_EML = @[pic_eml]
,	LANE_RMK = @[lane_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
,	PIC_CRR_DESC = @[pic_crr_desc]
WHERE	SLAN_CD = @[slan_cd]
AND	LANE_PIC_SEQ = @[lane_pic_seq]			]]></sql>
			<params>
				<param name="vsl_slan_nm" type="12" value="" out="N"/>
				<param name="lane_mng_usr_desc" type="12" value="" out="N"/>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
				<param name="usr_nm" type="12" value="" out="N"/>
				<param name="jb_desc" type="12" value="" out="N"/>
				<param name="pic_vsl_desc" type="12" value="" out="N"/>
				<param name="pic_phn_no" type="12" value="" out="N"/>
				<param name="mphn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="lane_pic_usr_eml" type="12" value="" out="N"/>
				<param name="pic_eml" type="12" value="" out="N"/>
				<param name="lane_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pic_crr_desc" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="lane_pic_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
