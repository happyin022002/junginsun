<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOCheckDupOffHireTimeCrRSQL">
			<desc><![CDATA[Credit Off-Hire Time Duplicaton 체크한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'Y','N') IS_OK
FROM OPF_VNOR
WHERE VSL_CD = @[vsl_cd]
AND VNOR_OFFH_FM_DT = TO_DATE(@[vnor_offh_fm_dt], 'yyyy.mm.dd.hh24:mi')
AND VNOR_OFFH_TO_DT = TO_DATE(@[vnor_offh_to_dt], 'yyyy.mm.dd.hh24:mi')
AND CR_CHK_FLG = 'Y'
			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vnor_offh_fm_dt" type="12" value="" out="N"/>
				<param name="vnor_offh_to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
