<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOSearchFormulaNoForLoadingRSQL">
			<desc><![CDATA[PSO_FORMULA  FOML_NO IN (1, 2)]]></desc>
			<sql><![CDATA[
SELECT A.FOML_NO
      ,A.FOML_NM
      ,A.PSO_FOML_MZD_CD
      ,A.FOML_DESC
      ,A.FOML_SYS_DESC
      ,A.UPD_MNU_NO 
FROM   PSO_FORMULA A
WHERE  1=1
AND    A.FOML_NO IN (1, 2)
ORDER BY A.FOML_NO			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
