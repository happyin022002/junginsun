<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOcheckDoublePayInvRSQL">
			<desc><![CDATA[Double  Pay Check : VVD/YD_CD/COST_CD/COST_OFC 내에서 기존 Invoice가 존재하는지 유무 체크]]></desc>
			<sql><![CDATA[
SELECT INV_NO 
FROM PSO_CHARGE PC, PSO_CHG_DTL PCD
WHERE 1=1
AND PC.VNDR_SEQ = @[vndr_seq]
AND PC.COST_OFC_CD = @[cost_ofc_cd]
AND PC.YD_CD = @[yd_cd]
AND PCD.LGS_COST_CD = @[lgs_cost_cd]
AND PCD.VSL_CD = @[vsl_cd]
AND PCD.SKD_VOY_NO =  @[skd_voy_no]
AND PCD.SKD_DIR_CD =  @[skd_dir_cd]
AND PC.ISS_CTY_CD = PCD.ISS_CTY_CD
AND PC.SO_SEQ = PCD.SO_SEQ
GROUP BY INV_NO			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cost_ofc_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
