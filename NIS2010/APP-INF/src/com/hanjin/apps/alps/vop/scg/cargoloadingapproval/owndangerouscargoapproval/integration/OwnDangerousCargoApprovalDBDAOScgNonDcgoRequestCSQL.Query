<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgNonDcgoRequestCSQL">
			<desc><![CDATA[OwnDangerousCargoApprovalDBDAOScgNonDcgoRequest]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_NON_DG_CGO_UDECL_HIS 
(	BKG_NO
	, RQST_DT
	, NON_DCGO_RQST_SEQ
	, UDECL_DT
	, RQST_OFC_CD
	, VSL_CD
	, SKD_VOY_NO
	, SKD_DIR_CD
	, SLAN_CD
	, ON_BRD_FLG
	, CSTMS_DESC
	, CNTR_MF_GDS_DESC
	, CMDT_DESC
	, XTER_RMK
	, INTER_RMK
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
)
SELECT A.BKG_NO
		, A.RQST_DT
        , A.NON_DCGO_RQST_SEQ
		, SYSDATE --UDECL_DT
        , A.RQST_OFC_CD
        , A.VSL_CD
        , A.SKD_VOY_NO
        , A.SKD_DIR_CD
        , A.SLAN_CD
        , 'N'--ON_BRD_FLG
        , A.CSTMS_DESC
        , A.CNTR_MF_GDS_DESC
        , A.CMDT_DESC
        , A.XTER_RMK
        , A.INTER_RMK
        , @[cre_usr_id]
        , SYSDATE
        , @[upd_usr_id]
        , SYSDATE
FROM    SCG_NON_DG_CGO_KW_RQST A
WHERE 1=1
AND A.BKG_NO = @[bkg_no]
AND A.NON_DCGO_RQST_SEQ = @[non_dcgo_rqst_seq]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="non_dcgo_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
