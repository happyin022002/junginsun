<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAOAddRdrMoveCSQL">
			<desc><![CDATA[DESC Enter..
Ticket ID : CHM-201007765-01
개발자 : 박희동(2010-12-20)
수정내용 : UPD_SYS_CD 컬럼추가...N으로 setting]]></desc>
			<sql><![CDATA[
INSERT INTO RDR_MOVE(VSL_CD
,                    VOY_NO
,                    DIR_CD
,                    PORT_CD
,                    CALL_IND
,                    REGION
,                    ARRIVAL_DATE  
,                    BERTH_DATE    
,                    UNBERTH_DATE  
,                    DEPARTURE_DATE
,                    IDX           
,                    UPDATE_USER   
,                    UPDATE_TIME 
,                    UPD_SYS_CD
)  
VALUES( @[vsl_cd]
,       @[voy_no]
,       @[dir_cd]        
,       @[port_cd]   
,       (SELECT   NVL( MAX(CALL_IND), 0 )+1 FROM RDR_MOVE A 
          WHERE   A.VSL_CD = @[vsl_cd]
            AND   A.VOY_NO = @[voy_no]
            AND   A.DIR_CD = @[dir_cd]
            AND   A.REGION = @[region]
        )
,       @[region]
,       TO_DATE(@[arr_time],'YYYYMMDDHH24MI')
,       TO_DATE(@[berth_time],'YYYYMMDDHH24MI')
,       TO_DATE(@[unberth_time],'YYYYMMDDHH24MI')
,       TO_DATE(@[dep_time],'YYYYMMDDHH24MI')
,       @[idx]           
,       @[update_user]
,       SYSDATE 
,       'N'
)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="arr_time" type="12" value="" out="N"/>
				<param name="berth_time" type="12" value="" out="N"/>
				<param name="unberth_time" type="12" value="" out="N"/>
				<param name="dep_time" type="12" value="" out="N"/>
				<param name="idx" type="12" value="" out="N"/>
				<param name="update_user" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
