<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierRestrictionDBDAOFileMaxSeqRSQL">
			<desc><![CDATA[파일의 seq값을 검색]]></desc>
			<sql><![CDATA[
SELECT 
NVL(MAX(IMDG_CRR_RSTR_FILE_SEQ)+1,1) file_seq
FROM SCG_IMDG_CRR_RSTR_FILE
WHERE VSL_OPR_TP_CD= @[vsl_opr_tp_cd]			]]></sql>
			<params>
				<param name="vsl_opr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
