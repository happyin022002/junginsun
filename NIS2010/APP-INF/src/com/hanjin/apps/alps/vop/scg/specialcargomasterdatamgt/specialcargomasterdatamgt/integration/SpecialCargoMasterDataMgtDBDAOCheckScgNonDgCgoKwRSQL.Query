<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOCheckScgNonDgCgoKwRSQL">
			<desc><![CDATA[NON D/G CARGO 의 주의화물 KEYWORD 중복 입력을 체크하기 위함]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT FROM SCG_NON_DG_CGO_KW 
WHERE NON_DCGO_KW_NM = @[non_dcgo_kw_nm]
  AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="non_dcgo_kw_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
