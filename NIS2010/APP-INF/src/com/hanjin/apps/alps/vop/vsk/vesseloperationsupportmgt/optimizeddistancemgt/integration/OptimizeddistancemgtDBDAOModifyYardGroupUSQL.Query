<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OptimizeddistancemgtDBDAOModifyYardGroupUSQL">
			<desc><![CDATA[해당 Port관련 Gourp의 정보를 Update한다.]]></desc>
			<sql><![CDATA[
MERGE  INTO  VSK_YD_GRP
USING  DUAL  ON
(    
    1=1
    AND    YD_CD = @[yd_cd]	
)
WHEN MATCHED THEN
  UPDATE SET

	  YD_GRP_ID  = @[yd_grp_id]
    , UPD_USR_ID = @[usr_id]
    , UPD_DT     = sysdate
    
WHEN NOT MATCHED THEN
  INSERT
  (
      YD_CD
    , YD_GRP_ID
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
    
  )
  VALUES
  (
	  @[yd_cd]
	, @[yd_grp_id]
	, @[usr_id]
    , sysdate
	, @[usr_id]
    , sysdate
    
  )

			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="yd_grp_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
