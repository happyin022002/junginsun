<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOSearchAccountBySimulationRSQL">
			<desc><![CDATA[Tariff에 등록되어 있는 Account 조회
=====================================================================
2011.03.28 진마리아 [선처리(SRM-201114694)] 사업계획 항비 로직 수정 요청]]></desc>
			<sql><![CDATA[
/**** Tariff에 등록되어 있는 Account 조회 ****/
SELECT distinct T2.ACCT_CD          ACCT_CD   
      ,T3.ACCT_ENG_NM      ACCT_NM
FROM   TES_LGS_COST     T2
      ,MDM_ACCOUNT      T3
      ,(SELECT DISTINCT A.LGS_COST_CD
        FROM   PSO_YD_CHG A
        WHERE  1 = 1
        AND    A.YD_CD LIKE @[port_cd] || NVL(@[yard_cd], '%')
--        AND    A.CPLS_FLG = 'Y'
        AND    A.LST_FLG = 'Y'
       #if( ${issue_date} != '' )
        AND    TO_DATE(REPLACE(@[issue_date], '-', ''), 'YYYYMMDD') BETWEEN A.EFF_DT AND A.EXP_DT
       #end
       ) T4
WHERE  1=1
AND    T2.ACCT_CD = T3.ACCT_CD
AND    T2.LGS_COST_CD = T4.LGS_COST_CD
AND    T2.ACCT_CD <> '999999'
AND    T2.LGS_COST_CD LIKE '____' || '%'
ORDER  BY 1			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="yard_cd" type="12" value="" out="N"/>
				<param name="issue_date" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
