<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselPositionPollingManagementDBDAOCreateDlyRcvSeqRSQL">
			<desc><![CDATA[Create the Daily Receive Sequence]]></desc>
			<sql><![CDATA[
SELECT  NVL(MAX(X.DLY_RCV_SEQ)+1,1) 
FROM    VSK_VSL_PSN_PLNG_HDR X
WHERE   1=1
AND     X.RCV_DT = @[rcv_dt] 			]]></sql>
			<params>
				<param name="rcv_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
