<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierRestrictionDBDAOSearchFileNameRSQL">
			<desc><![CDATA[첨부파일명을 검색한다]]></desc>
			<sql><![CDATA[
SELECT
 FILE_NM||' ('||(select count(*) from SCG_IMDG_CRR_RSTR_FILE where VSL_OPR_TP_CD = @[crr_cd])||')' file_nm
 FROM SCG_IMDG_CRR_RSTR_FILE
 WHERE VSL_OPR_TP_CD = @[crr_cd] and  rownum='1'			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
