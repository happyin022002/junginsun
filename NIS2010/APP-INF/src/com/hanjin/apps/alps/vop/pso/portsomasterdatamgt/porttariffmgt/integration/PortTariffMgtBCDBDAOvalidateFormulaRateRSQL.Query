<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOvalidateFormulaRateRSQL">
			<desc><![CDATA[formula 생성시 기본적으로 Rate[OBJECT NO : 45 ] 는 등록되어야 되는 사항]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN instr(ltrim(@[foml_sys_desc]),'[45]') <> 0     THEN  'Y' 
            ELSE 'N'  
       END
FROM DUAL			]]></sql>
			<params>
				<param name="foml_sys_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
