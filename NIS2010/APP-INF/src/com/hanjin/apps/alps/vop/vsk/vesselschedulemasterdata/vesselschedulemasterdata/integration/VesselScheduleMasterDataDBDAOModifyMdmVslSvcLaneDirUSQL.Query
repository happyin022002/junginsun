<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMasterDataDBDAOModifyMdmVslSvcLaneDirUSQL">
			<desc><![CDATA[ModifyMdmVslSvcLaneDir]]></desc>
			<sql><![CDATA[
UPDATE	MDM_VSL_SVC_LANE_DIR
SET	SVC_SCP_BND_CD		=  DECODE(@[ibflag], 'D', NULL, @[svc_scp_bnd_cd]),
UPD_USR_ID = @[upd_usr_id],
UPD_DT = SYSDATE
WHERE	DELT_FLG		= 'N'
AND	VSL_SLAN_CD		= @[vsl_slan_cd]
AND	VSL_SLAN_DIR_CD = @[vsl_slan_dir_cd]			]]></sql>
			<params>
				<param name="ibflag" type="12" value="" out="N"/>
				<param name="svc_scp_bnd_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
