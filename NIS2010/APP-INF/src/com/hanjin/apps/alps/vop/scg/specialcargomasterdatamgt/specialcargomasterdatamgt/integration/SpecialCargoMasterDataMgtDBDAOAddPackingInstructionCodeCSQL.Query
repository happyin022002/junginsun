		<query name="SpecialCargoMasterDataMgtDBDAOAddPackingInstructionCodeCSQL">
			<desc><![CDATA[AddPackingInstructionCode]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_PCK_INSTR(
       IMDG_PCK_INSTR_CD
      ,IMDG_PCK_INSTR_SEQ
      ,PCK_INSTR_TP_CTNT 
      ,PCK_DIV_CD 
      ,PCK_DESC 
      ,PCK_DESC_USE_FLG 
      ,PCK_REGU_USE_FLG 
      ,INR_PCK_USE_FLG 
      ,INTMD_PCK_USE_FLG 
      ,OUTR_PCK_USE_FLG 
      ,OUTR_PCK_MAX_CAPA_FLG 
      ,SGL_PCK_USE_FLG 
      ,SGL_PCK_MAX_CAPA_FLG 
      ,PRSS_DESC
      ,PRSS_DESC_USE_FLG 
      ,ADD_REGU_DESC 
      ,ADD_REGU_DESC_USE_FLG 
      ,SPCL_PCK_REGU_USE_FLG 
      ,GAS_USE_FLG 
      ,PTBL_TNK_USE_FLG
      ,PCK_EXPT_FLG
      ,DELT_FLG
      ,CRE_USR_ID 
      ,CRE_DT 
      ,UPD_USR_ID 
      ,UPD_DT 
)
SELECT @[imdg_pck_instr_cd]
      ,NVL(MAX(IMDG_PCK_INSTR_SEQ), 0) + 1
      ,@[pck_instr_tp_ctnt] 
      ,@[pck_div_cd]
      ,@[pck_desc]
      ,@[pck_desc_use_flg]
      ,@[pck_regu_use_flg] 
      ,@[inr_pck_use_flg] 
      ,@[intmd_pck_use_flg] 
      ,@[outr_pck_use_flg]
      ,@[outr_pck_max_capa_flg]
      ,@[sgl_pck_use_flg]
      ,@[sgl_pck_max_capa_flg]
      ,@[prss_desc]
      ,@[prss_desc_use_flg]
      ,@[add_regu_desc]
      ,@[add_regu_desc_use_flg]
      ,@[spcl_pck_regu_use_flg]
      ,@[gas_use_flg]
      ,@[ptbl_tnk_use_flg]
      ,@[pck_expt_flg]
      ,'N'
      ,@[cre_usr_id]
      ,SYSDATE 
      ,@[cre_usr_id]
      ,SYSDATE
  FROM SCG_PCK_INSTR
 WHERE IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="pck_instr_tp_ctnt" type="12" value="" out="N"/>
				<param name="pck_div_cd" type="12" value="" out="N"/>
				<param name="pck_desc" type="12" value="" out="N"/>
				<param name="pck_desc_use_flg" type="12" value="" out="N"/>
				<param name="pck_regu_use_flg" type="12" value="" out="N"/>
				<param name="inr_pck_use_flg" type="12" value="" out="N"/>
				<param name="intmd_pck_use_flg" type="12" value="" out="N"/>
				<param name="outr_pck_use_flg" type="12" value="" out="N"/>
				<param name="outr_pck_max_capa_flg" type="12" value="" out="N"/>
				<param name="sgl_pck_use_flg" type="12" value="" out="N"/>
				<param name="sgl_pck_max_capa_flg" type="12" value="" out="N"/>
				<param name="prss_desc" type="12" value="" out="N"/>
				<param name="prss_desc_use_flg" type="12" value="" out="N"/>
				<param name="add_regu_desc" type="12" value="" out="N"/>
				<param name="add_regu_desc_use_flg" type="12" value="" out="N"/>
				<param name="spcl_pck_regu_use_flg" type="12" value="" out="N"/>
				<param name="gas_use_flg" type="12" value="" out="N"/>
				<param name="ptbl_tnk_use_flg" type="12" value="" out="N"/>
				<param name="pck_expt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
