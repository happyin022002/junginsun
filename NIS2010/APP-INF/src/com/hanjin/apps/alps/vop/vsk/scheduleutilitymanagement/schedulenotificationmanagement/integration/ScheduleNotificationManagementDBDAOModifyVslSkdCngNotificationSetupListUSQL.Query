<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScheduleNotificationManagementDBDAOModifyVslSkdCngNotificationSetupListUSQL">
			<desc><![CDATA[Vessel Schedule Notice Setup 정보 PK 업데이트]]></desc>
			<sql><![CDATA[
UPDATE		VSK_VSL_SKD_NTFC_STUP	X
SET			X.VSL_SLAN_CD			= @[vsl_slan_cd]
		,	X.SKD_DIR_CD			= @[skd_dir_cd]
		,	X.VPS_PORT_CD			= @[vps_port_cd]
WHERE		1 = 1
AND			X.USR_ID				= NVL(@[upd_usr_id],@[login_usr_id])
AND			X.VSL_SLAN_CD			= @[org_vsl_slan_cd]
AND			X.SKD_DIR_CD			= @[org_skd_dir_cd]
AND			X.VPS_PORT_CD			= @[org_vps_port_cd]			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="login_usr_id" type="12" value="" out="N"/>
				<param name="org_vsl_slan_cd" type="12" value="" out="N"/>
				<param name="org_skd_dir_cd" type="12" value="" out="N"/>
				<param name="org_vps_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
