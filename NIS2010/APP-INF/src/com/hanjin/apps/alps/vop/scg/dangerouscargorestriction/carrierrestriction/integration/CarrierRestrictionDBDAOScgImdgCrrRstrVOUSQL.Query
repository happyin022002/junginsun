<sqls>
	<querys>
		<query name="CarrierRestrictionDBDAOScgImdgCrrRstrVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE  SCG_IMDG_CRR_RSTR 
   SET	VSL_OPR_TP_CD         =  @[vsl_opr_tp_cd],        
		IMDG_CRR_RSTR_SEQ     =  @[imdg_crr_rstr_seq],    
		IMDG_UN_NO            =  @[imdg_un_no],           
		IMDG_UN_NO_SEQ        =  @[imdg_un_no_seq],       
		IMDG_CLSS_CD          =  @[imdg_clss_cd],         
		IMDG_CRR_RSTR_EXPT_CD =  @[imdg_crr_rstr_expt_cd],
		SLAN_CD               =  @[slan_cd],              
		CRR_REGU_DESC         =  @[crr_regu_desc],        
		UPD_USR_ID            =  @[upd_usr_id],           
		UPD_DT                =  SYSDATE
 WHERE	VSL_OPR_TP_CD         =  @[vsl_opr_tp_cd] 
   AND	IMDG_CRR_RSTR_SEQ     =  @[imdg_crr_rstr_seq]			]]></sql>
			<params>
				<param name="vsl_opr_tp_cd" type="12" value="" out="N"/>
				<param name="imdg_crr_rstr_seq" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="imdg_crr_rstr_expt_cd" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="crr_regu_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
