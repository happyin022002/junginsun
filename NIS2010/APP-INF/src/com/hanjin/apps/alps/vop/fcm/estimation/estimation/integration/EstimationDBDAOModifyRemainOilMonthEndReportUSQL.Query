<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationDBDAOModifyRemainOilMonthEndReportUSQL">
			<desc><![CDATA[Modify a report about month-end remained oil of vessel]]></desc>
			<sql><![CDATA[
UPDATE FCM_RMN_OIL_MON_END_RPT
SET    FOIL_RMN_WGT           = @[foil_rmn_wgt]
	   ,DOIL_RMN_WGT          = @[doil_rmn_wgt]
	   ,CLND_OIL_RMN_WGT      = @[clnd_oil_rmn_wgt]
	   ,SYS_OIL_RMN_WGT       = @[sys_oil_rmn_wgt]
	   ,GNR_OIL_RMN_WGT       = @[gnr_oil_rmn_wgt]
	   ,LOW_SULP_DOIL_RMN_WGT = @[low_sulp_doil_rmn_wgt]
	   ,TRBN_GNR_OIL_RMN_WGT  = @[trbn_gnr_oil_rmn_wgt ]
	   ,LOW_SULP_FOIL_RMN_WGT = @[low_sulp_foil_rmn_wgt]
	   ,UPD_USR_ID            = @[upd_usr_id]
	   ,UPD_DT                = SYSDATE
WHERE 1=1
AND REV_YRMON=REPLACE(@[rev_yrmon], '-', '')
AND VSL_CD=@[vsl_cd]
AND SKD_VOY_NO=@[skd_voy_no]
AND SKD_DIR_CD=@[skd_dir_cd]			]]></sql>
			<params>
				<param name="foil_rmn_wgt" type="12" value="" out="N"/>
				<param name="doil_rmn_wgt" type="12" value="" out="N"/>
				<param name="clnd_oil_rmn_wgt" type="12" value="" out="N"/>
				<param name="sys_oil_rmn_wgt" type="12" value="" out="N"/>
				<param name="gnr_oil_rmn_wgt" type="12" value="" out="N"/>
				<param name="low_sulp_doil_rmn_wgt" type="12" value="" out="N"/>
				<param name="trbn_gnr_oil_rmn_wgt " type="12" value="" out="N"/>
				<param name="low_sulp_foil_rmn_wgt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
