<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchPackingInstructionPKGMethodListRSQLRSQL">
			<desc><![CDATA[PackingInstruction Creation]]></desc>
			<sql><![CDATA[
SELECT IMDG_PCK_INSTR_CD
     , IMDG_PCK_INSTR_SEQ
     , REGU_DP_NO
     , SUB_SEQ
     , IMDG_PCK_MZD_CD 
     , PCK_REF_CD
     , REF_DIV_NO
     , CMB_PCK_MAX_WGT    max_mass
     , LQD_MAX_WGT  max_ctnt
     , DELT_FLG delt_flg
FROM SCG_PCK_REGU_MZD
WHERE 1=1
AND IMDG_PCK_INSTR_CD     = @[imdg_pck_instr_cd]
AND IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]
AND NVL(DELT_FLG,'N')  ='N'			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="P001" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
