<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOSearchPortOfcCdListRSQL">
			<desc><![CDATA[해당 Port의 Office Code List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT OFC_CD AS INTG_CD_VAL_CTNT, OFC_CD AS INTG_CD_VAL_DP_DESC
FROM MDM_YARD
WHERE YD_CD LIKE @[vnor_fm_port_cd] || '%'
AND DELT_FLG = 'N'
GROUP BY OFC_CD
ORDER BY OFC_CD			]]></sql>
			<params>
				<param name="vnor_fm_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
