<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StevedoreDamageMgtDBDAOOpfStvDmgVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE OPF_STV_DMG SET 
	STV_DMG_NO = @[stv_dmg_no]
,	VSL_CD = @[vsl_cd]
,	SKD_VOY_NO = @[skd_voy_no]
,	SKD_DIR_CD = @[skd_dir_cd]
,	VPS_PORT_CD = @[vps_port_cd]
,	STV_DMG_EVNT_DT = TO_DATE(@[stv_dmg_evnt_dt],'YYYY-MM-DD')
,	VSL_OSHP_CNTR_BLK_TP_CD = @[vsl_oshp_cntr_blk_tp_cd]
,	STV_DMG_REF_NO = @[stv_dmg_ref_no]
,	CLM_HNDL_OFC_CD = @[clm_hndl_ofc_cd]
,	DMG_AUTH_STS_CD = @[dmg_auth_sts_cd]
#if (${auth_usr_id} != '') 
,	AUTH_USR_ID = @[auth_usr_id]
#end 
#if (${auth_dt} != '') 
,	AUTH_DT = TO_DATE(@[auth_dt],'YYYY-MM-DD')
#end
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE 
WHERE	STV_DMG_NO = @[stv_dmg_no]			]]></sql>
			<params>
				<param name="stv_dmg_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="stv_dmg_evnt_dt" type="12" value="" out="N"/>
				<param name="vsl_oshp_cntr_blk_tp_cd" type="12" value="" out="N"/>
				<param name="stv_dmg_ref_no" type="12" value="" out="N"/>
				<param name="clm_hndl_ofc_cd" type="12" value="" out="N"/>
				<param name="dmg_auth_sts_cd" type="12" value="" out="N"/>
				<param name="auth_usr_id" type="12" value="" out="N"/>
				<param name="auth_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
