<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOSearchLaneCheckRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT
	A.VSL_SLAN_CD, 
	A.VSL_SLAN_NM, 
	A.VSL_SVC_TP_CD, 
	A.spcl_cgo_rqst_tgt_lane_flg,
	A.TML_PROD_RPT_FLG,
	A.UPD_USR_ID,A.UPD_DT,
	(SELECT D.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL D
	  WHERE D.INTG_CD_ID = 'CD00717' 
		AND D.INTG_CD_VAL_CTNT = A.VSL_SVC_TP_CD )SVC_TYPE_NAME
FROM MDM_VSL_SVC_LANE A
WHERE 1 = 1
AND A.spcl_cgo_rqst_tgt_lane_flg = 'Y'
AND A.DELT_FLG='N'
AND A.VSL_SLAN_CD = @[vsl_slan_cd]
ORDER BY A.VSL_SLAN_CD			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
