<sqls>
	<querys>
		<query name="PortInformationMgtDBDAOVskPortCnlTrScgVOUSQL">
			<desc><![CDATA[SKD_DIR_CD 추가 -- 2014.06.16]]></desc>
			<sql><![CDATA[
UPDATE VSK_PORT_CNL_TR_SCG SET 
	TR_SCG_RTO = @[tr_scg_rto] 
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	LOC_CD = @[loc_cd]
AND	TR_SEQ = @[tr_seq]
AND SKD_DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="tr_scg_rto" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="tr_seq" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
