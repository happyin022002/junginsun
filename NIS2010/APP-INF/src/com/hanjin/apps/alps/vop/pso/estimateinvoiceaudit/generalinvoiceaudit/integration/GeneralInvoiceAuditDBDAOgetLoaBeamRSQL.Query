<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetLoaBeamRSQL">
			<desc><![CDATA[Tariff 비용 계산을 위해 해당 vvd의 LOA * Beam을 구한다.]]></desc>
			<sql><![CDATA[
SELECT ROUND (LOA_LEN * VSL_WDT, 2)
FROM MDM_VSL_CNTR
WHERE VSL_CD = substr(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
