<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOSearchVslSkdByPfSkdRSQL">
			<desc><![CDATA[SearchVslSkdByPfSkd]]></desc>
			<sql><![CDATA[
SELECT	VSL_CD,	SKD_VOY_NO, SKD_DIR_CD,	TO_CHAR(N1ST_PORT_BRTH_DT, 'YYYY-MM-DD HH24:MI') AS N1ST_PORT_BRTH_DT
FROM	VSK_VSL_SKD
WHERE	VSL_SLAN_CD	= @[vsl_slan_cd]
AND	PF_SKD_TP_CD	= @[pf_svc_tp_cd]
ORDER BY VSL_CD,SKD_VOY_NO, SKD_DIR_CD			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="pf_svc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
