<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchScgGuidDtlRSQL">
			<desc><![CDATA[SearchScgGuidDtl]]></desc>
			<sql><![CDATA[
SELECT  SPCL_CGO_GUID_CD AS SPCL_CGO_GUID_CD
,SPCL_CGO_GUID_SEQ AS SPCL_CGO_GUID_SEQ
,HDR_CTNT AS HDR_CTNT
,FTR_CTNT AS FTR_CTNT
,CRE_USR_ID AS CRE_USR_ID
,CRE_DT AS CRE_DT
,UPD_USR_ID AS UPD_USR_ID
,TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI:SS') AS UPD_DT
FROM    SCG_GUID_DTL
WHERE   1=1
AND     SPCL_CGO_GUID_CD = @[scg_flg]
#if (${subject_cd} == 'S')
AND     UPPER(HDR_CTNT) LIKE UPPER('%' || @[subject] || '%')
#end

#if (${subject_cd} == 'W')
AND     UPPER(FTR_CTNT) LIKE UPPER('%' || @[subject] || '%')
#end
ORDER BY NLSSORT(HDR_CTNT, 'NLS_SORT = GENERIC_M_CI' ), SPCL_CGO_GUID_SEQ			]]></sql>
			<params>
				<param name="scg_flg" type="12" value="" out="N"/>
				<param name="subject" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
