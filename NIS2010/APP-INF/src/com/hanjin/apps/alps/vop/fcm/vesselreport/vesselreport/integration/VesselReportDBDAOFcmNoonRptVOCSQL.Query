<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselReportDBDAOFcmNoonRptVOCSQL">
			<desc><![CDATA[FCM_NOON_RPT_LOG table 에서 FCM_NOON_RPT table 로 데이타 생성]]></desc>
			<sql><![CDATA[

INSERT INTO FCM_NOON_RPT
SELECT
VSL_CD,
SUBSTR(VOY_DIR_CD, 1, 4) SKD_VOY_NO,
SUBSTR(VOY_DIR_CD, 5, 1) SKD_DIR_CD,
NOON_RPT_DT,
VSL_SLAN_CD,
REF_NO,
NOON_RPT_LAT,
NOON_RPT_LON,
WND_FRC WND_DIR_CTNT,
'' WND_SCL_NO,
SEA_FRC OCN_CRNT_CTNT,
'' SEA_STE_NO,
VIS_RNG_NO,
REPLACE(SAIL_HRMNT, ' ', '0'),
NVGT_ML_DIST,
SAIL_AVG_SPD,
MN_PWR,
ENG_ML_DIST,
SAIL_AVG_RPM_PWR,
SLP_RT,
LOD_IND_QTY,
RMN_DIST,
RMN_AVG_SPD,
CRS_NO,
MN_FOIL_CSM_QTY,
MN_LOW_SULP_FOIL_CSM_QTY,
GNR_FOIL_CSM_QTY,
GNR_LOW_SULP_FOIL_CSM_QTY,
BLR_FOIL_CSM_QTY,
BLR_LOW_SULP_FOIL_CSM_QTY,
MN_DOIL_CSM_QTY,
MN_LOW_SULP_DOIL_CSM_QTY,
NXT_PORT_CD,
NXT_PORT_ETA_DT,
BILGE_CHK_FLG,
HLD_GAS_CHK_FLG,
HLD_TEMP_CHK_FLG,
BLST_XCH_FLG,
HLD_CLN_FLG,
PSC_PRPR_FLG,
DG_SAIL_FLG,
DG_SAIL_RMK,
NOON_RPT_RMK,
'TEST' CRE_USR_ID,
SYSDATE CRE_DT,
'TEST' UPD_USR_ID,
SYSDATE UPD_DT
FROM FCM_NOON_RPT_LOG
WHERE RCV_SEQ = (SELECT MAX(RCV_SEQ) FROM FCM_NOON_RPT_LOG)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
