<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOAddPsoTrfAtchFileCSQL">
			<desc><![CDATA[addPsoTrfAtchFile]]></desc>
			<sql><![CDATA[
insert into 
PSO_TRF_ATCH_FILE (
	 YD_CHG_NO
	,FILE_SAVE_ID
    ,ATCH_FILE_DIV_CD
	,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT
    ,CTRT_SCR_FLG
) values (
	 @[yd_chg_no]
	,@[file_sav_id]
    ,@[atch_file_div_cd]
	,@[cre_usr_id]
	,sysdate
	,@[upd_usr_id]
	,sysdate
	,DECODE(@[ctrt_scr_flg], 1, 'Y', 'N')
)			]]></sql>
			<params>
				<param name="yd_chg_no" type="12" value="1" out="N"/>
				<param name="file_sav_id" type="12" value="1" out="N"/>
				<param name="atch_file_div_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="1" out="N"/>
				<param name="upd_usr_id" type="12" value="1" out="N"/>
				<param name="ctrt_scr_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
