<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOcheckTurningPortRSQL">
			<desc><![CDATA[checkTurningPort]]></desc>
			<sql><![CDATA[
SELECT nvl(max(1), 0)
FROM VSK_VSL_PORT_SKD
WHERE 
VSL_CD = substr(@[vvd], 1, 4)
AND SKD_VOY_NO = substr(@[vvd], 5, 4)
AND SKD_DIR_CD = substr(@[vvd], 9)
AND YD_CD = @[yd_cd]
AND TURN_PORT_IND_CD IN ('Y','N')			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
