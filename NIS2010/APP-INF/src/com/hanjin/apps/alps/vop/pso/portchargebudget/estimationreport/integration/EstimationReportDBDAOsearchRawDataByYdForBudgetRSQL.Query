<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationReportDBDAOsearchRawDataByYdForBudgetRSQL">
			<desc><![CDATA[Raw data : 추정결산 YD별 집계]]></desc>
			<sql><![CDATA[
--PSO 사업계획에서 조회한 YD별 집계
SELECT RLANE_CD,
       VSL_CD,
       SKD_VOY_NO,
       SKD_DIR_CD,
       YD_CD,
       DECODE(YD_CD, 'PAPACT1', 0, 'EGSUZT1', 0, SUM(INV_USD_AMT)) PORT_CHARGE,
       DECODE(YD_CD, 'PAPACT1', SUM(INV_USD_AMT), 'EGSUZT1', SUM(INV_USD_AMT), 0) CANAL_FEE,
       (SELECT CNTR_DZN_CAPA FROM MDM_VSL_CNTR WHERE VSL_CD = EXPN.VSL_CD) CNTR_DZN_CAPA
FROM   PSO_TGT_YD_EXPN EXPN
WHERE  1=1
AND    REV_YRMON= REPLACE(@[rev_yrmon],'-','')
AND    PSO_BZTP_CD='1'
AND    BUD_SCNR_NO = substr(@[rev_yrmon],0,4) || @[bud_str]
GROUP BY RLANE_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, YD_CD
ORDER BY RLANE_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, YD_CD			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="bud_str" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
