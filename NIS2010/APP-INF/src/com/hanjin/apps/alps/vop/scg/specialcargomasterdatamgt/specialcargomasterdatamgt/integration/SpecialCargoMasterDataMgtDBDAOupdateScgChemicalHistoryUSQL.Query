<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOupdateScgChemicalHistoryUSQL">
			<desc><![CDATA[Chemical history를 update하기 위함.]]></desc>
			<sql><![CDATA[
UPDATE SCG_CHEM_HIS SET
       chem_nm          = @[chem_nm]
      ,chem_abst_svc_no = @[chem_abst_svc_no1]||@[chem_abst_svc_no2]||@[chem_abst_svc_no3]
      ,spcl_cgo_flg     = @[spcl_cgo_flg]
      ,co_nm            = @[co_nm]
      ,chem_opin_knd_cd = @[chem_opin_knd_cd]
      ,cmt_ctnt           = @[cmt_ctnt]
      ,imdg_spcl_provi_no = @[imdg_spcl_provi_no]
      ,rqst_ofc_cd        = @[rqst_ofc_cd]
      ,rqst_id            = @[rqst_id]
      ,rqst_dt            = @[rqst_dt]
      ,re_msg_id          = @[re_msg_id]
      ,re_msg_dt          = @[re_msg_dt]
      ,upd_usr_id         = @[upd_usr_id]
      ,upd_dt             = sysdate
WHERE chem_seq = @[chem_seq]			]]></sql>
			<params>
				<param name="chem_nm" type="12" value="" out="N"/>
				<param name="chem_abst_svc_no1" type="12" value="" out="N"/>
				<param name="chem_abst_svc_no2" type="12" value="" out="N"/>
				<param name="chem_abst_svc_no3" type="12" value="" out="N"/>
				<param name="spcl_cgo_flg" type="12" value="" out="N"/>
				<param name="co_nm" type="12" value="" out="N"/>
				<param name="chem_opin_knd_cd" type="12" value="" out="N"/>
				<param name="cmt_ctnt" type="12" value="" out="N"/>
				<param name="imdg_spcl_provi_no" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="rqst_id" type="12" value="" out="N"/>
				<param name="rqst_dt" type="12" value="" out="N"/>
				<param name="re_msg_id" type="12" value="" out="N"/>
				<param name="re_msg_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="chem_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
