<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOAddPackingInstructionRegulationPKGMethodRefListCSQL">
			<desc><![CDATA[Packing Instruction Creation Ref.]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_PCK_REF
( PCK_REF_CD
, REF_DIV_NO
, IMDG_PCK_INSTR_CD
, IMDG_PCK_INSTR_SEQ
, REF_DESC
, DELT_FLG
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
) 
SELECT @[pck_ref_cd]
    , @[ref_div_no]
    , @[imdg_pck_instr_cd]
    , NVL(MAX(imdg_pck_instr_seq), 0) + 1
    , @[ref_desc]
    , 'N'
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
FROM SCG_PCK_REF     
WHERE 1=1      
 AND PCK_REF_CD     = @[pck_ref_cd]
 AND REF_DIV_NO     = @[ref_div_no] 
 AND IMDG_PCK_INSTR_CD     = @[imdg_pck_instr_cd]			]]></sql>
			<params>
				<param name="pck_ref_cd" type="12" value="PKG" out="N"/>
				<param name="ref_div_no" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_cd" type="12" value="P002" out="N"/>
				<param name="ref_desc" type="12" value="These inner packagings shall be sift-proof." out="N"/>
				<param name="cre_usr_id" type="12" value="test" out="N"/>
				<param name="upd_usr_id" type="12" value="test" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
