<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualScheduleMgtDBDAOAddVskActPortSkdCSQL">
			<desc><![CDATA[Actual Schedule을 입력합니다.
---------------------------------------------------------------------------------------
2010.12.20 진마리아 [CHM-201007578-01] INP_DT, INP_USR_ID 컬럼 입력값 조건 변경
2011.10.04 김민아 [CHM-201112983-01] Actual SKD Creation 및 Inquiry 화면 및 로직 변경. ATA,ATB,ATD 별 Remark 칼럼 추가]]></desc>
			<sql><![CDATA[
/******************************************************************************/
/* java파일 생성 후 java파일에서 '.NUMERIC, .FLOAT' 을 '.VARCHAR' 로 치환하여야 함. */
/******************************************************************************/
INSERT INTO VSK_ACT_PORT_SKD (
	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	VPS_PORT_CD
,	CLPT_IND_SEQ
,	PORT_SKD_STS_CD
,	LST_ETA_DT
,	ACT_ARR_DT
,	VSL_ARR_DLAY_RSN_CD
,	ACT_ATA_INP_DT
,	ACT_ATA_INP_USR_ID
,	LST_ETB_DT
,	ACT_BRTH_DT
,	VSL_BRTH_DLAY_RSN_CD
,	ACT_ATB_INP_DT
,	ACT_ATB_INP_USR_ID
,	LST_ETD_DT
,	ACT_DEP_DT
,	VSL_DEP_DLAY_RSN_CD
,	ACT_ATD_INP_DT
,	ACT_ATD_INP_USR_ID
,	ARR_FOIL_WGT
,	ARR_LOW_SULP_FOIL_WGT
,	ARR_DOIL_WGT
,	ARR_LOW_SULP_DOIL_WGT
,	ARR_FRSH_WTR_WGT
,	ARR_BLST_WGT
,	ARR_FWDDR_HGT
,	ARR_AFTDR_HGT
,	ARR_GM_HGT
,	ARR_TUG_BOT_KNT
,	SPL_FOIL_WGT
,	SPL_LOW_SULP_FOIL_WGT
,	SPL_DOIL_WGT
,	SPL_LOW_SULP_DOIL_WGT
,	SPL_FRSH_WTR_WGT
,	DEP_LOW_SULP_FOIL_WGT
,	DEP_FOIL_WGT
,	DEP_LOW_SULP_DOIL_WGT
,	DEP_DOIL_WGT
,	DEP_FRSH_WTR_WGT
,	DEP_BLST_WGT
,	DEP_FWDDR_HGT
,	DEP_AFTDR_HGT
,	DEP_GM_HGT
,	DEP_TUG_BOT_KNT
,	TTL_SLG_WGT
,	TTL_GBG_QTY
,	PLT_LST_UNLD_DT
,	BFR_BRTH_ANK_DRP_DT
,	BFR_BRTH_ANK_OFF_DT
,	AFT_UNBRTH_ANK_DRP_DT
,	AFT_UNBRTH_ANK_OFF_DT
,	DIFF_RMK
,	ACT_ARR_RMK
,	ACT_BRTH_RMK
,	ACT_DEP_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT

) VALUES( 
	@[vsl_cd]
,	@[skd_voy_no]
,	@[skd_dir_cd]
,	@[vps_port_cd]
,	@[clpt_ind_seq]
,	@[port_skd_sts_cd]
,	TO_DATE(@[lst_eta_dt],'YYYYMMDDHH24MI')
,	TO_DATE(@[act_arr_dt],'YYYYMMDDHH24MI')
,	@[vsl_arr_dlay_rsn_cd]
,	CASE WHEN @[act_arr_dt] IS NULL THEN NULL				/* ACT_ARR_DT == NULL 이면, ACT_ATA_INP_DT = NULL */
		 WHEN @[act_ata_inp_dt] IS NULL THEN SYSDATE		/* ACT_ARR_DT != NULL 이고 ACT_ATA_INP_DT 입력값이 없으면, ACT_ATA_INP_DT = SYSDATE */
         ELSE TO_DATE(@[act_ata_inp_dt], 'YYYYMMDDHH24MISS')	/* ACT_ATA_INP_DT 입력값 */
    END
,	CASE WHEN @[act_arr_dt] IS NULL THEN NULL
		 WHEN @[act_ata_inp_usr_id] IS NULL THEN @[upd_usr_id]
		 ELSE @[act_ata_inp_usr_id]
    END
,	TO_DATE(@[lst_etb_dt],'YYYYMMDDHH24MI')
,	TO_DATE(@[act_brth_dt],'YYYYMMDDHH24MI')
,	@[vsl_brth_dlay_rsn_cd]
,	CASE WHEN @[act_brth_dt] IS NULL THEN NULL
		 WHEN @[act_atb_inp_dt] IS NULL THEN SYSDATE
		 ELSE TO_DATE(@[act_atb_inp_dt], 'YYYYMMDDHH24MISS')
    END
,	CASE WHEN @[act_brth_dt] IS NULL THEN NULL
		 WHEN @[act_atb_inp_usr_id] IS NULL THEN @[upd_usr_id]
		 ELSE @[act_atb_inp_usr_id]
    END
,	TO_DATE(@[lst_etd_dt],'YYYYMMDDHH24MI')
,	TO_DATE(@[act_dep_dt],'YYYYMMDDHH24MI')
,	@[vsl_dep_dlay_rsn_cd]
,	CASE WHEN @[act_dep_dt] IS NULL THEN NULL
		 WHEN @[act_atd_inp_dt] IS NULL THEN SYSDATE
		 ELSE TO_DATE(@[act_atd_inp_dt], 'YYYYMMDDHH24MISS')
	END
,	CASE WHEN @[act_dep_dt] IS NULL THEN NULL
		 WHEN @[act_atd_inp_usr_id] IS NULL THEN @[upd_usr_id]
		 ELSE @[act_atd_inp_usr_id]
    END
,	TO_NUMBER(NVL(@[arr_foil_wgt]              , 0))
,	TO_NUMBER(NVL(@[arr_low_sulp_foil_wgt]     , 0))
,	TO_NUMBER(NVL(@[arr_doil_wgt]              , 0))
,	TO_NUMBER(NVL(@[arr_low_sulp_doil_wgt]     , 0))
,	TO_NUMBER(NVL(@[arr_frsh_wtr_wgt]          , 0))
,	TO_NUMBER(NVL(@[arr_blst_wgt]              , 0))
,	TO_NUMBER(NVL(@[arr_fwddr_hgt]             , 0))
,	TO_NUMBER(NVL(@[arr_aftdr_hgt]             , 0))
,	TO_NUMBER(NVL(@[arr_gm_hgt]                , 0))
,	TO_NUMBER(NVL(@[arr_tug_bot_knt]           , 0))
,	TO_NUMBER(NVL(@[spl_foil_wgt]              , 0))
,	TO_NUMBER(NVL(@[spl_low_sulp_foil_wgt]     , 0))
,	TO_NUMBER(NVL(@[spl_doil_wgt]              , 0))
,	TO_NUMBER(NVL(@[spl_low_sulp_doil_wgt]     , 0))
,	TO_NUMBER(NVL(@[spl_frsh_wtr_wgt]          , 0))
,	TO_NUMBER(NVL(@[dep_low_sulp_foil_wgt]     , 0))
,	TO_NUMBER(NVL(@[dep_foil_wgt]              , 0))
,	TO_NUMBER(NVL(@[dep_low_sulp_doil_wgt]     , 0))
,	TO_NUMBER(NVL(@[dep_doil_wgt]              , 0))
,	TO_NUMBER(NVL(@[dep_frsh_wtr_wgt]          , 0))
,	TO_NUMBER(NVL(@[dep_blst_wgt]              , 0))
,	TO_NUMBER(NVL(@[dep_fwddr_hgt]             , 0))
,	TO_NUMBER(NVL(@[dep_aftdr_hgt]             , 0))
,	TO_NUMBER(NVL(@[dep_gm_hgt]                , 0))
,	TO_NUMBER(NVL(@[dep_tug_bot_knt]           , 0))
,	TO_NUMBER(NVL(@[ttl_slg_wgt]               , 0))
,	TO_NUMBER(NVL(@[ttl_gbg_qty]               , 0))
,	TO_DATE(@[plt_lst_unld_dt]       ,'YYYYMMDDHH24MI')
,	TO_DATE(@[bfr_brth_ank_drp_dt]   ,'YYYYMMDDHH24MI')
,	TO_DATE(@[bfr_brth_ank_off_dt]   ,'YYYYMMDDHH24MI')
,	TO_DATE(@[aft_unbrth_ank_drp_dt] ,'YYYYMMDDHH24MI')
,	TO_DATE(@[aft_unbrth_ank_off_dt] ,'YYYYMMDDHH24MI')
,	@[diff_rmk]
,	@[act_arr_rmk]
,	@[act_brth_rmk]
,	@[act_dep_rmk]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)
			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="port_skd_sts_cd" type="12" value="" out="N"/>
				<param name="lst_eta_dt" type="12" value="" out="N"/>
				<param name="act_arr_dt" type="12" value="" out="N"/>
				<param name="vsl_arr_dlay_rsn_cd" type="12" value="" out="N"/>
				<param name="act_ata_inp_dt" type="12" value="" out="N"/>
				<param name="act_ata_inp_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="lst_etb_dt" type="12" value="" out="N"/>
				<param name="act_brth_dt" type="12" value="" out="N"/>
				<param name="vsl_brth_dlay_rsn_cd" type="12" value="" out="N"/>
				<param name="act_atb_inp_dt" type="12" value="" out="N"/>
				<param name="act_atb_inp_usr_id" type="12" value="" out="N"/>
				<param name="lst_etd_dt" type="12" value="" out="N"/>
				<param name="act_dep_dt" type="12" value="" out="N"/>
				<param name="vsl_dep_dlay_rsn_cd" type="12" value="" out="N"/>
				<param name="act_atd_inp_dt" type="12" value="" out="N"/>
				<param name="act_atd_inp_usr_id" type="12" value="" out="N"/>
				<param name="arr_foil_wgt" type="6" value="" out="N"/>
				<param name="arr_low_sulp_foil_wgt" type="6" value="" out="N"/>
				<param name="arr_doil_wgt" type="6" value="" out="N"/>
				<param name="arr_low_sulp_doil_wgt" type="6" value="" out="N"/>
				<param name="arr_frsh_wtr_wgt" type="6" value="" out="N"/>
				<param name="arr_blst_wgt" type="6" value="" out="N"/>
				<param name="arr_fwddr_hgt" type="6" value="" out="N"/>
				<param name="arr_aftdr_hgt" type="6" value="" out="N"/>
				<param name="arr_gm_hgt" type="6" value="" out="N"/>
				<param name="arr_tug_bot_knt" type="2" value="" out="N"/>
				<param name="spl_foil_wgt" type="6" value="" out="N"/>
				<param name="spl_low_sulp_foil_wgt" type="6" value="" out="N"/>
				<param name="spl_doil_wgt" type="6" value="" out="N"/>
				<param name="spl_low_sulp_doil_wgt" type="6" value="" out="N"/>
				<param name="spl_frsh_wtr_wgt" type="6" value="" out="N"/>
				<param name="dep_low_sulp_foil_wgt" type="6" value="" out="N"/>
				<param name="dep_foil_wgt" type="6" value="" out="N"/>
				<param name="dep_low_sulp_doil_wgt" type="6" value="" out="N"/>
				<param name="dep_doil_wgt" type="6" value="" out="N"/>
				<param name="dep_frsh_wtr_wgt" type="6" value="" out="N"/>
				<param name="dep_blst_wgt" type="6" value="" out="N"/>
				<param name="dep_fwddr_hgt" type="6" value="" out="N"/>
				<param name="dep_aftdr_hgt" type="6" value="" out="N"/>
				<param name="dep_gm_hgt" type="6" value="" out="N"/>
				<param name="dep_tug_bot_knt" type="2" value="" out="N"/>
				<param name="ttl_slg_wgt" type="12" value="" out="N"/>
				<param name="ttl_gbg_qty" type="12" value="" out="N"/>
				<param name="plt_lst_unld_dt" type="12" value="" out="N"/>
				<param name="bfr_brth_ank_drp_dt" type="12" value="" out="N"/>
				<param name="bfr_brth_ank_off_dt" type="12" value="" out="N"/>
				<param name="aft_unbrth_ank_drp_dt" type="12" value="" out="N"/>
				<param name="aft_unbrth_ank_off_dt" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="act_arr_rmk" type="12" value="" out="N"/>
				<param name="act_brth_rmk" type="12" value="" out="N"/>
				<param name="act_dep_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
