<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAORemoveVskSwapCstPortDSQL">
			<desc><![CDATA[Vsk_Swap_Cst_Port  자기 자신의 VVD를 지운다]]></desc>
			<sql><![CDATA[
DELETE FROM VSK_SWAP_CST_PORT
WHERE	1 = 1
#if (${sim_dt} != '') 
AND SIM_DT = TO_DATE(@[sim_dt], 'YYYY-MM-DD')
AND	SIM_NO = TO_NUMBER(@[sim_no])
#else 
AND	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
#end			]]></sql>
			<params>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
