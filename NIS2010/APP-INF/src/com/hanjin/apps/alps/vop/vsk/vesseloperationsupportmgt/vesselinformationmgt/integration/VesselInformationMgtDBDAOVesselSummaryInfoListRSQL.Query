<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselInformationMgtDBDAOVesselSummaryInfoListRSQL">
			<desc><![CDATA[Vessel Summary 정보를 조회한다.]]></desc>
			<sql><![CDATA[
#if (${vsl_type} == 'CNTR')
    SELECT  VSL_CD        -- Vessel Code    
          , VSL_ENG_NM    -- Name : ENG, China
          , VSL_LOCL_NM   -- Carrier Code
          , CRR_CD        -- Carrier TYPE     
          , BLK_CRR_TP_CD    -- Owner   
          , VSL_OWN_IND_CD   -- Ship Type
          , VSL_TYPE         -- Capacity
          
           --Capacity
          , CNTR_VSL_CLSS_CAPA
          , CNTR_DZN_CAPA
          , CNTR_PNM_CAPA
           
          -- Identification 
          , LLOYD_NO
          , CALL_SGN_NO
          , RGST_NO
          
          -- Class
          , CLSS_NO_RGST_AREA_NM
          , VSL_CLSS_NO
         
          -- Registry
          , CNT_CD
          , CNT_NM
          , PICLB_DESC
          
          -- Built & Delivery date
          , VSL_BLDR_NM
          , VSL_HL_NO
          , VSL_KEL_LY_DT
          , VSL_LNCH_DT
          , VSL_DE_DT
          , RGST_DT
          
          -- Communication
          , PHN_NO
          , FAX_NO
          , TLX_NO
          , VSL_EML
          
          -- Reefer
          , RF_RCPT_KNT
          , RF_RCPT_MAX_KNT
          
          -- Speed
          , ECN_SPD
          , VSL_SVC_SPD
          , MAX_SPD
          , BLST_WGT_SPD1
          , LDN_WGT_SPD1
          
          -- Dimension
          , LOA_LEN
          , LBP_LEN
          , VSL_WDT
          , VSL_DPTH
          , VSL_HGT
          , SMR_DRFT_HGT
          , FBD_CAPA
          , WNT_DRFT_HGT
          , TROP_DRFT_HGT
          
          -- Tonnage
          , DPL_CAPA
          , DWT_WGT
          , LGT_SHP_TONG_WGT
          , WNT_DWT_WGT
          , TROP_DWT_WGT
          
          -- Gross Tons
          , GRS_RGST_TONG_WGT
          , PNM_GT_WGT
          , SUZ_GT_WGT
         
          -- Net Tons 
          , NET_RGST_TONG_WGT
          , PNM_NET_TONG_WGT
          , SUZ_NET_TONG_WGT
          , MADN_VOY_SUZ_NET_TONG_WGT
          , INTL_TONG_CERTI_FLG
         
          -- Tank Capacity
          , FOIL_CAPA
          , DOIL_CAPA
          , FRSH_WTR_CAPA
          , BLST_TNK_CAPA
          
          -- Consumption
          , FOIL_CSM
          , DOIL_CSM
          , FRSH_WTR_CSM
          , FOIL_BLST_CSM2
          , PORT_FOIL_TON_CSM
          , PORT_IDLE_DOIL_TON_CSM
          , PORT_WRK_DOIL_TON_CSM
          
          -- Main Engine
          , MN_ENG_MKR_NM
          , MN_ENG_TP_DESC
          , MN_ENG_BHP_PWR
          , MN_ENG_RPM_PWR
          
          -- Generator Engine
          , GNR_MKR_NM
          , GNR_TP_DESC
          , GNR_BHP_PWR
          , GNR_RPM_PWR
          
          -- Bow Thrust
          , BWTHST_MKR_NM
          , BWTHST_TP_DESC
          , BWTHST_BHP_PWR
          , BWTHST_RPM_PWR
          
          -- RPM & Slow Steaming
          , CTCL_RPM_NO
          , CTCL_TO_RPM_NO
          , OP_MIN_RPM_NO
          , OP_MIN_SPD
          , SLW_STMNG_FLG
          , SPR_SLW_STMNG_FLG
          , FUEL_SAV_EQ_FLG
          , VSL_LOD_RTO
          
          -- Design Max Load Hold/Deck
          , IN_HLD_PER_TR_KNT
          , IN_HLD_PER_ROW_KNT
          , HTCH_CVR_IN_HLD_KNT
          , ON_DECK_PER_TR_KNT
          , ON_DECK_PER_ROW_KNT
          
          -- Other
          , CRW_KNT
		  , VSL_SLAN_CD
		  , VSL_SVC_TP_CD
		  , DECODE(AMP_TP_CD, '', 'N/A', 'C', 'CNTR', 'F', 'FIXED', AMP_TP_CD) AS AMP_TP_CD
FROM (
        SELECT  
              
                T1.VSL_CD                                          AS VSL_CD        -- Vessel Code    
              , T1.VSL_ENG_NM                                      AS VSL_ENG_NM    -- Name : ENG, China
              , T1.VSL_LOCL_NM                                     AS VSL_LOCL_NM   -- Carrier Code
              , NVL(T1.CRR_CD, T4.ACT_CRR_CD)                      AS CRR_CD        -- Carrier TYPE     
              , NULL                                               AS BLK_CRR_TP_CD      -- Owner   
              , DECODE(T1.VSL_OWN_IND_CD, 'O', 'SML', 'CHARTER')   AS VSL_OWN_IND_CD      -- Ship Type
              , 'CNTR'                                             AS VSL_TYPE   -- Capacity
              
               --Capacity
              , T1.CNTR_VSL_CLSS_CAPA                              AS CNTR_VSL_CLSS_CAPA
              , T1.CNTR_DZN_CAPA                                   AS CNTR_DZN_CAPA
              , T1.CNTR_PNM_CAPA                                   AS CNTR_PNM_CAPA
               
              -- Identification 
              , T1.LLOYD_NO                                        AS LLOYD_NO
              , T1.CALL_SGN_NO                                     AS CALL_SGN_NO
              , T1.RGST_NO                                         AS RGST_NO
              
              -- Class
              , T1.CLSS_NO_RGST_AREA_NM                            AS CLSS_NO_RGST_AREA_NM
              , T1.VSL_CLSS_NO                                     AS VSL_CLSS_NO
             
              -- Registry
              , SUBSTR(T1.RGST_PORT_CD, 1, 2)                      AS CNT_CD
              , T3.CNT_NM                                          AS CNT_NM
              , T1.PICLB_DESC                                      AS PICLB_DESC
              
              -- Built & Delivery date
              , T1.VSL_BLDR_NM                                     AS VSL_BLDR_NM
              , T1.VSL_HL_NO                                       AS VSL_HL_NO
              , TO_CHAR(T1.VSL_KEL_LY_DT, 'YYYY-MM-DD')        AS VSL_KEL_LY_DT
              , TO_CHAR(T1.VSL_LNCH_DT  , 'YYYY-MM-DD')        AS VSL_LNCH_DT
              , TO_CHAR(T1.VSL_DE_DT    , 'YYYY-MM-DD')        AS VSL_DE_DT
              , TO_CHAR(T1.RGST_DT      , 'YYYY-MM-DD')        AS RGST_DT
              
              -- Communication
              , T1.PHN_NO                                          AS PHN_NO
              , T1.FAX_NO                                          AS FAX_NO
              , T1.TLX_NO                                          AS TLX_NO
              , T1.VSL_EML                                         AS VSL_EML
              
              -- Reefer
              , T1.RF_RCPT_KNT                                     AS RF_RCPT_KNT
              , T1.RF_RCPT_MAX_KNT                                 AS RF_RCPT_MAX_KNT
              
              -- Speed
              , T1.ECN_SPD                                         AS ECN_SPD
              , T1.VSL_SVC_SPD                                     AS VSL_SVC_SPD
              , T1.MAX_SPD                                         AS MAX_SPD
              , NULL                                               AS BLST_WGT_SPD1
              , NULL                                               AS LDN_WGT_SPD1
              
              -- Dimension
              , T1.LOA_LEN                                         AS LOA_LEN
              , T1.LBP_LEN                                         AS LBP_LEN
              , T1.VSL_WDT                                         AS VSL_WDT
              , T1.VSL_DPTH                                        AS VSL_DPTH
              , T1.VSL_HGT                                         AS VSL_HGT
              , T1.SMR_DRFT_HGT                                    AS SMR_DRFT_HGT
              , T1.FBD_CAPA                                        AS FBD_CAPA
              , NULL                                               AS WNT_DRFT_HGT
              , NULL                                               AS TROP_DRFT_HGT
              
              -- Tonnage
              , T1.DPL_CAPA                                        AS DPL_CAPA
              , T1.DWT_WGT                                         AS DWT_WGT
              , T1.LGT_SHP_TONG_WGT                                AS LGT_SHP_TONG_WGT
              , NULL                                               AS WNT_DWT_WGT
              , NULL                                               AS TROP_DWT_WGT
              
              -- Gross Tons
              , T1.GRS_RGST_TONG_WGT                               AS GRS_RGST_TONG_WGT
              , T1.PNM_GT_WGT                                      AS PNM_GT_WGT
              , T1.SUZ_GT_WGT                                      AS SUZ_GT_WGT
             
              -- Net Tons 
              , T1.NET_RGST_TONG_WGT                               AS NET_RGST_TONG_WGT
              , T1.PNM_NET_TONG_WGT                                AS PNM_NET_TONG_WGT
              , T1.SUZ_NET_TONG_WGT                                AS SUZ_NET_TONG_WGT
              , T1.MADN_VOY_SUZ_NET_TONG_WGT                       AS MADN_VOY_SUZ_NET_TONG_WGT
              , T1.INTL_TONG_CERTI_FLG                             AS INTL_TONG_CERTI_FLG
             
              -- Tank Capacity
              , T1.FOIL_CAPA                                       AS FOIL_CAPA
              , T1.DOIL_CAPA                                       AS DOIL_CAPA
              , T1.FRSH_WTR_CAPA                                   AS FRSH_WTR_CAPA
              , T1.BLST_TNK_CAPA                                   AS BLST_TNK_CAPA
              
              -- Consumption
              , T1.FOIL_CSM                                        AS FOIL_CSM
              , T1.DOIL_CSM                                        AS DOIL_CSM
              , T1.FRSH_WTR_CSM                                    AS FRSH_WTR_CSM
              , NULL                                               AS FOIL_BLST_CSM2
              , NULL                                               AS PORT_FOIL_TON_CSM
              , NULL                                               AS PORT_IDLE_DOIL_TON_CSM
              , NULL                                               AS PORT_WRK_DOIL_TON_CSM
              
              -- Main Engine
              , T1.MN_ENG_MKR_NM                                   AS MN_ENG_MKR_NM
              , T1.MN_ENG_TP_DESC                                  AS MN_ENG_TP_DESC
              , T1.MN_ENG_BHP_PWR                                  AS MN_ENG_BHP_PWR
              , T1.MN_ENG_RPM_PWR                                  AS MN_ENG_RPM_PWR
              
              -- Generator Engine
              , T1.GNR_MKR_NM                                      AS GNR_MKR_NM
              , T1.GNR_TP_DESC                                     AS GNR_TP_DESC
              , T1.GNR_BHP_PWR                                     AS GNR_BHP_PWR
              , T1.GNR_RPM_PWR                                     AS GNR_RPM_PWR
              
              -- Bow Thrust
              , T1.BWTHST_MKR_NM                                  AS BWTHST_MKR_NM
              , T1.BWTHST_TP_DESC                                 AS BWTHST_TP_DESC
              , T1.BWTHST_BHP_PWR                                 AS BWTHST_BHP_PWR
              , T1.BWTHST_RPM_PWR                                 AS BWTHST_RPM_PWR
              
              -- RPM & Slow Steaming
              , T2.CTCL_RPM_NO                                    AS CTCL_RPM_NO
              , T2.CTCL_TO_RPM_NO                                 AS CTCL_TO_RPM_NO
              , T2.OP_MIN_RPM_NO                                  AS OP_MIN_RPM_NO
              , T2.OP_MIN_SPD                                     AS OP_MIN_SPD
              , T2.SLW_STMNG_FLG                                  AS SLW_STMNG_FLG
              , T2.SPR_SLW_STMNG_FLG                              AS SPR_SLW_STMNG_FLG
              , T2.FUEL_SAV_EQ_FLG                                AS FUEL_SAV_EQ_FLG
              , T2.VSL_LOD_RTO                                    AS VSL_LOD_RTO
              
              -- Design Max Load Hold/Deck
              , T2.IN_HLD_PER_TR_KNT                              AS IN_HLD_PER_TR_KNT
              , T2.IN_HLD_PER_ROW_KNT                             AS IN_HLD_PER_ROW_KNT
              , T2.HTCH_CVR_IN_HLD_KNT                            AS HTCH_CVR_IN_HLD_KNT
              , T2.ON_DECK_PER_TR_KNT                             AS ON_DECK_PER_TR_KNT
              , T2.ON_DECK_PER_ROW_KNT                            AS ON_DECK_PER_ROW_KNT
              
              -- Other
              , T1.CRW_KNT                                        AS CRW_KNT
    		  , T4.VSL_SLAN_CD
    		  , T4.VSL_SVC_TP_CD
              , ROW_NUMBER() OVER(ORDER BY T4.VPS_ETA_DT DESC) AS RNUM
			  , T4.VPS_ETA_DT
              , T2.AMP_TP_CD  
                FROM    MDM_VSL_CNTR        T1,
                        VSK_VSL_ADD_SPEC    T2,
                        MDM_COUNTRY         T3
                        ,
                        (
                        SELECT  VSL_CD, VSL_SLAN_CD, ACT_CRR_CD, VSL_SVC_TP_CD, VPS_ETA_DT
                        FROM    (
                                SELECT  S1.VSL_CD, S1.VSL_SLAN_CD, S1.ACT_CRR_CD, ROW_NUMBER() OVER (PARTITION BY S1.VSL_CD ORDER BY S2.VPS_ETA_DT DESC) AS SEQ, S3.VSL_SVC_TP_CD, S2.VPS_ETA_DT
                                FROM    VSK_VSL_SKD         S1,
                                        VSK_VSL_PORT_SKD    S2,
                                        MDM_VSL_SVC_LANE    S3
                                WHERE   1=1
                                AND     S1.VSL_CD           = S2.VSL_CD
                                AND     S1.SKD_VOY_NO       = S2.SKD_VOY_NO
                                AND     S1.SKD_DIR_CD       = S2.SKD_DIR_CD
                                AND     S1.VSL_SLAN_CD      = S3.VSL_SLAN_CD
                				#if (${vsl_cd} != '')
                                AND     S1.VSL_CD           = @[vsl_cd] -- :Vessel
                				#end
            					           
                                AND     S2.VPS_ETA_DT       BETWEEN TO_DATE(@[fm_date], 'YYYY-MM-DD')  AND  TO_DATE(@[to_date], 'YYYY-MM-DD') -- To Period                   
                                ) T4
                        WHERE   1=1
                        AND     SEQ = 1
                        ) T4
                WHERE   1=1
                AND     T1.VSL_CD                           = T2.VSL_CD (+)
                AND     SUBSTR(T1.RGST_PORT_CD, 1, 2)       = T3.CNT_CD (+)
                AND     T1.VSL_CD                           = T4.VSL_CD 
                AND     T1.DELT_FLG  						<> 'Y'
                #if (${vsl_cd} != '')
                AND     T1.VSL_CD                    		= @[vsl_cd] --   :Vessel
                #end
                
                #if (${vsl_svc_tp_cd} != '' && ${vsl_svc_tp_cd} != 'A')
                AND     T4.VSL_SVC_TP_CD               = @[vsl_svc_tp_cd] --   :Using type
                #end
            
                #if (${vsl_own_ind_cd} != '' && ${vsl_own_ind_cd} != 'A')
                AND     T1.VSL_OWN_IND_CD                   = @[vsl_own_ind_cd] --   :Owner (O : OWNER, C : CHARTER)
                #end
            
                #if (${crr_cd} != '')
                AND     NVL(T1.CRR_CD, T4.ACT_CRR_CD)       = @[crr_cd] --	:Carrier
                #end
            
            	#if (${vsl_slan_cd} != '')
                AND     T4.VSL_SLAN_CD                      = @[vsl_slan_cd] --   :Vessel Lane
                #end         
            
                #if (${cntr_vsl_clss_capa} != '')
                AND     T1.CNTR_VSL_CLSS_CAPA               = @[cntr_vsl_clss_capa] --   :Class
                #end
                ORDER BY 1
                )
WHERE   1=1


    
#else
SELECT    VSL_CD
        , VSL_ENG_NM
        , VSL_LOCL_NM
        , CRR_CD
        , VSL_OWN_IND_CD            
        , VSL_TYPE
        
        -- Capacity
        , CNTR_VSL_CLSS_CAPA
        , CNTR_DZN_CAPA
        
        -- Identification
        , LLOYD_NO
        , CALL_SGN_NO
        , RGST_NO
        
        -- Class
        , CLSS_NO_RGST_AREA_NM
        , VSL_CLSS_NO
        
        --Registry
        , CNT_CD
        , CNT_NM
        , PICLB_DESC
        
        -- Built & Delivery date
        , VSL_BLDR_NM
        , VSL_HL_NO
        , VSL_KEL_LY_DT
        , VSL_LNCH_DT
        , VSL_DE_DT
        , RGST_DT
        
        -- Communication
        , PHN_NO
        , FAX_NO
        , TLX_NO
        , VSL_EML
        
        -- Reefer
        , RF_RCPT_KNT
        , RF_RCPT_MAX_KNT
        
        -- Speed
        , ECN_SPD
        , VSL_SVC_SPD
        , MAX_SPD
        , BLST_WGT_SPD1   -- Ballast Speed1
        , LDN_WGT_SPD1    -- Laden Speed1
        
        -- Dimension
        , LOA_LEN 
        , LBP_LEN
        , VSL_WDT
        , VSL_DPTH
        , VSL_HGT
        , SMR_DRFT_HGT
        , WNT_DRFT_HGT    -- Winter Draft
        , TROP_DRFT_HGT   -- Tropical Draft
        , FBD_CAPA
        
        -- Tonnage
        , DPL_CAPA
        , DWT_WGT
        , WNT_DWT_WGT     -- Winter DWT
        , TROP_DWT_WGT    -- Tropical DWT
        , LGT_SHP_TONG_WGT    
        
        -- Gross Tons
        , GRS_RGST_TONG_WGT
        , PNM_GT_WGT
        , SUZ_GT_WGT
        
        -- Net Tons
        , NET_RGST_TONG_WGT 
        , PNM_NET_TONG_WGT
        , SUZ_NET_TONG_WGT
        , MADN_VOY_SUZ_NET_TONG_WGT
        , INTL_TONG_CERTI_FLG
        
        -- Tank Capacity
        , FOIL_CAPA
        , DOIL_CAPA
        , FRSH_WTR_CAPA
        , BLST_TNK_CAPA
        
        -- Consumption
        , FOIL_CSM            -- FO
        , FOIL_BLST_CSM2      -- FO
        , PORT_FOIL_TON_CSM   -- FO
        , DOIL_CSM            -- DO
        , PORT_IDLE_DOIL_TON_CSM  -- DO
        , PORT_WRK_DOIL_TON_CSM   -- DO
        , FRSH_WTR_CSM            -- FW
        
        -- Main Engine
        , MN_ENG_MKR_NM
        , MN_ENG_TP_DESC
        , MN_ENG_BHP_PWR
        , MN_ENG_RPM_PWR
        
        -- Generator Engine
        , GNR_MKR_NM
        , GNR_TP_DESC
        , GNR_BHP_PWR
        , GNR_RPM_PWR
        
        -- Bow Thrust
        , BWTHST_MKR_NM
        , BWTHST_TP_DESC
        , BWTHST_BHP_PWR
        , BWTHST_RPM_PWR
        
        -- RPM & Slow Steaming
        , CTCL_RPM_NO
        , CTCL_TO_RPM_NO
        , OP_MIN_RPM_NO
        , OP_MIN_SPD
        , SLW_STMNG_FLG
        , SPR_SLW_STMNG_FLG
        , FUEL_SAV_EQ_FLG
        , VSL_LOD_RTO
        
        -- Design Max Load Hold/Deck
        , IN_HLD_PER_TR_KNT
        , IN_HLD_PER_ROW_KNT
        , HTCH_CVR_IN_HLD_KNT
        , ON_DECK_PER_TR_KNT
        , ON_DECK_PER_ROW_KNT
FROM (            
            SELECT  VSL_CD                                                   AS VSL_CD
                    , T1.VSL_ENG_NM                                          AS VSL_ENG_NM
                    , T1.VSL_KRN_NM                                          AS VSL_LOCL_NM
                    , T1.BLK_CRR_TP_CD                                       AS CRR_CD
                    , DECODE(T1.VSL_OWN_IND_CD, 'O', 'SML', 'CHARTER')       AS VSL_OWN_IND_CD            
                    , 'BLK'                                                  AS VSL_TYPE
                    
                    -- Capacity
                    , NULL                                                   AS CNTR_VSL_CLSS_CAPA
                    , NULL                                                   AS CNTR_DZN_CAPA
                    
                    -- Identification
                    , T1.IMO_NO                                              AS LLOYD_NO
                    , T1.CALL_SGN_NO                                         AS CALL_SGN_NO
                    , NULL                                                   AS RGST_NO
                    
                    -- Class
                    , T1.BLK_VSL_CLSS_CD                                     AS CLSS_NO_RGST_AREA_NM
                    , NULL                                                   AS VSL_CLSS_NO
                    
                    --Registry
                    , T1.VSL_RGST_CNT_CD                                     AS CNT_CD
                    , T3.CNT_NM                                              AS CNT_NM
                    , NULL                                                   AS PICLB_DESC
                    
                    -- Built & Delivery date
                    , T1.VSL_BLDR_NM                                         AS VSL_BLDR_NM
                    , NULL                                                   AS VSL_HL_NO
                    , T1.VSL_BLD_DT||'0000'                                  AS VSL_KEL_LY_DT
                    , TO_CHAR(T1.LNCH_DT      , 'YYYY-MM-DD')            AS VSL_LNCH_DT
                    , TO_CHAR(T1.BLK_VSL_DE_DT, 'YYYY-MM-DD')            AS VSL_DE_DT
                    , TO_CHAR(T1.RGST_DT      , 'YYYY-MM-DD')            AS RGST_DT
                    
                    -- Communication
                    , NULL                                                   AS PHN_NO
                    , NULL                                                   AS FAX_NO
                    , NULL                                                   AS TLX_NO
                    , NULL                                                   AS VSL_EML
                    
                    -- Reefer
                    , NULL                                                   AS RF_RCPT_KNT
                    , NULL                                                   AS RF_RCPT_MAX_KNT
                    
                    -- Speed
                    , NULL                                                   AS ECN_SPD
                    , NULL                                                   AS VSL_SVC_SPD
                    , NULL                                                   AS MAX_SPD
                    , T1.BLST_WGT_SPD1                                       AS BLST_WGT_SPD1   -- Ballast Speed1
                    , T1.LDN_WGT_SPD1                                        AS LDN_WGT_SPD1    -- Laden Speed1
                    
                    -- Dimension
                    , T1.LOA_LEN                                             AS LOA_LEN 
                    , NULL                                                   AS LBP_LEN
                    , T1.VSL_BM_WDT                                          AS VSL_WDT
                    , NULL                                                   AS VSL_DPTH
                    , NULL                                                   AS VSL_HGT
                    , T1.SMR_DRFT_HGT                                        AS SMR_DRFT_HGT
                    , T1.WNT_DRFT_HGT                                        AS WNT_DRFT_HGT    -- Winter Draft
                    , T1.TROP_DRFT_HGT                                       AS TROP_DRFT_HGT   -- Tropical Draft
                    , NULL                                                   AS FBD_CAPA
                    
                    -- Tonnage
                    , NULL                                                   AS DPL_CAPA
                    , T1.SMR_DWT_WGT                                         AS DWT_WGT
                    , T1.WNT_DWT_WGT                                         AS WNT_DWT_WGT     -- Winter DWT
                    , T1.TROP_DWT_WGT                                        AS TROP_DWT_WGT    -- Tropical DWT
                    , T1.LGT_SHP_TONG_WGT                                    AS LGT_SHP_TONG_WGT    
                    
                    -- Gross Tons
                    , T1.GRS_RGST_TONG_WGT                                   AS GRS_RGST_TONG_WGT
                    , T1.PNM_GT_WGT                                          AS PNM_GT_WGT
                    , T1.SUZ_GT_WGT                                          AS SUZ_GT_WGT
                    
                    -- Net Tons
                    , T1.NET_RGST_TONG_WGT                                   AS NET_RGST_TONG_WGT 
                    , T1.PNM_NET_TONG_WGT                                    AS PNM_NET_TONG_WGT
                    , T1.SUZ_NET_TONG_WGT                                    AS SUZ_NET_TONG_WGT
                    , T1.SUZ_GT_WGT                                          AS MADN_VOY_SUZ_NET_TONG_WGT
                    , NULL                                                   AS INTL_TONG_CERTI_FLG
                    
                    -- Tank Capacity
                    , T1.FOIL_CAPA                                           AS FOIL_CAPA
                    , T1.DOIL_CAPA                                           AS DOIL_CAPA
                    , T1.FRSH_WTR_CAPA                                       AS FRSH_WTR_CAPA
                    , NULL                                                   AS BLST_TNK_CAPA
                    
                    -- Consumption
                    , T1.FOIL_LDN_CSM1                                       AS FOIL_CSM            -- FO
                    , T1.FOIL_BLST_CSM2                                      AS FOIL_BLST_CSM2      -- FO
                    , T1.PORT_FOIL_TON_CSM                                   AS PORT_FOIL_TON_CSM   -- FO
                    , T1.SEA_DOIL_TON_CSM                                    AS DOIL_CSM            -- DO
                    , T1.PORT_IDLE_DOIL_TON_CSM                              AS PORT_IDLE_DOIL_TON_CSM  -- DO
                    , T1.PORT_WRK_DOIL_TON_CSM                               AS PORT_WRK_DOIL_TON_CSM   -- DO
                    , NULL                                                   AS FRSH_WTR_CSM            -- FW
                    
                    -- Main Engine
                    , T1.MN_ENG_MKR_NM                                       AS MN_ENG_MKR_NM
                    , NULL                                                   AS MN_ENG_TP_DESC
                    , T1.MN_ENG_BHP_PWR                                      AS MN_ENG_BHP_PWR
                    , NULL                                                   AS MN_ENG_RPM_PWR
                    
                    -- Generator Engine
                    , NULL                                                   AS GNR_MKR_NM
                    , NULL                                                   AS GNR_TP_DESC
                    , NULL                                                   AS GNR_BHP_PWR
                    , NULL                                                   AS GNR_RPM_PWR
                    
                    -- Bow Thrust
                    , NULL                                                   AS BWTHST_MKR_NM
                    , NULL                                                   AS BWTHST_TP_DESC
                    , NULL                                                   AS BWTHST_BHP_PWR
                    , NULL                                                   AS BWTHST_RPM_PWR
                    
                    -- RPM & Slow Steaming
                    , NULL                                                   AS CTCL_RPM_NO
                    , NULL                                                   AS CTCL_TO_RPM_NO
                    , NULL                                                   AS OP_MIN_RPM_NO
                    , NULL                                                   AS OP_MIN_SPD
                    , NULL                                                   AS SLW_STMNG_FLG
                    , NULL                                                   AS SPR_SLW_STMNG_FLG
                    , NULL                                                   AS FUEL_SAV_EQ_FLG
                    , NULL                                                   AS VSL_LOD_RTO
                    
                    -- Design Max Load Hold/Deck
                    , NULL                                                   AS IN_HLD_PER_TR_KNT
                    , NULL                                                   AS IN_HLD_PER_ROW_KNT
                    , NULL                                                   AS HTCH_CVR_IN_HLD_KNT
                    , NULL                                                   AS ON_DECK_PER_TR_KNT
                    , NULL                                                   AS ON_DECK_PER_ROW_KNT
                    
                    
                    , NULL                                                   AS CRW_KNT
                    , ROW_NUMBER() OVER(ORDER BY T1.UPD_DT DESC) AS RNUM
            FROM    MDM_VSL_BLK         T1,
                    MDM_COUNTRY         T3
            WHERE   1=1
            AND     T1.VSL_RGST_CNT_CD  = T3.CNT_CD (+)
            AND     T1.DELT_FLG  		<> 'Y'
            #if (${vsl_cd} != '')
            AND     T1.VSL_CD                    = @[vsl_cd] --   :Vessel
            #end
        
            #if (${vsl_own_ind_cd} != '' && ${vsl_own_ind_cd} != 'A')
            AND     T1.VSL_OWN_IND_CD            = @[vsl_own_ind_cd] 
            #end
        
            #if (${crr_cd} != '')
            AND     T1.BLK_CRR_TP_CD             = @[crr_cd] 
            #end
            )
WHERE   1=1


#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="fm_date" type="12" value="" out="N"/>
				<param name="to_date" type="12" value="" out="N"/>
				<param name="vsl_svc_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_own_ind_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="cntr_vsl_clss_capa" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
