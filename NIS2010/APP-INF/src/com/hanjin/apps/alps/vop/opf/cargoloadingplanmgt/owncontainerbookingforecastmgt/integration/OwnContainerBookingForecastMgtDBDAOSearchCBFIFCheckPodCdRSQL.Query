<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOSearchCBFIFCheckPodCdRSQL">
			<desc><![CDATA[OwnContainerBookingForecastMgtDBDAOSearchCBFIFCheckPodCd]]></desc>
			<sql><![CDATA[
SELECT DECODE(CNT,0,'N','Y') POD_CD
 FROM (
SELECT COUNT(1) CNT FROM STO_CD_PORT
WHERE PORT_CD = @[pod_chk]
AND PORT_CD NOT IN ('EGSUZ','PAPAC')
)			]]></sql>
			<params>
				<param name="pod_chk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
