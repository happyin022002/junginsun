<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScheduleTransmitManagementDBDAOModifyEtaSendResultUSQL">
			<desc><![CDATA[기전송된 ETA 정보에 대하여, RPM에 대한 결과값을 저장합니다.]]></desc>
			<sql><![CDATA[
UPDATE VSK_VSL_PORT_SKD_TRSM_HIS
SET RPM_ADJ_DT = TO_DATE(@[rpm_adj_dt], 'YYYY-MM-DD HH24:MI')
  , ORG_RPM_PWR = @[org_rpm_pwr]
  , CRNT_RPM_PWR = @[crnt_rpm_pwr]
  , UPD_USR_ID = @[upd_usr_id]
  , LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_FNC(@[vps_port_cd])
  , UPD_DT = SYSDATE
WHERE VSL_CD = @[vsl_cd]
  AND SKD_VOY_NO = @[skd_voy_no]
  AND SKD_DIR_CD = @[skd_dir_cd]
  AND VPS_PORT_CD = @[vps_port_cd]
  AND CLPT_IND_SEQ = @[clpt_ind_seq]
  AND TRSM_HIS_SEQ = @[trsm_his_seq]			]]></sql>
			<params>
				<param name="rpm_adj_dt" type="12" value="" out="N"/>
				<param name="org_rpm_pwr" type="12" value="" out="N"/>
				<param name="crnt_rpm_pwr" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="trsm_his_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
