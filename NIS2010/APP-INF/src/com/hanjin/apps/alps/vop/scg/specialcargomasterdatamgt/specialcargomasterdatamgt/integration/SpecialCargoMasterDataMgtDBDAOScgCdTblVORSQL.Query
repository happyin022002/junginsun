<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgCdTblVORSQL">
			<desc><![CDATA[ScgCdTblVO]]></desc>
			<sql><![CDATA[
SELECT 
	CD_TBL_ID
,	CD_TBL_NM
,	CD_TBL_DESC
,	DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_CD_TBL
WHERE	1 = 1
#if (${delt_flg} != '') 
AND NVL(DELT_FLG,'N') = @[delt_flg]
#end
#if (${cd_tbl_id} != '') 
AND CD_TBL_ID = @[cd_tbl_id]
#end
#if (${cd_tbl_nm} != '') 
AND	CD_TBL_NM like @[cd_tbl_nm]||'%'
#end
ORDER BY CD_TBL_ID			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cd_tbl_id" type="12" value="" out="N"/>
				<param name="cd_tbl_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
