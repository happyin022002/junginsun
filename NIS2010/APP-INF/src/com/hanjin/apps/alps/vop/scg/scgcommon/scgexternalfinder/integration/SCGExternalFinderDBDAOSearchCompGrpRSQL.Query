<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGExternalFinderDBDAOSearchCompGrpRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  
        A.IMDG_COMP_GRP_CD   ,
        A.IMDG_COMP_GRP_DESC ,
        A.DELT_FLG ,
        A.CRE_USR_ID ,
        A.CRE_DT ,
        A.UPD_USR_ID ,
        A.UPD_DT
  FROM  SCG_IMDG_COMP_GRP A
 WHERE  A.DELT_FLG = 'N'
#if (${imdg_comp_grp_cd} != '') 
   AND  A.IMDG_COMP_GRP_CD = @[imdg_comp_grp_cd]
#end			]]></sql>
			<params>
				<param name="imdg_comp_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
