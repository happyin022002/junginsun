<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOScgRfCgoVOCSQL">
			<desc><![CDATA[BKG_RF_CGO에서 SCG_RF_CGO으로 DATA 복제]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_RF_CGO (
	CNTR_VOL_QTY
,	CBM_PER_HR_QTY
,	BKG_NO
,	SPCL_CGO_APRO_RQST_SEQ
,	RC_SEQ
,	CNTR_TPSZ_CD
,	CNTR_NO
,	PCK_TP_CD
,	PCK_QTY
,	NET_WGT
,	GRS_WGT
,	WGT_UT_CD
,	CMDT_CD
,	CMDT_DESC
,	FDO_TEMP
,	CDO_TEMP
,	CNTR_VENT_TP_CD
,	VENT_RTO
,	HUMID_NO
,	DIFF_RMK
,	RF_DCGO_SEQ
,	PWR_SPL_CBL_FLG
,	VLTG_NO
,	CTRL_ATMS_FLG
,	MODI_ATMS_FLG
,	HUMID_CTRL_FLG
,	CNTR_DRN_CD
,	CLNG_TP_CD
,	RQST_DT
,	RQST_GDT
,	RQST_USR_ID
,	SPCL_CGO_APRO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
)
SELECT 
	CNTR_VOL_QTY
,	CBM_PER_HR_QTY
,	BKG_NO
#if (${spcl_cgo_apro_rqst_seq} != '') 
,	@[spcl_cgo_apro_rqst_seq]
#else
,	(	SELECT MAX(SPCL_CGO_APRO_RQST_SEQ) FROM SCG_APRO_RQST WHERE BKG_NO = @[bkg_no] AND SPCL_CGO_CATE_CD = 'RF'	)
#end
,	RC_SEQ
,	CNTR_TPSZ_CD
,	CNTR_NO
,	PCK_TP_CD
,	PCK_QTY
,	NET_WGT
,	GRS_WGT
,	WGT_UT_CD
,	CMDT_CD
,	CMDT_DESC
,	FDO_TEMP
,	CDO_TEMP
,	CNTR_VENT_TP_CD
,	VENT_RTO
,	HUMID_NO
,	DIFF_RMK
,	RF_DCGO_SEQ
,	PWR_SPL_CBL_FLG
,	VLTG_NO
,	CTRL_ATMS_FLG
,	MODI_ATMS_FLG
,	HUMID_CTRL_FLG
,	CNTR_DRN_CD
,	CLNG_TP_CD
,	RQST_DT
,	RQST_GDT
,	RQST_USR_ID
,	SPCL_CGO_APRO_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM BKG_RF_CGO
WHERE BKG_NO = @[bkg_no]
#if (${spcl_cgo_apro_rqst_seq} != '' || ${cgo_seq} == '') 
AND SPCL_CGO_APRO_CD not in ('C','D')
#end
#if (${cgo_seq} != '') 
AND RC_SEQ = @[cgo_seq]
#end			]]></sql>
			<params>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cgo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
