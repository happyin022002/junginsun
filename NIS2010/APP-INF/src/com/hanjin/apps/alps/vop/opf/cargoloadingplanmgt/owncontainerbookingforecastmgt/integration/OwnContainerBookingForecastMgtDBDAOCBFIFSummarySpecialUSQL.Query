<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOCBFIFSummarySpecialUSQL">
			<desc><![CDATA[OwnContainerBookingForecastMgtDBDAOCBFIFSummarySpecial]]></desc>
			<sql><![CDATA[
UPDATE OPF_CGO_BKG_FCAST_SPCL_SMRY
SET    CNTR_TPSZ_CD			= NVL(@[cntr_tpsz_cd]		, CNTR_TPSZ_CD)
		, DCGO_FLG          = NVL(@[dcgo_flg]			, DCGO_FLG)
		, RC_FLG            = DECODE(DCGO_FLG, 'Y', DECODE(NVL(@[cargo_type], RC_FLG)		, 'R', 'Y', 'N'), DECODE(@[cargo_type], 'T', '', 'Y', 'Y', RC_FLG))
		, AWK_CGO_FLG       = DECODE(DCGO_FLG, 'Y', DECODE(NVL(@[cargo_type], AWK_CGO_FLG)	, 'A', 'Y', 'N'), AWK_CGO_FLG)
		, BB_CGO_FLG        = NVL(@[bb_cgo_flg]         , BB_CGO_FLG)
		, BDL_CGO_FLG       = NVL(@[bdl_cgo_flg]        , BDL_CGO_FLG)
		, STWG_CGO_FLG      = NVL(@[stwg_cgo_flg]       , STWG_CGO_FLG)
		--, MTY_BKG_FLG       = NVL([mty_bkg_flg]        , MTY_BKG_FLG)
		, MTY_BKG_FLG       = DECODE(DCGO_FLG, 'Y', DECODE(NVL(@[cargo_type], MTY_BKG_FLG)	, 'E', 'Y', 'N'), MTY_BKG_FLG)
		, CRN_PST_STS_CD    = @[crn_pst_sts_cd]
		, IMDG_UN_NO        = NVL(@[imdg_un_no]         , IMDG_UN_NO)
		, IMDG_CLSS_CD      = NVL(@[imdg_clss_cd]       , IMDG_CLSS_CD)
		, MRN_POLUT_FLG     = NVL(@[mrn_polut_flg]      , MRN_POLUT_FLG)
		, CNTR_QTY          = NVL(@[cntr_qty]           , CNTR_QTY)
		, STWG_CD           = @[stwg_cd]
		, FWRD_OVR_DIM_LEN  = NVL(@[fwrd_ovr_dim_len]   , '0')
		, BKWD_OVR_DIM_LEN  = NVL(@[bkwd_ovr_dim_len]   , '0')
		, HGT_OVR_DIM_LEN   = NVL(@[hgt_ovr_dim_len]    , '0')
		, LF_SD_OVR_DIM_LEN = NVL(@[lf_sd_ovr_dim_len]  , '0')
		, RT_SD_OVR_DIM_LEN = NVL(@[rt_sd_ovr_dim_len]  , '0')
		, CBF_RMK           = @[cbf_rmk]
		, IMDG_LMT_QTY_FLG  = NVL(@[imdg_lmt_qty_flg]   , IMDG_LMT_QTY_FLG)
		, USD_BKG_TTL_QTY   = NVL(@[usd_bkg_ttl_qty]    , '0')
		, VOID_20FT_QTY     = NVL(@[void_20ft_qty]      , '0')
	    , UPD_USR_ID 		= @[upd_usr_id]
	    , UPD_DT = SYSDATE
   		, POD_CD = ( SELECT NVL(ISO_PORT_CD,PORT_CD) FROM STO_CD_PORT
                      WHERE PORT_CD = @[pod_nm] 
                        AND ROWNUM  = 1  )
   		, BLCK_STWG_CD 			= SUBSTR(@[pod_nm],3,4)
		, PRCT_FLG     			= NVL(@[prct_flg]      			, PRCT_FLG)
		, IMDG_SUBS_RSK_LBL_CD  = NVL(@[imdg_subs_rsk_lbl_cd]   , IMDG_SUBS_RSK_LBL_CD)
		, BKG_REV_MCGO_FLG		= @[bkg_rev_mcgo_flg] 
 WHERE VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]
   AND YD_CD = @[yd_cd]
   AND POL_CLPT_IND_SEQ = @[pol_clpt_ind_seq]
   AND CRR_CD     = @[crr_cd]
   AND POD_CD = @[pod_cd]
   AND BLCK_STWG_CD = @[blck_stwg_cd]
   AND CBF_SPCL_SMRY_SEQ = @[cbf_spcl_smry_seq]			]]></sql>
			<params>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="dcgo_flg" type="12" value="" out="N"/>
				<param name="cargo_type" type="12" value="" out="N"/>
				<param name="bb_cgo_flg" type="12" value="" out="N"/>
				<param name="bdl_cgo_flg" type="12" value="" out="N"/>
				<param name="stwg_cgo_flg" type="12" value="" out="N"/>
				<param name="crn_pst_sts_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="mrn_polut_flg" type="12" value="" out="N"/>
				<param name="cntr_qty" type="12" value="" out="N"/>
				<param name="stwg_cd" type="12" value="" out="N"/>
				<param name="fwrd_ovr_dim_len" type="12" value="" out="N"/>
				<param name="bkwd_ovr_dim_len" type="12" value="" out="N"/>
				<param name="hgt_ovr_dim_len" type="12" value="" out="N"/>
				<param name="lf_sd_ovr_dim_len" type="12" value="" out="N"/>
				<param name="rt_sd_ovr_dim_len" type="12" value="" out="N"/>
				<param name="cbf_rmk" type="12" value="" out="N"/>
				<param name="imdg_lmt_qty_flg" type="12" value="" out="N"/>
				<param name="usd_bkg_ttl_qty" type="12" value="" out="N"/>
				<param name="void_20ft_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pod_nm" type="12" value="" out="N"/>
				<param name="prct_flg" type="12" value="" out="N"/>
				<param name="imdg_subs_rsk_lbl_cd" type="12" value="" out="N"/>
				<param name="bkg_rev_mcgo_flg" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="blck_stwg_cd" type="12" value="" out="N"/>
				<param name="cbf_spcl_smry_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
