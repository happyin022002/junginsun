<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgPckReguPkgOrgPrxVORSQL">
			<desc><![CDATA[데이터를 insert를 한다.]]></desc>
			<sql><![CDATA[
SELECT 
  IMDG_PCK_INSTR_CD
, IMDG_PCK_INSTR_SEQ
, REGU_DP_NO     
, SUB_SEQ         
, IMDG_UN_NO  
, ORG_PRX_DESC
, IMDG_PCK_CD      
, MAX_CAPA_QTY    
, IMDG_CTRL_TEMP   
, IMDG_EMER_TEMP   
, DELT_FLG     

FROM SCG_PCK_REGU_ORG_PRX
WHERE IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
AND   IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]
AND   REGU_DP_NO    = @[regu_dp_no]
AND   DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
				<param name="regu_dp_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
