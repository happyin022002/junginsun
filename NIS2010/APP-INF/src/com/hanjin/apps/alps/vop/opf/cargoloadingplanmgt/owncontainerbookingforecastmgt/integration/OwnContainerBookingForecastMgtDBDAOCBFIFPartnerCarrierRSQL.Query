<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOCBFIFPartnerCarrierRSQL">
			<desc><![CDATA[PARTNER EDI의 Carrier code가져오기]]></desc>
			<sql><![CDATA[
WITH V_CRR AS 
   ( SELECT CRR_CD   
      FROM MDM_CARRIER
     WHERE DELT_FLG = 'N' 
       AND CRR_CD   = @[crr_nm]
     UNION 
     SELECT UNIV_CD AS CRR_CD
       FROM STO_CD_EDI_CNVT
      WHERE CATE_CD  = 'OPER'
        AND PRIV_CD  = @[crr_nm] 
        AND PRNR_CD  = 'ZZZZZ') 
  SELECT  DECODE( (SELECT COUNT(1) FROM V_CRR), 1, CRR_CD, NULL) AS CRR_CD
    FROM V_CRR			]]></sql>
			<params>
				<param name="crr_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
