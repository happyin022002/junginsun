<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetScntRSQL">
			<desc><![CDATA[getScnt]]></desc>
			<sql><![CDATA[
SELECT NVL(A.MADN_VOY_SUZ_NET_TONG_WGT, 0)
FROM   MDM_VSL_CNTR A
WHERE  VSL_CD = SUBSTR(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
