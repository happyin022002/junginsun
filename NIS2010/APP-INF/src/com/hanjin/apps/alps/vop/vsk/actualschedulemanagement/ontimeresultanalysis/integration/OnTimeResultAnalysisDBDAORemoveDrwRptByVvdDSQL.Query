<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnTimeResultAnalysisDBDAORemoveDrwRptByVvdDSQL">
			<desc><![CDATA[Drewry Report 항목 삭제]]></desc>
			<sql><![CDATA[
DELETE 
FROM VSK_VSL_DRW_SKD
WHERE   1=1
AND	 VSL_CD				= SUBSTR(@[vvd],1,4)
AND	 SKD_VOY_NO			= SUBSTR(@[vvd],5,4)
AND	 SKD_DIR_CD			= SUBSTR(@[vvd],9,1)           
AND  POL_CD				= @[pol_cd]            
AND  POL_YD_CD			= @[pol_yd_cd]         
AND  POL_CLPT_IND_SEQ	= @[pol_clpt_ind_seq]  
AND  POL_CLPT_SEQ		= @[pol_clpt_seq]      
AND  POD_CD				= @[pod_cd]            
AND  POD_YD_CD			= @[pod_yd_cd]         
AND  POD_CLPT_IND_SEQ	= @[pod_clpt_ind_seq]  
AND  POD_CLPT_SEQ		= @[pod_clpt_seq]      
AND  SLAN_CD			= @[slan_cd]			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_yd_cd" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="pol_clpt_seq" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pod_yd_cd" type="12" value="" out="N"/>
				<param name="pod_clpt_ind_seq" type="12" value="" out="N"/>
				<param name="pod_clpt_seq" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
