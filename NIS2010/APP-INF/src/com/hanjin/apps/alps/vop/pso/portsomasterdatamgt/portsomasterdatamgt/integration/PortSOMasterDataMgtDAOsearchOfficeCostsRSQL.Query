<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortSOMasterDataMgtDAOsearchOfficeCostsRSQL">
			<desc><![CDATA[searchOfficeCosts]]></desc>
			<sql><![CDATA[
SELECT
	T1.ACCT_CD, 
	T2.LGS_COST_CD, 
	T1.LGS_COST_FULL_NM, 
	T3.ACCT_ENG_NM pagerows
FROM
	TES_LGS_COST T1,
	PSO_INV_OFC_COST T2, 
    MDM_ACCOUNT T3
WHERE
	T1.LGS_COST_CD = T2.LGS_COST_CD
	AND T2.OFC_CD = @[ofc_cd]
	AND T2.LGS_COST_CD > ' '
	AND T1.ACCT_CD <> '511911'
	AND T1.ACCT_CD = T3.ACCT_CD
ORDER BY 1			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
