<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOVskCustTrdPrnrIDCSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
UPDATE VSK_CUST_SKD_EDI_SET
   SET USE_FLG         = 'N',
       UPD_USR_ID      = @[upd_usr_id],
	   UPD_DT          = TO_DATE(SUBSTR(@[upd_dt], 0, 14), 'YYYYMMDDHH24MISS')
 WHERE EDI_STUP_ID != @[edi_stup_id]
   AND CUST_TRD_PRNR_ID = @[cust_trd_prnr_id]
   AND USE_FLG != 'N'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="edi_stup_id" type="12" value="" out="N"/>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
