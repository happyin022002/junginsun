<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationReportDBDAOsearchRawDataByVBPForBudgetRSQL">
			<desc><![CDATA[raw data : 추정결산 VBP별 집계]]></desc>
			<sql><![CDATA[
--사업계획 Row데이터 VVD(VBP용) 
SELECT RLANE_CD ,
       VSL_CD,
       SKD_VOY_NO,
       SKD_DIR_CD,
       MAX(PORT_CHG_AMT) PORT_CHARGE,
       MAX(CNL_FEE_AMT) CANAL_FEE,
       (SELECT CNTR_DZN_CAPA FROM MDM_VSL_CNTR WHERE VSL_CD = PLN.VSL_CD) CNTR_DZN_CAPA
FROM   PSO_BUD_TRF_PLN PLN
WHERE  1=1
AND    BUD_SCNR_NO= substr(@[rev_yrmon],0,4) || @[bud_str]
AND    SUBSTR(BUD_YRMON,1,4)= substr(@[rev_yrmon],0,4)
AND    SUBSTR(BUD_YRMON,5,2)= substr(@[rev_yrmon],6,2)
AND    SUB_TRD_CD <>'IP'
AND    PORT_CHG_AMT+CNL_FEE_AMT<>0
group by RLANE_CD,VSL_CD,SKD_VOY_NO,SKD_DIR_CD
ORDER BY RLANE_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="bud_str" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
