<sqls>
	<querys>
		<query name="OwnDangerousCargoApprovalDBDAOOwnApprovalRequestVORSQL">
			<desc><![CDATA[Mail Preview 항목들을 조회한다.,,,]]></desc>
			<sql><![CDATA[
SELECT
   M.FROM_PSN
 , M.TO_PSN
 , M.CC_PSN
 , M.SUBJECT
 , M.ATTACH_FILE
 , M.BODY_HEADER
 , M.BODY_FOOTER
 , M.BODY_CONTS
 , '' CRR_CD
 , '' BKG_REF_NO
 , '' SPCL_CGO_RQST_SEQ
 , '' BKG_NO
 , '' SPCL_CGO_APRO_RQST_SEQ
 , '' VSL_PRE_PST_CD
 , '' VSL_SEQ
 , '' RGN_SHP_OPR_CD
 , '' SCG_FLG
 , '' SEND_TYPE
 , '' USER_ID
FROM TABLE(SCG_MAIL_INFO_FNC(@[crr_cd],@[bkg_ref_no],@[spcl_cgo_rqst_seq],@[bkg_no],@[spcl_cgo_apro_rqst_seq],@[vsl_pre_pst_cd],@[vsl_seq],@[rgn_shp_opr_cd],@[scg_flg],@[send_type],@[user_id])) M			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="bkg_ref_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_rqst_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="spcl_cgo_apro_rqst_seq" type="12" value="" out="N"/>
				<param name="vsl_pre_pst_cd" type="12" value="" out="N"/>
				<param name="vsl_seq" type="12" value="" out="N"/>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
				<param name="scg_flg" type="12" value="" out="N"/>
				<param name="send_type" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
