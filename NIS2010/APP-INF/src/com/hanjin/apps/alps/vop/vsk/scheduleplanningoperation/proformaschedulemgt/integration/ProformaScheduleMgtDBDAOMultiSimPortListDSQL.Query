<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOMultiSimPortListDSQL">
			<desc><![CDATA[터미널 정보 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM VSK_SIM_TML_INFO 
  WHERE SIM_DT     = @[sim_dt]
	AND SIM_NO     = @[sim_no]
  #if (${skd_dir_cd} != '') 
    AND SKD_DIR_CD = @[skd_dir_cd]  
  #end
  #if (${tml_cd} != '') 
    AND TML_CD     = @[tml_cd]   
  #end
  #if (${vsl_dbl_call_seq} != '') 
    AND VSL_DBL_CALL_SEQ  = @[vsl_dbl_call_seq]
  #end			]]></sql>
			<params>
				<param name="sim_dt" type="12" value="" out="N"/>
				<param name="sim_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="tml_cd" type="12" value="" out="N"/>
				<param name="vsl_dbl_call_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
