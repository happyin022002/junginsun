				<param name="clm_hndl_usr_id" type="12" value="" out="N"/>
				<param name="stv_dmg_respb_pty_co_nm" type="12" value="" out="N"/>
				<param name="stv_dmg_respb_pty_pic_nm" type="12" value="" out="N"/>
				<param name="stv_dmg_respb_pty_pic_tit_nm" type="12" value="" out="N"/>
				<param name="stv_dmg_cmpn_dt" type="12" value="" out="N"/>
				<param name="cmpn_curr_cd" type="12" value="" out="N"/>
				<param name="cmpn_cost_locl_amt" type="7" value="" out="N"/>
				<param name="cmpn_cost_usd_amt" type="7" value="" out="N"/>
				<param name="cmpn_acct_no" type="12" value="" out="N"/>
				<param name="cmpn_eml_snd_no" type="12" value="" out="N"/>
				<param name="cmpn_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
