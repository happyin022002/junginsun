			<desc><![CDATA[OpfStvDmgCmpnVO Update Query]]></desc>
			<sql><![CDATA[
UPDATE OPF_STV_DMG_CMPN SET 
	STV_DMG_NO = @[stv_dmg_no]
,	STV_DMG_CMPN_PROC_STS_CD = @[stv_dmg_cmpn_proc_sts_cd]
,	CLM_HNDL_OFC_CD = @[clm_hndl_ofc_cd]
,	CLM_HNDL_USR_ID = @[clm_hndl_usr_id]
,	STV_DMG_RESPB_PTY_CO_NM = @[stv_dmg_respb_pty_co_nm]
,	STV_DMG_RESPB_PTY_PIC_NM = @[stv_dmg_respb_pty_pic_nm]
,	STV_DMG_RESPB_PTY_PIC_TIT_NM = @[stv_dmg_respb_pty_pic_tit_nm]
,	STV_DMG_CMPN_DT = TO_DATE(@[stv_dmg_cmpn_dt],'YYYY-MM-DD')
,	CMPN_CURR_CD = @[cmpn_curr_cd]
,	CMPN_COST_LOCL_AMT = @[cmpn_cost_locl_amt]
,	CMPN_COST_USD_AMT = @[cmpn_cost_usd_amt]
,	CMPN_ACCT_NO = @[cmpn_acct_no]
,	CMPN_EML_SND_NO = @[cmpn_eml_snd_no]
,	CMPN_RMK = @[cmpn_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE 
WHERE	STV_DMG_NO = @[stv_dmg_no]			]]></sql>
			<params>
				<param name="stv_dmg_no" type="12" value="" out="N"/>
				<param name="stv_dmg_cmpn_proc_sts_cd" type="12" value="" out="N"/>
