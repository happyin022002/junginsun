<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OperationNPerformMasterDataMgtDBDAOOpfRstwgRsnCdVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE 		OPF_RSTWG_RSN_CD 			X
SET		 	X.RSTWG_RSN_CD 				= @[rstwg_rsn_cd] 
		----,	X.RSTWG_CD_TP_CD 			= [rstwg_cd_tp_cd] 
		,	X.RSTWG_RSN_CD_FULL_DESC 	= @[rstwg_rsn_cd_full_desc] 
		----,	X.CO_CD 					= [co_cd] 
		,	X.DELT_FLG 					= @[delt_flg] 
		,	X.UPD_USR_ID 				= @[upd_usr_id] 
		,	X.UPD_DT 					= SYSDATE 
WHERE		1 = 1
AND			X.RSTWG_CD_TP_CD 			= 'R' 				/* 'R' : RESTOW REASON, 'S' : SHIFTING METHOD */
AND			X.RSTWG_RSN_CD 				= @[rstwg_rsn_cd]			]]></sql>
			<params>
				<param name="rstwg_rsn_cd" type="12" value="" out="N"/>
				<param name="rstwg_rsn_cd_full_desc" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
