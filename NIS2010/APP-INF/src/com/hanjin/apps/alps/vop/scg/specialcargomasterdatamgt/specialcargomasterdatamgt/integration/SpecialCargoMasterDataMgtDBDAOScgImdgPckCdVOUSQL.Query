<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgPckCdVOUSQL">
			<desc><![CDATA[Packaging Code (Creation) 수정]]></desc>
			<sql><![CDATA[
UPDATE SCG_IMDG_PCK_CD SET 
	EAI_IF_FLG           = null, 
	IMDG_PCK_DESC = @[imdg_pck_desc]
,	IMDG_PCK_TP_CD = @[imdg_pck_tp_cd]
,	DELT_FLG = @[delt_flg]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
,   PCK_KND_CD  = @[pck_knd_cd]
,   PCK_MTRL_TP_CD  = @[pck_mtrl_tp_cd]
WHERE	IMDG_PCK_CD = @[imdg_pck_cd]			]]></sql>
			<params>
				<param name="imdg_pck_desc" type="12" value="" out="N"/>
				<param name="imdg_pck_tp_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pck_knd_cd" type="12" value="" out="N"/>
				<param name="pck_mtrl_tp_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
