<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalInformationMgtDBDAOVskPortGntrCrnSpclCgoHndlInfoAttachFileDSQL">
			<desc><![CDATA[G/Crane 작업시 Special Cargo Handling Information에 대한 첨부파일 삭제]]></desc>
			<sql><![CDATA[
DELETE
FROM       VSK_PORT_GNTR_HNDL_FILE   X
WHERE      X.LOC_CD                  = @[port_cd]
AND        X.ATCH_FILE_SEQ           = @[atch_file_seq]  
			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="atch_file_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
