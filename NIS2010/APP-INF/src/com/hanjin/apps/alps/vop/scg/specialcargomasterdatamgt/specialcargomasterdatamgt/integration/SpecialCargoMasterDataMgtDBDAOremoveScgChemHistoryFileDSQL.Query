<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOremoveScgChemHistoryFileDSQL">
			<desc><![CDATA[delete file]]></desc>
			<sql><![CDATA[
DELETE 
FROM SCG_CHEM_HIS_FILE
WHERE chem_seq = @[chem_seq]
  AND atch_file_div_cd = @[atch_file_div_cd]
  AND atch_file_seq = @[atch_file_seq]			]]></sql>
			<params>
				<param name="chem_seq" type="12" value="" out="N"/>
				<param name="atch_file_div_cd" type="12" value="" out="N"/>
				<param name="atch_file_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
