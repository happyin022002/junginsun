<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnTimeResultAnalysisDBDAORemoveDrwTrdInfoSetupDSQL">
			<desc><![CDATA[Drewry Report의 Trade Setup에서 Trade 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
MERGE INTO VSK_DRW_TGT_TRD DRW
    USING
    (
        SELECT  @[drw_trd_cd]                DRW_TRD_CD
        FROM    DUAL
    )   SRC
    ON
    (
                DRW.DRW_TRD_CD = SRC.DRW_TRD_CD	
    )
WHEN MATCHED THEN 
UPDATE
SET       
                  DELT_FLG          = 'Y'
                , UPD_USR_ID        = @[upd_usr_id]
                , UPD_DT            = SYSDATE			]]></sql>
			<params>
				<param name="drw_trd_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
