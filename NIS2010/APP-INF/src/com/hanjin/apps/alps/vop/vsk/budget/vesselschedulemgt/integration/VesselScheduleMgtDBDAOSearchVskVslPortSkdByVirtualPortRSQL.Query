<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchVskVslPortSkdByVirtualPortRSQL">
			<desc><![CDATA[Virtual Port에 대한 VVD Port 정보를 조회합니다.

* History
* 2011.02.22 진마리아 CHM-201108456-01 사업계획용 스케줄 정보 노출 대응을 위한 시스템 업데이트건]]></desc>
			<sql><![CDATA[
SELECT 
	PORT_SKP_TP_CD
,	PORT_SKP_RSN_CD
,	PORT_SKP_RSN_OFFR_RMK
,	TTL_DLAY_HRS
,	TS_PORT_CD
,	USD_FLG
,	NOON_RPT_INP_FLG
,	DEP_RPT_INP_FLG
,	ACT_INP_FLG
,	PRT_CHK_FLG
,	CRE_USR_ID
,	TO_CHAR(CRE_DT, 'YYYYMMDDHH24MISS') AS CRE_DT
,	UPD_USR_ID
,	TO_CHAR(UPD_DT, 'YYYYMMDDHH24MISS') AS UPD_DT
,	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	VPS_PORT_CD
,	CLPT_IND_SEQ
,	CLPT_SEQ
,	SLAN_CD
,	PORT_SKD_STS_CD
,	YD_CD
,	CALL_YD_IND_SEQ
, TO_CHAR(PF_ETA_DT, 'YYYYMMDDHH24MI') AS PF_ETA_DT
, TO_CHAR(PF_ETB_DT, 'YYYYMMDDHH24MI') AS PF_ETB_DT
, TO_CHAR(PF_ETD_DT, 'YYYYMMDDHH24MI') AS PF_ETD_DT
, TO_CHAR(INIT_ETA_DT, 'YYYYMMDDHH24MI') AS INIT_ETA_DT
, TO_CHAR(INIT_ETB_DT, 'YYYYMMDDHH24MI') AS INIT_ETB_DT
, TO_CHAR(INIT_ETD_DT, 'YYYYMMDDHH24MI') AS INIT_ETD_DT
, TO_CHAR(VPS_ETA_DT, 'YYYYMMDDHH24MI') AS VPS_ETA_DT
, TO_CHAR(VPS_ETB_DT, 'YYYYMMDDHH24MI') AS VPS_ETB_DT
, TO_CHAR(VPS_ETD_DT, 'YYYYMMDDHH24MI') AS VPS_ETD_DT
,	VSL_DLAY_RSN_CD
,	VSL_DLAY_RSN_DESC
,	VSL_DLAY_RSN_LOC_CD
,	SHP_CALL_NO
,	SHP_CALL_NO_UPD_USR_ID
,	TO_CHAR(SHP_CALL_NO_UPD_DT, 'YYYYMMDDHH24MI') AS SHP_CALL_NO_UPD_DT
,	TML_VSL_CD
,	TML_VOY_NO
,	TO_CHAR(FT_DT, 'YYYYMMDDHH24MI') AS FT_DT
,	PLISM_YD_CD
,	PLISM_VSL_CD
,	PLISM_VOY_NO
,	SKD_CNG_STS_CD
,	TURN_PORT_FLG
,	TURN_PORT_IND_CD
,	TURN_SKD_VOY_NO
,	TURN_SKD_DIR_CD
,	TURN_CLPT_IND_SEQ
,	IB_CGO_QTY
,	OB_CGO_QTY
,	VPS_RMK
,	PHS_IO_RSN_CD
,	PHS_IO_RMK
,	SKD_BRTH_NO
,	INIT_SKD_INP_FLG
,	OFC_INP_FLG
,	EDI_SND_KNT
FROM VSK_BUD_VSL_PORT_SKD
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
ORDER BY CLPT_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
