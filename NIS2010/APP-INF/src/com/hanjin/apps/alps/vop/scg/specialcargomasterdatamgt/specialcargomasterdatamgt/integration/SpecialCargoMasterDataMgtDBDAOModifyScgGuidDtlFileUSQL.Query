<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOModifyScgGuidDtlFileUSQL">
			<desc><![CDATA[ModifyScgGuidDtlFile]]></desc>
			<sql><![CDATA[
UPDATE SCG_GUID_DTL_FILE
		 FILE_NM      = @[file_nm]
        ,FILE_SAV_ID      = @[file_sav_id]
    	,UPD_USR_ID    = @[upd_usr_id]
    	,UPD_DT        = SYSDATE
WHERE	SPCL_CGO_GUID_CD = @[spcl_cgo_guid_cd]
AND	SPCL_CGO_GUID_SEQ = @[spcl_cgo_guid_seq]
AND SPCL_CGO_GUID_ATCH_FILE_SEQ = @[spcl_cgo_guid_atch_file_seq]			]]></sql>
			<params>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="spcl_cgo_guid_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_guid_seq" type="12" value="" out="N"/>
				<param name="spcl_cgo_guid_atch_file_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
