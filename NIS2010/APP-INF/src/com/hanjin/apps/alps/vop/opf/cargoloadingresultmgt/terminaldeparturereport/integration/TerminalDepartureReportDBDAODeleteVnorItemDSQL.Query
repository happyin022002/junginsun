<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAODeleteVnorItemDSQL">
			<desc><![CDATA[VNOR 의 Item을 삭제합니다.]]></desc>
			<sql><![CDATA[
DELETE FROM OPF_VNOR_ITM
WHERE VSL_CD = @[vsl_cd]
AND VNOR_SEQ = @[vnor_seq]
AND VNOR_ITM_SEQ = @[vnor_itm_seq]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vnor_seq" type="12" value="" out="N"/>
				<param name="vnor_itm_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
