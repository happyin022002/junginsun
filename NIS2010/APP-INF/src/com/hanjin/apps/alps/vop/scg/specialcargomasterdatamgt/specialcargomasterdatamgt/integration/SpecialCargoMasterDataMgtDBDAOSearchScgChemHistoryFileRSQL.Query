<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchScgChemHistoryFileRSQL">
			<desc><![CDATA[history file]]></desc>
			<sql><![CDATA[
SELECT
  A.CHEM_SEQ
, A.ATCH_FILE_DIV_CD
, A.ATCH_FILE_SEQ
, B.FILE_UPLD_NM AS FILE_NM
, B.FILE_SAV_ID
, A.CRE_USR_ID
, A.UPD_USR_ID
, TO_CHAR(A.CRE_DT,'YYYYMMDD') CRE_DT
, TO_CHAR(A.UPD_DT,'YYYYMMDD') UPD_DT
 FROM  scg_chem_his_file a, com_upld_file b
WHERE  a.chem_seq         = @[chem_seq]
  AND  a.atch_file_div_cd = @[atch_file_div_cd]
  AND  a.file_sav_id      = b.file_sav_id			]]></sql>
			<params>
				<param name="chem_seq" type="12" value="" out="N"/>
				<param name="atch_file_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
