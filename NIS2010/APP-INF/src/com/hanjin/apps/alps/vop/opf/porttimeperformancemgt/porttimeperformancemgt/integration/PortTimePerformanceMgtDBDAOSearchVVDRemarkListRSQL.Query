<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTimePerformanceMgtDBDAOSearchVVDRemarkListRSQL">
			<desc><![CDATA[2012.06.08 허철용 [CHM-201217512-01] PTRP 화면에 Dashboard 기능 추가]]></desc>
			<sql><![CDATA[
SELECT VSL_CD || SKD_VOY_NO || SKD_DIR_CD AS VVD
     , VSL_CD
     , TO_CHAR(SKD_VOY_NO) SKD_VOY_NO
     , SKD_DIR_CD
     , VPS_PORT_CD
     , CLPT_IND_SEQ
     , RHQ_CD
     , SLAN_CD
     , VVD_RMK
     , VVD_RHQ_RMK
  FROM OPF_PORT_TM_RMK
 WHERE 1 = 1
   AND VSL_CD      = @[vsl_cd]
   AND SKD_VOY_NO  = @[skd_voy_no]
   AND SKD_DIR_CD  = @[skd_dir_cd]
   AND VPS_PORT_CD = @[vps_port_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
