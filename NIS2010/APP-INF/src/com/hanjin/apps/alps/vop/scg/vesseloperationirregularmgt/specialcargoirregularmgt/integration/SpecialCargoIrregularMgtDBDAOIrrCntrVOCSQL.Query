<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoIrregularMgtDBDAOIrrCntrVOCSQL">
			<desc><![CDATA[SPCL CGO Irregular Container 생성]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_IRR_CNTR (
	VSL_CD
,	SKD_VOY_NO
,	SKD_DIR_CD
,	SPCL_CGO_IRR_SEQ
,	SPCL_CGO_IRR_CNTR_SEQ
,	BKG_NO
,	BKG_REF_NO
,	CNTR_NO
,	CNTR_REF_NO
,	CGO_SEQ
,	CNTR_TPSZ_CD
,	IMDG_UN_NO
,	IMDG_UN_NO_SEQ
,	IMDG_CLSS_CD
,   IMDG_COMP_GRP_CD
,	PRP_SHP_NM
,	SPCL_CGO_CATE_CD
,	AWK_CGO_GRS_WGT
,	AWK_CGO_NET_WGT
,	DIM_LEN
,	DIM_WDT
,	DIM_HGT
,	CMDT_DESC
,	CELL_PSN_NO
,   BL_REF_NO
,   POR_CD
,   POL_CD
,   POD_CD
,   DEL_CD
,   SHPR_NM
,   FWRD_NM
,   CNEE_NM
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[vsl_cd]
,	@[skd_voy_no]
,	@[skd_dir_cd]
,	@[spcl_cgo_irr_seq]
,	@[spcl_cgo_irr_cntr_seq]
#if (${cgo_opr_cd} == 'SML' || ${cgo_opr_cd} == 'SEN') 
,	@[bkg_no]
,	''
,	@[cntr_no]
,	''
#else
,	''
,	@[bkg_no]
,	''
,	@[cntr_no]
#end
,	@[cgo_seq]
,	@[cntr_tpsz_cd]
,	@[imdg_un_no]
,	@[imdg_un_no_seq]
,	@[imdg_clss_cd]
,	@[imdg_comp_grp_cd]
,	@[prp_shp_nm]
,	@[spcl_cgo_cate_cd]
,	@[awk_cgo_grs_wgt]
,	@[awk_cgo_net_wgt]
,	@[dim_len]
,	@[dim_wdt]
,	@[dim_hgt]
,	@[cmdt_desc]
,	@[cell_psn_no]
,   @[bl_no]
,   @[por_cd]
,   @[pol_cd]
,   @[pod_cd]
,   @[del_cd]
,   @[s_cust_nm]
,   @[f_cust_nm]
,   @[c_cust_nm]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_irr_seq" type="12" value="" out="N"/>
				<param name="spcl_cgo_irr_cntr_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cgo_seq" type="2" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="2" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="imdg_comp_grp_cd" type="12" value="" out="N"/>
				<param name="prp_shp_nm" type="12" value="" out="N"/>
				<param name="spcl_cgo_cate_cd" type="12" value="" out="N"/>
				<param name="awk_cgo_grs_wgt" type="12" value="" out="N"/>
				<param name="awk_cgo_net_wgt" type="12" value="" out="N"/>
				<param name="dim_len" type="12" value="" out="N"/>
				<param name="dim_wdt" type="12" value="" out="N"/>
				<param name="dim_hgt" type="12" value="" out="N"/>
				<param name="cmdt_desc" type="12" value="" out="N"/>
				<param name="cell_psn_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="s_cust_nm" type="12" value="" out="N"/>
				<param name="f_cust_nm" type="12" value="" out="N"/>
				<param name="c_cust_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
