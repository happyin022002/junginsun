<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerLinesDangerousCargoApprovalDBDAOPreRestrictionLQRegulatoryRSQL">
			<desc><![CDATA[PreRestrictionLQRegulatory]]></desc>
			<sql><![CDATA[
SELECT 'Limited Q'||''''||'TY' DIV_NM
      ,CASE WHEN IMDG_LMT_QTY > 0 THEN IMDG_LMT_QTY||' '||IMDG_LMT_QTY_MEAS_UT_CD 
            ELSE '0'
       END REGU_VAL
      ,'Max Weight' MAX_WGT_NM
#if(${in_imdg_pck_qty1} != '' || ${in_imdg_pck_cd1} != '')
      ,CASE WHEN TO_NUMBER(@[grs_wgt])/TO_NUMBER(@[out_imdg_pck_qty1]) > 30 THEN 'Invaild'
			WHEN IMDG_LMT_QTY_MEAS_UT_CD = 'L' THEN
	#if(${grs_capa_qty} != '')
              CASE WHEN IMDG_LMT_QTY < TO_NUMBER(@[grs_capa_qty])/TO_NUMBER(@[in_imdg_pck_qty1]) THEN 'Invalid'
              ELSE 'Valid' END
    #elseif(${net_wgt} != '')
              CASE WHEN IMDG_LMT_QTY < TO_NUMBER(@[net_wgt])/TO_NUMBER(@[in_imdg_pck_qty1]) THEN 'Invalid'
              ELSE 'Valid' END
	#else
			'Invalid'
	#end
            WHEN IMDG_LMT_QTY_MEAS_UT_CD = 'KG' THEN
	#if(${net_wgt} != '')
              CASE WHEN IMDG_LMT_QTY < TO_NUMBER(@[net_wgt])/TO_NUMBER(@[in_imdg_pck_qty1]) THEN 'Invalid'
              ELSE 'Valid' END
	#else
			'Invalid'
	#end
            WHEN IMDG_LMT_QTY_MEAS_UT_CD IS NULL THEN 'Invalid'
            END MAX_WGT_RSLT
#else
      ,'Invalid' MAX_WGT_RSLT
#end
      ,'PKG Type' PKG_TP_NM
      ,'N/A' PKG_TP_RSLT
  FROM (
       SELECT IMDG_UN_NO
             ,CASE WHEN IMDG_LMT_QTY_MEAS_UT_CD IN ('MLG', 'ML', 'G' )
                   THEN TO_CHAR(IMDG_LMT_QTY/1000, '9990.99')
              ELSE TO_CHAR(IMDG_LMT_QTY) END IMDG_LMT_QTY
             ,CASE WHEN IMDG_LMT_QTY_MEAS_UT_CD IN ('MLG', 'G') THEN 'KG'
                   WHEN IMDG_LMT_QTY_MEAS_UT_CD = 'ML' THEN 'L'
              ELSE NVL(IMDG_LMT_QTY_MEAS_UT_CD, 'KG') END IMDG_LMT_QTY_MEAS_UT_CD
         FROM SCG_IMDG_UN_NO
        WHERE IMDG_UN_NO = @[imdg_un_no]
          AND IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
        )
UNION ALL
SELECT 'Excepted Q'||''''||'TY' DIV_NM
      ,IMDG_EXPT_QTY_CD REGU_VAL
      ,'Max Weight' MAX_WGT_NM
      ,'N/A' MAX_WGT_RSLT
      ,'PKG Type' PKG_TP_NM
      ,'N/A' PKG_TP_RSLT
  FROM SCG_IMDG_UN_NO S
 WHERE S.IMDG_UN_NO = @[imdg_un_no]
   AND S.IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
UNION ALL
SELECT 'Packing Instruction' DIV_NM
      ,N1ST_IMDG_PCK_INSTR_CD||NVL2(N2ND_IMDG_PCK_INSTR_CD, ', '||N2ND_IMDG_PCK_INSTR_CD, NULL)
       ||NVL2(N3RD_IMDG_PCK_INSTR_CD, ', '||N3RD_IMDG_PCK_INSTR_CD, NULL)
       ||NVL2(N1ST_IMDG_IBC_INSTR_CD, ', '||N1ST_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N2ND_IMDG_IBC_INSTR_CD, ', '||N2ND_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N3RD_IMDG_IBC_INSTR_CD, ', '||N3RD_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N4TH_IMDG_IBC_INSTR_CD, ', '||N4TH_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N5TH_IMDG_IBC_INSTR_CD, ', '||N5TH_IMDG_IBC_INSTR_CD, NULL)
       ||NVL2(N1ST_IMDG_PCK_PROVI_CD, '('||N1ST_IMDG_PCK_PROVI_CD
       ||NVL2(N2ND_IMDG_PCK_PROVI_CD, ', '||N2ND_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N3RD_IMDG_PCK_PROVI_CD, ', '||N3RD_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N4TH_IMDG_PCK_PROVI_CD, ', '||N4TH_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N5TH_IMDG_PCK_PROVI_CD, ', '||N5TH_IMDG_PCK_PROVI_CD, NULL)
             ||NVL2(N1ST_IMDG_IBC_PROVI_CD, ', '||N1ST_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N2ND_IMDG_IBC_PROVI_CD, ', '||N2ND_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N3RD_IMDG_IBC_PROVI_CD, ', '||N3RD_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N4TH_IMDG_IBC_PROVI_CD, ', '||N4TH_IMDG_IBC_PROVI_CD, NULL)
             ||NVL2(N5TH_IMDG_IBC_PROVI_CD, ', '||N5TH_IMDG_IBC_PROVI_CD, NULL)||')', NULL) REGU_VAL
      ,'Max Weight' MAX_WGT_NM
      ,'N/A' MAX_WGT_RSLT
      ,'PKG Type' PKG_TP_NM
      ,'N/A' PKG_TP_RSLT
  FROM SCG_IMDG_UN_NO S
 WHERE S.IMDG_UN_NO = @[imdg_un_no]
   AND S.IMDG_UN_NO_SEQ = @[imdg_un_no_seq]			]]></sql>
			<params>
				<param name="grs_wgt" type="12" value="" out="N"/>
				<param name="out_imdg_pck_qty1" type="12" value="" out="N"/>
				<param name="grs_capa_qty" type="12" value="" out="N"/>
				<param name="in_imdg_pck_qty1" type="12" value="" out="N"/>
				<param name="net_wgt" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
