				<param name="sim_ttl_foil_csm_wgt" type="12" value="" out="N"/>
				<param name="sim_buf_sea_spd" type="12" value="" out="N"/>
				<param name="sim_buf_sea_foil_csm_wgt" type="12" value="" out="N"/>
