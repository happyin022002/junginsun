			<sql><![CDATA[
UPDATE FCM_BNK_CSM_PF_DTL_SIM
SET                   
vsl_clss_cd               = @[vsl_clss_cd]              
,sim_max_spd               = @[sim_max_spd]              
,sim_min_spd               = @[sim_min_spd]              
,sim_run_aux_spd           = @[sim_run_aux_spd]              
,sim_sea_spd               = @[sim_sea_spd]              
,sim_sea_foil_csm_wgt      = @[sim_sea_foil_csm_wgt]     
,sim_mnvr_foil_csm_wgt     = @[sim_mnvr_foil_csm_wgt]    
,sim_port_foil_csm_wgt     = @[sim_port_foil_csm_wgt]    
,sim_gnr_port_foil_csm_wgt = @[sim_gnr_port_foil_csm_wgt]
,sim_gnr_sea_foil_csm_wgt  = @[sim_gnr_sea_foil_csm_wgt] 
,sim_ttl_foil_csm_wgt      = @[sim_ttl_foil_csm_wgt]     
,sim_buf_sea_spd           = @[sim_buf_sea_spd]          
,sim_buf_sea_foil_csm_wgt  = @[sim_buf_sea_foil_csm_wgt] 
,sim_buf_gnr_foil_csm_wgt  = @[sim_buf_gnr_foil_csm_wgt] 
,sim_buf_ttl_foil_csm_wgt  = @[sim_buf_ttl_foil_csm_wgt]                   
,upd_usr_id                = @[upd_usr_id]               
,upd_dt                    = sysdate     
WHERE vsl_slan_cd =@[vsl_slan_cd]
AND pf_svc_tp_cd =@[pf_svc_tp_cd]
AND bnk_csm_sim_no =@[bnk_csm_sim_no]
AND vsl_clss_cd =@[vsl_clss_cd]    
AND port_cd =@[port_cd]
AND skd_dir_cd =@[skd_dir_cd]
AND clpt_seq =@[clpt_seq]			]]></sql>
			<params>
				<param name="vsl_clss_cd" type="12" value="" out="N"/>
				<param name="sim_max_spd" type="12" value="" out="N"/>
				<param name="sim_min_spd" type="12" value="" out="N"/>
