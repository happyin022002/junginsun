<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOPodComboRSQL">
			<desc><![CDATA[PodCombo조회]]></desc>
			<sql><![CDATA[
SELECT DECODE(A.SKD_CNG_STS_CD, 'S', NULL, A.VPS_PORT_CD||A.CLPT_IND_SEQ) AS POD_CD
  FROM (
        SELECT A.VPS_PORT_CD, 
               A.SKD_VOY_NO, 
               A.CLPT_SEQ, 
               A.SKD_CNG_STS_CD,
               A.CLPT_IND_SEQ
          FROM VSK_VSL_PORT_SKD A
         WHERE A.VSL_CD		= @[vsl_cd]
           AND A.SKD_VOY_NO = @[skd_voy_no]
           AND A.SKD_DIR_CD = @[skd_dir_cd]
           AND A.CLPT_SEQ   > (SELECT CLPT_SEQ
								 FROM VSK_VSL_PORT_SKD
								WHERE VSL_CD              =	@[vsl_cd]
								  AND SKD_VOY_NO          =	@[skd_voy_no]
							      AND SKD_DIR_CD          =	@[skd_dir_cd]
								  AND YD_CD||CLPT_IND_SEQ =	@[yd_cd]
							   )	
        ) A,
        MDM_LOCATION	B
 WHERE A.VPS_PORT_CD = B.LOC_CD
 ORDER BY A.CLPT_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="HJPO" out="N"/>
				<param name="skd_voy_no" type="12" value="0012" out="N"/>
				<param name="skd_dir_cd" type="12" value="W" out="N"/>
				<param name="yd_cd" type="12" value="KRPUSYG" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
