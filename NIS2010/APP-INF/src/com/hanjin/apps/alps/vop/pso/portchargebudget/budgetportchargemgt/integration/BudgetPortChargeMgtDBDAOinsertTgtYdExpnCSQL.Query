<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BudgetPortChargeMgtDBDAOinsertTgtYdExpnCSQL">
			<desc><![CDATA[메인 리스트에서 선택한 VVD 별 Budget 정보를 재 생성한다.
=========================================
History
2012.08.20 진마리아 CHM-201219078-01 사업계획 - 시나리오 연도 추가]]></desc>
			<sql><![CDATA[
INSERT INTO PSO_TGT_YD_EXPN(
        PSO_BZTP_CD,
        VSL_CD,
        SKD_VOY_NO,
        SKD_DIR_CD,
        YD_CD,
        LGS_COST_CD,
        IO_BND_CD,
        REV_YRMON,
        LOCL_CURR_CD,
        INV_LOCL_AMT,
        INV_USD_AMT,
        XPR_DESC,
        FOML_DESC,
        YD_CHG_NO,
        YD_CHG_VER_SEQ,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT,
        RLANE_CD,
        VNDR_SEQ,
        BUD_SCNR_NO
)
		VALUES ( 
		 @[pso_bztp_cd]
        ,@[vsl_cd]
        ,@[skd_voy_no]
        ,@[skd_dir_cd]
        ,@[yd_cd]
        ,@[lgs_cost_cd]
        ,NVL(@[io_bnd_cd], 'A')
        ,@[rev_yrmon]--TO_CHAR(sysdate, 'YYYYMM') /*ap_eff_dt rev_yrmon*/
        ,
 		@[locl_curr_cd]
         /*(SELECT curr_cd FROM mdm_currency
          WHERE cnt_cd =  :locl_curr_cd) */
        ,decode(sign(@[inv_locl_amt]), -1, 0, null, null, @[inv_locl_amt] ) 
        ,decode(sign(@[inv_usd_amt]), -1, 0, null, null, @[inv_usd_amt] ) 
        ,@[xpr_desc]
        ,@[foml_desc]
        ,@[yd_chg_no]
        ,@[yd_chg_ver_seq]
        ,@[cre_usr_id]
        ,sysdate /*cre_dt*/
        ,@[upd_usr_id]
        ,sysdate /*upd_dt*/
		,nvl(@[rlane_cd], PSO_GET_REV_LANE_FNC(@[vsl_cd],@[skd_voy_no],@[skd_dir_cd], substr(@[yd_cd], 1, 5 ) ))
		,@[vndr_seq]
        ,nvl(@[bud_scnr_no],'999912')
		)			]]></sql>
			<params>
				<param name="pso_bztp_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="inv_locl_amt" type="12" value="" out="N"/>
				<param name="inv_usd_amt" type="12" value="" out="N"/>
				<param name="xpr_desc" type="12" value="" out="N"/>
				<param name="foml_desc" type="12" value="" out="N"/>
				<param name="yd_chg_no" type="12" value="" out="N"/>
				<param name="yd_chg_ver_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="bud_scnr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
