<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgUnNoRSQL">
			<desc><![CDATA[Segregation Groups (Creation) 의 United Nations(UN) Number 조회]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_UN_NO
FROM SCG_IMDG_UN_NO
WHERE	IMDG_UN_NO = @[imdg_un_no]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="imdg_un_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
