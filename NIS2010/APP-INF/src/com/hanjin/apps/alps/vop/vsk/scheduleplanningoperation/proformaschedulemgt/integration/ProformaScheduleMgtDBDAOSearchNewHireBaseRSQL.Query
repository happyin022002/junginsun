<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOSearchNewHireBaseRSQL">
			<desc><![CDATA[SearchNewHireBase]]></desc>
			<sql><![CDATA[
SELECT  '' AS VSL_OWNR_FLG, TO_NUMBER(NVL(@[n1st_vsl_clss_cd],'0')) AS VSL_CSL1, TO_NUMBER(NVL(@[n2nd_vsl_clss_cd],'0')) AS VSL_CSL2,  TO_NUMBER(NVL(@[n3rd_vsl_clss_cd],'0')) AS VSL_CSL3 FROM DUAL
UNION ALL
SELECT 'Y'
       , NVL( ROUND(AVG(DECODE(T2.CNTR_VSL_CLSS_CAPA, @[n1st_vsl_clss_cd], T1.DHIR_AMT / T2.CNTR_VSL_CLSS_CAPA)), 1), 0 ) AS DAILY_HIRE_BY_CLASS01
       , NVL( ROUND(AVG(DECODE(T2.CNTR_VSL_CLSS_CAPA, @[n2nd_vsl_clss_cd], T1.DHIR_AMT / T2.CNTR_VSL_CLSS_CAPA)), 1), 0 ) AS DAILY_HIRE_BY_CLASS02
       , NVL( ROUND(AVG(DECODE(T2.CNTR_VSL_CLSS_CAPA, @[n3rd_vsl_clss_cd], T1.DHIR_AMT / T2.CNTR_VSL_CLSS_CAPA)), 1), 0 ) AS DAILY_HIRE_BY_CLASS03
FROM    VSK_DLY_HIR T1, MDM_VSL_CNTR T2
WHERE   1   = 1
AND     T1.VSL_CD   = T2.VSL_CD
AND     PLN_YR      = @[slt_prc_wrk_yr]
AND     T2.CNTR_VSL_CLSS_CAPA IN (@[n1st_vsl_clss_cd], @[n2nd_vsl_clss_cd], @[n3rd_vsl_clss_cd])
UNION ALL
SELECT 'N',0,0,0 FROM DUAL			]]></sql>
			<params>
				<param name="n1st_vsl_clss_cd" type="12" value="" out="N"/>
				<param name="n2nd_vsl_clss_cd" type="12" value="" out="N"/>
				<param name="n3rd_vsl_clss_cd" type="12" value="" out="N"/>
				<param name="slt_prc_wrk_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
