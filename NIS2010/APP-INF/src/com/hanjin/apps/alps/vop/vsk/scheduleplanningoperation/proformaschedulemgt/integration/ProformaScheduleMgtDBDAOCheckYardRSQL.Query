<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOCheckYardRSQL">
			<desc><![CDATA[CheckYard]]></desc>
			<sql><![CDATA[
SELECT	COUNT(*) AS CNT
FROM	MDM_YARD
WHERE	DELT_FLG	= 'N'
AND		YD_CD 		= @[yd_cd]
AND		'Y'			= YD_FCTY_TP_MRN_TML_FLG
AND		ROWNUM		= 1			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
