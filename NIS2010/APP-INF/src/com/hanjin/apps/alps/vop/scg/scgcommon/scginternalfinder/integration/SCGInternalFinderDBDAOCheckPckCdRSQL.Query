<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCGInternalFinderDBDAOCheckPckCdRSQL">
			<desc><![CDATA[CheckPckCd]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_PCK_INSTR_CD 
,	MAX(IMDG_PCK_INSTR_SEQ)+1 AS IMDG_PCK_INSTR_SEQ 
,	MAX(IMDG_PCK_INSTR_SEQ) AS PCK_CD_SEQ_MAX 
,	MIN(IMDG_PCK_INSTR_SEQ) AS PCK_CD_SEQ_MIN 
,	COUNT(IMDG_PCK_INSTR_SEQ) AS PCK_CD_SEQ_CNT
,   SYSDATE AS UPD_DT
FROM SCG_PCK_INSTR
WHERE	IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
GROUP BY IMDG_PCK_INSTR_CD			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
