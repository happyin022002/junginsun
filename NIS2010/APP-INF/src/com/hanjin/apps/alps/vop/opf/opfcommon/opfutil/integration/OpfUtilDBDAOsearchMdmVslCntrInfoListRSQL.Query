<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OpfUtilDBDAOsearchMdmVslCntrInfoListRSQL">
			<desc><![CDATA[dd]]></desc>
			<sql><![CDATA[
SELECT VSL_CD, DECODE(CRR_CD||VSL_OWN_IND_CD, 'SMLO', 'CO', 'CC') AS VSL_OSHP_CNTR_BLK_TP_CD
               , (
                SELECT  S.INTG_CD_VAL_DESC
                FROM    COM_INTG_CD_DTL S
                WHERE   S.INTG_CD_ID    = 'CD02594'
                AND     S.INTG_CD_VAL_CTNT  = DECODE(CRR_CD||VSL_OWN_IND_CD, 'SMLO', 'CO', 'CC')
               ) AS VSL_OSHP_CNTR_BLK_TP_CD_DESC
FROM  MDM_VSL_CNTR
WHERE  VSL_CD = @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
