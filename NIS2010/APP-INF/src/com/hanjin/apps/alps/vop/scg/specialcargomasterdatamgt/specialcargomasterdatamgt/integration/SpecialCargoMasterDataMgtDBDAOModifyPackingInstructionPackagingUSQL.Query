		<query name="SpecialCargoMasterDataMgtDBDAOModifyPackingInstructionPackagingUSQL">
			<desc><![CDATA[ModifyPackingInstructionPackaging]]></desc>
			<sql><![CDATA[
UPDATE SCG_PCK_STY
   SET PCK_TP_CD           = @[pck_tp_cd]  
      ,PCK_MTRL_TP_CD      = @[pck_mtrl_tp_cd]              
      ,IMDG_PCK_CD              = @[imdg_pck_cd]           
      ,IMDG_PCK_DESC              = @[imdg_pck_desc]           
      ,PCK_REF_CD              = @[pck_ref_cd]           
      ,PCK_REF_NO          = @[pck_ref_no]       
      ,GRP_N1ST_PROHI_FLG     = @[grp_n1st_prohi_flg]  
      ,GRP_N1ST_REF_NO        = @[grp_n1st_ref_no]     
      ,GRP_N1ST_QTY           = @[grp_n1st_qty]        
      ,GRP_N1ST_MEAS_UT_CD       = @[grp_n1st_meas_ut_cd]    
      ,GRP_N2ND_REF_NO       = @[grp_n2nd_ref_no]    
      ,GRP_N2ND_PROHI_FLG    = @[grp_n2nd_prohi_flg] 
      ,GRP_N2ND_QTY          = @[grp_n2nd_qty]       
      ,GRP_N2ND_MEAS_UT_CD      = @[grp_n2nd_meas_ut_cd]   
      ,GRP_N3RD_REF_NO      = @[grp_n3rd_ref_no]   
      ,GRP_N3RD_PROHI_FLG   = @[grp_n3rd_prohi_flg]
      ,GRP_N3RD_QTY         = @[grp_n3rd_qty]      
      ,GRP_N3RD_MEAS_UT_CD     = @[grp_n3rd_meas_ut_cd]  
      ,IMDG_UN_NO          = @[imdg_un_no]       
      ,SPCL_PCK_DESC            = @[spcl_pck_desc] 
      ,DELT_FLG            = @[delt_flg]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE
 WHERE IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
   AND IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]
   AND PCK_STY_CD = @[pck_sty_cd]
   AND SUB_SEQ = @[sub_seq]			]]></sql>
			<params>
				<param name="pck_tp_cd" type="12" value="" out="N"/>
				<param name="pck_mtrl_tp_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_desc" type="12" value="" out="N"/>
				<param name="pck_ref_cd" type="12" value="" out="N"/>
				<param name="pck_ref_no" type="12" value="" out="N"/>
				<param name="grp_n1st_prohi_flg" type="12" value="" out="N"/>
				<param name="grp_n1st_ref_no" type="12" value="" out="N"/>
				<param name="grp_n1st_qty" type="12" value="" out="N"/>
				<param name="grp_n1st_meas_ut_cd" type="12" value="" out="N"/>
				<param name="grp_n2nd_ref_no" type="12" value="" out="N"/>
				<param name="grp_n2nd_prohi_flg" type="12" value="" out="N"/>
				<param name="grp_n2nd_qty" type="12" value="" out="N"/>
				<param name="grp_n2nd_meas_ut_cd" type="12" value="" out="N"/>
				<param name="grp_n3rd_ref_no" type="12" value="" out="N"/>
				<param name="grp_n3rd_prohi_flg" type="12" value="" out="N"/>
				<param name="grp_n3rd_qty" type="12" value="" out="N"/>
				<param name="grp_n3rd_meas_ut_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="spcl_pck_desc" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
				<param name="pck_sty_cd" type="12" value="" out="N"/>
				<param name="sub_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
