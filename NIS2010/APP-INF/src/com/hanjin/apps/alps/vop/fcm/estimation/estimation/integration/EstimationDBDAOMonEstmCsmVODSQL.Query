<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationDBDAOMonEstmCsmVODSQL">
			<desc><![CDATA[Remove the monthly estimation consumption of a specific month.]]></desc>
			<sql><![CDATA[
DELETE FCM_ESTM_MON_CSM
WHERE  EXE_YRMON = @[exe_yrmon]			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
