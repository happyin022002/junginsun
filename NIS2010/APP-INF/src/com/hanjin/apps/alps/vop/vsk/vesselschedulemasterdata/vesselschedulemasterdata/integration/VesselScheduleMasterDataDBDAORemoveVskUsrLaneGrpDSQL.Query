<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMasterDataDBDAORemoveVskUsrLaneGrpDSQL">
			<desc><![CDATA[사용자별 Lane Group 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM VSK_USR_LANE_GRP
WHERE 1=1
AND LANE_GRP_NM = @[src_lane_grp_nm]
AND VSL_SLAN_CD = @[src_vsl_slan_cd]			]]></sql>
			<params>
				<param name="src_lane_grp_nm" type="12" value="" out="N"/>
				<param name="src_vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
