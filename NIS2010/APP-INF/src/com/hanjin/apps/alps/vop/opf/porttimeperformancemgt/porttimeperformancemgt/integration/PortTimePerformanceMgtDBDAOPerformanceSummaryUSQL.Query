<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTimePerformanceMgtDBDAOPerformanceSummaryUSQL">
			<desc><![CDATA[PerformanceSummary]]></desc>
			<sql><![CDATA[
UPDATE TDR_HEADER
 SET 
   EXCEPT_CD = DECODE(@[except_cd], '1', 'Y', null)
  ,EXCEPT_POB_FLG = DECODE(@[except_pob_flg], '1', 'Y', null)
  ,UPD_SYS_CD = 'N' 
  ,UPDATE_USER = @[update_user]
  ,UPDATE_TIME = SYSDATE
 WHERE VSL_CD   = @[vsl_cd]
   AND VOY_NO   = @[skd_voy_no]
   AND DIR_CD   = @[skd_dir_cd]
   AND PORT_CD  = @[port_cd]
   AND CALL_IND = @[clpt_ind_seq]			]]></sql>
			<params>
				<param name="except_cd" type="12" value="" out="N"/>
				<param name="except_pob_flg" type="12" value="" out="N"/>
				<param name="update_user" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
