<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOMergePsoChgXprDtlCSQL">
			<desc><![CDATA[PSO_CHG_XPR_DTL <merge>]]></desc>
			<sql><![CDATA[
MERGE INTO PSO_CHG_XPR_DTL A
USING DUAL ON (    A.CHG_XPR_NO  = @[chg_xpr_no]
               AND A.CHG_XPR_SEQ = @[chg_xpr_seq])
WHEN MATCHED
THEN UPDATE SET 
             A.PSO_CTRL_STMT_TP_CD   = @[pso_ctrl_stmt_tp_cd]                                               
            ,A.COND_NO               = @[cond_no]                    
            ,A.DFLT_FOML_FLG         = @[dflt_foml_flg]              
            ,A.FOML_NO               = @[foml_no]                    
            ,A.UPPR_CHG_XPR_SEQ      = @[uppr_chg_xpr_seq]                            
            ,A.UPD_USR_ID            = @[cre_usr_id]                 
            ,A.UPD_DT                = SYSDATE     
     WHERE  1=1
     AND    A.CHG_XPR_NO  = @[chg_xpr_no]
     AND    A.CHG_XPR_SEQ = @[chg_xpr_seq]
WHEN NOT MATCHED
THEN INSERT (A.CHG_XPR_NO
            ,A.CHG_XPR_SEQ
            ,A.PSO_CTRL_STMT_TP_CD
            ,A.COND_NO
            ,A.DFLT_FOML_FLG
            ,A.FOML_NO
            ,A.UPPR_CHG_XPR_SEQ
            ,A.CRE_USR_ID
            ,A.CRE_DT
            ,A.UPD_USR_ID
            ,A.UPD_DT)
     VALUES (@[chg_xpr_no]                 
            ,@[chg_xpr_seq]                
            ,@[pso_ctrl_stmt_tp_cd]        
            ,@[cond_no]                    
            ,@[dflt_foml_flg]              
            ,@[foml_no]                    
            ,@[uppr_chg_xpr_seq]           
            ,@[cre_usr_id]                 
            ,SYSDATE                     
            ,@[cre_usr_id]                 
            ,SYSDATE                       
     )			]]></sql>
			<params>
				<param name="chg_xpr_no" type="12" value="" out="N"/>
				<param name="chg_xpr_seq" type="12" value="" out="N"/>
				<param name="pso_ctrl_stmt_tp_cd" type="12" value="" out="N"/>
				<param name="cond_no" type="12" value="" out="N"/>
				<param name="dflt_foml_flg" type="12" value="" out="N"/>
				<param name="foml_no" type="12" value="" out="N"/>
				<param name="uppr_chg_xpr_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
