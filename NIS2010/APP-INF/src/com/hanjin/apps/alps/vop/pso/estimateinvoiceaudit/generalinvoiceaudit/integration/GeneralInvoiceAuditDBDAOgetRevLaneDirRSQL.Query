<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetRevLaneDirRSQL">
			<desc><![CDATA[getRevLaneDir]]></desc>
			<sql><![CDATA[
SELECT   max(L.RLANE_CD) || '|' || max(L.RLANE_DIR_CD)||'|'||  max(L.REV_YRMON)
  FROM   AR_MST_REV_VVD L
 WHERE   L.VSL_CD = @[vsl_cd]
         AND L.SKD_VOY_NO = @[skd_voy_no]
         AND L.SKD_DIR_CD = @[skd_dir_cd]
         AND L.RLANE_CD =  NVL(PSO_GET_REV_LANE_FNC(@[vsl_cd], @[skd_voy_no], @[skd_dir_cd], SUBSTR (@[yd_cd], 1, 5), @[io_flag]), L.RLANE_CD)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="io_flag" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
