<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOSearchSkdDirPortCdforProformaListRSQL">
			<desc><![CDATA[특정 Vessel Service Lane의 Proforma 데이터중 direction+port 조합으로 데이터 추출한다.]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT
        X.SKD_DIR_CD	AS SKD_DIR_CD
      , X.PORT_CD		AS VPS_PORT_CD
FROM    VSK_PF_SKD_DTL   X
WHERE   1 = 1
AND     X.VSL_SLAN_CD    = @[vsl_slan_cd]

ORDER BY X.SKD_DIR_CD ASC
      ,  X.PORT_CD    ASC			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
