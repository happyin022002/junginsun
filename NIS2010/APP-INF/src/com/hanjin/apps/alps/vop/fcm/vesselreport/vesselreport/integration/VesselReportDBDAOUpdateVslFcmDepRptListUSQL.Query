<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselReportDBDAOUpdateVslFcmDepRptListUSQL">
			<desc><![CDATA[Departure Report Inquiry 업데이트]]></desc>
			<sql><![CDATA[
UPDATE FCM_DEP_RPT
SET AVG_EXPT_FLG = @[avg_expt_flg]
WHERE VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]
AND DEP_PORT_CD = @[dep_port_cd]
AND CLPT_IND_SEQ = @[clpt_ind_seq]			]]></sql>
			<params>
				<param name="avg_expt_flg" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="dep_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
