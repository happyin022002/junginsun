<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchSvcLaneDirByVvdRSQL">
			<desc><![CDATA[VVD 로 VSL_SLAN_DIR_SEQ 를 찾아온다.

* History
* 2011.02.22 진마리아 CHM-201108456-01 사업계획용 스케줄 정보 노출 대응을 위한 시스템 업데이트건]]></desc>
			<sql><![CDATA[
SELECT  VSL_SLAN_DIR_CD, NUM VSL_SLAN_DIR_SEQ
FROM    (
            SELECT  ROWNUM  NUM, VSL_SLAN_DIR_CD
            FROM    (
                        SELECT  VSL_SLAN_DIR_CD, VSL_SLAN_DIR_SEQ
                        FROM    MDM_VSL_SVC_LANE_DIR
                        WHERE   1            = 1 
                        AND     VSL_SLAN_CD  = (
                                                SELECT  VSL_SLAN_CD
                                                FROM    VSK_BUD_VSL_SKD
                                                WHERE   VSL_CD = @[vsl_cd]
                                                AND     SKD_VOY_NO = @[skd_voy_no]
                                                AND     SKD_DIR_CD = @[skd_dir_cd]
                                                )
                        AND     DELT_FLG     = 'N'
                        ORDER BY VSL_SLAN_DIR_SEQ
                    )
        )
ORDER BY NUM			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
