<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselInformationMgtDBDAOLoadableListVORSQL">
			<desc><![CDATA[Performance 의 Loadable 정보를 확인한다.

//History
2014.04.16 박다은 [CHM-201429675-01] Voyage Performance내 Lane Code 구분]]></desc>
			<sql><![CDATA[
SELECT  X.VSL_SLAN_CTNT
      , X.HC_INCL_BSA_QTY
      , X.HC_XCLD_BSA_QTY
      , X.CTRT_BSA_UT_WGT
      , X.ACT_BSA_UT_WGT
      , X.TTL_BSA_WGT
      
      , X.HD_HUL_HC_HLD_QTY
      , X.HD_HUL_HC_DECK_QTY
      , X.HD_HUL_HC_INCL_QTY
      , X.HD_HUL_HC_XCLD_QTY
      , X.HD_HUL_ACT_UT_WGT
      , X.HD_HUL_TTL_WGT
      
      , X.BAK_HUL_HC_HLD_QTY
      , X.BAK_HUL_HC_DECK_QTY
      , X.BAK_HUL_HC_INCL_QTY
      , X.BAK_HUL_HC_XCLD_QTY
      , X.BAK_HUL_ACT_UT_WGT
      , X.BAK_HUL_TTL_WGT
          
 FROM   VSK_VSL_LANE_LDB_CAPA     X
WHERE   1 = 1
  AND   X.VSL_SLAN_CD             = @[h_vsl_slan_cd]
  AND   X.CNTR_DZN_CAPA           IN
                                  (
                                  ----------------------------------------------
                                  SELECT     VC.CNTR_DZN_CAPA
                                  FROM       MDM_VSL_CNTR   VC
                                  WHERE      VC.VSL_CD      = @[vsl_cd]
                                  ----------------------------------------------
                                  )			]]></sql>
			<params>
				<param name="h_vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
