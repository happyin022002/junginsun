      ,CAPA_MASS_MIN_QTY   = @[capa_mass_min_qty]
      ,CAPA_MASS_MAX_QTY   = @[capa_mass_max_qty]
      ,CAPA_MASS_MEAS_UT_CD   = @[capa_mass_meas_ut_cd]
