<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OwnContainerBookingForecastMgtDBDAOCBFIFSummaryCSQL">
			<desc><![CDATA[OwnContainerBookingForecastMgtDBDAOCBFIFSummary]]></desc>
			<sql><![CDATA[
INSERT INTO OPF_CGO_BKG_FCAST_SMRY	(VSL_CD
					, SKD_VOY_NO
					, SKD_DIR_CD
					, YD_CD
					, POL_CLPT_IND_SEQ
					, CRR_CD
					, POD_CD
					, BLCK_STWG_CD
					, CBF_IND_FLG
					, CRE_USR_ID
					, CRE_DT
					, UPD_USR_ID
					, UPD_DT
					)
SELECT VSL_CD
	, SKD_VOY_NO
	, SKD_DIR_CD
	, YD_CD
	, POL_CLPT_IND_SEQ
	, @[crr_cd]
	, POD_CD
	, BLCK_STWG_CD
	, CBF_IND_FLG
	, CRE_USR_ID
	, SYSDATE
	, UPD_USR_ID
	, SYSDATE
FROM OPF_CGO_BKG_FCAST_SMRY
 WHERE VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]
   AND YD_CD = @[pol_cd]
   AND POL_CLPT_IND_SEQ = @[pol_clpt_ind_seq]
   AND CRR_CD     = 'SML'
   --AND CRR_CD     = [crr_cd]
   --AND POD_CD = [pod_cd]
   --AND BLCK_STWG_CD = [blck_stwg_cd]			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
