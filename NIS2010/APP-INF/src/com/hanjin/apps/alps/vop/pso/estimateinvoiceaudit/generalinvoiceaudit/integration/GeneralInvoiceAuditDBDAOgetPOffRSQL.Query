<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetPOffRSQL">
			<desc><![CDATA[Tariff 비용 계산을 위해 Pilot Off를 구한다.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(PILOT_DEP,'HH24MI')
FROM TDR_HEADER
where vsl_cd = substr(@[vvd], 1, 4)
and VOY_NO = substr(@[vvd], 5, 4)
and DIR_CD=  substr(@[vvd], 9, 1)
and PORT_CD = substr(@[yd_cd], 1, 5)			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
