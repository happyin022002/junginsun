<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchSkdStsCdRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  DECODE(VSL_CLSS_FLG, 'T', 'RDY', 'ACT') AS SKD_STS_CD
FROM    MDM_VSL_CNTR
WHERE   VSL_CD = @[vsl_cd]
AND		DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
