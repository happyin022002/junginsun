<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOSearchYardListRSQL">
			<desc><![CDATA[SearchYardList]]></desc>
			<sql><![CDATA[
SELECT  T1.YD_CD , YD_NM
	, NVL(PLT_MNVR_TM_HRS, 0) AS MNVR_IN_HRS
	, NVL(PLT_MNVR_TM_HRS, 0) AS MNVR_OUT_HRS
FROM	MDM_YARD T1, VSK_PORT_MNVR T2
WHERE	1		= 1
AND	T1.YD_CD	= T2.YD_CD (+)
AND	T1.DELT_FLG	= 'N'
AND	T1.YD_CHR_CD	= 'N'
AND	T1.YD_CD	=  @[yd_cd]
			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
