<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAORemovePsoTrfAtchFileDSQL">
			<desc><![CDATA[removePsoTrfAtchFile]]></desc>
			<sql><![CDATA[
delete
  from  PSO_TRF_ATCH_FILE
 where  YD_CHG_NO = @[yd_chg_no]
   and  FILE_SAVE_ID = @[file_sav_id]			]]></sql>
			<params>
				<param name="yd_chg_no" type="12" value="1" out="N"/>
				<param name="file_sav_id" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
