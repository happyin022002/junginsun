<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanalTransitFeeBalanceBCDBDAOsearchCanalTzFeeBalDtlRSQL">
			<desc><![CDATA[searchCanalTzFeeBalDtl]]></desc>
			<sql><![CDATA[
select TO_CHAR(VPS_ETB_DT,'YYYY-MM-DD') rev_yrmon
,A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD vvd
,SUM(B.RQST_AMT) amount
,'' vndr_seq
,'' pso_bztp_cd
,'' cnl_tz_bztp_cd
,'' yd_cd
FROM PSO_CNL_TZ_FEE  A, PSO_CNL_TZ_FEE_DTL  B, VSK_VSL_PORT_SKD C
WHERE 
A.NTC_YRMON = @[rev_yrmon]
AND A.VNDR_SEQ = @[vndr_seq]
AND A.PSO_BZTP_CD   = '5'
AND A.YD_CD = @[yd_cd]
AND A.CNL_TZ_BZTP_CD = @[cnl_tz_bztp_cd]
AND A.CNL_TZ_PROC_STS_CD in ( 'A','Q' )
AND A.PSO_BZTP_CD   = B.PSO_BZTP_CD
AND A.VSL_CD        = B.VSL_CD
AND A.SKD_VOY_NO    = B.SKD_VOY_NO
AND A.SKD_DIR_CD    = B.SKD_DIR_CD
AND A.YD_CD         = B.YD_CD
AND A.CALL_SEQ      = B.CALL_SEQ 
AND C.VSL_CD        = B.VSL_CD
AND C.SKD_VOY_NO    = B.SKD_VOY_NO
AND C.SKD_DIR_CD    = B.SKD_DIR_CD
AND C.YD_CD         = B.YD_CD
GROUP BY  VPS_ETB_DT, A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="cnl_tz_bztp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
