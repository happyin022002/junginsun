,	STWG_N2ND_RMK = @[stwg_n2nd_rmk]
,	SPCL_CGO_AUTH_FLG = NVL(@[spcl_cgo_auth_flg],'N')
,	APRO_REF_NO = @[apro_ref_no]
,	CBF_RMK = @[cbf_rmk]
