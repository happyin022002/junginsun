<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOSearchPackingInstructionPackagingRSQL">
			<desc><![CDATA[SearchPackingInstructionPackaging]]></desc>
			<sql><![CDATA[
SELECT IMDG_PCK_INSTR_CD
      ,IMDG_PCK_INSTR_SEQ
      ,PCK_STY_CD
      ,SUB_SEQ
      ,PCK_TP_CD
      ,PCK_MTRL_TP_CD
      ,IMDG_PCK_CD
      ,IMDG_PCK_DESC
      ,PCK_REF_CD
      ,PCK_REF_NO
      ,GRP_N1ST_PROHI_FLG
      ,GRP_N1ST_REF_NO
      ,GRP_N1ST_QTY
      ,GRP_N1ST_MEAS_UT_CD
      ,GRP_N2ND_REF_NO
      ,GRP_N2ND_PROHI_FLG
      ,GRP_N2ND_QTY
      ,GRP_N2ND_MEAS_UT_CD
      ,GRP_N3RD_REF_NO
      ,GRP_N3RD_PROHI_FLG
      ,GRP_N3RD_QTY
      ,GRP_N3RD_MEAS_UT_CD
      ,IMDG_UN_NO
      ,SPCL_PCK_DESC 
      ,DELT_FLG
  FROM SCG_PCK_STY
 WHERE IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
   AND IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]
   AND PCK_STY_CD = @[pck_sty_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
				<param name="pck_sty_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
