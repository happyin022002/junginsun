<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OpfUtilDBDAOVskVslPortEtaVORSQL">
			<desc><![CDATA[VVD의 Port 및 ETA를 조회합니다.

2011.11.21 김민아 [CHM-201114254-01] [VOP-OPF/SDMS] Repaur VVD 및 Port 설정 기능 변경]]></desc>
			<sql><![CDATA[
SELECT  A.VPS_PORT_CD
       ,A.CLPT_IND_SEQ
       ,TO_CHAR(A.VPS_ETA_DT, 'YYYY-MM-DD') AS VPS_ETA_DT
  FROM  (
        SELECT	A.VPS_PORT_CD
               ,A.SKD_VOY_NO
               ,A.CLPT_SEQ
               ,A.SKD_CNG_STS_CD
               ,A.YD_CD
               ,A.TURN_PORT_IND_CD
               ,A.CLPT_IND_SEQ
               ,A.VPS_ETA_DT
          FROM  VSK_VSL_PORT_SKD A
         WHERE  A.VSL_CD		= @[vsl_cd]
           AND  A.SKD_VOY_NO    = @[voy_no]
           AND  A.SKD_DIR_CD    = @[dir_cd]
           AND  A.TURN_PORT_IND_CD NOT IN ('D','V','F')
           AND  NVL(A.SKD_CNG_STS_CD, ' ') <> 'S'
        ) A
       ,MDM_LOCATION B
WHERE	A.VPS_PORT_CD = B.LOC_CD
ORDER   BY  CLPT_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
