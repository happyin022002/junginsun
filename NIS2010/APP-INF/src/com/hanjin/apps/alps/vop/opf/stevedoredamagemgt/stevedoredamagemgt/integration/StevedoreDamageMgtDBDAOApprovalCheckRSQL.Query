<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StevedoreDamageMgtDBDAOApprovalCheckRSQL">
			<desc><![CDATA[Approval Check SQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT 
FROM COM_USER
WHERE OFC_CD = 'PUSMOV'
AND   PSN_ENG_NM = 'Team Manager'
AND   USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
