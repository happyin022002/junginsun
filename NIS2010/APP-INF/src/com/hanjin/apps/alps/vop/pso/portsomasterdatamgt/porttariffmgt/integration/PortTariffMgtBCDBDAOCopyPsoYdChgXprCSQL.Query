<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOCopyPsoYdChgXprCSQL">
			<desc><![CDATA[CopyPsoYdChgXpr]]></desc>
			<sql><![CDATA[
INSERT INTO PSO_YD_CHG_XPR
 ( YD_CHG_XPR_NO,
   PSO_CHG_TP_CD,
   YD_CHG_NO, 
   YD_CHG_VER_SEQ,
   CHG_XPR_NO,
   CRE_USR_ID,
   CRE_DT,
   UPD_USR_ID,
   UPD_DT ) 
SELECT 
      (SELECT /*+ INDEX_DESC(Z XPKPSO_YD_CHG_XPR) */ NVL(MAX(Z.YD_CHG_XPR_NO), 0) + 1 YD_CHG_XPR_NO
         FROM PSO_YD_CHG_XPR Z
        WHERE  ROWNUM = 1),
        PSO_CHG_TP_CD,  
        @[new_yd_chg_no],
        1,
        @[new_chg_xpr_no],
        @[cre_usr_id],
        SYSDATE,
        @[cre_usr_id],
        SYSDATE
  FROM  PSO_YD_CHG_XPR
  WHERE YD_CHG_NO      = @[yd_chg_no]
    AND YD_CHG_VER_SEQ = @[yd_chg_ver_seq]
    AND CHG_XPR_NO     = @[chg_xpr_no]			]]></sql>
			<params>
				<param name="new_yd_chg_no" type="12" value="" out="N"/>
				<param name="new_chg_xpr_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="yd_chg_no" type="12" value="" out="N"/>
				<param name="yd_chg_ver_seq" type="12" value="" out="N"/>
				<param name="chg_xpr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
