<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BudgetPortChargeMgtDBDAOdeleteGlEstmIfErpDSQL">
			<desc><![CDATA[deleteGlEstmIfErp]]></desc>
			<sql><![CDATA[
DELETE FROM GL_ESTM_IF_ERP
WHERE SYS_SRC_ID = 'PSO'
AND  EXE_YRMON = (SELECT MAX(EXE_YRMON) from GL_ESTM_IF_ERP
                  WHERE SYS_SRC_ID = 'PSO'
                  AND EXE_YRMON > ' '
				  AND VSL_CD = @[vsl_cd]
			      AND SKD_VOY_NO = @[skd_voy_no]
                  AND SKD_DIR_CD = @[skd_dir_cd]
                  AND LOC_CD = SUBSTR(@[yd_cd],1,5)
                  )
AND VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]
AND LOC_CD = substr(@[yd_cd],1,5)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
