<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselPositionPollingManagementDBDAOSearchPositionPollingDetailListRSQL">
			<desc><![CDATA[SearchPositionPollingDetailList]]></desc>
			<sql><![CDATA[
SELECT	X.RCV_DT
    ,   X.DLY_RCV_SEQ
    ,   X.IF_RCV_SEQ
    ,   TO_CHAR(X.PLNG_GEN_GDT,'YYYYMMDDHH24MISS') PLNG_GEN_GDT
    ,   X.VSL_CD
    ,   X.VSL_ENG_NM
    ,   X.CALL_SGN_NO
    ,   X.LLOYD_NO
    ,   X.SKD_VOY_NO
    ,   X.SKD_DIR_CD
    ,   X.VSL_LAT
    ,   X.VSL_LON
    ,   X.VSL_SPD
	,   X.VSL_PROG_DIR_CTNT    
FROM	VSK_VSL_PSN_PLNG_DTL X
WHERE	1 = 1
AND		X.DLY_RCV_SEQ 		= @[dly_rcv_seq]
AND		X.RCV_DT			= @[rcv_dt]
AND		X.VSL_CD			IS NOT NULL			]]></sql>
			<params>
				<param name="dly_rcv_seq" type="12" value="" out="N"/>
				<param name="rcv_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
