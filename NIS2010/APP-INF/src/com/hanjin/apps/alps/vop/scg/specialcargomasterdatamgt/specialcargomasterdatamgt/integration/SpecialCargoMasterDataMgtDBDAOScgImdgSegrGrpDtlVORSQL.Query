<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgSegrGrpDtlVORSQL">
			<desc><![CDATA[Segregation Groups (Creation) 의 Heavy metals and their salts 조회]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_SEGR_GRP_NO
,	IMDG_UN_NO
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_IMDG_SEGR_GRP_DTL
WHERE	IMDG_SEGR_GRP_NO = @[imdg_segr_grp_no]
ORDER BY
    IMDG_UN_NO			]]></sql>
			<params>
				<param name="imdg_segr_grp_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
