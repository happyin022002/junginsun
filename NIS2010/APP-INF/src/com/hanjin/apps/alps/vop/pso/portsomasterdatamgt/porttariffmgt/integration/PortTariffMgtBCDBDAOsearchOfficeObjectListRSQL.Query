<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchOfficeObjectListRSQL">
			<desc><![CDATA[searchOfficeObjectList]]></desc>
			<sql><![CDATA[
SELECT     
b.obj_list_no,
b.pso_obj_cd,
--(SELECT z.intg_cd_val_dp_desc
--  FROM com_intg_cd_dtl z
--   where z.intg_cd_id = 'CD01846'
--   and b.pso_obj_cd = z.intg_cd_val_ctnt
--  ) 
b.OBJ_LIST_ABBR_NM pso_obj_cd_dsp, --scalar 
           b.obj_list_nm,
           (SELECT z.intg_cd_val_dp_desc
  FROM com_intg_cd_dtl z
   where z.intg_cd_id = 'CD01848'
   and b.pso_meas_ut_cd = z.intg_cd_val_ctnt
  ) pso_meas_ut_cd_dsp , --scalar CD01848
  pso_meas_ut_cd,
	row_no,
	a.ofc_cd pso_ofc_cd
    FROM   PSO_INV_OFC_OBJ_LIST a, pso_obj_list b
   WHERE   
a.obj_list_no = b.obj_list_no 
--AND row_no = 1
#if( ${pso_ofc_cd}!='')
AND ofc_cd = @[pso_ofc_cd]
#end
ORDER BY   obj_list_seq			]]></sql>
			<params>
				<param name="pso_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
