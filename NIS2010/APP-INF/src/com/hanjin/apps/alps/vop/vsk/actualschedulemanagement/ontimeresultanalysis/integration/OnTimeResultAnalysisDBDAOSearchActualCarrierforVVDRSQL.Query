<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnTimeResultAnalysisDBDAOSearchActualCarrierforVVDRSQL">
			<desc><![CDATA[VVD에 대해서 ACTUAL CARRIER 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT    NVL(VS.ACT_CRR_CD,VC.CRR_CD)      AS ACT_CRR_CD
FROM      MDM_VSL_CNTR     VC
       ,  VSK_VSL_SKD      VS
WHERE     VC.VSL_CD        = VS.VSL_CD 
AND       VS.VSL_CD        = @[vsl_cd]
AND       VS.SKD_VOY_NO    = @[voy_no]
AND       VS.SKD_DIR_CD    = @[dir_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
