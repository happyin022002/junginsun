<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselInformationMgtDBDAOVSLPerformanceDetailVORSQL">
			<desc><![CDATA[RPM & Slow Steaming, Design Max Load Hold/Deck, OPT Trim, ESI Score 를 조회한다.

//History
2014.04.16 박다은 [CHM-201429675-01] Voyage Performance내 Lane Code 구분]]></desc>
			<sql><![CDATA[
SELECT 
        VSL_CD              
      ,	CTCL_RPM_NO
      ,	CTCL_TO_RPM_NO        
      ,	OP_MIN_RPM_NO      
      ,	OP_MIN_SPD         
      ,	SLW_STMNG_FLG      
      ,	SPR_SLW_STMNG_FLG  
      ,	FUEL_SAV_EQ_FLG    
      ,	IN_HLD_PER_TR_KNT  
      ,	IN_HLD_PER_ROW_KNT 
      ,	HTCH_CVR_IN_HLD_KNT
      ,	ON_DECK_PER_TR_KNT 
      ,	ON_DECK_PER_ROW_KNT
      ,	BOW_HGT            
      ,	TRSM_HGT        
      ,	SHP_IDX_SCRE
	  , VSL_LOD_RTO
	  , AMP_TP_CD
  FROM VSK_VSL_ADD_SPEC
 WHERE VSL_CD = @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
