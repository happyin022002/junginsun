<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchCstSkdHisByVvdRSQL">
			<desc><![CDATA[Coastal SKD 를 조회한다.
-------------------------------------------------------------------------------------------
2011.10.26 김민아 [CHM-201114112-01] VSL SKD History Inquiry 화면 로직 변경 : 페이징 처리]]></desc>
			<sql><![CDATA[
SELECT  BFR_VSL_CD
       ,BFR_SKD_VOY_NO
       ,BFR_SKD_DIR_CD
       ,BFR_VSL_SLAN_CD
       ,BFR_VPS_ETA_DT
       ,BFR_VPS_ETB_DT
       ,BFR_VPS_ETD_DT
       ,BFR_VPS_PORT_CD
       ,BFR_YD_CD
       ,AFT_VSL_CD
       ,AFT_SKD_VOY_NO
       ,AFT_SKD_DIR_CD
       ,AFT_VSL_SLAN_CD
       ,AFT_VPS_ETA_DT
       ,AFT_VPS_ETB_DT
       ,AFT_VPS_ETD_DT
       ,AFT_VPS_PORT_CD
       ,AFT_YD_CD
       ,VSKD_CNG_TP_CD
       ,CRE_DT
       ,CRE_USR_ID
       ,UPD_DT
       ,UPD_USR_ID
       ,DIFF_RMK
	   ,SORT_DATE
       ,TOTAL_CNT
FROM   (
        SELECT	(BFR_VSL_CD || BFR_SKD_VOY_NO || BFR_SKD_DIR_CD) AS BFR_VSL_CD
                , BFR_SKD_VOY_NO
                , BFR_SKD_DIR_CD
                --, (BFR_VSL_CD || BFR_SKD_VOY_NO || BFR_SKD_DIR_CD) AS BFR_VVD
                , BFR_VSL_SLAN_CD
                , TO_CHAR(BFR_VPS_ETA_DT, 'YYYYMMDDHH24MI') AS BFR_VPS_ETA_DT
                , TO_CHAR(BFR_VPS_ETB_DT, 'YYYYMMDDHH24MI') AS BFR_VPS_ETB_DT
                , TO_CHAR(BFR_VPS_ETD_DT, 'YYYYMMDDHH24MI') AS BFR_VPS_ETD_DT
                , BFR_VPS_PORT_CD
                , DECODE(BFR_YD_CD, NULL, '', SUBSTR(BFR_YD_CD, 6, 2)) AS BFR_YD_CD
                , (AFT_VSL_CD || AFT_SKD_VOY_NO || AFT_SKD_DIR_CD) AS AFT_VSL_CD
                , AFT_SKD_VOY_NO
                , AFT_SKD_DIR_CD
                --, (AFT_VSL_CD || AFT_SKD_VOY_NO || AFT_SKD_DIR_CD) AS AFT_VVD
                , AFT_VSL_SLAN_CD
                , TO_CHAR(AFT_VPS_ETA_DT, 'YYYYMMDDHH24MI') AS AFT_VPS_ETA_DT
                , TO_CHAR(AFT_VPS_ETB_DT, 'YYYYMMDDHH24MI') AS AFT_VPS_ETB_DT
                , TO_CHAR(AFT_VPS_ETD_DT, 'YYYYMMDDHH24MI') AS AFT_VPS_ETD_DT
                , AFT_VPS_PORT_CD
                , DECODE(AFT_YD_CD, NULL, '', SUBSTR(AFT_YD_CD, 6, 2)) AS AFT_YD_CD
                , VSKD_CNG_TP_CD
                , TO_CHAR(CRE_DT, 'YYYYMMDD') AS CRE_DT
                , CRE_USR_ID
                , TO_CHAR(UPD_DT, 'YYYYMMDD') AS UPD_DT
                , UPD_USR_ID
                , DIFF_RMK
        		, UPD_DT AS SORT_DATE
        		, ROW_NUMBER() OVER(ORDER BY UPD_DT DESC) AS RNUM
                , COUNT(1) OVER() AS TOTAL_CNT
        FROM	VSK_VSL_SKD_HIS
        WHERE	BFR_VSL_CD	= @[vsl_cd]
        AND 	VSKD_CNG_NO LIKE '0%'
        AND		BFR_SKD_VOY_NO	LIKE NVL(@[skd_voy_no], '') || '%'
        AND		BFR_SKD_DIR_CD	LIKE NVL(@[skd_dir_cd], '') || '%'
        #if (${vsl_slan_cd} != '') 
        AND		BFR_VSL_SLAN_CD	= @[vsl_slan_cd]
        #end
        #if (${vps_port_cd} != '') 
        AND		BFR_VPS_PORT_CD	= @[vps_port_cd]
        #end
        #if (${fm_dt} != '' && ${to_dt} != '') 
        AND		CRE_DT		BETWEEN	TO_DATE(@[fm_dt], 'YYYY-MM-DD')
        					AND		TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
        #end
        )
WHERE   1 = 1
  AND   RNUM BETWEEN (@[page_no]-1)*@[pagerows]+1 AND @[page_no]*@[pagerows]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="page_no" type="12" value="" out="N"/>
				<param name="pagerows" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
