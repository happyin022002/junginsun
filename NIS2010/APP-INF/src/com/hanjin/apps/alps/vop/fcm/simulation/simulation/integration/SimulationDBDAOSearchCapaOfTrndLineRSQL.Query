<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SimulationDBDAOSearchCapaOfTrndLineRSQL">
			<desc><![CDATA[Trnd Line이 생성되어 있는 Capa를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT VSL_CLSS_CD
FROM FCM_TRND_LINE
WHERE VSL_CLSS_CD IS NOT NULL
AND TRND_LINE_USE_TP_CD='N'
ORDER BY TO_NUMBER(VSL_CLSS_CD)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
