<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierRestrictionDBDAOScgImdgCrrRstrVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT   A.VSL_OPR_TP_CD ,
         ''IMDG_CRR_RSTR_SEQ           ,
         ''IMDG_UN_NO                  ,
         ''IMDG_UN_NO_SEQ              ,
         ''IMDG_CLSS_CD                ,
         ''IMDG_COMP_GRP_CD            ,
         ''IMDG_CRR_RSTR_EXPT_CD       ,
         ''SLAN_CD                     ,
         ''CRR_REGU_DESC               ,
         ''CRE_USR_ID                  ,
         ''CRE_DT                      ,
         ''UPD_USR_ID    
  FROM   SCG_IMDG_CRR_RSTR A
 WHERE   1 = 1
#if (${vsl_opr_tp_cd} != '') 
   AND A.VSL_OPR_TP_CD = @[vsl_opr_tp_cd]
#end 
#if (${imdg_clss_cd} != '') 
   AND A.IMDG_CLSS_CD= @[imdg_clss_cd]
   AND A.IMDG_UN_NO IS NULL
#end
#if (${imdg_un_no} != '') 
   AND   A.IMDG_UN_NO  = @[imdg_un_no]
   AND   A.IMDG_UN_NO_SEQ  = @[imdg_un_no_seq]
#end
GROUP BY  A.VSL_OPR_TP_CD
ORDER BY A.VSL_OPR_TP_CD			]]></sql>
			<params>
				<param name="vsl_opr_tp_cd" type="12" value="" out="N"/>
				<param name="imdg_clss_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
