<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationDBDAOSearchAutoIFStatusRSQL">
			<desc><![CDATA[Search auto I/F status.]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*), 1, 'AUTO', 0, 'NON-AUTO', '') AUTO_STATUS
FROM   FCM_ESTM_WK_CSM_IF
WHERE  1=1
AND    SUBSTR(BSE_YRMON, 1, 4) = '9999'
AND    BSE_WK = '99'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
