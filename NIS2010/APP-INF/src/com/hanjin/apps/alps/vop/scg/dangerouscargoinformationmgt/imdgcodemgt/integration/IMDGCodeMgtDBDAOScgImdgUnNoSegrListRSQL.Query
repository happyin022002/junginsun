<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IMDGCodeMgtDBDAOScgImdgUnNoSegrListRSQL">
			<desc><![CDATA[ScgImdgUnNoSegr 조회]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_UN_NO
,	IMDG_UN_NO_SEQ
,   MAX(CLSS_CD_11) AS CLSS_CD_11
,   MAX(CLSS_CD_12) AS CLSS_CD_12
,   MAX(CLSS_CD_15) AS CLSS_CD_15
,   MAX(CLSS_CD_13) AS CLSS_CD_13
,   MAX(CLSS_CD_16) AS CLSS_CD_16
,   MAX(CLSS_CD_14) AS CLSS_CD_14
,   MAX(CLSS_CD_21) AS CLSS_CD_21
,   MAX(CLSS_CD_22) AS CLSS_CD_22
,   MAX(CLSS_CD_23) AS CLSS_CD_23
,   MAX(CLSS_CD_3) AS CLSS_CD_3
,   MAX(CLSS_CD_41) AS CLSS_CD_41
,   MAX(CLSS_CD_42) AS CLSS_CD_42
,   MAX(CLSS_CD_43) AS CLSS_CD_43
,   MAX(CLSS_CD_51) AS CLSS_CD_51
,   MAX(CLSS_CD_52) AS CLSS_CD_52
,   MAX(CLSS_CD_61) AS CLSS_CD_61
,   MAX(CLSS_CD_62) AS CLSS_CD_62
,   MAX(CLSS_CD_7) AS CLSS_CD_7
,   MAX(CLSS_CD_8) AS CLSS_CD_8
,   MAX(CLSS_CD_9) AS CLSS_CD_9
FROM (
    SELECT 
    	IMDG_UN_NO
    ,	IMDG_UN_NO_SEQ
    ,   DECODE(IMDG_CLSS_CD,'1.1',IMDG_SEGR_CD) AS CLSS_CD_11
    ,   DECODE(IMDG_CLSS_CD,'1.2',IMDG_SEGR_CD) AS CLSS_CD_12
    ,   DECODE(IMDG_CLSS_CD,'1.5',IMDG_SEGR_CD) AS CLSS_CD_15
    ,   DECODE(IMDG_CLSS_CD,'1.3',IMDG_SEGR_CD) AS CLSS_CD_13
    ,   DECODE(IMDG_CLSS_CD,'1.6',IMDG_SEGR_CD) AS CLSS_CD_16
    ,   DECODE(IMDG_CLSS_CD,'1.4',IMDG_SEGR_CD) AS CLSS_CD_14
    ,   DECODE(IMDG_CLSS_CD,'2.1',IMDG_SEGR_CD) AS CLSS_CD_21
    ,   DECODE(IMDG_CLSS_CD,'2.2',IMDG_SEGR_CD) AS CLSS_CD_22
    ,   DECODE(IMDG_CLSS_CD,'2.3',IMDG_SEGR_CD) AS CLSS_CD_23
    ,   DECODE(IMDG_CLSS_CD,'3',IMDG_SEGR_CD) AS CLSS_CD_3
    ,   DECODE(IMDG_CLSS_CD,'4.1',IMDG_SEGR_CD) AS CLSS_CD_41
    ,   DECODE(IMDG_CLSS_CD,'4.2',IMDG_SEGR_CD) AS CLSS_CD_42
    ,   DECODE(IMDG_CLSS_CD,'4.3',IMDG_SEGR_CD) AS CLSS_CD_43
    ,   DECODE(IMDG_CLSS_CD,'5.1',IMDG_SEGR_CD) AS CLSS_CD_51
    ,   DECODE(IMDG_CLSS_CD,'5.2',IMDG_SEGR_CD) AS CLSS_CD_52
    ,   DECODE(IMDG_CLSS_CD,'6.1',IMDG_SEGR_CD) AS CLSS_CD_61
    ,   DECODE(IMDG_CLSS_CD,'6.2',IMDG_SEGR_CD) AS CLSS_CD_62
    ,   DECODE(IMDG_CLSS_CD,'7',IMDG_SEGR_CD) AS CLSS_CD_7
    ,   DECODE(IMDG_CLSS_CD,'8',IMDG_SEGR_CD) AS CLSS_CD_8
    ,   DECODE(IMDG_CLSS_CD,'9',IMDG_SEGR_CD) AS CLSS_CD_9
    FROM SCG_IMDG_UN_NO_SEGR
	WHERE	IMDG_UN_NO = @[imdg_un_no]
	AND	IMDG_UN_NO_SEQ = @[imdg_un_no_seq]
)
GROUP BY IMDG_UN_NO, IMDG_UN_NO_SEQ
			]]></sql>
			<params>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="imdg_un_no_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
