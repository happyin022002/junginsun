<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMasterDataDBDAOModifyVskCanalVendorUSQL">
			<desc><![CDATA[ModifyVskCanalVendor]]></desc>
			<sql><![CDATA[
MERGE INTO   VSK_CNL_VNDR T
USING        (
             SELECT     @[vsl_slan_cd]      AS VSL_SLAN_CD
                      , @[cnl_agn_vndr_seq] AS CNL_AGN_VNDR_SEQ   
                      , @[cre_usr_id]       AS CRE_USR_ID    
                      , SYSDATE             AS CRE_DT        
                      , @[upd_usr_id]       AS UPD_USR_ID    
                      , SYSDATE             AS UPD_DT                   
                FROM    DUAL 
              ) X          
ON            (
              T.VSL_SLAN_CD = X.VSL_SLAN_CD
     AND      T.CNL_AGN_VNDR_SEQ = X.CNL_AGN_VNDR_SEQ         
              )
WHEN MATCHED THEN      
              UPDATE SET 
                           T.UPD_USR_ID     = X.UPD_USR_ID
                         , T.UPD_DT         = X.UPD_DT
WHEN NOT MATCHED THEN
              INSERT   (
                          T.VSL_SLAN_CD   
                        , T.CNL_AGN_VNDR_SEQ     
                        , T.CRE_USR_ID      
                        , T.CRE_DT          
                        , T.UPD_USR_ID      
                        , T.UPD_DT
                       )            
              VALUES   (  X.VSL_SLAN_CD           
                        , X.CNL_AGN_VNDR_SEQ   
                        , X.CRE_USR_ID        
                        , X.CRE_DT           
                        , X.UPD_USR_ID     
                        , X.UPD_DT           
                        )			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="cnl_agn_vndr_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
