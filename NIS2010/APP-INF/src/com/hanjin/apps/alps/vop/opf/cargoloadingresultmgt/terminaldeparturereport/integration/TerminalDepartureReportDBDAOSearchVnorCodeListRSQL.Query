<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOSearchVnorCodeListRSQL">
			<desc><![CDATA[코드 리스트를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  INTG_CD_ID
       ,INTG_CD_VAL_CTNT
       ,INTG_CD_VAL_DP_DESC
       ,INTG_CD_VAL_DESC
FROM COM_INTG_CD_DTL
WHERE INTG_CD_ID = @[intg_cd_id]
#if(${intg_cd_val_ctnt} != '')
AND SUBSTR(INTG_CD_VAL_CTNT, 0, 1) = @[intg_cd_val_ctnt]
#end
ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="intg_cd_id" type="12" value="" out="N"/>
				<param name="intg_cd_val_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
