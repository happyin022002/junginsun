<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOAddPackingInstructionPortableTankCSQL">
			<desc><![CDATA[AddPackingInstructionPortableTank]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_PCK_PTB_TNK(
       PTBL_TNK_INSTR_CD
      ,SUB_SEQ
      ,IMDG_UN_NO
      ,MIN_TST_PRSS
      ,MIN_SHL_THCK_CTNT
      ,BTM_OPN_PROVI_CTNT
      ,PRS_RLF_PROVI_CTNT
      ,SBST_DESC
      ,MAT_SUB_DESC_REF_DIV_NO
      ,FILL_DGR_CTNT
      ,IMDG_CTRL_TEMP
      ,CTRL_TEMP_REF_DIV_NO
      ,IMDG_EMER_TEMP
      ,EMER_TEMP_REF_DIV_NO
      ,MAX_ALW_WRK_SML_PRSS
	  ,MAX_ALW_WRK_BARE_PRSS
	  ,MAX_ALW_WRK_SUN_SHLD_PRSS
	  ,MAX_ALW_WRK_INSLT_PRSS
      ,OPN_BLW_LQD_LVL_CD
      ,MAX_FILL_DNST_CTNT
      ,PCK_REF_CD
      ,REF_DIV_NO 
      ,DELT_FLG
      ,CRE_USR_ID 
      ,CRE_DT 
      ,UPD_USR_ID 
      ,UPD_DT 
)
SELECT @[ptbl_tnk_instr_cd]
      ,NVL(MAX(SUB_SEQ), 0) + 1
      ,@[imdg_un_no]
      ,@[min_tst_prss]
      ,@[min_shl_thck_ctnt]
      ,@[btm_opn_provi_ctnt]
      ,@[prs_rlf_provi_ctnt]
      ,@[sbst_desc]
      ,@[mat_sub_desc_REF_DIV_NO]
      ,@[fill_dgr_ctnt]
      ,@[imdg_ctrl_temp]
      ,@[cntr_temp_REF_DIV_NO]
      ,@[imdg_emer_temp]
      ,@[emer_temp_REF_DIV_NO]
      ,@[max_alw_wrk_sml_prss]
  	  ,@[max_alw_wrk_bare_prss]
	  ,@[max_alw_wrk_sun_shld_prss]
	  ,@[max_alw_wrk_inslt_prss]
      ,@[opn_blw_lqd_lvl_cd]
      ,@[max_fill_dnst_ctnt]
      ,@[pck_ref_cd]
      ,@[ref_div_no]
      ,'N'
      ,@[cre_usr_id]
      ,SYSDATE 
      ,@[cre_usr_id]
      ,SYSDATE
 FROM SCG_PCK_PTB_TNK
WHERE PTBL_TNK_INSTR_CD     = @[ptbl_tnk_instr_cd]			]]></sql>
			<params>
				<param name="ptbl_tnk_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_un_no" type="12" value="" out="N"/>
				<param name="min_tst_prss" type="12" value="" out="N"/>
				<param name="min_shl_thck_ctnt" type="12" value="" out="N"/>
				<param name="btm_opn_provi_ctnt" type="12" value="" out="N"/>
				<param name="prs_rlf_provi_ctnt" type="12" value="" out="N"/>
				<param name="sbst_desc" type="12" value="" out="N"/>
				<param name="mat_sub_desc_REF_DIV_NO" type="12" value="" out="N"/>
				<param name="fill_dgr_ctnt" type="12" value="" out="N"/>
				<param name="imdg_ctrl_temp" type="12" value="" out="N"/>
				<param name="cntr_temp_REF_DIV_NO" type="12" value="" out="N"/>
				<param name="imdg_emer_temp" type="12" value="" out="N"/>
				<param name="emer_temp_REF_DIV_NO" type="12" value="" out="N"/>
				<param name="max_alw_wrk_sml_prss" type="12" value="" out="N"/>
				<param name="max_alw_wrk_bare_prss" type="12" value="" out="N"/>
				<param name="max_alw_wrk_sun_shld_prss" type="12" value="" out="N"/>
				<param name="max_alw_wrk_inslt_prss" type="12" value="" out="N"/>
				<param name="opn_blw_lqd_lvl_cd" type="12" value="" out="N"/>
				<param name="max_fill_dnst_ctnt" type="12" value="" out="N"/>
				<param name="pck_ref_cd" type="12" value="" out="N"/>
				<param name="ref_div_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
