<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOsearchUseIDFormulaDetaillRSQL">
			<desc><![CDATA[현재 Terminal에서 해당 Formula를 사용하고 있는 Terminal List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT A.YD_CD YD_CD
      ,B.ACCT_CD ACCT_CD
      ,A.LGS_COST_CD COST_CD
      ,D.VNDR_LGL_ENG_NM VNDR_NM
      ,A.YD_CHG_VER_SEQ VER
      ,A.PSO_CHG_TP_CD CHG_TP
      ,C.INTG_CD_VAL_DP_DESC CHG_TP_NM
      ,@[id_no] ID_NO
      ,'F' ID_TP
FROM   (
        SELECT DISTINCT T3.YD_CD
              ,T3.LGS_COST_CD
              ,T3.YD_CHG_VER_SEQ
              ,T2.PSO_CHG_TP_CD              
              ,T3.VNDR_SEQ
        FROM   PSO_CHG_XPR_DTL T1
              ,PSO_YD_CHG_XPR T2
              ,PSO_YD_CHG T3
        WHERE  1=1
        AND    T1.CHG_XPR_NO = T2.CHG_XPR_NO
        AND    T2.YD_CHG_NO = T3.YD_CHG_NO
        AND    T2.YD_CHG_VER_SEQ = T3.YD_CHG_VER_SEQ
        AND    T1.FOML_NO = @[id_no]
       ) A
      ,TES_LGS_COST B
      ,(SELECT B.INTG_CD_VAL_CTNT
              ,B.INTG_CD_VAL_DP_DESC
        FROM   COM_INTG_CD     A
              ,COM_INTG_CD_DTL B
        WHERE  1 = 1
        AND    A.INTG_CD_ID = 'CD01842'
        AND    A.INTG_CD_ID = B.INTG_CD_ID) C
       ,MDM_VENDOR D
WHERE   1=1   
AND     A.LGS_COST_CD = B.LGS_COST_CD     
AND     A.PSO_CHG_TP_CD = C.INTG_CD_VAL_CTNT
AND     D.DELT_FLG = 'N'
AND     D.VNDR_SEQ = A.VNDR_SEQ
ORDER  BY 1
         ,2
         ,3
         ,4
         ,5
         ,6			]]></sql>
			<params>
				<param name="id_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
