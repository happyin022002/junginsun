<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchEdiHdToExceptKorRSQL">
			<desc><![CDATA[한국지역을 제외한 타지역에 EDI 발송을 위한 헤더생성]]></desc>
			<sql><![CDATA[
SELECT	'$$$MSGSTART:'
	|| RPAD(NVL(TRIM(@[sender_id]	),' '),20,' ')
	|| RPAD(NVL(TRIM(@[receiver_id]	),' '),20,' ')
	|| RPAD(NVL(TRIM('IFTSAI'		),' '),10,' ')
	|| @[header_seq] AS HEADER
FROM	DUAL			]]></sql>
			<params>
				<param name="sender_id" type="12" value="" out="N"/>
				<param name="receiver_id" type="12" value="" out="N"/>
				<param name="header_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
