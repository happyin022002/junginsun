<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOSearchFomlNoByObjListNoRSQL">
			<desc><![CDATA[[VOP_PSO_0002화면 전용] OBJECT_LIST_NO와 Rate_Type으로 FORMULA_NO를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT A.FOML_NO
      ,A.FOML_NM
      ,A.PSO_FOML_MZD_CD
      ,A.FOML_DESC
      ,A.FOML_SYS_DESC  
FROM   PSO_FORMULA  A
      ,PSO_FOML_DTL B 
WHERE  1=1
AND    B.OBJ_LIST_NO = 45
AND    B.RT_OBJ_LIST_NO = (SELECT OBJ_LIST_NO 
                           FROM   PSO_OBJ_LIST 
                           WHERE  PSO_OBJ_CD = @[pso_obj_cd] 
                           AND    PSO_MEAS_UT_CD = @[pso_meas_ut_cd])
AND    B.FOML_SEQ = DECODE(@[pso_trf_tp_cd], 'R', 3, 'S', 1, 'F', 3)
AND    B.FOML_NO = A.FOML_NO
AND    A.UPD_MNU_NO = 1
			]]></sql>
			<params>
				<param name="pso_obj_cd" type="12" value="" out="N"/>
				<param name="pso_meas_ut_cd" type="12" value="" out="N"/>
				<param name="pso_trf_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
