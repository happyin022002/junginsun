<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMgtDBDAOSearchEdiHdSeqForTerminalTransmitRSQL">
			<desc><![CDATA[Terminal로의 EDI 발송을 위한 EDI Header Sequence 생성]]></desc>
			<sql><![CDATA[
SELECT		@[indicator]
		|| 	TO_CHAR(SYSDATE,'RRMMDD') 
		|| 	LTRIM(TO_CHAR(VSK_EDI_SEQ.NEXTVAL, '00000')) AS HEADERSEQ
FROM	DUAL			]]></sql>
			<params>
				<param name="indicator" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
