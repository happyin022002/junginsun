<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOCodeInfoVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
#if (${grd_nm} == 'CD0XXXX') 
SELECT 
	@[grd_nm]	GRD_NM
	,A.CRR_CD CODE
	,A.CRR_NM NAME
FROM   MDM_CARRIER A
WHERE  1=1
AND    A.DELT_FLG = 'N'
#if (${code} != '') 
AND     UPPER(A.CRR_CD) LIKE @[code] || '%'
#end
#if (${name} != '') 
AND     UPPER(A.CRR_NM) LIKE '%' || UPPER(@[name]) || '%'
#end
ORDER BY A.CRR_CD
#else 
SELECT 
	@[grd_nm]	GRD_NM
	,A.INTG_CD_VAL_CTNT CODE
	,A.INTG_CD_VAL_DESC NAME
FROM   COM_INTG_CD_DTL A
WHERE  1=1
AND    A.INTG_CD_ID = @[grd_nm]
#if (${code} != '') 
AND     A.INTG_CD_VAL_CTNT LIKE @[code] || '%'
#end
#if (${name} != '') 
AND     UPPER(A.INTG_CD_VAL_DESC) LIKE '%' || UPPER(@[name]) || '%'
#end
ORDER BY A.INTG_CD_VAL_DP_SEQ
#end			]]></sql>
			<params>
				<param name="grd_nm" type="12" value="" out="N"/>
				<param name="code" type="12" value="" out="N"/>
				<param name="name" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
