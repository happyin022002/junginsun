<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceDBDAOFcmMonFoilSavCostVOCSQL">
			<desc><![CDATA[Monthly Fuel Oil Saving Cost 정보를 생성한다.

History
2012.04.04 진마리아 CHM-201216636-01 [FCM] ALPS 모델 및 DB 구조 불일치 복구
2012.07.18 이혜민    CHM-201219005-01 [FCM] fuel_adt 컬럼명 fuel_adtv 로 수정]]></desc>
			<sql><![CDATA[
/* Monthly Fuel Oil Saving Cost 정보를 생성한다. */
INSERT INTO FCM_MON_FUEL_OIL_SAV_COST (
  SAV_ITM_CD,
  SAV_COST_CRE_YRMON,
  VSL_CD,
  SKD_VOY_NO,
  SKD_DIR_CD,
  VPS_PORT_CD,
  VSL_CLSS_CD,
  VSL_CLSS_SUB_CD,
  VSL_SLAN_CD,
  NVGT_DIST,
  AVG_SPD,
  AVG_RPM_PWR,
  LOD_IND_QTY,
  FOIL_CSM_WGT,
  FOIL_CSM_COST_AMT,
  SAV_RTO,
  SAV_QTY,
  SAV_COST_AMT,
  PORT_PAIR_SEA_FOIL_CSM_WGT,
  FUEL_ADTV_CSM_WGT,
  FUEL_ADTV_UC_AMT,
  FUEL_ADTV_CSM_COST_AMT,
  FUEL_ADTV_DEP_CSM_COST_AMT,
  FUEL_ADTV_SAV_COST_AMT,
  CRE_USR_ID,
  CRE_DT,
  UPD_USR_ID,
  UPD_DT,
  CLPT_IND_SEQ
) VALUES(
  @[sav_itm_cd],
  @[sav_cost_cre_yrmon],
  @[vsl_cd],
  @[skd_voy_no],
  @[skd_dir_cd],
  @[vps_port_cd],
  @[vsl_clss_cd],
  @[vsl_clss_sub_cd],
  @[vsl_slan_cd],
  @[nvgt_dist],
  @[avg_spd],
  @[avg_rpm_pwr],
  @[lod_ind_qty],
  @[foil_csm_wgt],
  @[foil_csm_cost_amt],
  @[sav_rto],
  @[sav_qty],
  @[sav_cost_amt],
  @[port_pair_sea_foil_csm_wgt],
  @[fuel_adtv_csm_wgt],
  @[fuel_adtv_uc_amt],
  @[fuel_adtv_csm_cost_amt],
  @[fuel_adtv_dep_csm_cost_amt],
  @[fuel_adtv_sav_cost_amt],
  @[cre_usr_id],
  SYSDATE,
  @[cre_usr_id],
  SYSDATE,
  @[clpt_ind_seq]
)			]]></sql>
			<params>
				<param name="sav_itm_cd" type="12" value="" out="N"/>
				<param name="sav_cost_cre_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_sub_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="nvgt_dist" type="12" value="" out="N"/>
				<param name="avg_spd" type="12" value="" out="N"/>
				<param name="avg_rpm_pwr" type="12" value="" out="N"/>
				<param name="lod_ind_qty" type="12" value="" out="N"/>
				<param name="foil_csm_wgt" type="12" value="" out="N"/>
				<param name="foil_csm_cost_amt" type="12" value="" out="N"/>
				<param name="sav_rto" type="12" value="" out="N"/>
				<param name="sav_qty" type="12" value="" out="N"/>
				<param name="sav_cost_amt" type="12" value="" out="N"/>
				<param name="port_pair_sea_foil_csm_wgt" type="12" value="" out="N"/>
				<param name="fuel_adtv_csm_wgt" type="12" value="" out="N"/>
				<param name="fuel_adtv_uc_amt" type="12" value="" out="N"/>
				<param name="fuel_adtv_csm_cost_amt" type="12" value="" out="N"/>
				<param name="fuel_adtv_dep_csm_cost_amt" type="12" value="" out="N"/>
				<param name="fuel_adtv_sav_cost_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
