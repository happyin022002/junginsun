<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOcheckVvdLevelRSQL">
			<desc><![CDATA[checkVvdLevel]]></desc>
			<sql><![CDATA[
SELECT
	DISTINCT 'OK'
FROM
	ar_mst_rev_vvd F
WHERE
	F.vsl_cd = @[vsl_cd]
	AND F.skd_voy_no = @[skd_voy_no]
	AND F.skd_dir_cd = @[skd_dir_cd]
	AND EXISTS (
	    	SELECT
	    		'Y'
	    	FROM
	    		mdm_account
	    	WHERE
	    		acct_cd = DECODE(@[acct_cd], '110911', acct_cd, '564611', acct_cd, @[acct_cd])
	    		AND F.vvd_com_lvl IN (DECODE(vvd_lvl_flg1, 'Y', '1', ''), DECODE(vvd_lvl_flg2, 'Y', '2', ''), 
	    		                     DECODE(vvd_lvl_flg3, 'Y', '3', ''), DECODE(vvd_lvl_flg4, 'Y', '4', ''), 
	    		                     DECODE(vvd_lvl_flg5, 'Y', '5', ''), DECODE(vvd_lvl_flg6, 'Y', '6', ''))
     )			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
