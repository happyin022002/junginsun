<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneInformationMgtDBDAOVskLanePicVOCSQL">
			<desc><![CDATA[Lane PIC 정보를 추가 합니다.
===============================================
History
2011.04.28 진마리아 [CHM-201110229-01] Lane informtion내 PIC의 Vessel 칼럼을 Carrier로 변경 요청건]]></desc>
			<sql><![CDATA[
INSERT INTO VSK_LANE_PIC (
	SLAN_CD
,	RGN_SHP_OPR_CD
,	LANE_PIC_SEQ
,	LANE_PIC_TP_CD
,	VSL_SLAN_NM
,	LANE_MNG_USR_DESC
,	USR_NM
,	JB_DESC
,	PIC_VSL_DESC
,	PIC_PHN_NO
,	MPHN_NO
,	FAX_NO
,	LANE_PIC_USR_EML
,	PIC_EML
,	LANE_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	PIC_CRR_DESC
) VALUES( 
	@[slan_cd]
,	@[rgn_shp_opr_cd]
,	@[lane_pic_seq]
,	@[lane_pic_tp_cd]
,	@[vsl_slan_nm]
,	@[lane_mng_usr_desc]
,	@[usr_nm]
,	@[jb_desc]
,	@[pic_vsl_desc]
,	@[pic_phn_no]
,	@[mphn_no]
,	@[fax_no]
,	@[lane_pic_usr_eml]
,	@[pic_eml]
,	@[lane_rmk]
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,	@[pic_crr_desc]
)			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="rgn_shp_opr_cd" type="12" value="" out="N"/>
				<param name="lane_pic_seq" type="2" value="" out="N"/>
				<param name="lane_pic_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_nm" type="12" value="" out="N"/>
				<param name="lane_mng_usr_desc" type="12" value="" out="N"/>
				<param name="usr_nm" type="12" value="" out="N"/>
				<param name="jb_desc" type="12" value="" out="N"/>
				<param name="pic_vsl_desc" type="12" value="" out="N"/>
				<param name="pic_phn_no" type="12" value="" out="N"/>
				<param name="mphn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="lane_pic_usr_eml" type="12" value="" out="N"/>
				<param name="pic_eml" type="12" value="" out="N"/>
				<param name="lane_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pic_crr_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
