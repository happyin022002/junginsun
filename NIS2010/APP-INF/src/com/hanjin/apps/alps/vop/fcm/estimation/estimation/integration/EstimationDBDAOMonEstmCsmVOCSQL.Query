<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationDBDAOMonEstmCsmVOCSQL">
			<desc><![CDATA[Add the monthly estimation consumption of a specific month.]]></desc>
			<sql><![CDATA[
INSERT INTO FCM_ESTM_MON_CSM VALUES(
	 REPLACE(@[exe_yrmon], '-', '')
	,REPLACE(@[rev_yrmon], '-', '')
	,@[rlane_cd]
	,@[vsl_cd]
	,@[skd_voy_no]
	,@[skd_dir_cd]
	,@[rev_dir_cd]
	,@[estm_vvd_tp_cd]
	,@[csm_oil_tp_cd]
	,@[pre_estm_vvd_tp_cd]
	,@[crr_cd]
	,@[to_flg]
	,@[err_flg]
	,@[itm_err_cd]
	,@[cntr_dzn_capa]
	,@[trnd_line_seq]
	,@[trnd_line_no]
	,@[act_st_port_cd]
	,@[act_st_dt]
	,@[act_end_port_cd]
	,@[act_end_dt]
	,@[estm_seq_no]
	,@[tmp_st_port_cd]
	,@[tmp_st_dt]
	,@[tmp_end_port_cd]
	,@[tmp_end_dt]
	,@[mon_bgn_invt_wgt]
	,@[pre_vvd_invt_wgt]
	,@[po_rcv_wgt]
	,@[bod_wgt]
	,@[bor_wgt]
	,@[voy_sea_dys]
	,@[voy_port_dys]
	,@[estm_sea_dys]
	,@[estm_port_dys]
	,@[voy_end_rmn_wgt]
	,@[voy_end_csm_wgt]
	,@[mon_end_rmn_wgt]
	,@[mon_end_csm_wgt]
	,@[estm_mon_csm_wgt]
	,@[cre_usr_id]
	,SYSDATE
	,@[upd_usr_id]
	,SYSDATE
)			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
