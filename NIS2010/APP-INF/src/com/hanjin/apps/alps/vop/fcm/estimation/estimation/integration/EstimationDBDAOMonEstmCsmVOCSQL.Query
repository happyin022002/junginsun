				<param name="to_flg" type="12" value="" out="N"/>
				<param name="err_flg" type="12" value="" out="N"/>
				<param name="itm_err_cd" type="12" value="" out="N"/>
				<param name="cntr_dzn_capa" type="12" value="" out="N"/>
				<param name="trnd_line_seq" type="12" value="" out="N"/>
