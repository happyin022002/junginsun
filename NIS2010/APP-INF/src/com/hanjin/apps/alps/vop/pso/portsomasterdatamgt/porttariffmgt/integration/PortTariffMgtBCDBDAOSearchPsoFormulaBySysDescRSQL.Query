<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOSearchPsoFormulaBySysDescRSQL">
			<desc><![CDATA[FOML_SYS_DESC로 Formula 조회하기]]></desc>
			<sql><![CDATA[
/**** FOML_SYS_DESC로 Formula 조회하기 ****/
SELECT A.* 
FROM   PSO_FORMULA A
WHERE  1=1
AND    A.FOML_SYS_DESC = @[foml_sys_desc]			]]></sql>
			<params>
				<param name="foml_sys_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
