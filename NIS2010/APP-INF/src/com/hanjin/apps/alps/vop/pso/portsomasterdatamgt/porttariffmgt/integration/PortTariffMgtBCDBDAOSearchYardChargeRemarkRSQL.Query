<sqls>
	<querys>
		<query name="PortTariffMgtBCDBDAOSearchYardChargeRemarkRSQL">
			<desc><![CDATA[PortTariffMgtBCDBDAOSearchYardChargeRemarkRSQL]]></desc>
			<sql><![CDATA[
select  YD_CHG_NO
       ,YD_CHG_VER_SEQ
       ,PORT_TRF_RMK
  from  PSO_YD_CHG
 where  YD_CHG_NO = @[yd_chg_no]
   and  YD_CHG_VER_SEQ = @[yd_chg_ver_seq]			]]></sql>
			<params>
				<param name="yd_chg_no" type="12" value="858" out="N"/>
				<param name="yd_chg_ver_seq" type="12" value="005" out="N"/>
			</params>
		</query>
	</querys>
