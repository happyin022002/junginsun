<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOUpdateScgNonDgCgoKwUSQL">
			<desc><![CDATA[COMMON NAME을 수정하기 위함]]></desc>
			<sql><![CDATA[
UPDATE scg_non_dg_cgo_kw  SET cmt_ctnt    = @[cmt_ctnt],
                              upd_usr_id  = @[upd_usr_id] ,
                              kw_incl_flg = DECODE(@[kw_incl_flg],'1','Y','0','','') ,
                              upd_dt      = SYSDATE
                        WHERE non_dcgo_kw_seq  = @[non_dcgo_kw_seq]			]]></sql>
			<params>
				<param name="cmt_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="kw_incl_flg" type="12" value="" out="N"/>
				<param name="non_dcgo_kw_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
