<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOUpdateVnorItemUSQL">
			<desc><![CDATA[VNOR 의 Item을 갱신합니다.]]></desc>
			<sql><![CDATA[
UPDATE OPF_VNOR_ITM
SET VNOR_ITM_CD = @[vnor_itm_cd]
    ,VNOR_ITM_OFC_CD = @[vnor_itm_ofc_cd]
    ,VNOR_ITM_UT_CD = @[vnor_itm_ut_cd]
    ,VNOR_ITM_VAL = @[vnor_itm_val]
    ,VNOR_ITM_STS_CD = @[vnor_itm_sts_cd]
	,VNOR_ITM_RMK = @[vnor_itm_rmk]
	,ATCH_FILE_LNK_ID = @[atch_file_lnk_id]
	,ATCH_FILE_KNT = @[atch_file_knt]
	,UPD_USR_ID = @[upd_usr_id]
	,UPD_DT = SYSDATE
WHERE VSL_CD = @[vsl_cd]
AND VNOR_SEQ = @[vnor_seq]
AND VNOR_ITM_SEQ = @[vnor_itm_seq]			]]></sql>
			<params>
				<param name="vnor_itm_cd" type="12" value="" out="N"/>
				<param name="vnor_itm_ofc_cd" type="12" value="" out="N"/>
				<param name="vnor_itm_ut_cd" type="12" value="" out="N"/>
				<param name="vnor_itm_val" type="12" value="" out="N"/>
				<param name="vnor_itm_sts_cd" type="12" value="" out="N"/>
				<param name="vnor_itm_rmk" type="12" value="" out="N"/>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="atch_file_knt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vnor_seq" type="12" value="" out="N"/>
				<param name="vnor_itm_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
