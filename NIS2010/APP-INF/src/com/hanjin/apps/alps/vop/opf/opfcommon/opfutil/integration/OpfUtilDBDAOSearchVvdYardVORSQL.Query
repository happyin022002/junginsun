<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OpfUtilDBDAOSearchVvdYardVORSQL">
			<desc><![CDATA[Search VvdYard]]></desc>
			<sql><![CDATA[
SELECT  DECODE(SKD_CNG_STS_CD, 'S', NULL, YD_CD) AS VAL,
		DECODE(SKD_CNG_STS_CD, 'S', NULL, YD_CD) || '|' || DECODE(SKD_CNG_STS_CD, 'S', NULL, 
        (   SELECT  YD_NM
            FROM    MDM_YARD
            WHERE   YD_CD       =   A.YD_CD
        )) AS NAME
        ,CLPT_IND_SEQ
FROM    VSK_VSL_PORT_SKD    A
WHERE  VSL_CD     = @[vsl_cd]
AND    SKD_VOY_NO = @[skd_voy_no]
AND    SKD_DIR_CD = @[skd_dir_cd]
AND    TURN_PORT_IND_CD NOT IN ('D','V')
AND    DECODE(TURN_PORT_IND_CD, 'F', DECODE(TURN_SKD_VOY_NO, 'N', 'N', 'F'), 'N') <> 'F'
ORDER BY  CLPT_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
