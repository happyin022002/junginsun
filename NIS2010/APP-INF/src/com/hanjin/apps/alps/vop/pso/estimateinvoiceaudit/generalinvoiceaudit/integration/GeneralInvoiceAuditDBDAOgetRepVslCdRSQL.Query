<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetRepVslCdRSQL">
			<desc><![CDATA[getRepVslCd : TBN1~ 형태의 항차의 대체 VSL_CD를 구한다.]]></desc>
			<sql><![CDATA[
SELECT decode(vsl_clss_flg, 'T', 
             (select min(vsl_cd) from mdm_vsl_cntr 
              where cntr_vsl_clss_capa = @[cntr_vsl_clss_capa]
             )
             ,vsl_cd)
FROM mdm_vsl_cntr
WHERE vsl_cd = SUBSTR(@[vvd], 1, 4)			]]></sql>
			<params>
				<param name="cntr_vsl_clss_capa" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
