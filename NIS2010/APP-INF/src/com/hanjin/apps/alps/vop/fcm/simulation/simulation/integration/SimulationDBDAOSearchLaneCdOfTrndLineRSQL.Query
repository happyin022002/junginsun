<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SimulationDBDAOSearchLaneCdOfTrndLineRSQL">
			<desc><![CDATA[Trnd Line이 생성되어 있는 Lane Code를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT DECODE(VSL_SLAN_CD,'A','ALL',VSL_SLAN_CD) VSL_SLAN_CD
FROM FCM_TRND_LINE
WHERE 1=1
#if (${vsl_clss_cd} != '') 
AND VSL_CLSS_CD=@[vsl_clss_cd]
#end
#if (${vsl_clss_sub_cd} != '') 
AND VSL_CLSS_SUB_CD=@[vsl_clss_sub_cd]
#end
AND VSL_SLAN_CD IS NOT NULL
AND TRND_LINE_USE_TP_CD='N'
ORDER BY VSL_SLAN_CD			]]></sql>
			<params>
				<param name="vsl_clss_cd" type="12" value="" out="N"/>
				<param name="vsl_clss_sub_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
