   SET PCK_INSTR_TP_CTNT = @[pck_instr_tp_ctnt]
      ,PCK_DIV_CD = @[pck_div_cd]
      ,PCK_DESC = @[pck_desc]
      ,PCK_DESC_USE_FLG = @[pck_desc_use_flg]
      ,PCK_REGU_USE_FLG = @[pck_regu_use_flg] 
      ,INR_PCK_USE_FLG = @[inr_pck_use_flg] 
      ,INTMD_PCK_USE_FLG = @[intmd_pck_use_flg]
      ,OUTR_PCK_USE_FLG = @[outr_pck_use_flg]
      ,OUTR_PCK_MAX_CAPA_FLG = @[outr_pck_max_capa_flg]
      ,SGL_PCK_USE_FLG = @[sgl_pck_use_flg]
      ,SGL_PCK_MAX_CAPA_FLG = @[sgl_pck_max_capa_flg]
      ,PRSS_DESC = @[prss_desc]
      ,PRSS_DESC_USE_FLG = @[prss_desc_use_flg]
      ,ADD_REGU_DESC = @[add_regu_desc]
      ,ADD_REGU_DESC_USE_FLG = @[add_regu_desc_use_flg]
      ,SPCL_PCK_REGU_USE_FLG = @[spcl_pck_regu_use_flg]
      ,GAS_USE_FLG = @[gas_use_flg]
      ,PCK_EXPT_FLG = @[pck_expt_flg]
      ,DELT_FLG = @[delt_flg]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE
 WHERE IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
   AND IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]			]]></sql>
