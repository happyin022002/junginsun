<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VoyagePerformanceMgtDBDAOSearchCgoStowageCraneArrangementRSQL">
			<desc><![CDATA[Crane 조회]]></desc>
			<sql><![CDATA[
SELECT   COUNT(1) AS CGO_FROM_STWG_ARRANGEMENT
FROM     (
         --==========================================
          SELECT   DISTINCT        
                   BC.CRANE_ID
          FROM     BAY_CRANE       BC 
          WHERE    BC.VSL_CD       = @[vsl_cd]
          AND      BC.VOY_NO       = @[skd_voy_no]
          AND      BC.DIR_CD       = @[skd_dir_cd]
          AND      BC.PORT_CD      = @[vps_port_cd]
          AND      BC.CALL_IND     = @[clpt_ind_seq]
          --AND      BC.JOB_TYPE     IN ('L','D')
         --==========================================     
         ) X
			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
