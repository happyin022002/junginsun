<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOSearchLastBafRateRSQL">
			<desc><![CDATA[vvd, yard,account 의 최종 baf rate가져오기]]></desc>
			<sql><![CDATA[
SELECT NVL(extractvalue(xmltype(nvl(max(X.INV_COND_DESC),'<o></o>')), '//o176'),0) AS bafrt
 FROM (
SELECT  T2.*--NVL(extractvalue(xmltype(nvl(max(T2.INV_COND_DESC),'<o></o>')), '//o176'),0) AS bafrt
 FROM PSO_CHARGE T1, PSO_CHG_DTL T2
 WHERE T1.ISS_CTY_CD  = T2.ISS_CTY_CD
   AND T1.SO_SEQ      = T2.SO_SEQ
   AND T1.VNDR_SEQ    = @[vndr_seq] 
   AND T1.YD_CD       = @[yd_cd] 
   AND T2.LGS_COST_CD = @[lgs_cost_cd]
ORDER BY T2.ISS_CTY_CD, T2.SO_SEQ DESC, SO_DTL_SEQ DESC ) X
WHERE ROWNUM =1			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
