<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProformaScheduleMgtDBDAOSearchPfSkdNewestRSQL">
			<desc><![CDATA[SearchPfSkdNewest]]></desc>
			<sql><![CDATA[
SELECT 	
    VSL_SLAN_CD
,	PF_SVC_TP_CD
,	SLAN_STND_FLG
,	SVC_DUR_DYS
,	STND_SVC_SPD
,	BRTH_ITVAL_DYS
,	MML_USD_FLG
,	SIM_DT
,	SIM_NO
,	N1ST_VSL_CLSS_CD
,	N1ST_VSL_CLSS_KNT
,	N2ND_VSL_CLSS_CD
,	N2ND_VSL_CLSS_KNT
,	N3RD_VSL_CLSS_CD
,	N3RD_VSL_CLSS_KNT
,	CLPT_KNT
,	TTL_DIST
,	MAX_SPD
,	AVG_SPD
,	DELT_FLG
,	PF_SKD_RMK
,   UPD_DT
,   CRE_DT
,   CRE_USR_ID
,   UPD_USR_ID
FROM(
SELECT 
	VSL_SLAN_CD
,	PF_SVC_TP_CD
,	SLAN_STND_FLG
,	SVC_DUR_DYS
,	STND_SVC_SPD
,	BRTH_ITVAL_DYS
,	MML_USD_FLG
,	SIM_DT
,	SIM_NO
,	N1ST_VSL_CLSS_CD
,	N1ST_VSL_CLSS_KNT
,	N2ND_VSL_CLSS_CD
,	N2ND_VSL_CLSS_KNT
,	N3RD_VSL_CLSS_CD
,	N3RD_VSL_CLSS_KNT
,	CLPT_KNT
,	TTL_DIST
,	MAX_SPD
,	AVG_SPD
,	DELT_FLG
,	PF_SKD_RMK
,   UPD_DT
,   CRE_DT
,   CRE_USR_ID
,   UPD_USR_ID
FROM  VSK_PF_SKD
WHERE VSL_SLAN_CD = @[vsl_slan_cd]
AND DELT_FLG='N'
ORDER BY UPD_DT DESC
)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
