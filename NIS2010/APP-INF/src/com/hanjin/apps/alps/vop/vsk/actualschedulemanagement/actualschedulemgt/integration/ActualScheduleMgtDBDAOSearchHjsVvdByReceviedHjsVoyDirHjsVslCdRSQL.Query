<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualScheduleMgtDBDAOSearchHjsVvdByReceviedHjsVoyDirHjsVslCdRSQL">
			<desc><![CDATA[EDI로 수신된 VVD가 SML Vessel Schedule과 맵핑되는 VVD를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  VSL_CD, SKD_VOY_NO, SKD_DIR_CD
FROM    VSK_VSL_PORT_SKD
WHERE   VSL_CD      = @[edi_vsl_nm]
AND     SKD_VOY_NO  = @[edi_skd_voy_no]
AND     SKD_DIR_CD  = @[edi_skd_dir_nm]
AND     YD_CD       = @[yd_cd]
AND     'S'        != NVL(SKD_CNG_STS_CD, ' ')
GROUP BY VSL_CD, SKD_VOY_NO, SKD_DIR_CD			]]></sql>
			<params>
				<param name="edi_vsl_nm" type="12" value="" out="N"/>
				<param name="edi_skd_voy_no" type="12" value="" out="N"/>
				<param name="edi_skd_dir_nm" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
