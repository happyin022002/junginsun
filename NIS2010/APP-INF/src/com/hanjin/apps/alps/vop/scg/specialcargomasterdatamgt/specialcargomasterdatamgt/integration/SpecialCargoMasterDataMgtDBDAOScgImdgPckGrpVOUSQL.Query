<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgPckGrpVOUSQL">
			<desc><![CDATA[Packing Group (Creation) 수정]]></desc>
			<sql><![CDATA[
UPDATE SCG_IMDG_PCK_GRP SET 
	PCK_DGR_NM = @[pck_dgr_nm]
,	FLSH_PNT_TEMP_CTNT = @[flsh_pnt_temp_ctnt]
,	INIT_BOIL_PNT_TEMP_CTNT = @[init_boil_pnt_temp_ctnt]
,	DRP_HGT_CTNT = @[drp_hgt_ctnt]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	IMDG_PCK_GRP_CD = @[imdg_pck_grp_cd]
			]]></sql>
			<params>
				<param name="pck_dgr_nm" type="12" value="" out="N"/>
				<param name="flsh_pnt_temp_ctnt" type="12" value="" out="N"/>
				<param name="init_boil_pnt_temp_ctnt" type="12" value="" out="N"/>
				<param name="drp_hgt_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="imdg_pck_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
