<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OptimizeddistancemgtDBDAORemoveOptimizedDistanceDSQL">
			<desc><![CDATA[선택한 Optimized Distance Data 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM VSK_PORT_OPMZ_DIST
 WHERE 1=1
   AND FM_YD_CD = @[sheet_fm_port_cd]	
   AND FM_YD_GRP_ID = @[sheet_fm_yd_grp_cd]	
   AND TO_YD_CD = @[sheet_to_port_cd]
   AND TO_YD_GRP_ID = @[sheet_to_yd_grp_cd]			]]></sql>
			<params>
				<param name="sheet_fm_port_cd" type="12" value="" out="N"/>
				<param name="sheet_fm_yd_grp_cd" type="12" value="" out="N"/>
				<param name="sheet_to_port_cd" type="12" value="" out="N"/>
				<param name="sheet_to_yd_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
