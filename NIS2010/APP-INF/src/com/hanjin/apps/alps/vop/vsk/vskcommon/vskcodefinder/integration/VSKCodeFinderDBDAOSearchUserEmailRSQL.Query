<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VSKCodeFinderDBDAOSearchUserEmailRSQL">
			<desc><![CDATA[사용자 Email Address 추출]]></desc>
			<sql><![CDATA[
SELECT   	X.USR_EML
FROM     	COM_USER     	X
WHERE		X.USR_ID		= @[usr_id]
			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
