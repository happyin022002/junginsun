<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleMasterDataDBDAOModifyVskUsrLaneGrpUSQL">
			<desc><![CDATA[사용자별 Lane Group 정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE VSK_USR_LANE_GRP SET 
	LANE_GRP_NM = @[lane_grp_nm]
,	VSL_SLAN_CD = @[vsl_slan_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	1=1
AND LANE_GRP_NM = @[src_lane_grp_nm]
AND VSL_SLAN_CD = @[src_vsl_slan_cd]			]]></sql>
			<params>
				<param name="lane_grp_nm" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="src_lane_grp_nm" type="12" value="" out="N"/>
				<param name="src_vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
