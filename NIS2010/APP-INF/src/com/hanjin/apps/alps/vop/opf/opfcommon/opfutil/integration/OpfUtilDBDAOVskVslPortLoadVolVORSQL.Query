<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OpfUtilDBDAOVskVslPortLoadVolVORSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT	DECODE(A.SKD_CNG_STS_CD, 'S', NULL, A.VPS_PORT_CD) AS VPS_PORT_CD,
        DECODE(A.SKD_CNG_STS_CD, 'S', NULL, B.LOC_NM) AS LOC_NM,
        A.CLPT_SEQ
FROM	(
            SELECT	A.VPS_PORT_CD, A.SKD_VOY_NO, A.CLPT_SEQ, A.SKD_CNG_STS_CD
            FROM	VSK_VSL_PORT_SKD A
            WHERE	A.VSL_CD		=	@[vsl_cd]
            AND     A.SKD_VOY_NO    =   @[skd_voy_no]
            AND     A.SKD_DIR_CD    =   @[skd_dir_cd]
            AND     A.CLPT_SEQ      >   (	SELECT	CLPT_SEQ
											FROM	VSK_VSL_PORT_SKD
											WHERE	VSL_CD		=	@[vsl_cd]
											AND		SKD_VOY_NO 	=	@[skd_voy_no]
											AND		SKD_DIR_CD	=	@[skd_dir_cd]
											AND		YD_CD		=	@[yd_cd]
                                            AND     CLPT_IND_SEQ = @[clpt_ind_seq]
										)	
        )	A				,
        MDM_LOCATION	B
WHERE	A.VPS_PORT_CD		=	B.LOC_CD
#if (${conti_cd} == '1')
AND		B.CONTI_CD			<>	(	SELECT	CONTI_CD
                                    FROM	MDM_LOCATION
                                    WHERE	LOC_CD		=	@[port_cd]
                                )
#elseif (${conti_cd} == '2')
AND		B.CONTI_CD			= 	(	SELECT	CONTI_CD
                                    FROM	MDM_LOCATION
                                    WHERE	LOC_CD		=	@[port_cd]
                                )
#end
ORDER BY A.CLPT_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
