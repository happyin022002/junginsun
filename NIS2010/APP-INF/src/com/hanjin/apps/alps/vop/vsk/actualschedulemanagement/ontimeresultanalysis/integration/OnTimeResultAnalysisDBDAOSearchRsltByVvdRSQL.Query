<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnTimeResultAnalysisDBDAOSearchRsltByVvdRSQL">
			<desc><![CDATA[VVD에 대해서 VSK_VSL_SKD_RSLT 테이블을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
	T2.VSL_SLAN_CD
,	T1.VSL_CD
,	T1.SKD_VOY_NO
,	T1.SUB_TRD_DIR_CD
,	T1.VPS_PORT_CD
,	T1.CLPT_IND_SEQ
,	T1.CLPT_SEQ
,	T1.SKD_DIR_CD
,	T1.ACT_INP_YRMON
,	T1.SKD_CNG_STS_CD
,	TO_CHAR(T1.PF_ETB_DT, 'YYYY-MM-DD HH24:MI') PF_ETB_DT
,	TO_CHAR(T1.PF_ETD_DT, 'YYYY-MM-DD HH24:MI') PF_ETD_DT
,	TO_CHAR(T1.ACT_BRTH_DT, 'YYYY-MM-DD HH24:MI') ACT_BRTH_DT
,	TO_CHAR(T1.ACT_DEP_DT, 'YYYY-MM-DD HH24:MI') ACT_DEP_DT
,	T1.ARR_DLAY_HRS1
,	T1.ARR_DLAY_HRS2
,	T1.ARR_DLAY_RSN_CD1
,	T1.ARR_DLAY_RSN_CD2
,	T1.ARR_RMK1
,	T1.ARR_RMK2
,	T1.DEP_DLAY_HRS1
,	T1.DEP_DLAY_HRS2
,	T1.DEP_DLAY_RSN_CD1
,	T1.DEP_DLAY_RSN_CD2
,	T1.DEP_RMK1
,	T1.DEP_RMK2
,	T1.INCL_BRTH_DLAY_HRS
,	T1.INCL_DEP_DLAY_HRS
,	T1.XCLD_BRTH_DLAY_HRS
,	T1.XCLD_DEP_DLAY_HRS
,	T1.VSKD_RSLT_XCLD_CD
FROM VSK_VSL_SKD_RSLT T1, VSK_VSL_SKD T2
WHERE 1=1
AND T1.VSL_CD = T2.VSL_CD
AND T1.SKD_VOY_NO = T2.SKD_VOY_NO
AND T1.SKD_DIR_CD = T2.SKD_DIR_CD
AND T1.VSL_CD = @[vsl_cd]
AND T1.SKD_VOY_NO = @[voy_no]
AND T1.SUB_TRD_DIR_CD = @[dir_cd]
ORDER BY T1.CLPT_SEQ			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
