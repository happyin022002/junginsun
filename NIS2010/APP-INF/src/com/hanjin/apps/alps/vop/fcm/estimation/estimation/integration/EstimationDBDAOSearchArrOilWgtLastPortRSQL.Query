<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EstimationDBDAOSearchArrOilWgtLastPortRSQL">
			<desc><![CDATA[Search a arrival oil weight at the last calling port.
==============================================]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN 'HFO'=@[csm_oil_tp_cd] THEN
           ARR_FOIL_WGT + ARR_LOW_SULP_FOIL_WGT
       WHEN 'MDO'=@[csm_oil_tp_cd] THEN
           ARR_DOIL_WGT + ARR_LOW_SULP_DOIL_WGT
       ELSE 0 END VOY_END_RMN_WGT
FROM (
    SELECT TO_NUMBER(NVL(TRIM(REPLACE(REPLACE(ARR_FOIL_WGT, CHR(13), ''), CHR(10), '')),0))           ARR_FOIL_WGT
          ,TO_NUMBER(NVL(TRIM(REPLACE(REPLACE(ARR_LOW_SULP_FOIL_WGT, CHR(13), ''), CHR(10), '')),0))  ARR_LOW_SULP_FOIL_WGT
          ,TO_NUMBER(NVL(TRIM(REPLACE(REPLACE(ARR_DOIL_WGT, CHR(13), ''), CHR(10), '')),0))           ARR_DOIL_WGT
          ,TO_NUMBER(NVL(TRIM(REPLACE(REPLACE(ARR_LOW_SULP_DOIL_WGT, CHR(13), ''), CHR(10), '')),0))  ARR_LOW_SULP_DOIL_WGT
    FROM FCM_DEP_RPT
    WHERE 1=1
    AND VSL_CD=@[vsl_cd]
    AND DEP_PORT_CD=@[end_port_cd]
    --AND CLPT_IND_SEQ='1'
    AND VPS_ETB_DT BETWEEN TO_DATE(REPLACE(SUBSTR(@[end_dt], 1, 10), '-', ''), 'YYYYMMDD')-1
                       AND TO_DATE(REPLACE(SUBSTR(@[end_dt], 1, 10), '-', ''), 'YYYYMMDD')+1.99999
)			]]></sql>
			<params>
				<param name="csm_oil_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="end_port_cd" type="12" value="" out="N"/>
				<param name="end_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
