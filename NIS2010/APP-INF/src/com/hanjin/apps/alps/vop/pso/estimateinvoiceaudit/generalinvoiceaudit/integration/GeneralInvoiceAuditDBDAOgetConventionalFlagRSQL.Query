<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOgetConventionalFlagRSQL">
			<desc><![CDATA[VSL 등록 국가 FLAG]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN CNT = 0 THEN '''N'''
            ELSE '''Y'''
            END AS FLG
 FROM (  SELECT COUNT(1) CNT
           FROM MDM_VSL_CNTR
          WHERE VSL_CD = SUBSTR( @[vvd], 1, 4)
            AND VSL_RGST_CNT_CD IN (
              'AL','AO','AG','AN','AU','AT','BS','BB',
              'BE','BZ','BM','GW','BW','BN','BG','BF',
              'BI','CM','CV','KY','TD','CZ','CN','KM',
              'CD','CK','HR','CY','DK','DJ','DM','DO',
              'EG','CQ','ER','EE','ET','FJ','FI','FK',
              'FR','GA','GE','DE','GH','GI','GD','GN',
              'HT','HK','HU','IS','IN','IR','IQ','IE',
              'IL','IT','CI','JM','JP','JO','KE','KI',
              'LV','LB','LS','LR','LY','LT','LU','MG',
              'MY','MW','ML','MT','IM','MH','MR','MX',
              'MC','MZ','NA','NR','NZ','NG','NO','PK',
              'PW','PG','PL','PT','RO','RW','RU','KN',
              'SH','LC','MF','VC','WS','SN','RS','SC',
              'SL','SK','SI','SB','SO','ZA','ES','LK',
              'SD','SR','SE','CH','TZ','TO','TT','TN',
              'TR','TC','TV','UG','UA','GB','US','VU',
              'VI','VG','ZM','ZW' ) )			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
