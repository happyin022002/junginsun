<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BudgetPortChargeMgtBCDBDAOdeleteTransitListYardDSQL">
			<desc><![CDATA[insert 하기 전에 해당건 삭제 먼저 처리]]></desc>
			<sql><![CDATA[
DELETE PSO_TGT_YD_SKD 
 WHERE PSO_BZTP_CD = @[bztp_cd]
   AND( VSL_CD , SKD_VOY_NO, SKD_DIR_CD ) IN ( SELECT  VSL_CD , SKD_VOY_NO , SKD_DIR_CD
                                                 FROM  PSO_TGT_VVD  
                                                WHERE EXPN_YRMON = REPLACE(@[tgt_date], '-', '')
                                                  AND PSO_BZTP_CD = @[bztp_cd] )


			]]></sql>
			<params>
				<param name="bztp_cd" type="12" value="" out="N"/>
				<param name="tgt_date" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
