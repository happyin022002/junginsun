<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalInformationMgtDBDAOVskPortGntrCrnSpclCgoHndlInfoAttachFileCSQL">
			<desc><![CDATA[G/Crane 작업시 Special Cargo Handling Information에 대한 첨부파일 저장]]></desc>
			<sql><![CDATA[
INSERT  INTO   VSK_PORT_GNTR_HNDL_FILE   X
               (  	X.LOC_CD
               	,  	X.ATCH_FILE_SEQ
               	,  	X.FILE_NM
               	,  	X.FILE_SAV_ID
               	,  	X.CRE_USR_ID
               	,  	X.CRE_DT
               	,  	X.UPD_USR_ID
               	,  	X.UPD_DT  
               )
VALUES         (
					@[port_cd]			--//X.LOC_CD       //--
                ,	(SELECT NVL(MAX(XX.ATCH_FILE_SEQ)+1,1) FROM VSK_PORT_GNTR_HNDL_FILE XX WHERE XX.LOC_CD = @[port_cd])	--//X.ATCH_FILE_SEQ//--
                ,	@[file_nm]			--//X.FILE_NM      //--
                ,	@[file_sav_id]		--//X.FILE_SAV_ID  //--
                ,	@[upd_usr_id]		--//X.CRE_USR_ID   //--
                ,	SYSDATE				--//X.CRE_DT       //--
                ,	@[upd_usr_id]		--//X.UPD_USR_ID   //--
                ,	SYSDATE				--//X.UPD_DT       //--
				)   

			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
