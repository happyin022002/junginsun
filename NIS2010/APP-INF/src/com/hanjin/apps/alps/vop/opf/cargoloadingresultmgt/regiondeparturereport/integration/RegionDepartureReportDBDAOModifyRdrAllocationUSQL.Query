<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RegionDepartureReportDBDAOModifyRdrAllocationUSQL">
			<desc><![CDATA[RDR ALLOCATION 수정.

Ticket ID : CHM-201007765-01
개발자 : 박희동(2010-12-20)
수정내용 : UPD_SYS_CD 컬럼추가...N으로 setting]]></desc>
			<sql><![CDATA[
UPDATE  RDR_ALLOCATION
   SET  BSA_TYPE     = @[bsa_type]    

,       BSA_SLOT     = @[basic_slot]     
,       SWAP_SLOT    = @[swap_slot]   
,       RELEASE_SLOT = @[release_slot]

,       BSA_WGT      = @[basic_wgt]     
,       SWAP_WGT     = @[swap_wgt]    
,       RELEASE_WGT  = @[release_wgt] 

,       UPDATE_TIME  = SYSDATE
,       UPDATE_USER  = @[update_user]
,       UPD_SYS_CD   = 'N'
WHERE  VSL_CD    = @[vsl_cd] 
  AND  VOY_NO    = @[voy_no] 
  AND  DIR_CD    = @[dir_cd] 
  AND  REGION    = @[region] 
  AND  OPR_CD    = @[opr_cd] 
  AND  SEGMENT   = @[segment]			]]></sql>
			<params>
				<param name="bsa_type" type="12" value="" out="N"/>
				<param name="basic_slot" type="12" value="" out="N"/>
				<param name="swap_slot" type="12" value="" out="N"/>
				<param name="release_slot" type="12" value="" out="N"/>
				<param name="basic_wgt" type="12" value="" out="N"/>
				<param name="swap_wgt" type="12" value="" out="N"/>
				<param name="release_wgt" type="12" value="" out="N"/>
				<param name="update_user" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="voy_no" type="12" value="" out="N"/>
				<param name="dir_cd" type="12" value="" out="N"/>
				<param name="region" type="12" value="" out="N"/>
				<param name="opr_cd" type="12" value="" out="N"/>
				<param name="segment" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
