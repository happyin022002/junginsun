<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SetupDBDAOFcmDepRptErrRtRSQL">
			<desc><![CDATA[ERROR RATE SETTING]]></desc>
			<sql><![CDATA[
WITH VSL_INFO AS (
SELECT VSL_CD, VSL_ENG_NM , CNTR_VSL_CLSS_CAPA, MN_ENG_MKR_NM, 
       DECODE(MN_ENG_HOR_PWR_UT_CD,'KW',MN_ENG_BHP_PWR,ROUND(MN_ENG_BHP_PWR*1.35941,2)) AS MN_ENG_BHP_PWR,
       DECODE(MN_ENG_HOR_PWR_UT_CD,'KW',ROUND(MN_ENG_BHP_PWR*0.73561,2), MN_ENG_BHP_PWR) AS MN_ENG_RPM_PWR,
       MN_ENG_TP_DESC, TBCGR_RPM_PWR
FROM 
( SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_VSL_CLSS_CAPA
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_BHP_PWR
    ,T1.MN_ENG_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.MN_ENG_MKR_NM
    ,T1.MN_ENG_BHP_PWR
    ,T2.MN_ENG_TP_DESC
    ,T1.MN_ENG_RPM_PWR AS TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD     = T2.VSL_CD(+)
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG   = 'N'
    AND T1.FDR_DIV_CD = 'T'
    AND T1.CRR_CD     = 'SML'
)T1
, (
    SELECT
    T1.VSL_CD
    ,NVL((
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_VENDOR B
    WHERE  A.OWNR_SEQ = B.FLET_MGMT_OWNR_VNDR_SEQ
    AND T1.VNDR_SEQ=B.VNDR_SEQ(+)), (
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_CUSTOMER B
    WHERE  A.OWNR_SEQ  = B.FLET_MGMT_OWNR_CUST_SEQ
    AND T1.CUST_CNT_CD = B.CUST_CNT_CD(+)
    AND T1.CUST_SEQ    = B.CUST_SEQ(+)
    )) OWNR_NM
    ,TO_CHAR(TO_DATE(VSL_BLD_DT, 'YYYYMMDD'), 'YYYY-MM-DD') VSL_BLD_DT
    ,VSL_DZND_CAPA
    ,BSE_14TON_VSL_CAPA
    ,TO_CHAR(EFF_DT, 'YYYY-MM-DD') FM_DT
    ,TO_CHAR(EXP_DT, 'YYYY-MM-DD') TO_DT
    FROM (
        SELECT
        ROW_NUMBER() OVER(PARTITION BY VSL_CD ORDER BY EXP_DT DESC) SEQ
        ,T1.*
        FROM FMS_CONTRACT T1
        WHERE 1=1
        AND T1.FLET_CTRT_TP_CD!='TO'
        AND T1.EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD')
    ) T1
    WHERE T1.SEQ=1
)T2
WHERE 1=1
AND T1.VSL_CD=T2.VSL_CD

UNION ALL
SELECT T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_VSL_CLSS_CAPA
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_BHP_PWR
    ,T1.MN_ENG_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.MN_ENG_MKR_NM
    ,T2.MN_ENG_TP_DESC
    ,T1.MN_ENG_BHP_PWR
    ,T1.MN_ENG_RPM_PWR AS TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD=T2.VSL_CD
    AND T2.VSL_CD NOT IN (SELECT VSL_CD FROM FMS_CONTRACT WHERE FLET_CTRT_TP_CD!='TO' AND EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD'))
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG='N'
    AND T1.FDR_DIV_CD='T'
    AND T1.CRR_CD='SML'
)T1
WHERE 1=1
UNION ALL
SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_VSL_CLSS_CAPA   
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_BHP_PWR
    ,T1.MN_ENG_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.MN_ENG_MKR_NM
    ,T1.MN_ENG_BHP_PWR
    ,T2.MN_ENG_TP_DESC
    ,T1.MN_ENG_RPM_PWR AS TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD=T2.VSL_CD --NOT OUTER JOIN
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG='N'
    AND T1.FDR_DIV_CD='T'
    AND T1.CRR_CD<>'SML'
)T1
, (
    SELECT
    T1.VSL_CD
    ,NVL((
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_VENDOR B
    WHERE  A.OWNR_SEQ = B.FLET_MGMT_OWNR_VNDR_SEQ
    AND T1.VNDR_SEQ=B.VNDR_SEQ(+)), (
    SELECT A.OWNR_NM
    FROM   FMS_OWNER A
         , MDM_CUSTOMER B
    WHERE  A.OWNR_SEQ = B.FLET_MGMT_OWNR_CUST_SEQ
    AND T1.CUST_CNT_CD = B.CUST_CNT_CD(+)
    AND T1.CUST_SEQ = B.CUST_SEQ(+)
    )) OWNR_NM
    ,TO_CHAR(TO_DATE(VSL_BLD_DT, 'YYYYMMDD'), 'YYYY-MM-DD') VSL_BLD_DT
    ,VSL_DZND_CAPA
    ,BSE_14TON_VSL_CAPA
    ,TO_CHAR(EFF_DT, 'YYYY-MM-DD') FM_DT
    ,TO_CHAR(EXP_DT, 'YYYY-MM-DD') TO_DT
    FROM (
        SELECT
        ROW_NUMBER() OVER(PARTITION BY VSL_CD ORDER BY EXP_DT DESC) SEQ
        ,T1.*
        FROM FMS_CONTRACT T1
        WHERE 1=1
        AND T1.FLET_CTRT_TP_CD!='TO'
        AND T1.EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD')
    ) T1
    WHERE T1.SEQ=1
)T2
WHERE 1=1
AND T1.VSL_CD=T2.VSL_CD

UNION ALL
SELECT T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_VSL_CLSS_CAPA
    ,T1.MN_ENG_MKR_NM
    ,T1.MN_ENG_BHP_PWR
    ,T1.MN_ENG_TP_DESC
    ,T1.TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
FROM (
    SELECT
     T1.VSL_CD
    ,T1.VSL_ENG_NM
    ,T1.CNTR_DZN_CAPA CNTR_VSL_CLSS_CAPA
    ,T2.MN_ENG_MKR_NM
    ,T1.MN_ENG_BHP_PWR
    ,T2.MN_ENG_TP_DESC
    ,T1.MN_ENG_RPM_PWR AS TBCGR_RPM_PWR
    ,T1.MN_ENG_HOR_PWR_UT_CD
    FROM MDM_VSL_CNTR T1, FCM_VSL_CNTR_RGST T2
    WHERE 1=1
    AND T1.VSL_CD=T2.VSL_CD
    AND T2.VSL_CD NOT IN (SELECT VSL_CD FROM FMS_CONTRACT WHERE FLET_CTRT_TP_CD!='TO' AND EXP_DT > TO_DATE('2011-08-01', 'YYYY-MM-DD'))
    AND NVL(T1.VSL_CLSS_FLG, ' ')='N'
    AND T1.DELT_FLG   ='N'
    AND T1.FDR_DIV_CD ='T'
    AND T1.CRR_CD<>'SML'
    ORDER BY T1.VSL_CD
)T1
WHERE 1=1 )  ) 
SELECT A.VSL_CD,             A.VSL_ENG_NM,    A.CNTR_VSL_CLSS_CAPA,    A.MN_ENG_MKR_NM,            A.MN_ENG_RPM_PWR,          A.MN_ENG_BHP_PWR, A.MN_ENG_TP_DESC,
       B.NVGT_ML_DIST_RT,    B.ENG_ML_DIST_RT,B.MNVR_IN_ML_DIST_RT,    B.MNVR_OUT_ML_DIST_RT,      B.AVG_SPD_RT,              B.AVG_RPM_PWR_RT,
       B.PRLR_PCH_RT,        B.SAIL_TM_RT,    B.ARR_FOIL_RT,           B.ARR_LOW_SULP_FOIL_RT,     B.ARR_DOIL_RT,             B.ARR_LOW_SULP_DOIL_RT,
       B.DEP_FOIL_RT,        B.DEP_LOW_SULP_FOIL_RT, B.DEP_DOIL_RT,    B.DEP_LOW_SULP_DOIL_RT,     B.SEA_STMNG_MN_ENG_RT,     B.PORT_TTL_RT, 
       B.PORT_TTL_HR_RT_RT,  B.SB_ENG_DT_RT,  B.PLT_IN_DT_RT,          B.BFR_BRTH_ANK_DRP_DT_RT,   B.BFR_BRTH_ANK_OFF_DT_RT,  B.VPS_ETB_DT_RT,                 
       B.CGO_WRK_ST_DT_RT,   B.CGO_WRK_END_DT_RT, B.VPS_ETD_DT_RT,     B.AFT_UNBRTH_ANK_DRP_DT_RT, B.AFT_UNBRTH_ANK_OFF_DT_RT,B.PLT_OUT_DT_RT,
       B.RUP_DT_RT,          B.FCNTR_OBRD_TEU_RT, B.MCNTR_OBRD_TEU_RT, B.TTL_CNTR_OBRD_TEU_RT,B.DEP_CGO_RT, B.RF_CNTR_DCHG_KNT_RT, A.TBCGR_RPM_PWR,
       B.RF_CNTR_LOD_KNT_RT, B.RF_CNTR_OBRD_KNT_RT
 FROM VSL_INFO A, FCM_DEP_RPT_ERR_RT_SET B
WHERE A.VSL_CD = B.VSL_CD(+)
#if (${vsl_cd} != '')
  AND A.VSL_CD = @[vsl_cd]
#end
ORDER BY A.CNTR_VSL_CLSS_CAPA DESC, A.VSL_CD			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
