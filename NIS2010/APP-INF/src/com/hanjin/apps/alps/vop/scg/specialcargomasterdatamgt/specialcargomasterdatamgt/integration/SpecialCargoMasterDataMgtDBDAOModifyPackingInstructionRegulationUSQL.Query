<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOModifyPackingInstructionRegulationUSQL">
			<desc><![CDATA[ModifyPackingInstructionRegulation]]></desc>
			<sql><![CDATA[
UPDATE SCG_PCK_REGU
   SET REGU_MN_DESC = @[regu_mn_desc]
      ,DELT_FLG       = @[delt_flg]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE
 WHERE IMDG_PCK_INSTR_CD = @[imdg_pck_instr_cd]
   AND IMDG_PCK_INSTR_SEQ = @[imdg_pck_instr_seq]  
   AND REGU_DP_NO = @[regu_dp_no]			]]></sql>
			<params>
				<param name="regu_mn_desc" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_cd" type="12" value="" out="N"/>
				<param name="imdg_pck_instr_seq" type="12" value="" out="N"/>
				<param name="regu_dp_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
