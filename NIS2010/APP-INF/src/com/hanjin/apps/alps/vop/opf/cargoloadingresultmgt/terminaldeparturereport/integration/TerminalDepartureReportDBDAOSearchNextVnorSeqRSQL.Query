<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalDepartureReportDBDAOSearchNextVnorSeqRSQL">
			<desc><![CDATA[Next VNOR_SEQ 조회한다.]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(VNOR_SEQ), 0)+1 AS NEXT_VNOR_SEQ
FROM OPF_VNOR 
WHERE VSL_CD = @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
