<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="StevedoreDamageMgtDBDAOStvDmgNoRSQL">
			<desc><![CDATA[Stv_Dmg_No Select Query.]]></desc>
			<sql><![CDATA[
SELECT 
	lpad(NVL(MAX(SUBSTR(STV_DMG_NO,9,3)),0)+1,3,'0') AS STV_DMG_SEQ 
FROM OPF_STV_DMG
WHERE	STV_DMG_NO LIKE @[vsl_cd]||'%'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
