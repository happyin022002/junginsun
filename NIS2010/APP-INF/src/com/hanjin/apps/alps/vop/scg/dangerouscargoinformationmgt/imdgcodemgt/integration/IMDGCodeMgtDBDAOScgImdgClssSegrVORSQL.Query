,   MAX(CLSS_CD_62) AS CLSS_CD_62
,   MAX(CLSS_CD_7) AS CLSS_CD_7
,   MAX(CLSS_CD_8) AS CLSS_CD_8
,   MAX(CLSS_CD_9) AS CLSS_CD_9
FROM (
