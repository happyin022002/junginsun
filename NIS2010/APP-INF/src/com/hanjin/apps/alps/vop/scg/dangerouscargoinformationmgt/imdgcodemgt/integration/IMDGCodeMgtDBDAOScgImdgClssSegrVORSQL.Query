    ROW_IMDG_CLSS_CD
,   DECODE(COL_IMDG_CLSS_CD,'1.1',IMDG_SEGR_CD) AS CLSS_CD_11
,   DECODE(COL_IMDG_CLSS_CD,'1.2',IMDG_SEGR_CD) AS CLSS_CD_12
,   DECODE(COL_IMDG_CLSS_CD,'1.5',IMDG_SEGR_CD) AS CLSS_CD_15
,   DECODE(COL_IMDG_CLSS_CD,'1.3',IMDG_SEGR_CD) AS CLSS_CD_13
,   DECODE(COL_IMDG_CLSS_CD,'1.6',IMDG_SEGR_CD) AS CLSS_CD_16
,   DECODE(COL_IMDG_CLSS_CD,'1.4',IMDG_SEGR_CD) AS CLSS_CD_14
,   DECODE(COL_IMDG_CLSS_CD,'2.1',IMDG_SEGR_CD) AS CLSS_CD_21
,   DECODE(COL_IMDG_CLSS_CD,'2.2',IMDG_SEGR_CD) AS CLSS_CD_22
,   DECODE(COL_IMDG_CLSS_CD,'2.3',IMDG_SEGR_CD) AS CLSS_CD_23
,   DECODE(COL_IMDG_CLSS_CD,'3',IMDG_SEGR_CD) AS CLSS_CD_3
,   DECODE(COL_IMDG_CLSS_CD,'4.1',IMDG_SEGR_CD) AS CLSS_CD_41
,   DECODE(COL_IMDG_CLSS_CD,'4.2',IMDG_SEGR_CD) AS CLSS_CD_42
,   DECODE(COL_IMDG_CLSS_CD,'4.3',IMDG_SEGR_CD) AS CLSS_CD_43
,   DECODE(COL_IMDG_CLSS_CD,'5.1',IMDG_SEGR_CD) AS CLSS_CD_51
,   DECODE(COL_IMDG_CLSS_CD,'5.2',IMDG_SEGR_CD) AS CLSS_CD_52
,   DECODE(COL_IMDG_CLSS_CD,'6.1',IMDG_SEGR_CD) AS CLSS_CD_61
,   DECODE(COL_IMDG_CLSS_CD,'6.2',IMDG_SEGR_CD) AS CLSS_CD_62
,   DECODE(COL_IMDG_CLSS_CD,'7',IMDG_SEGR_CD) AS CLSS_CD_7
,   DECODE(COL_IMDG_CLSS_CD,'8',IMDG_SEGR_CD) AS CLSS_CD_8
,   DECODE(COL_IMDG_CLSS_CD,'9',IMDG_SEGR_CD) AS CLSS_CD_9
,   DECODE(ROW_IMDG_CLSS_CD,'1.1',1,'1.2',2,'1.5',3,'1.3',4,'1.6',5,'1.4',6,'2.1',7,'2.2',8,'2.3',9,'3',10,'4.1',11,'4.2',12,'4.3',13,'5.1',14,'5.2',15,'6.1',16,'6.2',17,'7',18,'8',19,'9',20,21) AS  SORT
FROM SCG_IMDG_CLSS_SEGR
WHERE ROW_IMDG_CLSS_CD IN  ('1.1','1.2','1.5','1.3','1.6','1.4','2.1','2.2','2.3','3','4.1','4.2','4.3','5.1','5.2','6.1','6.2','7','8','9')
)
GROUP BY ROW_IMDG_CLSS_CD
ORDER BY SORT
			]]></sql>
