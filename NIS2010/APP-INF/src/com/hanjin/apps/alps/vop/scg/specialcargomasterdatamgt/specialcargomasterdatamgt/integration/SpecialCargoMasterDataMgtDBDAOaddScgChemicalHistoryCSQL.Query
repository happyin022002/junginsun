<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOaddScgChemicalHistoryCSQL">
			<desc><![CDATA[chemical history저장]]></desc>
			<sql><![CDATA[
INSERT INTO SCG_CHEM_HIS (
       chem_seq
      ,chem_nm
      ,chem_abst_svc_no
      ,spcl_cgo_flg
      ,co_nm
      ,rqst_ofc_cd
      ,rqst_id
      ,rqst_dt
      ,cre_usr_id
      ,cre_dt
      ,upd_usr_id
      ,upd_dt)
 SELECT NVL(MAX(chem_seq) + 1,1)
      ,@[chem_nm]
      ,@[chem_abst_svc_no1]||@[chem_abst_svc_no2]||@[chem_abst_svc_no3]
      ,@[spcl_cgo_flg]
      ,@[co_nm]
      ,@[rqst_ofc_cd]
      ,@[rqst_id]
      ,SYSDATE
      ,@[cre_usr_id]
      ,sysdate
      ,@[upd_usr_id]
      ,sysdate
FROM SCG_CHEM_HIS			]]></sql>
			<params>
				<param name="chem_nm" type="12" value="" out="N"/>
				<param name="chem_abst_svc_no1" type="12" value="" out="N"/>
				<param name="chem_abst_svc_no2" type="12" value="" out="N"/>
				<param name="chem_abst_svc_no3" type="12" value="" out="N"/>
				<param name="spcl_cgo_flg" type="12" value="" out="N"/>
				<param name="co_nm" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="rqst_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
