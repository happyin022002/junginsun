<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselReportDBDAOModifyFcmNoonRptLogPostUSQL">
			<desc><![CDATA[Create noon report log. Update result of creation.]]></desc>
			<sql><![CDATA[
UPDATE FCM_NOON_RPT_LOG
SET    IF_FLG     =@[if_flg]
      ,EAI_IF_RMK =@[eai_if_rmk]
WHERE RCV_DT      =@[rcv_dt]
AND   RCV_SEQ     =@[rcv_seq]			]]></sql>
			<params>
				<param name="if_flg" type="12" value="" out="N"/>
				<param name="eai_if_rmk" type="12" value="" out="N"/>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="rcv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
