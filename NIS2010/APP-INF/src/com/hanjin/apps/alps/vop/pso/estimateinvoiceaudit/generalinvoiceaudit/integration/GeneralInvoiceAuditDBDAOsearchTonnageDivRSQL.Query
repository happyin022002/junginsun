<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralInvoiceAuditDBDAOsearchTonnageDivRSQL">
			<desc><![CDATA[CN,IT,JP의 TONNAGE비용을 해당 VVD별로 배분하기 위함.]]></desc>
			<sql><![CDATA[
SELECT DECODE(CNT,0,'N','Y') FLAG
FROM (
SELECT COUNT(1) CNT 
  FROM PSO_CHARGE A, PSO_CHG_DTL B
 WHERE A.ISS_CTY_CD    = B.ISS_CTY_CD
   AND A.SO_SEQ        = B.SO_SEQ
   AND A.INV_NO        = @[inv_no] 
   AND A.VNDR_SEQ      = @[vndr_seq] 
   AND SUBSTR(A.YD_CD ,1,2 ) IN ('CN','JP','IT') 
   AND SUBSTR(A.YD_CD ,1,5 ) <>   'CNHKG'
   AND B.LGS_COST_CD    = 'PTDUTN' 
   AND B.COST_CALC_EFF_FM_DT IS NOT NULL
   AND B.COST_CALC_EFF_TO_DT IS NOT NULL )			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
