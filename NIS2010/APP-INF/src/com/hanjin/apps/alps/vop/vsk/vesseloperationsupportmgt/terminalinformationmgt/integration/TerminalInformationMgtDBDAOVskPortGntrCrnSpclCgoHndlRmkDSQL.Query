<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalInformationMgtDBDAOVskPortGntrCrnSpclCgoHndlRmkDSQL">
			<desc><![CDATA[G/Crane에 대한 Special Cargo Handling Remark 데이터를 삭제처리한다.]]></desc>
			<sql><![CDATA[
DELETE
FROM        VSK_PORT_GNTR_HNDL_INFO  X
WHERE       X.LOC_CD                 = substr(@[yd_cd],1,5)			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
