<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselPassagePlanManagementDBDAOCreatePassagePlanDtRSQL">
			<desc><![CDATA[VSM에서 EAI로 I/F받은 Passage Plan 데이터를 Insert.]]></desc>
			<sql><![CDATA[
--SELECT 
--     '' AS VSL_CD
--    ,'' AS SKD_VOY_NO
--    ,'' AS PASG_PLN_DT
--    ,'' AS DEP_PORT_CD
--    ,'' AS ARR_PORT_CD
--    ,'' AS PASG_PLN_TIT_NM
--    ,'' AS VSL_SLAN_CD
--    ,'' AS AVG_VSL_SPD
--    ,'' AS DEP_PLN_DT
--    ,'' AS ARR_PLN_DT
--    ,'' AS ARR_DEP_PLC_DIFF_HRS
--    ,'' AS VSL_APRO_DOC_NO
--    ,'' AS TTL_MLG_DIST
--    ,'' AS PORT_TO_PORT_MLG_DIST
--    ,'' AS SELF_SHP_FLG
--    ,'' AS CRE_USR_ID
--    ,'' AS CRE_DT
--    ,'' AS UPD_USR_ID
--    ,'' AS UPD_DT
--    ,'' AS SKD_DIR_CD
--    ,'' AS PLT_STN_DESC
--FROM DUAL

INSERT INTO VSK_PASG_PLN_RPT(
          VSL_CD
        , SKD_VOY_NO
        , SKD_DIR_CD
        , PASG_PLN_DT
        , DEP_PORT_CD
        , ARR_PORT_CD
        , PASG_PLN_TIT_NM
        , VSL_SLAN_CD
        , PLT_STN_DESC
        , AVG_VSL_SPD
        , DEP_PLN_DT
        , ARR_PLN_DT
        , ARR_DEP_PLC_DIFF_HRS
        , VSL_APRO_DOC_NO
        , TTL_MLG_DIST
        , PORT_TO_PORT_MLG_DIST
        , SELF_SHP_FLG
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT
      )
VALUES(
          @[vsl_cd]
        , @[skd_voy_no]
        , @[skd_dir_cd]
        , TO_DATE(@[pasg_pln_dt], 'YYYY-MM-DD HH24:MI:SS')
        , @[dep_port_cd]
        , @[arr_port_cd] 
        , @[pasg_pln_tit_nm]
        , @[vsl_slan_cd]
        , @[plt_stn_desc]
        , @[avg_vsl_spd]
        , TO_DATE(@[dep_pln_dt], 'YYYY-MM-DD HH24:MI:SS')
        , TO_DATE(@[arr_pln_dt], 'YYYY-MM-DD HH24:MI:SS')
        , @[arr_dep_plc_diff_hrs]
        , @[vsl_apro_doc_no]
        , @[ttl_mlg_dist]
        , @[port_to_port_mlg_dist]
        , @[self_shp_flg]
        , 'EAIUSER_VSK'
        , SYSDATE
        , 'EAIUSER_VSK'
        , SYSDATE
)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="pasg_pln_dt" type="12" value="" out="N"/>
				<param name="dep_port_cd" type="12" value="" out="N"/>
				<param name="arr_port_cd" type="12" value="" out="N"/>
				<param name="pasg_pln_tit_nm" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="plt_stn_desc" type="12" value="" out="N"/>
				<param name="avg_vsl_spd" type="12" value="" out="N"/>
				<param name="dep_pln_dt" type="12" value="" out="N"/>
				<param name="arr_pln_dt" type="12" value="" out="N"/>
				<param name="arr_dep_plc_diff_hrs" type="12" value="" out="N"/>
				<param name="vsl_apro_doc_no" type="12" value="" out="N"/>
				<param name="ttl_mlg_dist" type="12" value="" out="N"/>
				<param name="port_to_port_mlg_dist" type="12" value="" out="N"/>
				<param name="self_shp_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
