<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoIrregularMgtDBDAOIrrCntrMaxSeqRSQL">
			<desc><![CDATA[SCG_IRR_CNTR 테이블의 Max Seqence 가져오기]]></desc>
			<sql><![CDATA[
SELECT 
	NVL(MAX(SPCL_CGO_IRR_CNTR_SEQ),0)+1 SPCL_CGO_IRR_CNTR_SEQ
FROM SCG_IRR_CNTR
WHERE	VSL_CD = @[vsl_cd]
AND	SKD_VOY_NO = @[skd_voy_no]
AND	SKD_DIR_CD = @[skd_dir_cd]
AND	SPCL_CGO_IRR_SEQ = @[spcl_cgo_irr_seq]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="spcl_cgo_irr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
