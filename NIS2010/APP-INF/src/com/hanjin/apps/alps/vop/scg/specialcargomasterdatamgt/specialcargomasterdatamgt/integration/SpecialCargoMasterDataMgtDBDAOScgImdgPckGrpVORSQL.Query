<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoMasterDataMgtDBDAOScgImdgPckGrpVORSQL">
			<desc><![CDATA[Packing Group (Creation) 조회]]></desc>
			<sql><![CDATA[
SELECT 
	IMDG_PCK_GRP_CD
,	PCK_DGR_NM
,	FLSH_PNT_TEMP_CTNT
,	INIT_BOIL_PNT_TEMP_CTNT
,	DRP_HGT_CTNT
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM SCG_IMDG_PCK_GRP
WHERE	1 = 1
#if (${imdg_pck_grp_cd} != '') 
AND IMDG_PCK_GRP_CD = @[imdg_pck_grp_cd]
#end
ORDER BY IMDG_PCK_GRP_CD			]]></sql>
			<params>
				<param name="imdg_pck_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
