<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerLinesDangerousCargoApprovalDBDAOVesselOperatorVORSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT B.CRR_CD
FROM  BKG_VVD          A                  
    , MDM_VSL_CNTR     B
WHERE A.BKG_NO   = @[bkg_no]                
AND A.VSL_CD   = B.VSL_CD
AND B.CRR_CD = 'SML'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
