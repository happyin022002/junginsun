<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementImportDBDAOSearchContractOfficeRSQL">
			<desc><![CDATA[Contract Office 유효성 체크]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
  FROM MDM_ORGANIZATION
WHERE 1=1
  AND OFC_CD= @[ctrt_ofc_cd]
  AND NVL(DELT_FLG, 'N') <> 'Y'
			]]></sql>
			<params>
				<param name="ctrt_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
