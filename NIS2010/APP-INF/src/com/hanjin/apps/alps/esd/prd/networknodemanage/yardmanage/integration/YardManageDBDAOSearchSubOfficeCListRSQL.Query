<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="YardManageDBDAOSearchSubOfficeCListRSQL">
			<desc><![CDATA[spcode,spname,country code 조회]]></desc>
			<sql><![CDATA[
SELECT LPAD(TO_CHAR(VNDR_SEQ), 6, '0') VNDR_SEQ,
       VNDR_LGL_ENG_NM,
       VNDR_CNT_CD
FROM MDM_VENDOR
WHERE VNDR_SEQ = @[chk_vndr_seq]			]]></sql>
			<params>
				<param name="chk_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
