<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDADAOGetInbondNbrDtItNbrDtRSQL">
			<desc><![CDATA[Get INBOND_NBR, INBOND_NBR_DT, IT_NBR_DT]]></desc>
			<sql><![CDATA[
SELECT	CAIB.IBD_TRSP_NO AS INBOND_NBR
		,TO_CHAR( CAIB.IBD_TRSP_ISS_DT, 'YYYY-MM-DD HH24:MI') AS INBOND_NBR_DT
		,( SELECT TO_CHAR( MAX(ARR_DT), 'YYYY-MM-DD HH24:MI') 
          FROM BKG_CSTMS_ADV_RSLT
          WHERE CNT_CD  = CABL.CNT_CD 
          AND   BL_NO   = CABL.BL_NO
          AND   DSPO_CD = '1J'
         ) AS IT_NBR_DT
FROM  BKG_BOOKING BKGM
      JOIN BKG_CSTMS_ADV_BL CABL
      ON ( CABL.CNT_CD  = 'US'
           AND CABL.BKG_NO = BKGM.BKG_NO
           AND CABL.BL_NO  = BKGM.BL_NO  )  
      LEFT OUTER JOIN BKG_CSTMS_ADV_IBD CAIB
      ON ( CABL.CNT_CD     = CAIB.CNT_CD
           AND CABL.BL_NO  = CAIB.BL_NO )   
WHERE BKGM.BKG_NO  = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
