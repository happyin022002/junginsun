<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SceAdminManageDBDAOSearchLeaMonthlyWorkCandiateRSQL">
			<desc><![CDATA[LEA 월간 결산 대상 Data 조회]]></desc>
			<sql><![CDATA[
SELECT 
    DISTINCT BKG_NO,
    CNTR_NO, 
    INV_COST_ACT_GRP_CD COST_ACT_GRP_CD,
    INV_COST_ACT_GRP_SEQ COST_ACT_GRP_SEQ, 
    TO_CHAR(ADD_MONTHS(SYSDATE,-1), 'YYYYMM')||'LEA' CRE_USR_ID, 
    SYSDATE CRE_DT, 
    TO_CHAR(ADD_MONTHS(SYSDATE,-1), 'YYYYMM')||'LEA' UPD_USR_ID, 
    SYSDATE UPD_DT
FROM LEA_CSR_MNTR
WHERE
    EXE_YRMON = NVL(@[leaAccMon], TO_CHAR(ADD_MONTHS(SYSDATE,-1),'YYYYMM'))
AND INV_SYS_ID='TRS'
AND ACT_MAPG_RSLT_CD in ('ND','AG','BK')
			]]></sql>
			<params>
				<param name="leaAccMon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
