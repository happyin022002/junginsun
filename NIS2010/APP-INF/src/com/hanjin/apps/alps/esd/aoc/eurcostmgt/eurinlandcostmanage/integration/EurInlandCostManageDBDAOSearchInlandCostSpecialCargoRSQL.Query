<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurInlandCostManageDBDAOSearchInlandCostSpecialCargoRSQL">
			<desc><![CDATA[searchInlandCostSpecialCargo]]></desc>
			<sql><![CDATA[
SELECT TRSP_CRR_MOD_CD
      ,CNTR_SZ_CD
      ,CURR_CD
      ,DCGO_SVC_FLG
      ,DG_FX_RT
      ,DG_FX_RTO
      ,OVWT_CGO_SVC_FLG
      ,MIN_CGO_WGT
      ,MAX_CGO_WGT
      ,OVR_WGT_FX_RT
      ,OVR_WGT_FX_RTO
      ,COST_TRF_NO
      ,'' CRE_USR_ID
      ,'' UPD_USR_ID
  FROM AOC_EUR_INLND_SPCL_TRF_DTL    
 WHERE COST_TRF_NO = @[in_cost_trf_no]
   AND TRSP_CRR_MOD_CD IN (
                            SELECT  D.TRSP_CRR_MOD_CD
                            FROM    AOC_EUR_INLND_TRF_HDR H
                                  , AOC_EUR_INLND_TRF_DTL D  
                            WHERE   H.COST_TRF_NO = D.COST_TRF_NO
                            AND     H.COST_TRF_STS_CD IN ('B','U','C')
                            AND     D.COST_SEL_ROUT_FLG = 'Y'
                            AND     D.DELT_FLG    = 'N'
                            AND     H.COST_TRF_NO = @[in_cost_trf_no]
                            AND     H.CNT_CD      = @[in_cnt_cd]
                          )
ORDER BY CNTR_SZ_CD,TRSP_CRR_MOD_CD,CURR_CD			]]></sql>
			<params>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
				<param name="in_cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
