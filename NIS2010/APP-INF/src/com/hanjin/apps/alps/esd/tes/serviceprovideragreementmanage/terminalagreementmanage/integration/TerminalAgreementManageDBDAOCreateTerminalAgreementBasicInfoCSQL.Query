<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOCreateTerminalAgreementBasicInfoCSQL">
			<desc><![CDATA[Agreement Header Info Insert]]></desc>
			<sql><![CDATA[
INSERT	INTO TES_TML_AGMT_HDR (                                               
		  tml_agmt_ofc_cty_cd                
		, tml_agmt_seq                       
		, tml_agmt_ver_no
		, yd_cd                              
		, vndr_seq                           
		, ctrt_ofc_cd
		, cre_ofc_cd
		, eff_fm_dt
		, eff_to_dt
		, auto_xtd_flg                       
		, agmt_rmk
		, tml_agmt_sts_cd
		, delt_flg
		, cre_usr_id
		, cre_dt
		, upd_usr_id
		, upd_dt
		, locl_cre_dt
		, agmt_apro_dt
		, agmt_cfm_dt
		, agmt_cfm_usr_nm
		, agmt_cfm_usr_id
		)
VALUES	(                        
		  SUBSTR(@[yd_cd], 3, 3)
		, (SELECT NVL(MAX(tml_agmt_seq), 0) + 1 FROM  TES_TML_AGMT_HDR)
		, @[tml_agmt_ver_no]                            
		, @[yd_cd]                             
		, @[vndr_seq]                            
		, @[ctrt_ofc_cd]                            
		, @[cre_ofc_cd]                              
		, TO_DATE(@[eff_fm_dt], 'YYYY-MM-DD')      
		, TO_DATE(@[eff_to_dt], 'YYYY-MM-DD')         
		, @[auto_xtd_flg]                            
		, @[agmt_rmk]                            
		, 'P'                                  
		, 'N'                                  
		, @[cre_usr_id]                            
		, SYSDATE
		, @[cre_usr_id]  
		, SYSDATE
		, GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])     
		, TO_DATE(@[agmt_apro_dt], 'YYYY-MM-DD')
		, TO_DATE(@[agmt_cfm_dt], 'YYYY-MM-DD')
		, (select usr_nm from com_user where usr_id = @[agmt_cfm_usr_id])
		, @[agmt_cfm_usr_id]                                    
		)			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="tml_agmt_ver_no" type="2" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="auto_xtd_flg" type="12" value="" out="N"/>
				<param name="agmt_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="agmt_apro_dt" type="12" value="" out="N"/>
				<param name="agmt_cfm_dt" type="12" value="" out="N"/>
				<param name="agmt_cfm_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
