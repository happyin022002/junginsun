<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="COPSearchDBDAOModifyTRSSOCnddtCreationUSQL">
			<desc><![CDATA[TRS S/O Candidate Creation (RQST By BKG에 의해 삭제된 S/O Candidate 복구 재생성)]]></desc>
			<sql><![CDATA[
UPDATE SCE_PLN_SO_LIST 
SET TRSP_SO_STS_CD = 'P'
WHERE 1=1 
AND COP_NO = @[cop_no]
AND CTRL_OFC_CD = @[ctrl_ofc_cd]
AND COST_ACT_GRP_SEQ = @[cost_act_grp_seq]
AND @[trsp_so_sts_cd] IN ('D', 'N')
			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="" out="N"/>
				<param name="trsp_so_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
