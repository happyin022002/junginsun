<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KpiTargetDBDAOSearchKpiSpSvcCateRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT SP_KPI_ID
    , SP_KPI_NM
    , EV_SVC_CATE_CD
    , SP_KPI_DESC
    , SP_KPI_TP_CD
FROM SPE_SP_SVC_CATE_KPI
WHERE DELT_FLG = 'N'
AND EV_SVC_CATE_CD = @[s_ev_svc_cate_cd]			]]></sql>
			<params>
				<param name="s_ev_svc_cate_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
