<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccrualBatchExecuteResultDBDAOCreateYearlyCostCodeCSQL">
			<desc><![CDATA[CreateYearlyCostCode]]></desc>
			<sql><![CDATA[
INSERT INTO LEA_YRY_COST_BUD (
	BSE_YR,                      
	CTRL_OFC_CD,       
	COA_COST_SRC_CD,
	ACCT_CD,       
	TTL_USD_AMT,       
	MON_BUD_USD_AMT,      
	MNL_FLG,
	CRE_USR_ID,       
	CRE_DT,       
	UPD_USR_ID,       
	UPD_DT               
)VALUES(
  @[bse_yr],
  @[ctrl_ofc_cd],
  @[coa_cost_src_cd],
 (SELECT  ACCT_CD FROM LEA_LGS_COST WHERE COA_COST_SRC_CD = @[coa_cost_src_cd] AND DELT_FLG ='N'),
  0,  
  0,
  'N',
  @[cre_usr_id],
  SYSDATE,
  @[cre_usr_id],
  SYSDATE
)			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="coa_cost_src_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
