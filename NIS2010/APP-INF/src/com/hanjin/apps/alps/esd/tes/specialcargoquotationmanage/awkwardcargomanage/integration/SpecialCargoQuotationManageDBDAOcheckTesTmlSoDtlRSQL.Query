<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOcheckTesTmlSoDtlRSQL">
			<desc><![CDATA[checkTesTmlSoDtl]]></desc>
			<sql><![CDATA[
SELECT 'X' FLAG 
FROM DUAL 
WHERE NOT EXISTS ( SELECT * FROM TES_TML_SO_DTL
WHERE TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
AND TML_SO_SEQ = @[tml_so_seq]
AND BKG_NO = @[bkg_no]
AND LGS_COST_CD = @[lgs_cost_cd])			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
