<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewSvcOrdAfterHisCSQL">
			<desc><![CDATA[W/O Confirm시, After History 기록]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_SVC_ORD_HIS
(
   TRSP_TMP_SEQ
  ,CTRL_OFC_CD
  ,COP_NO
  ,EQ_NO
  ,COST_ACT_GRP_SEQ
  ,COST_ACT_GRP_CD
  ,TRSP_CRR_MOD_CD
  ,TRSP_BND_CD
  ,TRSP_SO_OFC_CTY_CD
  ,TRSP_SO_SEQ
  ,TRSP_SO_STS_CD
  ,TRSP_SO_STS_NM
  ,BKG_NO
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
)
SELECT 6 TRSP_TMP_SEQ
       ,CTRL_OFC_CD
       ,COP_NO
	   ,@[eq_no] EQ_NO
       ,COST_ACT_GRP_SEQ
       ,COST_ACT_GRP_CD
       ,TRSP_MOD_CD
       ,PCTL_IO_BND_CD
	   ,substr(@[cre_ofc_cd],1,3)
       ,@[trsp_so_seq]
       ,TRSP_SO_STS_CD
	   ,'Confirm' TRSP_SO_STS_NM
	   ,@[bkg_no]
       ,@[cre_usr_id] CRE_USR_ID
       ,SYSDATE CRE_DT
       ,@[upd_usr_id] UPD_USR_ID
       ,SYSDATE UPD_DT
  FROM SCE_PLN_SO_LIST
 WHERE COP_NO = @[cop_no]
   AND COST_ACT_GRP_SEQ = @[cost_act_grp_seq]			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
