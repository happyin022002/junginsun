<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchCostcdDetailRSQL">
			<desc><![CDATA[CodeManageDBDAOSearchCostcdDetail
LGS COST CODE 조회]]></desc>
			<sql><![CDATA[
SELECT	LGS_COST_CD
FROM	TES_LGS_COST
WHERE	1	= 1
AND		LGS_COST_CD_CLSS_LVL	= 'A'
AND		LGS_COST_SUBJ_CD		= @[lgs_cost_subj_cd]
ORDER BY LGS_COST_CD DESC			]]></sql>
			<params>
				<param name="lgs_cost_subj_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
