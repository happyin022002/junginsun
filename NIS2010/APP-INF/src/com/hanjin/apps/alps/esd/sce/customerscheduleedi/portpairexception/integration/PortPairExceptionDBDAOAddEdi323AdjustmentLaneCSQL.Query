<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairExceptionDBDAOAddEdi323AdjustmentLaneCSQL">
			<desc><![CDATA[sce_323_adj_lane 테이블에 인서트함]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_323_ADJ_LANE
	(	
		 ADJ_RGST_DT
		,ADJ_SEQ
		,SLAN_CD
		,DELT_FLG
		,CRE_USR_ID
		,CRE_DT
		,UPD_USR_ID
		,UPD_DT
	) 
SELECT	
		 @[adj_rgst_dt]	
		,@[adj_seq]		
		,SPLIT_SLAN_CD
		,'N'
		,@[cre_usr_id]
		,SYSDATE
		,@[upd_usr_id]
		,SYSDATE
FROM	(
			SELECT
					REGEXP_SUBSTR( SPLIT_SLAN_CD, '[^,]+', 1, LEVEL ) AS SPLIT_SLAN_CD
			FROM	(
						SELECT	@[slan_cds] AS SPLIT_SLAN_CD
						FROM	DUAL
					)
			CONNECT BY 
					REGEXP_SUBSTR( SPLIT_SLAN_CD, '[^,]+', 1, LEVEL ) IS NOT NULL 				
		)			]]></sql>
			<params>
				<param name="adj_rgst_dt" type="12" value="" out="N"/>
				<param name="adj_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="slan_cds" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
