<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAORemoveSOCandidateRSQL">
			<desc><![CDATA[Candidate Delete 대상을 조회 (LCL Master/Slave 모두 조회)]]></desc>
			<sql><![CDATA[
SELECT  B.COP_NO
       ,B.COST_ACT_GRP_SEQ
       ,A.BKG_NO
  FROM SCE_COP_HDR A,
       SCE_PLN_SO_LIST B,
       BKG_BOOKING C
 WHERE A.COP_NO = B.COP_NO
   AND A.BKG_NO = C.BKG_NO
   AND A.CNTR_NO     = @[eq_no]
   AND C.POL_CD      = @[pol_cd]
   AND A.TRNK_VSL_CD = SUBSTR(@[trunkvvd],1,4)
   AND A.TRNK_SKD_VOY_NO = SUBSTR(@[trunkvvd],5,4)
   AND A.TRNK_SKD_DIR_CD = SUBSTR(@[trunkvvd],9,1)
   AND A.COP_STS_CD NOT IN ('X', 'O')
   AND B.TRSP_SO_STS_CD in ( 'P')
   AND B.CTRL_OFC_CD = @[ctrl_ofc_cd]
   AND B.N1ST_NOD_CD = @[fm_nod_cd]||@[fm_nod_yard]
   AND A.CNTR_NO <> 'SMCU0000000'
   AND NVL2(A.MST_COP_NO, 'P', 'X') = 'X' -- LCL CNTR중 Slave건만 조회한다.			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="trunkvvd" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yard" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
