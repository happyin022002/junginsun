<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdi17536801CntrEurDetailsRSQL">
			<desc><![CDATA[SearchEdi17536801CntrEurDetails]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC (CRO XPKBKG_FULL_CGO_RLSE_ORD) */
       CRO.CO_BDG_ID 								EUR_BADGECODE,
       CRO.CGO_CRR_ID 								EUR_ONCARRIAGE_ID,
       TO_CHAR(CRO.RLSE_CRE_DT, 'YYYYMMDDHH24MI') 	EUR_RELEASE_DT,
       TO_CHAR(CRO.RLSE_EXP_DT, 'YYYYMMDDHH24MI') 	EUR_EXPIRY_DT,
       CRO.PIN_NO									EUR_PIN,
       CRO.VEH_RGST_ID 								EUR_VEHICLE_ID,
       CRO.ROAD_HLG_ID 								EUR_RHIDS,
       CRO.UQ_VSL_ID_NO 							EUR_UVI
  FROM BKG_FULL_CGO_RLSE_ORD  CRO,
       TRS_TRSP_SVC_ORD SO
 WHERE 1=1
   AND SO.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND SO.TRSP_SO_SEQ = @[trsp_so_seq]
   AND SO.HJL_NO IS NULL
   AND SO.DELT_FLG <> 'Y'
   AND CRO.BKG_NO = SO.BKG_NO
   AND CRO.CNTR_NO = SO.EQ_NO
   AND CRO.PIN_NO IS NOT NULL
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
