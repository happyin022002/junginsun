<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchCodescListSubjRSQL">
			<desc><![CDATA[SearchCodescListSubj]]></desc>
			<sql><![CDATA[
SELECT	LGS_COST_CD AS LGS_COST_SUBJ_CD
FROM	TES_LGS_COST
WHERE	1	= 1
AND		LGS_COST_CD IN (
						SELECT	LC.LGS_COST_SUBJ_CD
						FROM	TES_LGS_COST LC
								, TES_TML_SO_COST SC
						WHERE	1	= 1
						AND		LC.LGS_COST_CD	= SC.LGS_COST_CD
						AND		LC.LGS_COST_SUBJ_CD IS NOT NULL 
						AND		LC.DELT_FLG		= 'N'
						)
AND		LGS_COST_CD_CLSS_LVL	= 'S'
ORDER BY LGS_COST_CD					
			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
