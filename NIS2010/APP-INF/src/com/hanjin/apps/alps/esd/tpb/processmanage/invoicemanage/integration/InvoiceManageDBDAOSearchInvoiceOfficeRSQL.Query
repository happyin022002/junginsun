<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOSearchInvoiceOfficeRSQL">
			<desc><![CDATA[TPB -> TPB Process -> TPB Invoice Setting
Invoice Office 유무 확인]]></desc>
			<sql><![CDATA[
SELECT COUNT(INV_ISS_OFC_CD) CNT
  FROM TPB_INV_SH_SET
 WHERE INV_ISS_OFC_CD = @[s_inv_iss_ofc_cd]			]]></sql>
			<params>
				<param name="s_inv_iss_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
