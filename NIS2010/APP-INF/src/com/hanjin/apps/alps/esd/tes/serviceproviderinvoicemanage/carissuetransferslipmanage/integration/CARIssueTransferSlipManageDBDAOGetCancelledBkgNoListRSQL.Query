<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CARIssueTransferSlipManageDBDAOGetCancelledBkgNoListRSQL">
			<desc><![CDATA[GetCancelledBkgNoList]]></desc>
			<sql><![CDATA[
SELECT H.TML_SO_OFC_CTY_CD, H.TML_SO_SEQ, C.TML_SO_CNTR_LIST_SEQ, 
       C.CNTR_NO, B.BKG_NO, B.BL_NO, B.VSL_CD, B.SKD_VOY_NO, B.SKD_DIR_CD   
FROM TES_TML_SO_HDR H, TES_TML_SO_CNTR_LIST C, BKG_BOOKING B
WHERE H.TML_SO_OFC_CTY_CD = C.TML_SO_OFC_CTY_CD
AND H.TML_SO_SEQ = C.TML_SO_SEQ
AND H.TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
AND H.TML_SO_SEQ = @[tml_so_seq]
AND B.BKG_NO = (SELECT FM_BKG_NO FROM BKG_BOOKING K
				WHERE K.BKG_NO = C.BKG_NO
				AND K.BKG_STS_CD = 'X')
AND B.BKG_STS_CD IN ('F','W')			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
