<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SupplementSOManageDBDAOInsertSOMstCSQL">
			<desc><![CDATA[새로운 SO를 생성]]></desc>
			<sql><![CDATA[
INSERT
INTO TRS_TRSP_SVC_ORD 
( 
    PRNT_TRSP_SO_OFC_CTY_CD, 
    PRNT_TRSP_SO_SEQ, 
    EQ_KND_CD, 
    EQ_TPSZ_CD, 
    EQ_NO, 
    TRSP_BND_CD, 
    VNDR_SEQ, 
    TRSP_COST_DTL_MOD_CD, 
    TRSP_CRR_MOD_CD, 
    CGO_TP_CD, 
    CMDT_CD, 
    CNTR_WGT, 
    WGT_MEAS_UT_CD, 
    FM_NOD_CD, 
    TO_NOD_CD, 
    VIA_NOD_CD, 
    DOR_NOD_CD, 
    VSL_CD, 
    SKD_VOY_NO, 
    SKD_DIR_CD, 
    SLAN_CD, 
    FDR_VSL_CD, 
    FDR_SKD_VOY_NO, 
    FDR_SKD_DIR_CD, 
    ORG_BKG_NO, 
    BKG_NO, 
    BL_NO, 
    DOR_SVC_TP_CD, 
    DOR_DE_ADDR, 
    DTN_USE_FLG, 
    MLT_STOP_DE_FLG, 
    TRSP_PURP_RSN, 
    CMB_SO_RLT_STS_FLG, 
    TRSP_SO_CMB_TP_CD, 
    TRSP_SO_CMB_SEQ,     -- /* TRSP_SO_CMB_TP_CD = 'BD' < -- Bundling Count, <> 'BD' <-- NULL */        
    CHSS_MGST_TRSP_TP_CD, 
    SPCL_INSTR_RMK, 
    INTER_RMK, 
    FCTRY_NM, 
    COP_NO, 
    -- ACT_GRP_CD, 	-- KYS 삭제 : 칼럼 삭제됨???
    COST_ACT_GRP_CD,
    COST_ACT_GRP_SEQ,  
     --#* Supplement S/O New Insert Data *# 
    TRSP_SO_OFC_CTY_CD, 
    TRSP_SO_SEQ, 
    TRSP_SO_TP_CD, 
    TRSP_SPL_SO_TP_CD, 
    TRSP_SO_STS_CD, 
    TRSP_AGMT_OFC_CTY_CD, 
    TRSP_AGMT_SEQ, 
    CUST_NOMI_TRKR_FLG, 
    CUST_CNT_CD, 
    CUST_SEQ, 
    TRSP_AGMT_WY_TP_CD, 
    TRSP_AGMT_RT_TP_CD, 
    CURR_CD, 
    BZC_AMT, 
    FUEL_SCG_AMT, 
    --#, OVR_WGT_SCG_AMT, 
    ETC_ADD_AMT, 
    SPL_ISS_RSN, 
    LGS_COST_CD, 
    ACCT_CD, 
    DELT_FLG, 
    CRE_OFC_CD, 
    CRE_USR_ID, 
    CRE_DT, 
    UPD_USR_ID, 
    UPD_DT, 
    CNTR_KGS_WGT, 
    CNTR_LBS_WGT, 
	LOCL_CRE_DT, 
	LOCL_UPD_DT
)
SELECT 
  X.TRSP_SO_OFC_CTY_CD,
  X.TRSP_SO_SEQ,
  X.EQ_KND_CD,
  X.EQ_TPSZ_CD,
  X.EQ_NO,
  X.TRSP_BND_CD,
  X.VNDR_SEQ,
  X.TRSP_COST_DTL_MOD_CD,
  X.TRSP_CRR_MOD_CD,
  X.CGO_TP_CD,
  X.CMDT_CD,
  X.CNTR_WGT,
  X.WGT_MEAS_UT_CD,
  X.FM_NOD_CD,
  X.TO_NOD_CD,
  X.VIA_NOD_CD,
  X.DOR_NOD_CD,
  X.VSL_CD,
  X.SKD_VOY_NO,
  X.SKD_DIR_CD,
  X.SLAN_CD,
  X.FDR_VSL_CD,
  X.FDR_SKD_VOY_NO,
  X.FDR_SKD_DIR_CD,
  X.ORG_BKG_NO,
  X.BKG_NO,
  X.BL_NO,
  X.DOR_SVC_TP_CD,
  X.DOR_DE_ADDR,
  X.DTN_USE_FLG,
  X.MLT_STOP_DE_FLG,
  X.TRSP_PURP_RSN,
  X.CMB_SO_RLT_STS_FLG,
  X.TRSP_SO_CMB_TP_CD,          	-- TRSP_SO_CMB_TP_CD = 'BD' < -- Bundling Count, <> 'BD' <-- NULL 	
  DECODE(X.TRSP_SO_CMB_TP_CD, 'BD', X.TRSP_SO_CMB_SEQ, NULL),
  X.CHSS_MGST_TRSP_TP_CD,
  X.SPCL_INSTR_RMK,
  X.INTER_RMK,
  X.FCTRY_NM,
  X.COP_NO,
  -- X.ACT_GRP_CD,			-- KYS 삭제 : 칼럼 삭제됨???
  X.COST_ACT_GRP_CD,
  X.COST_ACT_GRP_SEQ, 
  --/*  Supplement S/O New Insert Data */
  X.TRSP_SO_OFC_CTY_CD, 								-- /* TRSP_SO_OFC_CTY_CD */
  @[trsp_so_seq],        								-- /* TRSP_SO_SEQ */
  'S',                  								-- /* TRSP_SO_TP_CD */
  @[trsp_spl_so_tp_cd], 								-- /* TRSP_SPL_SO_TP_CD - AS/RR */
  'C', 													-- /* TRSP_SO_STS_CD */
  @[trsp_agmt_ofc_cty_cd], 								-- /* TRSP_AGMT_OFC_CTY_CD */
  @[trsp_agmt_seq], 									-- /* TRSP_AGMT_SEQ */
  X.CUST_NOMI_TRKR_FLG, 								-- /* CUST_NOMI_TRKR_FLG */
  X.CUST_CNT_CD, 										-- /* CUST_CNT_CD */
  X.CUST_SEQ, 											-- /* CUST_SEQ */
  '', 													-- /* TRSP_AGMT_WY_TP_CD */
  '', 													-- /* TRSP_AGMT_RT_TP_CD */
  @[curr_cd], 											-- /* CURR_CD */
  @[bzc_amt], 											-- /* BZC_AMT */
  @[fuel_scg_amt], 										-- /* FUEL_SCG_AMT */
  @[etc_add_amt], 										-- /* ETC_ADD_AMT */
  @[spl_iss_rsn], 										-- /* SPL_ISS_RSN */
  X.LGS_COST_CD,
  X.ACCT_CD,
  'N', 													-- /* DELT_FLG */
  @[cre_ofc_cd], 										-- /* CRE_OFC_CD */
  @[cre_usr_id], 										-- /* CRE_USR_ID */
  SYSDATE, 	 											-- /* CRE_DT */
  @[upd_usr_id], 										-- /* UPD_USR_ID */,
  SYSDATE, 												-- /* UPD_DT */
  X.CNTR_KGS_WGT, 										-- /* CNTR_KGS_WGT */
  X.CNTR_LBS_WGT, 										-- /* CNTR_LBS_WGT */
  GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd]), 	-- /* LOCL_CRE_DT */
  GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd])	 	-- /* LOCL_UPD_DT */
FROM TRS_TRSP_SVC_ORD X
WHERE X.TRSP_SO_OFC_CTY_CD = @[prnt_trsp_so_ofc_cty_cd] -- /* PRNT_TRSP_SO_OFC_CTY_CD */
  	AND X.TRSP_SO_SEQ = @[prnt_trsp_so_seq] 			-- /* PRNT_TRSP_SO_SEQ */ 
	-- /* 2008.04.29 ETS OPEN */
  	AND X.HJL_NO IS NULL			]]></sql>
			<params>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="trsp_spl_so_tp_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="bzc_amt" type="12" value="" out="N"/>
				<param name="fuel_scg_amt" type="12" value="" out="N"/>
				<param name="etc_add_amt" type="12" value="" out="N"/>
				<param name="spl_iss_rsn" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="prnt_trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="prnt_trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
