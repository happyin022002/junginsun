<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentManageDBDAOUpdateReviewStepEndUSQL">
			<desc><![CDATA[Review Step Update]]></desc>
			<sql><![CDATA[
UPDATE TPB_ADJ_N2ND_RVW_HIS
   SET N2ND_RVW_AVAL_FLG = 'N'
 WHERE N3PTY_NO = @[n3pty_no]
   AND ADJ_STS_SEQ = @[adj_sts_seq]
   AND ADJ_N2ND_RVW_SEQ = @[adj_n2nd_rvw_seq]
   AND ADJ_N2ND_RVW_STS_CD = @[adj_n2nd_rvw_sts_cd]
   AND NVL(N2ND_RVW_AVAL_FLG,'X') = 'Y'			]]></sql>
			<params>
				<param name="n3pty_no" type="12" value="" out="N"/>
				<param name="adj_sts_seq" type="12" value="" out="N"/>
				<param name="adj_n2nd_rvw_seq" type="12" value="" out="N"/>
				<param name="adj_n2nd_rvw_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
