<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESeBillingManageBizType002DBDAOUpdateEDInvoiceCntrListVVDUSQL">
			<desc><![CDATA[정규 VVD가 없어서 CALL SIGN / LLOYD CD / BKG REF NO로 VVD Update해야 할 CNTR LIST 대상 Update하기]]></desc>
			<sql><![CDATA[
UPDATE TES_EDI_SO_CNTR_LIST D
SET
D.VSL_CD        = @[vsl_cd]
, D.SKD_VOY_NO    = @[skd_voy_no]
, D.SKD_DIR_CD    = @[skd_dir_cd]
, D.CNTR_RMK    = @[cntr_rmk]
WHERE 1=1
AND D.TML_EDI_SO_OFC_CTY_CD = @[tml_edi_so_ofc_cty_cd]
AND D.TML_EDI_SO_SEQ = @[tml_edi_so_seq]
AND D.TML_EDI_SO_CNTR_LIST_SEQ = @[tml_edi_so_cntr_list_seq]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="cntr_rmk" type="12" value="" out="N"/>
				<param name="tml_edi_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_edi_so_seq" type="12" value="" out="N"/>
				<param name="tml_edi_so_cntr_list_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
