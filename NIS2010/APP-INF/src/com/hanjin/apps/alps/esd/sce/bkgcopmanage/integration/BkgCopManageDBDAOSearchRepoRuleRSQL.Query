<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchRepoRuleRSQL">
			<desc><![CDATA[type size 별로 호환 가능한 type size 를 조회한다.]]></desc>
			<sql><![CDATA[
select prov_cntr_tpsz_cd
from
sce_cop_cntr_repo_rule
where
cntr_tpsz_cd = @[cntr_tpsz_cd]
and nvl(act_flg, 'N') = 'Y'			]]></sql>
			<params>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
