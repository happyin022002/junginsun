<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalProductivityDBDAOAddEGEVMappingCSQL">
			<desc><![CDATA[Terminal Productivity Target Input 을 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO SPE_EV_GRP_TML_PROD_TGT(EG_ID
                                  , EG_TML_PROD_TGT_SEQ
                                  , EV_YR
                                  , SP_SEQ
                                  , TML_CD
                                  , KPI_TGT_RTO
                                  , KPI_RMK
                                  , CRE_USR_ID
                                  , CRE_DT
                                  , UPD_USR_ID
                                  , UPD_DT 
                                  )VALUES(
                                    @[eg_id]
                                  , SPE_EV_GRP_TML_PROD_TGT_SEQ.NEXTVAL
                                  , @[ev_yr]
                                  , @[sp_seq]
                                  , @[tml_cd]
                                  , @[kpi_tgt_rto]
                                  , @[kpi_rmk]
                                  , @[cre_usr_id]
                                  , SYSDATE
                                  , @[upd_usr_id]
                                  , SYSDATE
                                  )			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="ev_yr" type="12" value="" out="N"/>
				<param name="sp_seq" type="12" value="" out="N"/>
				<param name="tml_cd" type="12" value="" out="N"/>
				<param name="kpi_tgt_rto" type="12" value="" out="N"/>
				<param name="kpi_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
