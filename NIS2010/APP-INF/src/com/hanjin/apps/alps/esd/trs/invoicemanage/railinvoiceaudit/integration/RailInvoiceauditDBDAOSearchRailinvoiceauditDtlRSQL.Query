<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOSearchRailinvoiceauditDtlRSQL">
			<desc><![CDATA[RailInvoice에 대한 Detail 정보 조회]]></desc>
			<sql><![CDATA[
SELECT  TRSP_INV_CO_IND_CD
      , TRSP_INV_TP_CD
      , TO_CHAR(RAIL_BIL_DT, 'YYYYMMDD') RAIL_BIL_DT
      , TO_CHAR(WBL_DT, 'YYYYMMDD') WBL_DT
      , WBL_NO
      , INV_RMK
      , SUB_INV_SEQ
      , TRSP_SO_OFC_CTY_CD
      , TRSP_SO_SEQ
      , SUB_RAIL_SEQ
      , ORG_TRSP_RAIL_INV_AUD_CD
      , CRNT_TRSP_RAIL_INV_AUD_CD
      , PAY_FLG
      , EQ_NO
      , EQ_TPSZ_CD
      , CGO_TP_CD
      , DMST_REPO_FLG
      , SUBSTR(FM_NOD_CD, 0, 5) FM_NOD_CD1
      , SUBSTR(FM_NOD_CD, 6, 7) FM_NOD_CD2
      , SUBSTR(TO_NOD_CD, 0, 5) TO_NOD_CD1
      , SUBSTR(TO_NOD_CD, 6, 7) TO_NOD_CD2
      , FM_NOD_CD
      , TO_NOD_CD
      , INV_ORG_NOD_NM
      , INV_DEST_NOD_NM
      , CURR_CD
      , BZC_AMT
      , FUEL_SCG_AMT
      , OVR_WGT_SCG_AMT
      , NEGO_AMT
      , INV_CURR_CD
      , INV_BZC_AMT
      , INV_BIL_AMT
      , INV_ETC_ADD_AMT
      , TMP_TRSP_RAIL_INV_AUD_CD
      , HZD_MTRL_SCG_AMT
      , ETC_ADD_AMT
FROM    TRS_TRSP_RAIL_INV_DTL
WHERE   INV_NO = @[invNo]
AND     INV_VNDR_SEQ = @[invVndrSeq]
AND     CRNT_TRSP_RAIL_INV_AUD_CD = @[railInvAudCd]			]]></sql>
			<params>
				<param name="invNo" type="12" value="" out="N"/>
				<param name="invVndrSeq" type="12" value="" out="N"/>
				<param name="railInvAudCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
