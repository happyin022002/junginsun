<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanLinkManageDBDAOOceanLinkChkSqlRSQL">
			<desc><![CDATA[기등록된 Ocean Link의 존재유무를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT 'X' 
FROM PRD_PF_TZ_TM 
WHERE FM_PORT_CD = @[polprot] 
AND TO_PORT_CD = @[podprot] 
AND VSL_SLAN_CD = @[lanecd]
AND DELT_FLG <> 'Y'			]]></sql>
			<params>
				<param name="polprot" type="12" value="" out="N"/>
				<param name="podprot" type="12" value="" out="N"/>
				<param name="lanecd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
