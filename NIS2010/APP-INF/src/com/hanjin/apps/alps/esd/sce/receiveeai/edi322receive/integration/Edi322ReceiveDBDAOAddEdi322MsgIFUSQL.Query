<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi322ReceiveDBDAOAddEdi322MsgIFUSQL">
			<desc><![CDATA[AddEdi322MsgIF]]></desc>
			<sql><![CDATA[
MERGE INTO SCE_EDI_DIR_IF D	                                                    
		     USING ( SELECT TO_DATE(@[EVENT_DT], 'yyyy/mm/dd HH24:MI:SS') iEVNT_DT, @[CNTR_NO] iEQ_NO,@[EVENT_STS] iEDI_STS_CD, @[EVENT_YARD] iNOD_CD,                       
		 			   @[BL_NO] iBL_EDI_322_NO, @[BKG_NO]iBKG_EDI_322_NO, @[PICKUP_NO] iPKUP_EDI_322_NO,                                                           
		 			    sysdate iCRE_DT,sysdate iUPD_DT, @[TMNL_ID] iCRE_USR_ID, @[TMNL_ID] iUPD_USR_ID  FROM DUAL ) S	                                                    
		     ON    (D.EVNT_DT = S.iEVNT_DT AND D.EQ_NO   = S.iEQ_NO AND D.EDI_STS_CD = S.iEDI_STS_CD)	                             
		    WHEN MATCHED THEN  	                                                       
		 	UPDATE SET D.NOD_CD          = S.iNOD_CD,                                               	                                 
		 		   D.BL_EDI_322_NO   = S.iBL_EDI_322_NO,                                                      
		 		   D.BKG_EDI_322_NO  = S.iBKG_EDI_322_NO,                                                     
		 		   D.PKUP_EDI_322_NO = S.iPKUP_EDI_322_NO,          
                   D.UPD_USR_ID      = S.iUPD_USR_ID,                                 
		 		   D.UPD_DT          = S.iUPD_DT                                                    
		    WHEN NOT MATCHED THEN INSERT (D.EVNT_DT, D.EQ_NO, D.EDI_STS_CD, D.NOD_CD,                                               
		 				    D.BL_EDI_322_NO, D.BKG_EDI_322_NO, D.PKUP_EDI_322_NO
                            ,D.CRE_USR_ID,D.UPD_USR_ID,D.UPD_DT
                            , D.CRE_DT) 	                                               
		 	VALUES  (S.iEVNT_DT, S.iEQ_NO, S.iEDI_STS_CD, S.iNOD_CD,                                                     
		 		 S.iBL_EDI_322_NO, S.iBKG_EDI_322_NO, S.iPKUP_EDI_322_NO
                 ,S.iCRE_USR_ID,S.iUPD_USR_ID,S.iUPD_DT
                 , S.iCRE_DT)


			]]></sql>
			<params>
				<param name="EVENT_DT" type="12" value="2007/05/07 23:59:00" out="N"/>
				<param name="CNTR_NO" type="12" value="INBU4843907" out="N"/>
				<param name="EVENT_STS" type="12" value="NF" out="N"/>
				<param name="EVENT_YARD" type="12" value="USKCKR1" out="N"/>
				<param name="BL_NO" type="12" value="" out="N"/>
				<param name="BKG_NO" type="12" value="SHAA22110807" out="N"/>
				<param name="PICKUP_NO" type="12" value="4EB3AD" out="N"/>
				<param name="TMNL_ID" type="12" value="BNSF" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
