<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsCommonDBDAOSearchMltMorOnyFlgRSQL">
			<desc><![CDATA[해당 오피스가 Multi More Candidate 필수 대상 인지 확인 (미국, 캐나다)]]></desc>
			<sql><![CDATA[
SELECT 'Y' MLT_MOR_ONY_FLG
  FROM MDM_ORGANIZATION A ,
       MDM_LOCATION B
 WHERE A.LOC_CD = B.LOC_CD
   AND A.DELT_FLG = 'N'
   AND B.DELT_FLG = 'N'
   AND B.CNT_CD IN ('US', 'CA')
   AND TRS_COMMON_PKG.TRS_GET_RHQ_OFC_CD(A.OFC_CD) = 'NYCRA'
   and A.OFC_CD = @[OFC_CD]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="OFC_CD" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
