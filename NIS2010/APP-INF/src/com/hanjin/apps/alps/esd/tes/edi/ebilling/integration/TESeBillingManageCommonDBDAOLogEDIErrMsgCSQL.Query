<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESeBillingManageCommonDBDAOLogEDIErrMsgCSQL">
			<desc><![CDATA[EDI error log]]></desc>
			<sql><![CDATA[
INSERT INTO TES_EDI_SO_ERR_LOG (
EDI_SO_ERR_LOG_SEQ
, EDI_MSG
, ERR_LOG_RMK
, TML_INV_EDI_SEQ
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
) VALUES (
TES_EDI_SO_ERR_LOG_SEQ.NEXTVAL
, SUBSTRB(@[edi_msg],1,82)
, SUBSTRB(@[err_log_rmk],1,1000)
, @[tml_inv_edi_seq]
, 'SYSTEM'
, SYSDATE
, 'SYSTEM'
, SYSDATE
)			]]></sql>
			<params>
				<param name="edi_msg" type="12" value="" out="N"/>
				<param name="err_log_rmk" type="12" value="" out="N"/>
				<param name="tml_inv_edi_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
