<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DeliveryMonitorDBDAOsearchDeliveryRSQL">
			<desc><![CDATA[Delivery Monitoring Summary 조회]]></desc>
			<sql><![CDATA[
SELECT YR_WEEK
      ,DT_PERIOD
      ,SO_OFC_CD
      ,SO_TP_CD
      ,BND_CD
      ,FM_NOD_CD
      ,TO_NOD_CD
      ,VIA_NOD_CD
      ,DOR_NOD_CD
      ,(SELECT ZN_NM FROM MDM_ZONE X WHERE X.ZN_CD = DOR_NOD_CD||'01') DOR_NM
      ,FM_DT
      ,TO_DT
      ,SO_CNT
      ,WO_CNT
      ,DO_CNT
      ,CNTR_CNT
      ,CASE WHEN BKG_CNT <= 921 THEN TO_CHAR(SUBSTR(BKG_DESC, 1, 3990)) END BKG_DESC
      ,CASE WHEN BKG_CNT > 307 AND BKG_CNT <= 921 THEN TO_CHAR(SUBSTR(BKG_DESC, 3991, 3991)) END BKG_DESC2
      ,CASE WHEN BKG_CNT > 614 AND BKG_CNT <= 921 THEN TO_CHAR(SUBSTR(BKG_DESC, 7982, 3991)) END BKG_DESC3
  FROM (
    SELECT WK YR_WEEK
          ,MIN(TO_CHAR(WK_FM_DT,'YYYY.MM.DD')) || '~' ||MIN(TO_CHAR(WK_TO_DT,'YYYY.MM.DD')) DT_PERIOD
          ,CTRL_OFC_CD SO_OFC_CD
          ,PCTL_COST_MOD_CD SO_TP_CD
          ,PCTL_IO_BND_CD BND_CD
          ,FM_NOD_CD
          ,TO_NOD_CD
          ,VIA_NOD_CD
          ,DOR_NOD_CD
          ,MIN(TO_CHAR(DT, 'YYYYMMDD')) FM_DT
          ,MAX(TO_CHAR(DT, 'YYYYMMDD')) TO_DT
          ,COUNT(SO_NO)  SO_CNT
          ,COUNT(WO_NO)  WO_CNT
          ,COUNT(DO_NO)  DO_CNT
          ,COUNT(*) CNTR_CNT
          ,COUNT(BKG_NO) BKG_CNT
          ,SUBSTR(XMLAGG(XMLELEMENT(X, ',', BKG_NO) ORDER BY BKG_NO).EXTRACT('//text()').getCLOBVal() ,2) BKG_DESC
      FROM ( SELECT CTRL_OFC_CD
                  ,TRSP_SO_STS_CD
                  ,PCTL_COST_MOD_CD
                  ,N1ST_NOD_PLN_DT
                  ,COP_NO
                  ,COST_ACT_GRP_SEQ
                  ,PCTL_IO_BND_CD
                  ,FM_NOD_CD
                  ,TO_NOD_CD
                  ,VIA_NOD_CD
                  ,DOR_NOD_CD
                  ,SUBSTR(AA.WK, 1, INSTR(AA.WK, '$', 1, 1) - 1) AS WK
                  ,TO_DATE(SUBSTR(AA.WK, INSTR(AA.WK, '$', 1, 1) + 1, INSTR(AA.WK, '$', 1, 2) - INSTR(AA.WK, '$', 1, 1) - 1), 'YYYYMMDD') AS WK_FM_DT
                  ,TO_DATE(SUBSTR(AA.WK, INSTR(AA.WK, '$', 1, 2) + 1, INSTR(AA.WK, '$', 1, 3) - INSTR(AA.WK, '$', 1, 2) - 1), 'YYYYMMDD') AS WK_TO_DT
                  ,DT
                  ,BKG_NO
                  ,(SELECT X.TRSP_SO_OFC_CTY_CD||X.TRSP_SO_SEQ
                      FROM TRS_TRSP_SVC_ORD X
                     WHERE X.COP_NO = AA.COP_NO
                       AND X.COST_ACT_GRP_SEQ = AA.COST_ACT_GRP_SEQ
                       AND X.DELT_FLG = 'N'
                       AND ROWNUM = 1
                   ) 
                 ||(SELECT X.TRSP_SO_OFC_CTY_CD||X.TRSP_SO_SEQ
                      FROM TRS_TRSP_RAIL_BIL_ORD X
                     WHERE X.COP_NO = AA.COP_NO
                       AND X.COST_ACT_GRP_SEQ = AA.COST_ACT_GRP_SEQ
                       AND ROWNUM = 1
                   ) SO_NO
                  ,(SELECT Y.TRSP_WO_OFC_CTY_CD||Y.TRSP_WO_SEQ
                      FROM TRS_TRSP_SVC_ORD X
                          ,TRS_TRSP_WRK_ORD Y
                     WHERE X.TRSP_WO_OFC_CTY_CD = Y.TRSP_WO_OFC_CTY_CD
                       AND X.TRSP_WO_SEQ = Y.TRSP_WO_SEQ
                       AND X.COP_NO = AA.COP_NO
                       AND X.COST_ACT_GRP_SEQ = AA.COST_ACT_GRP_SEQ
                       AND X.DELT_FLG = 'N'
                       AND ROWNUM = 1) 
                 ||(SELECT X.TRSP_SO_OFC_CTY_CD||X.TRSP_SO_SEQ
                      FROM TRS_TRSP_RAIL_BIL_ORD X
                          ,TRS_TRSP_EDI_RAIL_ORD Y
                     WHERE X.TRSP_SO_OFC_CTY_CD = Y.TRSP_SO_OFC_CTY_CD
                       AND X.TRSP_SO_SEQ = Y.TRSP_SO_SEQ
                       AND X.COP_NO = AA.COP_NO
                       AND X.COST_ACT_GRP_SEQ = AA.COST_ACT_GRP_SEQ
                       AND X.DELT_FLG = 'N'
                       AND ROWNUM = 1
                   ) WO_NO
                  ,(SELECT X.BL_NO||X.EQ_NO
                      FROM TRS_TRSP_SVC_ORD X
                     WHERE X.BKG_NO = AA.BKG_NO
                       AND X.EQ_NO  = AA.CNTR_NO
                       AND X.CRE_OFC_CD = AA.CTRL_OFC_CD
                       AND X.DOR_DE_ADDR IS NOT NULL
                       AND X.DELT_FLG = 'N'
                       AND X.TRSP_SO_TP_CD = 'Y'
                       AND ROWNUM = 1) DO_NO
              FROM (
                    SELECT
                          #if ($arr_bkg_no.size() > 0)
                            /*+ INDEX (B XAK1SCE_COP_HDR) */
                          #elseif ($arr_cntr_no.size() > 0)
                            /*+ INDEX (B XAK9SCE_COP_HDR) ORDERED */
                          #else
                            /*+ INDEX (A XAK2SCE_PLN_SO_LIST) */
                          #end
                           A.CTRL_OFC_CD
                          ,A.TRSP_SO_STS_CD
                          ,A.PCTL_COST_MOD_CD -- C: CY, Z: ZONE
                          ,A.N1ST_NOD_PLN_DT
                          ,A.COP_NO
                          ,A.COST_ACT_GRP_SEQ
                          ,A.PCTL_IO_BND_CD
                          ,SUBSTR(A.N1ST_NOD_CD,1,5)  FM_NOD_CD
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('TO' , A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     TO_NOD_CD     
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('VIA', A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     VIA_NOD_CD   
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('DOR', A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     DOR_NOD_CD
                          ,(SELECT WK.COST_YR||WK.COST_WK||'$'
                                  ||WK.SLS_FM_DT||'$'
                                  ||WK.SLS_TO_DT||'$'
                              FROM MAS_WK_PRD WK
                             WHERE A.N1ST_NOD_PLN_DT BETWEEN TO_DATE (WK.SLS_FM_DT, 'rrrrmmdd') AND TO_DATE (WK.SLS_TO_DT, 'rrrrmmdd') + 0.99999
                           ) WK
                          ,A.N1ST_NOD_PLN_DT DT
                          ,B.BKG_NO
                          ,B.CNTR_NO
                      FROM SCE_PLN_SO_LIST A
                          ,SCE_COP_HDR     B
                          #if($arr_so_no.size() > 0 || $arr_wo_no.size() > 0) 
                            ,TRS_TRSP_SVC_ORD SO
                          #end
                     WHERE A.COP_NO = B.COP_NO
                          #if($arr_so_no.size() > 0) 
                            AND A.COP_NO = SO.COP_NO
                            AND A.COST_ACT_GRP_SEQ = SO.COST_ACT_GRP_SEQ
                            AND SO.SO_NO IN (
                              #foreach( ${key} IN ${arr_so_no}) 
                                #if($velocityCount == 1) 
                                  ( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #else
                                 ,( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #end
                              #end
                              )
                          #end
    
                          #if($arr_wo_no.size() > 0) 
                            AND A.COP_NO = SO.COP_NO
                            AND A.COST_ACT_GRP_SEQ = SO.COST_ACT_GRP_SEQ
                            AND SO.WO_NO IN (
                              #foreach( ${key} IN ${arr_wo_no}) 
                                #if($velocityCount == 1) 
                                  ( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #else
                                 ,( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #end
                              #end
                              )
                          #end
    
                          #if($arr_ofc_cd.size() > 0) 
                            AND A.CTRL_OFC_CD IN (
                              #foreach( ${key} in ${arr_ofc_cd}) 
                                #if($velocityCount < $arr_ofc_cd.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if($arr_bkg_no.size() > 0) 
                            AND B.BKG_NO IN (
                              #foreach( ${key} in ${arr_bkg_no}) 
                                #if($velocityCount < $arr_bkg_no.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if($arr_cntr_no.size() > 0) 
                            AND B.CNTR_NO IN (
                              #foreach( ${key} in ${arr_cntr_no}) 
                                #if($velocityCount < $arr_cntr_no.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if (${sBndCd} != '' && ${sBndCd} != 'ALL')
                            AND A.PCTL_IO_BND_CD = @[sBndCd]
                          #end
    
                          #if (${sStsCd} != '') 
                            #if (${sStsCd} == 'P')
                              AND A.TRSP_SO_STS_CD = 'P'
                            #elseif (${sStsCd} == 'C')
                              AND A.TRSP_SO_STS_CD IN ('C', 'R')
                            #elseif (${sStsCd} == 'I')
                              AND A.TRSP_SO_STS_CD IN ('I')
                            #end
                          #else
                            AND A.TRSP_SO_STS_CD LIKE '%'
                          #end
                       AND B.COP_STS_CD <> 'M'
                       AND B.COP_STS_CD <> 'O'
                       AND B.COP_STS_CD <> 'X'
                       AND DECODE (B.MST_COP_NO, B.COP_NO, 'P', 'X') = 'P'
                       AND A.TRSP_SO_STS_CD LIKE '%'
                       AND A.PCTL_COST_MOD_CD LIKE '%'           
                       AND A.N1ST_NOD_PLN_DT BETWEEN TO_DATE(@[sFromDt], 'YYYYMMDD') AND TO_DATE(@[sToDt], 'YYYYMMDD')
                       AND (A.PCTL_COST_MOD_CD = 'C' OR (A.PCTL_COST_MOD_CD = 'Z' AND A.PCTL_IO_BND_CD = 'O' AND B.CFM_OB_DOR_ARR_DT IS NULL) OR (A.PCTL_COST_MOD_CD = 'Z' AND A    .PCTL_IO_BND_CD = 'I' AND B.CFM_APNT_DT IS NULL))
                 UNION ALL
                    SELECT 
                          #if ($arr_bkg_no.size() > 0)
                            /*+ INDEX (B XAK1SCE_COP_HDR) */
                          #elseif ($arr_cntr_no.size() > 0)
                            /*+ INDEX (B XAK9SCE_COP_HDR) ORDERED */
                          #else
                            /*+ INDEX (B XAK7SCE_COP_HDR) */
                          #end
                           A.CTRL_OFC_CD
                          ,A.TRSP_SO_STS_CD
                          ,A.PCTL_COST_MOD_CD -- C: CY, Z: ZONE
                          ,A.N1ST_NOD_PLN_DT
                          ,A.COP_NO
                          ,A.COST_ACT_GRP_SEQ
                          ,A.PCTL_IO_BND_CD
                          ,SUBSTR(A.N1ST_NOD_CD,1,5)  FM_NOD_CD
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('TO' , A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     TO_NOD_CD     
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('VIA', A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     VIA_NOD_CD   
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('DOR', A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     DOR_NOD_CD
                          ,(SELECT WK.COST_YR||WK.COST_WK||'$'
                                  ||WK.SLS_FM_DT||'$'
                                  ||WK.SLS_TO_DT||'$'
                              FROM MAS_WK_PRD WK
                             WHERE B.CFM_OB_DOR_ARR_DT BETWEEN TO_DATE (WK.SLS_FM_DT, 'rrrrmmdd') AND TO_DATE (WK.SLS_TO_DT, 'rrrrmmdd') + 0.99999
                           ) WK
                          ,B.CFM_OB_DOR_ARR_DT DT
                          ,B.BKG_NO
                          ,B.CNTR_NO
                      FROM SCE_PLN_SO_LIST A
                          ,SCE_COP_HDR     B
                          #if($arr_so_no.size() > 0 || $arr_wo_no.size() > 0) 
                            ,TRS_TRSP_SVC_ORD SO
                          #end
                     WHERE A.COP_NO = B.COP_NO
                          #if($arr_so_no.size() > 0) 
                            AND A.COP_NO = SO.COP_NO
                            AND A.COST_ACT_GRP_SEQ = SO.COST_ACT_GRP_SEQ
                            AND SO.SO_NO IN (
                              #foreach( ${key} IN ${arr_so_no}) 
                                #if($velocityCount == 1) 
                                  ( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #else
                                 ,( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #end
                              #end
                              )
                          #end
    
                          #if($arr_wo_no.size() > 0) 
                            AND A.COP_NO = SO.COP_NO
                            AND A.COST_ACT_GRP_SEQ = SO.COST_ACT_GRP_SEQ
                            AND SO.WO_NO IN (
                              #foreach( ${key} IN ${arr_wo_no}) 
                                #if($velocityCount == 1) 
                                  ( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #else
                                 ,( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #end
                              #end
                              )
                          #end
    
                          #if($arr_ofc_cd.size() > 0) 
                            AND A.CTRL_OFC_CD IN (
                              #foreach( ${key} in ${arr_ofc_cd}) 
                                #if($velocityCount < $arr_ofc_cd.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if($arr_bkg_no.size() > 0) 
                            AND B.BKG_NO IN (
                              #foreach( ${key} in ${arr_bkg_no}) 
                                #if($velocityCount < $arr_bkg_no.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if($arr_cntr_no.size() > 0) 
                            AND B.CNTR_NO IN (
                              #foreach( ${key} in ${arr_cntr_no}) 
                                #if($velocityCount < $arr_cntr_no.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if (${sBndCd} != '' && ${sBndCd} != 'ALL')
                            AND A.PCTL_IO_BND_CD = @[sBndCd]
                          #end
    
                          #if (${sStsCd} != '') 
                            #if (${sStsCd} == 'P')
                              AND A.TRSP_SO_STS_CD = 'P'
                            #elseif (${sStsCd} == 'C')
                              AND A.TRSP_SO_STS_CD IN ('C', 'R')
                            #elseif (${sStsCd} == 'I')
                              AND A.TRSP_SO_STS_CD IN ('I')
                            #end
                          #else
                            AND A.TRSP_SO_STS_CD LIKE '%'
                          #end
                       AND B.COP_STS_CD <> 'M'
                       AND B.COP_STS_CD <> 'O'
                       AND B.COP_STS_CD <> 'X'
                       AND DECODE (B.MST_COP_NO, B.COP_NO, 'P', 'X') = 'P'
                       AND A.PCTL_IO_BND_CD = 'O'
                       AND A.PCTL_COST_MOD_CD = 'Z'
                       AND B.CFM_OB_DOR_ARR_DT BETWEEN TO_DATE(@[sFromDt], 'YYYYMMDD') AND TO_DATE(@[sToDt], 'YYYYMMDD')
                 UNION ALL
                    SELECT 
                          #if ($arr_bkg_no.size() > 0)
                            /*+ INDEX (B XAK1SCE_COP_HDR) */
                          #elseif ($arr_cntr_no.size() > 0)
                            /*+ INDEX (B XAK9SCE_COP_HDR) ORDERED */
                          #else
                            /*+ INDEX (B XAK13SCE_COP_HDR) */
                          #end
                           A.CTRL_OFC_CD
                          ,A.TRSP_SO_STS_CD
                          ,A.PCTL_COST_MOD_CD -- C: CY, Z: ZONE
                          ,A.N1ST_NOD_PLN_DT
                          ,A.COP_NO
                          ,A.COST_ACT_GRP_SEQ
                          ,A.PCTL_IO_BND_CD                    
                          ,SUBSTR(A.N1ST_NOD_CD,1,5)  FM_NOD_CD
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('TO' , A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     TO_NOD_CD     
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('VIA', A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     VIA_NOD_CD   
                          ,TRS_CYDOOR_COMM_PKG.GET_SO_NODE_FNC('DOR', A.PCTL_IO_BND_CD, A.PCTL_COST_MOD_CD, A.N1ST_NOD_CD, A.N2ND_NOD_CD, A.N3RD_NOD_CD, A.N4TH_NOD_CD, 'NODE')     DOR_NOD_CD
                          ,(SELECT WK.COST_YR||WK.COST_WK||'$'
                                  ||WK.SLS_FM_DT||'$'
                                  ||WK.SLS_TO_DT||'$'
                              FROM MAS_WK_PRD WK
                             WHERE B.CFM_APNT_DT BETWEEN TO_DATE (WK.SLS_FM_DT, 'rrrrmmdd') AND TO_DATE (WK.SLS_TO_DT, 'rrrrmmdd') + 0.99999
                           ) WK
                          ,B.CFM_APNT_DT DT
                          ,B.BKG_NO
                          ,B.CNTR_NO
                      FROM SCE_PLN_SO_LIST A
                          ,SCE_COP_HDR     B
                          #if($arr_so_no.size() > 0 || $arr_wo_no.size() > 0) 
                            ,TRS_TRSP_SVC_ORD SO
                          #end
                     WHERE A.COP_NO = B.COP_NO
                          #if($arr_so_no.size() > 0) 
                            AND A.COP_NO = SO.COP_NO
                            AND A.COST_ACT_GRP_SEQ = SO.COST_ACT_GRP_SEQ
                            AND SO.SO_NO IN (
                              #foreach( ${key} IN ${arr_so_no}) 
                                #if($velocityCount == 1) 
                                  ( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #else
                                 ,( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #end
                              #end
                              )
                          #end
    
                          #if($arr_wo_no.size() > 0) 
                            AND A.COP_NO = SO.COP_NO
                            AND A.COST_ACT_GRP_SEQ = SO.COST_ACT_GRP_SEQ
                            AND SO.WO_NO IN (
                              #foreach( ${key} IN ${arr_wo_no}) 
                                #if($velocityCount == 1) 
                                  ( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #else
                                 ,( SUBSTR('${key}',1,3),SUBSTR('${key}',4) )
                                #end
                              #end
                              )
                          #end
    
                          #if($arr_ofc_cd.size() > 0) 
                            AND A.CTRL_OFC_CD IN (
                              #foreach( ${key} in ${arr_ofc_cd}) 
                                #if($velocityCount < $arr_ofc_cd.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if($arr_bkg_no.size() > 0) 
                            AND B.BKG_NO IN (
                              #foreach( ${key} in ${arr_bkg_no}) 
                                #if($velocityCount < $arr_bkg_no.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if($arr_cntr_no.size() > 0) 
                            AND B.CNTR_NO IN (
                              #foreach( ${key} in ${arr_cntr_no}) 
                                #if($velocityCount < $arr_cntr_no.size()) 
                                  UPPER('${key}'), 
                                #else 
                                  UPPER('${key}')
                                #end 
                              #end
                            )
                          #end
    
                          #if (${sBndCd} != '' && ${sBndCd} != 'ALL')
                            AND A.PCTL_IO_BND_CD = @[sBndCd]
                          #end
    
                          #if (${sStsCd} != '') 
                            #if (${sStsCd} == 'P')
                              AND A.TRSP_SO_STS_CD = 'P'
                            #elseif (${sStsCd} == 'C')
                              AND A.TRSP_SO_STS_CD IN ('C', 'R')
                            #elseif (${sStsCd} == 'I')
                              AND A.TRSP_SO_STS_CD IN ('I')
                            #end
                          #else
                            AND A.TRSP_SO_STS_CD LIKE '%'
                          #end
                       AND B.COP_STS_CD <> 'M'
                       AND B.COP_STS_CD <> 'O'
                       AND B.COP_STS_CD <> 'X'
                       AND DECODE (B.MST_COP_NO, B.COP_NO, 'P', 'X') = 'P'
                       AND A.PCTL_IO_BND_CD = 'I'
                       AND A.PCTL_COST_MOD_CD = 'Z'
                       AND B.CFM_APNT_DT BETWEEN TO_DATE(@[sFromDt], 'YYYYMMDD') AND TO_DATE(@[sToDt], 'YYYYMMDD')
                    ) AA
             WHERE 1=1
            #if (${sDrNm} != '') 
              AND EXISTS (SELECT 1 
                            FROM MDM_ZONE X
                           WHERE X.ZN_CD BETWEEN DOR_NOD_CD||'01' AND DOR_NOD_CD||'99'
                             AND UPPER(ZN_NM) LIKE UPPER('%'|| @[sDrNm] ||'%')   
                         )
            #end
            #if (${sFmLoc} != '') 
              AND FM_NOD_CD LIKE @[sFmLoc]||'%'
            #end
            #if (${sToLoc} != '') 
              AND TO_NOD_CD LIKE @[sToLoc]||'%'
            #end
            #if (${sViaLoc} != '') 
              AND VIA_NOD_CD LIKE @[sViaLoc]||'%'
            #end
            #if (${sDrLoc} != '') 
              AND DOR_NOD_CD LIKE @[sDrLoc]||'%'
            #end
             ) AAA
        WHERE 1=1
            #if (${sDoYn} == 'Y') 
              AND DO_NO IS NOT NULL
            #elseif (${sDoYn} == 'N') 
              AND DO_NO IS NULL
            #end
    GROUP BY WK
          ,CTRL_OFC_CD
          ,PCTL_COST_MOD_CD
          ,PCTL_IO_BND_CD
          ,FM_NOD_CD
          ,TO_NOD_CD
          ,VIA_NOD_CD
          ,DOR_NOD_CD
    )			]]></sql>
