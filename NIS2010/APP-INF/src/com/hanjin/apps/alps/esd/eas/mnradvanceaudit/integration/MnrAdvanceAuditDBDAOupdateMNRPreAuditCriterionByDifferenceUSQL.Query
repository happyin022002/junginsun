<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MnrAdvanceAuditDBDAOupdateMNRPreAuditCriterionByDifferenceUSQL">
			<desc><![CDATA[EAS_MNR_PRE_AUD_RTO_CFG 의 Auditing Flag 변경]]></desc>
			<sql><![CDATA[
UPDATE EAS_MNR_PRE_AUD_RTO_CFG
SET 
    MNR_VRFY_TP_AUD_FLG = DECODE(@[obj_pre_aud], '1','Y','0','N')
	, EXPN_MAX_PRMT_RTO = @[ratio]
	, VRFY_RMK = @[aut_rmk]
    , UPD_OFC_CD = @[upt_ofc_cd]
    , UPD_USR_ID = @[upd_usr_id]
    , UPD_DT = SYSDATE 
WHERE
    AUD_OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="obj_pre_aud" type="12" value="" out="N"/>
				<param name="ratio" type="12" value="" out="N"/>
				<param name="aut_rmk" type="12" value="" out="N"/>
				<param name="upt_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
