<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOSearchDwellEmailListRSQL">
			<desc><![CDATA[Dwell Emailing list를 조회]]></desc>
			<sql><![CDATA[
SELECT
SUBSC_EML
, EML_FM_SRC_NM
, COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD00960', B.EML_PROC_STS_CD) EML_STS
--, TO_CHAR(A.CRE_DT,'YYYY-MM-DD HH24:MI:SS') EML_SND_DT
, TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS', A.CRE_DT, 'USNYC'), 'YYYY-MM-DD HH24:MI:SS') EML_SND_DT
FROM SCE_DWLL_NTFC_EML_SND_RSLT A
, COM_EML_SND_INFO B
WHERE 1=1
AND A.EML_SND_NO = B.EML_SND_NO
#if( ${eml_addr} != '' )
AND A.SUBSC_EML LIKE '%'||@[eml_addr]||'%'
#end
#if( ${eml_snd_dt} != '' )
AND A.EML_SND_DT = TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('USNYC', TO_DATE(@[eml_snd_dt] ||'0300', 'YYYYMMDDHH24MI'), 'KRPUS'), 'YYYYMMDD')
#end
#if( ${cust_cd} != '' )
and A.DWLL_CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
and A.DWLL_CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))
#end
#if( ${cntr_no} != '' )
AND A.CNTR_NO = @[cntr_no]
#end
GROUP BY
SUBSC_EML
, EML_FM_SRC_NM
, B.EML_PROC_STS_CD
, A.CRE_DT			]]></sql>
			<params>
				<param name="eml_addr" type="12" value="" out="N"/>
				<param name="eml_snd_dt" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
