<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonPopUpManageDBDAOSearchServiceOfficeCodeManageRSQL">
			<desc><![CDATA[SearchServiceOfficeCodeManage]]></desc>
			<sql><![CDATA[
SELECT                                                              
		 	LEVEL, DECODE(LEVEL,                                              
		 	1, OFC_CD,                                                        
		 	2, '      '||OFC_CD,                                              
		 	3, '            '||OFC_CD,                                        
		 	4, '                  '||OFC_CD,                                  
		 	5, '                        '||OFC_CD,                            
		 	6, '                           '||OFC_CD,                         
		 	7, '                                 '||OFC_CD,                   
		 	8, '                                       '||OFC_CD,             
		 	9, '                                             '||OFC_CD,       
		 	10, '                                                  '||OFC_CD, 
		 	'') OFC_CD_NAME, OFC_CD																						
		 FROM MDM_ORGANIZATION                                               
		 WHERE DELT_FLG = 'N'                                                
		 CONNECT BY NOCYCLE PRIOR OFC_CD = PRNT_OFC_CD 
         START WITH OFC_CD like UPPER(@[sel_ofc_cd]) || '%'
		 ORDER SIBLINGS BY OFC_CD			]]></sql>
			<params>
				<param name="sel_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
