<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QualitativeEvaluationDBDAOMultiQualitativeEvaluationUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE SPE_SP_QUAL_EV
   SET EV_GRD_CHK_CD   = @[ev_grd_chk_cd]                                               
     , EV_WGT_RSLT_RT  = @[ev_wgt_rslt_rt]        
     , UPD_USR_ID      = @[cre_usr_id]                 
     , UPD_DT          = SYSDATE     
WHERE  1=1
AND EV_YR 		  = @[ev_yr]
AND EV_MON		  = @[ev_mon]
AND EG_ID 		  = @[eg_id]
AND SP_KPI_ID	  = @[sp_kpi_id]
AND SP_SEQ 	      = @[sp_seq]
AND QUAL_EV_SEQ   = @[qual_ev_seq]
AND EVR_USR_ID    = @[cre_usr_id]			]]></sql>
			<params>
				<param name="ev_grd_chk_cd" type="12" value="" out="N"/>
				<param name="ev_wgt_rslt_rt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="ev_yr" type="12" value="" out="N"/>
				<param name="ev_mon" type="12" value="" out="N"/>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="sp_kpi_id" type="12" value="" out="N"/>
				<param name="sp_seq" type="12" value="" out="N"/>
				<param name="qual_ev_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
