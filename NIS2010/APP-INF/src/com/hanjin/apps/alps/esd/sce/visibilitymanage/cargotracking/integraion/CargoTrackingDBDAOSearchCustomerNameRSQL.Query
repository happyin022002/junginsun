<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CargoTrackingDBDAOSearchCustomerNameRSQL">
			<desc><![CDATA[for Selection searchCustomerName]]></desc>
			<sql><![CDATA[
SELECT  cust_lgl_eng_nm  FROM    mdm_customer
where   1=1

#if(${cust_value1} != '')
AND cust_cnt_cd = @[cust_value1]
#end

#if(${cust_value2} != '')
AND     cust_seq    = @[cust_value2]
#end			]]></sql>
			<params>
				<param name="cust_value1" type="12" value="" out="N"/>
				<param name="cust_value2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
