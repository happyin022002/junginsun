<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceEdiHitDBDAOSearchEdiOrgInvRSQL">
			<desc><![CDATA[PDF 파일 맵핑]]></desc>
			<sql><![CDATA[
SELECT COUNT(A.INV_NO) INV_CNT
  FROM TRS_INV_EDI_RCV A
      ,TRS_INV_EDI_RCV_FILE B
 WHERE A.INV_NO = B.INV_NO(+)
   AND A.INV_VNDR_SEQ = B.INV_VNDR_SEQ(+)
   AND A.INV_NO = @[inv_no]
   AND A.INV_VNDR_SEQ = @[inv_vndr_seq]
   AND B.FILE_NM(+) = @[file_nm]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_vndr_seq" type="12" value="" out="N"/>
				<param name="file_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
