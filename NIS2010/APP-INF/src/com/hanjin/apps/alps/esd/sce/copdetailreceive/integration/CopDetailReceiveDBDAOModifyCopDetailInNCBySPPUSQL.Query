<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDetailInNCBySPPUSQL">
			<desc><![CDATA[ModifyCopDetailInNCBySPP]]></desc>
			<sql><![CDATA[
            UPDATE sce_cop_dtl
            SET    act_sts_cd = 'C'
            WHERE  cop_no      = @[v_cop_no]
            AND    cop_dtl_seq = @[v_cop_dtl_seq]
            AND    act_sts_cd  = 'N'			]]></sql>
			<params>
				<param name="v_cop_no" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
