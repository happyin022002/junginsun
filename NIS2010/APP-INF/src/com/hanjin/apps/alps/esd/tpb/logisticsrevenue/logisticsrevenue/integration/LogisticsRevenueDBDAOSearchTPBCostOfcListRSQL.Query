<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LogisticsRevenueDBDAOSearchTPBCostOfcListRSQL">
			<desc><![CDATA[SearchTPBCostOfcList]]></desc>
			<sql><![CDATA[
SELECT   OFC_CD
FROM     TPB_HNDL_OFC
WHERE    1 = 1
AND      N3PTY_OFC_TP_CD = 'T'
AND      DELT_FLG = 'N'
AND      (
              N3PTY_CTRL_OFC_CD = @[ofc_cd]
           OR OFC_CD = @[ofc_cd]
         )
ORDER BY OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
