<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSearch03SingleTransportationSOManageRSQL">
			<desc><![CDATA[TRS_TRSP_SVC_ORD, EQR_REPO_EXE_SO_IF, MST_CONTAINER  테이블 조회]]></desc>
			<sql><![CDATA[
SELECT   X.REPO_PLN_ID
		,X.PLN_YRWK
		,X.REF_ID
		,X.REF_SEQ
		,X.TRSP_SO_OFC_CTY_CD
		,X.TRSP_SO_SEQ
		,X.REPO_PURP_RMK
		,X.FM_NOD_CD
		,X.TO_NOD_CD
		,X.TRSP_SO_CMB_SEQ
		,X.TRSP_COST_DTL_MOD_NAME
		,X.EQ_NO
		,X.EQ_TPSZ_CD
		,X.TRSP_CRR_MOD_CD
		,X.LESSOR
		,X.LSTM_CD
		,X.OWNR_CO_CD
		,X.EQ_USED
		,X.MOVEMENT_STS
		,X.CREATION_YARD
		,X.EVENT_DATE
        ,X.SPOT_BID_FLG
        ,X.SPOT_BID_NO
        ,X.SPOT_BID_DUE_DT
		,X.TRSP_MTY_RQST_DT
		,X.INTER_RMK
		,X.SPCL_INSTR_RMK
		,X.CRE_USR_ID
		,X.UPD_USR_ID
        ,CASE WHEN X.WO_NO IS NULL
                 THEN TO_NUMBER(SUBSTR(X.PRD_DIST, INSTR(X.PRD_DIST, '/') + 1))  
              ELSE X.TTL_DIST 
         END AS TTL_DIST 
        ,CASE WHEN X.WO_NO IS NULL
                 THEN SUBSTR(X.PRD_DIST, 1, INSTR(X.PRD_DIST, '/') - 1)
              ELSE  X.LNK_DIST_DIV_CD 
        END AS LNK_DIST_DIV_CD 
FROM ( SELECT  A.REPO_PLN_ID
              ,A.PLN_YRWK
			  ,A.REF_ID
              ,A.REF_SEQ
              ,A.TRSP_SO_OFC_CTY_CD
			  ,A.TRSP_SO_SEQ
			  ,B.REPO_PURP_RMK
			  ,A.FM_NOD_CD
			  ,A.TO_NOD_CD
			  ,A.TRSP_SO_CMB_SEQ
			  ,DECODE(A.TRSP_COST_DTL_MOD_CD, 'CN', 'On-Hire', 'CF', 'Off-Hire', 'Empty Repo') TRSP_COST_DTL_MOD_NAME
			  ,A.EQ_NO
			  ,A.EQ_TPSZ_CD
			  ,A.TRSP_CRR_MOD_CD
			  ,(C.AGMT_CTY_CD||C.AGMT_SEQ) LESSOR
			  ,C.LSTM_CD
			  ,A.OWNR_CO_CD
			  ,C.CNTR_USE_CO_CD EQ_USED
			  ,C.CNMV_STS_CD MOVEMENT_STS
			  ,C.CRNT_YD_CD CREATION_YARD
			  ,TO_CHAR(C.CNMV_DT, 'YYYY-MM-DD') EVENT_DATE
			  ,TO_CHAR(A.TRSP_MTY_RQST_DT, 'YYYYMMDDHH24MISS') TRSP_MTY_RQST_DT
			  ,A.INTER_RMK
			  ,A.SPCL_INSTR_RMK
			  ,A.CRE_USR_ID
			  ,A.UPD_USR_ID
			  ,A.TTL_DIST, A.LNK_DIST_DIV_CD
	 		  ,A.TRSP_WO_OFC_CTY_CD || TRSP_WO_SEQ WO_NO
	          ,TRS_COMMON_PKG.GET_PRD_DISTANCE_FNC (A.FM_NOD_CD, A.TO_NOD_CD, A.VIA_NOD_CD, A.DOR_NOD_CD, A.TRSP_BND_CD, A.TRSP_COST_DTL_MOD_CD, A.TRSP_CRR_MOD_CD) PRD_DIST
              ,NVL(A.SPOT_BID_FLG,'N') SPOT_BID_FLG
              ,A.SPOT_BID_NO
              ,TO_CHAR(A.SPOT_BID_DUE_DT,'YYYY-MM-DD HH24:MI:SS') SPOT_BID_DUE_DT
		  FROM TRS_TRSP_SVC_ORD A
              ,EQR_REPO_EXE_SO_IF B
              ,MST_CONTAINER C 
 		WHERE A.REPO_PLN_ID = B.REPO_PLN_ID 
		  AND A.PLN_YRWK = B.PLN_YRWK 
 		  AND A.REF_ID = B.REF_ID 
  		  AND A.REF_SEQ = B.REF_SEQ 
  		  AND A.EQ_NO = C.CNTR_NO(+) 
		  AND A.TRSP_SO_TP_CD = 'M' 
		  AND A.CRE_OFC_CD = @[sctrlofccd] 
		  AND A.TRSP_SO_STS_CD IN ('C', 'R') 
		  AND A.DELT_FLG = 'N' 
  		  AND A.HJL_NO IS NULL  
#if (${sfrmreqdate} != '' && ${sstoreqdate} != '')
	#if (${srad_date} == 'R')
		AND A.TRSP_MTY_RQST_DT BETWEEN TO_DATE(@[sfrmreqdate], 'YYYYMMDD') AND TO_DATE(@[sstoreqdate], 'YYYYMMDD') + 0.99999
	#elseif (${srad_date} == 'S')
		AND A.LOCL_CRE_DT BETWEEN TO_DATE(@[sfrmreqdate], 'YYYYMMDD') AND TO_DATE(@[sstoreqdate], 'YYYYMMDD') + 0.99999 	
	#elseif (${srad_date} == 'B')
		AND A.SPOT_BID_DUE_DT BETWEEN TO_DATE(@[sfrmreqdate], 'YYYYMMDD') AND TO_DATE(@[sstoreqdate], 'YYYYMMDD') + 0.99999 	
	#end 	  
#end 

#if (${chk} == 'OK' && ((${skind} != 'ALL') || (${stransmodcd} != 'ALL') || (${sfrmnode} != '') || (${stonode} != '') || ($arrCntr.size() > 0)  || ($arrReferencno.size() > 0))) 
 AND ( 1=1
#end

#if (${skind} != 'ALL')
	AND (A.TRSP_MTY_COST_MOD_CD = @[skind])
#end

#if (${stransmodcd} != 'ALL')
 AND (A.TRSP_CRR_MOD_CD = @[stransmodcd]) 
#end

#if (${sfrmnode} != '')
	AND (A.FM_NOD_CD LIKE @[sfrmnode]||'%' )
#end

#if (${stonode} != '')
	AND (A.TO_NOD_CD LIKE @[stonode]||'%' )
#end
#if (${spotBidFlg} != '')
	AND A.SPOT_BID_FLG = @[spotBidFlg]
#end
#if (${spotBidNo} != '')
	AND A.SPOT_BID_NO = @[spotBidNo]
#end

#if ($arrCntr.size() > 0) 
   AND (A.EQ_NO IN (
#foreach($cntrKey in ${arrCntr}) 
	#if($velocityCount < $arrCntr.size()) 
		'$cntrKey',
	#else 
		'$cntrKey'
	#end 
#end 
  ))                             
#end 

#if ($arrReferencno.size() > 0) 
   AND (A.REF_ID IN (
#foreach($referKey in ${arrReferencno}) 
	#if($velocityCount < $arrReferencno.size()) 
		'$referKey',
	#else 
		'$referKey'
	#end 
#end 
  ))                             
#end

#if (${chk} == 'OK' && ((${skind} != 'ALL') || (${stransmodcd} != 'ALL') || (${sfrmnode} != '') || (${stonode} != '') || ($arrCntr.size() > 0)  || ($arrReferencno.size() > 0))) 
OR TRSP_SO_CMB_SEQ IN ( 
	#foreach($strcmbKey IN ${arrStrcmbseq}) 
		$strcmbKey,
	#end  
	 0 ))
#end
	) X
ORDER BY X.TRSP_SO_CMB_SEQ			]]></sql>
			<params>
				<param name="sctrlofccd" type="12" value="1" out="N"/>
				<param name="sfrmreqdate" type="12" value="20090909" out="N"/>
				<param name="sstoreqdate" type="12" value="20090909" out="N"/>
				<param name="skind" type="12" value="" out="N"/>
				<param name="stransmodcd" type="12" value="" out="N"/>
				<param name="sfrmnode" type="12" value="M" out="N"/>
				<param name="stonode" type="12" value="" out="N"/>
				<param name="spotBidFlg" type="12" value="" out="N"/>
				<param name="spotBidNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
