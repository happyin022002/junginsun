<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifyOdSoStsPlnedUSQL">
			<desc><![CDATA[od-so 의 status 를 planned 로 원복한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_PLN_SO_LIST SET
	TRSP_SO_STS_CD = 'P'
WHERE
	COP_NO = @[cop_no]
	AND COST_ACT_GRP_CD LIKE 'OD%'			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
