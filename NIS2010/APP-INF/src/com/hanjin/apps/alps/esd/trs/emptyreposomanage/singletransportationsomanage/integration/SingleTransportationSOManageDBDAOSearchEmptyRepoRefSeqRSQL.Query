<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSearchEmptyRepoRefSeqRSQL">
			<desc><![CDATA[EQR_REPO_EXE_SO_IF의 REF_ID 생성]]></desc>
			<sql><![CDATA[
SELECT 'H'
       ||@[fm_loc_cd]
       ||SUBSTR(@[pln_yrwk],3,4)
       ||'W'     
       ||LPAD(TRS_TRSP_MT_REF_ID_SEQ1.NEXTVAL,3,0)     
       ||@[so_if_div_cd]   REF_ID 
  FROM DUAL			]]></sql>
			<params>
				<param name="fm_loc_cd" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="so_if_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
