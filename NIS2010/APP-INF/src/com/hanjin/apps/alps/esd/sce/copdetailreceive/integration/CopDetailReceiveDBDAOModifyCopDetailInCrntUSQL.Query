<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDetailInCrntUSQL">
			<desc><![CDATA[ModifyCopDetailInCrnt]]></desc>
			<sql><![CDATA[
update  sce_cop_dtl
           set  act_sts_cd      = 'F'
                ,act_dt         = to_date(@[in_act_dt],'YYYYMMDDHH24MISS')
                ,act_rcv_tp_cd  = @[in_act_rcv_tp_cd]
                ,upd_usr_id     = @[in_usr_id]
                ,upd_dt         = sysdate
                ,edi_msg_tp_cd  = @[v_edi_msg_tp_cd]
                ,vndr_seq       = @[v_vndr_seq]
                ,act_sts_mapg_cd = @[in_act_sts_mapg_cd]
                ,ACT_DAT_RCV_DT =  TO_DATE(@[v_org_act_rcv_dt],'YYYY/MM/DD HH24:MI:SS')
         where  act_sts_cd  = 'C'
           and  cop_no      = @[v_cop_no]
           and  cop_grp_seq = @[v_cop_grp_seq]
           and  cop_dtl_seq = @[v_cop_dtl_seq]			]]></sql>
			<params>
				<param name="in_act_dt" type="12" value="" out="N"/>
				<param name="in_act_rcv_tp_cd" type="12" value="" out="N"/>
				<param name="in_usr_id" type="12" value="" out="N"/>
				<param name="v_edi_msg_tp_cd" type="12" value="" out="N"/>
				<param name="v_vndr_seq" type="12" value="" out="N"/>
				<param name="in_act_sts_mapg_cd" type="12" value="" out="N"/>
				<param name="v_org_act_rcv_dt" type="12" value="" out="N"/>
				<param name="v_cop_no" type="12" value="" out="N"/>
				<param name="v_cop_grp_seq" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
