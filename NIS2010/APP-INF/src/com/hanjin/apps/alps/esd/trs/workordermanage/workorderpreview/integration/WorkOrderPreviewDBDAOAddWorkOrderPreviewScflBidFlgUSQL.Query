<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewScflBidFlgUSQL">
			<desc><![CDATA[AddWorkOrderPreviewScflBidFlg]]></desc>
			<sql><![CDATA[
UPDATE TRS_SPOT_BID_VNDR
   SET SCFL_BID_FLG = 'Y',
       LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd]),
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = SYSDATE
 WHERE 1=1
   AND SPOT_BID_NO = @[spot_bid_no]
   AND VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="spot_bid_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
