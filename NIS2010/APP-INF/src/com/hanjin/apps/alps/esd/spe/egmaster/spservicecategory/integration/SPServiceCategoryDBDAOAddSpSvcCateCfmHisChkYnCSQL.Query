<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SPServiceCategoryDBDAOAddSpSvcCateCfmHisChkYnCSQL">
			<desc><![CDATA[업체의 서비스 카테고리 정보를 사용자가 선택을 안한경우 히스토리 테이블에 체크안함으로 저장한다]]></desc>
			<sql><![CDATA[
INSERT INTO SPE_SP_SVC_CATE_STUP_HIS(SP_SEQ
                                   , EV_SVC_CATE_CD
                                   , SP_SVC_CATE_HIS_SEQ
                                   , SP_RGN_GRP_OFC_CD
                                   , SP_CTRL_OFC_CD
                                   , SP_CTRT_OFC_CD
                                   , INP_DT
                                   , DELT_FLG
                                   , CHK_FLG
                                   , CRE_USR_ID
                                   , CRE_DT
                                   , UPD_USR_ID
                                   , UPD_DT
                                  )VALUES(
                                     @[sp_seq]
                                   , ' '
                                   , (SELECT COUNT(1)+1 FROM SPE_SP_SVC_CATE_STUP_HIS WHERE SP_SEQ = @[sp_seq] AND EV_SVC_CATE_CD = NVL('',' '))
                                   , @[sp_grp_ofc_cd]
                                   , @[sp_ctrl_ofc_cd]
                                   , @[sp_ctrt_ofc_cd]
                                   , TO_CHAR(SYSDATE,'YYYYMMDD')
                                   , 'N'
                                   , 'Y'
                                   , @[cre_usr_id]
                                   , SYSDATE
                                   , @[upd_usr_id]
                                   , SYSDATE
                                  )			]]></sql>
			<params>
				<param name="sp_seq" type="12" value="" out="N"/>
				<param name="sp_grp_ofc_cd" type="12" value="" out="N"/>
				<param name="sp_ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="sp_ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
