<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifySceCopDetailVLScheduleByOceanUSQL">
			<desc><![CDATA[ModifySceCopDetailVLScheduleByOcean]]></desc>
			<sql><![CDATA[
UPDATE SCE_COP_DTL 
SET    ESTM_DT     = TO_DATE(@[estm_dt],'YYYY/MM/DD HH24:MI:SS')
      ,ESTM_GDT    = GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR(NOD_CD,1,5), TO_DATE(@[estm_dt],'YYYY/MM/DD HH24:MI:SS'), 'GMT')  
      ,UPD_USR_ID  = 'VPS_VL' 
      ,UPD_DT      = SYSDATE   
WHERE  COP_NO      = @[cop_no] 
AND    COP_DTL_SEQ = @[cop_dtl_seq]

			]]></sql>
			<params>
				<param name="estm_dt" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
