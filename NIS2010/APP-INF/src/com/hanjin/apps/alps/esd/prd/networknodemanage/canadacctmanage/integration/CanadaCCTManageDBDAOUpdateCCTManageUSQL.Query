			<sql><![CDATA[
UPDATE PRD_CND_TML_CCT_MGMT
   SET ARR_SUN_DYS = @[arr_sun_dys],
       ARR_MON_DYS = @[arr_mon_dys],
       ARR_TUE_DYS = @[arr_tue_dys],
       ARR_WED_DYS = @[arr_wed_dys],
       ARR_THU_DYS = @[arr_thu_dys],
       ARR_FRI_DYS = @[arr_fri_dys],
       ARR_SAT_DYS = @[arr_sat_dys],
       UPD_USR_ID  = @[upd_usr_id],
       CCT_HRMNT   = @[cct_hrmnt],
       UPD_DT = sysdate
 WHERE POR_CD = @[por_cd]
   AND POL_NOD_CD = @[pol_nod_cd]			]]></sql>
			<params>
				<param name="arr_sun_dys" type="12" value="" out="N"/>
				<param name="arr_mon_dys" type="12" value="" out="N"/>
				<param name="arr_tue_dys" type="12" value="" out="N"/>
				<param name="arr_wed_dys" type="12" value="" out="N"/>
				<param name="arr_thu_dys" type="12" value="" out="N"/>
				<param name="arr_fri_dys" type="12" value="" out="N"/>
				<param name="arr_sat_dys" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cct_hrmnt" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
