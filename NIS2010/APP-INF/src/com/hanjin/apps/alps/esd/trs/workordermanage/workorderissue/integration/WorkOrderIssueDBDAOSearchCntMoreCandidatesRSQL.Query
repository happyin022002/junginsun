<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderIssueDBDAOSearchCntMoreCandidatesRSQL">
			<desc><![CDATA[More CNT Candidates Search]]></desc>
			<sql><![CDATA[
SELECT B.VNDR_SEQ
      ,B.VNDR_LGL_ENG_NM  VNDR_NM
      ,A.CTRT_CUST_CNT_CD CUST_CNT_CD
      ,A.CTRT_CUST_SEQ    CUST_SEQ
      ,A.CUST_NOMI_TRKR_IND_CD CNT_TP_CD
  FROM TRS_CUST_NOMI_TRKR A
      ,MDM_VENDOR B
 WHERE A.VNDR_SEQ = B.VNDR_SEQ
   AND A.PRC_CTRT_TP_CD = DECODE(@[sc_no],'','R','S')
   AND A.PRC_CTRT_NO =NVL(@[sc_no],@[rfa_no])
   AND A.IO_BND_CD = @[trsp_bnd_cd]
   AND A.ORG_NOD_CD = CASE WHEN @[trsp_bnd_cd] = 'I' AND LENGTH(A.ORG_NOD_CD)= 5 THEN @[fm_nod_cd]
                           WHEN @[trsp_bnd_cd] = 'I' AND LENGTH(A.ORG_NOD_CD)= 7 THEN @[fm_nod_cd]||@[fm_nod_yard]
                           WHEN @[trsp_bnd_cd] = 'O' AND LENGTH(A.ORG_NOD_CD)= 5 THEN @[dor_nod_cd]
                           WHEN @[trsp_bnd_cd] = 'O' AND LENGTH(A.ORG_NOD_CD)= 7 THEN @[dor_nod_cd]||@[dor_nod_yard]
                       END 
   AND A.DEST_NOD_CD = CASE WHEN @[trsp_bnd_cd] = 'I' AND LENGTH(A.DEST_NOD_CD)= 5 THEN @[dor_nod_cd]
                            WHEN @[trsp_bnd_cd] = 'I' AND LENGTH(A.DEST_NOD_CD)= 7 THEN @[dor_nod_cd]||@[dor_nod_yard]
                            WHEN @[trsp_bnd_cd] = 'O' AND LENGTH(A.DEST_NOD_CD)= 5 THEN @[to_nod_cd]
                            WHEN @[trsp_bnd_cd] = 'O' AND LENGTH(A.DEST_NOD_CD)= 7 THEN @[to_nod_cd]||@[to_nod_yard]
						END 
   AND A.DISP_STS_CD = '03' --Approval status			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="trsp_bnd_cd" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yard" type="12" value="" out="N"/>
				<param name="dor_nod_cd" type="12" value="" out="N"/>
				<param name="dor_nod_yard" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_yard" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
