<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EDI315SendDBDAOSearchCopPlanDateRSQL">
			<desc><![CDATA[COP Detail 내의 EDI_STND_STS_CD에 해당하는 Plan Date를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(B.PLN_DT, 'YYYYMMDDHH24MISS') PLN_DT
  FROM SCE_COP_HDR A
     , SCE_COP_DTL B
 WHERE A.COP_NO = B.COP_NO
   AND A.BKG_NO = @[bkg_no]
   AND A.CNTR_NO = @[cntr_no]
   AND A.COP_STS_CD <> 'X'
   AND B.STND_EDI_STS_CD = @[stnd_edi_sts_cd]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="stnd_edi_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
