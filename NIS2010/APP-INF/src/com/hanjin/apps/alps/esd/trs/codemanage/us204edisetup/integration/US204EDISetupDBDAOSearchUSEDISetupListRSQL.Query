<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="US204EDISetupDBDAOSearchUSEDISetupListRSQL">
			<desc><![CDATA[US EDI Setup List 조회]]></desc>
			<sql><![CDATA[
SELECT 
    A.VNDR_SEQ,
    A.VNDR_LGL_ENG_NM,
    A.USA_EDI_CD,
    B.EDI_RCVR_NM,
    DECODE(B.EDI_RCVR_NM_USE_FLG, 'N', '0', 'Y', '1') EDI_RCVR_NM_USE_FLG,
    B.CRE_OFC_CD,
    B.UPD_USR_ID,
    TO_CHAR(B.UPD_DT, 'YYYYMMDD') UPD_DT
FROM MDM_VENDOR A, TRS_EDI_USA_RCVR_DTL B
WHERE 1=1
#if(${vndrSeq} != '')
    AND A.VNDR_SEQ = @[vndrSeq]
#end
    AND A.VNDR_SEQ = B.VNDR_SEQ(+)
    AND A.VNDR_CNT_CD = 'US'
    AND A.WO_EDI_USE_FLG = 'Y'
    AND A.USA_EDI_CD IS NOT NULL
    AND A.DELT_FLG = @[deltFlg]
ORDER BY EDI_RCVR_NM			]]></sql>
			<params>
				<param name="vndrSeq" type="12" value="" out="N"/>
				<param name="deltFlg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
