<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderRemarkDBDAOMultiWorkOrderRemarkCSQL">
			<desc><![CDATA[MultiWorkOrderRemark]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_WRK_ORD_INSTR	(
            WO_INSTR_OFC_CD    	,
            TRSP_BND_CD        	,
            TRSP_COST_MOD_CD   	,
            TRSP_CRR_MOD_CD    	,
            WO_INSTR_RMK       	,
            CRE_OFC_CD         	,
            CRE_USR_ID         	,
            CRE_DT             	,
            UPD_USR_ID			,
            UPD_DT             	,
			LOCL_CRE_DT			, 
			LOCL_UPD_DT	
) VALUES ( 	@[wo_instr_ofc_cd]  ,
           	@[trsp_bnd_cd]      ,
           	@[trsp_cost_mod_cd] ,
           	@[trsp_crr_mod_cd]  ,
           	@[wo_instr_rmk]     ,
           	@[cre_ofc_cd]       ,
           	@[cre_usr_id]       ,
          	SYSDATE				,
           	@[cre_usr_id]		, 
           	SYSDATE,              
 			GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), 
			GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
)			]]></sql>
			<params>
				<param name="wo_instr_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_bnd_cd" type="12" value="" out="N"/>
				<param name="trsp_cost_mod_cd" type="12" value="" out="N"/>
				<param name="trsp_crr_mod_cd" type="12" value="" out="N"/>
				<param name="wo_instr_rmk" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
