<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChAnalysisReportDBDAOSearchVvdRSQL">
			<desc><![CDATA[vvd 검색결과값을 리턴함]]></desc>
			<sql><![CDATA[
SELECT
		VSL_CD
FROM	VSK_VSL_SKD
WHERE	VSL_CD||SKD_VOY_NO||SKD_DIR_CD =  @[TRUNK_VVD]			]]></sql>
			<params>
				<param name="TRUNK_VVD" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
