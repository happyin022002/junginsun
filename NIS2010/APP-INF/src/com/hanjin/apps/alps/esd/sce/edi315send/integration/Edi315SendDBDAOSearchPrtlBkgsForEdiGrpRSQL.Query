<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchPrtlBkgsForEdiGrpRSQL">
			<desc><![CDATA[Edi315SendDBDAOSearchPrtlBkgsForEdiGrpRSQL]]></desc>
			<sql><![CDATA[
SELECT HDR.BKG_NO, B.BL_NO 
FROM SCE_COP_HDR HDR, BKG_BOOKING B 
WHERE HDR.BKG_NO = B.BKG_NO 
    AND (CNTR_NO,TRNK_VSL_CD,TRNK_SKD_VOY_NO,TRNK_SKD_DIR_CD) IN ( 
        SELECT 
            CASE WHEN CNTR_NO = 'SMCU0000000' THEN 'PSEUDO CNTR' ELSE CNTR_NO END AS CNTR_NO, 
            CASE WHEN TRNK_VSL_CD IN ('SMXX','SMYY','SMZZ') THEN 'PSEUDO VESSEL' ELSE TRNK_VSL_CD END AS TRNK_VSL_CD, 
            CASE WHEN TRNK_VSL_CD IN ('SMXX','SMYY','SMZZ') THEN 'PSEUDO VESSEL' ELSE TRNK_SKD_VOY_NO END AS TRNK_SKD_VOY_NO, 
            CASE WHEN TRNK_VSL_CD IN ('SMXX','SMYY','SMZZ') THEN 'PSEUDO VESSEL' ELSE TRNK_SKD_DIR_CD END AS TRNK_SKD_DIR_CD 
        FROM SCE_COP_HDR A 
        WHERE BKG_NO = @[bkg_no] 
            AND CNTR_NO = @[cntr_no] 
    )
    AND (
        EXISTS (
            SELECT '1'
            FROM BKG_CUSTOMER 
            WHERE BKG_NO = HDR.BKG_NO 
                AND (CUST_CNT_CD,CUST_SEQ) IN ( 
                    SELECT CUST_CNT_CD,CUST_SEQ
                    FROM EDI_GRP_CUST
                    WHERE EDI_GRP_CD IN (
                        SELECT EDI_GRP_CD
                        FROM EDI_GROUP
                        WHERE 1=1 
                            AND CUST_TRD_PRNR_ID IN (
								#foreach($ctpid IN ${cust_trd_prnr_id})
									#if($velocityCount == 1 ) 
										'$ctpid'
									#else 
										,'$ctpid' 
									#end 
								#end
							) 
                            AND CUST_CNT_CD IS NOT NULL
                        )
                     )
        )
        OR EXISTS (
            SELECT SC_NO
            FROM BKG_BOOKING
            WHERE BKG_NO = HDR.BKG_NO
                AND SC_NO IN (
                    SELECT SC_NO
                    FROM EDI_GRP_CUST
                    WHERE EDI_GRP_CD IN (
                        SELECT EDI_GRP_CD
                        FROM EDI_GROUP
                        WHERE 1=1 
                            AND CUST_TRD_PRNR_ID IN (
								#foreach($ctpid IN ${cust_trd_prnr_id})
									#if($velocityCount == 1 ) 
										'$ctpid'
									#else 
										,'$ctpid' 
									#end 
								#end
							) 
                            AND SC_NO IS NOT NULL
                    )
                )
            ) 
        )
UNION 
SELECT HDR.BKG_NO, B.BL_NO
FROM SCE_COP_HDR HDR, BKG_BOOKING B
WHERE HDR.BKG_NO = B.BKG_NO 
    AND HDR.BKG_NO = @[bkg_no] AND HDR.COP_STS_CD != 'X'
    AND ( HDR.CNTR_NO='SMCU0000000' OR TRNK_VSL_CD IN ('SMXX','SMYY','SMZZ') ) 
    AND ROWNUM=1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="PHX000319700" out="N"/>
				<param name="cntr_no" type="12" value="SMCU0000000" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
