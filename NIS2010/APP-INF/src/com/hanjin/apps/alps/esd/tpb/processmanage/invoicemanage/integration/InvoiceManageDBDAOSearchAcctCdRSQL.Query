<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOSearchAcctCdRSQL">
			<desc><![CDATA[2012.11.29 변종건 [CHM-201220985-01] [TPB] PSO에 대한 3자구상 개발관련 - Account 정보 조회]]></desc>
			<sql><![CDATA[
SELECT  ACCT_CD NAME
      , ACCT_ENG_NM VAL
FROM    MDM_ACCOUNT
WHERE   1 = 1
and     (
             ACCT_CD LIKE '5117%'
          OR ACCT_CD LIKE '5118%'
        )
AND     DELT_FLG = 'N'
AND     ESTM_TGT_FLG = 'Y'
ORDER BY ACCT_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
