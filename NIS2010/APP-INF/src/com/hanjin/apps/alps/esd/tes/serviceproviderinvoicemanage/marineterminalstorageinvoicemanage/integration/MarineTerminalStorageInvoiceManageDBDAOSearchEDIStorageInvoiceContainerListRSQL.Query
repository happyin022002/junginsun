<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalStorageInvoiceManageDBDAOSearchEDIStorageInvoiceContainerListRSQL">
			<desc><![CDATA[SearchEDIStorageInvoiceContainerList]]></desc>
			<sql><![CDATA[
SELECT
  C.CNTR_NO,
  C.CNTR_STY_CD,
  --// GI와 GO은 현재 숫자형태의 12자리만 받고 있다. 단, HIT/YICT는 8자리라 뒤에 0000를 붙여준다.
  CASE 
  WHEN C.INV_GATE_IN_TM_MSG IS NOT NULL AND LENGTH(C.INV_GATE_IN_TM_MSG) = 8 THEN C.INV_GATE_IN_TM_MSG||'0000' 
  WHEN C.INV_GATE_IN_TM_MSG IS NOT NULL AND LENGTH(C.INV_GATE_IN_TM_MSG) = 12 THEN C.INV_GATE_IN_TM_MSG
  END INV_GATE_IN_DT,
  CASE 
  WHEN C.INV_GATE_OUT_TM_MSG IS NOT NULL AND LENGTH(C.INV_GATE_OUT_TM_MSG) = 8 THEN C.INV_GATE_OUT_TM_MSG||'0000' 
  WHEN C.INV_GATE_OUT_TM_MSG IS NOT NULL AND LENGTH(C.INV_GATE_OUT_TM_MSG) = 12 THEN C.INV_GATE_OUT_TM_MSG
  END INV_GATE_OUT_DT,
  TES_GET_EDI_CNTR_TPSZ_CD_FNC(H.VNDR_SEQ,C.CNTR_TPSZ_CD,C.CNTR_NO) CNTR_TPSZ_CD
FROM TES_EDI_SO_HDR H, TES_EDI_SO_CNTR_LIST C
WHERE H.TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
AND H.TML_SO_SEQ = @[tml_so_seq]
AND NVL(H.DELT_FLG,'N') <> 'Y'
AND H.TML_INV_TP_CD IN ('SC','SE','ST') --// HIT/YICT는 SC와 SE로 Storage를.. 그 외의 S/P는 기본적으로 ST로 Storage를 나타낸다.
AND H.TML_EDI_SO_OFC_CTY_CD = C.TML_EDI_SO_OFC_CTY_CD
AND H.TML_EDI_SO_SEQ = C.TML_EDI_SO_SEQ			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
