<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanRouteManageDBDAOInsertOceanRouteCSQL">
			<desc><![CDATA[InsertOceanRoute]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_OCN_ROUT 
	( ORG_LOC_CD, DEST_LOC_CD, ROUT_SEQ, TS_IND_CD, LNK_KNT, OCN_ROUT_PRIO_CD, OCN_ROUT_PRIO_CNG_FLG, UPD_IND_CD,  
	N1ST_POL_CD, N1ST_POD_CD, N1ST_LANE_CD, N1ST_SKD_DIR_CD, N1ST_LANE_FDR_FLG, 
	N2ND_POL_CD, N2ND_POD_CD, N2ND_LANE_CD, N2ND_SKD_DIR_CD, N2ND_LANE_FDR_FLG, 
	N3RD_POL_CD, N3RD_POD_CD, N3RD_LANE_CD, N3RD_SKD_DIR_CD, N3RD_LANE_FDR_FLG, 
	N4TH_POL_CD, N4TH_POD_CD, N4TH_LANE_CD, N4TH_SKD_DIR_CD, N4TH_LANE_FDR_FLG, 
	TZTM_HRS,  
	N1ST_TZTM_HRS, N2ND_TZTM_HRS, N3RD_TZTM_HRS, N4TH_TZTM_HRS, 
	N1ST_STAY_TM_HRS, N2ND_STAY_TM_HRS, N3RD_STAY_TM_HRS, 
	PF_IND_CD, FDR_USD_FLG, OCN_ROUT_USR_RMK,OCN_ROUT_RMK, 
	CRE_OFC_CD, UPD_OFC_CD, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT, OCN_ROUT_UPD_DT,
    N1ST_LNK_DIST, N2ND_LNK_DIST ,N3RD_LNK_DIST ,N4TH_LNK_DIST,  OCN_ROUT_TMP_FLG,OCN_ROUT_TMP_EXP_DT ,
	OCN_ROUT_TMP_RMK
    )  
VALUES 
	( RTRIM(@[s_pol]), RTRIM(@[s_pod]),TO_NUMBER( @[max_seq] ) +1, @[ts_ind_cd], TO_NUMBER( @[s_lnk_cnt] ) , NVL(@[s_prior], '0'), 'Y', @[s_route_flg],  
	@[s_pol1],@[s_pod1],@[s_lane],@[s_dir1],@[s_fdr_flg1],   
	@[s_pol2],@[s_pod2],@[s_ts1_lane],@[s_dir2],@[s_fdr_flg2],  
	@[s_pol3],@[s_pod3],@[s_ts2_lane],@[s_dir3],@[s_fdr_flg3],  
	@[s_pol4],@[s_pod4],@[s_ts3_lane],@[s_dir4],@[s_fdr_flg4],  
	NVL(@[s_n1st_tztm_hrs], 0)+NVL(@[s_n2nd_tztm_hrs], 0)+NVL(@[s_n3rd_tztm_hrs], 0)+NVL(@[s_n4th_tztm_hrs], 0) ,  
	NVL(@[s_n1st_tztm_hrs], 0), NVL(@[s_n2nd_tztm_hrs], 0), NVL(@[s_n3rd_tztm_hrs], 0), NVL(@[s_n4th_tztm_hrs], 0),  
	NVL(@[s_n1st_stay_tm_hrs], 0), NVL(@[s_n2nd_stay_tm_hrs], 0), NVL(@[s_n3rd_stay_tm_hrs], 0),  
	'N', @[s_f_u], REPLACE(REPLACE(REPLACE(@[s_route_rmk], CHR(34)),CHR(9),' '), CHR(13)||CHR(10),' '), @[menu],  
	@[cre_ofc_cd],@[cre_ofc_cd],@[cre_usr_id], SYSDATE, @[upd_usr_id], SYSDATE, SYSDATE,
    ( SELECT NVL(DECODE(@[s_fdr_flg1],'Y', (SELECT STND_DIST FROM VSK_PORT_DIST WHERE FM_LOC_CD = @[s_pol1] AND TO_LOC_CD = @[s_pod1]), 
		(SELECT LNK_DIST FROM PRD_PF_TZ_TM WHERE VSL_SLAN_CD = @[s_lane] AND FM_PORT_CD = @[s_pol1] AND TO_PORT_CD = @[s_pod1] )),0) 
		FROM DUAL 
	 ), 
	 ( SELECT NVL(DECODE(@[s_fdr_flg2], 'Y', (SELECT STND_DIST FROM VSK_PORT_DIST WHERE FM_LOC_CD = @[s_pol2] AND TO_LOC_CD = @[s_pod2]), 
		(SELECT LNK_DIST FROM PRD_PF_TZ_TM WHERE VSL_SLAN_CD = @[s_ts1_lane] AND FM_PORT_CD = @[s_pol2] AND TO_PORT_CD = @[s_pod2] )),0) 
		FROM DUAL 
	 ), 
	 ( SELECT NVL(DECODE(@[s_fdr_flg3], 'Y', (SELECT STND_DIST FROM VSK_PORT_DIST WHERE FM_LOC_CD = @[s_pol3] AND TO_LOC_CD = @[s_pod3]), 
		(SELECT LNK_DIST FROM PRD_PF_TZ_TM WHERE VSL_SLAN_CD = @[s_ts2_lane] AND FM_PORT_CD = @[s_pol3] AND TO_PORT_CD = @[s_pod3] )),0) 
		FROM DUAL 
	 ), 
	 ( SELECT NVL(DECODE(@[s_fdr_flg4], 'Y', (SELECT STND_DIST FROM VSK_PORT_DIST WHERE FM_LOC_CD = @[s_pol4] AND TO_LOC_CD = @[s_pod4]), 
		(SELECT LNK_DIST FROM PRD_PF_TZ_TM WHERE VSL_SLAN_CD = @[s_ts3_lane] AND FM_PORT_CD = @[s_pol4] AND TO_PORT_CD = @[s_pod4] )),0) 
		FROM DUAL 
	 )
	  ,@[s_ocn_rout_tmp_flg],to_date(@[s_ocn_rout_tmp_exp_dt],'yyyy-mm-dd')
      ,REPLACE(REPLACE(REPLACE(  @[s_route_note] , CHR(34)),CHR(9),' '), CHR(13)||CHR(10),' ')
    )			]]></sql>
			<params>
				<param name="s_pol" type="12" value="" out="N"/>
				<param name="s_pod" type="12" value="" out="N"/>
				<param name="max_seq" type="12" value="" out="N"/>
				<param name="ts_ind_cd" type="12" value="" out="N"/>
				<param name="s_lnk_cnt" type="12" value="" out="N"/>
				<param name="s_prior" type="12" value="" out="N"/>
				<param name="s_route_flg" type="12" value="" out="N"/>
				<param name="s_pol1" type="12" value="" out="N"/>
				<param name="s_pod1" type="12" value="" out="N"/>
				<param name="s_lane" type="12" value="" out="N"/>
				<param name="s_dir1" type="12" value="" out="N"/>
				<param name="s_fdr_flg1" type="12" value="" out="N"/>
				<param name="s_pol2" type="12" value="" out="N"/>
				<param name="s_pod2" type="12" value="" out="N"/>
				<param name="s_ts1_lane" type="12" value="" out="N"/>
				<param name="s_dir2" type="12" value="" out="N"/>
				<param name="s_fdr_flg2" type="12" value="" out="N"/>
				<param name="s_pol3" type="12" value="" out="N"/>
				<param name="s_pod3" type="12" value="" out="N"/>
				<param name="s_ts2_lane" type="12" value="" out="N"/>
				<param name="s_dir3" type="12" value="" out="N"/>
				<param name="s_fdr_flg3" type="12" value="" out="N"/>
				<param name="s_pol4" type="12" value="" out="N"/>
				<param name="s_pod4" type="12" value="" out="N"/>
				<param name="s_ts3_lane" type="12" value="" out="N"/>
				<param name="s_dir4" type="12" value="" out="N"/>
				<param name="s_fdr_flg4" type="12" value="" out="N"/>
				<param name="s_n1st_tztm_hrs" type="12" value="" out="N"/>
				<param name="s_n2nd_tztm_hrs" type="12" value="" out="N"/>
				<param name="s_n3rd_tztm_hrs" type="12" value="" out="N"/>
				<param name="s_n4th_tztm_hrs" type="12" value="" out="N"/>
				<param name="s_n1st_stay_tm_hrs" type="12" value="" out="N"/>
				<param name="s_n2nd_stay_tm_hrs" type="12" value="" out="N"/>
				<param name="s_n3rd_stay_tm_hrs" type="12" value="" out="N"/>
				<param name="s_f_u" type="12" value="" out="N"/>
				<param name="s_route_rmk" type="12" value="" out="N"/>
				<param name="menu" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="s_ocn_rout_tmp_flg" type="12" value="" out="N"/>
				<param name="s_ocn_rout_tmp_exp_dt" type="12" value="" out="N"/>
				<param name="s_route_note" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
