<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DODInvoiceMgtDBDAOSearchPayerContactPointRSQL">
			<desc><![CDATA[SearchPayerContactPoint]]></desc>
			<sql><![CDATA[
SELECT  CUST_CNT_CD||LPAD(CUST_SEQ,6,'0') AS CUST_CD
       ,CUST_CNT_CD
       ,CUST_SEQ
       ,CUST_CNTC_PNT_SEQ
       ,CNTC_PNT_NM
       ,CNTC_PNT_PHN_NO
       ,CNTC_PNT_FAX_NO
       ,CNTC_PNT_EML          
FROM EAS_PAYR_CNTC_PNT A
WHERE  CUST_CNT_CD 	= substr(@[s_cust_cd],1,2)
AND CUST_SEQ 		= substr(@[s_cust_cd],3,6)
ORDER BY CUST_CNTC_PNT_SEQ			]]></sql>
			<params>
				<param name="s_cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
