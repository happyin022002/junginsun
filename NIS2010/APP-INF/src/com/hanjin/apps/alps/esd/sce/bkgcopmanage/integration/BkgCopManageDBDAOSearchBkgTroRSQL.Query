<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchBkgTroRSQL">
			<desc><![CDATA[BKG_TRO 의 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 	BKG_NO, 
	IO_BND_CD, 
	RTN_TRO_FLG, 
	TRO_SEQ, 
	RCV_TERM_CD, 
	TO_CHAR(RQST_DT, 'YYYYMMDDHH24MISS') AS RQST_DT, 
	ZN_CD, 
	DOR_LOC_CD, 
	DOR_PST_NO, 
	BIZ_RGST_NO, 
	CFM_FLG, 
	TO_CHAR(CFM_DT, 'YYYYMMDDHH24MISS') AS CFM_DT, 
	PCTL_NO, 
	VSL_CD, 
	SKD_VOY_NO, 
	SKD_DIR_CD, 
	POL_CD, 
	POD_CD, 
	TRO_BKG_NO
FROM
	BKG_TRO
WHERE
	BKG_NO = @[bkg_no]
	AND IO_BND_CD = @[io_bnd_cd]
	AND TRO_SEQ = @[tro_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
