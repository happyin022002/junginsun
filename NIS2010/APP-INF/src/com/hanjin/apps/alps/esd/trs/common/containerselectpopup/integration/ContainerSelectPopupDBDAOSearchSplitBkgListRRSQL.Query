<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSelectPopupDBDAOSearchSplitBkgListRRSQL">
			<desc><![CDATA[current bkg와 split된 bkg list 조회]]></desc>
			<sql><![CDATA[
SELECT
    A.BKG_NO
FROM
    BKG_BOOKING A
WHERE
    A.BKG_NO          =  @[bkgNo]
UNION
SELECT
    A.BKG_NO
FROM
    BKG_BOOKING A
WHERE
    A.BKG_NO          =  @[orgBkgNo]
UNION
SELECT
    A.BKG_NO
FROM
    BKG_BOOKING A
WHERE
    A.FM_BKG_NO       =     @[bkgNo]
UNION
SELECT
    A.BKG_NO
FROM
    BKG_BOOKING A
WHERE
    A.FM_BKG_NO        =    @[orgBkgNo]
UNION
SELECT
A.BKG_NO
FROM
BKG_BOOKING A
WHERE A.BKG_NO = (SELECT X.TO_BKG_NO
                    FROM BKG_BOOKING X
                   WHERE X.BKG_NO = @[bkgNo]
                     AND X.TO_BKG_NO IS NOT NULL
                     AND X.BKG_STS_CD = 'X'
                 )


			]]></sql>
			<params>
				<param name="bkgNo" type="12" value="" out="N"/>
				<param name="orgBkgNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
