<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerEdiDBDAOUpdatePerformanceDataCSQL">
			<desc><![CDATA[UpdatePerformanceData]]></desc>
			<sql><![CDATA[
MERGE INTO SCE_PERF_RPT_FOM C
USING ( SELECT @[s_cre_usr_id] CRE_USR_ID
            ,  @[s_edi_grp_cd] EDI_GRP_CD
            ,  @[s_rpt_col_seq] RPT_COL_SEQ
            ,  @[s_rpt_col_nm] RPT_COL_NM
            ,  @[s_rpt_col_desc] RPT_COL_DESC
            ,  @[s_edi_sts_flg] EDI_STS_FLG
            ,  DECODE(@[s_use_flg],'1','Y','N') USE_FLG
            ,  @[s_cre_usr_id] UPD_USR_ID 
        FROM DUAL) S
ON (C.CRE_USR_ID = S.CRE_USR_ID
    AND C.EDI_GRP_CD = S.EDI_GRP_CD
    AND C.RPT_COL_SEQ = S.RPT_COL_SEQ
    AND C.RPT_COL_NM = S.RPT_COL_NM
    )
WHEN MATCHED THEN
UPDATE
SET C.USE_FLG = S.USE_FLG
 ,  C.UPD_DT = SYSDATE
 ,  C.UPD_USR_ID = S.UPD_USR_ID
WHEN NOT MATCHED THEN   
INSERT (C.CRE_USR_ID
        ,C.EDI_GRP_CD
        ,C.RPT_COL_SEQ
        ,C.RPT_COL_NM
        ,C.RPT_COL_DESC
        ,C.EDI_STS_FLG
        ,C.USE_FLG
        ,C.CRE_DT
        ,C.UPD_USR_ID
        ,C.UPD_DT
        )
 VALUES (S.CRE_USR_ID
        ,S.EDI_GRP_CD
        ,S.RPT_COL_SEQ
        ,S.RPT_COL_NM
        ,S.RPT_COL_DESC
        ,S.EDI_STS_FLG
        ,S.USE_FLG
        ,SYSDATE
        ,S.UPD_USR_ID
        ,SYSDATE
        )			]]></sql>
			<params>
				<param name="s_cre_usr_id" type="12" value="" out="N"/>
				<param name="s_edi_grp_cd" type="12" value="" out="N"/>
				<param name="s_rpt_col_seq" type="12" value="" out="N"/>
				<param name="s_rpt_col_nm" type="12" value="" out="N"/>
				<param name="s_rpt_col_desc" type="12" value="" out="N"/>
				<param name="s_edi_sts_flg" type="12" value="" out="N"/>
				<param name="s_use_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
