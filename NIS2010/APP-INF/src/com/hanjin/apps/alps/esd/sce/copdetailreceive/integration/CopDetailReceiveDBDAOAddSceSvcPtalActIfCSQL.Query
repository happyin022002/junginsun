,@[vps_port_cd]
,@[clpt_ind_seq]
,@[vsl_dlay_rsn_cd]
,@[vsl_dlay_rsn_desc]
,@[vps_loc_cd]
,@[act_cd]
,to_date(@[rail_dest_n1st_eta_dt],'YYYY/MM/DD HH24:MI:SS')
)			]]></sql>
