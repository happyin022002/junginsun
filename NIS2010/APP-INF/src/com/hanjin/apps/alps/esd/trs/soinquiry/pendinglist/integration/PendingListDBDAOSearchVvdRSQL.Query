<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PendingListDBDAOSearchVvdRSQL">
			<desc><![CDATA[PendingList Vvd Search]]></desc>
			<sql><![CDATA[
SELECT																									
		VSL_CD																							
FROM VSK_VSL_SKD									
WHERE VSL_CD||SKD_VOY_NO||SKD_DIR_CD =  @[trunk_vvd]			]]></sql>
			<params>
				<param name="trunk_vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
