<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CandidateConfirmDBDAOSearchCandidateListRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT   /*+ NO_PUSH_PRED(B) */
         '0' CFM_I
       , '0' CFM_G
       , '0' CFM_N
       , '0' CFM_R
       , '0' CFM_D
       , A.OTS_DTL_SEQ                                                               /* HIDDEN */
       , A.N3PTY_NO                                                                  /* TPB NO. */
       , A.N3PTY_SRC_SUB_SYS_CD                                                      /* EXP.TYPE - MAIN */
       , TPB_GET_N3PTY_BIL_TP_NM_FNC(A.N3PTY_BIL_TP_CD) N3PTY_BIL_TP_NM              /* EXP.TYPE - SUB */
       , A.N3PTY_BIL_TP_CD
       , A.N3PTY_SRC_NO                                                              /* S/P INV NO. */
       , A.VNDR_CUST_DIV_CD                                                          /* 3RD PARTY - TYPE */
       , CASE A.VNDR_CUST_DIV_CD                                                     /* 3RD PARTY - CODE */
              WHEN 'V' THEN LPAD(A.VNDR_SEQ,6,0)
              WHEN 'C' THEN A.CUST_CNT_CD||LPAD(A.CUST_SEQ,6,0)
              WHEN 'S' THEN A.N3PTY_OFC_CD
         END TRD_PARTY_VAL
       , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD01132',A.EQ_KND_CD) EQ_KND_NM
       , A.EQ_KND_CD
       , A.EQ_NO
       , A.EQ_TPSZ_CD
       , A.BKG_NO           G_BKG_NO_ALL
       , A.BL_NO   G_BL_NO_ALL
       , DECODE(A.N3PTY_BIL_TP_CD
       , 'JO',VVD_CD
       , A.VSL_CD||A.SKD_VOY_NO||SUBSTR(A.FINC_DIR_CD,1,1)) G_VVD
       , A.IF_CURR_CD
       , A.IF_AMT
       , TPB_GET_INV_CURR_CHG_FNC(A.IF_CURR_CD,'USD',A.IF_AMT, SYSDATE) IF_AMT_USD
	   , A.IF_RMK                            	/* INTERFACED REMARK */
       , A.CFM_AMT                                                                   	/* CONFIRMED AMOUNT */
#if (${s_if_ofc_cd} != '')
       , TO_CHAR(TPB_GET_LCL_DATE_FNC(A.IF_DT,@[s_if_ofc_cd]),'YYYY-MM-DD HH24:MI') IF_DT /* I/F DATE */
#elseif (${s_if_ofc_cd} == '')
	   , TO_CHAR(TPB_GET_LCL_DATE_FNC(A.IF_DT,A.IF_OFC_CD),'YYYY-MM-DD HH24:MI') IF_DT    /* I/F DATE */
#end
       , A.IF_OFC_CD                                                                 	/* INTERFACED BY - OFFICE */
       , A.IF_USR_ID                                                                 	/* INTERFACED BY - ID */
       , (SELECT USR_NM FROM COM_USER WHERE USR_ID = A.IF_USR_ID) AS IF_USR_NM     	/* INTERFACED BY - NAME */
       , A.CFM_RMK                                                               	    /* DESCRIPTION */
       , A.RVW_OFC_CD                                                                	/* REVIEWED BY - OFFICE */
       , A.RVW_USR_ID                                                                	/* REVIEWED BY - ID */
       , A.N3PTY_NON_CFM_RSN_CD                                                      	/* REASON FOR NON-TPB */
	   , CASE WHEN LENGTH(B.N3PTY_NON_CFM_RSN) > 0 THEN 'Y' ELSE 'N' END NON_CFM_RSN_FLG   --CHM-201114303 [TPB] Non-TPB Candidate Remark 란 추가 요청        
       , A.CSR_NO                                                                    	/* CSR NO. (ONLY JO CASE) */
       , A.VNDR_CNT_CD                                                               	/* HIDDEN */
       , A.VNDR_SEQ  	                                                             	/* HIDDEN */
       , A.CUST_CNT_CD                                                               	/* HIDDEN */
       , A.CUST_SEQ 	                                                             	/* HIDDEN */
       , A.N3PTY_OFC_CD                                                              	/* HIDDEN */
       , A.N3PTY_IF_TP_CD                                                            	/* HIDDEN */
       , A.SRC_VNDR_SEQ                                                              	/* HIDDEN */
       , A.TML_INV_TP_CD TES_GUBUN                                                   	/* HIDDEN */
       , (SELECT COUNT(0)FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD00921' AND INTG_CD_VAL_CTNT = A.SRC_VNDR_SEQ) TRS_GUBUN  /* HIDDEN */
       , A.ESTM_SEQ_NO                                                              	/* HIDDEN */
       , A.ESTM_RVIS_NO                                                              	/* HIDDEN */
       , A.ESTM_DTL_SEQ_NO                                                           	/* HIDDEN */
       , A.ESTM_SYS_AREA_GRP_ID  ESTM_SVR_ID                                         	/* HIDDEN */
       , NVL(A.COST_EXPT_FLG,'N') COST_EXPT_FLG                                      	/* HIDDEN */
       , CASE WHEN A.N3PTY_IF_TP_CD = 'S' AND A.N3PTY_EXPN_TP_CD IN ('TRS','TES') THEN (CASE WHEN LENGTH(A.FINC_DIR_CD)=2 THEN 'Y' ELSE 'N' END)
              ELSE 'Y'
         END CHECK_FINC_DIR_CD  			                             	            /* HIDDEN */
       , A.OFC_CD                                                                    	/* HIDDEN */
       , TP.IDA_SAC_CD
FROM     TPB_OTS_DTL A
       , (
           SELECT   C.OTS_DTL_SEQ
                  , C.N3PTY_NON_CFM_DT
                  , C.N3PTY_NON_CFM_RSN_CD
                  , C. N3PTY_NON_CFM_RSN
           FROM     TPB_NON_CFM_RSN_DESC C
                  , (
                      SELECT   OTS_DTL_SEQ
                             , MAX(N3PTY_NON_CFM_DT) AS N3PTY_NON_CFM_DT
                      FROM     TPB_NON_CFM_RSN_DESC
                      GROUP BY OTS_DTL_SEQ
                    ) D
           WHERE    1 = 1
           AND      C.OTS_DTL_SEQ = D.OTS_DTL_SEQ
           AND      C.N3PTY_NON_CFM_DT = D.N3PTY_NON_CFM_DT
         ) B
       , TPB_N3RD_PTY_BIL_TP TP
WHERE    1 = 1
AND      A.OTS_DTL_SEQ = B.OTS_DTL_SEQ(+)
AND      A.N3PTY_BIL_TP_CD = TP.N3PTY_BIL_TP_CD
AND      TP.ACT_FLG = 'Y'
AND      A.N3PTY_BIL_TP_CD != 'JO'

#if (${s_vvd} != '') 
AND      DECODE(A.N3PTY_BIL_TP_CD,'JO',VVD_CD,A.VSL_CD||A.SKD_VOY_NO||SUBSTR(A.FINC_DIR_CD,1,1)) = @[s_vvd]
#end
#if (${s_bkg_no_all} != '') 
AND      A.BKG_NO = @[s_bkg_no_all]
#end
#if (${s_bl_no_all} != '') 
AND      A.BL_NO = @[s_bl_no_all]
#end 
#if (${s_csr_no} != '') 
AND      A.CSR_NO = @[s_csr_no]
#end
#if (${s_n3pty_src_no} != '') 
AND      A.N3PTY_SRC_NO = @[s_n3pty_src_no]
#end 
#if (${s_n3pty_src_sub_sys_cd} != '')
AND      A.N3PTY_SRC_SUB_SYS_CD = @[s_n3pty_src_sub_sys_cd]
#end 
#if (${s_n3pty_bil_tp_cd} != '')
AND      A.N3PTY_BIL_TP_CD = @[s_n3pty_bil_tp_cd]
#end 
AND      A.CXL_FLG IS NULL
#if (${s_nr} != 'N')
AND      A.N3PTY_DELT_TP_CD IN ('N', 'S')
#else
    #if (${s_ofc_cd_for_rhq} != '')
		/* 2014.10.07 S 는 RHQ 대행 Office 를 뜻합니다. 즉, RHQ는 아니지만 RHQ 업무를 할 수 있습니다. */
        #if (${s_ofc_cd_for_rhq} == ${s_rhq_cd_for_rhq} || ${s_ofc_top_lvl} == 'S')
AND      A.N3PTY_DELT_TP_CD IN ('N', 'S')
        #else
AND      EXISTS
         (
           SELECT   'X' 
           FROM     TPB_HNDL_OFC
           WHERE    1 = 1
           AND      N3PTY_OFC_TP_CD = 'T'
           AND      DELT_FLG = 'N'
           AND      OFC_CD = @[s_ofc_cd_for_rhq]
         )   
AND      A.N3PTY_DELT_TP_CD NOT IN ('D')     
        #end 
    #end 
#end
AND      A.OFC_CD IN
         (
           SELECT   OFC_CD
           FROM     TPB_HNDL_OFC
           WHERE    1 = 1
           AND      N3PTY_OFC_TP_CD = 'T'
           AND      DELT_FLG = 'N'
#if (${s_if_rhq_cd} != '')
           AND      RHQ_CD = @[s_if_rhq_cd]
#end
#if (${s_if_ctrl_cd} != '')
           AND      N3PTY_CTRL_OFC_CD = @[s_if_ctrl_cd]

#end
#if (${s_if_ofc_cd} != '') 
           AND      OFC_CD = @[s_if_ofc_cd]    
#end
         )

AND      ( 
#if (${s_nr} == 'I' || ${s_nr} == 'IR') 
               (                                           
                     A.N3PTY_CFM_CD = 'I'
                 AND (     A.IF_DT >= TPB_GET_SRCH_DATE_FNC(@[s_sdate],'YYYY-MM-DD',nvl(@[s_if_ofc_cd],A.IF_OFC_CD))
                       AND A.IF_DT < TPB_GET_SRCH_DATE_FNC(@[s_edate],'YYYY-MM-DD',nvl(@[s_if_ofc_cd],A.IF_OFC_CD))+1
                     )
               )
#end 
#if (${s_nr} == 'IR') 
           OR
#end 
#if (${s_nr} == 'R' || ${s_nr} == 'IR')
               (
                     A.N3PTY_CFM_CD = 'R'
                 AND (     A.RVW_DT >= TPB_GET_SRCH_DATE_FNC(@[s_sdate],'YYYY-MM-DD',nvl(@[s_if_ofc_cd],A.IF_OFC_CD))
                       AND A.RVW_DT < TPB_GET_SRCH_DATE_FNC(@[s_edate],'YYYY-MM-DD',nvl(@[s_if_ofc_cd],A.IF_OFC_CD))+1
                     )
               )
#end
#if (${s_nr} == 'N') 
               (
                     A.N3PTY_CFM_CD = 'N'
                 AND (     A.CFM_DT >= TPB_GET_SRCH_DATE_FNC(@[s_sdate],'YYYY-MM-DD',nvl(@[s_if_ofc_cd],A.IF_OFC_CD))
                       AND A.CFM_DT < TPB_GET_SRCH_DATE_FNC(@[s_edate],'YYYY-MM-DD',nvl(@[s_if_ofc_cd],A.IF_OFC_CD))+1
                     )
               )
#end 
         )
ORDER BY OTS_DTL_SEQ			]]></sql>
			<params>
				<param name="s_if_ofc_cd" type="12" value="null" out="N"/>
				<param name="s_vvd" type="12" value="" out="N"/>
				<param name="s_bkg_no_all" type="12" value="" out="N"/>
				<param name="s_bl_no_all" type="12" value="" out="N"/>
				<param name="s_csr_no" type="12" value="" out="N"/>
				<param name="s_n3pty_src_no" type="12" value="" out="N"/>
				<param name="s_n3pty_src_sub_sys_cd" type="12" value="" out="N"/>
				<param name="s_n3pty_bil_tp_cd" type="12" value="" out="N"/>
				<param name="s_ofc_cd_for_rhq" type="12" value="" out="N"/>
				<param name="s_if_rhq_cd" type="12" value="" out="N"/>
				<param name="s_if_ctrl_cd" type="12" value="" out="N"/>
				<param name="s_sdate" type="12" value="2013-05-01" out="N"/>
				<param name="s_edate" type="12" value="2013-07-31" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
