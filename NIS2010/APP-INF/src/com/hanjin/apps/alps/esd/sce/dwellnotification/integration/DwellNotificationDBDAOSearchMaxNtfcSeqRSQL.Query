<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOSearchMaxNtfcSeqRSQL">
			<desc><![CDATA[SCE_DWLL_CUST_SVC_LIST 테이블에서 CUSTOMER CODE에 등록된 NTFC_SEQ의 MAX을 값을 구한다.]]></desc>
			<sql><![CDATA[
select NVL(max(NTFC_SEQ),0) SEQ from SCE_DWLL_CUST_SVC_LIST
where DWLL_CUST_CNT_CD =@[cust_cnr_cd]
and DWLL_CUST_SEQ =@[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnr_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
