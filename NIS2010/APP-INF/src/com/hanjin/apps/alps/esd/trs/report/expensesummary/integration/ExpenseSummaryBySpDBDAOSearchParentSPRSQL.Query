<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExpenseSummaryBySpDBDAOSearchParentSPRSQL">
			<desc><![CDATA[조회대상 S/P의 Parent S/P를 조회]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ, VNDR_LGL_ENG_NM, '1' DIV  
   FROM MDM_VENDOR                          
  WHERE VNDR_SEQ IN (SELECT PRNT_VNDR_SEQ   
                       FROM MDM_VENDOR      
                      WHERE VNDR_SEQ = @[svc_provider])			]]></sql>
			<params>
				<param name="svc_provider" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
