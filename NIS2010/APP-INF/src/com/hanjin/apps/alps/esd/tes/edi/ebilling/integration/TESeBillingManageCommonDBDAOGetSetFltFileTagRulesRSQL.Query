<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESeBillingManageCommonDBDAOGetSetFltFileTagRulesRSQL">
			<desc><![CDATA[VNDR단위에 처리 대상 TAG RULE]]></desc>
			<sql><![CDATA[
SELECT 
M.SNDR_ID, V.EDI_VNDR_SEQ, 
T.FLT_FILE_TAG_NM, T.TBL_NM, T.TBL_VO_NM, T.HDR_TAG_FLG, T.SAV_SEQ, T.PRNT_TAG_NM, T.TAG_VLD_KNT, T.MNL_CNTR_RCV_TAG_FLG
FROM TES_EDI_RCV_RULE_MN M, TES_EDI_RCV_RULE_VNDR_MGMT V, TES_EDI_RCV_FLT_FILE_TAG T
WHERE 1=1
AND M.EDI_RCV_RULE_MN_SEQ = V.EDI_RCV_RULE_MN_SEQ
AND M.SNDR_ID = @[sndr_id]
AND NVL(M.DELT_FLG,'N') <> 'Y'
--AND NVL(M.CFM_FLG,'N') = 'Y'
AND V.EDI_VNDR_SEQ = @[vndr_seq]
AND V.EDI_RCV_RULE_MN_SEQ = T.EDI_RCV_RULE_MN_SEQ
AND V.EDI_VNDR_SEQ = T.EDI_VNDR_SEQ
ORDER BY T.SAV_SEQ			]]></sql>
			<params>
				<param name="sndr_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
