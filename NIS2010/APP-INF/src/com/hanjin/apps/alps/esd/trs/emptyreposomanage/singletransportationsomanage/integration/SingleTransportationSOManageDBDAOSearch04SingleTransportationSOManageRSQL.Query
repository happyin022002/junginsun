<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSearch04SingleTransportationSOManageRSQL">
			<desc><![CDATA[f_cmd : SEARCH04]]></desc>
			<sql><![CDATA[
SELECT A.CNTR_NO EQ_NO, A.CNTR_TPSZ_CD EQ_TPSZ_CD, 
    A.AGMT_CTY_CD||A.AGMT_SEQ LESSOR,
    A.LSTM_CD, A.OWNR_CO_CD, A.CNTR_USE_CO_CD EQ_USED,
    A.CNMV_STS_CD MOVEMENT_STS, A.CRNT_YD_CD CREATION_YARD,
    TO_CHAR(A.CNMV_DT, 'YYYY-MM-DD') EVENT_DATE,
	DECODE(B.OFFH_YD_CD, NULL,'',B.OFFH_YD_CD) OFFH_YD_CD 
FROM MST_CONTAINER A, (SELECT CNTR_NO, OFFH_YD_CD  FROM LSE_AVAL_OFFH
WHERE OFFH_DUE_DT > TO_CHAR(LAST_DAY(ADD_MONTHS(SYSDATE, -1)), 'YYYYMMDD')
AND OFFH_STS_CD IN ('R','C') ) B
WHERE 1=1
AND A.CNTR_NO = B.CNTR_NO (+)
AND A.CNTR_NO LIKE @[scntr_no]||'%'			]]></sql>
			<params>
				<param name="scntr_no" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
