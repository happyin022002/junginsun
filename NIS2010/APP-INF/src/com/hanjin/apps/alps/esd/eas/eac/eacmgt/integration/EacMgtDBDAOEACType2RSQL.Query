<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOEACType2RSQL">
			<desc><![CDATA[eac type 코드 값에 따라 콤보값을 변경하게 한다.]]></desc>
			<sql><![CDATA[
SELECT n3pty_expn_tp_cd AS SYS,
       n3pty_bil_tp_cd AS code_cd,
       n3pty_bil_tp_nm AS code_nm,
       n3pty_bil_tp_cd AS ord
  FROM TPB_N3RD_PTY_BIL_TP
 WHERE act_flg = 'Y'
   AND n3pty_expn_tp_cd = DECODE(NVL(@[n3pty_expn_tp_cd],'0'),'0',@[s_eac_expn_tp_cd],@[n3pty_expn_tp_cd])
   AND n3pty_if_tp_cd = 'S'
   AND n3pty_bil_tp_cd != 'JO'
 ORDER BY n3pty_expn_tp_cd, ord			]]></sql>
			<params>
				<param name="n3pty_expn_tp_cd" type="12" value="" out="N"/>
				<param name="s_eac_expn_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
