<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESInterfaceManageDBDAOGetVndrOfcCdRSQL">
			<desc><![CDATA[GetVndrOfcCd]]></desc>
			<sql><![CDATA[
SELECT V.OFC_CD
FROM MDM_VENDOR V
WHERE V.VNDR_SEQ = @[vndr_seq]
AND NVL(V.DELT_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
