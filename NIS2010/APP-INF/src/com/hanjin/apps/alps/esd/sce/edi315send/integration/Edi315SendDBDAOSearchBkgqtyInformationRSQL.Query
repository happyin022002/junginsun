<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchBkgqtyInformationRSQL">
			<desc><![CDATA[Edi315SendDBDAOSearchBkgqtyInformationRSQL]]></desc>
			<sql><![CDATA[
select 
    WGT_UT_CD W_UNIT
   ,ACT_WGT WEIGHT
   ,MEAS_UT_CD MEA_UNIT
   ,MEAS_QTY MEA_QTY
   ,PCK_TP_CD P_UNIT
   ,PCK_QTY PACKAGE
   ,REPLACE(REPLACE(NVL(DEL_NM, ' '), CHR(13), ''), CHR(10), '') bl_store_nbr
from BKG_BL_DOC 
WHERE 1=1 
and BKG_NO = @[e_bkg_no]			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
