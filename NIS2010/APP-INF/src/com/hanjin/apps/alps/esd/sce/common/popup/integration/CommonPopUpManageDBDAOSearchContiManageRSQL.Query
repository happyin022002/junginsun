<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonPopUpManageDBDAOSearchContiManageRSQL">
			<desc><![CDATA[select conti list]]></desc>
			<sql><![CDATA[
select cnt_cd, cnt_nm 
from mdm_country 
where 1=1 
#if(${conti_cd} != "N")
	and SCONTI_CD like @[conti_cd] || '%' 
#end
#if(${sub_conti_cd} != "N")
	and SCONTI_CD like @[sub_conti_cd] || '%' 
#end
#if(${cnt_cd} != "")
	and CNT_CD = @[cnt_cd]
#end
#if(${cnt_nm} != "")
	and CNT_NM = @[cnt_nm] 
#end
and DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="sub_conti_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="cnt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
