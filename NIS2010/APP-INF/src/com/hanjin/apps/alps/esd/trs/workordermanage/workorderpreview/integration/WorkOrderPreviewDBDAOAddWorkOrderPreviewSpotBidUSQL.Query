<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewSpotBidUSQL">
			<desc><![CDATA[AddWorkOrderPreviewSpotBid]]></desc>
			<sql><![CDATA[
UPDATE TRS_SPOT_BID SB
   SET TRSP_WO_OFC_CTY_CD = @[trsp_wo_ofc_cty_cd],
       TRSP_WO_SEQ = @[trsp_wo_seq],
       SPOT_BID_STS_CD = 'I',
       GLINE_VNDR_SEQ = @[gline_vndr_seq],
       GLINE_CURR_CD = @[gline_curr_cd],
       GLINE_BZC_AMT = @[gline_bzc_amt],
       GLINE_FUEL_SCG_AMT = @[gline_fuel_scg_amt],
       GLINE_TOLL_FEE_AMT = @[gline_toll_fee_amt],
       GLINE_SCG_VAT_AMT = @[gline_scg_vat_amt],
       GLINE_TTL_AMT = @[gline_ttl_amt],
       LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd]),
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = SYSDATE
 WHERE 1=1
   AND SPOT_BID_NO = @[spot_bid_no]
   AND TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
   AND TRSP_SO_SEQ = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
				<param name="gline_vndr_seq" type="12" value="" out="N"/>
				<param name="gline_curr_cd" type="12" value="" out="N"/>
				<param name="gline_bzc_amt" type="12" value="" out="N"/>
				<param name="gline_fuel_scg_amt" type="12" value="" out="N"/>
				<param name="gline_toll_fee_amt" type="12" value="" out="N"/>
				<param name="gline_scg_vat_amt" type="12" value="" out="N"/>
				<param name="gline_ttl_amt" type="12" value="" out="N"/>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="spot_bid_no" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
