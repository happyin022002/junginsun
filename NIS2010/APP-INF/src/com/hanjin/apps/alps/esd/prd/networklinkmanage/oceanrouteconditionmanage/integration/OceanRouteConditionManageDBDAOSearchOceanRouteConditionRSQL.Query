<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanRouteConditionManageDBDAOSearchOceanRouteConditionRSQL">
			<desc><![CDATA[SearchOceanRouteCondition]]></desc>
			<sql><![CDATA[
SELECT m.port_cd, m.vsl_slan_cd, m.skd_dir_cd, m.crr_cd, m.tml_cd, y.yd_nm, 
       TO_CHAR (m.cre_dt, 'yyyy-mm-dd') cre_dt, 
       TO_CHAR (m.upd_dt, 'yyyy-mm-dd') upd_dt, m.upd_usr_id 
  FROM prd_port_tml_mtx m, mdm_yard y 
 WHERE port_cd = @[i_port_cd]  AND m.tml_cd = y.yd_cd(+)

#if(${i_vsl_slan_cd} !=  '') 
    AND m.vsl_slan_cd = @[i_vsl_slan_cd]
#end
#if(${i_crr_cd} != '')
    AND m.crr_cd = @[i_crr_cd]
#end
#if(${i_tml_cd} != '')
    AND m.tml_cd = @[i_tml_cd]
#end			]]></sql>
			<params>
				<param name="i_port_cd" type="12" value="" out="N"/>
				<param name="i_vsl_slan_cd" type="12" value="" out="N"/>
				<param name="i_crr_cd" type="12" value="" out="N"/>
				<param name="i_tml_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
