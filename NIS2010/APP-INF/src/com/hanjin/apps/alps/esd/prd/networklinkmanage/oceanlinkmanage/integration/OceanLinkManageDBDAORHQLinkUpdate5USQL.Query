<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanLinkManageDBDAORHQLinkUpdate5USQL">
			<desc><![CDATA[RHQLinkUpdate5]]></desc>
			<sql><![CDATA[
UPDATE PRD_OCN_ROUT 
SET   N4TH_LANE_CD = @[s_lane] , 
      N4TH_SKD_DIR_CD = @[s_dr] , 
      N4TH_TZTM_HRS = TO_NUMBER(@[s_t_time])  
WHERE N4TH_POL_CD = @[h_from]  
AND   N4TH_POD_CD = @[h_to]  
AND   N4TH_LANE_FDR_FLG = 'Y'			]]></sql>
			<params>
				<param name="s_lane" type="12" value="" out="N"/>
				<param name="s_dr" type="12" value="" out="N"/>
				<param name="s_t_time" type="12" value="" out="N"/>
				<param name="h_from" type="12" value="" out="N"/>
				<param name="h_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
