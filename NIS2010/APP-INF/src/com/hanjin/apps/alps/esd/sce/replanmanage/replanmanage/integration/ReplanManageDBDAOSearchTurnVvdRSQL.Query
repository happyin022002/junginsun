<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOSearchTurnVvdRSQL">
			<desc><![CDATA[VVD 로 Turn VVD 를 조회하여 return 한다.]]></desc>
			<sql><![CDATA[
SELECT 												  	
	VSL_CD,TURN_SKD_VOY_NO,TURN_SKD_DIR_CD,TURN_CLPT_IND_SEQ   
FROM VSK_VSL_PORT_SKD                                      
	WHERE   VSL_CD = @[vsl_cd]                                         
	AND     SKD_VOY_NO = @[skd_voy_no]                                     
	AND     SKD_DIR_CD = @[skd_dir_cd]                                     
	AND     CALL_YD_IND_SEQ = @[call_yd_ind_seq]                                   
	AND     YD_CD = @[nod_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="call_yd_ind_seq" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
