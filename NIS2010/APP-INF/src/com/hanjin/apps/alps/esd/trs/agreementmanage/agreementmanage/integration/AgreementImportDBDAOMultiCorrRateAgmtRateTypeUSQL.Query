<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementImportDBDAOMultiCorrRateAgmtRateTypeUSQL">
			<desc><![CDATA[Rate Type Update]]></desc>
			<sql><![CDATA[
UPDATE TRS_AGMT_RT_TP
   SET TRSP_AGMT_RT_TP_CD = @[trsp_agmt_rt_tp_cd]
      ,CGO_TP_CD          = @[cgo_tp_cd]
      ,CUST_NOMI_TRKR_FLG = @[cust_nomi_trkr_flg]
      ,CUST_NOMI_TRKR_IND_CD =  @[cust_nomi_trkr_ind_cd]
      ,CUST_CNT_CD        = @[cust_cnt_cd]
      ,CUST_SEQ           = @[cust_seq]
      ,TRSP_COST_MOD_CD   = @[trsp_cost_mod_cd]
      ,AGMT_TRSP_TP_CD    = @[agmt_trsp_tp_cd]
      ,CMDT_GRP_CD        = @[cmdt_grp_cd]
      ,RAIL_SVC_TP_CD     = @[rail_svc_tp_cd]
      ,UPD_USR_ID         = @[fm_account_usr_id]
      ,UPD_OFC_CD         = @[fm_account_ofc_cd] 
      ,UPD_DT             = SYSDATE       
 WHERE TRSP_AGMT_OFC_CTY_CD   = @[trsp_agmt_ofc_cty_cd]
   AND TRSP_AGMT_SEQ          = @[trsp_agmt_seq]
   AND TRSP_AGMT_RT_TP_SER_NO = @[trsp_agmt_rt_tp_ser_no]			]]></sql>
			<params>
				<param name="trsp_agmt_rt_tp_cd" type="12" value="" out="N"/>
				<param name="cgo_tp_cd" type="12" value="" out="N"/>
				<param name="cust_nomi_trkr_flg" type="12" value="" out="N"/>
				<param name="cust_nomi_trkr_ind_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="trsp_cost_mod_cd" type="12" value="" out="N"/>
				<param name="agmt_trsp_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_grp_cd" type="12" value="" out="N"/>
				<param name="rail_svc_tp_cd" type="12" value="" out="N"/>
				<param name="fm_account_usr_id" type="12" value="" out="N"/>
				<param name="fm_account_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="trsp_agmt_rt_tp_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
