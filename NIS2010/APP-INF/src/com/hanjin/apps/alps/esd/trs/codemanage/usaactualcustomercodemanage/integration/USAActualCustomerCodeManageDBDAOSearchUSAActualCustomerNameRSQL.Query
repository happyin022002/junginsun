<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USAActualCustomerCodeManageDBDAOSearchUSAActualCustomerNameRSQL">
			<desc><![CDATA[Actual Customer Name 조회]]></desc>
			<sql><![CDATA[
SELECT  cust_cnt_cd||cust_seq act_cust_cnt_cd
       ,cust_lgl_eng_nm act_cust_nm
FROM MDM_CUSTOMER
WHERE delt_flg <> 'Y'
AND   cust_cnt_cd||cust_seq = @[act_cust_cnt_cd]
AND   NVL(NMD_CUST_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="act_cust_cnt_cd" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
