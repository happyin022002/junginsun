<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOCheckCsrNoForApHdrDtrbRSQL">
			<desc><![CDATA[CheckCsrNoForApHdrDtrb]]></desc>
			<sql><![CDATA[
SELECT	COUNT(H.CSR_NO) 	CSR_KNT				
FROM	AP_INV_HDR  		H					
		,AP_INV_DTRB 		D					
WHERE	H.CSR_NO    			= D.CSR_NO			
		AND H.SRC_CTNT  		= 'SO_TRANS'		
		AND H.IF_FLG    		= 'Y'    			
		AND H.RCV_ERR_FLG		IS NULL				
		AND H.CSR_NO    		= @[CSR_NO]
			]]></sql>
			<params>
				<param name="CSR_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
