<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchCutOffTimeRSQL">
			<desc><![CDATA[CuttOffTime 항목 조회]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(NVL(MNL_SET_DT, SYS_SET_DT), 'YYYYMMDDHH24MI') AS COFF_DT
FROM BKG_CLZ_TM
WHERE BKG_NO = @[bkg_no]
AND CLZ_TP_CD = 'R'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="SEL174890200" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
