<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetItemPoNoRSQL">
			<desc><![CDATA[BKG_REF_DTL 테이블의 PO_NO (항목 별 아이템) Multiple 건 조회]]></desc>
			<sql><![CDATA[
SELECT REPLACE(REPLACE(REPLACE(NVL(PO_NO, ' '), CHR(13), ' '), CHR(10), ' '), CHR(34), '') item_po_nbr 
FROM BKG_REF_DTL 
WHERE 1=1
#if(${e_bkg_no} != '') 
     and bkg_no = @[e_bkg_no]
#end

#if(${e_cntr_no} != '') 
     and cntr_no = @[e_cntr_no]
#end			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
				<param name="e_cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
