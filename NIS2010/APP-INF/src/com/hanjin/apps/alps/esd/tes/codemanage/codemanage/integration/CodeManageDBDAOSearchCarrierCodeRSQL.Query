<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchCarrierCodeRSQL">
			<desc><![CDATA[SearchCarrierCode]]></desc>
			<sql><![CDATA[
SELECT
	CRR_CD
    , CRR_NM
    , CRE_USR_ID
    , TO_CHAR(CRE_DT, 'YYYYMMDDHH24MISS') CRE_DT
	, UPD_USR_ID
    , TO_CHAR(UPD_DT, 'YYYYMMDDHH24MISS') UPD_DT
	, DELT_FLG
FROM MDM_CARRIER
WHERE ( DELT_FLG IS NULL OR DELT_FLG='N' )			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
