<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOAddDwllNtfcExptHistoryCSQL">
			<desc><![CDATA[Email  sending Exception History을 저장을 한다.]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_DWLL_CUST_EXPT_HIS
         (SC_NO, 
          DWLL_CUST_CNT_CD,
          DWLL_CUST_SEQ,
          CRE_USR_ID,
          CRE_DT,
          UPD_USR_ID,
          UPD_DT,
          FM_EFF_DT,
          TO_EFF_DT,
          TML_DWLL_FLG,
          ENR_DWLL_FLG,
          DEST_DWLL_FLG,
          VSL_DLAY_FLG,
          DWLL_EXPT_RMK,
          EXPT_SET_USR_ID,
          EXPT_SET_DT,
          HIS_CRE_DT,
		  DELT_FLG,
          BKG_TML_DWLL_FLG,
          BKG_ENR_DWLL_FLG,
          BKG_DEST_DWLL_FLG,
          BKG_VSL_DLAY_FLG
         )
SELECT SC_NO,
       DWLL_CUST_CNT_CD,
       DWLL_CUST_SEQ,
       CRE_USR_ID,
       CRE_DT,
       UPD_USR_ID,
       UPD_DT,
       FM_EFF_DT,
       TO_EFF_DT,
       TML_DWLL_FLG,
       ENR_DWLL_FLG,
       DEST_DWLL_FLG,
       VSL_DLAY_FLG,
       DWLL_EXPT_RMK,
       EXPT_SET_USR_ID,
       EXPT_SET_DT,
       SYSDATE,
       DELT_FLG,
       BKG_TML_DWLL_FLG,
       BKG_ENR_DWLL_FLG,
       BKG_DEST_DWLL_FLG,
       BKG_VSL_DLAY_FLG
FROM SCE_DWLL_NTFC_CUST_EXPT
WHERE DWLL_CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND   DWLL_CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
