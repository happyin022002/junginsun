<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanFeederCostManageDBDAOSearchFeederCostDGRSQL">
			<desc><![CDATA[2012.08.06 변종건 [CHM-201219483-01] 구주 Hinterland T/F -  TRS (Transportation) - Cost Inquiry 추가 요건 반영
- Dangerous 조회]]></desc>
			<sql><![CDATA[
SELECT  DG.COST_TRF_NO
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03047', M.COST_TRF_STS_CD) COST_TRF_STS_NM
      , TO_CHAR(M.EFF_FM_DT, 'YYYY-MM-DD') EFF_FM_DT
      , D.FM_NOD_CD
      , D.TO_NOD_CD
      , DECODE(D.PCTL_IO_BND_CD, 'O', 'PRE', 'I', 'POST') PCTL_IO_BND_NM
      , D.DIR_CD
      , D.WTR_RCV_TERM_CD
      , D.WTR_DE_TERM_CD
      , D.VNDR_SEQ
      , ( SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR V WHERE V.VNDR_SEQ = D.VNDR_SEQ ) VNDR_NM
      , GREATEST( NVL(D.TRSP_20FT_AGMT_OLD_FLG,'N'), NVL(D.TRSP_40FT_AGMT_OLD_FLG,'N') ) AGMT_OLD_FLG
      , D.FDR_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT
      , DG.IMDG_N1ST_CLSS_SVC_FLG
      , DG.IMDG_N1ST_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N1ST_CLSS_SCG_AMT AS IMDG_N1ST_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N1ST_CLSS_SCG_AMT AS IMDG_N1ST_CLSS_40FT_TTL_AMT
      , DG.IMDG_N2ND_CLSS_SVC_FLG
      , DG.IMDG_N2ND_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N2ND_CLSS_SCG_AMT AS IMDG_N2ND_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N2ND_CLSS_SCG_AMT AS IMDG_N2ND_CLSS_40FT_TTL_AMT
      , DG.IMDG_N3RD_CLSS_SVC_FLG
      , DG.IMDG_N3RD_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N3RD_CLSS_SCG_AMT AS IMDG_N3RD_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N3RD_CLSS_SCG_AMT AS IMDG_N3RD_CLSS_40FT_TTL_AMT
      , DG.IMDG_N4TH_CLSS_SVC_FLG
      , DG.IMDG_N4TH_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N4TH_CLSS_SCG_AMT AS IMDG_N4TH_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N4TH_CLSS_SCG_AMT AS IMDG_N4TH_CLSS_40FT_TTL_AMT
      , DG.IMDG_N5TH_CLSS_SVC_FLG
      , DG.IMDG_N5TH_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N5TH_CLSS_SCG_AMT AS IMDG_N5TH_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N5TH_CLSS_SCG_AMT AS IMDG_N5TH_CLSS_40FT_TTL_AMT
      , DG.IMDG_N6TH_CLSS_SVC_FLG
      , DG.IMDG_N6TH_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N6TH_CLSS_SCG_AMT AS IMDG_N6TH_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N6TH_CLSS_SCG_AMT AS IMDG_N6TH_CLSS_40FT_TTL_AMT
      , DG.IMDG_N7TH_CLSS_SVC_FLG
      , DG.IMDG_N7TH_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N7TH_CLSS_SCG_AMT AS IMDG_N7TH_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N7TH_CLSS_SCG_AMT AS IMDG_N7TH_CLSS_40FT_TTL_AMT
      , DG.IMDG_N8TH_CLSS_SVC_FLG
      , DG.IMDG_N8TH_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N8TH_CLSS_SCG_AMT AS IMDG_N8TH_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N8TH_CLSS_SCG_AMT AS IMDG_N8TH_CLSS_40FT_TTL_AMT
      , DG.IMDG_N9TH_CLSS_SVC_FLG
      , DG.IMDG_N9TH_CLSS_SCG_AMT
      , D.FDR_20FT_TTL_AMT + DG.IMDG_N9TH_CLSS_SCG_AMT AS IMDG_N9TH_CLSS_20FT_TTL_AMT
      , D.FDR_40FT_TTL_AMT + DG.IMDG_N9TH_CLSS_SCG_AMT AS IMDG_N9TH_CLSS_40FT_TTL_AMT
      , TO_CHAR(M.LOCL_CRE_DT, 'YYYY-MM-DD HH24:MI:SS') LOCL_CRE_DT
      , ( SELECT C.USR_NM FROM COM_USER C WHERE  C.USR_ID = M.CRE_USR_ID ) CRE_USR_NM
      , M.CRE_OFC_CD
      , TO_CHAR(M.LOCL_UPD_DT, 'YYYY-MM-DD HH24:MI:SS') LOCL_UPD_DT
      , ( SELECT C.USR_NM FROM COM_USER C WHERE  C.USR_ID = M.UPD_USR_ID ) UPD_USR_NM
      , M.UPD_OFC_CD
FROM    TRS_FDR_DG_COST_TRF DG
      , TRS_FDR_COST_TRF D
      , TRS_FDR_COST_TRF_HDR M
WHERE   DG.COST_TRF_NO = D.COST_TRF_NO
AND     DG.COST_TRF_ROUT_SEQ = D.COST_TRF_ROUT_SEQ
AND     DG.COST_TRF_NO = M.COST_TRF_NO

#if(${date_flg} == 'B')
AND     M.LOCL_CRE_DT BETWEEN TO_DATE(REPLACE(@[from_dt],'-',''), 'YYYYMMDD') AND TO_DATE(REPLACE(@[to_dt],'-',''), 'YYYYMMDD') + 0.999999
#end

#if(${date_flg} == 'U')
AND     M.LOCL_UPD_DT BETWEEN TO_DATE(REPLACE(@[from_dt],'-',''), 'YYYYMMDD') AND TO_DATE(REPLACE(@[to_dt],'-',''), 'YYYYMMDD') + 0.999999
#end

#if(${date_flg} == 'C')
AND     M.EFF_FM_DT BETWEEN TO_DATE(REPLACE(@[from_dt],'-',''), 'YYYYMMDD') AND TO_DATE(REPLACE(@[to_dt],'-',''), 'YYYYMMDD') + 0.999999
#end

#if(${eff_to_dt} != '')
AND     TO_DATE(REPLACE(@[eff_to_dt],'-',''), 'YYYYMMDD') BETWEEN EFF_FM_DT AND EFF_TO_DT + 0.999999
#end

-- Cost Tariff No
#if (${cost_trf_no} != '')
AND     M.COST_TRF_NO IN (
    #foreach ($user_costTrfNos IN ${costTrfNos})
        #if($velocityCount < $costTrfNos.size())
            '$user_costTrfNos',
        #else
            '$user_costTrfNos'
        #end
    #end
                         )
#end

-- Pre/Post
#if (${pctl_io_bnd_cd} != 'ALL')
AND     D.PCTL_IO_BND_CD = @[pctl_io_bnd_cd]
#end

-- From
#if (${from_nod_cd} != '')
AND     (    SUBSTR(D.FM_NOD_CD, 1,2) IN (
    #foreach ($user_fromNodCds IN ${fromNodCds})
        #if($velocityCount < $fromNodCds.size())
            '$user_fromNodCds',
        #else
            '$user_fromNodCds'
        #end
    #end
                                         )
          OR SUBSTR(D.FM_NOD_CD, 1,5) IN (
    #foreach ($user_fromNodCds IN ${fromNodCds})
        #if($velocityCount < $fromNodCds.size())
            '$user_fromNodCds',
        #else
            '$user_fromNodCds'
        #end
    #end
                                         )
          OR D.FM_NOD_CD IN (
    #foreach ($user_fromNodCds IN ${fromNodCds})
        #if($velocityCount < $fromNodCds.size())
            '$user_fromNodCds',
        #else
            '$user_fromNodCds'
        #end
    #end
                                         )
        )
#end

-- To
#if (${to_nod_cd} != '')
AND     (    SUBSTR(D.TO_NOD_CD, 1,2) IN (
    #foreach ($user_toNodCds IN ${toNodCds})
        #if($velocityCount < $toNodCds.size())
            '$user_toNodCds',
        #else
            '$user_toNodCds'
        #end
    #end
                                           )
          OR SUBSTR(D.TO_NOD_CD, 1,5) IN (
    #foreach ($user_toNodCds IN ${toNodCds})
        #if($velocityCount < $toNodCds.size())
            '$user_toNodCds',
        #else
            '$user_toNodCds'
        #end
    #end
                                           )
          OR D.TO_NOD_CD IN (
    #foreach ($user_toNodCds IN ${toNodCds})
        #if($velocityCount < $toNodCds.size())
            '$user_toNodCds',
        #else
            '$user_toNodCds'
        #end
    #end
                              )
        )
#end

#if (${bnt_flg} == 'N')
AND     ROWNUM < 3001
#end

ORDER BY
        M.COST_TRF_NO
      , M.COST_TRF_STS_CD
      , M.EFF_FM_DT
      , D.FM_NOD_CD
      , D.TO_NOD_CD
      , D.PCTL_IO_BND_CD			]]></sql>
			<params>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="pctl_io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
