<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyWrongCNTRUSQL">
			<desc><![CDATA[ModifyWrongCNTR]]></desc>
			<sql><![CDATA[
        update sce_cop_hdr
            set cntr_no = @[iWrongCntrNo],
                cnmv_yr = @[iWrongCnmvYr]
        where cop_no = @[chgCopNo]			]]></sql>
			<params>
				<param name="iWrongCntrNo" type="12" value="" out="N"/>
				<param name="iWrongCnmvYr" type="12" value="" out="N"/>
				<param name="chgCopNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
