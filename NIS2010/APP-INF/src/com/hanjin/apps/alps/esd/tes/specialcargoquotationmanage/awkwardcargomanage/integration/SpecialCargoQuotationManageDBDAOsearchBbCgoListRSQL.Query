<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOsearchBbCgoListRSQL">
			<desc><![CDATA[searchBbCgoList]]></desc>
			<sql><![CDATA[
SELECT 
a.DIM_LEN
,	a.DIM_WDT
,	a.DIM_HGT
,	a.GRS_WGT
,	a.WGT_UT_CD
FROM BKG_BB_CGO a
WHERE	a.BKG_NO = @[bkg_no]
ORDER BY a.BB_CGO_SEQ			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
