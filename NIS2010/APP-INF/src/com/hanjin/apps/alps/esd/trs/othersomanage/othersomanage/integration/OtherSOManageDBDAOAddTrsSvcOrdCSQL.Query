	@[COST_MODE]									,	
	@[FORM_USR_OFC_CD]								,	
	@[FORM_CRE_USR_ID]								,	
	SYSDATE											,	
	@[FORM_CRE_USR_ID]								,	
	SYSDATE											,	
	GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC( @[FORM_USR_OFC_CD])	,	
	GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC( @[FORM_USR_OFC_CD])	,	
	'N'		                                        ,
    @[WTR_RCV_TERM_CD]                              ,
    @[WTR_DE_TERM_CD]								,
	@[TTL_DIST]										,
	@[LNK_DIST_DIV_CD]								,
	@[NEGO_RMK]

)			]]></sql>
