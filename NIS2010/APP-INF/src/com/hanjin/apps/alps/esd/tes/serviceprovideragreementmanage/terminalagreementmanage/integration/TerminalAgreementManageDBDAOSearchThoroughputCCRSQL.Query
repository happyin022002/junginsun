		<query name="TerminalAgreementManageDBDAOSearchThoroughputCCRSQL">
			<desc><![CDATA[Thoroughput Cost Code 조회]]></desc>
			<sql><![CDATA[
SELECT	a.lgs_cost_cd
		, a.lgs_cost_full_nm
FROM	TES_LGS_COST a, TES_TML_AGMT_VRFY_MZD b
WHERE	a.lgs_cost_cd = b.lgs_cost_cd 
AND		b.tml_thrp_cost_cd_flg	= 'Y'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
