<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceSurchargeDBDAOsearchInvoiceSurchargeInquiryRSQL">
			<desc><![CDATA[search Invoice Surcharge Inquiry]]></desc>
			<sql><![CDATA[
SELECT a.lgs_cost_cd,
	(select lgs_cost_full_nm from tes_lgs_cost where lgs_cost_cd = a.lgs_cost_cd) lgs_cost_nm,
	NVL(a.inv_scg_amt,'0') scg_amt,
	DECODE(SUBSTR(a.lgs_cost_cd,3,LENGTH(lgs_cost_cd)-1),
						  'ALAL', '', 	--Additional Labor
						  'LWAL', '', 	--Barge low water surcharge
					   	  'CHAL', '',--(select chss_tpsz_desc from chs_chss_tp_sz where chss_tpsz_cd = a.inv_chss_mgst_tpsz_cd), 	//CHZ Usage
						  'DPAL', '', 	--Drop & Pull (Drop & Pickup)
						  'DRAL', (select intg_cd_val_dp_desc from com_intg_cd_dtl where intg_cd_id = 'CD00820' and intg_cd_val_ctnt = a.inv_dry_run_rlbl_pty_tp_cd), 	--Dry Run
						  'FRAL', '', 	--Ferry Cost
						  'FIAL', a.inv_fne_cuz_desc, 	--Fine
	                      'FGAL', (select intg_cd_val_dp_desc from com_intg_cd_dtl where intg_cd_id = 'CD00821' and intg_cd_val_ctnt = a.inv_fumg_cost_tp_cd), 	--Fumigation
						  'GNAL', '',--(select mgst_tp_nm from mgs_mgst_tp_sz where mgst_tp_cd = a.inv_mgst_tpsz_cd), 	//Gen-set
						  'HZAL', '', 	--HAZMAT
	                      'INAL', (select intg_cd_val_dp_desc from com_intg_cd_dtl where intg_cd_id = 'CD00822' and intg_cd_val_ctnt = a.inv_insp_rf_pti_cstms_tp_cd), --Inspection
					      'LFAL', a.inv_lftg_knt || ',' || a.inv_lftg_cuz_desc, 	--Lifting Charge
						  'MDAL', (select yd_nm from mdm_yard where yd_cd = a.inv_stop_loc_nod_cd), 	--Multistop Delivery
						  'OSAL', '', 	--Over Size
						  'OWAL', a.inv_grs_wgt, 	--Over Weight
						  'PPAL', a.inv_incrt_dt, 	--Pre-Pull
						  'RCAL', '', 	--Redirection Charge
						  'SSAL', (select yd_nm from mdm_yard where yd_cd = a.inv_scl_stop_plc_nod_cd), 	--Scale Stop
				 	      'SRAL', a.inv_sto_dys, 	--Storage
						  'STAL', a.inv_ob_bkg_no, 	--Street Turn
						  'SNAL', '', 	--Sunday Running
					      'SFAL', '', 	--Swing/Flip
						  'TDAL', '', 	--T-DOC Fee
					      'TLAL', '', 	--Toll
						  'WTAL', a.inv_wt_hrs, 	--Waiting Charge
						  'OTAL', a.inv_otr_rmk, 	--Other Surcharge
						  'FUWD', '', --Fuel Surcharge ? WD
						  'FURD', '', --Fuel Surcharge ? RD
						  'FUTD', '', --Fuel Surcharge ? TD
						  'FUWR', '', --Fuel Surcharge ? WR
						  'FURT', '', --Fuel Surcharge ? RT
						  'CDAL',  a.INV_CHSS_NO || ',' || TO_CHAR(a.INV_INCUR_DT, 'YYYYMMDD') --Chassis Drayage  
        							  
		 ) inv_scg_opt
FROM trs_trsp_scg_dtl a
WHERE a.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
  AND a.trsp_so_seq = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
