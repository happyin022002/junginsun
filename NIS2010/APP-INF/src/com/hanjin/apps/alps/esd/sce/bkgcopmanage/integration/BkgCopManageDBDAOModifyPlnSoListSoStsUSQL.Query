<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifyPlnSoListSoStsUSQL">
			<desc><![CDATA[sce_pln_so_list 의 so status 를 변경 update 한다. (fm_cop_no 의 od 영역의 so status 를 unmatch 건이 아닐 경우에만 가져온다)]]></desc>
			<sql><![CDATA[
update sce_pln_so_list a
set
	trsp_so_sts_cd = NVL((select trsp_so_sts_cd from sce_pln_so_list where cop_no = @[fm_cop_no]
						and cost_act_grp_cd like 'OD%' and rownum = 1), a.trsp_so_sts_cd)
where
	cop_no = @[to_cop_no]
	and cost_act_grp_cd like 'OD%'			]]></sql>
			<params>
				<param name="fm_cop_no" type="12" value="" out="N"/>
				<param name="to_cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
