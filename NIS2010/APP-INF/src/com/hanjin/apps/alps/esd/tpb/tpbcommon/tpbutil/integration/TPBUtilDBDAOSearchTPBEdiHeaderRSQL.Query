<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TPBUtilDBDAOSearchTPBEdiHeaderRSQL">
			<desc><![CDATA[헤더를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT '$$$MSGSTART:'||
        RPAD(NVL(TRIM(@[sndr_id]),' '),20,' ')||
        RPAD(NVL(TRIM(@[rcv_id]),' '),20,' ')||
        RPAD(NVL(TRIM(@[msg_id]),' '),10,' ')||
        RPAD(NVL(TRIM('TPI'),' '),3) ||
        TO_CHAR(SYSDATE,'rrmmdd') ||
        LTRIM(TO_CHAR(TPB_EDI_SEQ.NEXTVAL, '000009')) STR_FLATFILE
FROM DUAL 			]]></sql>
			<params>
				<param name="sndr_id" type="12" value="" out="N"/>
				<param name="rcv_id" type="12" value="" out="N"/>
				<param name="msg_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
