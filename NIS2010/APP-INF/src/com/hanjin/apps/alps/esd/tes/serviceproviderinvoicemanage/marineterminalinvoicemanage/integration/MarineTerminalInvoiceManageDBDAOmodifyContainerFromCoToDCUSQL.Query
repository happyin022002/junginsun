<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOmodifyContainerFromCoToDCUSQL">
			<desc><![CDATA[modifyContainerFromCoToDC]]></desc>
			<sql><![CDATA[
UPDATE TES_TML_SO_CNTR_LIST
SET  VRFY_RSLT_IND_CD = 'DC',
     DSCR_IND_CD = 'FR',
	 UPD_USR_ID = @[upd_usr_id],
	 UPD_DT = SYSDATE
WHERE	 TML_SO_OFC_CTY_CD =     @[tml_so_ofc_cty_cd]
AND	     TML_SO_SEQ =            @[tml_so_seq]
AND      VSL_CD =                @[vsl_cd] 
AND      SKD_VOY_NO =            @[skd_voy_no] 
AND      SKD_DIR_CD =            @[skd_dir_cd] 
AND      IO_BND_CD  =            @[io_bnd_cd]
AND     ( RCVDE_TERM_IND_CD like '%I%' or RCVDE_TERM_IND_CD like '%O%' )			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
