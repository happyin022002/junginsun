<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDetailInCBySPPUSQL">
			<desc><![CDATA[ModifyCopDetailInCBySPP]]></desc>
			<sql><![CDATA[
        UPDATE sce_cop_dtl
        SET     act_sts_cd = 'F'
                ,act_dt     = to_date(@[in_act_dt],'YYYYMMDDHH24MISS')
                ,act_rcv_tp_cd = @[in_act_rcv_tp_cd]
                ,upd_usr_id = @[in_usr_id]
                ,upd_dt     = SYSDATE
                ,ACT_DAT_RCV_DT =  TO_DATE(@[in_act_dat_rcv_dt],'YYYY/MM/DD HH24:MI:SS')
        WHERE  act_sts_cd = 'C'
        AND    cop_no      = @[v_cop_no]
        AND    cop_dtl_seq = @[v_cop_dtl_seq]			]]></sql>
			<params>
				<param name="in_act_dt" type="12" value="" out="N"/>
				<param name="in_act_rcv_tp_cd" type="12" value="" out="N"/>
				<param name="in_usr_id" type="12" value="" out="N"/>
				<param name="in_act_dat_rcv_dt" type="12" value="" out="N"/>
				<param name="v_cop_no" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
