<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCreateManageDBDAOUpdatePrdDtlByAutoIRGUSQL">
			<desc><![CDATA[Update new Auto Inland Route Guide Sequence]]></desc>
			<sql><![CDATA[
UPDATE PRD_PROD_CTL_ROUT_DTL
   SET ROUT_SEQ = TO_NUMBER(@[rout_seq])
WHERE PCTL_NO LIKE @[hd_pctl_no] || '%'
 AND PCTL_IO_BND_CD = @[io_bnd_cd]
 AND ROUT_SEQ = -1 -- 향후 Sequence 반영을 감안해 999999999에서 -1로 대체함
 AND ROUT_ORG_NOD_CD  = @[rout_org_nod_cd]  -- 시작노드와 종료노드가 같으면 동일한 Route이므로 PC번호와 상관없이 Update
 AND ROUT_DEST_NOD_CD = @[rout_dest_nod_cd]			]]></sql>
			<params>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="hd_pctl_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="rout_org_nod_cd" type="12" value="" out="N"/>
				<param name="rout_dest_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
