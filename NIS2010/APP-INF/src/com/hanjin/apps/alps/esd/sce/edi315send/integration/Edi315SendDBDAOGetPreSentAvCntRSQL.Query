<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetPreSentAvCntRSQL">
			<desc><![CDATA[GetPreSentAvCnt]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM SCE_EDI_SND_RSLT
 WHERE 1=1
   AND BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]
   AND EDI_GRP_CD = @[edi_grp_cd]
   AND EDI_STS_CD IN (SELECT ORG_EDI_STS_CD
                        FROM SCE_EDI_MNG_STS
                       WHERE PST_EDI_GRP_CD = @[edi_grp_cd]
                         AND EDI_PST_STS_CD = @[edi_sts]
                         AND ORG_EDI_STS_CD = @[org_edi_sts]
                      )			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="edi_grp_cd" type="12" value="" out="N"/>
				<param name="edi_sts" type="12" value="" out="N"/>
				<param name="org_edi_sts" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
