<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOInitializeSceActRcvIfUSQL">
			<desc><![CDATA[SCE_ACT_RCV_IF 의 bkg_no, cntr_no 에 해당되는 interface list 를 초기화 한다.]]></desc>
			<sql><![CDATA[
update sce_act_rcv_if
set
act_umch_tp_cd = '00',
cop_evnt_seq = (case when cop_evnt_seq >= 6 then 0 else cop_evnt_seq end)
where
bkg_no = @[bkg_no]
and cntr_no = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
