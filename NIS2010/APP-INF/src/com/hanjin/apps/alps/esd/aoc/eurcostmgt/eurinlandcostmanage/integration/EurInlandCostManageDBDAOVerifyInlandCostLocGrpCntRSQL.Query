<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EurInlandCostManageDBDAOVerifyInlandCostLocGrpCntRSQL">
			<desc><![CDATA[2012.11.26 변종건 [CHM-201220395-01] TF 참여기간 이후의 후속작업]]></desc>
			<sql><![CDATA[
SELECT  COUNT(1) CNT
FROM    AOC_EUR_INLND_TRF_DTL
WHERE   1 = 1
AND     COST_TRF_NO = @[in_cost_trf_no]
AND     LOC_GRP_NO IS NULL
AND     COST_SEL_ROUT_FLG = 'Y'
AND     DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
