<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOmodifyAwkCgoBasicTrfHdrUSQL">
			<desc><![CDATA[modifyAwkCgoBasicTrfHdr]]></desc>
			<sql><![CDATA[
MERGE INTO TES_AWK_CGO_TRF_HDR V
USING  DUAL
ON (    V.YD_CD = @[yd_cd]
)
WHEN MATCHED THEN
    UPDATE SET
		MN_YD_FLG = DECODE(@[mn_yd_flg],'1','Y','0','N','N'),
        UPD_USR_ID = @[upd_usr_id],
        UPD_DT = SYSDATE
WHEN NOT MATCHED THEN
    INSERT (
        YD_CD,
        MN_YD_FLG,
        DELT_FLG,
        CRE_USR_ID,
        CRE_DT,
    	UPD_USR_ID,
    	UPD_DT
	) VALUES (
    	@[yd_cd],
        DECODE(@[mn_yd_flg],'1','Y','0','N','N'),
		'N',
        (SELECT 
		Y.OFC_CD
		FROM MDM_YARD Y
		WHERE 1=1
		AND Y.YD_CD LIKE @[yd_cd]),
        SYSDATE, 
        @[upd_usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="mn_yd_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
