<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOSearchContainerInfoRSQL">
			<desc><![CDATA[RailInvoice에 대한 Container 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
    @[sSeq] SEQ,
    (
        SELECT 
            CNTR_TPSZ_CD
        FROM 
            MST_CONTAINER
        WHERE CNTR_NO = @[sCntrNo]
    ) EQ_TPSZ_CD
FROM DUAL			]]></sql>
			<params>
				<param name="sSeq" type="12" value="" out="N"/>
				<param name="sCntrNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
