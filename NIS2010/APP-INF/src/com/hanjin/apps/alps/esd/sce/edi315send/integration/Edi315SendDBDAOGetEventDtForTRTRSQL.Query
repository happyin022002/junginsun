<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetEventDtForTRTRSQL">
			<desc><![CDATA[GetEventDtForTRT]]></desc>
			<sql><![CDATA[
SELECT to_char(ACT_DT + TO_NUMBER(@[delay_t])/24,'yyyymmddhh24miss') AS ACT_DT
FROM (
SELECT *
FROM SCE_EDI_SND_RSLT
WHERE 1=1
AND EDI_GRP_CD = @[edi_grp_cd]
AND EDI_STS_CD = @[edi_tp]
AND EDI_SUB_STS_CD =@[edi_sub_sts_cd]
AND CNTR_NO = @[cntr_no]
AND BKG_NO = @[bkg_no]
ORDER BY EDI_SND_KNT DESC
)
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="delay_t" type="12" value="" out="N"/>
				<param name="edi_grp_cd" type="12" value="" out="N"/>
				<param name="edi_tp" type="12" value="" out="N"/>
				<param name="edi_sub_sts_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
