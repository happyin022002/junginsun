<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetForwardRefnoRSQL">
			<desc><![CDATA[for getForwardRefno]]></desc>
			<sql><![CDATA[
select REF_NO FW_REF_NBR
from bkg_customer 
where 1=1
AND BKG_NO = @[e_bkg_no]
and BKG_CUST_TP_CD  = 'F'			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
