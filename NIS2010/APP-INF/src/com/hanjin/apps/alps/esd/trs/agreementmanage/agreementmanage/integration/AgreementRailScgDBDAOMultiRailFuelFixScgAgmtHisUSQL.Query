<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRailScgDBDAOMultiRailFuelFixScgAgmtHisUSQL">
			<desc><![CDATA[US RAIL Surcharge 화면의 US RAIL Agreement Fixed, Fuel Surcharge History Update]]></desc>
			<sql><![CDATA[
UPDATE TRS_AGMT_RAIL_SCG_RT_HIS A
       SET DELT_FLG = 'Y'
      ,UPD_USR_ID = @[sUsrId]
      ,UPD_DT = SYSDATE
WHERE (TRSP_AGMT_SCG_SEQ, TRSP_AGMT_RT_HIS_SEQ) IN (
         SELECT TRSP_AGMT_SCG_SEQ, MAX(TRSP_AGMT_RT_HIS_SEQ)
           FROM TRS_AGMT_RAIL_SCG_RT_HIS
          WHERE TRSP_AGMT_SCG_SEQ   = @[trsp_agmt_scg_seq]
          GROUP BY TRSP_AGMT_SCG_SEQ
       )			]]></sql>
			<params>
				<param name="sUsrId" type="12" value="" out="N"/>
				<param name="trsp_agmt_scg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
