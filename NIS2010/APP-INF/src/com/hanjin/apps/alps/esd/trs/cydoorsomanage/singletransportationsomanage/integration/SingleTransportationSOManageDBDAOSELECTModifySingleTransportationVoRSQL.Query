<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSELECTModifySingleTransportationVoRSQL">
			<desc><![CDATA[S/O Correction SO Seq No 조회]]></desc>
			<sql><![CDATA[
SELECT
  TRSP_SO_OFC_CTY_CD
  TRSP_SO_SEQ
FROM
  TRS_TRSP_SVC_ORD A
WHERE
   TRSP_SO_OFC_CTY_CD = @[SoOfcCtyCd]
AND TRSP_SO_SEQ       = @[SoSeq]
AND DELT_FLG          = 'N'
 			]]></sql>
			<params>
				<param name="SoOfcCtyCd" type="12" value="1" out="N"/>
				<param name="SoSeq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
