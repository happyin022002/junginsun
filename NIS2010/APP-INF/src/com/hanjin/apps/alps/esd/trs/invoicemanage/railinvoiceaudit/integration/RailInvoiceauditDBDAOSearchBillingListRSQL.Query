<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOSearchBillingListRSQL">
			<desc><![CDATA[searchBillingList SELECT]]></desc>
			<sql><![CDATA[
SELECT
       Z.TRSP_SO_OFC_CTY_CD
     , Z.TRSP_SO_SEQ
     , Z.RAIL_CMB_THRU_TP_CD
     , Z.CGO_TP_CD
     , Z.CURR_CD
     , SUM(Z.BZC_AMT)           AS BZC_AMT
     , SUM(Z.FUEL_SCG_AMT)      AS FUEL_SCG_AMT
     , SUM(Z.OVR_WGT_SCG_AMT)   AS OVR_WGT_SCG_AMT
     , SUM(Z.HZD_MTRL_SCG_AMT)  AS HZD_MTRL_SCG_AMT
     , SUM(Z.ETC_ADD_AMT)   	AS ETC_ADD_AMT
     , SUM(Z.NEGO_AMT)          AS NEGO_AMT
     , Z.RAIL_BIL_DT
     , Z.EQ_TPSZ_CD
     , Z.FM_NOD_CD
     , SUBSTR(Z.FM_NOD_CD, 1,5) AS FM_NOD_CD1
     , SUBSTR(Z.FM_NOD_CD, 6,2) AS FM_NOD_CD2
     , Z.TO_NOD_CD
     , SUBSTR(Z.TO_NOD_CD, 1,5) AS TO_NOD_CD1
     , SUBSTR(Z.TO_NOD_CD, 6,2) AS TO_NOD_CD2
     , INV_NO
     , TRSP_INV_AUD_STS_CD
     , VNDR_SEQ
     , VNDR_ABBR_NM
     , SPCL_INSTR_RMK
     , 'SML' TRSP_INV_CO_IND_CD
FROM
    (
	 SELECT
           B.TRSP_SO_OFC_CTY_CD
         , B.TRSP_SO_SEQ
         , A.RAIL_CMB_THRU_TP_CD
         , A.CGO_TP_CD
         , B.CURR_CD
         , B.BZC_AMT
         , B.FUEL_SCG_AMT
         , B.OVR_WGT_SCG_AMT
         , B.HZD_MTRL_SCG_AMT
         , B.ETC_ADD_AMT
         , B.NEGO_AMT
         , TO_CHAR(A.LOCL_CRE_DT,'YYYYMMDD') AS RAIL_BIL_DT
         , A.EQ_NO
         , A.EQ_TPSZ_CD
         , CASE WHEN A.RAIL_CMB_THRU_TP_CD IN('C2T','C2C','C3T') THEN
          (
           SELECT C.FM_NOD_CD
           FROM   TRS_TRSP_RAIL_BIL_VNDR_SET C
           WHERE  C.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD
           AND    C.TRSP_SO_SEQ        = B.TRSP_SO_SEQ
           AND    C.SUB_RAIL_SEQ       = 1
          )
           WHEN A.RAIL_CMB_THRU_TP_CD IN('C3S') AND B.SUB_RAIL_SEQ IN (1, 2) THEN
          (
           SELECT C.FM_NOD_CD
           FROM   TRS_TRSP_RAIL_BIL_VNDR_SET C
           WHERE  C.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD
           AND    C.TRSP_SO_SEQ        = B.TRSP_SO_SEQ
           AND    C.SUB_RAIL_SEQ       = 1
          )
           ELSE B.FM_NOD_CD
           END FM_NOD_CD
         , CASE WHEN A.RAIL_CMB_THRU_TP_CD IN('C2T','C2C') THEN
          (
           SELECT C.TO_NOD_CD
           FROM   TRS_TRSP_RAIL_BIL_VNDR_SET C
           WHERE  C.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD
           AND    C.TRSP_SO_SEQ        = B.TRSP_SO_SEQ
           AND    C.SUB_RAIL_SEQ       = 2
          )
           WHEN A.RAIL_CMB_THRU_TP_CD IN('C3T') THEN
          (
           SELECT C.TO_NOD_CD
           FROM   TRS_TRSP_RAIL_BIL_VNDR_SET C
           WHERE  C.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD
           AND    C.TRSP_SO_SEQ        = B.TRSP_SO_SEQ
           AND    C.SUB_RAIL_SEQ       = 3
          )
           WHEN A.RAIL_CMB_THRU_TP_CD IN('C3S') AND B.SUB_RAIL_SEQ IN (1,2) THEN
          (
           SELECT C.TO_NOD_CD
           FROM   TRS_TRSP_RAIL_BIL_VNDR_SET C
           WHERE  C.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD
           AND    C.TRSP_SO_SEQ        = B.TRSP_SO_SEQ
           AND    C.SUB_RAIL_SEQ       = 2
          )
           ELSE B.TO_NOD_CD
           END TO_NOD_CD
         , D.INV_NO
         , D.TRSP_INV_AUD_STS_CD
         , C.VNDR_SEQ
         , C.VNDR_ABBR_NM
         , A.SPCL_INSTR_RMK
         , E.WBL_DT
     FROM  TRS_TRSP_RAIL_BIL_ORD       A
         , TRS_TRSP_RAIL_BIL_VNDR_SET  B
         , MDM_VENDOR                  C
         , TRS_TRSP_RAIL_INV_WRK       D
         , TRS_TRSP_RAIL_INV_DTL       E
     WHERE A.TRSP_SO_OFC_CTY_CD  = B.TRSP_SO_OFC_CTY_CD
     AND   A.TRSP_SO_SEQ         = B.TRSP_SO_SEQ
     AND   A.RAIL_CMB_THRU_TP_CD IS NOT NULL
     AND   B.INV_VNDR_SEQ        IS NULL
     AND   B.PAIR_VNDR_SEQ       = @[rail_road_code]
     AND   A.EQ_NO               = @[eq_no]
     AND   A.TRSP_SO_STS_CD IN ('I','C')
     AND   A.DELT_FLG            ='N'
     AND   A.LOCL_CRE_DT BETWEEN ADD_MONTHS(SYSDATE, -12)
     AND   SYSDATE
     AND   B.PAIR_VNDR_SEQ      = C.VNDR_SEQ
     AND   A.TRSP_SO_OFC_CTY_CD = E.TRSP_SO_OFC_CTY_CD(+)
     AND   A.TRSP_SO_SEQ        = E.TRSP_SO_SEQ(+)	
	 AND   A.EQ_NO            	= E.EQ_NO(+)	
	 AND   @[rail_road_code]    = E.INV_VNDR_SEQ(+)
     AND   E.INV_NO             = D.INV_NO(+)
     AND   E.INV_VNDR_SEQ       = D.INV_VNDR_SEQ(+)
     AND   D.DELT_FLG(+)        ='N'
    ) Z
GROUP BY
      Z.TRSP_SO_OFC_CTY_CD
    , Z.TRSP_SO_SEQ
    , Z.RAIL_CMB_THRU_TP_CD
    , Z.CGO_TP_CD
    , Z.CURR_CD
    , Z.RAIL_BIL_DT
    , Z.EQ_NO
    , Z.EQ_TPSZ_CD
    , Z.FM_NOD_CD
    , Z.TO_NOD_CD
    , Z.INV_NO
    , Z.TRSP_INV_AUD_STS_CD
    , Z.VNDR_SEQ
    , Z.VNDR_ABBR_NM
    , Z.SPCL_INSTR_RMK

UNION ALL

SELECT
       Z.TRSP_SO_OFC_CTY_CD
     , Z.TRSP_SO_SEQ
     , '' RAIL_CMB_THRU_TP_CD
     , Z.CGO_TP_CD
     , Z.CURR_CD
     , SUM(Z.BZC_AMT)     AS BZC_AMT
     , 0                  AS FUEL_SCG_AMT
     , 0                  AS OVR_WGT_SCG_AMT
     , 0                  AS HZD_MTRL_SCG_AMT
     , SUM(Z.ETC_ADD_AMT) AS ETC_ADD_AMT
     , 0                  AS NEGO_AMT
     , Z.RAIL_BIL_DT
     , Z.EQ_TPSZ_CD
     , Z.FM_NOD_CD
     , SUBSTR(Z.FM_NOD_CD, 1,5) AS FM_NOD_CD1
     , SUBSTR(Z.FM_NOD_CD, 6,2) AS FM_NOD_CD2
     , Z.TO_NOD_CD
     , SUBSTR(Z.TO_NOD_CD, 1,5) AS TO_NOD_CD1
     , SUBSTR(Z.TO_NOD_CD, 6,2) AS TO_NOD_CD2
     , INV_NO
     , TRSP_INV_AUD_STS_CD
     , VNDR_SEQ
     , VNDR_ABBR_NM
     , ''                       AS SPCL_INSTR_RMK
     , DECODE(Z.TRSP_CO_IND_CD ,'T', 'DOM','H', 'NIS','S', 'SEN') AS TRSP_INV_CO_IND_CD
FROM
    (
     SELECT
           A.CNTR_NO AS EQ_NO
         , F.CNTR_TPSZ_CD AS EQ_TPSZ_CD
         , A.TRSP_SO_OFC_CTY_CD
         , A.TRSP_SO_SEQ
         , 'F' AS CGO_TP_CD
         , 'USD' AS CURR_CD
         , B.AGMT_RT_AMT AS BZC_AMT
         , NVL('', 0) AS ETC_ADD_AMT
         , CASE WHEN A.DMST_INV_STY_CD = 'T' THEN
          (
           SELECT FM_YD_CD
           FROM   DOM_RAIL_SO_MST       DOM_A
                , DOM_RAIL_SO_DTL       DOM_B
           WHERE  DOM_A.TRSP_SO_OFC_CTY_CD = DOM_B.TRSP_SO_OFC_CTY_CD
           AND    DOM_A.TRSP_SO_SEQ = DOM_B.TRSP_SO_SEQ
           AND    DOM_A.TRSP_SO_OFC_CTY_CD = A.TRSP_SO_OFC_CTY_CD
           AND    DOM_A.TRSP_SO_SEQ = A.TRSP_SO_SEQ 
           AND    DOM_A.CNTR_NO = A.CNTR_NO
           AND    DOM_B.SUB_RAIL_SEQ  = 1
          )
           ELSE B.FM_YD_CD
           END  FM_NOD_CD
         , CASE WHEN A.DMST_INV_STY_CD = 'T' THEN
          (
           NVL(
               (SELECT TO_YD_CD
                FROM   DOM_RAIL_SO_MST       DOM_A
                     , DOM_RAIL_SO_DTL       DOM_B
                WHERE  DOM_A.TRSP_SO_OFC_CTY_CD = DOM_B.TRSP_SO_OFC_CTY_CD
                AND    DOM_A.TRSP_SO_SEQ       = DOM_B.TRSP_SO_SEQ
                AND    DOM_A.TRSP_SO_OFC_CTY_CD= A.TRSP_SO_OFC_CTY_CD
                AND    DOM_A.TRSP_SO_SEQ       = A.TRSP_SO_SEQ
                AND    DOM_A.CNTR_NO             = A.CNTR_NO
                AND    DOM_B.SUB_RAIL_SEQ  = 2
              ),
              (SELECT TO_YD_CD
               FROM   DOM_RAIL_SO_MST       DOM_A
                    , DOM_RAIL_SO_DTL       DOM_B
               WHERE  DOM_A.TRSP_SO_OFC_CTY_CD = DOM_B.TRSP_SO_OFC_CTY_CD
               AND    DOM_A.TRSP_SO_SEQ       = DOM_B.TRSP_SO_SEQ
               AND    DOM_A.TRSP_SO_OFC_CTY_CD= A.TRSP_SO_OFC_CTY_CD
               AND    DOM_A.TRSP_SO_SEQ       = A.TRSP_SO_SEQ
               AND    DOM_A.CNTR_NO              = A.CNTR_NO
               AND    DOM_B.SUB_RAIL_SEQ   = 1
              )
             )
           )
           ELSE B.TO_YD_CD
           END TO_NOD_CD
         , TO_CHAR(A.CRE_DT ,'YYYYMMDD') AS RAIL_BIL_DT
         , D.INV_NO
         , D.TRSP_INV_AUD_STS_CD
         , C.VNDR_SEQ
         , C.VNDR_ABBR_NM
         , E.WBL_DT
         , 'T' AS TRSP_CO_IND_CD
     FROM  DOM_RAIL_SO_MST       A
         , DOM_RAIL_SO_DTL       B
         , MDM_VENDOR            C
         , TRS_TRSP_RAIL_INV_WRK D
         , TRS_TRSP_RAIL_INV_DTL E
         , MST_CONTAINER         F
         , DOM_BOOKING           G
    WHERE  A.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD
    AND    A.TRSP_SO_SEQ = B.TRSP_SO_SEQ
	AND	   B.PAIR_VNDR_SEQ = @[rail_road_code]
    AND    A.CNTR_NO = @[eq_no]
	AND    A.DELT_FLG          = 'N'
    AND    A.CRE_DT	BETWEEN ADD_MONTHS(SYSDATE, -12) AND SYSDATE  
    AND    B.PAIR_VNDR_SEQ = C.VNDR_SEQ
	AND    A.CNTR_NO = F.CNTR_NO
    AND    A.DMST_BKG_NO = G.DMST_BKG_NO
    AND    A.CNTR_NO = G.CNTR_NO
    AND    A.TRSP_SO_OFC_CTY_CD= E.TRSP_SO_OFC_CTY_CD(+)
    AND    A.TRSP_SO_SEQ       = E.TRSP_SO_SEQ(+)
    AND    A.CNTR_NO           = E.EQ_NO(+)	
	AND    @[rail_road_code]   = E.INV_VNDR_SEQ(+)
    AND    E.INV_NO            = D.INV_NO(+)
    AND    E.INV_VNDR_SEQ      = D.INV_VNDR_SEQ(+)
    AND    D.DELT_FLG(+)       ='N'
	AND    ( 
				D.TRSP_INV_AUD_STS_CD IS NULL 
			 	OR D.TRSP_INV_AUD_STS_CD ='SV'
			)
    ) Z
GROUP BY
      Z.TRSP_SO_OFC_CTY_CD
    , Z.TRSP_SO_SEQ
    , Z.CGO_TP_CD
    , Z.CURR_CD
    , Z.RAIL_BIL_DT
    , Z.EQ_NO
    , Z.EQ_TPSZ_CD
    , Z.FM_NOD_CD
    , Z.TO_NOD_CD
    , Z.INV_NO
    , Z.TRSP_INV_AUD_STS_CD
    , Z.VNDR_SEQ
    , Z.VNDR_ABBR_NM
    , Z.TRSP_CO_IND_CD			]]></sql>
			<params>
				<param name="rail_road_code" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
