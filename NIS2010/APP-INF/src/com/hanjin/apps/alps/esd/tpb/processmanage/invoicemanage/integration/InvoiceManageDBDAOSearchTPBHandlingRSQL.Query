<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOSearchTPBHandlingRSQL">
			<desc><![CDATA[TPB Handling]]></desc>
			<sql><![CDATA[
SELECT   /*+ INDEX(B XAK1TPB_OTS_GRP) */
         A.N3PTY_NO
       , B.N3PTY_INV_NO
       , 0 AS LST_N3PTY_INV_RVIS_SEQ
       , '---' AS N3PTY_INV_RVIS_CD
       , B.N3PTY_EXPN_TP_CD
       , B.N3PTY_BIL_TP_CD
       , TPB_GET_N3PTY_BIL_TP_NM_FNC(B.N3PTY_BIL_TP_CD) AS N3PTY_BIL_TP_NM
       , NVL(A.N3PTY_TP_CD,'G') AS  N3PTY_IF_TP_CD
       , A.N3PTY_SRC_NO
       , A.EQ_NO
       , A.VNDR_CUST_DIV_CD
       , DECODE(B.VNDR_CUST_DIV_CD,'V',LPAD(B.VNDR_SEQ,6,0),'C',B.CUST_CNT_CD||LPAD(B.CUST_SEQ,6,0),'S',B.N3PTY_OFC_CD,'') AS N3PTY_CD
       , DECODE(B.VNDR_CUST_DIV_CD,'V',VNDR_LGL_ENG_NM,'C',CUST_LGL_ENG_NM,'S',B.N3PTY_OFC_CD,'') AS N3PTY_NM
       , B.VSL_CD||B.SKD_VOY_NO||B.FINC_DIR_CD AS REV_VVD
       , A.IF_CURR_CD, (SELECT SUM(IF_AMT) FROM TPB_OTS_DTL WHERE N3PTY_NO = B.N3PTY_NO) AS IF_AMT
       , TPB_GET_USD_AMT_FNC((SELECT SUM(IF_AMT) FROM TPB_OTS_DTL WHERE N3PTY_NO = B.N3PTY_NO),A.IF_CURR_CD,TPB_GET_LCL_DATE_FNC(A.IF_DT,B.OFC_CD)) AS IF_AMT_USD
       , A.CFM_CURR_CD AS CURR_CD
       , B.OTS_AMT
       , B.INV_AMT AS RVS_AMT
       , CASE WHEN ( SELECT COUNT(1) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ) = 0 THEN A.CFM_USR_ID
    	      ELSE ( SELECT CRE_USR_ID FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO AND OTS_STS_SEQ = ( SELECT MAX(OTS_STS_SEQ) FROM TPB_OTS_GRP_STS WHERE N3PTY_NO = B.N3PTY_NO AND OTS_STS_CD = 'T' ) )
       	 END AS CFM_USR_ID
       , CASE WHEN ( SELECT COUNT(1) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO) = 0 THEN ( SELECT USR_NM FROM COM_USER U WHERE U.USR_ID = A.CFM_USR_ID )
    	      ELSE ( SELECT USR_NM FROM COM_USER U WHERE U.USR_ID = (SELECT CRE_USR_ID FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO AND OTS_STS_SEQ = (SELECT MAX(OTS_STS_SEQ) FROM TPB_OTS_GRP_STS WHERE N3PTY_NO = B.N3PTY_NO AND OTS_STS_CD = 'T' ) ) )
       	 END AS CFM_USR_NM
       , CASE WHEN ( SELECT COUNT(1) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ) = 0 THEN TO_CHAR(TPB_GET_LCL_DATE_FNC(B.CFM_DT,@[s_user_ofc_cd]),'YYYY-MM-DD HH24:MI')
    	      ELSE TO_CHAR(TPB_GET_LCL_DATE_FNC(( SELECT MAX(CRE_DT) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ),@[s_user_ofc_cd]),'YYYY-MM-DD HH24:MI')
    	 END AS CFM_DT
       , CASE WHEN ( SELECT COUNT(1) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ) = 0 THEN TRUNC(SYSDATE - B.CFM_DT)
    	      ELSE TRUNC(SYSDATE - ( SELECT MAX(CRE_DT) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ))
    	 END AS OVERDUE
       , '' AS ERPIF_USR_ID
       , '' AS ERPIF_USR_NM
       , '' AS ERPIF_DT
       , TO_CHAR(TPB_GET_RCVR_ACT_YN_FNC(B.N3PTY_NO)) AS RCVR_ACT_YN
       , CASE WHEN A.VNDR_CUST_DIV_CD = 'V' THEN 'Y'
              WHEN A.VNDR_CUST_DIV_CD = 'C' THEN 'Y'
              ELSE 'N'
         END AS INVOICE_ABLE
       , 'N' AS REVISE_ABLE
       , 'N' AS ERPIF_ABLE
       , ( SELECT COUNT(DISTINCT N3PTY_BIL_TP_CD) FROM TPB_OTS_DTL K WHERE K.N3PTY_NO = A.N3PTY_NO ) AS LENGTH_N3PTY_BIL_TP_CD
       , '' AS IDA_TAX_SEQ
       , CASE WHEN B.VNDR_CUST_DIV_CD = 'V' THEN TO_CHAR(B.VNDR_SEQ)
              WHEN B.VNDR_CUST_DIV_CD = 'C' THEN B.CUST_CNT_CD||TO_CHAR(B.CUST_SEQ)
              WHEN B.VNDR_CUST_DIV_CD = 'S' THEN B.N3PTY_OFC_CD
              ELSE ''
         END AS N3PTY_CD_O
       , CASE WHEN ( SELECT COUNT(*) FROM TPB_ADJ_STS WHERE N3PTY_NO = B.N3PTY_NO AND STL_STS_LST_FLG = 'Y' AND N3PTY_STL_TP_CD = 'O' AND STL_APRO_OFC_CD = STL_TO_CLT_CNG_OFC_CD AND B.N3PTY_INV_NO IS NULL ) = 0 THEN 'N' ELSE 'Y' END AS ROCROLLBACK_ABLE
FROM     TPB_OTS_DTL A
       , TPB_OTS_GRP B
       , TPB_OTS_GRP_STS C
WHERE    1 = 1
AND      A.N3PTY_NO = B.N3PTY_NO
AND      B.N3PTY_NO = C.N3PTY_NO
AND      A.N3PTY_NO_DP_SEQ = 1
AND      A.N3PTY_DELT_TP_CD IN ('N','S')
AND      B.N3PTY_DELT_TP_CD = 'N'
AND      C.OTS_STS_LST_FLG = 'Y'
AND      C.OTS_STS_CD IN ('O','M','J')

#if (${n3pty_if_tp_cd} != '')
AND      A.N3PTY_BIL_TP_CD IN ( SELECT N3PTY_BIL_TP_CD FROM TPB_N3RD_PTY_BIL_TP WHERE N3PTY_IF_TP_CD = @[n3pty_if_tp_cd] )
#end

#if (${s_n3pty_no} == '' && ${s_n3pty_inv_no_search} == '' && ${s_bkg_no_all} == '' && ${s_bl_no_all} == '')
	#if (${s_status} == 'O')
AND      C.OTS_STS_CD IN ('O','M','J')
	#elseif (${s_status} != 'O' && ${s_status} != '')
AND      1 = 0
	#end
#end

#if (${s_n3pty_no_strs_link} != '' && $s_n3pty_no_strs_link.size() > 0)
AND      A.N3PTY_NO IN ( NULL
    #foreach($s_n3pty_no_strs_link IN ${s_n3pty_no_strs_link})
        #if($velocityCount < $s_n3pty_no_strs_link.size())
	 	,'$s_n3pty_no_strs_link',
	 	#else
	 	,'$s_n3pty_no_strs_link'
	 	#end
    #end
	 )
#else
	#if (${s_n3pty_no} == '' && ${s_n3pty_inv_no_search} == '' && ${s_sdate} != '' && ${s_edate} != '')
AND      B.CFM_DT BETWEEN TPB_GET_SRCH_DATE_FNC(@[s_sdate],'YYYY-MM-DD',@[s_user_ofc_cd]) AND TPB_GET_SRCH_DATE_FNC(@[s_edate],'YYYY-MM-DD',@[s_user_ofc_cd]) + 1
	#end
	#if (${s_n3pty_no} != '')
AND      A.N3PTY_NO = @[s_n3pty_no]
	#end
	#if (${s_n3pty_inv_no_search} != '')
AND      1 = 0
	#end
	#if (${s_n3pty_expn_tp_cd} != '')
AND      B.N3PTY_EXPN_TP_CD = @[s_n3pty_expn_tp_cd]
	#end
	#if (${s_n3pty_src_no} != '')
AND      A.N3PTY_SRC_NO = @[s_n3pty_src_no]
	#end
	#if (${s_vvd} != '')
AND      B.VSL_CD = SUBSTRB(@[s_vvd],1,4)
AND      B.SKD_VOY_NO = SUBSTRB(@[s_vvd],5,4)
AND      B.FINC_DIR_CD LIKE SUBSTRB(@[s_vvd],9)||'%'
	#end
	#if (${s_bkg_no_all} != '')
AND      A.BKG_NO = @[s_bkg_no_all]
	#end
	#if (${s_bl_no_all} != '')
AND      A.BL_NO = @[s_bl_no_all]
	#end
	#if (${s_eq_knd_cd} != '')
AND      A.EQ_KND_CD = @[s_eq_knd_cd]
	#end
	#if (${s_eq_no} != '')
AND      A.EQ_NO = @[s_eq_no]
	#end
	#if ( ${s_vndr_cust_div_cd} != '') 
AND      A.VNDR_CUST_DIV_CD = @[s_vndr_cust_div_cd]
		#if ( ${s_trd_party_val} != '') 
			#if ( ${s_vndr_cust_div_cd} == 'V') 
AND      A.VNDR_SEQ = TO_NUMBER(@[s_trd_party_val])
			#elseif( ${s_vndr_cust_div_cd} == 'C')
AND      A.CUST_CNT_CD = SUBSTRB(TRIM(@[s_trd_party_val]),1,2)
				#if ( $s_trd_party_val.length() > 2)
AND      A.CUST_SEQ = TO_NUMBER(SUBSTRB(TRIM(@[s_trd_party_val]),3))
				#end
			#elseif( ${s_vndr_cust_div_cd} == 'S') 
AND      A.N3PTY_OFC_CD = @[s_trd_party_val]
			#end
		#end
	#end
#end
UNION ALL
SELECT   /*+ INDEX(B XAK1TPB_OTS_GRP) */
         A.N3PTY_NO
       , B.N3PTY_INV_NO
       , R.N3PTY_INV_RVIS_SEQ AS LST_N3PTY_INV_RVIS_SEQ
       , R.N3PTY_INV_RVIS_CD
       , B.N3PTY_EXPN_TP_CD
       , B.N3PTY_BIL_TP_CD
       , TPB_GET_N3PTY_BIL_TP_NM_FNC(B.N3PTY_BIL_TP_CD) AS N3PTY_BIL_TP_NM
       , NVL(A.N3PTY_TP_CD,'G') AS  N3PTY_IF_TP_CD
       , A.N3PTY_SRC_NO
       , A.EQ_NO
       , A.VNDR_CUST_DIV_CD
       , DECODE(B.VNDR_CUST_DIV_CD,'V',LPAD(B.VNDR_SEQ,6,0),'C',B.CUST_CNT_CD||LPAD(B.CUST_SEQ,6,0),'S',B.N3PTY_OFC_CD,'') AS N3PTY_CD
       , DECODE(B.VNDR_CUST_DIV_CD,'V',VNDR_LGL_ENG_NM,'C',CUST_LGL_ENG_NM,'S',B.N3PTY_OFC_CD,'') AS N3PTY_NM
       , B.VSL_CD||B.SKD_VOY_NO||B.FINC_DIR_CD AS REV_VVD
       , A.IF_CURR_CD, (SELECT SUM(IF_AMT) FROM TPB_OTS_DTL WHERE N3PTY_NO = B.N3PTY_NO) AS IF_AMT
       , TPB_GET_USD_AMT_FNC((SELECT SUM(IF_AMT) FROM TPB_OTS_DTL WHERE N3PTY_NO = B.N3PTY_NO),A.IF_CURR_CD,TPB_GET_LCL_DATE_FNC(A.IF_DT,B.OFC_CD)) AS IF_AMT_USD
       , A.CFM_CURR_CD AS CURR_CD
       , B.OTS_AMT
       , B.INV_AMT AS RVS_AMT
       , CASE WHEN ( SELECT COUNT(1) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ) = 0 THEN A.CFM_USR_ID
    	      ELSE ( SELECT CRE_USR_ID FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO AND OTS_STS_SEQ = ( SELECT MAX(OTS_STS_SEQ) FROM TPB_OTS_GRP_STS WHERE N3PTY_NO = B.N3PTY_NO AND OTS_STS_CD = 'T' ) )
       	 END AS CFM_USR_ID
       , CASE WHEN ( SELECT COUNT(1) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO) = 0 THEN ( SELECT USR_NM FROM COM_USER U WHERE U.USR_ID = A.CFM_USR_ID )
    	      ELSE ( SELECT USR_NM FROM COM_USER U WHERE U.USR_ID = (SELECT CRE_USR_ID FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO AND OTS_STS_SEQ = (SELECT MAX(OTS_STS_SEQ) FROM TPB_OTS_GRP_STS WHERE N3PTY_NO = B.N3PTY_NO AND OTS_STS_CD = 'T' ) ) )
       	 END AS CFM_USR_NM
       , CASE WHEN ( SELECT COUNT(1) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ) = 0 THEN TO_CHAR(TPB_GET_LCL_DATE_FNC(B.CFM_DT,@[s_user_ofc_cd]),'YYYY-MM-DD HH24:MI')
    	      ELSE TO_CHAR(TPB_GET_LCL_DATE_FNC(( SELECT MAX(CRE_DT) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD = 'T' AND N3PTY_NO = B.N3PTY_NO ),@[s_user_ofc_cd]),'YYYY-MM-DD HH24:MI')
    	 END AS CFM_DT
       , TRUNC(SYSDATE - B.CFM_DT) AS OVERDUE
       , NVL2(R.CLT_DT, R.UPD_USR_ID, '') AS ERPIF_USR_ID
       , NVL2(R.CLT_DT, ( SELECT NVL(MAX(USR_NM), R.UPD_USR_ID) FROM COM_USER U WHERE U.USR_ID(+) = R.UPD_USR_ID ), '') AS ERPIF_USR_NM
       , TO_CHAR(TPB_GET_LCL_DATE_FNC(R.CLT_DT,@[s_user_ofc_cd]),'YYYY-MM-DD HH24:MI') AS ERPIF_DT
       , TO_CHAR(TPB_GET_RCVR_ACT_YN_FNC(B.N3PTY_NO)) AS RCVR_ACT_YN
       , 'N' AS INVOICE_ABLE
       , 'Y' AS REVISE_ABLE
       , CASE WHEN R.CLT_AGN_FLG = 'Y' THEN 'N'
       	      WHEN R.N3PTY_INV_STS_CD = 'N' THEN 'Y'
    	      ELSE 'N'
    	 END AS ERPIF_ABLE
       , ( SELECT COUNT(DISTINCT N3PTY_BIL_TP_CD) FROM TPB_OTS_DTL K WHERE K.N3PTY_NO=A.N3PTY_NO ) AS LENGTH_N3PTY_BIL_TP_CD
       , TO_CHAR(R.IDA_TAX_SEQ) AS IDA_TAX_SEQ
       , CASE WHEN B.VNDR_CUST_DIV_CD = 'V' THEN TO_CHAR(B.VNDR_SEQ)
              WHEN B.VNDR_CUST_DIV_CD = 'C' THEN B.CUST_CNT_CD||TO_CHAR(B.CUST_SEQ)
              WHEN B.VNDR_CUST_DIV_CD = 'S' THEN B.N3PTY_OFC_CD
              ELSE ''
         END AS N3PTY_CD_O
       , CASE WHEN ( SELECT COUNT(*) FROM TPB_ADJ_STS WHERE N3PTY_NO = B.N3PTY_NO AND STL_STS_LST_FLG = 'Y' AND N3PTY_STL_TP_CD = 'O' AND STL_APRO_OFC_CD = STL_TO_CLT_CNG_OFC_CD AND B.N3PTY_INV_NO IS NULL ) = 0 THEN 'N' ELSE 'Y' END AS ROCROLLBACK_ABLE
FROM     TPB_OTS_DTL A
       , TPB_OTS_GRP B
       , TPB_OTS_GRP_STS C
       , TPB_INVOICE V
       , TPB_INV_RVIS R
WHERE    1 = 1
AND      A.N3PTY_NO = B.N3PTY_NO
AND      B.N3PTY_NO = C.N3PTY_NO
AND      B.N3PTY_INV_NO = V.N3PTY_INV_NO
AND      B.N3PTY_INV_NO = R.N3PTY_INV_NO
AND      V.LST_N3PTY_INV_RVIS_SEQ = R.N3PTY_INV_RVIS_SEQ
AND      A.N3PTY_NO_DP_SEQ = 1
AND      A.N3PTY_DELT_TP_CD IN ('N','S')
AND      B.N3PTY_DELT_TP_CD = 'N'
AND      C.OTS_STS_LST_FLG = 'Y'
AND      C.OTS_STS_CD IN ('I','Y','A','L','N','E')
AND      V.N3PTY_DELT_TP_CD = 'N'
AND      R.N3PTY_DELT_TP_CD = 'N'
AND      B.N3PTY_BIL_TP_CD != 'JO'
AND      B.OFC_CD = @[s_user_ofc_cd]

#if (${n3pty_if_tp_cd} != '')
AND      A.N3PTY_BIL_TP_CD IN ( SELECT N3PTY_BIL_TP_CD FROM TPB_N3RD_PTY_BIL_TP WHERE N3PTY_IF_TP_CD = @[n3pty_if_tp_cd] )
#end

#if (${if_flag} == 'N')
AND      R.CLT_DT IS NULL
#end

#if (${s_n3pty_no} == '' && ${s_n3pty_inv_no_search} == '' && ${s_bkg_no_all} == '' && ${s_bl_no_all} == '')
	#if (${s_status} == 'O')
AND      1 = 0
	#elseif (${s_status} == 'I')
AND      C.OTS_STS_CD IN ('I','Y','N')
	#elseif (${s_status} == 'E')
AND      C.OTS_STS_CD IN ('A','L','E')
	#end
#end

#if (${s_n3pty_no_strs_link} != '' && $s_n3pty_no_strs_link.size() > 0)
AND      A.N3PTY_NO IN ( NULL
	 #foreach($s_n3pty_no_strs_link IN ${s_n3pty_no_strs_link})
	 	#if($velocityCount < $s_n3pty_no_strs_link.size())
	 	,'$s_n3pty_no_strs_link',
	 	#else
	 	,'$s_n3pty_no_strs_link'
	 	#end
	 #end
         )
#else
	#if (${s_n3pty_no} == '' && ${s_n3pty_inv_no_search} == '' && ${s_sdate} != '' && ${s_edate} != '')
AND      B.CFM_DT BETWEEN TPB_GET_SRCH_DATE_FNC(@[s_sdate],'YYYY-MM-DD',@[s_user_ofc_cd]) AND TPB_GET_SRCH_DATE_FNC(@[s_edate],'YYYY-MM-DD',@[s_user_ofc_cd]) + 1
	#end
	#if (${s_n3pty_no} != '')
AND      A.N3PTY_NO = @[s_n3pty_no]
	#end
	#if (${s_n3pty_inv_no_search} != '')
AND      B.N3PTY_INV_NO = @[s_n3pty_inv_no_search]
	#end
	#if (${s_n3pty_expn_tp_cd} != '')
AND      B.N3PTY_EXPN_TP_CD = @[s_n3pty_expn_tp_cd]
	#end
	#if (${s_n3pty_src_no} != '')
AND      A.N3PTY_SRC_NO = @[s_n3pty_src_no]
	#end
	#if (${s_vvd} != '')
AND      B.VSL_CD = SUBSTRB(@[s_vvd],1,4)
AND      B.SKD_VOY_NO = SUBSTRB(@[s_vvd],5,4)
AND      B.FINC_DIR_CD LIKE SUBSTRB(@[s_vvd],9)||'%'
	#end
	#if (${s_bkg_no_all} != '')
AND      A.BKG_NO = @[s_bkg_no_all]
	#end
	#if (${s_bl_no_all} != '')
AND      A.BL_NO = @[s_bl_no_all]
	#end
	#if (${s_eq_knd_cd} != '')
AND      A.EQ_KND_CD = @[s_eq_knd_cd]
	#end
	#if (${s_eq_no} != '')
AND      A.EQ_NO = @[s_eq_no]
	#end
	#if ( ${s_vndr_cust_div_cd} != '') 
AND      A.VNDR_CUST_DIV_CD = @[s_vndr_cust_div_cd]
		#if ( ${s_trd_party_val} != '') 
			#if ( ${s_vndr_cust_div_cd} == 'V') 
AND      A.VNDR_SEQ = TO_NUMBER(@[s_trd_party_val])
			#elseif( ${s_vndr_cust_div_cd} == 'C')
AND      A.CUST_CNT_CD = SUBSTRB(TRIM(@[s_trd_party_val]),1,2)
				#if ( $s_trd_party_val.length() > 2)
AND      A.CUST_SEQ = TO_NUMBER(SUBSTRB(TRIM(@[s_trd_party_val]),3))
				#end
			#elseif( ${s_vndr_cust_div_cd} == 'S') 
AND      A.N3PTY_OFC_CD = @[s_trd_party_val]
			#end
		#end
	#end
#end			]]></sql>
			<params>
				<param name="s_user_ofc_cd" type="12" value="" out="N"/>
				<param name="n3pty_if_tp_cd" type="12" value="" out="N"/>
				<param name="s_sdate" type="12" value="" out="N"/>
				<param name="s_edate" type="12" value="" out="N"/>
				<param name="s_n3pty_no" type="12" value="" out="N"/>
				<param name="s_n3pty_expn_tp_cd" type="12" value="" out="N"/>
				<param name="s_n3pty_src_no" type="12" value="" out="N"/>
				<param name="s_vvd" type="12" value="" out="N"/>
				<param name="s_bkg_no_all" type="12" value="" out="N"/>
				<param name="s_bl_no_all" type="12" value="" out="N"/>
				<param name="s_eq_knd_cd" type="12" value="" out="N"/>
				<param name="s_eq_no" type="12" value="" out="N"/>
				<param name="s_vndr_cust_div_cd" type="12" value="" out="N"/>
				<param name="s_trd_party_val" type="12" value="" out="N"/>
				<param name="s_n3pty_inv_no_search" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
