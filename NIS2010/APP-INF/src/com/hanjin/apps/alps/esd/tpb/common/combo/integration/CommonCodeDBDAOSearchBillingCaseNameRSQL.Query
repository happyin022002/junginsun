<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchBillingCaseNameRSQL">
			<desc><![CDATA[SearchBillingCaseName]]></desc>
			<sql><![CDATA[
SELECT n3pty_bil_tp_cd AS cd,
       n3pty_bil_tp_nm AS name,
       n3pty_bil_tp_cd AS ord
  FROM TPB_N3RD_PTY_BIL_TP
 WHERE act_flg = 'Y'
#if (${s_bil_tp_cd} == '')
   AND n3pty_expn_tp_cd = @[s_n3pty_expn_tp_cd]
#else
   AND n3pty_bil_tp_cd = @[s_bil_tp_cd]
#end
#if (${s_n3pty_if_tp_cd} != '')
   AND n3pty_if_tp_cd = @[s_n3pty_if_tp_cd]
#end
#if (${s_jo_display} == 'N')
   AND n3pty_bil_tp_cd != 'JO'
#end
 ORDER BY ord			]]></sql>
			<params>
				<param name="s_n3pty_expn_tp_cd" type="12" value="" out="N"/>
				<param name="s_bil_tp_cd" type="12" value="" out="N"/>
				<param name="s_n3pty_if_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
