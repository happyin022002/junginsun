<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PodManageDBDAOUpdatePodUSQL">
			<desc><![CDATA[UpdatePod]]></desc>
			<sql><![CDATA[
UPDATE PRD_POD_MGMT
   SET ARR_STE_CD = @[del_state] 
     , APLY_SVC_MOD_FLG = @[service] 
 	 , BKG_RMK = @[remarks] 
 	 , DELT_FLG = DECODE(@[del_flag] , '1', 'Y', 'N')
     , UPD_OFC_CD = @[upd_ofc_cd]
 	 , UPD_USR_ID = @[upd_usr_id] 
 	 , UPD_DT = SYSDATE
 WHERE POD_CD = @[pod_code] 
   AND SLAN_CD = @[lane_code] 
   AND DEL_CD = @[del_code] 
   AND BKG_DE_TERM_CD = @[del_term] 
   AND TRSP_MOD_CD = @[trans_mode]
   AND PCTL_IO_BND_CD = @[pctl_io_bnd_cd]			]]></sql>
			<params>
				<param name="del_state" type="12" value="" out="N"/>
				<param name="service" type="12" value="" out="N"/>
				<param name="remarks" type="12" value="" out="N"/>
				<param name="del_flag" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pod_code" type="12" value="" out="N"/>
				<param name="lane_code" type="12" value="" out="N"/>
				<param name="del_code" type="12" value="" out="N"/>
				<param name="del_term" type="12" value="" out="N"/>
				<param name="trans_mode" type="12" value="" out="N"/>
				<param name="pctl_io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
