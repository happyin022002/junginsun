<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccrualBatchExecuteResultDBDAOSearchRHQRSQL">
			<desc><![CDATA[SearchRHQ]]></desc>
			<sql><![CDATA[
SELECT    XX.AR_HD_QTR_OFC_CD  AS RHQ
FROM      MDM_ORGANIZATION   XX
WHERE     XX.OFC_CD          = @[ofc_cd]  /* LOGIN OFFICE CODE <== SESSION INFO. */
			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
