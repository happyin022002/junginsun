<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MnrAdvanceAuditDBDAOdeleteMNRPreAuditCriterionByErrorCodeDSQL">
			<desc><![CDATA[M&R Pre-Audit Criterion By Error Code에서 사용자가 입력한 Office Code에 대한 레코드 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM EAS_MNR_PRE_AUD_VRFY_CFG A
WHERE A.AUD_OFC_CD = @[i_ofc_cd]			]]></sql>
			<params>
				<param name="i_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
