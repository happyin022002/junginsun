<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOmodifyAwkCgoAddOnTrfHdrCSQL">
			<desc><![CDATA[modifyAwkCgoAddOnTrfHdr]]></desc>
			<sql><![CDATA[
INSERT INTO TES_AWK_CGO_ADON_HDR(
        FM_LOC_CD
        ,FM_NOD_YD_NO
        ,TO_LOC_CD
        ,TO_NOD_YD_NO
        ,COND_NO
        ,TML_AWK_ADON_VER_NO
        ,VNDR_SEQ
        ,VNDR_NM
        ,CALC_RMK
        ,LST_UPD_USR_ID
        ,LST_UPD_DT
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
	) VALUES (
		@[fm_loc_cd],
        CASE WHEN @[fm_nod_yd_no] IS NULL 
        THEN ' '
        ELSE NVL(TRIM(@[fm_nod_yd_no]), ' ')
        END,
        @[to_loc_cd],
        CASE WHEN @[to_nod_yd_no] IS NULL 
        THEN ' '
        ELSE NVL(TRIM(@[to_nod_yd_no]), ' ')
        END,
		@[cond_no],
		(SELECT NVL(MAX(TML_AWK_ADON_VER_NO),0)+1
        FROM TES_AWK_CGO_ADON_HDR 
        WHERE FM_LOC_CD = @[fm_loc_cd]
        AND FM_NOD_YD_NO = CASE WHEN @[fm_nod_yd_no] IS NULL 
                           THEN ' '
                           ELSE NVL(TRIM(@[fm_nod_yd_no]), ' ')
                           END
        AND TO_LOC_CD = @[to_loc_cd]
        AND TO_NOD_YD_NO = CASE WHEN @[to_nod_yd_no] IS NULL 
                           THEN ' '
                           ELSE NVL(TRIM(@[to_nod_yd_no]), ' ')
                           END
        AND COND_NO = @[cond_no]), 
        '',
        @[vndr_nm],
        @[calc_rmk],
        @[lst_upd_usr_id],
        SYSDATE,
        CASE WHEN (SELECT NVL(MAX(TML_AWK_ADON_VER_NO),0)+1
		FROM TES_AWK_CGO_ADON_HDR
		WHERE FM_LOC_CD = @[fm_loc_cd]
        AND FM_NOD_YD_NO = CASE WHEN @[fm_nod_yd_no] IS NULL 
                           THEN ' '
                           ELSE NVL(TRIM(@[fm_nod_yd_no]), ' ')
                           END
        AND TO_LOC_CD = @[to_loc_cd]
        AND TO_NOD_YD_NO = CASE WHEN @[to_nod_yd_no] IS NULL 
                           THEN ' '
                           ELSE NVL(TRIM(@[to_nod_yd_no]), ' ')
                           END
		AND COND_NO = @[cond_no]) > 1 
		THEN 
			(SELECT CRE_USR_ID
			FROM TES_AWK_CGO_ADON_HDR
			WHERE FM_LOC_CD = @[fm_loc_cd]
            AND FM_NOD_YD_NO = CASE WHEN @[fm_nod_yd_no] IS NULL 
                               THEN ' '
                               ELSE NVL(TRIM(@[fm_nod_yd_no]), ' ')
                               END
            AND TO_LOC_CD = @[to_loc_cd]
            AND TO_NOD_YD_NO = CASE WHEN @[to_nod_yd_no] IS NULL 
                               THEN ' '
                               ELSE NVL(TRIM(@[to_nod_yd_no]), ' ')
                               END
			AND COND_NO = @[cond_no]
			AND TML_AWK_ADON_VER_NO = (SELECT NVL(MAX(TML_AWK_ADON_VER_NO),0)
									  FROM TES_AWK_CGO_ADON_HDR
									  WHERE FM_LOC_CD = @[fm_loc_cd]
                                        AND FM_NOD_YD_NO = CASE WHEN @[fm_nod_yd_no] IS NULL 
                               							   THEN ' '
                               							   ELSE NVL(TRIM(@[fm_nod_yd_no]), ' ')
                               							   END
                                        AND TO_LOC_CD = @[to_loc_cd]
                                        AND TO_NOD_YD_NO = CASE WHEN @[to_nod_yd_no] IS NULL 
                               							   THEN ' '
                               							   ELSE NVL(TRIM(@[to_nod_yd_no]), ' ')
                               							   END
                                        AND COND_NO = @[cond_no])
            )
		ELSE (SELECT OFC_CD
			  FROM (
    		 		SELECT 
    		 		DISTINCT
    		 		Y.OFC_CD, 
    		 		COUNT(Y.OFC_CD) OVER (PARTITION BY SUBSTR(Y.YD_CD,1,5), Y.OFC_CD) CNT
    		 		FROM MDM_YARD Y
    		 		WHERE 1=1
    		 		AND Y.YD_CD LIKE @[fm_loc_cd]||@[fm_nod_yd_no]||'%' --5자리 이상
    		 		ORDER BY CNT DESC
    				)
			  WHERE ROWNUM = 1) 
		END, 
        SYSDATE, 
        @[upd_usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="fm_loc_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yd_no" type="12" value="" out="N"/>
				<param name="to_loc_cd" type="12" value="" out="N"/>
				<param name="to_nod_yd_no" type="12" value="" out="N"/>
				<param name="cond_no" type="12" value="" out="N"/>
				<param name="vndr_nm" type="12" value="" out="N"/>
				<param name="calc_rmk" type="12" value="" out="N"/>
				<param name="lst_upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
