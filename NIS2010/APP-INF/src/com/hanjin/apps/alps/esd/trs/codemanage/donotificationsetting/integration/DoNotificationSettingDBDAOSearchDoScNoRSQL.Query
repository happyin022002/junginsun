<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DoNotificationSettingDBDAOSearchDoScNoRSQL">
			<desc><![CDATA[D/O S/C정보 조회]]></desc>
			<sql><![CDATA[
SELECT A3.CUST_CNT_CD|| LPAD(A3.CUST_SEQ, 6, 0) CUST_CD
      ,(SELECT X.CUST_LGL_ENG_NM FROM MDM_CUSTOMER X WHERE X.CUST_CNT_CD = A3.CUST_CNT_CD AND X.CUST_SEQ = A3.CUST_SEQ) CUST_NM
      ,TO_CHAR(A4.CTRT_EFF_DT,'YYYYMMDD') EFF_DT
      ,TO_CHAR(A4.CTRT_EXP_DT,'YYYYMMDD') EXP_DT
FROM PRI_SP_HDR A1
    ,PRI_SP_MN A2
    ,PRI_SP_CTRT_PTY A3
    ,PRI_SP_DUR A4
    ,PRI_SP_MQC A5
WHERE A1.SC_NO = @[sc_no]
  AND A1.PROP_NO = A2.PROP_NO
  AND A2.AMDT_SEQ = (SELECT MAX(AMDT_SEQ)
                      FROM PRI_SP_MN
                     WHERE PROP_NO = A1.PROP_NO
                       AND PROP_STS_CD = 'F')
  AND A2.PROP_NO  = A3.PROP_NO
  AND A2.AMDT_SEQ = A3.AMDT_SEQ
  AND A3.PRC_CTRT_PTY_TP_CD = 'C'
  AND A2.PROP_NO  = A4.PROP_NO
  AND A2.AMDT_SEQ = A4.AMDT_SEQ
  AND A2.PROP_NO  = A5.PROP_NO
  AND A2.AMDT_SEQ = A5.AMDT_SEQ 			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
