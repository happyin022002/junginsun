<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanRouteConditionManageDBDAOMergeCallingTmlMtxExptUSQL">
			<desc><![CDATA[MergeCallingTmlMtxExpt]]></desc>
			<sql><![CDATA[
MERGE INTO prd_port_tml_expt c  
    USING (  
            SELECT @[port_cd]  port_cd, @[vsl_slan_cd]  vsl_slan_cd, @[skd_dir_cd]  skd_dir_cd, @[tml_cd]  tml_cd  
              FROM DUAL) t  
    ON( c.port_cd  = t.port_cd AND c.vsl_slan_cd = t.vsl_slan_cd  
        AND c.skd_dir_cd= t.skd_dir_cd AND c.tml_cd = t.tml_cd )  
 WHEN MATCHED THEN  
    UPDATE SET c.upd_usr_id = @[upd_usr_id]   
      ,c.upd_dt = SYSDATE  
 WHEN NOT MATCHED THEN  


    INSERT
(
PORT_CD                                                                                                                                                                                                           
,VSL_SLAN_CD                                                                                                                                                                                                    
,TML_CD                                                                                                                                                                                                         
,SKD_DIR_CD                                                                                                                                                                                                  
,DELT_FLG                                                                                                                                                                                                 
,CRE_USR_ID                                                                                                                                                                                           
,CRE_DT                                                                                                                                                                                                          
,UPD_USR_ID                                                                                                                                                                                            
,UPD_DT                                                                                                                                                                                                                 
)

 VALUES (@[port_cd] , @[vsl_slan_cd] , @[tml_cd] , @[skd_dir_cd] , 'N' ,
			@[cre_usr_id], SYSDATE  ,@[upd_usr_id] , SYSDATE
            )			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="tml_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
