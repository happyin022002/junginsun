<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOSearchCNTRNumberRSQL">
			<desc><![CDATA[SearchCNTRNumber]]></desc>
			<sql><![CDATA[
SELECT P.TML_SO_OFC_CTY_CD, P.TML_SO_SEQ, P.TML_SO_TMP_SEQ, P.CNTR_NO TMP_CNTR_NO, C.CNTR_NO MST_CNTR_NO
FROM   TES_FILE_IMP_TMP P, MST_CONTAINER C
WHERE  P.tml_so_ofc_cty_cd = @[tml_so_ofc_cty_cd]
AND    P.tml_so_seq        = @[tml_so_seq]
AND    SUBSTR(P.CNTR_NO,1,10) = SUBSTR(C.CNTR_NO,1,10)
			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
