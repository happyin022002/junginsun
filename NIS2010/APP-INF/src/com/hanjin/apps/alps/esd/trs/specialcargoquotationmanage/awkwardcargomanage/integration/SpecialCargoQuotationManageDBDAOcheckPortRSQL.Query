<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOcheckPortRSQL">
			<desc><![CDATA[port 를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT 'X' AS FLAG
FROM   MDM_LOCATION
WHERE  CALL_PORT_FLG = 'Y'
AND    DELT_FLG = 'N'
#if (${yd_cd} != '')
AND    LOC_CD = @[yd_cd] --5자리 Port Code
#end			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
