				<param name="shpr_ev_grd_cd" type="12" value="" out="N"/>
				<param name="ev_ctnt" type="12" value="" out="N"/>
				<param name="HID_CRE_USR_ID" type="12" value="" out="N"/>
				<param name="HID_CRE_DT" type="12" value="" out="N"/>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
			</params>
		</query>
