SET	wo_ev_grd_cd	 	= @[wo_ev_grd_cd]			  			
	,shpr_ev_grd_cd	 	= @[shpr_ev_grd_cd]			  			
	,ev_ctnt	 		= nvl(@[ev_ctnt],'')
