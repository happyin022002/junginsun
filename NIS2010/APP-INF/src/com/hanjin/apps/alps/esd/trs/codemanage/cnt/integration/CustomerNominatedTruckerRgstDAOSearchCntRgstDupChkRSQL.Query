<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerNominatedTruckerRgstDAOSearchCntRgstDupChkRSQL">
			<desc><![CDATA[등록된 CNT가 있는지 확인]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(*) > 0  THEN 'Y'
            ELSE 'N'
        END AS DUP_FLG     
  FROM TRS_CUST_NOMI_TRKR A
 WHERE 1=1 
  AND A.CTRT_CUST_CNT_CD = SUBSTR(@[ctrt_cust_cd],1,2)
  AND A.CTRT_CUST_SEQ = SUBSTR(@[ctrt_cust_cd],3) 
  AND A.VNDR_SEQ = @[vndr_seq]
  AND A.ORG_NOD_CD = @[org_nod_cd] || @[org_nod_yard]
  AND A.DEST_NOD_CD = @[dest_nod_cd] || @[dest_nod_yard]
  AND A.MTY_PKUP_RTN_YD_CD = @[mty_pkup_rtn_yd_cd]
  AND A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
  AND A.PRC_CTRT_NO = @[prc_ctrt_no]			]]></sql>
			<params>
				<param name="ctrt_cust_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="org_nod_cd" type="12" value="" out="N"/>
				<param name="org_nod_yard" type="12" value="" out="N"/>
				<param name="dest_nod_cd" type="12" value="" out="N"/>
				<param name="dest_nod_yard" type="12" value="" out="N"/>
				<param name="mty_pkup_rtn_yd_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="prc_ctrt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
