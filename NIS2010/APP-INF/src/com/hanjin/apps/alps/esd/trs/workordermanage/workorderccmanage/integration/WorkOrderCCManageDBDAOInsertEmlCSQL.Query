<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderCCManageDBDAOInsertEmlCSQL">
			<desc><![CDATA[새로운 이메일 정보를 인서트한다.]]></desc>
			<sql><![CDATA[
INSERT
INTO TRS_TRSP_WRK_ORD_CC_EML
(
    VNDR_SEQ
    , WO_EML
    , WO_CC_SEQ
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
)
VALUES 
(
    @[vndr_seq]
    ,REPLACE(@[wo_eml], ',', ';')
    , (
        SELECT NVL(MAX(WO_CC_SEQ), 0)+1
        FROM TRS_TRSP_WRK_ORD_CC_EML
        WHERE VNDR_SEQ = @[vndr_seq]
     ) 
     , @[cre_usr_id]
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
)			]]></sql>
			<params>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="wo_eml" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
