<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementImportDBDAOMultiCorrScgAgmtScgRateUSQL">
			<desc><![CDATA[Surcharge Rate 수정]]></desc>
			<sql><![CDATA[
UPDATE TRS_AGMT_SCG_RT
   SET EFF_FM_DT         = TO_DATE(@[eff_fm_dt],'YYYYMMDDHH24MISS')
      ,EFF_TO_DT         = TO_DATE(@[eff_to_dt],'YYYYMMDDHH24MISS')
      ,TO_WGT            = @[to_wgt]
      ,WGT_MEAS_UT_CD    = @[wgt_meas_ut_cd]
      ,CURR_CD           = @[curr_cd]
      ,TRSP_ONE_WY_RT    = @[trsp_one_wy_rt]
      ,TRSP_RND_RT       = @[trsp_rnd_rt]
      ,UPD_USR_ID        = @[fm_account_usr_id]
      ,UPD_DT            = SYSDATE
      ,EQ_KND_CD         = @[eq_knd_cd]
      ,AGMT_SCG_RT_DIV_CD = @[agmt_scg_rt_div_cd]
      ,DELT_FLG           = @[delt_flg]
      ,AGMT_EXP_DT        = NULL
      ,CFM_FLG           = 'N'
      ,CFM_USR_ID        = NULL
      ,CFM_DT            = NULL
 WHERE TRSP_AGMT_OFC_CTY_CD   = @[trsp_agmt_ofc_cty_cd]
   AND TRSP_AGMT_SEQ          = @[trsp_agmt_seq]
   AND TRSP_AGMT_RT_TP_SER_NO = @[trsp_agmt_rt_tp_ser_no]
   AND TRSP_AGMT_SCG_NOD_SEQ      = @[trsp_agmt_scg_nod_seq]
   AND TRSP_AGMT_SCG_RT_SEQ       = @[trsp_agmt_scg_rt_seq]			]]></sql>
			<params>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="to_wgt" type="12" value="" out="N"/>
				<param name="wgt_meas_ut_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="trsp_one_wy_rt" type="12" value="" out="N"/>
				<param name="trsp_rnd_rt" type="12" value="" out="N"/>
				<param name="fm_account_usr_id" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="agmt_scg_rt_div_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="trsp_agmt_rt_tp_ser_no" type="12" value="" out="N"/>
				<param name="trsp_agmt_scg_nod_seq" type="12" value="" out="N"/>
				<param name="trsp_agmt_scg_rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
