<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAOSearchCheckTesYardCodeRSQL">
			<desc><![CDATA[TERMINAL의 YARD VALIDATION CHECK]]></desc>
			<sql><![CDATA[
SELECT A.YD_CD
  FROM MDM_YARD A
 WHERE A.DELT_FLG = 'N'
   AND A.YD_CD = @[yd_cd]			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
