<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchDiffRouteRSQL">
			<desc><![CDATA[planned s/o list 에서 cost_act_grp_cd, n1st_nod_cd, trsp_mod_cd, n1st_nod_cd, n2nd_nod_cd, n3rd_nod_cd, n4th_nod_cd
를 비교하여 destination cop 와 일치하지 않을 경우를 판단한다.]]></desc>
			<sql><![CDATA[
select cost_act_grp_cd,
  cost_act_grp_seq,
  trsp_mod_cd,
  n1st_nod_cd,
  n2nd_nod_cd,
  n3rd_nod_cd,
  n4th_nod_cd
from sce_pln_so_list
where cop_no = @[org_cop_no]
  and cost_act_grp_cd != 'COMN'
union
select cost_act_grp_cd,
  cost_act_grp_seq,
  trsp_mod_cd,
  n1st_nod_cd,
  n2nd_nod_cd,
  n3rd_nod_cd,
  n4th_nod_cd
from sce_pln_so_list
where cop_no = @[dest_cop_no]
  and cost_act_grp_cd != 'COMN'
minus
select cost_act_grp_cd,
  cost_act_grp_seq,
  trsp_mod_cd,
  n1st_nod_cd,
  n2nd_nod_cd,
  n3rd_nod_cd,
  n4th_nod_cd
from sce_pln_so_list
where cop_no = @[dest_cop_no]
  and cost_act_grp_cd != 'COMN'			]]></sql>
			<params>
				<param name="org_cop_no" type="12" value="" out="N"/>
				<param name="dest_cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
