<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TPBFileUploadDBDAOSearchUploadFileInfoRSQL">
			<desc><![CDATA[SearchUploadFileInfo]]></desc>
			<sql><![CDATA[
SELECT file_no,
       file_no_seq,
       file_lgc_nm,
       file_phys_nm,
       file_path_nm,
       cre_usr_id,
       TO_CHAR(TPB_GET_LCL_DATE_FNC(cre_dt,@[s_user_ofc_cd]),'YYYY/MM/DD HH24:MI:SS') AS cre_dt,
       upd_usr_id,
       TO_CHAR(TPB_GET_LCL_DATE_FNC(upd_dt,@[s_user_ofc_cd]),'YYYY/MM/DD HH24:MI:SS') AS upd_dt
  FROM TPB_TTL_FILE_MGMT 
 WHERE file_no = @[s_file_no]
   AND delt_flg = 'N'
 ORDER BY file_no_seq			]]></sql>
			<params>
				<param name="s_user_ofc_cd" type="12" value="" out="N"/>
				<param name="s_file_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
