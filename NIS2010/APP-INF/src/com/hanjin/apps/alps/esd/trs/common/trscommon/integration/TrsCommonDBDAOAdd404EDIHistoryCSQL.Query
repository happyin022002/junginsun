<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsCommonDBDAOAdd404EDIHistoryCSQL">
			<desc><![CDATA[404 EDI 생성 시 관련 history 를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_SO_HIS
(
	TRSP_SO_OFC_CTY_CD
	,TRSP_SO_SEQ
	,TRSP_HIS_SEQ
	,TRSP_SO_EVNT_CD
	,EQ_NO
	,INV_NO
	,INV_VNDR_SEQ
	,TRSP_SO_STS_CD
	,SO_ROUT_DESC
	,COP_NO
	,COST_ACT_GRP_SEQ
	,COST_ACT_GRP_CD
	,UPLN_SO_FLG
	,RPLN_UMCH_FLG
	,COP_SO_STS_CD
	,COP_SO_ROUT_DESC
	,ROUT_RPLN_FLG
	,RQST_SRC_SYS_CD
	,TRSP_SO_HIS_DESC
	,BKG_NO
	,EQ_TPSZ_CD
    ,TRSP_WO_OFC_CTY_CD
	,TRSP_WO_SEQ 
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
    ,CRE_OFC_CD
    ,LOCL_CRE_DT
)
	SELECT 
	 T.TRSP_SO_OFC_CTY_CD
	,T.TRSP_SO_SEQ
	,TRS_TRSP_SO_HIS_SEQ1.NEXTVAL
	,@[trsp_so_evnt_cd] TRSP_SO_EVNT_CD-- 각 EVENT에서 넘겨준 값
	,T.EQ_NO
	,W.INV_NO
	,W.INV_VNDR_SEQ
	,T.TRSP_SO_STS_CD
	,'('||W.TRSP_MOD_CD||') ' || W.FM_NOD_CD ||'-'|| W.TO_NOD_CD AS SO_ROUT_DESC
	,T.COP_NO
	,T.COST_ACT_GRP_SEQ
	,T.COST_ACT_GRP_CD
	,'' AS UPLN_SO_FLG
	,'' AS RPLN_UMCH_FLG
	,(SELECT TRSP_SO_STS_CD FROM SCE_PLN_SO_LIST S WHERE S.COP_NO = T.COP_NO AND S.COST_ACT_GRP_SEQ = T.COST_ACT_GRP_SEQ) COP_SO_STS_CD
	,NVL(@[cop_pln_rout_desc],'---') COP_PLN_ROUT_DESC -- UI에서 ROUTE 변경전 TRANS MODE와 ROUTE
	,NVL(@[rout_rpln_flg],'') ROUT_RPLN_FLG -- Replan 여부
	,'TRS' RQST_SRC_SYS_CD
--'Requested By SP|RQST By BKG|TRS Own Reason', 'RBSP|RBB|TOR'
#if(  ${trsp_so_evnt_cd} == 'WC' )
	, (SELECT CASE NVL(WO_RJCT_RSN, 'N') WHEN 'RBSP' THEN 'Requested By SP'  || T.BIL_ISS_KNT
							 WHEN 'RBB' THEN 'RQST By BKG' || T.BIL_ISS_KNT
							 ELSE 'TRS Own Reason'  || T.BIL_ISS_KNT
			END TRSP_SO_HIS_DESC
		FROM TRS_TRSP_EDI_RAIL_ORD WHERE TRSP_SO_OFC_CTY_CD = T.TRSP_SO_OFC_CTY_CD AND TRSP_SO_SEQ = T.TRSP_SO_SEQ AND BIL_ISS_KNT = T.BIL_ISS_KNT )
#else
	, decode(@[trsp_so_his_desc],null,'',@[trsp_so_his_desc]||', ')|| 'Vendor : '||W.VNDR_SEQ ||'/'||(select VNDR_ABBR_NM from MDM_VENDOR MV where MV.VNDR_SEQ = W.VNDR_SEQ) ||', Currency :'||W.CURR_CD ||', Basic:'||W.BZC_AMT ||', ETC Surcharge:'||W.ETC_ADD_AMT|| ', FUEL Surcharge:'||W.FUEL_SCG_AMT||', Nego:'||
W.NEGO_AMT||', AGMT NO:'||W.TRSP_AGMT_OFC_CTY_CD|| W.TRSP_AGMT_SEQ TRSP_SO_HIS_DESC
#end
	,T.BKG_NO
	,T.EQ_TPSZ_CD
    ,'' AS TRSP_WO_OFC_CTY_CD
	,'' AS TRSP_WO_SEQ 
	, @[cre_usr_id]
	, Sysdate
	, @[cre_usr_id]
	, Sysdate
    , @[cre_ofc_cd]
    , GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
	  FROM TRS_TRSP_RAIL_BIL_VNDR_SET W, TRS_TRSP_RAIL_BIL_ORD T
	 WHERE W.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
	   AND W.TRSP_SO_SEQ = @[trsp_so_seq]
	   AND W.TRSP_SO_OFC_CTY_CD = T.TRSP_SO_OFC_CTY_CD
	   and W.TRSP_SO_SEQ = T.TRSP_SO_SEQ			]]></sql>
			<params>
				<param name="trsp_so_evnt_cd" type="12" value="" out="N"/>
				<param name="cop_pln_rout_desc" type="12" value="" out="N"/>
				<param name="rout_rpln_flg" type="12" value="" out="N"/>
				<param name="trsp_so_his_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
