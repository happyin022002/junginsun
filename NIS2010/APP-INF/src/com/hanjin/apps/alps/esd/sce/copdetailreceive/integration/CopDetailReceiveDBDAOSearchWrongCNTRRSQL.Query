<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchWrongCNTRRSQL">
			<desc><![CDATA[SearchWrongCNTR]]></desc>
			<sql><![CDATA[
SELECT d.cop_no    v_cop_no
      ,d.cntr_no   i_wrong_cntr_no 
      ,d.cnmv_yr   i_wrong_cnmv_yr
FROM sce_cop_hdr d
WHERE  d.cop_no    = @[inCopNo]			]]></sql>
			<params>
				<param name="inCopNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
