<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BiddingCandidateDBDAOInsertSpotBidCSQL">
			<desc><![CDATA[TRSP_SPOT_BID 테이블에 데이터 Insert]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_SPOT_BID(
	   SPOT_BID_NO
	  ,TRSP_SO_OFC_CTY_CD
	  ,TRSP_SO_SEQ
	  ,SPOT_BID_ST_DT
	  ,SPOT_BID_DUE_DT
	  ,SPOT_BID_STS_CD
	  ,LOCL_CRE_DT
	  ,LOCL_UPD_DT
	  ,CRE_OFC_CD
	  ,CRE_USR_ID
	  ,CRE_DT
	  ,UPD_USR_ID
	  ,UPD_DT
	)VALUES(
	   @[spot_bid_no]
	  ,@[trsp_so_ofc_cty_cd]
	  ,@[trsp_so_seq]
	  ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
	  ,TO_DATE(@[spot_bid_due_dt],'YYYYMMDD HH24MISS')
	  ,'P'
	  ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
	  ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
	  ,@[cre_ofc_cd]
	  ,@[usr_id]
	  ,SYSDATE
	  ,@[usr_id]
	  ,SYSDATE
	)			]]></sql>
			<params>
				<param name="spot_bid_no" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="spot_bid_due_dt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
