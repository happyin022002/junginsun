<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MnrAudDBDAOSearchHbarInquiryByHistoryRSQL">
			<desc><![CDATA[HBar Inquiry by History 조회]]></desc>
			<sql><![CDATA[
SELECT RHQ_CD
      ,MNR_FLG_YD_CD
      ,REG_OFC_CD
      ,EQ_NO
      ,EQ_TPSZ_CD
      ,HNGR_BAR_AMD_QTY
      ,WO_NO
      ,WO_CURR_CD
      ,WO_AMT
      ,VNDR_SEQ
      ,VNDR_NM
      ,BKG_NO
      ,BKG_GET_TOKEN_FNC(BKG_SCG_INFO,1,',') AS BKG_CURR_CD
      ,BKG_GET_TOKEN_FNC(BKG_SCG_INFO,2,',') AS BKG_SCG_AMT
      ,(CASE WHEN LENGTH(BKG_GET_TOKEN_FNC(BKG_SCG_INFO,3)) > 1 THEN 'Y' END) AS BKG_SCG_MIX_FLG
      ,POR_NOD_CD
      ,POL_NOD_CD
      ,SUBSTR(OFT_IN,1,1) OFT_IN 
  FROM (
        SELECT RHQ_CD
              ,MNR_FLG_YD_CD
              ,REG_OFC_CD
              ,EQ_NO
              ,EQ_TPSZ_CD
              ,HNGR_BAR_AMD_QTY
              ,WO_NO
              ,CURR_CD AS WO_CURR_CD
              ,MNR_WRK_AMT AS WO_AMT
              ,VNDR_SEQ
              ,VNDR_NM
              ,BKG_GET_TOKEN_FNC(BKG_INFO,1) AS BKG_NO
              ,BKG_GET_TOKEN_FNC(BKG_INFO,2) AS POR_NOD_CD
              ,BKG_GET_TOKEN_FNC(BKG_INFO,3) AS POL_NOD_CD
              ,(SELECT WM_CONCAT(CURR_CD||','||CHG_UT_AMT)
                  FROM BKG_CHG_RT X
                 WHERE X.BKG_NO = BKG_GET_TOKEN_FNC(BKG_INFO,1,',')
                   AND X.CHG_CD = 'GOH'
                   AND ((X.RAT_UT_CD = AA.EQ_TPSZ_CD) OR (CASE WHEN SUBSTR(AA.EQ_TPSZ_CD, 2,1) IN ('2', '3') THEN '20'
                                                               WHEN SUBSTR(AA.EQ_TPSZ_CD, 2,1) IN ('4', '5') THEN '40'
                                                               WHEN SUBSTR(AA.EQ_TPSZ_CD, 2,1) IN ('6', '7') THEN '45'
                                                          END
                                                         ) = X.RAT_UT_CD
                                                      OR (X.RAT_UT_CD = 'BX')
                       )
               ) BKG_SCG_INFO
			   ,(SELECT WM_CONCAT(FRT_INCL_XCLD_DIV_CD)
                  FROM BKG_CHG_RT X
                 WHERE X.BKG_NO = BKG_GET_TOKEN_FNC(BKG_INFO,1,',')
                   AND X.CHG_CD = 'GOH'
                   AND ((X.RAT_UT_CD = AA.EQ_TPSZ_CD) OR (CASE WHEN SUBSTR(AA.EQ_TPSZ_CD, 2,1) IN ('2', '3') THEN '20'
                                                               WHEN SUBSTR(AA.EQ_TPSZ_CD, 2,1) IN ('4', '5') THEN '40'
                                                               WHEN SUBSTR(AA.EQ_TPSZ_CD, 2,1) IN ('6', '7') THEN '45'
                                                          END
                                                         ) = X.RAT_UT_CD
                                                      OR (X.RAT_UT_CD = 'BX')
                       )
               ) OFT_IN
          FROM (
                SELECT (SELECT TRS_COMMON_PKG.TRS_GET_RHQ_OFC_CD(A.CRE_OFC_CD) FROM DUAL)AS RHQ_CD
                      ,A.MNR_FLG_YD_CD
                      ,A.CRE_OFC_CD AS REG_OFC_CD
                      ,A.EQ_NO
                      ,A.EQ_TPSZ_CD
                      ,NVL(A.HNGR_BAR_AMD_QTY, 0) AS HNGR_BAR_AMD_QTY
                      ,B.MNR_ORD_OFC_CTY_CD||B.MNR_ORD_SEQ AS WO_NO
                      ,B.CURR_CD
                      ,B.MNR_WRK_AMT
                      ,B.VNDR_SEQ
                      ,(SELECT X.VNDR_LGL_ENG_NM FROM MDM_VENDOR X WHERE X.VNDR_SEQ = B.VNDR_SEQ) VNDR_NM
                      ,(SELECT P.BKG_NO || ',' || P.POR_NOD_CD  || ',' || P.POL_NOD_CD
                          FROM BKG_VVD O
                              ,BKG_BOOKING P
                              ,VSK_VSL_PORT_SKD Q
                              ,BKG_CONTAINER R
                         WHERE 1=1
                           AND O.BKG_NO     = P.BKG_NO
                           AND O.POL_YD_CD  = P.POL_NOD_CD
                           AND O.VSL_CD     = Q.VSL_CD
                           AND O.SKD_VOY_NO = Q.SKD_VOY_NO
                           AND O.SKD_DIR_CD = Q.SKD_DIR_CD
                           AND O.POL_CD     = Q.VPS_PORT_CD
                           AND O.POL_CLPT_IND_SEQ = Q.CLPT_IND_SEQ   
                           AND O.POL_YD_CD  = Q.YD_CD
                           AND O.BKG_NO     = R.BKG_NO
                           AND P.HNGR_FLG = 'Y'
                           AND R.HNGR_FLG = 'Y'
                           AND P.BKG_STS_CD <> 'X'
                           AND SUBSTR(Q.YD_CD,1,2) IN (SELECT Y.CNT_CD
                                                        FROM MDM_ORGANIZATION X
                                                            ,MDM_LOCATION     Y
                                                       WHERE X.LOC_CD = Y.LOC_CD
                                                         AND X.OFC_CD = A.CRE_OFC_CD)
                           AND R.CNTR_NO = A.EQ_NO
                           AND Q.VPS_ETD_DT BETWEEN A.MNR_FLG_INP_DT - 50 AND A.MNR_FLG_INP_DT + 50
                           AND ROWNUM = 1
                           ) BKG_INFO
                  FROM  MNR_FLG_HIS A
                        ,MNR_ORD_HDR B
                 WHERE  A.MNR_ORD_OFC_CTY_CD = B.MNR_ORD_OFC_CTY_CD(+)
                   AND  A.MNR_ORD_SEQ        = B.MNR_ORD_SEQ(+)
                   AND  A.MNR_FLG_TP_CD = 'HGR'
                   AND  A.MNR_STS_FLG   = 'Y'
                   #if (${s_fm_dt} != ''|| ${s_to_dt} != '')
                     AND A.MNR_FLG_INP_DT BETWEEN TO_DATE(REPLACE(@[s_fm_dt],'-',''), 'YYYYMMDD') AND TO_DATE(REPLACE(@[s_to_dt],'-',''), 'YYYYMMDD') + 0.99999
                   #end
                   AND EXISTS (SELECT  1
                                 FROM  MDM_ORGANIZATION X
                                      ,MDM_LOCATION     Y
                                WHERE  X.LOC_CD = Y.LOC_CD
                                #if ( ${s_cnt_cd} != '')
                                AND Y.CNT_CD = @[s_cnt_cd]
                                #end
                                AND X.OFC_CD = A.CRE_OFC_CD
                                )
					#if($s_cntr_no.size() > 0) 
                	  AND A.EQ_NO IN (
                    	#foreach( ${key} in ${s_cntr_no}) 
                          #if($velocityCount == 1) 
                            '$key'
                          #else 
                            , '$key'
                          #end 
                        #end
                		)
           			#end
                    #if ( ${s_wo_flg} == 'Y')
                      AND B.MNR_ORD_OFC_CTY_CD IS NOT NULL
                    #elseif ( ${s_wo_flg} == 'N')
                      AND B.MNR_ORD_OFC_CTY_CD IS NULL
                    #end
					#if ( ${s_reg_ofc_cd} != '')
					  AND A.CRE_OFC_CD = @[s_reg_ofc_cd]
					#end
               ) AA
      ) AAA
WHERE  1 = 1
       #if ( ${s_loc_cd} != '' && ${s_yd_cd} != '')
       		AND ( AAA.POR_NOD_CD LIKE @[s_loc_cd] || @[s_yd_cd]|| '%' OR AAA.POL_NOD_CD LIKE @[s_loc_cd] || @[s_yd_cd]|| '%' )
       #elseif ( ${s_loc_cd} != '' && ${s_yd_cd} == '')
       		AND ( AAA.POR_NOD_CD LIKE @[s_loc_cd] || '%' OR AAA.POL_NOD_CD LIKE @[s_loc_cd] || '%' )
       #end			]]></sql>
			<params>
				<param name="s_fm_dt" type="12" value="" out="N"/>
				<param name="s_to_dt" type="12" value="" out="N"/>
				<param name="s_cnt_cd" type="12" value="" out="N"/>
				<param name="s_reg_ofc_cd" type="12" value="" out="N"/>
				<param name="s_loc_cd" type="12" value="" out="N"/>
				<param name="s_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
