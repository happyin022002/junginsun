<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOCheckApInvHdrGLDTRSQL">
			<desc><![CDATA[AP_INV_HDR GL_DATE 비교]]></desc>
			<sql><![CDATA[
SELECT    	GL_DT									
FROM      	AP_INV_HDR  		H					
WHERE     	H.GL_DT 			IS NOT NULL			
AND 		H.CSR_NO    		= @[CSR_NO]  				
			]]></sql>
			<params>
				<param name="CSR_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
