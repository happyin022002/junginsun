<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSMSendDBDAOSearchFlatFileToBeSentRSQL">
			<desc><![CDATA[SearchFlatFileToBeSent]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_ASC (SCE_CNTR_STS_MSG_FLT_FILE XPKSCE_CNTR_STS_MSG_FLT_FILE) */ 
		  EDI_SND_YRMONDY, 
		  EDI_SND_SEQ, 
		 EDI_SND_DESC,
		 ACT_RCV_DT, 
		  ACT_RCV_NO 
		FROM SCE_CNTR_STS_MSG_FLT_FILE 
		WHERE  
#if( ${is_all_send} == 'T')
		
			 NVL(RSLT_FLG, 'N') = 'N' 
#else
			  ACT_RCV_DT = @[act_rcv_dt]
			 AND ACT_RCV_NO = @[act_rcv_no] 
			 AND NVL(RSLT_FLG, 'N') = 'N' 
#end			]]></sql>
			<params>
				<param name="act_rcv_dt" type="12" value="" out="N"/>
				<param name="act_rcv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
