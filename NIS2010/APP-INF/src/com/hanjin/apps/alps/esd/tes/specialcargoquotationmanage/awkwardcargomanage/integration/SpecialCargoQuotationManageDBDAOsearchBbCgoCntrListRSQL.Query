<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOsearchBbCgoCntrListRSQL">
			<desc><![CDATA[searchBbCgoCntrList]]></desc>
			<sql><![CDATA[
SELECT 
CNTR_NO
,	CNTR_TPSZ_CD
,	CNTR_VOL_QTY
FROM BKG_CONTAINER
WHERE	BKG_NO = @[bkg_no]
AND	BB_CGO_FLG = 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
