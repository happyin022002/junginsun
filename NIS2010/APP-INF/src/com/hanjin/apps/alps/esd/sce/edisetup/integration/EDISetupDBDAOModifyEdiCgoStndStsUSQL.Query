<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EDISetupDBDAOModifyEdiCgoStndStsUSQL">
			<desc><![CDATA[ModifyEdiCgoStndSts]]></desc>
			<sql><![CDATA[
update edi_cgo_stnd_sts set 
                             EDI_STS_SEQ =         @[status_seq]
                            ,EDI_STS_DESC =        @[status_desc]
                            ,EDI_ORG_TP_CD =       @[origin_cd]
                            ,EDI_DEST_TP_CD =      @[dest_cd]
                            ,EAI_EVNT_DT = to_date(@[eai_date],'yyyy/mm/dd hh24:mi:ss')
                        where   
                                EDI_STND_STS_CD =      @[status_std_cd]
                            and CO_DIV_CD =            @[company_cd]
                            and (eai_evnt_dt < to_date(@[eai_date],'yyyy/mm/dd hh24:mi:ss')
                            or eai_evnt_dt is null
                            )			]]></sql>
			<params>
				<param name="status_seq" type="12" value="" out="N"/>
				<param name="status_desc" type="12" value="" out="N"/>
				<param name="origin_cd" type="12" value="" out="N"/>
				<param name="dest_cd" type="12" value="" out="N"/>
				<param name="eai_date" type="12" value="" out="N"/>
				<param name="status_std_cd" type="12" value="" out="N"/>
				<param name="company_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
