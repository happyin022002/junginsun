<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerEdiDBDAOCheckUsrPerformanceSettingInfoRSQL">
			<desc><![CDATA[CheckUsrPerformanceSettingInfo]]></desc>
			<sql><![CDATA[
SELECT COUNT(0) CNT
FROM EDI_USR_CUST
WHERE 1=1
AND CRE_USR_ID = @[cre_usr_id] 
AND EDI_GRP_CD = @[cs_grp_id]
AND EDI_STS_SEQ = 2			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cs_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
