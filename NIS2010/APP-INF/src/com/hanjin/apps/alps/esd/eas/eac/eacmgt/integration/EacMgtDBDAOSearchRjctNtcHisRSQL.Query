<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchRjctNtcHisRSQL">
			<desc><![CDATA[EAC Rejection Notice History 시퀀스 조회]]></desc>
			<sql><![CDATA[
SELECT NTC_HIS_SEQ AS code_cd, NTC_HIS_SEQ AS code_nm 
FROM   EAS_EXPN_AUD_CS_RJCT_HIS
WHERE  EAC_NO = @[eac_no]
ORDER BY NTC_HIS_SEQ

			]]></sql>
			<params>
				<param name="eac_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
