<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESeBillingManageCommonDBDAOGetEdiInvoiceListRSQL">
			<desc><![CDATA[F/F에 처리된 모든 INVOICE 조회]]></desc>
			<sql><![CDATA[
SELECT 
H.TML_EDI_SO_OFC_CTY_CD
, H.TML_EDI_SO_SEQ
, H.TML_INV_TP_CD
, H.TML_INV_STS_CD
, H.TML_INV_RJCT_STS_CD
, H.INV_OFC_CD
, H.COST_OFC_CD
, H.VNDR_SEQ
, H.YD_CD
, H.INV_NO
, H.CURR_CD
, H.RCV_DT
, H.ISS_DT
, H.TTL_INV_AMT
, H.VAT_AMT
, H.WHLD_TAX_AMT
, H.ATCH_TP_CD
, H.IO_IND_CD
, H.TML_SO_OFC_CTY_CD
, H.TML_SO_SEQ
, H.DELT_FLG
, H.IB_VVD_CD
, H.OB_VVD_CD
, H.INV_RJCT_RMK
, H.STO_DYS_IND_CD
, H.VLD_CHK_FLG
, H.FM_PRD_DT
, H.TO_PRD_DT
, H.TML_INV_EDI_SEQ
, H.FLT_FILE_MSG_ID
, H.SNDR_ID
, H.RCVR_ID
, H.EDI_MSG
, H.EDI_RCV_RULE_MN_SEQ
, H.ATB_DT
, H.LOCL_CRE_DT
, H.LOCL_UPD_DT
, H.CRE_USR_ID
, H.CRE_DT
, H.UPD_USR_ID
, H.UPD_DT
FROM TES_EDI_SO_HDR H
WHERE 1=1
AND H.TML_INV_EDI_SEQ = @[tml_inv_edi_seq]
AND H.TML_SO_OFC_CTY_CD IS NULL
AND H.TML_SO_SEQ IS NULL
AND NVL(H.DELT_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="tml_inv_edi_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
