<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveSummaryDBDAOSearchCreditBySourceListRSQL">
			<desc><![CDATA[Search Credit & Incentive by Source system]]></desc>
			<sql><![CDATA[
SELECT X.CR_SRC
       ,X.RHQ_CD
       ,X.INV_OFC_CD OFC_CD
       ,NVL(SUM(X.TTL_INCNT_AMT),0) ESTM_AMT
       ,NVL(SUM(X.TTL_RCV_AMT),0)   RCV_AMT
       ,NVL(SUM(X.TTL_RMN_AMT),0)   BAL_AMT
 FROM  ( 
#if (${cr_src_cd} =='')
		  SELECT A.RHQ_CD
                ,A.INV_OFC_CD
                ,'Transportation' CR_SRC
                ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_INCNT_AMT,0)) TTL_INCNT_AMT
                ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RCV_AMT,0)) TTL_RCV_AMT
                ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RMN_AMT,0)) TTL_RMN_AMT
          FROM EAS_TRSP_INCNT A
         WHERE A.DELT_FLG = 'N'
		#if (${rhq_ofc_cd} !='')
           AND A.RHQ_CD = @[rhq_ofc_cd]
        #end
		#if (${bse_yr} !='')
           AND A.BSE_YR = @[bse_yr]
        #end
         UNION ALL
        SELECT A.RHQ_CD
              ,A.INV_OFC_CD
              ,'Terminal' CR_SRC
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_INCNT_AMT,0)) TTL_INCNT_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RCV_AMT,0)) TTL_RCV_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RMN_AMT,0)) TTL_RMN_AMT
          FROM EAS_TML_INCNT A
         WHERE A.DELT_FLG = 'N'
		#if (${rhq_ofc_cd} !='')
           AND A.RHQ_CD = @[rhq_ofc_cd]
        #end
		#if (${bse_yr} !='')
           AND A.BSE_YR = @[bse_yr]
        #end
         UNION ALL
        SELECT RHQ_CD
              ,OFC_CD
              ,'Vessel Operation' CR_SRC
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_INCNT_AMT,0)) TTL_INCNT_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RCV_AMT,0)) TTL_RCV_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RMN_AMT,0)) TTL_RMN_AMT
          FROM EAS_VSL_YRY_CR A
         WHERE A.DELT_FLG = 'N'
		#if (${rhq_ofc_cd} !='')
           AND A.RHQ_CD = @[rhq_ofc_cd]
        #end
		#if (${bse_yr} !='')
           AND A.BSE_YR = @[bse_yr]
        #end
#end
#if (${cr_src_cd} =='TRS')
		  SELECT A.RHQ_CD
                ,A.INV_OFC_CD
                ,'Transportation' CR_SRC
                ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_INCNT_AMT,0)) TTL_INCNT_AMT
                ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RCV_AMT,0)) TTL_RCV_AMT
                ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RMN_AMT,0)) TTL_RMN_AMT
          FROM EAS_TRSP_INCNT A
         WHERE A.DELT_FLG = 'N'
		#if (${rhq_ofc_cd} !='')
           AND A.RHQ_CD = @[rhq_ofc_cd]
        #end
		#if (${bse_yr} !='')
           AND A.BSE_YR = @[bse_yr]
        #end
#end
#if (${cr_src_cd} =='TES')
        SELECT A.RHQ_CD
              ,A.INV_OFC_CD
              ,'Terminal' CR_SRC
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_INCNT_AMT,0)) TTL_INCNT_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RCV_AMT,0)) TTL_RCV_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RMN_AMT,0)) TTL_RMN_AMT
          FROM EAS_TML_INCNT A
         WHERE A.DELT_FLG = 'N'
		#if (${rhq_ofc_cd} !='')
           AND A.RHQ_CD = @[rhq_ofc_cd]
        #end
		#if (${bse_yr} !='')
           AND A.BSE_YR = @[bse_yr]
        #end
#end
#if (${cr_src_cd} =='VSL')
        SELECT RHQ_CD
              ,OFC_CD INV_OFC_CD
              ,'Vessel Operation' CR_SRC
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_INCNT_AMT,0)) TTL_INCNT_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RCV_AMT,0)) TTL_RCV_AMT
              ,TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(A.CURR_CD,NVL(A.TTL_RMN_AMT,0)) TTL_RMN_AMT
          FROM EAS_VSL_YRY_CR A
         WHERE A.DELT_FLG = 'N'
		#if (${rhq_ofc_cd} !='')
           AND A.RHQ_CD = @[rhq_ofc_cd]
        #end
		#if (${bse_yr} !='')
           AND A.BSE_YR = @[bse_yr]
        #end
#end
         ) X
 GROUP BY X.CR_SRC
         ,X.RHQ_CD
         ,X.INV_OFC_CD
ORDER BY  X.CR_SRC
         ,X.RHQ_CD
         ,X.INV_OFC_CD			]]></sql>
			<params>
				<param name="rhq_ofc_cd" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
