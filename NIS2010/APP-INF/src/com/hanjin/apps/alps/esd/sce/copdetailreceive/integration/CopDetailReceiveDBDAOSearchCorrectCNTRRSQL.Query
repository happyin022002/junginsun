<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchCorrectCNTRRSQL">
			<desc><![CDATA[SearchCorrectCNTR]]></desc>
			<sql><![CDATA[
SELECT c.cop_no    v_chg_cop_no
      ,c.cntr_no   i_correct_cntr_no
      ,c.cnmv_yr   i_correct_cnmv_yr
FROM sce_cop_dtl  a
    ,sce_act_act_mapg b
    ,sce_cop_hdr c
WHERE  a.cop_no          = c.cop_no
AND    c.bkg_no          = @[inBkgNo]
AND    a.nod_cd          = @[inOrgYdCd]
AND    a.act_cd          = b.act_cd
AND    b.act_rcv_tp_cd   = @[pActRcvTpCd]
AND    c.cop_sts_cd      not in ( 'X', 'O' )
AND    b.act_sts_mapg_cd = 'OC'
AND    c.cntr_tpsz_cd    = @[inCntrTpszCd] 
AND    c.CNTR_NO         <> 'SMCU0000000'			]]></sql>
			<params>
				<param name="inBkgNo" type="12" value="" out="N"/>
				<param name="inOrgYdCd" type="12" value="" out="N"/>
				<param name="pActRcvTpCd" type="12" value="" out="N"/>
				<param name="inCntrTpszCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
