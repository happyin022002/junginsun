<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffConditionManageDBDAOsearchTariffCondRSQL">
			<desc><![CDATA[searchTariffCond]]></desc>
			<sql><![CDATA[
SELECT 
CASE
WHEN C.TML_AWK_CGO_COND_TP_CD = 'B'
THEN 'Basic'
WHEN C.TML_AWK_CGO_COND_TP_CD = 'T'
THEN 'T/S'
WHEN C.TML_AWK_CGO_COND_TP_CD = 'A'
THEN 'Add-On'
WHEN C.TML_AWK_CGO_COND_TP_CD = 'S'
THEN 'Shuttle'
WHEN C.TML_AWK_CGO_COND_TP_CD = 'F'
THEN 'Feeder'
WHEN C.TML_AWK_CGO_COND_TP_CD = 'C'
THEN 'Common'
END TML_AWK_CGO_COND_TP_NM
, C.TML_AWK_CGO_COND_TP_CD
, C.COND_NO
, C.COND_DESC
, C.UPD_USR_ID
, TO_CHAR(C.UPD_DT, 'YYYY-MM-DD') AS UPD_DT
FROM TES_TRF_COND C
WHERE 1=1
AND C.COND_NO > '0'
AND NVL(C.DELT_FLG, 'N') <> 'Y'
#if(${ui_id} != 'ESD_TES_0050')
AND C.COND_CRE_STS_CD = 'C'
#end
#if(${tml_awk_cgo_cond_tp_cd} != '')
AND TML_AWK_CGO_COND_TP_CD IN ( @[tml_awk_cgo_cond_tp_cd], 'C') --// (입력값) cnd 유형
#end
#if(${cond_no} != '' )
AND C.COND_NO = @[cond_no] --// (입력값) cnd 번호;
#end
#if(${cond_desc} != '' )
AND UPPER(C.COND_DESC) LIKE '%'||UPPER(@[cond_desc])||'%' --// (입력값) cnd nm
#end
ORDER BY C.COND_NO, C.TML_AWK_CGO_COND_TP_CD			]]></sql>
			<params>
				<param name="tml_awk_cgo_cond_tp_cd" type="12" value="" out="N"/>
				<param name="cond_no" type="12" value="" out="N"/>
				<param name="cond_desc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
