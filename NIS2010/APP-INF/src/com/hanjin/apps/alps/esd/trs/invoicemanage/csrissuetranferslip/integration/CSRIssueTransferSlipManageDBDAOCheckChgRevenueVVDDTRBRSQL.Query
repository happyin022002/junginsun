<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOCheckChgRevenueVVDDTRBRSQL">
			<desc><![CDATA[CheckChgRevenueVVDDTRB]]></desc>
			<sql><![CDATA[
SELECT  ROUND(NVL(SUM(D.INV_AMT), 999),0)	TTL_AMT				
FROM    AP_INV_DTRB   						D					
WHERE   D.CSR_NO      						= @[CSR_NO]
			]]></sql>
			<params>
				<param name="CSR_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
