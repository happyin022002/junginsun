<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCopActCdRSQL">
			<desc><![CDATA[cop_no + cop_dtl_seq에 해당되는 act_cd를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
ACT_CD
FROM
SCE_COP_DTL
WHERE 1=1
AND COP_NO=@[cop_no]
AND COP_DTL_SEQ=@[cop_dtl_seq]

			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
