<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAORemoveMnrCreditUsedDSQL">
			<desc><![CDATA[M&R Credit Used Delete]]></desc>
			<sql><![CDATA[
UPDATE EAS_MNR_CR_USD
   SET DELT_FLG = 'Y'
      ,UPD_USR_ID = @[usr_id]
      ,UPD_DT = SYSDATE
 WHERE CR_ISS_NO = @[cr_iss_no]
#if (${cr_usd_seq} !='')
   AND CR_USD_SEQ = @[cr_usd_seq]
#end			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cr_iss_no" type="12" value="" out="N"/>
				<param name="cr_usd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
