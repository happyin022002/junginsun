<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsAdvanceAuditDBDAOconfirmTrsPreAuditCSQL">
			<desc><![CDATA[confirmTrsPreAudit insert]]></desc>
			<sql><![CDATA[
MERGE INTO EAS_TRSP_AUD K USING DUAL
ON( K.INV_NO = @[inv_no]
AND K.INV_VNDR_SEQ  = @[inv_vndr_seq]
AND K.TRSP_SO_TP_CD = @[trsp_so_tp_cd]
) 
WHEN MATCHED THEN
    UPDATE
       SET AUTO_AUD_CFM_USR_ID = DECODE(@[s_save_tp_cd], 'C', @[upd_usr_id], AUTO_AUD_CFM_USR_ID)
         , EXPN_AUD_STS_CD = DECODE(@[s_save_tp_cd], 'C', @[sel_aud_cd], EXPN_AUD_STS_CD)
         , EXPN_AUD_RSLT_RMK = DECODE(@[s_save_tp_cd], 'S', @[expn_aud_rslt_rmk], EXPN_AUD_RSLT_RMK)
         , EXPN_AUD_RSLT_USR_ID = DECODE(@[s_save_tp_cd], 'S', @[expn_aud_rslt_usr_id], EXPN_AUD_RSLT_USR_ID)
         , ATCH_FILE_LNK_ID = DECODE(@[s_save_tp_cd], 'C', @[atch_file_lnk_id], 'S', @[atch_file_lnk_id], ATCH_FILE_LNK_ID)
         , EXPN_AUD_RSLT_CD = DECODE(@[s_save_tp_cd], 'S', @[expn_aud_rslt_cd], EXPN_AUD_RSLT_CD)
		 , AUD_CURR_CD = @[inv_aud_curr_cd]
		 , AUD_DIFF_AMT = @[inv_aud_diff_amt]
		 , AUD_USD_DIFF_AMT = @[inv_aud_usd_diff_amt]
#if(${s_save_tp_cd} != '') -- OLTP 프로그램 수행시에만 실행 (배치에서 UPDATE 구문은 자료 이관시에만 실행)
         , CRE_OFC_CD  = @[cre_ofc_cd]
         , UPD_USR_ID  = @[upd_usr_id]
         , UPD_DT      = SYSDATE
         , LOCL_CRE_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
         , AUTO_AUD_CFM_DT = DECODE(@[s_save_tp_cd], 'C', GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), AUTO_AUD_CFM_DT)
#end
#if(${batch_tp_cd} != '') -- 배치 프로그램 수행시에만 실행
         , RHQ_CD = @[rhq_ofc_cd]
         , INV_OFC_CD = @[inv_ofc_cd]
         , INV_CFM_DT = TO_DATE(@[inv_cfm_dt], 'YYYYMMDDHH24MISS')
         , AUTO_EXPN_AUD_STS_CD = @[auto_aud_sts_cd]
         , CURR_CD = @[curr_cd]
         , WO_AMT = @[wo_amt]
         , INV_AMT = @[inv_amt]
         , CURR_CNG_FLG = @[curr_cng_flg]
         , INV_DIFF_AMT = @[inv_diff_amt]
         , INV_DIFF_RTO = @[inv_diff_pct]
         , AGMT_APLY_FLG = @[no_agmt_flg]
         , OPTM_ROUT_FLG = @[no_optm_rout_flg]
         , AVG_OVR_DIFF_AMT = @[exceed_avg_diff_amt]
         , INV_ISS_DT = TO_DATE(@[inv_iss_dt], 'YYYYMMDDHH24MISS')
         , INV_ISS_USR_ID = @[inv_iss_usr_id]
         , GEN_PAY_TERM_CD = @[pay_term_cd]
         , HJL_INV_NO = @[hjl_inv_no]
         , HJL_INV_VNDR_SEQ = @[hjl_inv_vndr_seq]
         , INV_DIFF_FLG = @[inv_diff_amt_flg]
         , AVG_OVR_DIFF_FLG = @[exceed_avg_flg]
#end
WHEN NOT MATCHED THEN
    INSERT(INV_NO
         , INV_VNDR_SEQ
         , TRSP_SO_TP_CD
         , EXPN_AUD_STS_CD
         , EXPN_AUD_RSLT_RMK
         , EXPN_AUD_RSLT_USR_ID
         , ATCH_FILE_LNK_ID
         , EXPN_AUD_RSLT_CD
         , CRE_OFC_CD
         , CRE_USR_ID
         , CRE_DT
         , UPD_USR_ID
         , UPD_DT
#if(${batch_tp_cd} != '') -- 배치 프로그램 수행시에만 실행
         , RHQ_CD
         , INV_OFC_CD
         , INV_CFM_DT
         , AUTO_EXPN_AUD_STS_CD
         , CURR_CD
         , WO_AMT
         , INV_AMT
         , CURR_CNG_FLG
         , INV_DIFF_AMT
         , INV_DIFF_RTO
         , AGMT_APLY_FLG
         , OPTM_ROUT_FLG
         , AVG_OVR_DIFF_AMT
         , INV_ISS_DT
         , INV_ISS_USR_ID
         , GEN_PAY_TERM_CD
         , HJL_INV_NO
         , HJL_INV_VNDR_SEQ
         , INV_DIFF_FLG
         , AVG_OVR_DIFF_FLG
#end
    )
    VALUES
    (
           @[inv_no]
         , @[inv_vndr_seq]
         , @[trsp_so_tp_cd]
         , DECODE(@[s_save_tp_cd], 'C', @[sel_aud_cd])
         , DECODE(@[s_save_tp_cd], 'S', @[expn_aud_rslt_rmk])
         , DECODE(@[s_save_tp_cd], 'S', @[expn_aud_rslt_usr_id])
         , DECODE(@[s_save_tp_cd], 'C', @[atch_file_lnk_id], 'S', @[atch_file_lnk_id])
         , DECODE(@[s_save_tp_cd], 'S', @[expn_aud_rslt_cd])
         , @[cre_ofc_cd]
         , @[cre_usr_id]                                                               
         , SYSDATE
         , @[upd_usr_id]
         , SYSDATE
#if(${batch_tp_cd} != '') -- 배치 프로그램 수행시에만 실행
         , @[rhq_ofc_cd]
         , @[inv_ofc_cd]
         , TO_DATE(@[inv_cfm_dt], 'YYYYMMDDHH24MISS')
         , @[auto_aud_sts_cd]
         , @[curr_cd]
         , @[wo_amt]
         , @[inv_amt]
         , @[curr_cng_flg]
         , @[inv_diff_amt]
         , @[inv_diff_pct]
         , @[no_agmt_flg]
         , @[no_optm_rout_flg]
         , @[exceed_avg_diff_amt]
         , TO_DATE(@[inv_iss_dt], 'YYYYMMDDHH24MISS')
         , @[inv_iss_usr_id]
         , @[pay_term_cd]
         , @[hjl_inv_no]
         , @[hjl_inv_vndr_seq]
         , @[inv_diff_amt_flg]
         , @[exceed_avg_flg]
#end
    )			]]></sql>
			<params>
				<param name="inv_no" type="12" value="22" out="N"/>
				<param name="inv_vndr_seq" type="12" value="222" out="N"/>
				<param name="trsp_so_tp_cd" type="12" value="1" out="N"/>
				<param name="s_save_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="55" out="N"/>
				<param name="sel_aud_cd" type="12" value="" out="N"/>
				<param name="expn_aud_rslt_rmk" type="12" value="" out="N"/>
				<param name="expn_aud_rslt_usr_id" type="12" value="" out="N"/>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="expn_aud_rslt_cd" type="12" value="" out="N"/>
				<param name="inv_aud_curr_cd" type="12" value="" out="N"/>
				<param name="inv_aud_diff_amt" type="12" value="" out="N"/>
				<param name="inv_aud_usd_diff_amt" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="55" out="N"/>
				<param name="rhq_ofc_cd" type="12" value="" out="N"/>
				<param name="inv_ofc_cd" type="12" value="" out="N"/>
				<param name="inv_cfm_dt" type="12" value="" out="N"/>
				<param name="auto_aud_sts_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="wo_amt" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="curr_cng_flg" type="12" value="" out="N"/>
				<param name="inv_diff_amt" type="12" value="" out="N"/>
				<param name="inv_diff_pct" type="12" value="" out="N"/>
				<param name="no_agmt_flg" type="12" value="" out="N"/>
				<param name="no_optm_rout_flg" type="12" value="" out="N"/>
				<param name="exceed_avg_diff_amt" type="12" value="" out="N"/>
				<param name="inv_iss_dt" type="12" value="" out="N"/>
				<param name="inv_iss_usr_id" type="12" value="" out="N"/>
				<param name="pay_term_cd" type="12" value="" out="N"/>
				<param name="hjl_inv_no" type="12" value="" out="N"/>
				<param name="hjl_inv_vndr_seq" type="12" value="" out="N"/>
				<param name="inv_diff_amt_flg" type="12" value="" out="N"/>
				<param name="exceed_avg_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="444" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
