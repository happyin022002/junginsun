<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceEdiHitDBDAOSaveInvoiceEdiAckLogListCSQL">
			<desc><![CDATA[INVOICE EDI ACK LOG LIST 저장]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_INV_EDI_ACK_LOG_LIST(
	   INV_EDI_SND_DT
	  ,INV_EDI_SND_LOG_SEQ
	  ,INV_EDI_RCV_SEQ
	  ,INV_NO
	  ,INV_VNDR_SEQ
	  ,CRE_USR_ID
	  ,CRE_DT
	  ,UPD_USR_ID
	  ,UPD_DT
	)VALUES(
	   @[inv_edi_snd_dt]
	  ,@[inv_edi_snd_log_seq]
	  ,@[inv_edi_rcv_seq]
	  ,@[inv_no]
	  ,@[inv_vndr_seq]
	  ,'HIT_INV_EDI' --cre_usr_id
	  ,SYSDATE       --cre_dt
	  ,'HIT_INV_EDI' --upd_usr_id
	  ,SYSDATE       --upd_dt
	)			]]></sql>
			<params>
				<param name="inv_edi_snd_dt" type="12" value="" out="N"/>
				<param name="inv_edi_snd_log_seq" type="12" value="" out="N"/>
				<param name="inv_edi_rcv_seq" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
