<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchOriginBookingInformationRSQL">
			<desc><![CDATA[Edi315SendDBDAOSearchOriginBookingInformationRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG_NO, BL_NO, BL_TP_CD 
FROM BKG_BOOKING 
WHERE BKG_NO IS NOT NULL 
	AND BKG_NO IN 
		( SELECT FM_BKG_NO FROM BKG_BOOKING WHERE BKG_NO=@[bkg_no] AND FM_BKG_NO IS NOT NULL )
	AND SPLIT_RSN_CD='M' 
	AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="RTM000052491" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
