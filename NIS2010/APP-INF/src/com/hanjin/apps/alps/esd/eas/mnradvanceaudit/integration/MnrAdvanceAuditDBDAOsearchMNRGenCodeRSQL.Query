<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MnrAdvanceAuditDBDAOsearchMNRGenCodeRSQL">
			<desc><![CDATA[ESD_EAS_0360 화면에서 MNR_GEN_CD를 사용하는 코드]]></desc>
			<sql><![CDATA[
SELECT MNR_CD_ID CODE_CD
     , MNR_CD_DP_DESC CODE_NM
     , MNR_CD_DP_SEQ
     , DECODE(MNR_CD_ID, 'UT', -1, MNR_CD_DP_SEQ) MNR_CD_DP_SEQ2
  FROM MNR_GEN_CD
 WHERE PRNT_CD_ID = 'CD00004'
   AND DELT_FLG = 'N'
   AND MNR_CD_ID != 'SS'
   AND MNR_CD_ID != 'SL'
   AND MNR_CD_ID != 'OF'
#if(${s_mnr_code_type} == 'W')
 ORDER BY MNR_CD_DP_SEQ2
#else
 ORDER BY MNR_CD_DP_SEQ
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
