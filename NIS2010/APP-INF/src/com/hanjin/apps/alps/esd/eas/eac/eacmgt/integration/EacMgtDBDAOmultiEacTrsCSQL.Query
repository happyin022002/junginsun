 @[trsp_so_ofc_cty_cd]
,@[trsp_so_seq]
,NVL(@[sub_rail_seq],1)
,@[eac_sys_if_cd]
,@[lgs_cost_cd]
,@[eac_no]
,NVL(@[usr_id], 'TRS')
,SYSDATE
,NVL(@[usr_id], 'TRS')
