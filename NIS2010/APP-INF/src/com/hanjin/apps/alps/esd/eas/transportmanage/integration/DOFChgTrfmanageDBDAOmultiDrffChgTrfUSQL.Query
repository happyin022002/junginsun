<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DOFChgTrfmanageDBDAOmultiDrffChgTrfUSQL">
			<desc><![CDATA[Drop Off Charge Tariff 수정]]></desc>
			<sql><![CDATA[
UPDATE EAS_DRFF_CHG_TRF_HDR H
SET H.FM_EFF_DT = REPLACE(@[fm_eff_dt],'-','')
,H.TO_EFF_DT = REPLACE(@[to_eff_dt],'-','')
,H.UPD_USR_ID = @[upd_usr_id]
,H.UPD_DT = SYSDATE
WHERE 1=1
AND H.DRFF_CHG_TRF_SEQ = @[drff_chg_trf_seq]
AND H.DRFF_CHG_TRF_VER_NO = @[drff_chg_trf_ver_no]			]]></sql>
			<params>
				<param name="fm_eff_dt" type="12" value="" out="N"/>
				<param name="to_eff_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="drff_chg_trf_seq" type="12" value="" out="N"/>
				<param name="drff_chg_trf_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
