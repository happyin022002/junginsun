<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESeBillingManageBizType000DBDAOGetTabColumnsRSQL">
			<desc><![CDATA[table의 column정보 조회]]></desc>
			<sql><![CDATA[
SELECT COLUMN_NAME
FROM ALL_TAB_COLUMNS
WHERE TABLE_NAME =  @[table_name]
ORDER BY COLUMN_ID			]]></sql>
			<params>
				<param name="table_name" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
