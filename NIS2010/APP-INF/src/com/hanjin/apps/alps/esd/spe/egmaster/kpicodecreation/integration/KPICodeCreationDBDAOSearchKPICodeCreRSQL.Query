<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KPICodeCreationDBDAOSearchKPICodeCreRSQL">
			<desc><![CDATA[KPI Code Creation 데이터를 조회한다]]></desc>
			<sql><![CDATA[
SELECT SP_KPI_ID
     , EV_SVC_CATE_CD
     , EV_SVC_CATE_CD AS EV_SVC_CATE_CODE
     , SP_KPI_NM
     , SP_KPI_DESC
     , SP_KPI_TP_CD
     , DELT_FLG
     , CRE_USR_ID
     , TO_CHAR(CRE_DT, 'YYYY.MM.DD') AS CRE_DT
     , UPD_USR_ID
     , TO_CHAR(UPD_DT, 'YYYY.MM.DD') AS UPD_DT
  FROM SPE_SP_SVC_CATE_KPI
 WHERE DELT_FLG = 'N'
#if(${s_ev_svc_cate_cd}!='')
   AND EV_SVC_CATE_CD = @[s_ev_svc_cate_cd]
#end
ORDER BY EV_SVC_CATE_CD,SP_KPI_ID			]]></sql>
			<params>
				<param name="s_ev_svc_cate_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
