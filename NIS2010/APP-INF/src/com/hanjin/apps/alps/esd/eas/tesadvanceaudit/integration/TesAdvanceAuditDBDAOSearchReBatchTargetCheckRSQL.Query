<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TesAdvanceAuditDBDAOSearchReBatchTargetCheckRSQL">
			<desc><![CDATA[실시간 배치대상에 이미 포함되어 있는지를 검사한다.]]></desc>
			<sql><![CDATA[
SELECT	'Y' AS PROG_FLG
FROM	EAS_AUTO_AUD_BAT
WHERE	SUB_SYS_CD				= 'TES'
AND		BAT_PROG_STS_CD 		= 'P'
AND		INV_NO					= @[inv_no]
AND		INV_VNDR_SEQ			= @[vndr_seq]
AND		INV_CFM_DT				= TO_DATE(@[inv_cfm_dt], 'YYYYMMDDHH24MISS')
AND		NVL(VSL_CD, 'X')		= NVL(@[vsl_cd], 'X')
AND		NVL(SKD_VOY_NO, 'X')	= NVL(@[skd_voy_no], 'X')
AND		NVL(SKD_DIR_CD, 'X')	= NVL(@[skd_dir_cd], 'X')
AND		NVL(IO_BND_CD, 'X')		= NVL(@[io_bnd_cd], 'X')			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="inv_cfm_dt" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
