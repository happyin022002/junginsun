<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOSearchBfCombineBkgInfoRSQL">
			<desc><![CDATA[Combine 된 booking no 로 combine 되기 이전 booking 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BKG_NO, BKG_STS_CD
            FROM BKG_BOOKING
            WHERE TO_BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
