<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchInvOfcCdRSQL">
			<desc><![CDATA[Invoice Office로 S/O Office Code를 조회]]></desc>
			<sql><![CDATA[
SELECT                   		
		    OFC_CD         
		FROM		  
			TRS_TRSP_INV_OFC	
		WHERE
			INV_OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
