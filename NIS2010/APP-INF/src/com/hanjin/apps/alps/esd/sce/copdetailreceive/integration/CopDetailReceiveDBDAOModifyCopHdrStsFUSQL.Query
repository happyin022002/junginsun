<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopHdrStsFUSQL">
			<desc><![CDATA[ModifyCopHdrStsF]]></desc>
			<sql><![CDATA[
update  sce_cop_hdr
           set  cop_sts_cd  = 'F'
                ,upd_dt      = SYSDATE
                ,upd_usr_id  = 'MVMT Close'
         where  cop_no      = @[IN_COP_NO] AND COP_STS_CD != 'X'
           and  (cop_no, @[v_cop_dtl_seq) ]) IN
                    (select cop_no
                            ,max(trim(cop_dtl_seq))
                      from  sce_cop_dtl
                     where  cop_no = @[IN_COP_NO]
                     group  by cop_no
                    )			]]></sql>
			<params>
				<param name="IN_COP_NO" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq) " type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
