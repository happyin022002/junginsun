<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementImportDBDAOUpdateHjlHndlFeeUSQL">
			<desc><![CDATA[2012.05.24 신동일 [CHM-201217633] HJL Handling Fee 수정]]></desc>
			<sql><![CDATA[
UPDATE TRS_HJL_HNDL_FEE
   SET VNDR_SEQ = @[vndr_seq]       
  	  ,CURR_CD = @[curr_cd]      
      ,COST_RCVR_AMT = @[cost_rcvr_amt]
      ,COMM_AMT = @[comm_amt] 
      ,TTL_AMT = @[ttl_amt]  
      ,EFF_FM_DT = TO_DATE(REPLACE(@[eff_fm_dt],'-',''),'YYYYMMDD')+0.00000
      ,CRE_USR_ID = @[usr_id] 
      ,CRE_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd]) 
      ,UPD_USR_ID = @[usr_id] 
      ,UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd]) 
 WHERE HJL_OFC_CD=  @[hjl_ofc_cd]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="cost_rcvr_amt" type="12" value="" out="N"/>
				<param name="comm_amt" type="12" value="" out="N"/>
				<param name="ttl_amt" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="hjl_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
