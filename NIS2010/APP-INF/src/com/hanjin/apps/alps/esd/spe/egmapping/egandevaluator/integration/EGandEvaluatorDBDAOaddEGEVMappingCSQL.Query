<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EGandEvaluatorDBDAOaddEGEVMappingCSQL">
			<desc><![CDATA[EG vs Evalulator Mapping 데이터를 입력 한다]]></desc>
			<sql><![CDATA[
INSERT INTO SPE_EV_GRP_EVR(
  EG_ID
, EV_KND_CD
, EV_KND_SEQ
, EVR_USR_ID
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
)VALUES(
  @[eg_id]
, @[ev_knd_cd]
,(SELECT COUNT(1)+1 FROM SPE_EV_GRP_EVR WHERE EG_ID = @[eg_id]  AND EV_KND_CD = @[ev_knd_cd])
, @[evr_usr_id]
, @[cre_usr_id]
, SYSDATE
, @[upd_usr_id]
, SYSDATE
)			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="ev_knd_cd" type="12" value="" out="N"/>
				<param name="evr_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
