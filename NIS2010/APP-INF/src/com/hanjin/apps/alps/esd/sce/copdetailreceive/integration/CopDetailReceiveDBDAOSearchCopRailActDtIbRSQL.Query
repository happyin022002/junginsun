<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchCopRailActDtIbRSQL">
			<desc><![CDATA[SearchCopRailActDtIb]]></desc>
			<sql><![CDATA[
SELECT (SELECT COUNT(ACT_DT) FROM   SCE_COP_DTL WHERE  COP_NO      = @[cop_no]
                                                AND    COP_DTL_SEQ > 6000
                                                AND    ACT_CD      = 'FIRRAD'
                                                AND    ACT_DT  IS NOT NULL ) cop_actra_chk
FROM DUAL			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
