<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOInsertEACRegistrationCSQL">
			<desc><![CDATA[EAC Registration 데이터를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_EXPN_AUD_CS_MGMT( EAC_NO
                                , EAC_INP_DT
                                , AUDR_OFC_CD
                                , AUDR_USR_ID
                                , EAC_EXPN_TP_CD 
                                , EAC_TP_CD
                                , EAC_BIL_TP_CD
                                , EAC_COST_DESC
                                , VNDR_SEQ
                                , N3PTY_SRC_NO
                                , N3PTY_SRC_DT
                                , YD_CD
                                , RESPB_OFC_CD                                
                                , CURR_CD
                                , INV_AMT
                                , INV_CNG_AMT
                                , INV_AUD_AMT
                                , INV_AUD_USD_AMT
                                , EAC_YRMON
                                , EAC_RSN_CD
                                , EAC_STS_CD
                                , EAC_DESC
                                , EAC_RJCT_RSN 
                                , EAC_INTER_RMK
                                , EAC_RSN_DESC
                                , EAC_CMPL_CD
                                , EAC_CMPL_DT
                                , NTC_HIS_SEQ
                                , EML_SUBJ_CTNT
                                , EML_CTNT
                                , VVD_CD_CTNT
                                , EAC_APRO_TP_CD
                                , STL_AMT
                                , EXPN_EVID_DESC
                                , EAC_SYS_IF_CD
                                , WO_NO_CTNT
                                , VNDR_CNTC_PNT_SEQ
                                , KPI_OFC_CD
                                , CRE_USR_ID
                                , CRE_DT
                                , UPD_USR_ID
                                , UPD_DT
                                , EAC_CMPL_USR_ID
                                )VALUES(
                                   @[eac_no]
                                ,  to_date(replace(@[eac_inp_dt],'-',''),'YYYYMMDD')
                                ,  @[audr_ofc_cd]
                                ,  @[audr_usr_id]
                                ,  @[eac_expn_tp_cd]
                                ,  @[eac_tp_cd]
                                ,  @[eac_bil_tp_cd]
                                ,  @[eac_cost_desc]
                                ,  @[vndr_seq]
                                ,  @[n3pty_src_no]
                                ,  to_date(replace(@[n3pty_src_dt],'-',''),'YYYYMMDD')
                                ,  @[yd_cd]
                                ,  @[respb_ofc_cd]
                                ,  @[curr_cd]
                                ,  replace(@[inv_amt],',','')
                                ,  replace(@[inv_cng_amt],',','')
                                ,  replace(@[inv_aud_amt],',','')
                                ,  replace(@[inv_aud_usd_amt],',','')
                                ,  replace(@[eac_yrmon],'-','')
                                ,  @[eac_rsn_cd]
                                ,  'IS'
                                ,  @[eac_desc]
                                ,  @[eac_desc]
                                ,  @[eac_inter_rmk]
                                ,  @[eac_rsn_desc]
                                ,  @[eac_cmpl_cd]
                                ,  SYSDATE   -- eac_cmpl_dt
                                ,  @[ntc_his_seq]
                                ,  @[eml_subj_ctnt]
                                ,  @[eml_ctnt]
                                ,  @[vvd_cd_ctnt]
                                ,  @[eac_apro_tp_cd]
                                ,  replace(@[stl_amt],',','')
                                ,  @[expn_evid_desc]
                                ,  @[eac_sys_if_cd]
                                ,  @[wo_no_ctnt]
                                ,  @[vndr_cntc_pnt_nm]
                                ,  (SELECT X.KPI_OFC_CD FROM EAS_EXPN_AUD_CS_PSON_CFG X WHERE X.EAC_USR_ID = @[audr_usr_id])
                                ,  @[audr_usr_id]
                                ,  SYSDATE
                                ,  @[audr_usr_id]
                                ,  SYSDATE
                                ,  DECODE(@[eac_cmpl_cd], 'C', @[audr_usr_id], 'H', @[audr_usr_id], NULL)
                                )			]]></sql>
			<params>
				<param name="eac_no" type="12" value="" out="N"/>
				<param name="eac_inp_dt" type="12" value="" out="N"/>
				<param name="audr_ofc_cd" type="12" value="" out="N"/>
				<param name="audr_usr_id" type="12" value="" out="N"/>
				<param name="eac_expn_tp_cd" type="12" value="" out="N"/>
				<param name="eac_tp_cd" type="12" value="" out="N"/>
				<param name="eac_bil_tp_cd" type="12" value="" out="N"/>
				<param name="eac_cost_desc" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="n3pty_src_no" type="12" value="" out="N"/>
				<param name="n3pty_src_dt" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="respb_ofc_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="inv_cng_amt" type="12" value="" out="N"/>
				<param name="inv_aud_amt" type="12" value="" out="N"/>
				<param name="inv_aud_usd_amt" type="12" value="" out="N"/>
				<param name="eac_yrmon" type="12" value="" out="N"/>
				<param name="eac_rsn_cd" type="12" value="" out="N"/>
				<param name="eac_desc" type="12" value="" out="N"/>
				<param name="eac_inter_rmk" type="12" value="" out="N"/>
				<param name="eac_rsn_desc" type="12" value="" out="N"/>
				<param name="eac_cmpl_cd" type="12" value="" out="N"/>
				<param name="ntc_his_seq" type="12" value="" out="N"/>
				<param name="eml_subj_ctnt" type="12" value="" out="N"/>
				<param name="eml_ctnt" type="12" value="" out="N"/>
				<param name="vvd_cd_ctnt" type="12" value="" out="N"/>
				<param name="eac_apro_tp_cd" type="12" value="" out="N"/>
				<param name="stl_amt" type="12" value="" out="N"/>
				<param name="expn_evid_desc" type="12" value="" out="N"/>
				<param name="eac_sys_if_cd" type="12" value="" out="N"/>
				<param name="wo_no_ctnt" type="12" value="" out="N"/>
				<param name="vndr_cntc_pnt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
