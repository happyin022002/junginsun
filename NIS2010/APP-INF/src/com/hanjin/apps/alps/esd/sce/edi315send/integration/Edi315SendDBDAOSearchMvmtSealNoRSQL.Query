<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchMvmtSealNoRSQL">
			<desc><![CDATA[BKG Seal No 없을 시 MVMT Seal No 참조]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_ASC (CM XPKCTM_MOVEMENT) */ REPLACE(REPLACE(NVL(CNTR_SEAL_NO, ' '), CHR(13), ''), CHR(10), '') CNMV_SEAL_NO
  FROM CTM_MOVEMENT CM
     , SCE_COP_HDR SH
     , SCE_COP_DTL SD
 WHERE SH.BKG_NO = @[e_bkg_no]
   AND SH.CNTR_NO = @[e_cntr_no]
   AND SH.BKG_NO = CM.BKG_NO
   AND SH.CNTR_NO = CM.CNTR_NO
   AND SH.COP_NO = SD.COP_NO
   AND SD.ACT_CD IN ('FLWMLO','FLVMLO')
   AND CM.MVMT_STS_CD = 'VL'
   AND CM.CRNT_VSL_CD = SD.VSL_CD
   AND CM.CRNT_SKD_VOY_NO = SD.SKD_VOY_NO
   AND CM.CRNT_SKD_DIR_CD = SD.SKD_DIR_CD
   AND CM.ORG_YD_CD = SD.NOD_CD
   AND SH.POL_NOD_CD = SD.NOD_CD
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
				<param name="e_cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
