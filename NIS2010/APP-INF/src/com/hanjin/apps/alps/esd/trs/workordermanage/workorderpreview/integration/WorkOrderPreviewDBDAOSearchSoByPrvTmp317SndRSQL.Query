<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchSoByPrvTmp317SndRSQL">
			<desc><![CDATA[SearchSoByPrvTmp317Snd]]></desc>
			<sql><![CDATA[
SELECT 
    SO.TRSP_SO_OFC_CTY_CD,
    SO.TRSP_SO_SEQ,
    (CASE WHEN SO.TRSP_WO_OFC_CTY_CD IS NULL OR SO.TRSP_WO_OFC_CTY_CD = '' THEN TMP.TRSP_WO_OFC_CTY_CD 
     ELSE SO.TRSP_WO_OFC_CTY_CD 
     END) TRSP_WO_OFC_CTY_CD,
    (CASE WHEN SO.TRSP_WO_SEQ IS NULL OR SO.TRSP_WO_SEQ = '' THEN TMP.TRSP_WO_SEQ 
     ELSE SO.TRSP_WO_SEQ 
     END) TRSP_WO_SEQ,
    TMP.VNDR_SEQ,
    TMP.WO_ISS_STS_CD,
    TMP.TRSP_CRR_MOD_CD,
    TMP.TRSP_COST_DTL_MOD_CD,
    VNDR.WO_EDI_USE_FLG,
    LOC.CONTI_CD,
    SO.EQ_NO,
    SO.FM_NOD_CD,
    SO.TRSP_BND_CD,
    SO.TRSP_CRR_MOD_CD,
    SO.EQ_KND_CD,
    SO.CGO_TP_CD
FROM TRS_TRSP_SVC_ORD SO,
    TRS_TRSP_WRK_ORD_PRV_TMP TMP,
    MDM_VENDOR VNDR,
    MDM_LOCATION LOC
WHERE TMP.TRSP_SO_OFC_CTY_CD = SO.TRSP_SO_OFC_CTY_CD
    AND TMP.TRSP_SO_SEQ = SO.TRSP_SO_SEQ
    AND LOC.LOC_CD = SUBSTR(SO.FM_NOD_CD, 1, 5)
    AND TMP.VNDR_SEQ = VNDR.VNDR_SEQ(+)
    AND SO.FM_NOD_CD in ('USORFM1', 'USORFM2')
    AND SO.TRSP_BND_CD = 'I'
    AND SO.TRSP_CRR_MOD_CD = 'TD'
    AND SO.EQ_KND_CD = 'U'
    AND SO.CGO_TP_CD = 'F'
    AND TMP.VNDR_SEQ = VNDR.VNDR_SEQ(+)
    AND TMP.WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
    AND TMP.WO_ISS_NO = @[wo_iss_no]
    AND TMP.WO_CXL_FLG = 'N'
    -- /* 2008.04.29 ETS OPEN */
    AND SO.HJL_NO IS NULL			]]></sql>
			<params>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
