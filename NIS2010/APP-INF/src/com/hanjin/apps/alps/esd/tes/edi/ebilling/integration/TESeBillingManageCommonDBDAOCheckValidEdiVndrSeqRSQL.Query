<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESeBillingManageCommonDBDAOCheckValidEdiVndrSeqRSQL">
			<desc><![CDATA[유효한 EDI VNDR 인지 여부 조회]]></desc>
			<sql><![CDATA[
SELECT 
    CASE WHEN NVL(COUNT(X.EDI_VNDR_SEQ),0) > 0 THEN 'Y' ELSE 'N' END CHK_VAL_VNDR
FROM (
    SELECT
    V.EDI_VNDR_SEQ
    FROM TES_EDI_RCV_RULE_MN M, TES_EDI_RCV_RULE_VNDR_MGMT V
    WHERE 1=1
    AND M.EDI_RCV_RULE_MN_SEQ = V.EDI_RCV_RULE_MN_SEQ
    AND NVL(M.DELT_FLG,'N') <> 'Y'
    --AND NVL(M.CFM_FLG,'N') = 'Y'
    AND V.EDI_VNDR_SEQ = @[vndr_seq]
) X					]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
