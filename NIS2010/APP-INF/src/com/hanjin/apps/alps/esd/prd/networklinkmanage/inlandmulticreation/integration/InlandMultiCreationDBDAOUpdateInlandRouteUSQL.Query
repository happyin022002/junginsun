<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandMultiCreationDBDAOUpdateInlandRouteUSQL">
			<desc><![CDATA[UpdateInlandRoute]]></desc>
			<sql><![CDATA[
UPDATE prd_inlnd_rout_mst SET
delt_flg = 'Y'
WHERE
rout_org_nod_cd	= @[org_node]
AND rout_dest_nod_cd = @[dest_node]
AND rout_seq = @[route_seq]			]]></sql>
			<params>
				<param name="org_node" type="12" value="" out="N"/>
				<param name="dest_node" type="12" value="" out="N"/>
				<param name="route_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
