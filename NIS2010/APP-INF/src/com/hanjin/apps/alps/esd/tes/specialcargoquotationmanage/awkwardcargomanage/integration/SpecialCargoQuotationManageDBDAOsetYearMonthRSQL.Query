<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOsetYearMonthRSQL">
			<desc><![CDATA[setYearMonth]]></desc>
			<sql><![CDATA[
SELECT NVL(TO_CHAR(AH.EXE_FM_DT,'YYYY-MM'),'') AS YEAR_MONTH
FROM TES_ACT_COST_HDR AH
WHERE 1=1
AND EXE_STS_CD = 'C'
AND NVL(DELT_FLG,'N') <> 'Y'
AND AH.TML_ACT_COST_SEQ = (
							SELECT MAX(H.TML_ACT_COST_SEQ)
							FROM TES_ACT_COST_HDR H
							WHERE 1=1
							AND H.EXE_STS_CD = 'C'
							AND NVL(H.DELT_FLG,'N') <> 'Y'
						   )			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
