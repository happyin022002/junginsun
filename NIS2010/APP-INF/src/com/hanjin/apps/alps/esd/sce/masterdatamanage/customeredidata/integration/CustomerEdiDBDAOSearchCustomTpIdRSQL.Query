<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerEdiDBDAOSearchCustomTpIdRSQL">
			<desc><![CDATA[SearchCustomTpId]]></desc>
			<sql><![CDATA[
SELECT DISTINCT GRP.EDI_GRP_CD,                             
      GRP.CUST_TRD_PRNR_ID,                                 
      GRP.EDI_GRP_DESC                                      
      FROM EDI_GROUP GRP, EDI_GRP_CUST CUST                       

WHERE 
     GRP.EDI_GRP_CD = CUST.EDI_GRP_CD                      
      AND GRP.CO_DIV_CD = CUST.CO_DIV_CD                    
#if(${tp_id} != '')      
      AND GRP.CUST_TRD_PRNR_ID = @[tp_id]
#end			]]></sql>
			<params>
				<param name="tp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
