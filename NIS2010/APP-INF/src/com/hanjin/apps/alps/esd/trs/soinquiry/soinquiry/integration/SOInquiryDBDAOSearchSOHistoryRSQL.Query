<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SOInquiryDBDAOSearchSOHistoryRSQL">
			<desc><![CDATA[(CY/Door 건) SO History를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT ROWNUM SEQ
      ,TRSP_SO_OFC_CTY_CD||TRSP_SO_SEQ SO_NO
      ,(SELECT D.INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL D WHERE D.INTG_CD_ID = 'CD02852' AND D.INTG_CD_VAL_CTNT = X.TRSP_SO_EVNT_CD) EVENT
      ,TO_CHAR(LOCL_CRE_DT, 'YYYY-MM-DD HH24:MI:SS') LOCL_CRE_DT
	  ,EQ_TPSZ_CD
      ,EQ_NO 
	  ,BKG_NO
	  ,TRSP_WO_OFC_CTY_CD||TRSP_WO_SEQ WO_NO
      ,INV_NO 
      ,INV_VNDR_SEQ 
      ,TRSP_SO_STS_CD 
      ,SO_ROUT_DESC
      ,ROUT_RPLN_FLG 
      ,COP_SO_STS_CD 
      ,COP_SO_ROUT_DESC 
      ,COP_NO 
      ,COST_ACT_GRP_SEQ 
      ,COST_ACT_GRP_CD 
      ,RPLN_UMCH_FLG 
      ,UPLN_SO_FLG 
      ,TRSP_SO_HIS_DESC		
      ,CRE_USR_ID 
      ,CRE_OFC_CD
      ,RQST_SRC_SYS_CD
FROM   TRS_TRSP_SO_HIS X
WHERE  TRSP_SO_OFC_CTY_CD = SUBSTR(@[sonumber],1,3)
AND    TRSP_SO_SEQ = SUBSTR(@[sonumber],4)
AND    (RQST_SRC_SYS_CD, TRSP_SO_EVNT_CD, NVL(ROUT_RPLN_FLG,'N')) NOT IN (('TRS','SC', 'Y'), ('TRS','SR', 'Y'))
ORDER BY TRSP_HIS_SEQ			]]></sql>
			<params>
				<param name="sonumber" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
