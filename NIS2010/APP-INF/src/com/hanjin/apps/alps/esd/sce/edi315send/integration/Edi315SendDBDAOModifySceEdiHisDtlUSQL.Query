<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOModifySceEdiHisDtlUSQL">
			<desc><![CDATA[for updating SCE_EDI_HIS_DTL]]></desc>
			<sql><![CDATA[
UPDATE SCE_EDI_HIS_DTL
SET 
   RSLT_FLG = @[e_rslt_flag],
   EDI_SND_RSLT_RMK =
nvl(@[e_rslt_remark]
	,decode(@[e_rslt_flag]
	, 'Y', 'SUCCESS(SENT)'
	, 'K', 'SUCCESS(AMS SAVED)'
	, 'L', 'SAVED'
	, 'F', 'FAILED'
	, 'R', 'RESERVED'
	, 'S', 'Send Process Skip.'
, ''))

WHERE EDI_RCV_DT     = @[e_rcv_dt]
AND EDI_RCV_SEQ      = to_number(@[e_rcv_seq])
AND EDI_RCV_DTL_SEQ  = to_number(@[e_rcv_dtl_seq])			]]></sql>
			<params>
				<param name="e_rslt_flag" type="12" value="" out="N"/>
				<param name="e_rslt_remark" type="12" value="" out="N"/>
				<param name="e_rcv_dt" type="12" value="" out="N"/>
				<param name="e_rcv_seq" type="12" value="" out="N"/>
				<param name="e_rcv_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
