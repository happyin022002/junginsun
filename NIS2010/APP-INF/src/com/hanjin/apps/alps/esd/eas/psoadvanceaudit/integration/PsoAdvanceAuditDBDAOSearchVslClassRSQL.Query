<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PsoAdvanceAuditDBDAOSearchVslClassRSQL">
			<desc><![CDATA[PsoAdvanceAuditDBDAOSearchVslClass ]]></desc>
			<sql><![CDATA[
SELECT  CNTR_VSL_CLSS_CAPA
FROM MDM_VSL_CNTR
WHERE 1=1
#if ( ${contractType} != '' && ${contractType} != 'ALL')
	AND VSL_CD IN (SELECT VSL_CD FROM FMS_CONTRACT WHERE FLET_CTRT_TP_CD = @[contractType]) 
#end
#if ( ${contractType} == '' ) 
	AND VSL_CD IN (SELECT VSL_CD FROM FMS_CONTRACT WHERE FLET_CTRT_TP_CD IN ('TI','T0', 'OW'))
#end
AND   CNTR_VSL_CLSS_CAPA IS NOT NULL
GROUP BY CNTR_VSL_CLSS_CAPA
ORDER BY CNTR_VSL_CLSS_CAPA			]]></sql>
			<params>
				<param name="contractType" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
