		<query name="Invoice210EdiDBDAOModifyTrsTrspInvEdiUSQL">
			<desc><![CDATA[modifyTrsTrspInvEdi UPDATE TRS_TRSP_INV_EDI TABLE]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_INV_EDI
SET    INV_RMK = @[inv_rmk],
       INV_EDI_RSLT_CD = @[inv_edi_rslt_cd]
WHERE  TRSP_INV_EDI_SEQ = @[trsp_inv_edi_seq]			]]></sql>
			<params>
				<param name="inv_rmk" type="12" value="" out="N"/>
				<param name="inv_edi_rslt_cd" type="12" value="" out="N"/>
				<param name="trsp_inv_edi_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
