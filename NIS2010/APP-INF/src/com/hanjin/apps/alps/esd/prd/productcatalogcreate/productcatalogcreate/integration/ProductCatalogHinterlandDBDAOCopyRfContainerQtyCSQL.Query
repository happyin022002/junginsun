<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ProductCatalogHinterlandDBDAOCopyRfContainerQtyCSQL">
			<desc><![CDATA[ProductCatalogHinterlandDBDAOCopyRfContainerQtyCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_PROD_CTL_QTY
(PCTL_NO, CNTR_TPSZ_CD, PCTL_QTY
 , REV_CNTR_TPSZ_CD, REV_PCTL_QTY
 , CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
select @[hd_pctl_no] ||LPAD(TO_NUMBER(SUBSTR(@[max_pctl_no], -4))+TO_NUMBER(SUBSTR(PCTL_NO, -4)) ,4,'0') pctl_no
     ,  'R'||DECODE(SUBSTR(CNTR_TPSZ_CD, -1), '4','5', SUBSTR(CNTR_TPSZ_CD, -1))  CNTR_TPSZ_CD
     , 1 PCTL_QTY
     , NULL REV_CNTR_TPSZ_CD
     , NULL REV_PCTL_QTY
     , CRE_USR_ID
     , SYSDATE CRE_DT
     , UPD_USR_ID
     , SYSDATE UPD_DT
from PRD_PROD_CTL_QTY
where PCTL_NO LIKE DECODE(@[hd_pctl_no], NULL, NULL, @[hd_pctl_no] || '%')			]]></sql>
			<params>
				<param name="hd_pctl_no" type="12" value="" out="N"/>
				<param name="max_pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
