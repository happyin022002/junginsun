<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchDelCdRSQL">
			<desc><![CDATA[COP_NO를 기준으로 DEL_CD를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT DEL_NOD_CD
 FROM SCE_COP_HDR
 WHERE COP_NO = @[cop_no]
-- AND STND_EDI_STS_CD = 'D'			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
