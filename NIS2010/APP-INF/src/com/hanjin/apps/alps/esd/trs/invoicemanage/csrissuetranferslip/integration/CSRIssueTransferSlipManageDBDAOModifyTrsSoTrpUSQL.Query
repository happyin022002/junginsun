<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOModifyTrsSoTrpUSQL">
			<desc><![CDATA[TRP FLAG UPDATE]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD  
   SET TRP_FLG = 'Y'
 WHERE 1=1
   AND (TRSP_SO_OFC_CTY_CD,TRSP_SO_SEQ) IN (SELECT SO.TRSP_SO_OFC_CTY_CD
											 	  ,SO.TRSP_SO_SEQ
  											  FROM TRS_TRSP_SVC_ORD SO
      											  ,TRS_TRSP_WRK_ORD WO
      											  ,TRS_TRSP_INV_WRK INV
 											 WHERE SO.TRSP_WO_OFC_CTY_CD = WO.TRSP_WO_OFC_CTY_CD
										       AND SO.TRSP_WO_SEQ = WO.TRSP_WO_SEQ
										       AND SO.INV_NO = INV.INV_NO
											   AND SO.INV_VNDR_SEQ = INV.INV_VNDR_SEQ
											   AND SO.DELT_FLG = 'N'
											   AND SO.TRSP_SO_TP_CD ='O'
											   AND SO.TRSP_COST_DTL_MOD_CD = 'ER'
											   AND SO.TRSP_CRR_MOD_CD = 'RD'
											   AND WO.CRE_OFC_CD = 'NYCRAO'
											   AND INV.INV_VNDR_SEQ = 181057
			                                   AND INV.CSR_NO = @[CSR_NO] )			]]></sql>
			<params>
				<param name="CSR_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
