<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentManageDBDAOSearchDifferentRHQCheckRSQL">
			<desc><![CDATA[RHQ Office의 Same Or Diff. 여부 확인]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN TPB_GET_HNDL_OFC_FNC('R',STL_RQST_OFC_CD) <> TPB_GET_HNDL_OFC_FNC('R',STL_TO_CLT_CNG_OFC_CD) 
            THEN 'Y' ELSE 'N' END DIFF_RHQ_CHK
  FROM TPB_ADJ_STS
 WHERE N3PTY_NO = @[n3pty_no]			]]></sql>
			<params>
				<param name="n3pty_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
