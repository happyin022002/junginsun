<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchGapDatetimeRSQL">
			<desc><![CDATA[SearchGapDatetime]]></desc>
			<sql><![CDATA[
SELECT LTRIM(TO_NUMBER(TO_CHAR((TO_DATE(@[aft_act_dt],'YYYY/MM/DD HH24:MI:SS') - nvl(TO_DATE(@[bfr_act_dt],'YYYYMMDDHH24MISS') ,estm_dt)),'0000000000.0000000000'))) estm_gap
FROM   SCE_COP_DTL 
WHERE  cop_no          = @[cop_no]
AND    cop_dtl_seq     = @[to_cop_dtl_seq]			]]></sql>
			<params>
				<param name="aft_act_dt" type="12" value="20100115000000" out="N"/>
				<param name="bfr_act_dt" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="CBOM9C01454922" out="N"/>
				<param name="to_cop_dtl_seq" type="12" value="6091" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
