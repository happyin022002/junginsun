<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MnrAdvanceAuditDBDAOupdateBatchStatusUSQL">
			<desc><![CDATA[실시간 배치 대상의 상태코드를 완료로 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE EAS_AUTO_AUD_BAT
   SET BAT_PROG_STS_CD = 'B'
      ,UPD_USR_ID = 'BATCH'
      ,UPD_DT     = SYSDATE
 WHERE INV_NO        = @[inv_no]
   AND INV_VNDR_SEQ  = @[vndr_seq]
   AND EQ_KND_CD     = @[eq_knd_cd]
   AND SUB_SYS_CD      = 'MNR'
   AND BAT_PROG_STS_CD = 'P'			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
