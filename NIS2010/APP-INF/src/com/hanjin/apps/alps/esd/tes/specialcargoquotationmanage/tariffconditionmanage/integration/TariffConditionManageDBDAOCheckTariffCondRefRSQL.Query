<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffConditionManageDBDAOCheckTariffCondRefRSQL">
			<desc><![CDATA[Condition 참조 여부 확인]]></desc>
			<sql><![CDATA[
SELECT 
CASE
WHEN COUNT(COND_NO) > 0 
THEN 'Y'
ELSE 'N'
END CHK_COND_NO_REF
FROM (
    SELECT COND_NO
    FROM TES_AWK_CGO_TRF_DTL
    WHERE 1=1
    AND COND_NO = @[cond_no]
    UNION ALL
    SELECT COND_NO
    FROM TES_AWK_CGO_TRF_TP_SZ
    WHERE 1=1
    AND COND_NO = @[cond_no]
    UNION ALL
    SELECT COND_NO
    FROM TES_AWK_CGO_ADON_HDR
    WHERE 1=1
    AND COND_NO = @[cond_no]
    UNION ALL
    SELECT COND_NO
    FROM TES_AWK_CGO_ADON_TP_SZ
    WHERE 1=1
    AND COND_NO = @[cond_no]
    UNION ALL
    SELECT COND_NO
    FROM TRS_AWK_CGO_TRF_HDR
    WHERE 1=1
    AND COND_NO = @[cond_no]
    UNION ALL
    SELECT COND_NO
    FROM TRS_AWK_CGO_TRF_TP_SZ
    WHERE 1=1
    AND COND_NO = @[cond_no]
)			]]></sql>
			<params>
				<param name="cond_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
