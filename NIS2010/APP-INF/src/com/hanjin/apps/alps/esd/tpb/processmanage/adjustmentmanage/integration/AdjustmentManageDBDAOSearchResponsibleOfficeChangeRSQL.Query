<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentManageDBDAOSearchResponsibleOfficeChangeRSQL">
			<desc><![CDATA[SearchResponsibleOfficeChange]]></desc>
			<sql><![CDATA[
#if (${s_date_flag_r} == 'IN')

SELECT DECODE(DECODE(A.STL_FWRD_OFC_CD, NULL, A.STL_TO_CLT_CNG_OFC_CD, A.STL_FWRD_OFC_CD),@[s_user_ofc_cd],'Y',
                 (SELECT RHQ_CD FROM (SELECT N3PTY_OFC_TP_CD, OFC_CD, N3PTY_CTRL_OFC_CD, RHQ_CD FROM TPB_HNDL_OFC WHERE OFC_CD = @[s_user_ofc_cd]) WHERE N3PTY_OFC_TP_CD='S'),'Y',
                 'N' ) AS APPROVAL,
           '0' AS CHK_REQ,
	#if (${n2nd_rvw_chk} == 'xx')
           NVL2(A.STL_APRO_OFC_CD,'0','1') AS CHK_APP,
           NVL2(A.STL_RJCT_OFC_CD,'0','1') AS CHK_REJ,
	#else
           '0' AS CHK_APP,
           '0' AS CHK_REJ,
	#end
           A.STL_TO_CLT_CNG_OFC_CD,
           C.N3PTY_NO,
           TO_CHAR(TPB_GET_LCL_DATE_FNC(A.STL_RQST_DT,@[s_user_ofc_cd]), 'YYYY-MM-DD HH24:MI') AS STL_RQST_DT,
           A.STL_RQST_OFC_CD,
           A.STL_RQST_USR_ID,
           TO_CHAR(TPB_GET_LCL_DATE_FNC(A.STL_APRO_DT,@[s_user_ofc_cd]), 'YYYY-MM-DD HH24:MI') AS STL_APRO_DT,
           A.STL_APRO_OFC_CD,
           A.STL_APRO_USR_ID,
           TO_CHAR(TPB_GET_LCL_DATE_FNC(A.STL_RJCT_DT,@[s_user_ofc_cd]), 'YYYY-MM-DD HH24:MI') AS STL_RJCT_DT,
           A.STL_RJCT_OFC_CD,
           A.STL_RJCT_USR_ID,
           B.N3PTY_SRC_SUB_SYS_CD,
           TPB_GET_N3PTY_BIL_TP_NM_FNC(B.N3PTY_BIL_TP_CD) AS N3PTY_BIL_TP_NM,   
           C.N3PTY_INV_NO,
           D.OTS_STS_CD,
           CASE WHEN D.OTS_STS_CD IN ('R','T','J') THEN COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD02799',C.OTS_STS_DTL_CD)
                ELSE COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD00588',D.OTS_STS_CD)
           END AS OTS_STS_CD_VAL,
           B.BKG_NO,-- || B.BKG_NO_SPLIT AS BKG_NO
           B.BL_NO,-- || B.BL_NO_TP || B.BL_NO_CHK AS BL_NO
           B.EQ_NO,
           --TPB_GET_RHQ_CD_FNC(C.OFC_CD) AS IF_RHQ_CD,
           TPB_GET_HNDL_OFC_FNC('R',NVL(B.N3PTY_OFC_CD,B.OFC_CD)) AS IF_RHQ_CD,
           C.OFC_CD AS IF_OFC_CD,
           DECODE( B.VNDR_CUST_DIV_CD,
                'V', LPAD(TO_CHAR(B.VNDR_SEQ),6,'0'),
                'C', B.CUST_CNT_CD || LPAD(TO_CHAR(B.CUST_SEQ),6,'0'),
                'S', B.N3PTY_OFC_CD, NULL) AS N3PTY,
    #if (${s_curr_cd_tp} == 'U')
           'USD' AS CURR_CD, 
           TPB_GET_USD_AMT_FNC(C.OTS_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS OTS_AMT,
    #elseif (${s_curr_cd_tp} != 'U')
           C.CURR_CD,
           C.OTS_AMT,
    #end
           TO_CHAR(TPB_GET_LCL_DATE_FNC(C.CFM_DT,@[s_user_ofc_cd]), 'YYYY-MM-DD HH24:MI') AS CFM_DT,
    #if (${s_curr_cd_tp} == 'U')
           TPB_GET_USD_AMT_FNC(C.INV_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS INV_AMT,
           TPB_GET_USD_AMT_FNC(C.CLT_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS CLT_AMT,
           TPB_GET_USD_AMT_FNC(C.ADJ_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS ADJ_AMT,
           TPB_GET_USD_AMT_FNC(C.BAL_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS BAL_AMT,
           --TPB_GET_USD_AMT_FNC(A.STL_CLT_OFC_CNG_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS STL_CLT_OFC_CNG_AMT,
    #elseif (${s_curr_cd_tp} != 'U')
           C.INV_AMT,
           C.CLT_AMT,
           C.ADJ_AMT,
           C.BAL_AMT,
           --A.STL_CLT_OFC_CNG_AMT,
    #end
           --,TPB_GET_CLT_ACT_YN_FNC(C.N3PTY_NO, C.N3PTY_INV_NO) AS CLT_ACT_YN
           (SELECT COUNT(1) FROM TPB_OTS_GRP_RCVR_ACT WHERE N3PTY_NO = C.N3PTY_NO AND N3PTY_CLT_RMK_TP_CD = 'M' AND ROWNUM = 1 ) AS CLT_ACT_YN,
           TO_CHAR( TPB_GET_LCL_DATE_FNC( (SELECT STL_RQST_DT FROM TPB_ADJ_STS WHERE N3PTY_NO = C.N3PTY_NO AND ADJ_STS_SEQ = 1),@[s_user_ofc_cd]),'YYYY-MM-DD HH24:MI') AS STL_RQST_DT,
    #if (${n2nd_rvw_chk} == 'Y' && ${tpb_nxt_prc_chk} == 'N' )
		   (SELECT DECODE(H.STL_FWRD_OFC_CD, NULL, H.STL_TO_CLT_CNG_OFC_CD, H.STL_FWRD_OFC_CD)
            FROM  TPB_ADJ_N2ND_RVW_HIS H
            WHERE H.N3PTY_NO = @[s_n3pty_no]
            AND H.STL_STS_LST_FLG = 'Y'
            AND H.ADJ_N2ND_RVW_STS_CD = DECODE(H.UPD_USR_ID
                                         ,TPB_GET_HNDL_OFC_FNC('R',H.STL_TO_CLT_CNG_OFC_CD),'R2'
                                         ,TPB_GET_HNDL_OFC_FNC('R',H.STL_RQST_OFC_CD),'R3','R4')
            AND NVL(H.N2ND_RVW_AVAL_FLG,'X') = 'Y'
            AND H.ADJ_N2ND_RVW_SEQ > 0
            AND ROWNUM =1 ) AS REVIEW_STEP,
    #else       
           DECODE(A.STL_FWRD_OFC_CD, NULL, A.STL_TO_CLT_CNG_OFC_CD, A.STL_FWRD_OFC_CD) AS REVIEW_STEP,
    #end     
           A.ADJ_STS_SEQ
      FROM 
           TPB_ADJ_STS A,  
           TPB_OTS_DTL B,
           TPB_OTS_GRP C,
           TPB_OTS_GRP_STS D
     WHERE 1=1
       AND A.N3PTY_NO = B.N3PTY_NO
       AND B.N3PTY_NO_DP_SEQ = 1
       AND B.N3PTY_NO = C.N3PTY_NO
       AND C.N3PTY_NO = D.N3PTY_NO
       AND C.N3PTY_DELT_TP_CD IN ('N','S')
--       AND NVL(B.N3PTY_TP_CD,'G') !='R'
	#if (${n2nd_rvw_chk} == 'Y' && ${tpb_nxt_prc_chk} == 'N' )
       AND (A.N3PTY_NO,A.ADJ_STS_SEQ) IN (SELECT H.N3PTY_NO,H.ADJ_STS_SEQ
                                            FROM  TPB_ADJ_N2ND_RVW_HIS H
                                           WHERE H.N3PTY_NO = @[s_n3pty_no]
                                             AND H.STL_STS_LST_FLG = 'Y'
                                             AND H.ADJ_N2ND_RVW_STS_CD = DECODE(H.UPD_USR_ID
                                                                         ,TPB_GET_HNDL_OFC_FNC('R',H.STL_TO_CLT_CNG_OFC_CD),'R2'
                                                                         ,TPB_GET_HNDL_OFC_FNC('R',H.STL_RQST_OFC_CD),'R3','R4')
                                             AND NVL(H.N2ND_RVW_AVAL_FLG,'X') = 'Y'
                                             AND H.ADJ_N2ND_RVW_SEQ > 0)
       
       AND D.OTS_STS_CD IN ('R','O','J')
	#elseif (${n2nd_rvw_chk} == 'N' && ${tpb_nxt_prc_chk} == 'N' && ${diff_rhq_chk} == 'Y')
       AND D.OTS_STS_CD IN ('R','O','J')
	#else
       AND D.OTS_STS_CD = 'R'
	#end
       AND A.STL_STS_LST_FLG = 'Y'
       AND D.OTS_STS_LST_FLG = 'Y'
       AND A.N3PTY_STL_TP_CD = 'O'
	#if (${s_n3pty_no} != '')
       AND C.N3PTY_NO = @[s_n3pty_no]
    #else
       AND C.CFM_DT >= TPB_GET_SRCH_DATE_FNC(@[s_calendar_date1], 'YYYY-MM-DD',@[s_user_ofc_cd])
       AND C.CFM_DT <  TPB_GET_SRCH_DATE_FNC(@[s_calendar_date2], 'YYYY-MM-DD',@[s_user_ofc_cd]) + 1
       #if (${s_vndr_cust_div_cd} != '')
            AND C.VNDR_CUST_DIV_CD = @[s_vndr_cust_div_cd]
            	#if (${s_trd_party_val} != '')
                    #if (${s_vndr_cust_div_cd} == 'V')
                        AND C.VNDR_SEQ = TO_NUMBER(@[s_trd_party_val])
                    #elseif (${s_vndr_cust_div_cd} == 'C')
                        AND C.CUST_CNT_CD = SUBSTRB(TRIM(@[s_trd_party_val]),1,2)
                        #if (${s_trd_party_val} != '')
                            AND C.CUST_SEQ = TO_NUMBER(SUBSTRB(TRIM(@[s_trd_party_val]),3))
                        #end
                    #elseif (${s_vndr_cust_div_cd} == 'S')
                        AND C.N3PTY_OFC_CD = @[s_trd_party_val]
                    #end
                #end
        #end
    #end
    #if (${n2nd_rvw_chk} == 'Y' && ${tpb_nxt_prc_chk} == 'N' )
        --'SELCON','SELCOE','SELCOT' 조직변경
	   AND (A.STL_FWRD_OFC_CD  IN (
        SELECT ATTR_CTNT1 AS OFC_CD FROM TPB_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'HO_OFC_CD'
        UNION ALL
        SELECT TPB_GET_HNDL_OFC_FNC('R', STL_RQST_OFC_CD) FROM DUAL
        UNION ALL
        SELECT TPB_GET_HNDL_OFC_FNC('R', STL_TO_CLT_CNG_OFC_CD) FROM DUAL
       )
            
    #else
       AND (A.STL_FWRD_OFC_CD IN ( SELECT OFC_CD
									 FROM ( SELECT DISTINCT T.RHQ_CD, T.N3PTY_CTRL_OFC_CD, T.OFC_CD
											  FROM TPB_HNDL_OFC T,
												   (SELECT N3PTY_OFC_TP_CD, OFC_CD, N3PTY_CTRL_OFC_CD, RHQ_CD FROM TPB_HNDL_OFC WHERE OFC_CD = @[s_user_ofc_cd]) L
											 WHERE 1=1
											   AND T.OFC_CD = L.OFC_CD
												OR T.N3PTY_CTRL_OFC_CD = L.N3PTY_CTRL_OFC_CD
												OR T.RHQ_CD = L.RHQ_CD
											   AND L.N3PTY_OFC_TP_CD = 'S'
UNION
SELECT   DISTINCT
         RHQ_CD
       , N3PTY_CTRL_OFC_CD
       , OFC_CD
FROM     TPB_HNDL_OFC
WHERE    1 = 1
AND      @[s_user_ofc_cd] IN ( SELECT ATTR_CTNT1 AS OFC_CD FROM TPB_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'HO_OFC_CD' ) --SELCON 조직 변경

										   ) WHERE 1=1
								   #if (${s_if_rhq_cd} != '')
											   AND RHQ_CD = @[s_if_rhq_cd]
									   #if (${s_if_ctrl_cd} != '' && ${s_if_rhq_cd} != ${s_user_ofc_cd} )  
											   AND N3PTY_CTRL_OFC_CD = @[s_if_ctrl_cd]
										   #if (${s_if_ofc_cd} != '' && ${s_if_rhq_cd} != ${s_user_ofc_cd} )
											   AND OFC_CD = @[s_if_ofc_cd]
										   #end
									   #end
								   #end
								  ) 
     #end    
        OR A.STL_TO_CLT_CNG_OFC_CD = @[s_user_ofc_cd]  
       AND A.STL_TO_CLT_CNG_OFC_CD IN ( SELECT OFC_CD
										  FROM ( SELECT DISTINCT T.RHQ_CD, T.N3PTY_CTRL_OFC_CD, T.OFC_CD
												   FROM TPB_HNDL_OFC T,
														(SELECT N3PTY_OFC_TP_CD, OFC_CD, N3PTY_CTRL_OFC_CD, RHQ_CD FROM TPB_HNDL_OFC WHERE OFC_CD = @[s_user_ofc_cd]) L
												  WHERE 1=1
													AND T.OFC_CD = L.OFC_CD
													 OR T.N3PTY_CTRL_OFC_CD = L.N3PTY_CTRL_OFC_CD
													 OR T.RHQ_CD = L.RHQ_CD
													AND L.N3PTY_OFC_TP_CD = 'S'
UNION
SELECT   DISTINCT
         RHQ_CD
       , N3PTY_CTRL_OFC_CD
       , OFC_CD
FROM     TPB_HNDL_OFC
WHERE    1 = 1
AND      @[s_user_ofc_cd] IN ( SELECT ATTR_CTNT1 AS OFC_CD FROM TPB_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'HO_OFC_CD' ) --SELCON 조직 변경
												) WHERE 1=1
										#if (${s_if_rhq_cd} != '')
													AND RHQ_CD = @[s_if_rhq_cd]
											#if (${s_if_ctrl_cd} != '')
													AND N3PTY_CTRL_OFC_CD = @[s_if_ctrl_cd]
												#if (${s_if_ofc_cd} != '')
													AND OFC_CD = @[s_if_ofc_cd]
												#end
											#end
										#end
									  ) )
    #if (${user_ofc_cd} == 'SELCON')   
       AND A.STL_FWRD_OFC_CD = 'SELCON'
    #end

     ORDER BY C.N3PTY_NO

#elseif (${s_date_flag_r} == 'OT')

    SELECT DECODE( B.OTS_STS_CD,'O',DECODE(C.OFC_CD,@[s_user_ofc_cd],'Y','N'),'N' ) AS APPROVAL,
           '0' AS CHK_REQ,
           '0' AS CHK_APP,
           '0' AS CHK_REJ,
           NULL AS STL_TO_CLT_CNG_OFC_CD,
           A.N3PTY_NO,
           ------------- request -------------
           '' AS STL_RQST_DT,
           '' AS STL_RQST_OFC_CD,
           '' AS STL_RQST_USR_ID,
           ------------- approve -------------
           '' AS STL_APRO_DT,
           '' AS STL_APRO_OFC_CD,
           '' AS STL_APRO_USR_ID,
           ------------- reject --------------
           '' AS STL_RJCT_DT,
           '' AS STL_RJCT_OFC_CD,
           '' AS STL_RJCT_USR_ID,
           -----------------------------------
           A.N3PTY_SRC_SUB_SYS_CD,
           TPB_GET_N3PTY_BIL_TP_NM_FNC(A.N3PTY_BIL_TP_CD) AS N3PTY_BIL_TP_NM,
           C.N3PTY_INV_NO,
           B.OTS_STS_CD,
           COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD00588',B.OTS_STS_CD) AS OTS_STS_CD_VAL,
           CASE WHEN B.OTS_STS_CD IN ('R','T','J') THEN COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD02799',C.OTS_STS_DTL_CD)
                ELSE COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD00588',B.OTS_STS_CD)
           END AS OTS_STS_CD_VAL,
           A.BKG_NO, -- || A.BKG_NO_SPLIT AS BKG_NO
           A.BL_NO, -- || A.BL_NO_TP || A.BL_NO_CHK AS BL_NO
           A.EQ_NO,
           --TPB_GET_RHQ_CD_FNC(C.OFC_CD) AS IF_RHQ_CD,
           TPB_GET_HNDL_OFC_FNC('R',NVL(A.N3PTY_OFC_CD,A.OFC_CD)) AS IF_RHQ_CD,
           C.OFC_CD AS IF_OFC_CD,
           DECODE(A.VNDR_CUST_DIV_CD,
                'V',LPAD(TO_CHAR(A.VNDR_SEQ),6,'0'),
                'C',A.CUST_CNT_CD || LPAD(TO_CHAR(A.CUST_SEQ),6,'0'),
                'S',A.N3PTY_OFC_CD, NULL) AS N3PTY,
    #if (${s_curr_cd_tp} == 'U')
           'USD' AS CURR_CD, 
           TPB_GET_USD_AMT_FNC(C.OTS_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS OTS_AMT,
    #else
           C.CURR_CD,
           C.OTS_AMT,
    #end
           TO_CHAR(TPB_GET_LCL_DATE_FNC(A.CFM_DT,@[s_user_ofc_cd]), 'YYYY-MM-DD HH24:MI') AS CFM_DT,
    #if (${s_curr_cd_tp} == 'U')
           TPB_GET_USD_AMT_FNC(C.INV_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS INV_AMT,
           TPB_GET_USD_AMT_FNC(C.CLT_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS CLT_AMT,
           TPB_GET_USD_AMT_FNC(C.ADJ_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS ADJ_AMT,
           TPB_GET_USD_AMT_FNC(C.BAL_AMT,C.CURR_CD,TPB_GET_LCL_DATE_FNC(C.CFM_DT,C.OFC_CD)) AS BAL_AMT,
           --NULL AS STL_CLT_OFC_CNG_AMT,
    #else
           C.INV_AMT,
           C.CLT_AMT,
           C.ADJ_AMT,
           C.BAL_AMT,
           --NULL AS STL_CLT_OFC_CNG_AMT,
    #end
           --TPB_GET_CLT_ACT_YN_FNC(A.N3PTY_NO, C.N3PTY_INV_NO) AS CLT_ACT_YN,
           (SELECT COUNT(1) FROM TPB_OTS_GRP_RCVR_ACT WHERE N3PTY_NO = C.N3PTY_NO AND N3PTY_CLT_RMK_TP_CD = 'M' AND ROWNUM = 1 ) AS CLT_ACT_YN,
           TO_CHAR( TPB_GET_LCL_DATE_FNC( (SELECT MIN(CRE_DT) FROM TPB_OTS_GRP_STS WHERE OTS_STS_CD='R' AND N3PTY_NO=C.N3PTY_NO)
                                         ,(SELECT DISTINCT OFC_CD FROM (SELECT N3PTY_OFC_TP_CD, OFC_CD, N3PTY_CTRL_OFC_CD, RHQ_CD FROM TPB_HNDL_OFC WHERE OFC_CD = @[s_user_ofc_cd])) )
                  ,'YYYY-MM-DD HH24:MI') AS STL_RQST_DT,
           NULL AS REVIEW_STEP
      FROM TPB_OTS_GRP_STS B,
           TPB_OTS_DTL A,
           TPB_OTS_GRP C
     WHERE 1=1
       AND A.N3PTY_NO = B.N3PTY_NO
       AND A.N3PTY_NO_DP_SEQ = 1
       AND A.N3PTY_NO = C.N3PTY_NO
       AND C.N3PTY_DELT_TP_CD IN ('N','S')
       AND B.OTS_STS_LST_FLG = 'Y'
       AND B.OTS_STS_CD IN ('O', 'J', 'M')
       AND A.N3PTY_BIL_TP_CD != 'JO'
--       AND NVL(A.N3PTY_TP_CD,'G') !='R'
	#if (${s_n3pty_no_strs_link} != '' && $s_n3pty_no_strs_link.size() > 0)
	   AND C.N3PTY_NO IN ( NULL
	   #foreach($s_n3pty_no_strs_link IN ${s_n3pty_no_strs_link})
	   		#if($velocityCount < $s_n3pty_no_strs_link.size())
	 		,'$s_n3pty_no_strs_link',
	 		#else
	 		,'$s_n3pty_no_strs_link'
	 		#end
	   #end
	   )
	#else
       #if (${s_n3pty_no} != '')
       		AND C.N3PTY_NO = @[s_n3pty_no]
       #else
       		AND A.CFM_DT >= TPB_GET_SRCH_DATE_FNC(@[s_calendar_date1], 'YYYY-MM-DD',@[s_user_ofc_cd])
       		AND A.CFM_DT <  TPB_GET_SRCH_DATE_FNC(@[s_calendar_date2], 'YYYY-MM-DD',@[s_user_ofc_cd]) + 1
       		#if (${s_vndr_cust_div_cd} != '')
            	AND C.VNDR_CUST_DIV_CD = @[s_vndr_cust_div_cd]
                	#if (${s_trd_party_val} != '')
                    	#if (${s_vndr_cust_div_cd} == 'V')
                        	AND C.VNDR_SEQ = TO_NUMBER(@[s_trd_party_val])
                    	#elseif (${s_vndr_cust_div_cd} == 'C')
                        	AND C.CUST_CNT_CD = SUBSTRB(TRIM(@[s_trd_party_val]),1,2)
                        	#if (${s_trd_party_val} != '')
                            	AND C.CUST_SEQ = TO_NUMBER(SUBSTRB(TRIM(@[s_trd_party_val]),3))
                        	#end
                    	#elseif (${s_vndr_cust_div_cd} == 'S')
                        	AND C.N3PTY_OFC_CD = @[s_trd_party_val]
                    	#end
                	#end
        	#end
       #end
        	AND C.OFC_CD IN (SELECT OFC_CD
							   FROM ( SELECT DISTINCT T.RHQ_CD, T.N3PTY_CTRL_OFC_CD, T.OFC_CD
										FROM TPB_HNDL_OFC T,
											 (SELECT N3PTY_OFC_TP_CD, OFC_CD, N3PTY_CTRL_OFC_CD, RHQ_CD FROM TPB_HNDL_OFC WHERE OFC_CD = @[s_user_ofc_cd]) L
									   WHERE 1=1
										 AND T.OFC_CD = L.OFC_CD
										  OR T.N3PTY_CTRL_OFC_CD = L.N3PTY_CTRL_OFC_CD
										  OR T.RHQ_CD = L.RHQ_CD
										 AND L.N3PTY_OFC_TP_CD = 'S'
UNION
SELECT   DISTINCT
         RHQ_CD
       , N3PTY_CTRL_OFC_CD
       , OFC_CD
FROM     TPB_HNDL_OFC
WHERE    1 = 1
AND      @[s_user_ofc_cd] IN ( SELECT ATTR_CTNT1 AS OFC_CD FROM TPB_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'HO_OFC_CD' ) --SELCON 조직 변경
							 ) WHERE 1=1
							 #if (${s_if_rhq_cd} != '')
								 AND RHQ_CD = @[s_if_rhq_cd]
								 #if (${s_if_ctrl_cd} != '')
									AND N3PTY_CTRL_OFC_CD = @[s_if_ctrl_cd]
									#if (${s_if_ofc_cd} != '')
										AND OFC_CD = @[s_if_ofc_cd]
