<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESeBillingAckManageDBDAOCompleteAckEdiSndLogStsUSQL">
			<desc><![CDATA[ACK 전송 후 완료 표시하기]]></desc>
			<sql><![CDATA[
UPDATE TES_EDI_ACK_SND_LOG A
SET A.ACK_SND_STS_CD = 'S', A.ACT_SND_DT = SYSDATE
WHERE 1=1
AND A.EDI_SND_DT = @[edi_snd_dt]
AND A.SND_LOG_SEQ = @[snd_log_seq]			]]></sql>
			<params>
				<param name="edi_snd_dt" type="12" value="" out="N"/>
				<param name="snd_log_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
