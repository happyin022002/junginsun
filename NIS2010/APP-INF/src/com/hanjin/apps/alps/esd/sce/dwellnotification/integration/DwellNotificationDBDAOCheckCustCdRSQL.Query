<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOCheckCustCdRSQL">
			<desc><![CDATA[CheckCustCd]]></desc>
			<sql><![CDATA[
SELECT COUNT(0) KNT
FROM MDM_CUSTOMER
WHERE 1=1
AND CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND CUST_SEQ = SUBSTR(@[cust_cd],3)
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
