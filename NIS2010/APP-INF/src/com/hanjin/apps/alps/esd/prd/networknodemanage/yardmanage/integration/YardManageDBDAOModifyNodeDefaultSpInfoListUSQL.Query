<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="YardManageDBDAOModifyNodeDefaultSpInfoListUSQL">
			<desc><![CDATA[수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_YARD
   SET 
		UPD_DT = SYSDATE,
#if(${chk_os_d} == 'D')
			N4TH_VNDR_CNT_CD = @[n4th_vndr_cnt_cd],
			N4TH_VNDR_SEQ    = TO_NUMBER(@[n4th_vndr_seq]),			
#end
#if(${chk_os_e} == 'E')
			N5TH_VNDR_CNT_CD = @[n5th_vndr_cnt_cd],
			N5TH_VNDR_SEQ    = TO_NUMBER(@[n5th_vndr_seq]),		
#end
#if(${chk_os_f} == 'F')
			N6TH_VNDR_CNT_CD = @[n6th_vndr_cnt_cd],
			N6TH_VNDR_SEQ    = TO_NUMBER(@[n6th_vndr_seq]),	
#end
		UPD_USR_ID       = @[updusrid]
 WHERE YD_CD = @[yd_cd]			]]></sql>
			<params>
				<param name="n4th_vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="n4th_vndr_seq" type="12" value="" out="N"/>
				<param name="n5th_vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="n5th_vndr_seq" type="12" value="" out="N"/>
				<param name="n6th_vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="n6th_vndr_seq" type="12" value="" out="N"/>
				<param name="updusrid" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
