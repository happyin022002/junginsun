<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BiddingCandidateDBDAOSearchBiddingNoRSQL">
			<desc><![CDATA[Spot Bidding Number 채번]]></desc>
			<sql><![CDATA[
SELECT @[trsp_so_ofc_cty_cd]||TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),'YYYYMMDD')||LPAD(TRS_SPOT_BID_NO_SEQ.NEXTVAL,4,'0') SPOT_BID_NO
  FROM DUAL			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
