<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccrualBatchExecuteResultDBDAOCreateGlAcclIfExeYrmonCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO 	gl_accl_itrm_if ( 																									
          				exe_yrmon
					, 	sys_src_id
					, 	rev_yrmon
					, 	acct_cd
					, 	accl_seq
					, 	biz_ut_id
					, 	vsl_cd
					, 	skd_voy_no
					, 	skd_dir_cd
					, 	rev_dir_cd
					, 	estm_vvd_tp_cd
					, 	estm_ioc_div_cd
					, 	estm_vvd_hdr_id
					, 	estm_bc_div_cd
					, 	nod_cd
					, 	estm_cost_amt
					, 	act_cost_amt
					, 	accl_cost_amt
					, 	cre_usr_id
					, 	cre_dt
					, 	upd_usr_id
					, 	upd_dt
					)                                                                                       

SELECT					XX.EXE_YRMON
					,   XX.SYS_SRC_ID
					,   XX.REV_YRMON
					,   XX.ACCT_CD
					,   XX.ACCL_SEQ
					,   XX.BIZ_UT_ID
					,	XX.VSL_CD            		
					,	XX.SKD_VOY_NO 	
					,	XX.SKD_DIR_CD  	
					,	XX.REV_DIR_CD   	
					,	XX.ESTM_VVD_TP_CD  
					,	XX.ESTM_IOC_DIV_CD 
					,	XX.ESTM_VVD_HDR_ID 
					,	XX.ESTM_BC_DIV_CD  
					,	XX.NOD_CD   		
					,	XX.ESTM_COST_AMT   
					,	XX.ACT_COST_AMT 
					,	XX.ACCL_COST_AMT   
					,	XX.CRE_USR_ID   	
					,	XX.CRE_DT   		
					,	XX.UPD_USR_ID   	
					,	XX.UPD_DT   		
FROM					(          					
          				SELECT   	a.exe_yrmon                					/* exe_yrmon       */
          						,	'ESD'           			SYS_SRC_ID		/* sys_src_id      */
          						, 	a.rev_yrmon                					/* rev_yrmon       */
          						, 	a.acct_cd                  					/* acct_cd         */
          						, 	(b.accl_seq + ROWNUM)   	ACCL_SEQ     	/* accl_seq        */
          						, 	'CNTR'         				BIZ_UT_ID		/* biz_ut_id       */
          						, 	'CNTC'         				VSL_CD			/* vsl_cd          */
          						,	SUBSTR (rev_yrmon, 3, 4)	SKD_VOY_NO 		/* skd_voy_no      */
          						, 	'M'                     	SKD_DIR_CD  	/* skd_dir_cd      */
          						, 	'M'                     	REV_DIR_CD   	/* rev_dir_cd      */
          						, 	'RV'                    	ESTM_VVD_TP_CD	/* estm_vvd_tp_cd  */
          						, 	'XX'                    	ESTM_IOC_DIV_CD	/* estm_ioc_div_cd */
          						, 	'99999'                 	ESTM_VVD_HDR_ID	/* estm_vvd_hdr_id */
          						, 	'C'                     	ESTM_BC_DIV_CD	/* estm_bc_div_cd  */
          						, 	'HQCOL'                 	NOD_CD   		/* nod_cd          */
          						, 	SUM (a.estm_cost_amt)   	ESTM_COST_AMT   /* estm_cost_amt   */
          						, 	DECODE(a.acct_cd , '512181', sum(a.pre_act_cost_amt), '512381', sum(a.pre_act_cost_amt), 0 )	ACT_COST_AMT /* act_cost_amt */
          						, 	SUM (a.accl_cost_amt)   	ACCL_COST_AMT   /* accl_cost_amt   */
          						, 	'UI_MNL_SAV'            	CRE_USR_ID   	/* cre_usr_id      */
          						, 	SYSDATE                 	CRE_DT   		/* cre_dt          */
          						, 	'UI_MNL_SAV'            	UPD_USR_ID   	/* upd_usr_id      */
          						, 	SYSDATE                 	UPD_DT   		/* upd_dt          */                                                      
          				FROM		lea_acct_cost_amt a
          						,	(	SELECT 		/*+ INDEX_DESC(gl_accl_itrm_if XAK1GL_ACCL_ITRM_IF) */                                                        
                           							accl_seq                                                                                           
                      					FROM 		GL_ACCL_ITRM_IF                                                                                         
                     					WHERE		exe_yrmon 		= REPLACE(@[exe_yrmon], '-')  
                     					AND 		ROWNUM 			= 1
                     				) b                                 						
          				WHERE 		a.exe_yrmon 					= REPLACE(@[exe_yrmon], '-')                                   							                
          				AND 		a.accl_auto_cd 					IN ('A', 'M') 				/* 2012/01 FULL VOL. INCENTIVE ACCOUNT CODE 포함 */                                                                                   
          				AND			a.mnl_inp_flg 					= 'Y'   --Manual Rep Account 로 입력한 계정(Account Code)  
          				GROUP BY 	a.exe_yrmon
          						, 	a.rev_yrmon
          						, 	a.acct_cd
          						, 	(b.accl_seq + ROWNUM)
          						, 	SUBSTR (rev_yrmon, 3, 4)
          					
          				) XX
WHERE					NOT (XX.ESTM_COST_AMT = 0 AND XX.ACT_COST_AMT = 0 AND XX.ACCL_COST_AMT = 0)      /* COST AMT 0 인 데이터 제외처리 */			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
