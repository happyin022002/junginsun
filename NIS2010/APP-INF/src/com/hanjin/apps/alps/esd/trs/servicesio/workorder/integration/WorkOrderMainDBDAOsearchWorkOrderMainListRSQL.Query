<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderMainDBDAOsearchWorkOrderMainListRSQL">
			<desc><![CDATA[search Work Order Main List]]></desc>
			<sql><![CDATA[
select /*+ index_desc(a XAK3TRS_TRSP_WRK_ORD) */
       to_char(a.locl_cre_dt, 'yyyy-mm-dd') cre_dt,
				  a.wo_iss_sts_cd ,
				  decode(a.wo_iss_sts_cd,'C','Correction','I','Issued','J',
		      			'Rejected','N','Cancellation','P','Partially Rejected','R','Reissued' ) issue_type_nm,
				  a.trsp_wo_ofc_cty_cd ||a.trsp_wo_seq	  trsp_wo_no
  from trs_trsp_wrk_ord a
 where a.wo_vndr_seq = @[wo_vndr_seq]
   and nvl(a.wo_opn_flg,'N') != 'Y'
   and nvl(a.delt_flg,'N') != 'Y' --a.delt_flg != 'Y'
   and a.wo_iss_sts_cd in ('I','R')
   and a.inter_use_flg != 'Y'
   and rownum <= 5
   and a.locl_cre_dt >= to_date('20170213', 'yyyyMMdd')			]]></sql>
			<params>
				<param name="wo_vndr_seq" type="2" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
