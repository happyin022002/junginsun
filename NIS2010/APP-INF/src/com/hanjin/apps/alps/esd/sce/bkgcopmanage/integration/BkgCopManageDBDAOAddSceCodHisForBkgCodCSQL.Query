<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOAddSceCodHisForBkgCodCSQL">
			<desc><![CDATA[Booking COD 발생 시 SCE COD History 데이터 생성]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_COD_HIS 
(
	COD_RCV_SEQ,
	BKG_NO,
	OLD_POD_YD_CD,
	OLD_DEL_YD_CD,
	NEW_POD_YD_CD,
	NEW_DEL_YD_CD,
	COD_MAPG_STS_CD,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
)
SELECT
	SCE_COD_HIS_SEQ1.NEXTVAL AS COP_RCV_SEQ,
	@[bkg_no],
	@[old_pod_yd_cd],
	@[old_del_yd_cd],
	@[new_pod_yd_cd],
	@[new_del_yd_cd],
	'00',
	'COD_SYS',
	SYSDATE,
	'COD_SYS',
	SYSDATE
FROM DUAL			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="old_pod_yd_cd" type="12" value="" out="N"/>
				<param name="old_del_yd_cd" type="12" value="" out="N"/>
				<param name="new_pod_yd_cd" type="12" value="" out="N"/>
				<param name="new_del_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
