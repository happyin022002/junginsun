<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchMdmLocRSQL">
			<desc><![CDATA[SearchMdmLoc]]></desc>
			<sql><![CDATA[
select conti_cd  v_chkconti 
        from mdm_location 
        where loc_cd = substr(@[in_nod_cd],1,5)			]]></sql>
			<params>
				<param name="in_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
