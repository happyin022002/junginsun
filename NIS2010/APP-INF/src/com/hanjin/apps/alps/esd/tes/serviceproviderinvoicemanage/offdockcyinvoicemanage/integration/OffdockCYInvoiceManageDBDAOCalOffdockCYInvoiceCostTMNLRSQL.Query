        = DECODE(NVL(LD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'9','Y','N'),'N')
 AND    NVL(L.TML_TRNS_MOD_CD,'S') = DECODE(NVL(LD.TML_TRNS_MOD_CD,'S'),'S',NVL(L.TML_TRNS_MOD_CD,'S'),NVL(LD.TML_TRNS_MOD_CD,'S'))
 GROUP BY DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT'),
