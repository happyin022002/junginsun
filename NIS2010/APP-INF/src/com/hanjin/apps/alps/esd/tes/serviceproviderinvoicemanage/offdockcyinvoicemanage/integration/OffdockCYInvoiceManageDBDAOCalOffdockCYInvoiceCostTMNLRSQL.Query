<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OffdockCYInvoiceManageDBDAOCalOffdockCYInvoiceCostTMNLRSQL">
			<desc><![CDATA[CalOffdockCYInvoiceCostTMNL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 'I' IBFLAG, 'A' CALC_TP_CD, 'TM' CALC_COST_GRP_CD,
       N.LGS_COST_CD, N.CNTR_TPSZ_CD, N.DCGO_CLSS_CD DCGO_IND_CD, N.VOL_TR_UT_CD,
       DECODE(N.THC,'T',N.CALC_VOL_QTY, 'L', L.CALC_VOL_QTY, 'G', G.CALC_VOL_QTY) CALC_VOL_QTY,
       DECODE(N.THC,'T',N.CALC_VOL_QTY, 'L', L.CALC_VOL_QTY, 'G', G.CALC_VOL_QTY) RVIS_VOL_QTY,
       DECODE(N.THC,'T',N.CTRT_RT, 'L',NVL(L.CTRT_RT, 0) + NVL(G.CTRT_RT, 0), 'G', NVL(G.CTRT_RT, 0) + NVL(L.CTRT_RT, 0)) * 2 CTRT_RT,
       DECODE(N.THC,'T',N.CALC_VOL_QTY, 'L', L.CALC_VOL_QTY, 'G', G.CALC_VOL_QTY) *
       (DECODE(N.THC,'T',N.CTRT_RT, 'L', NVL(L.CTRT_RT, 0) + NVL(G.CTRT_RT, 0), 'G', NVL(G.CTRT_RT, 0) + NVL(L.CTRT_RT, 0)) * 2) INV_AMT,
       DECODE(N.THC,'T',N.CALC_VOL_QTY, 'L', L.CALC_VOL_QTY, 'G', G.CALC_VOL_QTY)*
       (DECODE(N.THC,'T',N.CTRT_RT, 'L', NVL(L.CTRT_RT, 0) + NVL(G.CTRT_RT, 0), 'G', NVL(G.CTRT_RT, 0) + NVL(L.CTRT_RT, 0)) * 2) CALC_AMT,
       C.ACCT_CD, N.TML_AGMT_OFC_CTY_CD, N.TML_AGMT_SEQ, N.TML_AGMT_VER_NO, N.CURR_CHK, CURR_CD, 1 INV_XCH_RT,
       DECODE(N.RC_FLG,'Y','Y','N') RC_FLG, N.TML_TRNS_MOD_CD
FROM (
 SELECT DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') LGS_COST_CD,
        L.CNTR_TPSZ_CD, NVL(L.DCGO_CLSS_CD,'N') DCGO_CLSS_CD,
        SUM(DECODE(@[tml_calc_ind_cd],'TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) CALC_VOL_QTY,
        SUM(DECODE(@[tml_calc_ind_cd],'TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) RVIS_VOL_QTY,
        ND.TML_AGMT_VOL_UT_CD VOL_TR_UT_CD, NVL(ND.THC_TP_CD,'T') THC,
        DECODE(ND.TML_AGMT_VOL_UT_CD,'C',NC.AGMT_RT,ND.AGMT_UT_RT) CTRT_RT,
        SUM(DECODE(@[tml_calc_ind_cd],'TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) *
        DECODE(ND.TML_AGMT_VOL_UT_CD,'C',NC.AGMT_RT,ND.AGMT_UT_RT) INV_AMT,
        H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO, DECODE(@[curr_cd],ND.CURR_CD,'Y','N') CURR_CHK, ND.CURR_CD
        , NVL(DECODE(L.RC_FLG,'Y','Y','N'),'N') RC_FLG, NVL(L.TML_TRNS_MOD_CD,'S') TML_TRNS_MOD_CD
 FROM   TES_TML_SO_CNTR_LIST L, TES_TML_AGMT_HDR H,
        TES_TML_AGMT_DTL ND, TES_TML_AGMT_TP_SZ NC, TES_TML_AGMT_DG_CGO_CLSS NG
 WHERE  L.VRFY_RSLT_IND_CD    = 'CO'
 AND    L.TML_SO_OFC_CTY_CD   = @[tml_so_ofc_cty_cd]
 AND    L.TML_SO_SEQ          = @[tml_so_seq]
 AND    H.YD_CD               = @[yd_cd]
 AND    H.VNDR_SEQ            = @[vndr_seq]
 AND    H.TML_AGMT_STS_CD     = 'C'
 AND    H.DELT_FLG            = 'N'
 AND    TO_CHAR(H.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
 AND    TO_CHAR(H.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt]
 AND    H.TML_AGMT_VER_NO = ( SELECT MAX(M.TML_AGMT_VER_NO)
                              FROM   TES_TML_AGMT_HDR M
                              WHERE  M.YD_CD               = @[yd_cd]
                              AND    M.VNDR_SEQ            = @[vndr_seq]
                              AND    M.TML_AGMT_STS_CD     = 'C'
                              AND    M.DELT_FLG            = 'N'
                              AND    TO_CHAR(M.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
                              AND    TO_CHAR(M.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt] )
 AND    H.TML_AGMT_OFC_CTY_CD = ND.TML_AGMT_OFC_CTY_CD(+)
 AND    H.TML_AGMT_SEQ        = ND.TML_AGMT_SEQ(+)
 AND    H.TML_AGMT_VER_NO     = ND.TML_AGMT_VER_NO(+)
 AND    DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') = ND.LGS_COST_CD
 AND    ND.TML_AGMT_TP_CD      = 'T'
 AND    ND.AUTO_CALC_FLG       = 'Y'
 AND    NVL(ND.IO_BND_CD,'N')  = DECODE(NVL(ND.IO_BND_CD,'N'),'N','N','S','S',L.IO_BND_CD)
 AND    ND.TML_AGMT_OFC_CTY_CD = NC.TML_AGMT_OFC_CTY_CD(+)
 AND    ND.TML_AGMT_SEQ        = NC.TML_AGMT_SEQ(+)
 AND    ND.TML_AGMT_VER_NO     = NC.TML_AGMT_VER_NO(+)
 AND    ND.TML_AGMT_DTL_SEQ    = NC.TML_AGMT_DTL_SEQ(+)
 AND    DECODE(ND.TML_AGMT_VOL_UT_CD,'C',NC.CNTR_TPSZ_CD,'N')
        = DECODE(ND.TML_AGMT_VOL_UT_CD,'C',DECODE(L.CNTR_STY_CD, 'M', CASE WHEN L.CNTR_TPSZ_CD IS NOT NULL AND SUBSTR(L.CNTR_TPSZ_CD,1,1)='R'
                                                                           THEN L.CNTR_TPSZ_CD
                                                                           WHEN L.CNTR_TPSZ_CD IS NOT NULL AND SUBSTR(L.CNTR_TPSZ_CD,1,1)='O' 
                                                                           THEN L.CNTR_TPSZ_CD    
                                                                           ELSE 'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)
                                                                           END,
                                DECODE(SUBSTR(L.CNTR_TPSZ_CD,1,1),'R',DECODE(L.RC_FLG,     'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                  'O',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                  'F',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                  'P',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                  'A',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                  'S',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                  L.CNTR_TPSZ_CD)), 'N')
 AND    ND.TML_AGMT_OFC_CTY_CD = NG.TML_AGMT_OFC_CTY_CD(+)
 AND    ND.TML_AGMT_SEQ        = NG.TML_AGMT_SEQ(+)
 AND    ND.TML_AGMT_VER_NO     = NG.TML_AGMT_VER_NO(+)
 AND    ND.TML_AGMT_DTL_SEQ    = NG.TML_AGMT_DTL_SEQ(+)
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'N','N',NG.DCGO_APLY_TP_CD) = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'N','N','R')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','N',NG.DCGO_SAM_CLSS_FLG),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','N','Y'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N',NG.DCGO_NON_CLSS_FLG,'N'),'S',DECODE(L.DCGO_CLSS_CD,'N',NG.DCGO_NON_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','Y','N'),'S',DECODE(L.DCGO_CLSS_CD,'N','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'1',NG.DCGO_N1ST_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'1','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'2',NG.DCGO_N2ND_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'2','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'3',NG.DCGO_N3RD_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'3','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'4',NG.DCGO_N4TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'4','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'5',NG.DCGO_N5TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'5','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'6',NG.DCGO_N6TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'6','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'7',NG.DCGO_N7TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'7','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'8',NG.DCGO_N8TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'8','Y','N'),'N')
 AND    DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'9',NG.DCGO_N9TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(ND.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'9','Y','N'),'N')
 AND    NVL(L.TML_TRNS_MOD_CD,'S') = DECODE(NVL(ND.TML_TRNS_MOD_CD,'S'),'S',NVL(L.TML_TRNS_MOD_CD,'S'),NVL(ND.TML_TRNS_MOD_CD,'S'))
 GROUP BY DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT'),
        L.CNTR_TPSZ_CD, L.DCGO_CLSS_CD, ND.TML_AGMT_VOL_UT_CD, NVL(ND.THC_TP_CD,'T'),
        DECODE(ND.TML_AGMT_VOL_UT_CD,'C',NC.AGMT_RT,ND.AGMT_UT_RT),
        H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO, ND.CURR_CD, NVL(DECODE(L.RC_FLG,'Y','Y','N'),'N'),
		NVL(L.TML_TRNS_MOD_CD,'S')
        ) N,
 ( SELECT DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') LGS_COST_CD,
        L.CNTR_TPSZ_CD, NVL(L.DCGO_CLSS_CD,'N') DCGO_CLSS_CD,
        SUM(DECODE('TP','TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) CALC_VOL_QTY,
        SUM(DECODE('TP','TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) RVIS_VOL_QTY,
        TD.TML_AGMT_VOL_UT_CD VOL_TR_UT_CD, NVL(TD.THC_TP_CD,'T') THC,
        DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.AGMT_RT,TD.AGMT_UT_RT) CTRT_RT,
        SUM(DECODE('TP','TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) *
        DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.AGMT_RT,TD.AGMT_UT_RT) INV_AMT,
        H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO, NVL(L.TML_TRNS_MOD_CD,'S') TML_TRNS_MOD_CD
 FROM   TES_TML_SO_CNTR_LIST L, TES_TML_AGMT_HDR H,
        TES_TML_AGMT_DTL TD, TES_TML_AGMT_TP_SZ TC, TES_TML_AGMT_DG_CGO_CLSS TG
 WHERE  L.VRFY_RSLT_IND_CD    = 'CO'
 AND    L.TML_SO_OFC_CTY_CD   = @[tml_so_ofc_cty_cd]
 AND    L.TML_SO_SEQ          = @[tml_so_seq]
 AND    H.YD_CD               = @[yd_cd]
 AND    H.VNDR_SEQ            = @[vndr_seq]
 AND    H.TML_AGMT_STS_CD     = 'C'
 AND    H.DELT_FLG            = 'N'
 AND    TO_CHAR(H.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
 AND    TO_CHAR(H.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt]
 AND    H.TML_AGMT_VER_NO = ( SELECT MAX(M.TML_AGMT_VER_NO)
                              FROM   TES_TML_AGMT_HDR M
                              WHERE  M.YD_CD               = @[yd_cd]
                              AND    M.VNDR_SEQ            = @[vndr_seq]
                              AND    M.TML_AGMT_STS_CD     = 'C'
                              AND    M.DELT_FLG            = 'N'
                              AND    TO_CHAR(M.EFF_FM_DT,'YYYYMMDD') <= @[to_prd_dt]
                              AND    TO_CHAR(M.EFF_TO_DT,'YYYYMMDD') >= @[to_prd_dt] )
 AND    H.TML_AGMT_OFC_CTY_CD = TD.TML_AGMT_OFC_CTY_CD(+)
 AND    H.TML_AGMT_SEQ        = TD.TML_AGMT_SEQ(+)
 AND    H.TML_AGMT_VER_NO     = TD.TML_AGMT_VER_NO(+)
 AND    DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') = TD.LGS_COST_CD
 AND    TD.TML_AGMT_TP_CD      = 'T'
 AND    TD.AUTO_CALC_FLG       = 'Y'
 AND    NVL(TD.THC_TP_CD,'T')  = 'T'
 AND    NVL(TD.IO_BND_CD,'N')  = DECODE(NVL(TD.IO_BND_CD,'N'),'N','N','S','S',L.IO_BND_CD)
 AND    TD.TML_AGMT_OFC_CTY_CD = TC.TML_AGMT_OFC_CTY_CD(+)
 AND    TD.TML_AGMT_SEQ        = TC.TML_AGMT_SEQ(+)
 AND    TD.TML_AGMT_VER_NO     = TC.TML_AGMT_VER_NO(+)
 AND    TD.TML_AGMT_DTL_SEQ    = TC.TML_AGMT_DTL_SEQ(+)
 AND    DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.CNTR_TPSZ_CD,'N')
    = DECODE(TD.TML_AGMT_VOL_UT_CD,'C',DECODE(L.CNTR_STY_CD, 'M', CASE WHEN L.CNTR_TPSZ_CD IS NOT NULL AND SUBSTR(L.CNTR_TPSZ_CD,1,1)='R'
                                                                       THEN L.CNTR_TPSZ_CD
                                                                       WHEN L.CNTR_TPSZ_CD IS NOT NULL AND SUBSTR(L.CNTR_TPSZ_CD,1,1)='O' 
                                                                       THEN L.CNTR_TPSZ_CD  
                                                                       ELSE 'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)
                                                                       END,
                                 DECODE(SUBSTR(L.CNTR_TPSZ_CD,1,1),'R',DECODE(L.RC_FLG,     'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                   'O',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                   'F',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                   'P',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                   'A',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                   'S',DECODE(L.AWK_CGO_FLG,'Y',L.CNTR_TPSZ_CD,'D'||SUBSTR(L.CNTR_TPSZ_CD,2,1)),
                                                                   L.CNTR_TPSZ_CD)), 'N')
 AND    TD.TML_AGMT_OFC_CTY_CD = TG.TML_AGMT_OFC_CTY_CD(+)
 AND    TD.TML_AGMT_SEQ        = TG.TML_AGMT_SEQ(+)
 AND    TD.TML_AGMT_VER_NO     = TG.TML_AGMT_VER_NO(+)
 AND    TD.TML_AGMT_DTL_SEQ    = TG.TML_AGMT_DTL_SEQ(+)
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'N','N',TG.DCGO_APLY_TP_CD) = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'N','N','R')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','N',TG.DCGO_SAM_CLSS_FLG),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','N','Y'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N',TG.DCGO_NON_CLSS_FLG,'N'),'S',DECODE(L.DCGO_CLSS_CD,'N',TG.DCGO_NON_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'A',DECODE(L.DCGO_CLSS_CD,'N','Y','N'),'S',DECODE(L.DCGO_CLSS_CD,'N','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'1',TG.DCGO_N1ST_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'1','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'2',TG.DCGO_N2ND_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'2','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'3',TG.DCGO_N3RD_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'3','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'4',TG.DCGO_N4TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'4','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'5',TG.DCGO_N5TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'5','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'6',TG.DCGO_N6TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'6','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'7',TG.DCGO_N7TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'7','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'8',TG.DCGO_N8TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'8','Y','N'),'N')
 AND    DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'9',TG.DCGO_N9TH_CLSS_FLG,'N'),'N')
        = DECODE(NVL(TD.DCGO_APLY_TP_CD,'N'),'S',DECODE(L.DCGO_CLSS_CD,'9','Y','N'),'N')
 AND    NVL(L.TML_TRNS_MOD_CD,'S') = DECODE(NVL(TD.TML_TRNS_MOD_CD,'S'),'S',NVL(L.TML_TRNS_MOD_CD,'S'),NVL(TD.TML_TRNS_MOD_CD,'S'))
 GROUP BY DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT'),
        L.CNTR_TPSZ_CD, L.DCGO_CLSS_CD, TD.TML_AGMT_VOL_UT_CD, NVL(TD.THC_TP_CD,'T'),
        DECODE(TD.TML_AGMT_VOL_UT_CD,'C',TC.AGMT_RT,TD.AGMT_UT_RT),
        H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO, NVL(L.TML_TRNS_MOD_CD,'S')
        ) T,
( SELECT DECODE(L.CNTR_STY_CD,'F','TMFDFL','TMFDMT') LGS_COST_CD,
        L.CNTR_TPSZ_CD, NVL(L.DCGO_CLSS_CD,'N') DCGO_CLSS_CD,
        SUM(DECODE('TP','TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) CALC_VOL_QTY,
        SUM(DECODE('TP','TP',DECODE(L.INV_GATE_OUT_DT,NULL,0,1),1)) RVIS_VOL_QTY,
