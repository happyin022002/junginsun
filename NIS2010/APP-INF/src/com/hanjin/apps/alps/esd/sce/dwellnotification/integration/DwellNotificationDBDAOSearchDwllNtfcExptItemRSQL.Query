<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOSearchDwllNtfcExptItemRSQL">
			<desc><![CDATA[SearchDwllNtfcExptItem]]></desc>
			<sql><![CDATA[
SELECT 
    A.SC_NO
,   @[cust_cd] AS CUST_CD
,   A.CTRT_PTY_NM
,  CASE WHEN SIGN(DECODE(TML_DWLL_FLG,'Y',1,0) + DECODE(ENR_DWLL_FLG,'Y',1,0) + DECODE(DEST_DWLL_FLG,'Y',1,0) + DECODE(VSL_DLAY_FLG,'Y',1,0)) = 0 THEN NULL
            ELSE TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', CTRT_EFF_DT, GLOBALDATE_PKG.GET_LOCCD_FNC(@[ofc_cd])), 'YYYYMMDD')
        END EFF_DT
,  CASE WHEN SIGN(DECODE(TML_DWLL_FLG,'Y',1,0) + DECODE(ENR_DWLL_FLG,'Y',1,0) + DECODE(DEST_DWLL_FLG,'Y',1,0) + DECODE(VSL_DLAY_FLG,'Y',1,0)) = 0 THEN NULL
            WHEN TO_EFF_DT >= '99991230' THEN TO_EFF_DT
            ELSE TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', CTRT_EXP_DT, GLOBALDATE_PKG.GET_LOCCD_FNC(@[ofc_cd])), 'YYYYMMDD')
        END EXP_DT
,   DECODE(TML_DWLL_FLG,'Y',1,0) TML_DWLL_FLG
,   DECODE(ENR_DWLL_FLG,'Y',1,0) ENR_DWLL_FLG
,   DECODE(DEST_DWLL_FLG,'Y',1,0) DEST_DWLL_FLG
,   DECODE(VSL_DLAY_FLG,'Y',1,0) VSL_DLAY_FLG
,   DWLL_EXPT_RMK, EXPT_SET_USR_ID, TO_CHAR(EXPT_SET_DT, 'YYYY-MM-DD HH24:MI:SS') EXPT_SET_DT
,   (SELECT U.USR_NM FROM COM_USER U WHERE U.USR_ID = SEXP.EXPT_SET_USR_ID) EXPT_SET_USR_NAME
FROM 
(
    SELECT
    *
    FROM
    (
        SELECT
            ROW_NUMBER() OVER ( PARTITION BY CUST_CNT_CD, CUST_SEQ ORDER BY HDR.CRE_DT DESC, PTY.AMDT_SEQ DESC) SEL_SEQ
        ,   PTY.PROP_NO
        ,   PTY.AMDT_SEQ
        ,   HDR.SC_NO
        ,   PTY.CUST_CNT_CD
        ,   PTY.CUST_SEQ
        ,   PTY.CTRT_PTY_NM
        ,   PTY.PRC_CTRT_PTY_TP_CD
        , CTRT_EFF_DT
        , CTRT_EXP_DT
        FROM 
            PRI_SP_CTRT_PTY PTY
        ,   PRI_SP_HDR HDR
        ,   PRI_SP_DUR DUR
        WHERE 1=1
        AND PTY.PRC_CTRT_PTY_TP_CD='C'
        AND SUBSTR(HDR.SC_NO, 1,3) IN ('AEF', 'AEN', 'ANW','ASE','AWE','ASW', 'AWN', 'GLO', 'SAN', 'SAS', 'CEN','ANW','CEF','MME')
        AND HDR.PROP_NO = PTY.PROP_NO    
        AND PTY.PROP_NO = DUR.PROP_NO
        AND PTY.AMDT_SEQ = DUR.AMDT_SEQ
    )
    WHERE SEL_SEQ=1
    ) A
,   SCE_DWLL_NTFC_CUST_EXPT SEXP
WHERE 1=1
AND SEXP.DWLL_CUST_CNT_CD = A.CUST_CNT_CD(+)
AND SEXP.DWLL_CUST_SEQ = A.CUST_SEQ(+)
AND SEXP.DWLL_CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND SEXP.DWLL_CUST_SEQ = SUBSTR(@[cust_cd],3)
ORDER BY A.SC_NO			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
