<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceProviderManageDBDAOModifyMdmVendorInfoUSQL">
			<desc><![CDATA[ModifyMdmVendorInfo]]></desc>
			<sql><![CDATA[
UPDATE MDM_VENDOR
SET  IDA_PAN_NO = @[ida_pan_no]
    ,IDA_GST_RGST_NO = @[ida_gst_rgst_no]
    ,IDA_SPCL_ECN_ZN_UT_FLG = @[ida_spcl_ecn_zn_ut_flg]
    ,IDA_CNTC_PSON_NM = @[ida_cntc_pson_nm]
    ,IDA_ALTN_RCVR_NM = @[ida_altn_rcvr_nm]
	,IDA_SPCL_ECN_ZN_DOC_NO = @[ida_spcl_ecn_zn_doc_no]
	,IDA_SPCL_ECN_ZN_DOC_DESC = @[ida_spcl_ecn_zn_doc_desc]
	,IDA_CO_TYPE_CD = @[ida_co_type_cd]
	,IDA_GST_RGST_STS_CD = @[ida_gst_rgst_sts_cd]
	,IDA_GST_RGST_TP_CD = @[ida_gst_rgst_tp_cd]
	,UPD_USR_ID = @[upd_usr_id]
	,UPD_DT = SYSDATE
WHERE VNDR_SEQ = @[ida_vndr_seq]			]]></sql>
			<params>
				<param name="ida_pan_no" type="12" value="" out="N"/>
				<param name="ida_gst_rgst_no" type="12" value="" out="N"/>
				<param name="ida_spcl_ecn_zn_ut_flg" type="12" value="" out="N"/>
				<param name="ida_cntc_pson_nm" type="12" value="" out="N"/>
				<param name="ida_altn_rcvr_nm" type="12" value="" out="N"/>
				<param name="ida_spcl_ecn_zn_doc_no" type="12" value="" out="N"/>
				<param name="ida_spcl_ecn_zn_doc_desc" type="12" value="" out="N"/>
				<param name="ida_co_type_cd" type="12" value="" out="N"/>
				<param name="ida_gst_rgst_sts_cd" type="12" value="" out="N"/>
				<param name="ida_gst_rgst_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ida_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
