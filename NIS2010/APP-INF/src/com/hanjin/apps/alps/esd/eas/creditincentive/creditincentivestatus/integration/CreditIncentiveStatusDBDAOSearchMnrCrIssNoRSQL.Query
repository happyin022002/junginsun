<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAOSearchMnrCrIssNoRSQL">
			<desc><![CDATA[M&R Credit Insertì‹œ Credit Issue Number Search]]></desc>
			<sql><![CDATA[
SELECT 'MNR'||TO_CHAR(SYSDATE,'YYYY')||LPAD(CR_ISS_NO,4,'0') CR_ISS_NO
FROM ( SELECT CASE WHEN MAX(TO_NUMBER(SUBSTR(A.CR_ISS_NO,8,4))) IS NULL THEN 1
                   ELSE MAX(TO_NUMBER(SUBSTR(A.CR_ISS_NO,8,4)))+1
	           END AS CR_ISS_NO
         FROM EAS_MNR_CR_ISS A
        WHERE SUBSTR(A.CR_ISS_NO,1,7) = 'MNR'||TO_CHAR(SYSDATE,'YYYY')
      )			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
