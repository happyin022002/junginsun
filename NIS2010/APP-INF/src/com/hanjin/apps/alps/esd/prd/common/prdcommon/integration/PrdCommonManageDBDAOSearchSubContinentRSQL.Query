<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCommonManageDBDAOSearchSubContinentRSQL">
			<desc><![CDATA[SearchSubContinent]]></desc>
			<sql><![CDATA[
SELECT SCONTI_CD conti_code, SCONTI_NM conti_name FROM MDM_SUBCONTINENT WHERE CONTI_CD= @[conti_code] and nvl( delt_flg,'N') <> 'Y'			]]></sql>
			<params>
				<param name="conti_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
