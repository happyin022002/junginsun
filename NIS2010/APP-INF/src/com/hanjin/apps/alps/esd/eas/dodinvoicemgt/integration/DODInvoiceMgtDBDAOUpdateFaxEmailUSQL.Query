<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DODInvoiceMgtDBDAOUpdateFaxEmailUSQL">
			<desc><![CDATA[UpdateFaxEmail]]></desc>
			<sql><![CDATA[
UPDATE EAS_DOD_INV_MN
SET  
#if(${send_flg} == 'F')
	FAX_SND_USR_ID = @[cre_usr_id]
	,FAX_SND_DT = NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
#end
#if(${send_flg} == 'E')	
	EML_SND_USR_ID = @[cre_usr_id]
	,EML_SND_DT = NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
#end
WHERE DOD_INV_NO  = @[dod_inv_no]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="dod_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
