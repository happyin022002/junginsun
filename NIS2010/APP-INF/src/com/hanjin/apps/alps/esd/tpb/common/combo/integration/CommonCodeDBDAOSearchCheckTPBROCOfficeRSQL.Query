<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchCheckTPBROCOfficeRSQL">
			<desc><![CDATA[SearchCheckTPBROCOffice]]></desc>
			<sql><![CDATA[
SELECT STL_RQST_OFC_CD
FROM TPB_ADJ_STS
WHERE N3PTY_NO = @[n3pty_no]
AND   STL_STS_LST_FLG = 'Y'			]]></sql>
			<params>
				<param name="n3pty_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
