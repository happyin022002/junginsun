<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsAdvanceAuditDBDAOModifyTrsCrteRSQL">
			<desc><![CDATA[insert 전 이미 저장된 데이터인지 확인한다.]]></desc>
			<sql><![CDATA[
 SELECT COUNT(1) AS CNT FROM  EAS_TRSP_AUTO_AUD_CRTE
 WHERE AUD_OFC_CD            = @[aud_ofc_cd]
   AND TRSP_CRR_MOD_CD       = @[trsp_crr_mod_cd]
   AND CGO_TP_CD             = @[cgo_tp_cd]
   AND EXPN_AUD_CRTE_TP_CD   = @[expn_aud_crte_tp_cd]			]]></sql>
			<params>
				<param name="aud_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_crr_mod_cd" type="12" value="" out="N"/>
				<param name="cgo_tp_cd" type="12" value="" out="N"/>
				<param name="expn_aud_crte_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
