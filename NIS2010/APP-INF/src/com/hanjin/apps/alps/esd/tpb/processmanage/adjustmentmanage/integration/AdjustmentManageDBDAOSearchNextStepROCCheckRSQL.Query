<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentManageDBDAOSearchNextStepROCCheckRSQL">
			<desc><![CDATA[Next Step Check]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(H.N3PTY_NO),0,'N','Y') NXT_STP_ROC_CHK
  FROM TPB_ADJ_STS S, TPB_ADJ_N2ND_RVW_HIS H
 WHERE S.N3PTY_NO = @[n3pty_no]
   AND S.STL_STS_LST_FLG = 'Y'
   AND H.N3PTY_NO = S.N3PTY_NO
   AND H.ADJ_STS_SEQ = S.ADJ_STS_SEQ
   AND H.UPD_USR_ID = (
                         SELECT DECODE(N3PTY_OFC_TP_CD,'S',RHQ_CD,OFC_CD)
                           FROM TPB_HNDL_OFC
                          WHERE OFC_CD = @[user_ofc_cd]
                            AND DELT_FLG = 'N'
                            AND ROWNUM=1
                      )
   AND H.ADJ_N2ND_RVW_STS_CD = DECODE(H.UPD_USR_ID
                             ,TPB_GET_HNDL_OFC_FNC('R',S.STL_TO_CLT_CNG_OFC_CD),'R2'
                             ,TPB_GET_HNDL_OFC_FNC('R',S.STL_RQST_OFC_CD),'R3','R4')
   AND H.N2ND_RVW_AVAL_FLG IS NULL			]]></sql>
			<params>
				<param name="n3pty_no" type="12" value="" out="N"/>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
