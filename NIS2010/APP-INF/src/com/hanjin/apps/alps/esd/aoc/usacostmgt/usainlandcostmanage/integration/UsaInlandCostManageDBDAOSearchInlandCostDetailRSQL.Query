<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInlandCostManageDBDAOSearchInlandCostDetailRSQL">
			<desc><![CDATA[searchInlandCostDetail]]></desc>
			<sql><![CDATA[
SELECT  ROWNUM ROW_NUM
      , SUBSTR(PORT_NOD_CD, 1,5) || '-' || SUBSTR(LOC_NOD_CD, 1,5) PORT_LOC
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD01725',RCV_DE_TERM_CD) RCV_DE_TERM_NM
      , NVL(B.COST_SEL_ROUT_FLG, 'N') COST_SEL_ROUT_FLG
      , PORT_NOD_CD
      , HUB_NOD_CD
      , LOC_NOD_CD
      , TRSP_CRR_MOD_CD
      , B.MTY_PKUP_RTN_YD_CD
      , B.CURR_CD
      , INLND_20FT_TTL_AMT
      , INLND_40FT_TTL_AMT
      , SCC_CD
      , NVL(MB_20FT_RTO,0)||'%' MB_20FT_RTO
      , NVL(MB_40FT_RTO,0)||'%' MB_40FT_RTO
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', RAIL_20FT_BZC_COST_SRC_CD) RAIL_20FT_BZC_COST_SRC_NM
      , RAIL_20FT_BZC_COST_AMT
      , RAIL_20FT_BZC_COST_ADJ_AMT
      , RAIL_20FT_BZC_COST_TTL_AMT
      , RAIL_20FT_BZC_AGMT_WGT
      , RAIL_20FT_BZC_AGMT_WY_TP_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', RAIL_20FT_FUEL_SCG_SRC_CD) RAIL_20FT_FUEL_SCG_SRC_NM
      , RAIL_20FT_FUEL_SCG_AMT
      , RAIL_20FT_FUEL_SCG_ADJ_AMT
      , RAIL_20FT_FUEL_SCG_TTL_AMT
      , RAIL_20FT_FUEL_SCG_AGMT_WGT
      , RAIL_20FT_FUEL_AGMT_WY_TP_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', RAIL_40FT_BZC_COST_SRC_CD) RAIL_40FT_BZC_COST_SRC_NM
      , RAIL_40FT_BZC_COST_AMT
      , RAIL_40FT_BZC_COST_ADJ_AMT
      , RAIL_40FT_BZC_COST_TTL_AMT
      , RAIL_40FT_BZC_AGMT_WGT
      , RAIL_40FT_BZC_AGMT_WY_TP_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', RAIL_40FT_FUEL_SCG_SRC_CD) RAIL_40FT_FUEL_SCG_SRC_NM
      , RAIL_40FT_FUEL_SCG_AMT
      , RAIL_40FT_FUEL_SCG_ADJ_AMT
      , RAIL_40FT_FUEL_SCG_TTL_AMT
      , RAIL_40FT_FUEL_SCG_AGMT_WGT
      , RAIL_40FT_FUEL_AGMT_WY_TP_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', TRK_20FT_BZC_COST_SRC_CD) TRK_20FT_BZC_COST_SRC_NM
      , TRK_20FT_BZC_COST_AMT
      , TRK_20FT_BZC_COST_ADJ_AMT
      , TRK_20FT_BZC_COST_TTL_AMT
      , B.TRSP_AGMT_20FT_MTY_YD_CD
      , CASE WHEN NVL(B.MTY_PKUP_RTN_YD_CD, 1) = NVL(B.TRSP_AGMT_20FT_MTY_YD_CD, 1) THEN 'N' ELSE 'Y' END AS TRSP_DIFF_20FT
      , TRK_20FT_BZC_AGMT_WGT
      , TRK_20FT_BZC_AGMT_WY_TP_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', TRK_20FT_FUEL_SCG_SRC_CD) TRK_20FT_FUEL_SCG_SRC_NM
      , TRK_20FT_FUEL_SCG_AMT
      , TRK_20FT_FUEL_SCG_ADJ_AMT
      , TRK_20FT_FUEL_SCG_TTL_AMT
      , TRK_20FT_FUEL_SCG_AGMT_WGT
      , TRK_20FT_FUEL_AGMT_WY_TP_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', TRK_40FT_BZC_COST_SRC_CD) TRK_40FT_BZC_COST_SRC_NM
      , TRK_40FT_BZC_COST_AMT
      , TRK_40FT_BZC_COST_ADJ_AMT
      , TRK_40FT_BZC_COST_TTL_AMT
      , B.TRSP_AGMT_40FT_MTY_YD_CD
      , CASE WHEN NVL(B.MTY_PKUP_RTN_YD_CD, 1) = NVL(B.TRSP_AGMT_40FT_MTY_YD_CD, 1) THEN 'N' ELSE 'Y' END AS TRSP_DIFF_40FT
      , TRK_40FT_BZC_AGMT_WGT
      , TRK_40FT_BZC_AGMT_WY_TP_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', TRK_40FT_FUEL_SCG_SRC_CD) TRK_40FT_FUEL_SCG_SRC_NM
      , TRK_40FT_FUEL_SCG_AMT
      , TRK_40FT_FUEL_SCG_ADJ_AMT
      , TRK_40FT_FUEL_SCG_TTL_AMT
      , TRK_40FT_FUEL_SCG_AGMT_WGT
      , TRK_40FT_FUEL_AGMT_WY_TP_CD
      , MTY_TRSP_20FT_COST_AMT
      , MTY_TRSP_40FT_COST_AMT
      , DMST_20FT_COST_AMT
      , DMST_40FT_COST_AMT
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', TML_20FT_COST_SRC_CD) TML_20FT_COST_SRC_NM
      , TML_20FT_COST_AMT
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', TML_40FT_COST_SRC_CD) TML_40FT_COST_SRC_NM
      , TML_40FT_COST_AMT
      , N1ST_VNDR_SEQ
      , (SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR V WHERE V.VNDR_SEQ = N1ST_VNDR_SEQ) N1ST_VNDR_NM
      , N1ST_INLND_ROUT_CMB_FLG
      , N2ND_VNDR_SEQ
      , (SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR V WHERE V.VNDR_SEQ = N2ND_VNDR_SEQ) N2ND_VNDR_NM
      , N2ND_INLND_ROUT_CMB_FLG
      , N3RD_VNDR_SEQ
      , (SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR V WHERE V.VNDR_SEQ = N3RD_VNDR_SEQ) N3RD_VNDR_NM
      , DECODE(
                GREATEST(NVL(N1ST_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(N1ST_VNDR_40FT_AGMT_OLD_FLG,0), NVL(N2ND_VNDR_20FT_AGMT_OLD_FLG,0), NVL(N2ND_VNDR_40FT_AGMT_OLD_FLG,0), NVL(N3RD_VNDR_20FT_AGMT_OLD_FLG,0), NVL(N3RD_VNDR_40FT_AGMT_OLD_FLG,0)),'0',NULL,
                GREATEST(NVL(N1ST_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(N1ST_VNDR_40FT_AGMT_OLD_FLG,0), NVL(N2ND_VNDR_20FT_AGMT_OLD_FLG,0), NVL(N2ND_VNDR_40FT_AGMT_OLD_FLG,0), NVL(N3RD_VNDR_20FT_AGMT_OLD_FLG,0), NVL(N3RD_VNDR_40FT_AGMT_OLD_FLG,0))
        ) AS AGMT_OLD_FLG
      , A.COST_TRF_NO
      , COST_TRF_ROUT_SEQ
      , COST_ROUT_GRP_NO
      , RANK() OVER (PARTITION BY COST_ROUT_GRP_NO ORDER BY 
#if (${in_sort_by} == 'TT4')
                                                            INLND_40FT_TTL_AMT 
#end
#if (${in_sort_by} == 'TT2')
                                                            INLND_20FT_TTL_AMT 
#end
#if (${in_sort_by} == 'RB4')
                                                            DECODE(RAIL_40FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, RAIL_40FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'RF4')
                                                            DECODE(RAIL_40FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, RAIL_40FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'RB2')
                                                            DECODE(RAIL_20FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, RAIL_20FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'RF2')
                                                            DECODE(RAIL_20FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, RAIL_20FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'TB4')
                                                            DECODE(TRK_40FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, TRK_40FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'TF4')
                                                            DECODE(TRK_40FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, TRK_40FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'TB2')
                                                            DECODE(TRK_20FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, TRK_20FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'TF2')
                                                            DECODE(TRK_20FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, TRK_20FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'MT4')
                                                            MTY_TRSP_40FT_COST_AMT 
#end
#if (${in_sort_by} == 'MT2')
                                                            MTY_TRSP_20FT_COST_AMT 
#end
#if (${in_sort_by} == 'DT4')
                                                            DMST_40FT_COST_AMT 
#end
#if (${in_sort_by} == 'DT2')
                                                            DMST_20FT_COST_AMT 
#end
#if (${in_sort_by} == 'TM4')
                                                            DECODE(TML_40FT_COST_SRC_CD, 'A', 'A', 'X') ASC, TML_40FT_COST_AMT 
#end
#if (${in_sort_by} == 'TM2')
                                                            DECODE(TML_20FT_COST_SRC_CD, 'A', 'A', 'X') ASC, TML_20FT_COST_AMT 
#end
#if (${in_asc_desc} == 'asc')
                                                            ASC
#end
#if (${in_asc_desc} == 'desc')
                                                            DESC
#end
                                                          , COST_TRF_ROUT_SEQ
        ) COST_ROUT_GRP_NO_RNK
      , B.RCV_DE_TERM_CD
      , '' UPD_USR_ID
      , (
          SELECT  COUNT(1)
          FROM    AOC_USA_INLND_RF_TRF_DTL
          WHERE   1 = 1
          AND     COST_TRF_NO = B.COST_TRF_NO
          AND     COST_TRF_ROUT_SEQ = B.COST_TRF_ROUT_SEQ
          AND     DELT_FLG = 'N'
        ) RF_CNT
      , B.TOLL_FEE_AMT AS TOLL_FEE_AMT_20
      , B.TOLL_FEE_AMT AS TOLL_FEE_AMT_40
FROM    AOC_USA_INLND_TRF_HDR A
      , AOC_USA_INLND_TRF_DTL B      
WHERE   A.COST_TRF_NO = B.COST_TRF_NO
AND     B.COST_TRF_NO = @[in_cost_trf_no]
AND     B.DELT_FLG    = 'N'

#if (${s_cost_rout_grp_no} != '')
AND     B.COST_ROUT_GRP_NO IN (${s_cost_rout_grp_no})
#end

#if (${svc_mod_cd} != '')
AND     ( SUBSTR(B.PORT_NOD_CD, 1,5), B.TRSP_CRR_MOD_CD ) IN (
                                                               SELECT  PORT_CD
                                                                     , TRSP_CRR_MOD_CD
                                                               FROM    AOC_USA_IPI_PORT
                                                               WHERE   USA_COST_TRF_SVC_MOD_CD = @[svc_mod_cd]
                                                             )
#end

ORDER BY COST_ROUT_GRP_NO
#if (${in_sort_by} == 'TT4')
      , INLND_40FT_TTL_AMT 
#end
#if (${in_sort_by} == 'TT2')
      , INLND_20FT_TTL_AMT 
#end
#if (${in_sort_by} == 'RB4')
      , DECODE(RAIL_40FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, RAIL_40FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'RF4')
      , DECODE(RAIL_40FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, RAIL_40FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'RB2')
      , DECODE(RAIL_20FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, RAIL_20FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'RF2')
      , DECODE(RAIL_20FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, RAIL_20FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'TB4')
      , DECODE(TRK_40FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, TRK_40FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'TF4')
      , DECODE(TRK_40FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, TRK_40FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'TB2')
      , DECODE(TRK_20FT_BZC_COST_SRC_CD, 'A', 'A', 'X') ASC, TRK_20FT_BZC_COST_AMT 
#end
#if (${in_sort_by} == 'TF2')
      , DECODE(TRK_20FT_FUEL_SCG_SRC_CD, 'A', 'A', 'X') ASC, TRK_20FT_FUEL_SCG_AMT 
#end
#if (${in_sort_by} == 'MT4')
      , MTY_TRSP_40FT_COST_AMT 
#end
#if (${in_sort_by} == 'MT2')
      , MTY_TRSP_20FT_COST_AMT 
#end
#if (${in_sort_by} == 'DT4')
      , DMST_40FT_COST_AMT 
#end
#if (${in_sort_by} == 'DT2')
      , DMST_20FT_COST_AMT 
#end
#if (${in_sort_by} == 'TM4')
      , DECODE(TML_40FT_COST_SRC_CD, 'A', 'A', 'X') ASC
      , TML_40FT_COST_AMT 
#end
#if (${in_sort_by} == 'TM2')
      , DECODE(TML_20FT_COST_SRC_CD, 'A', 'A', 'X') ASC
      , TML_20FT_COST_AMT 
#end
#if (${in_asc_desc} == 'asc')
        ASC
#end
#if (${in_asc_desc} == 'desc')
        DESC
#end
      , COST_TRF_ROUT_SEQ			]]></sql>
			<params>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
				<param name="svc_mod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
