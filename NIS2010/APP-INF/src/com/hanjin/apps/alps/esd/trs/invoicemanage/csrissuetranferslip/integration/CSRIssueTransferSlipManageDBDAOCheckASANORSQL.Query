<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOCheckASANORSQL">
			<desc><![CDATA[ASANO를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT 		SO_IF_CD                       											
FROM 		MDM_ORGANIZATION               											
WHERE 		OFC_CD 			= (            											
								SELECT 		AP_OFC_CD        	/* AR_OFC_CD */		
