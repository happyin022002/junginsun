   trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
   AND trsp_so_seq = @[trsp_so_seq]
   AND trsp_so_cmb_seq = @[trsp_so_cmb_seq]
   AND delt_flg = 'N'
   AND hjl_no IS NULL			]]></sql>
