<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LogisticsRevenueDBDAOGetIndiaTaxInfoRSQL">
			<desc><![CDATA[India Tax 정보]]></desc>
			<sql><![CDATA[
SELECT IDA_TAX_SEQ
      ,EXPN_TAX
      ,EDU_TAX
      ,HIGH_EDU_TAX
      ,LOCL_TAX_RT
      ,N2ND_LOCL_TAX_RT
  FROM TPB_IDA_TAX
 WHERE 1 = 1
   AND IDA_TAX_SEQ = ( SELECT MAX(IDA_TAX_SEQ) FROM TPB_IDA_TAX WHERE EFF_DT < SYSDATE AND NVL(DELT_FLG,'N') = 'N')
   AND NVL(DELT_FLG,'N') = 'N'
   AND Exists ( SELECT 'X' FROM  MDM_ORGANIZATION B, MDM_LOCATION C
                WHERE B.OFC_CD = @[ofc_cd]
                AND B.LOC_CD = C.LOC_CD
                AND C.CNT_CD = 'IN' )			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
