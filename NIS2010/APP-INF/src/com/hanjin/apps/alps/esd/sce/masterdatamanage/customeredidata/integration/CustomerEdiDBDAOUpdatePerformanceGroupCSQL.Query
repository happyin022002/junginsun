<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerEdiDBDAOUpdatePerformanceGroupCSQL">
			<desc><![CDATA[UpdatePerformanceGroup]]></desc>
			<sql><![CDATA[
MERGE INTO EDI_USR_CUST C
USING ( SELECT @[user_id] CRE_USR_ID
            ,  @[edi_grp_id] EDI_GRP_CD
            ,  @[tp_id] CUST_TRD_PRNR_ID
            ,  @[edi_grp_desc] EDI_GRP_DESC
            ,  @[edi_cgo_rmk] EDI_CGO_RMK
            ,  @[user_id] UPD_USR_ID
        FROM DUAL) S
ON (C.CRE_USR_ID = S.CRE_USR_ID
    AND C.EDI_GRP_CD = S.EDI_GRP_CD
    AND C.EDI_STS_SEQ = 2
    )
WHEN MATCHED THEN
UPDATE
SET C.EDI_CGO_RMK = S.EDI_CGO_RMK
 ,  C.UPD_DT = SYSDATE
 ,  C.UPD_USR_ID = S.UPD_USR_ID
WHEN NOT MATCHED THEN   
INSERT (C.CRE_USR_ID
        ,C.EDI_GRP_CD
        ,C.EDI_STS_SEQ
        ,C.CUST_TRD_PRNR_ID
        ,C.EDI_GRP_DESC
        ,C.EDI_CGO_RMK
        ,C.CRE_DT
        ,C.UPD_USR_ID
        ,C.UPD_DT
        )
 VALUES (S.CRE_USR_ID
        ,S.EDI_GRP_CD
        , 2
        ,S.CUST_TRD_PRNR_ID
        ,S.EDI_GRP_DESC
        ,S.EDI_CGO_RMK
        ,SYSDATE
        ,S.UPD_USR_ID
        ,SYSDATE
        )			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="edi_grp_id" type="12" value="" out="N"/>
				<param name="tp_id" type="12" value="" out="N"/>
				<param name="edi_grp_desc" type="12" value="" out="N"/>
				<param name="edi_cgo_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
