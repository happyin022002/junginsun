<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOModifyTrsRailInvCntRSQL">
			<desc><![CDATA[Vender의 Invoice 갯수를 Count]]></desc>
			<sql><![CDATA[
SELECT
    COUNT(*) CNT
FROM TRS_TRSP_RAIL_INV_DTL
WHERE INV_NO 				= @[invNo]
AND   INV_VNDR_SEQ 			= @[invVndrSeq]			]]></sql>
			<params>
				<param name="invNo" type="12" value="" out="N"/>
				<param name="invVndrSeq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
