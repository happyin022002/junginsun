<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdi10514701AwkwardCgoRSQL">
			<desc><![CDATA[searchEdi_105147_01_AWKWARD_CGO]]></desc>
			<sql><![CDATA[
#if (${trsp_bnd_cd} == 'O' && ${trsp_cst_dtl_mod_cd} == 'DR') 
SELECT awk.ovr_fwrd_len awk_for_lgt
	  ,awk.ovr_bkwd_len awk_bak_lgt
	  ,awk.ovr_hgt awk_hgt_lgt
	  ,awk.ovr_lf_len awk_rgt_lgt
	  ,awk.ovr_rt_len  awk_lft_lgt
	  ,awk.ttl_dim_len awk_ttl_lgt
	  ,awk.ttl_dim_wdt awk_ttl_wdt
	  ,awk.ttl_dim_hgt awk_ttl_hgt
	  ,awk.grs_wgt awk_gross_wgt
	  ,awk.net_wgt awk_net_wgt
	  ,awk.pck_qty awk_pack_qty
	  ,awk.pck_tp_cd awk_pack_cd
	  ,TRIM(SUBSTR( REPLACE( REPLACE(REPLACE(REPLACE(pck.pck_nm ,CHR(13)||CHR(10),' '), '\"','*'),CHR(9),' '), CHR(10),' ') ,1,350)) awk_pack_desc 
  FROM bkg_awk_cgo awk
	  ,mdm_pck_tp pck
	  ,bkg_eur_tro th
	  ,trs_trsp_svc_ord so
 WHERE so.delt_flg <> 'Y'					
   AND so.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
   AND so.trsp_so_seq = @[trsp_so_seq]
   AND so.hjl_no IS NULL
   AND so.bkg_no = awk.bkg_no               
   AND th.awk_cgo_seq = awk.awk_cgo_seq    
   AND SUBSTR(so.COST_ACT_GRP_CD,1,2) = 'OD'    
   AND awk.pck_tp_cd = pck.pck_cd (+)           
   AND so.bkg_no  = th.bkg_no                
   AND so.tro_seq = th.tro_seq
   AND so.trsp_bnd_cd = th.io_bnd_cd
#else
SELECT awk.ovr_fwrd_len awk_for_lgt
	  ,awk.ovr_bkwd_len awk_bak_lgt
	  ,awk.ovr_hgt awk_hgt_lgt
	  ,awk.ovr_lf_len awk_rgt_lgt
	  ,awk.ovr_rt_len  awk_lft_lgt
	  ,awk.ttl_dim_len awk_ttl_lgt
	  ,awk.ttl_dim_wdt awk_ttl_wdt
	  ,awk.ttl_dim_hgt awk_ttl_hgt
	  ,awk.grs_wgt awk_gross_wgt
	  ,awk.net_wgt awk_net_wgt
	  ,awk.pck_qty awk_pack_qty
	  ,awk.pck_tp_cd awk_pack_cd
	  ,TRIM(SUBSTR( REPLACE( REPLACE(REPLACE(REPLACE(pck.pck_nm ,CHR(13)||CHR(10),' '), '\"','*'),CHR(9),' '), CHR(10),' ') ,1,350)) awk_pack_desc 
  FROM bkg_awk_cgo awk
	  ,mdm_pck_tp pck
	  ,trs_trsp_svc_ord so
 WHERE so.delt_flg <> 'Y'					
   AND so.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
   AND so.trsp_so_seq = @[trsp_so_seq]
   AND so.hjl_no IS NULL
   AND so.bkg_no = awk.bkg_no
   AND so.eq_no = awk.cntr_no
   AND awk.pck_tp_cd = pck.pck_cd (+)
   AND SUBSTR(so.COST_ACT_GRP_CD,1,2) <> 'OD'

#end			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
