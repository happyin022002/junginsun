<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchVerifyBkgNORSQL">
			<desc><![CDATA[Booking No 가 존재하는지 확인한다]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN CNT=0 THEN '0' 
            ELSE (SELECT BL_NO 
                  FROM   BKG_BOOKING B 
                  WHERE  BKG_NO = @[bkg_no]) 
       END AS BL_NO
FROM  (SELECT COUNT(BL_NO) CNT
       FROM   BKG_BOOKING B 
       WHERE  BKG_NO = @[bkg_no]
      )

			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
