<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOComboContactPointRSQL">
			<desc><![CDATA[Contact Point 를 콤보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VNDR_CNTC_PNT_SEQ,VNDR_CNTC_PNT_NM 
     FROM EAS_EXPN_AUD_CS_CNTC_PNT
    WHERE VNDR_SEQ = @[vndr_seq]
      AND NVL(DELT_FLG,'N') ='N'

			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
