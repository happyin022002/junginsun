				<param name="skd_rcv_tp_cd" type="12" value="" out="N"/>
				<param name="in_act_rcv_dt" type="12" value="" out="N"/>
				<param name="in_act_rcv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
