<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDetailHisSkdRcvTpCdUSQL">
			<desc><![CDATA[ModifyCopDetailHisSkdRcvTpCd]]></desc>
			<sql><![CDATA[
       UPDATE SCE_COP_SKD_HIS 
       SET    SKD_RCV_TP_CD = @[skd_rcv_tp_cd] 
             ,UPD_USR_ID    = 'VPS_EST' 
             ,UPD_DT        = SYSDATE 
       WHERE  VPS_RCV_DT    = @[in_act_rcv_dt] 
       AND    VPS_RCV_NO    = @[in_act_rcv_no]			]]></sql>
