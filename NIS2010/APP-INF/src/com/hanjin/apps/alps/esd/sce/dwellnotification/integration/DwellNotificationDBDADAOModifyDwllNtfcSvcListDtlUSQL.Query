<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDADAOModifyDwllNtfcSvcListDtlUSQL">
			<desc><![CDATA[Email List를 삭제 flag를 업데이트를한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_DWLL_CUST_SVC_LIST
SET DELT_FLG    = DECODE(@[ibflag], 'D', 'Y', 'N')
  , DELT_USR_ID = DECODE(@[ibflag], 'D', @[usr_id], NULL)
  , SUBSC_RMK   = @[subsc_rmk]
  , DELT_DT     = DECODE(@[ibflag], 'D', SYSDATE, NULL)
  , UPD_USR_ID  = @[usr_id]
  , UPD_DT      = SYSDATE
WHERE 1=1
  AND DWLL_CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
  AND DWLL_CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))
  AND NTFC_SEQ  = @[ntfc_seq]			]]></sql>
			<params>
				<param name="ibflag" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="subsc_rmk" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="ntfc_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
