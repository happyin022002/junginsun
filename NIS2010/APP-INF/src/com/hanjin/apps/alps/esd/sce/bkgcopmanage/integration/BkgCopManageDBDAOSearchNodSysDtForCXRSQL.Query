<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchNodSysDtForCXRSQL">
			<desc><![CDATA[CX status 전송을 위하여 Close 될 COP 의 마지막 node 와 현재 시간을 가져온다.]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC (SCE_COP_DTL XPKSCE_COP_DTL) */ 
	NOD_CD,  
    TO_CHAR( GLOBALDATE_PKG.TIME_CONV_FNC('KRSEL', sysdate, substr(NOD_CD,1,5)), 'YYYYMMDDHH24MISS' ) AS ACT_DT 
    FROM SCE_COP_DTL 
    WHERE COP_NO = ( 
        SELECT COP_NO 
        FROM SCE_COP_HDR 
        WHERE BKG_NO = @[bkg_no]
          AND CNTR_NO = @[cntr_no]
          AND COP_STS_CD <> 'X') 
      AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
