<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ControlOfficeExceptionCaseManageDBDAOVerifyControlOfficeCDRSQL">
			<desc><![CDATA[Office 정보 조회]]></desc>
			<sql><![CDATA[
SELECT OFC_CD
      ,DECODE(OFC_TP_CD, 'HO', '', 'HG', '', 'HQ', '', 'AQ', '',
                         'BB', '', 'SC', '', 'BS', '', 'BO', '', 'BA', '', 'NO' ) OFC_TP_CD
  FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[ctrl_ofc_cd]
   AND NVL(DELT_FLG,'N') = 'N' 			]]></sql>
			<params>
				<param name="ctrl_ofc_cd" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
