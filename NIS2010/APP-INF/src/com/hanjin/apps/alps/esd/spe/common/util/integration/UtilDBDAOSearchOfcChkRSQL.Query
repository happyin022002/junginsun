<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UtilDBDAOSearchOfcChkRSQL">
			<desc><![CDATA[Office 코드가 존재하는 값인지 조회한다]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS ISFLAG
  FROM MDM_ORGANIZATION
 WHERE DELT_FLG = 'N'
#if(${f_ctrt_ofc_cd} != '')
   AND OFC_CD = @[f_ctrt_ofc_cd]
#else
   AND OFC_CD = @[s_ctrt_ofc_cd]
#end

#if(${s_reg_gp} != '')
   AND PRNT_OFC_CD = @[s_reg_gp]
#end			]]></sql>
			<params>
				<param name="f_ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="s_ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="s_reg_gp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
