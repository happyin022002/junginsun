<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RehandExpmanageDBDAOOpfTdrAtchFileVORSQL">
			<desc><![CDATA[OpfTdrAtchFileVO]]></desc>
			<sql><![CDATA[
SELECT  VSL_CD
        ,SKD_VOY_NO
        ,SKD_DIR_CD
        ,VPS_PORT_CD
        ,CLPT_IND_SEQ
        ,CNTR_HNDL_KND_CD
        ,CNTR_NO
        ,ATCH_FILE_SEQ
        ,FILE_NM
        ,FILE_SAV_ID
        ,UPD_USR_ID
        ,TO_CHAR(UPD_DT,'YYYY-MM-DD HH24:MI:SS' ) AS UPD_DT
        ,CRE_USR_ID
        ,TO_CHAR(CRE_DT,'YYYY-MM-DD HH24:MI:SS' ) AS CRE_DT
        , '0' AS FILE_DOWNLOAD 
FROM    OPF_TDR_ATCH_FILE
WHERE   VSL_CD              = @[vsl_cd]
AND     SKD_VOY_NO          = @[skd_voy_no]
AND     SKD_DIR_CD          = @[skd_dir_cd]
AND     VPS_PORT_CD         = @[vps_port_cd]
AND     CLPT_IND_SEQ        = @[clpt_ind_seq]
AND     CNTR_HNDL_KND_CD    = @[cntr_hndl_knd_cd]
AND     CNTR_NO             = @[cntr_no]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="cntr_hndl_knd_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
