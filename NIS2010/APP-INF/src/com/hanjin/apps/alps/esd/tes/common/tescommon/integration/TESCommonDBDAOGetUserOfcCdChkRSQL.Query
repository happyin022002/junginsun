<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOGetUserOfcCdChkRSQL">
			<desc><![CDATA[GetUserOfcCdChk]]></desc>
			<sql><![CDATA[
SELECT (CASE WHEN MO.OFC_KND_CD  = '1' THEN 'Y' 
            WHEN CU.OFC_CD = @[ofc_cd] THEN 'Y'
       ELSE 'N' END) CHK_OFC_CD
FROM COM_USER CU, MDM_ORGANIZATION MO
WHERE CU.USR_ID = @[usr_id]
AND CU.USE_FLG = 'Y'
AND CU.OFC_CD = MO.OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
