<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerEdiDBDAOUpdatePerformanceCntRSQL">
			<desc><![CDATA[UpdatePerformanceCnt]]></desc>
			<sql><![CDATA[
select count(*) cnt
from edi_usr_cust
where cre_usr_id = @[user_id]
and edi_sts_seq = 2
and edi_grp_cd = @[edi_grp_id]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="edi_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
