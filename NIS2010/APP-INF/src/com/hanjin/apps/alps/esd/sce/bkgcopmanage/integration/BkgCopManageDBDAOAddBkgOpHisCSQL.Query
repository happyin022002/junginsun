<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOAddBkgOpHisCSQL">
			<desc><![CDATA[SCE_BKG_OP_HIS 에 booking 에서 operation call 한 정보를 남긴다.]]></desc>
			<sql><![CDATA[
call SCE_BKG_OP_HIS_PRC (
@[bkg_rcv_dt],
@[bkg_rcv_no],
@[bkg_no],
@[cntr_no],
@[bkg_evnt_tp_cd],
@[bkg_evnt_rmk],
@[pctl_no],
@[tro_seq],
@[tro_sub_seq],
@[io_bnd_cd],
@[conti_cd],
@[prt_flg]
)			]]></sql>
			<params>
				<param name="bkg_rcv_dt" type="12" value="" out="N"/>
				<param name="bkg_rcv_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_evnt_tp_cd" type="12" value="" out="N"/>
				<param name="bkg_evnt_rmk" type="12" value="" out="N"/>
				<param name="pctl_no" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
				<param name="tro_sub_seq" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="prt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
