<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOAddAproHisCSQL">
			<desc><![CDATA[History 테이블에 이력을 등록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_EXPN_AUD_CS_APRO_STEP(EAC_NO
                                    , APRO_STEP_SEQ
                                    , APRO_OFC_CD
                                    , APRO_USR_ID
                                    , EAC_STS_CD
                                    , LOCL_CRE_DT
                                    , EAC_APRO_RSN
                                    , CRE_USR_ID
                                    , CRE_DT
                                    , UPD_USR_ID
                                    , UPD_DT
                                    )VALUES(
                                     @[eac_no]       
                                    , NVL((SELECT MAX(APRO_STEP_SEQ)
                                           FROM EAS_EXPN_AUD_CS_APRO_STEP
                                           WHERE EAC_NO = @[eac_no]), 0) + 1
                                    ,@[ofc_cd]  
                                    ,@[usr_id]  
                                    ,@[eac_sts_cd]   
                                    , globaldate_pkg.time_local_ofc_fnc(@[ofc_cd]) 
                                    ,@[eac_apro_rsn] 
                                    ,@[usr_id]   
                                    ,SYSDATE
                                    ,@[usr_id]   
                                    ,SYSDATE
                                    )

			]]></sql>
			<params>
				<param name="eac_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="eac_sts_cd" type="12" value="" out="N"/>
				<param name="eac_apro_rsn" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
