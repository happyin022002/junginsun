SET    DELT_FLG     = 'Y'
     , UPD_USR_ID   = @[audr_usr_id]
     , UPD_DT       = SYSDATE
WHERE  EAC_NO       = @[eac_no]
AND    EAC_DTL_SEQ  = @[eac_dtl_seq]


			]]></sql>
			<params>
				<param name="audr_usr_id" type="12" value="" out="N"/>
				<param name="eac_no" type="12" value="" out="N"/>
				<param name="eac_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
