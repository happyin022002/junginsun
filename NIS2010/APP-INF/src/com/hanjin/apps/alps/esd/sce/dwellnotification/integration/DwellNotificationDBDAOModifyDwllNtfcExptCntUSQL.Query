<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOModifyDwllNtfcExptCntUSQL">
			<desc><![CDATA[SCE_DWLL_CNTR_EXPT 삭제를 한다.]]></desc>
			<sql><![CDATA[
DELETE FROM  SCE_DWLL_CNTR_EXPT

WHERE CNTR_NO         =  @[cntr_no] 
      AND CNMV_YR     =  @[cnmv_yr]          
      AND CNMV_ID_NO  =  @[cnmv_id_no]         
      AND CNMV_CYC_NO =  @[cnmv_cyc_no]        
      AND MST_BKG_NO  =  @[mst_bkg_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
				<param name="cnmv_cyc_no" type="12" value="" out="N"/>
				<param name="mst_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
