<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOSearchIndiaTaxInfoRSQL">
			<desc><![CDATA[searchIndiaTaxInfo]]></desc>
			<sql><![CDATA[
SELECT IDA_TAX_SEQ
	  ,OFC_CD
	  ,TO_CHAR(TPB_GET_LCL_DATE_FNC(EFF_DT,@[user_ofc_cd]),'YYYY-MM-DD HH24:MI') EFF_DT
	  ,EXPN_TAX
	  ,EDU_TAX
	  ,HIGH_EDU_TAX
	  ,TAX_RGST_NO
	  ,SVC_CATE_RMK
      ,PMNT_ACCT_NO
	  ,CRE_USR_ID
	  ,TO_CHAR(TPB_GET_LCL_DATE_FNC(CRE_DT,@[user_ofc_cd]),'YYYY-MM-DD HH24:MI') CRE_DT
	  ,UPD_USR_ID
	  ,TO_CHAR(TPB_GET_LCL_DATE_FNC(UPD_DT,@[user_ofc_cd]),'YYYY-MM-DD HH24:MI') UPD_DT
	  ,(SELECT COUNT(0) FROM TPB_INV_RVIS WHERE IDA_TAX_SEQ=A.IDA_TAX_SEQ) EDITABLE 
      ,NVL(LOCL_TAX_RT, 0) LOCL_TAX_RT
      ,NVL(N2ND_LOCL_TAX_RT,0) N2ND_LOCL_TAX_RT
  FROM TPB_IDA_TAX A
 WHERE 1=1
#if (${s_ofc_cd} != '') 
   AND OFC_CD = @[s_ofc_cd]
#end
#if (${s_eff_dt} != '') 
   AND EFF_DT = TO_DATE(@[s_eff_dt]) + (3.5/24)
#end
   AND (DELT_FLG != 'Y' OR DELT_FLG IS NULL)
 ORDER BY OFC_CD, EFF_DT DESC			]]></sql>
			<params>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="s_ofc_cd" type="12" value="" out="N"/>
				<param name="s_eff_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
