<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WORejectManageDBDAOmultiRejectWOListInvRSQL">
			<desc><![CDATA[multiRejectWOList]]></desc>
			<sql><![CDATA[
SELECT
	COUNT(TRSP_SO_OFC_CTY_CD) TRSP_SO_OFC_CTY_CD																									
FROM
		TRS_TRSP_SVC_ORD
WHERE 	1=1
AND		TRSP_SO_OFC_CTY_CD  =  @[TRSP_SO_OFC_CTY_CD]
AND 	TRSP_SO_SEQ			=  @[TRSP_SO_SEQ]
AND     TRSP_INV_ACT_STS_CD IS NULL			]]></sql>
			<params>
				<param name="TRSP_SO_OFC_CTY_CD" type="12" value="" out="N"/>
				<param name="TRSP_SO_SEQ" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
