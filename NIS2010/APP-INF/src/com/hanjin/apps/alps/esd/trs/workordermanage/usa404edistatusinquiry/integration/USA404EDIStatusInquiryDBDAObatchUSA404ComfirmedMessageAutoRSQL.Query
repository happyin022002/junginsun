<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USA404EDIStatusInquiryDBDAObatchUSA404ComfirmedMessageAutoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT 
	A.TRSP_SO_OFC_CTY_CD, A.TRSP_SO_SEQ, A.EQ_NO, 
	A.EQ_TPSZ_CD, A.PROV_VNDR_SEQ, A.PROV_USR_ID, 
	A.PROV_FAX_NO, A.PROV_EML, A.SHPR_CUST_NM, 
	A.SHPR_FAX_NO, TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(C.OFC_CD), 'YYYYMMDD HH24:MI:SS') SND_DT, 
	C.OFC_CD CRE_OFC_CD, A.CRE_USR_ID, A.PROV_CFM_MZD_CD 
FROM TRS_TRSP_RAIL_BIL_ORD A, TRS_TRSP_CFM_MSG_HIS B, MDM_VENDOR C 
WHERE A.TRSP_SO_OFC_CTY_CD = @[trspSoOfcCtyCd] AND A.TRSP_SO_SEQ = @[trspSoSeq] 
AND A.TRSP_RAIL_BIL_TP_CD  = 'W' 
AND A.TRSP_SO_OFC_CTY_CD   = B.TRSP_SO_OFC_CTY_CD(+) 
AND A.TRSP_SO_SEQ          = B.TRSP_SO_SEQ(+) 
AND A.PROV_VNDR_SEQ        = C.VNDR_SEQ 
AND A.DELT_FLG             = 'N' 
GROUP BY A.TRSP_SO_OFC_CTY_CD, A.TRSP_SO_SEQ, A.EQ_NO, A.EQ_TPSZ_CD, A.PROV_VNDR_SEQ, A.PROV_USR_ID, A.PROV_FAX_NO, 
A.PROV_EML, A.SHPR_CUST_NM, A.SHPR_FAX_NO, C.OFC_CD, A.CRE_USR_ID, A.PROV_CFM_MZD_CD			]]></sql>
			<params>
				<param name="trspSoOfcCtyCd" type="12" value="" out="N"/>
				<param name="trspSoSeq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
