<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandMultiCreationDBDAOInsertRouteMasterCSQL">
			<desc><![CDATA[InsertRouteMaster]]></desc>
			<sql><![CDATA[
INSERT INTO prd_inlnd_rout_mst
	(rout_org_nod_cd, rout_dest_nod_cd, rout_seq, prio_seq, inlnd_rout_inv_bil_patt_cd
	, rout_pln_cd,  wrs_full_cmdt_cd, wrs_mty_cmdt_cd, inlnd_rout_bkg_flg, pctl_io_bnd_cd
	, inlnd_rout_tmp_flg, inlnd_rout_rmk, hub_nod_cd, cre_dt, cre_usr_id, upd_dt, upd_usr_id, delt_flg
)
VALUES(@[route_org_node], @[route_dest_node], @[route_seq], @[prio_seq], @[inlnd_rout_inv_bil_patt_cd]
	, @[rout_pln_cd], @[wrs_full_cmdt], @[wrs_mty_cmdt_cd], NVL(@[inlnd_rout_bkg_flg],'N'), @[pctl_io_bnd_cd]
	, NVL(@[inlnd_rout_tmp_flg],'N'), @[inlnd_rout_rmk], @[hub_loc_cd], sysdate, @[user_id], sysdate, @[user_id], 'N'
)			]]></sql>
			<params>
				<param name="route_org_node" type="12" value="" out="N"/>
				<param name="route_dest_node" type="12" value="" out="N"/>
				<param name="route_seq" type="12" value="" out="N"/>
				<param name="prio_seq" type="12" value="" out="N"/>
				<param name="inlnd_rout_inv_bil_patt_cd" type="12" value="" out="N"/>
				<param name="rout_pln_cd" type="12" value="" out="N"/>
				<param name="wrs_full_cmdt" type="12" value="" out="N"/>
				<param name="wrs_mty_cmdt_cd" type="12" value="" out="N"/>
				<param name="inlnd_rout_bkg_flg" type="12" value="" out="N"/>
				<param name="pctl_io_bnd_cd" type="12" value="" out="N"/>
				<param name="inlnd_rout_tmp_flg" type="12" value="" out="N"/>
				<param name="inlnd_rout_rmk" type="12" value="" out="N"/>
				<param name="hub_loc_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
