<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInlandCostManageDBDAOMultiInlandCostUSQL">
			<desc><![CDATA[multiInlandCost]]></desc>
			<sql><![CDATA[
UPDATE  AOC_USA_INLND_TRF_DTL
SET     INLND_20FT_TTL_AMT              = INLND_20FT_ORG_TTL_AMT
                                        + NVL(@[rail_20ft_bzc_cost_adj_amt],0) + NVL(@[rail_20ft_fuel_scg_adj_amt],0) + NVL(@[trk_20ft_bzc_cost_adj_amt],0) + NVL(@[trk_20ft_fuel_scg_adj_amt],0) + NVL(@[mty_trsp_20ft_adj_cost_amt],0) + NVL(@[dmst_20ft_adj_cost_amt],0) + NVL(@[tml_20ft_adj_cost_amt],0)
      , INLND_40FT_TTL_AMT              = INLND_40FT_ORG_TTL_AMT
                                        + NVL(@[rail_40ft_bzc_cost_adj_amt],0) + NVL(@[rail_40ft_fuel_scg_adj_amt],0) + NVL(@[trk_40ft_bzc_cost_adj_amt],0) + NVL(@[trk_40ft_fuel_scg_adj_amt],0) + NVL(@[mty_trsp_40ft_adj_cost_amt],0) + NVL(@[dmst_40ft_adj_cost_amt],0) + NVL(@[tml_40ft_adj_cost_amt],0)
      , INLND_RAIL_20FT_TTL_AMT         = RAIL_20FT_BZC_COST_AMT + RAIL_20FT_FUEL_SCG_AMT
                                        + NVL(@[rail_20ft_bzc_cost_adj_amt],0) + NVL(@[rail_20ft_fuel_scg_adj_amt],0)
      , INLND_RAIL_40FT_TTL_AMT         = RAIL_40FT_BZC_COST_AMT + RAIL_40FT_FUEL_SCG_AMT
                                        + NVL(@[rail_40ft_bzc_cost_adj_amt],0) + NVL(@[rail_40ft_fuel_scg_adj_amt],0)
      , INLND_TRK_20FT_TTL_AMT          = TRK_20FT_BZC_COST_AMT + TRK_20FT_FUEL_SCG_AMT + TOLL_FEE_AMT + NVL(@[trk_20ft_bzc_cost_adj_amt],0) + NVL(@[trk_20ft_fuel_scg_adj_amt],0)
      , INLND_TRK_40FT_TTL_AMT          = TRK_40FT_BZC_COST_AMT + TRK_40FT_FUEL_SCG_AMT + TOLL_FEE_AMT + NVL(@[trk_40ft_bzc_cost_adj_amt],0) + NVL(@[trk_40ft_fuel_scg_adj_amt],0)
      , RAIL_20FT_BZC_COST_ADJ_AMT      = NVL(@[rail_20ft_bzc_cost_adj_amt],0)
      , RAIL_20FT_BZC_COST_TTL_AMT      = RAIL_20FT_BZC_COST_AMT + NVL(@[rail_20ft_bzc_cost_adj_amt],0)
      , RAIL_40FT_BZC_COST_ADJ_AMT      = NVL(@[rail_40ft_bzc_cost_adj_amt],0)
      , RAIL_40FT_BZC_COST_TTL_AMT      = RAIL_40FT_BZC_COST_AMT + NVL(@[rail_40ft_bzc_cost_adj_amt],0)
      , RAIL_20FT_FUEL_SCG_ADJ_AMT      = NVL(@[rail_20ft_fuel_scg_adj_amt],0)
      , RAIL_20FT_FUEL_SCG_TTL_AMT      = RAIL_20FT_FUEL_SCG_AMT + NVL(@[rail_20ft_fuel_scg_adj_amt],0)
      , RAIL_40FT_FUEL_SCG_ADJ_AMT      = NVL(@[rail_40ft_fuel_scg_adj_amt],0)
      , RAIL_40FT_FUEL_SCG_TTL_AMT      = RAIL_40FT_FUEL_SCG_AMT + NVL(@[rail_40ft_fuel_scg_adj_amt],0)
      , TRK_20FT_BZC_COST_ADJ_AMT       = NVL(@[trk_20ft_bzc_cost_adj_amt],0)
      , TRK_20FT_BZC_COST_TTL_AMT       = TRK_20FT_BZC_COST_AMT + NVL(@[trk_20ft_bzc_cost_adj_amt],0)
      , TRK_40FT_BZC_COST_ADJ_AMT       = NVL(@[trk_40ft_bzc_cost_adj_amt],0)
      , TRK_40FT_BZC_COST_TTL_AMT       = TRK_40FT_BZC_COST_AMT + NVL(@[trk_40ft_bzc_cost_adj_amt],0)
      , TRK_20FT_FUEL_SCG_ADJ_AMT       = NVL(@[trk_20ft_fuel_scg_adj_amt],0)
      , TRK_20FT_FUEL_SCG_TTL_AMT       = TRK_20FT_FUEL_SCG_AMT + NVL(@[trk_20ft_fuel_scg_adj_amt],0)
      , TRK_40FT_FUEL_SCG_ADJ_AMT       = NVL(@[trk_40ft_fuel_scg_adj_amt],0)
      , TRK_40FT_FUEL_SCG_TTL_AMT       = TRK_40FT_FUEL_SCG_AMT + NVL(@[trk_40ft_fuel_scg_adj_amt],0)
      , MTY_TRSP_20FT_ADJ_COST_AMT      = NVL(@[mty_trsp_20ft_adj_cost_amt],0)
      , MTY_TRSP_20FT_TTL_COST_AMT      = MTY_TRSP_20FT_COST_AMT + NVL(@[mty_trsp_20ft_adj_cost_amt],0)
      , MTY_TRSP_40FT_ADJ_COST_AMT      = NVL(@[mty_trsp_40ft_adj_cost_amt],0)
      , MTY_TRSP_40FT_TTL_COST_AMT      = MTY_TRSP_40FT_COST_AMT + NVL(@[mty_trsp_40ft_adj_cost_amt],0)
      , DMST_20FT_ADJ_COST_AMT          = NVL(@[dmst_20ft_adj_cost_amt],0)
      , DMST_20FT_TTL_COST_AMT          = DMST_20FT_COST_AMT + NVL(@[dmst_20ft_adj_cost_amt],0)
      , DMST_40FT_ADJ_COST_AMT          = NVL(@[dmst_40ft_adj_cost_amt],0)
      , DMST_40FT_TTL_COST_AMT          = DMST_40FT_COST_AMT + NVL(@[dmst_40ft_adj_cost_amt],0)
      , TML_20FT_ADJ_COST_AMT           = NVL(@[tml_20ft_adj_cost_amt],0)
      , TML_20FT_TTL_COST_AMT           = TML_20FT_COST_AMT + NVL(@[tml_20ft_adj_cost_amt],0)
      , TML_40FT_ADJ_COST_AMT           = NVL(@[tml_40ft_adj_cost_amt],0)
      , TML_40FT_TTL_COST_AMT           = TML_40FT_COST_AMT + NVL(@[tml_40ft_adj_cost_amt],0)
      , UPD_DT                          = SYSDATE
      , UPD_USR_ID                      = @[upd_usr_id]
WHERE   COST_TRF_NO                     = @[cost_trf_no]
AND     COST_TRF_ROUT_SEQ               = @[cost_trf_rout_seq]			]]></sql>
			<params>
				<param name="rail_20ft_bzc_cost_adj_amt" type="12" value="" out="N"/>
				<param name="rail_20ft_fuel_scg_adj_amt" type="12" value="" out="N"/>
				<param name="trk_20ft_bzc_cost_adj_amt" type="12" value="" out="N"/>
				<param name="trk_20ft_fuel_scg_adj_amt" type="12" value="" out="N"/>
				<param name="mty_trsp_20ft_adj_cost_amt" type="12" value="" out="N"/>
				<param name="dmst_20ft_adj_cost_amt" type="12" value="" out="N"/>
				<param name="tml_20ft_adj_cost_amt" type="12" value="" out="N"/>
				<param name="rail_40ft_bzc_cost_adj_amt" type="12" value="" out="N"/>
				<param name="rail_40ft_fuel_scg_adj_amt" type="12" value="" out="N"/>
				<param name="trk_40ft_bzc_cost_adj_amt" type="12" value="" out="N"/>
				<param name="trk_40ft_fuel_scg_adj_amt" type="12" value="" out="N"/>
				<param name="mty_trsp_40ft_adj_cost_amt" type="12" value="" out="N"/>
				<param name="dmst_40ft_adj_cost_amt" type="12" value="" out="N"/>
				<param name="tml_40ft_adj_cost_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_trf_no" type="12" value="" out="N"/>
				<param name="cost_trf_rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
