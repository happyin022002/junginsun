<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdi102297DrbgCntrRSQL">
			<desc><![CDATA[searchEdi_102297_DRBG_CNTR]]></desc>
			<sql><![CDATA[
#if (${trsp_bnd_cd} == 'O' && ${trsp_cst_dtl_mod_cd} == 'DR')

SELECT B.cntr_no,B.cntr_tpsz,B.cntr_sts,B.cntr_cmdt,B.cntr_wgt,B.cntr_pkg,B.cntr_spc,B.spc_rmk
FROM (
SELECT RPAD(so.eq_no, 11, ' ') cntr_no
	,MAX(RPAD(so.eq_tpsz_cd, 2, ' ')) cntr_tpsz
	,MAX('F') cntr_sts
	,MAX(RPAD(NVL(trs_filter_spc_char_fnc(cm.cntr_mf_gds_desc) ,' '), 100, ' ')) cntr_cmdt 
	,so.bkg_no bkg_no
	,MAX(RPAD(NVL(so.cntr_wgt,0), 12, ' ')) cntr_wgt
	,MAX(RPAD(NVL(bc.pck_qty,0), 7, ' ')) cntr_pkg
	,MAX(DECODE(spcl_cgo_cntr_tp_cd,'AK','A','DG','D','RF','R','RD','Y')) cntr_spc
	,MAX(DECODE(spcl_cgo_cntr_tp_cd
        ,'DG', '(Danger)/IMDG / '||(SELECT s2_dg.imdg_clss_cd||' /UN '||s2_dg.imdg_un_no||' / PSA '||s2_dg.psa_no
                                      FROM bkg_dg_cgo s2_dg
                                          ,bkg_tro_spcl_cgo_seq s2_tro
                                     WHERE 1=1
                                       AND s2_tro.bkg_no    = so.bkg_no
                                       AND s2_tro.tro_seq   = so.tro_seq
                                       AND s2_tro.io_bnd_cd = so.trsp_bnd_cd
                                       AND s2_dg.bkg_no     = so.bkg_no
                                       AND s2_dg.dcgo_seq   = s2_tro.tro_spcl_cgo_seq
                                       AND ROWNUM = 1
                                    )
		,'RF','(Reefer)/'||rf.cdo_temp||'/'||rf.vent_rto 
        ,'AK',DECODE(SUBSTR(so.eq_tpsz_cd, 1, 1), 'D', '','(Awkward) /  Length '||ROUND(ak.ttl_dim_len)||' / Width '||ROUND(ak.ttl_dim_wdt)||' / VOID '||ak.ovr_void_slt_qty) 
	)) spc_rmk
FROM bkg_container bc
	,bkg_awk_cgo ak
	,bkg_rf_cgo rf
	,bkg_cntr_mf_desc cm
	,trs_trsp_wrk_ord wo
	,trs_trsp_svc_ord so
WHERE so.delt_flg <> 'Y'
   	AND wo.trsp_wo_ofc_cty_cd = @[trsp_wo_ofc_cty_cd]
   	AND wo.trsp_wo_seq	= @[trsp_wo_seq]
	AND wo.trsp_wo_ofc_cty_cd = so.trsp_wo_ofc_cty_cd
	AND wo.trsp_wo_seq = so.trsp_wo_seq
	AND so.bkg_no = bc.bkg_no(+)
	AND so.eq_no = bc.cntr_no(+)
	AND so.bkg_no = ak.bkg_no(+)
	AND so.eq_no = ak.cntr_no(+)
	AND so.bkg_no = rf.bkg_no(+)
	AND so.eq_no = rf.cntr_no(+)
	AND so.bkg_no = cm.bkg_no(+)
	AND so.eq_no = cm.cntr_no(+)
	AND cm.cntr_mf_seq(+) = 1  
	AND so.hjl_no IS NULL
	AND wo.hjl_no IS NULL
GROUP BY so.eq_no,so.bkg_no
ORDER BY so.eq_no
) B

#else

SELECT C.cntr_no,C.cntr_tpsz,C.cntr_sts,C.cntr_cmdt,C.cntr_wgt,C.cntr_pkg,C.cntr_spc,C.spc_rmk
FROM (
SELECT RPAD(so.eq_no, 11, ' ') cntr_no
	,MAX(RPAD(so.eq_tpsz_cd, 2, ' ')) cntr_tpsz
	,MAX('F') cntr_sts
	,MAX(RPAD(NVL(trs_filter_spc_char_fnc(cm.cntr_mf_gds_desc) ,' '), 100, ' ')) cntr_cmdt 
	,so.bkg_no bkg_no
	,MAX(RPAD(NVL(so.cntr_wgt,0), 12, ' ')) cntr_wgt
	,MAX(RPAD(NVL(bc.pck_qty,0), 7, ' ')) cntr_pkg
	,MAX(DECODE(spcl_cgo_cntr_tp_cd,'AK','A','DG','D','RF','R','RD','Y')) cntr_spc
	,MAX(DECODE(spcl_cgo_cntr_tp_cd
		,'DG','(Danger)/IMDG / '||dg.imdg_clss_cd||' /UN '||dg.imdg_un_no||' / PSA '||psa_no 
		,'RF','(Reefer)/'||rf.cdo_temp||'/'||rf.vent_rto 
        ,'AK',DECODE(SUBSTR(so.eq_tpsz_cd, 1, 1), 'D', '','(Awkward) /  Length '||ROUND(ak.ttl_dim_len)||' / Width '||ROUND(ak.ttl_dim_wdt)||' / VOID '||ak.ovr_void_slt_qty) 
	)) spc_rmk
FROM bkg_container bc  
	,bkg_dg_cgo dg
	,bkg_awk_cgo ak
	,bkg_rf_cgo rf
	,bkg_cntr_mf_desc cm
    ,trs_spcl_cgo_pck_cd pck
	,trs_trsp_wrk_ord wo
	,trs_trsp_svc_ord so
WHERE so.delt_flg <> 'Y'
   	AND wo.trsp_wo_ofc_cty_cd = @[trsp_wo_ofc_cty_cd]
   	AND wo.trsp_wo_seq	= @[trsp_wo_seq]
	AND wo.trsp_wo_ofc_cty_cd = so.trsp_wo_ofc_cty_cd
	AND wo.trsp_wo_seq = so.trsp_wo_seq
	AND so.bkg_no = bc.bkg_no
	AND so.eq_no = bc.cntr_no
	AND so.bkg_no = dg.bkg_no(+)
	AND so.eq_no = dg.cntr_no(+)
	AND dg.out_imdg_pck_cd1 = pck.spcl_cgo_pck_cd(+)
	AND so.bkg_no = ak.bkg_no(+)
	AND so.eq_no = ak.cntr_no(+)
	AND so.bkg_no = rf.bkg_no(+)
	AND so.eq_no = rf.cntr_no(+)
	AND so.bkg_no = cm.bkg_no(+)
	AND so.eq_no = cm.cntr_no(+)
	AND cm.cntr_mf_seq(+) = 1  
	AND so.hjl_no IS NULL
	AND wo.hjl_no IS NULL
GROUP BY so.eq_no,so.bkg_no
ORDER BY so.eq_no
) C

#end			]]></sql>
			<params>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_wo_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
