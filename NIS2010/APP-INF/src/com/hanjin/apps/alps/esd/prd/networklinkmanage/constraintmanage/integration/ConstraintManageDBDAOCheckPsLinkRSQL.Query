<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOCheckPsLinkRSQL">
			<desc><![CDATA[CheckPsLink]]></desc>
			<sql><![CDATA[
SELECT  'X' 
FROM PRD_LNK_CNST_MGMT 
WHERE LNK_ORG_NOD_CD = @[lnk_org_nod_cd] 
AND LNK_DEST_NOD_CD = @[lnk_dest_nod_cd] 
AND TRSP_MOD_CD = @[trsp_mod_cd] 
AND LNK_CNST_ITM_CD = @[lnk_cnst_itm_cd] 
AND NVL(CMDT_CD,'N') = NVL(@[cmdt_cd],'N')
AND NVL(DELT_FLG,'N') <> 'Y'
AND NVL(VSL_SLAN_CD, ' ') = NVL(@[vsl_slan_cd], ' ')
AND NVL(VSL_CD, ' ')     = NVL(SUBSTR(@[vvd],1,4), ' ')
AND NVL(SKD_VOY_NO, ' ') = NVL(SUBSTR(@[vvd],5,4), ' ')
AND NVL(SKD_DIR_CD, ' ') = NVL(SUBSTR(@[vvd],9,1), ' ')
			]]></sql>
			<params>
				<param name="lnk_org_nod_cd" type="12" value="" out="N"/>
				<param name="lnk_dest_nod_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="lnk_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
