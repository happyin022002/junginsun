<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOModifySceCopDtlUSQL">
			<desc><![CDATA[for updating scecopdtl]]></desc>
			<sql><![CDATA[
UPDATE SCE_COP_DTL 
              SET    EDI_SND_TP_CD   = 'Y' 
                    ,EDI_ACT_SND_DT = decode(@[stnd_edi_sts_cd], 'PN', EDI_ACT_SND_DT, TO_DATE(substr(@[event_dt], 1, 14), 'YYYYMMDDHH24MISS'))
                    ,UPD_DT         = SYSDATE
              WHERE  COP_NO              = @[cop_no] 
              AND    cop_dtl_seq        = @[cop_dtl_seq]			]]></sql>
			<params>
				<param name="stnd_edi_sts_cd" type="12" value="" out="N"/>
				<param name="event_dt" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
