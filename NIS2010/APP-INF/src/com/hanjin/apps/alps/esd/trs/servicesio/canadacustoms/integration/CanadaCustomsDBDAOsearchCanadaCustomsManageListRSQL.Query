<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanadaCustomsDBDAOsearchCanadaCustomsManageListRSQL">
			<desc><![CDATA[EDI_CND_CSTMS_IBD테이블에서 해당 blNo의 데이타가 있는지 여부를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(BL_NO) FROM EDI_CND_CSTMS_IBD
WHERE 	BL_NO = @[bl_no]   	            
						]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
