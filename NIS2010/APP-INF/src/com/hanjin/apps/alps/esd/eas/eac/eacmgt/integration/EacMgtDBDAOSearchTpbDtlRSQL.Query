<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchTpbDtlRSQL">
			<desc><![CDATA[EAC Tpb DTL Grid 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT EAC_DTL_SEQ
     , EQ_KND_CD
     , EQ_NO
     , EQ_TPSZ_CD
     , DIFF_INV_AMT
     , EAC_NO
FROM   EAS_EXPN_AUD_N3RD_PTY_DTL
WHERE  EAC_NO =  @[eac_no]
AND    NVL(DELT_FLG,'N') != 'Y'

			]]></sql>
			<params>
				<param name="eac_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
