<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CARIssueTransferSlipManageDBDAOSearchDupInvoiceNoRSQL">
			<desc><![CDATA[SearchDupInvoiceNo]]></desc>
			<sql><![CDATA[
SELECT B.INV_NO
FROM AP_INV_HDR A, TES_TML_SO_HDR B 
WHERE A.CSR_NO = B.CSR_NO
AND A.CSR_NO = @[csr_no]
AND A.IF_FLG = 'E'
AND A.IF_ERR_RSN LIKE 'Duplicated Of Vendor Invoice No%'			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
