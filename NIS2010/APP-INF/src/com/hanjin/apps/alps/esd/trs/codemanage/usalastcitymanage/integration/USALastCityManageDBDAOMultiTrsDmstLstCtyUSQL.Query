<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USALastCityManageDBDAOMultiTrsDmstLstCtyUSQL">
			<desc><![CDATA[Trucker에게 중간 경유지를 지정해 주기 위한 USA Last City를 수정]]></desc>
			<sql><![CDATA[
UPDATE trs_dmst_lst_cty
SET	cre_ofc_cd 	= @[cre_ofc_cd],
  	cre_usr_id 	= @[cre_usr_id],
  	cre_dt 		= TO_DATE( @[cre_dt], 'YYYYMMDD HH24:MI:SS'),
  	upd_usr_id 	= @[upd_usr_id],
  	upd_dt 		= TO_DATE( @[upd_dt], 'YYYYMMDD HH24:MI:SS'),
  	delt_flg 	= @[delt_flg]
WHERE	org_loc_cd 	= @[org_loc_cd]
AND		dest_loc_cd = @[dest_loc_cd]
AND		lst_loc_cd 	= @[lst_loc_cd]			]]></sql>
			<params>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="org_loc_cd" type="12" value="" out="N"/>
				<param name="dest_loc_cd" type="12" value="" out="N"/>
				<param name="lst_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
