<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanadaCustomsDBDAOdeleteCanadaCustomsManageDSQL">
			<desc><![CDATA[blNo에 해당하는 EDI_CND_CSTMS_IBD 데이타 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM EDI_CND_CSTMS_IBD																	
WHERE 	BL_NO  = @[bl_no]    	               													
AND 	NVL(EAI_EVNT_DT, to_date(20070505,'yyyymmddhh24miss'))  <= to_date(@[eai_evnt_dt],'yyyymmddhh24miss')				]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
