<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceSurchargeDBDAOsearchWorkOrderSurchargeInquiryRSQL">
			<desc><![CDATA[WorkOrder Surcharge 조회]]></desc>
			<sql><![CDATA[
SELECT a.lgs_cost_cd, 
        		(select lgs_cost_full_nm from tes_lgs_cost where lgs_cost_cd = a.lgs_cost_cd) lgs_cost_nm, 
        		NVL(a.scg_amt,'0') scg_amt, 
        		DECODE(SUBSTR(a.lgs_cost_cd,3,LENGTH(lgs_cost_cd)-1), 
        							  'ALAL', '', 	--Additional Labor
        							  'LWAL', '', 	--Barge low water surcharge
        						   	  'CHAL', '',--(select chss_tpsz_desc from chs_chss_tp_sz where chss_tpsz_cd = a.chss_mgst_tpsz_cd), 	--CHZ Usage
        							  'DPAL', '', 	--Drop & Pull (Drop & Pickup)
        							  'DRAL', (select intg_cd_val_dp_desc from com_intg_cd_dtl where intg_cd_id = 'CD00820' and intg_cd_val_ctnt = a.dry_run_rlbl_pty_tp_cd), 	--Dry Run
        							  'FRAL', '', 	--Ferry Cost
        							  'FIAL', a.fne_cuz_desc, 	--Fine
        		                      'FGAL', (select intg_cd_val_dp_desc from com_intg_cd_dtl where intg_cd_id = 'CD00821' and intg_cd_val_ctnt = a.fumg_cost_tp_cd), 	--Fumigation
        							  'GNAL', '', --(select mgst_tp_nm from mgs_mgst_tp_sz where mgst_tp_cd = a.mgst_tpsz_cd), 	--Gen-set
        							  'HZAL', '', 	--HAZMAT
        		                      'INAL', (select intg_cd_val_dp_desc from com_intg_cd_dtl where intg_cd_id = 'CD00822' and intg_cd_val_ctnt = a.insp_rf_pti_cstms_tp_cd), 	--Inspection
        						      'LFAL', a.lftg_knt || ',' || a.lftg_cuz_desc, 	--Lifting Charge
        							  'MDAL', (select yd_nm from mdm_yard where yd_cd = a.stop_loc_nod_cd), 	--Multistop Delivery
        							  'OSAL', '', 	--Over Size
        							  'OWAL', a.grs_wgt, 	--Over Weight
        							  'PPAL', a.incrt_dt, 	--Pre-Pull
        							  'RCAL', '', 	--Redirection Charge
        							  'SSAL', (select yd_nm from mdm_yard where yd_cd = a.scl_stop_plc_nod_cd), 	--Scale Stop
        					 	      'SRAL', a.sto_dys, 	--Storage
        							  'STAL', a.ob_bkg_no, 	--Street Turn
        							  'SNAL', '', 	--Sunday Running
        						      'SFAL', '', 	--Swing/Flip
        							  'TDAL', '', 	--T-DOC Fee
        						      'TLAL', '', 	--Toll
        							  'WTAL', a.wt_hrs, 	--Waiting Charge
        							  'OTAL', a.otr_rmk, 	--Other Surcharge
        							  'FUWD', '', 
        							  'FURD', '', 
        							  'FUTD', '', 
        							  'FUWR', '', 
        							  'FURT', '', 
        						   	  'CDAL', a.CHSS_NO || ',' || TO_CHAR(a.INCUR_DT, 'YYYYMMDD') 	--Chassis Drayage
        			 ) scg_opt 
        	FROM trs_trsp_scg_dtl a 
            WHERE a.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
            AND a.trsp_so_seq = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
