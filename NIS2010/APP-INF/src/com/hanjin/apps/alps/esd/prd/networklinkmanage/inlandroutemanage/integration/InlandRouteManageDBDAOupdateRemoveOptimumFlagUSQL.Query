<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageDBDAOupdateRemoveOptimumFlagUSQL">
			<desc><![CDATA[해당 구간의 Optimum Route Flag를 제거한다.]]></desc>
			<sql><![CDATA[
update prd_inlnd_rout_mst
set INLND_ROUT_OPTM_FLG = 'N'
  , upd_usr_id = @[upd_usr_id]
  , upd_dt = SYSDATE
where ROUT_ORG_NOD_CD = @[rout_org_nod_cd]
 and ROUT_DEST_NOD_CD = @[rout_dest_nod_cd]
 and ROUT_SEQ <> TO_NUMBER(@[rout_seq]) -- optimum으로 등록하려는 것을 제외하고는 모두 uncheck
 and INLND_ROUT_OPTM_FLG = 'Y'
 and nvl(delt_flg,'N') = 'N'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rout_org_nod_cd" type="12" value="" out="N"/>
				<param name="rout_dest_nod_cd" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
