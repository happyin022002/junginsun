<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BiddingCandidateRegistrationDBDAOSearchSpotBidCnddtVndrListRSQL">
			<desc><![CDATA[SearchSpotBidCnddtVndrList]]></desc>
			<sql><![CDATA[
SELECT SPOT_BID_CNDDT_TERM_SEQ,
       LPAD(VNDR_SEQ, 6, '0') VNDR_SEQ,
       TRS_COMMON_PKG.GET_VNDR_FULL_NM_FNC(VNDR_SEQ) AS VNDR_NM,
       SPOT_BID_VNDR_EML,
       SP_PTAL_EXIST_FLG,
       UPD_USR_ID,
       (SELECT USR_NM FROM COM_USER WHERE 1=1 AND USR_ID = SBCV.UPD_USR_ID AND ROWNUM = 1) AS UPD_USR_NM,
       TO_CHAR(UPD_DT, 'yyyy-mm-dd-hh24:mi:ss') AS UPD_DT
  FROM TRS_SPOT_BID_CNDDT_VNDR SBCV
 WHERE 1=1
   AND SPOT_BID_CNDDT_TERM_SEQ = @[spot_bid_cnddt_term_seq]
#if(${vndr_seq} != '')
   AND VNDR_SEQ = @[vndr_seq]
#end
   AND DELT_FLG = 'N'
 ORDER BY VNDR_SEQ			]]></sql>
			<params>
				<param name="spot_bid_cnddt_term_seq" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
