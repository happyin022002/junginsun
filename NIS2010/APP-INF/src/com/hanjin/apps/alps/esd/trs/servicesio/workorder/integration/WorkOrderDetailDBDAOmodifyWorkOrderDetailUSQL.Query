<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderDetailDBDAOmodifyWorkOrderDetailUSQL">
			<desc><![CDATA[modify Work Order Detail]]></desc>
			<sql><![CDATA[
UPDATE  trs_trsp_svc_ord so 
SET	apnt_dt	 = TO_DATE( @[apnt_dt] , 'YYYYMMDD HH24:MI:SS'), 
de_dt	 = TO_DATE( @[de_dt] , 'YYYYMMDD HH24:MI:SS'), 
eq_no = @[eq_no], 
upd_usr_id = @[upd_usr_id], 
upd_dt = TO_DATE(@[upd_dt],'YYYYMMDDHH24MISS'), 
eq_tpsz_cd = (case when @[eq_tpsz_cd] = 'D4' OR @[eq_tpsz_cd] = 'D5' then @[eq_tpsz_cd] 
else so.eq_tpsz_cd 
end) 
WHERE  1=1 
AND  so.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd] 
AND  so.trsp_so_seq = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="apnt_dt" type="12" value="" out="N"/>
				<param name="de_dt" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
