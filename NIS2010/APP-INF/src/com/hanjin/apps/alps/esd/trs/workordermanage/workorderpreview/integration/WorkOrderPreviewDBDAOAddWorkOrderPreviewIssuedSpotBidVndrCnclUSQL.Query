<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOAddWorkOrderPreviewIssuedSpotBidVndrCnclUSQL">
			<desc><![CDATA[AddWorkOrderPreviewIssuedSpotBidVndrCncl]]></desc>
			<sql><![CDATA[
UPDATE TRS_SPOT_BID_VNDR TSBV
   SET SCFL_BID_FLG = 'N',
       LOCL_UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd]),
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = SYSDATE
 WHERE (SPOT_BID_NO, VNDR_SEQ ) IN (
        SELECT SPOT_BID_NO,
               VNDR_SEQ
          FROM TRS_TRSP_SVC_ORD SO
         WHERE (TRSP_SO_OFC_CTY_CD, TRSP_SO_SEQ) IN (
                SELECT TRSP_SO_OFC_CTY_CD,
                       TRSP_SO_SEQ
                  FROM TRS_TRSP_WRK_ORD_PRV_TMP
                 WHERE WO_PRV_GRP_SEQ = @[wo_prv_grp_seq]
                   AND WO_ISS_NO = @[wo_iss_no]
                   AND WO_CXL_FLG = 'Y' )
           AND HJL_NO IS NULL
           AND SPOT_BID_FLG = 'Y')			]]></sql>
			<params>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="wo_prv_grp_seq" type="12" value="" out="N"/>
				<param name="wo_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
