<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDetailNextStatus2ByVSKUSQL">
			<desc><![CDATA[ModifyCopDetailNextStatus2ByVSK]]></desc>
			<sql><![CDATA[
UPDATE  sce_cop_dtl
SET     act_sts_cd  = 'F'
WHERE   cop_no      = @[cop_no]
AND     (cop_no||cop_dtl_seq) <= (@[pre_cop_no]||@[pre_cop_dtl_seq])
AND     act_sts_cd  in ('N','C')
			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="pre_cop_no" type="12" value="" out="N"/>
				<param name="pre_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
