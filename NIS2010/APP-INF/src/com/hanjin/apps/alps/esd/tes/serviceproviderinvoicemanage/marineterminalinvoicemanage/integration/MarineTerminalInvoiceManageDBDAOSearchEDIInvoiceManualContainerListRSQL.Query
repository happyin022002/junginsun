<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOSearchEDIInvoiceManualContainerListRSQL">
			<desc><![CDATA[SearchEDIInvoiceManualContainerList

* 2011.08.08 윤태승 [CHM-201111118-1] MR Invoice Creation & Correction 의 Manual input 보완]]></desc>
			<sql><![CDATA[
SELECT @[lgs_cost_cd] RVIS_LGS_COST_CD, M.CNTR_NO RVIS_CNTR_NO,D.CNTR_TPSZ_CD RVIS_CNTR_TPSZ_CD --, M.BKG_NO, M.BKG_NO_SPLIT
, CASE WHEN D.CNTR_TPSZ_CD IS NOT NULL THEN 'Y'
  ELSE 'N'
  END EFF_CNTR_YN
FROM TES_EDI_SO_HDR H, TES_EDI_SO_DTL C, TES_EDI_SO_MNL_CNTR_LIST M, MST_CONTAINER D
WHERE NVL(H.DELT_FLG,'N') <> 'Y'
AND H.TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
AND H.TML_SO_SEQ = @[tml_so_seq]
AND H.TML_EDI_SO_OFC_CTY_CD = C.TML_EDI_SO_OFC_CTY_CD
AND H.TML_EDI_SO_SEQ = C.TML_EDI_SO_SEQ
AND C.TML_EDI_SO_OFC_CTY_CD = M.TML_EDI_SO_OFC_CTY_CD
AND C.TML_EDI_SO_SEQ = M.TML_EDI_SO_SEQ
AND C.TML_EDI_SO_DTL_SEQ = M.TML_EDI_SO_DTL_SEQ
AND M.CNTR_NO = D.CNTR_NO(+)
AND M.EDI_SO_DTL_ID = @[edi_so_dtl_id]
AND C.VSL_CD = SUBSTR(@[vvd],1,4)
AND C.SKD_VOY_NO = SUBSTR(@[vvd],5,4)
AND C.SKD_DIR_CD = SUBSTR(@[vvd],9,1)
AND C.IO_BND_CD = @[io_bnd_cd]			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="edi_so_dtl_id" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
