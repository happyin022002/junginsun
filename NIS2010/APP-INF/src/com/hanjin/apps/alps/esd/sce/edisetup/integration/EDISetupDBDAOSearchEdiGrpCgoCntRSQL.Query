<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EDISetupDBDAOSearchEdiGrpCgoCntRSQL">
			<desc><![CDATA[SearchEdiGrpCgoCnt]]></desc>
			<sql><![CDATA[
select  count(edi_grp_cd)
    from    edi_grp_cgo
    where   
    1=1
#if(${e_edi_grp_cd} != '')
    and edi_grp_cd      = @[e_edi_grp_cd]
#end
#if(${e_co_div_cd} != '')
    and     co_div_cd       = @[e_co_div_cd]
#end
#if(${e_edi_stnd_sts_cd} != '')
    and     edi_stnd_sts_cd = @[e_edi_stnd_sts_cd]
#end
#if(${e_EDI_STS_SEQ} != '')
    and     EDI_STS_SEQ     = @[e_EDI_STS_SEQ]
#end			]]></sql>
			<params>
				<param name="e_edi_grp_cd" type="12" value="" out="N"/>
				<param name="e_co_div_cd" type="12" value="" out="N"/>
				<param name="e_edi_stnd_sts_cd" type="12" value="" out="N"/>
				<param name="e_EDI_STS_SEQ" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
