<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOSearchHJSListOnlyListTrunkRSQL">
			<desc><![CDATA[SearchHJSListOnlyListTrunk]]></desc>
			<sql><![CDATA[
#if (${vrfy_tml_cd} != '' and ${yd_cd} != '' and ${tml_port_cd} != '' and ${vrfy_tml_cd} != ${tml_port_cd}) 
SELECT X.*  --// 구주 털보 아저씨 (2014-05-20)
FROM (
#else 
#end

SELECT CNTR_NO, CNTR_TPSZ_CD, CNTR_STY_CD, IOC_CD, LANE_CD, LANE_CD2, LOCL_TS_IND_CD, RCVDE_TERM_IND_CD, BKG_NO, IO_BND_CD,
			 DCGO_CLSS_CD, BB_CGO_FLG, AWK_CGO_FLG, RC_FLG, CNTR_RMK, DSCR_IND_CD, VVD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, ATB_DT, VNDR_SEQ,TML_TRNS_MOD_CD, DSCR_DTL_IND_CD,
			 WRK_DT, BL_NO, BL_NO_TP, BL_NO_CHK,  DC_FLG, CLPT_IND_SEQ, CALL_YD_IND_SEQ,
#if (${file_import_yn} == 'Y')
	VRFY_RSLT_IND_CD
#elseif (${file_import_yn} == 'N')
	DECODE(IO_BND_CD, LOWER(@[io_bnd_cd]),'DC',DECODE(DSCR_IND_CD,'HD','DC','CO')) VRFY_RSLT_IND_CD
#else
#end
	 FROM(
	 SELECT L.CNTR_NO        										CNTR_NO,
			L.TPSZ                                                 CNTR_TPSZ_CD,
			L.FM                                                   CNTR_STY_CD,
			L.IPC                                                  IOC_CD,
			L.LANE                                                 LANE_CD,
			L.LANE                                                 LANE_CD2,
            DECODE(L.PCTL_IO_BND_CD,'T',
			DECODE(R.TRSP_MOD_CD||O.TRSP_MOD_CD,'WDVD','T','VDWD','T','WDWD','T','VDVD','T',DECODE(L.FM,'M',NULL,'R',NULL,'L')),'L') LOCL_TS_IND_CD,
			L.RCVDE_TERM_IND_CD                                    RCVDE_TERM_IND_CD,
			L.BKG_NO                                               BKG_NO,
			--L.BKG_NO_SPLIT                                         BKG_NO_SPLIT,
			NVL(DECODE(L.FM,'M',DECODE(SUBSTR(@[yd_cd],1,5),L.POL,'O','I'),
					DECODE(NVL(R.TRSP_MOD_CD,'N')||NVL(O.TRSP_MOD_CD,'N'),'WDTD','I','WDRD','I','VDTD','I','VDRD','I','RDWD','O','RDVD','O','TDWD','O','TDVD','O',
														'WDN','I','VDN','I','NVD','O','NWD','O',
													   'WDVD',DECODE(@[vvd],R.VSL_CD||R.SKD_VOY_NO||R.SKD_DIR_CD,'I','O'),
													   'VDWD',DECODE(@[vvd],R.VSL_CD||R.SKD_VOY_NO||R.SKD_DIR_CD,'I','O'),
													   'WDWD',DECODE(@[vvd],R.VSL_CD||R.SKD_VOY_NO||R.SKD_DIR_CD,'I','O'),
													   'VDVD',DECODE(@[vvd],R.VSL_CD||R.SKD_VOY_NO||R.SKD_DIR_CD,'I','O'))),LOWER(@[io_bnd_cd])) IO_BND_CD,
			 NVL(L.DCGO_CLSS_CD,'N')	DCGO_CLSS_CD,
			 NVL(L.BB_CGO_FLG,'N')		BB_CGO_FLG,
			 NVL(L.AWK_CGO_FLG,'N')		AWK_CGO_FLG,
			 NVL(L.RC_FLG,'N')			RC_FLG,
			 L.CNTR_RMK||DB.CNTR_RMK	CNTR_RMK,
			 DECODE(DECODE(L.PCTL_IO_BND_CD,'T',
                    DECODE(R.TRSP_MOD_CD||O.TRSP_MOD_CD,'WDVD','T','VDWD','T','WDWD','T','VDVD','T',DECODE(L.FM,'M',NULL,'R',NULL,'L')),'L'),'L',
					DECODE(L.FM,'F',DECODE(@[io_bnd_cd],'O',DECODE(R.TRSP_MOD_CD,'TD','T','RD','R','WD','B','O'),
					  DECODE(RL,0,DECODE(TR,'RD','R','WD','B','TD','T','T'),'R')),'I',
					DECODE(O.TRSP_MOD_CD,'TD','T','RD','R','WD','B','O')),VSL_TP) TML_TRNS_MOD_CD,                   -- 추가(20070404)
			 CASE WHEN LENGTH(DB.CNTR_RMK) > 1   THEN 'HD' -- 'Double Bill' Check
--			         CASE WHEN ( SELECT COUNT(*) -- 'Double Bill' Check
--			                          FROM   TES_TML_SO_HDR A, TES_TML_SO_CNTR_LIST B             -- 추가(20070404)
--			                          WHERE  A.TML_SO_OFC_CTY_CD  = B.TML_SO_OFC_CTY_CD             -- 추가(20070404)
--			                          AND    A.TML_SO_SEQ         = B.TML_SO_SEQ             -- 추가(20070404)
--
--			                          AND    ( A.VNDR_SEQ, A.YD_CD ) IN ( SELECT DISTINCT VNDR_SEQ, YD_CD
--			                                                              FROM   TES_FILE_IMP_TMP
--			                                                              WHERE  TML_SO_OFC_CTY_CD  = @[]
--			                                                              AND    TML_SO_SEQ         = @[] )
--
--			                          AND    B.CNTR_NO            = L.CNTR_NO             -- 추가(20070404)
--			                          AND    B.CNTR_STY_CD        = L.FM	             -- 추가(20070404)
--			                          AND    B.VSL_CD             = SUBSTR(@[],1,4)             -- 추가(20070404)
--			                          AND    B.SKD_VOY_NO         = SUBSTR(@[],5,4)             -- 추가(20070404)
--			                          AND    B.SKD_DIR_CD         = SUBSTR(@[],9,1)             -- 추가(20070404)
--			                          AND    NVL(A.DELT_FLG,'N')  <> 'Y'             -- 추가(20070404)
--			                          AND    A.TML_INV_TP_CD      = 'TM') > 0 THEN 'HD'             -- 추가(20070404)
	-- 김기영 부장님 요청으로 double billing checking period를 현재 3개월에서 (+/-)7일로 수정 - 2007.07.16
	-- 김기영 부장님 요청으로 receive date 비교로직 삭제함. - 2008.04.17
--			                          AND    MONTHS_BETWEEN(TO_DATE(REPLACE(@[],'-'),'YYYY-MM-DD'),A.RCV_DT ) <= 0.25 AND ROWNUM = 1) > 0 THEN 'HD'  -- 추가(20070404) => 변수 : ATB DATE
					   ELSE 'HO'
				  END                                                                                DSCR_IND_CD,
			 @[vvd]																					VVD,
			 SUBSTR(@[vvd],1,4)																		VSL_CD,
			 SUBSTR(@[vvd],5,4)																		SKD_VOY_NO,
			 SUBSTR(@[vvd],9,1)																		SKD_DIR_CD,
			 @[atb_dt]																					ATB_DT,
			 @[vndr_seq]																					VNDR_SEQ,
			 ''																				DSCR_DTL_IND_CD,
			 ''																				WRK_DT,
			 ''																				BL_NO,
			 ''																				BL_NO_TP,
			 ''																				BL_NO_CHK,
			 'DC'																				VRFY_RSLT_IND_CD,
			 'Y'																				DC_FLG,
             @[clpt_ind_seq] 																CLPT_IND_SEQ,
			 @[call_yd_ind_seq]																CALL_YD_IND_SEQ
	 FROM ( SELECT B.CNTR_NO       CNTR_NO,
				   B.TPSZ          TPSZ,
				   B.FM            FM,
				   B.IPC           IPC,
				   B.LANE          LANE,
				   B.RCVDE_TERM_IND_CD       RCVDE_TERM_IND_CD,
				   MIN(B.BKG_NO)        BKG_NO,
				   --MIN(B.BKG_NO_SPLIT)  BKG_NO_SPLIT,
				   MAX(G.PCTL_NO)        COP_NO,
				   MAX(B.TP_CD)         TP_CD,
				   MAX(B.POL)           POL,
					MIN(B.DCGO_CLSS_CD)          DCGO_CLSS_CD,
					MIN(B.BB_CGO_FLG)            BB_CGO_FLG,
					B.RM            CNTR_RMK,
					MIN(B.AWK_CGO_FLG)   AWK_CGO_FLG,
					MIN(B.RC_FLG)        RC_FLG,
					B.VSL_TP        VSL_TP,
					B.TR            TR,
					B.RL            RL,
				   MIN(( SELECT MAX(PCTL_SEQ)
					 FROM   SCE_COP_HDR HD, PRD_PROD_CTL_ROUT_DTL GD
					 WHERE  H.CNTR_NO         = HD.CNTR_NO
					 AND    H.PCTL_NO          = HD.PCTL_NO
					 AND    B.BKG_NO          = HD.BKG_NO
					 --AND    B.BKG_NO_SPLIT    = HD.BKG_NO_SPLIT
					 AND    HD.PCTL_NO         = GD.PCTL_NO
					 AND    GD.NOD_LNK_DIV_CD = 'L'
					 AND    GD.PCTL_SEQ < G.PCTL_SEQ )) R_SEQ,
				   MAX(( SELECT MIN(PCTL_SEQ)
					 FROM   SCE_COP_HDR OD, PRD_PROD_CTL_ROUT_DTL OG
					 WHERE  OD.CNTR_NO        = H.CNTR_NO
					 AND    OD.PCTL_NO         = H.PCTL_NO
					 AND    OD.BKG_NO         = H.BKG_NO
					 --AND    OD.BKG_NO_SPLIT   = H.BKG_NO_SPLIT
					 AND    OD.PCTL_NO         = OG.PCTL_NO
					 AND    OG.NOD_LNK_DIV_CD = 'L'
					 AND    OG.PCTL_SEQ > G.PCTL_SEQ )) O_SEQ
                    --, G.PCTL_IO_BND_CD
                              ,(SELECT NVL(X.PCTL_IO_BND_CD,'') PCTL_IO_BND_CD
                                FROM PRD_PROD_CTL_ROUT_DTL X
                                WHERE 1=1
                                AND X.PCTL_NO = G.PCTL_NO
                                AND X.PCTL_SEQ = ( SELECT 
                                                        CASE
                                                        WHEN ORG_NOD_CD = @[yd_cd]
                                                        THEN PCTL_SEQ-1
                                                        WHEN DEST_NOD_CD = @[yd_cd] 
                                                        THEN PCTL_SEQ+1
                                                        END CUR_PCTL_SEQ
                                                    FROM PRD_PROD_CTL_ROUT_DTL X
                                                    WHERE 1=1
                                                    AND X.PCTL_NO = G.PCTL_NO
                                                    AND X.VSL_CD = SUBSTR(@[vvd],1,4)
                                                    AND X.SKD_VOY_NO = SUBSTR(@[vvd],5,4)
                                                    AND X.SKD_DIR_CD = SUBSTR(@[vvd],9,1)
                                                    AND ROWNUM = 1 )) PCTL_IO_BND_CD
	/** 2009-01-19: HJS Only의 Bkg정보를 물고오는 부분에 단순 min/max말고 rank를 주어 Bkg_no/Bkg_no_split을 제대로 물고오게 변경한다.  **/
			FROM   ( SELECT X.* FROM ( SELECT /*+ NO_EXPAND */ C.CNTR_NO CNTR_NO,
							C.CNTR_TPSZ_CD TPSZ,
							DENSE_RANK() OVER (PARTITION BY C.CNTR_NO ORDER BY V.CRE_DT DESC, V.BKG_NO DESC) BKG_RANK,
							DECODE(B.BKG_CGO_TP_CD,'F','F','B','F','R','R','M') FM,
							DECODE(BR.CONTI_CD,BD.CONTI_CD,'I','O') IPC,
							V.SLAN_CD LANE,
							B.RCV_TERM_CD||'/'||B.DE_TERM_CD RCVDE_TERM_IND_CD,
							V.BKG_NO BKG_NO,
							--V.BKG_NO_SPLIT BKG_NO_SPLIT,
							B.POL_CD        POL,
							B.BKG_CGO_TP_CD TP_CD,
							 CASE WHEN D.CNTR_NO = C.CNTR_NO  THEN NVL(SUBSTR(D.IMDG_CLSS_CD, 1, 1), 'N') ELSE 'N' END  DCGO_CLSS_CD,
							 NVL(C.BB_CGO_FLG,'N')							 BB_CGO_FLG,
							 --DECODE(NVL(A.OVR_VOID_SLT_QTY,0),0,'N','Y') AWK_CGO_FLG,
                             NVL(C.AWK_CGO_FLG,'N') AWK_CGO_FLG,
							 NVL(B.RC_FLG,'N') RC_FLG,
							 DECODE(NVL(C.BB_CGO_FLG,'N'),'Y','Break Bulk') RM,
							 DECODE(L.VSL_SVC_TP_CD,'O',DECODE((SELECT CRR_CD FROM MDM_VSL_CNTR  WHERE VSL_CD = S.VSL_CD),'SML','V','F'),'V') VSL_TP,  -- 추가(20070404)
						   ( SELECT MAX(TRSP_CRR_MOD_CD)
							 FROM   TRS_TRSP_SVC_ORD
							 WHERE  ( FM_NOD_CD      = @[yd_cd] OR       TO_NOD_CD      = @[yd_cd] )
							 AND    EQ_NO = C.CNTR_NO
							 AND    NVL(DELT_FLG,'N') <> 'Y'
							 AND    CRE_DT < TO_DATE(REPLACE(@[atb_dt],'-'),'YYYYMMDD') + 15 ) TR,    -- 추가(20070404)=>변수 : ATB DATE
						   ( SELECT COUNT(*)
							 FROM   TRS_TRSP_RAIL_BIL_ORD
							 WHERE  ( FM_NOD_CD      = @[yd_cd] OR       TO_NOD_CD      = @[yd_cd] )
							 AND    EQ_NO = C.CNTR_NO
							 AND    NVL(DELT_FLG,'N') <> 'Y'
							 AND    CRE_DT < TO_DATE(REPLACE(@[atb_dt],'-'),'YYYYMMDD') + 15 AND ROWNUM = 1 ) RL    -- 추가(20070404)=>ATB DATE
					 FROM   BKG_VVD V, BKG_CONTAINER C, BKG_BOOKING B, BKG_DG_CGO D, BKG_AWK_CGO A,
							 MDM_LOCATION BR, MDM_LOCATION BD,VSK_VSL_SKD S, MDM_VSL_SVC_LANE L
					 WHERE  V.VSL_CD           = SUBSTR(@[vvd],1,4)
					 AND    V.SKD_VOY_NO       = SUBSTR(@[vvd],5,4)
					 AND    V.SKD_DIR_CD       = SUBSTR(@[vvd],9,1)
					 AND    ( V.POL_CD     = SUBSTR(@[yd_cd],1,5)
					 OR     V.POD_CD       = SUBSTR(@[yd_cd],1,5) )
-- [CHM-201640637]MR Invoice에 Calling Sequence추가하고 CNTR List가져올 때 Verify로직 추가 (2016.03.28)
-- [CHM-201642766]INV Booking No.관련 yard calling Sequence검색로직 변경(2016.08.09)
#if(${call_yd_seq_chk} == 'Y')
					 AND    DECODE(@[io_bnd_cd],'I',V.POD_CLPT_IND_SEQ,V.POL_CLPT_IND_SEQ) = @[clpt_ind_seq] 
#else
#end
					 AND    V.BKG_NO           = C.BKG_NO(+)
					 --AND    V.BKG_NO_SPLIT     = C.BKG_NO_SPLIT(+)
					 AND    C.BKG_NO           = B.BKG_NO(+)
					 --AND    C.BKG_NO_SPLIT     = B.BKG_NO_SPLIT(+)
					 AND    B.BKG_STS_CD IN ('F','W')
					 AND    B.BKG_NO           = D.BKG_NO(+)
					 --AND    B.BKG_NO_SPLIT     = D.BKG_NO_SPLIT(+)
					 AND    C.BKG_NO           = A.BKG_NO(+)
					 --AND    C.BKG_NO_SPLIT     = A.BKG_NO_SPLIT(+)
					 AND    C.CNTR_NO          = A.CNTR_NO(+)
					 AND    B.POR_CD           = BR.LOC_CD
					 AND    B.POD_CD           = BD.LOC_CD
					 AND    V.VSL_CD           = S.VSL_CD(+)
					 AND    V.SKD_VOY_NO       = S.SKD_VOY_NO(+)
					 AND    V.SKD_DIR_CD       = S.SKD_DIR_CD(+)
					 AND    S.VSL_SLAN_CD          = L.VSL_SLAN_CD(+)
					 AND    L.DELT_FLG         = 'N' ) X WHERE BKG_RANK = 1 ) B, SCE_COP_HDR H, PRD_PROD_CTL_ROUT_DTL G
			WHERE  B.CNTR_NO             = H.CNTR_NO(+)
			AND    B.BKG_NO              = H.BKG_NO(+)
			--AND    B.BKG_NO_SPLIT        = H.BKG_NO_SPLIT(+)
			AND    NVL(H.COP_STS_CD,'N') <> 'X'
			AND    H.PCTL_NO              = G.PCTL_NO(+)
			AND    G.NOD_LNK_DIV_CD(+)   = 'N'
			--AND    G.ORG_NOD_CD(+)       = [yd_cd]  --vvd, sce, prd 에서 값을제대로 못가지고 와서 밑에쿼리로 변경
			AND    SUBSTR(G.ORG_NOD_CD(+),1,5)       = SUBSTR(@[yd_cd],1,5)
			 GROUP BY B.CNTR_NO,B.TPSZ,B.FM,B.IPC,B.LANE,B.RCVDE_TERM_IND_CD,B.RM,B.VSL_TP,B.TR,B.RL, G.PCTL_NO--, G.PCTL_IO_BND_CD
			) L, PRD_PROD_CTL_ROUT_DTL R, PRD_PROD_CTL_ROUT_DTL O,
-- [CHM-201539158]Invoice CNTR Verify로직 수정 요청(Double Billing비교 부분) 2015.12.10 속도개선을 위해 수정
-- [CHM-201640867]EDI 전송건 인보이스 UPLOAD 시간 과다 발생 (SQL Tunning 받은 결과 반영) - 2016.03.28
							 (SELECT /*+ LEADING(B) USE_NL(A) */ B.CNTR_NO, 'Double billing Inv : '||MAX(A.INV_NO) CNTR_RMK
							  FROM   TES_TML_SO_HDR A, TES_TML_SO_CNTR_LIST B
							  WHERE  A.TML_SO_OFC_CTY_CD  = B.TML_SO_OFC_CTY_CD
							  AND    A.TML_SO_SEQ         = B.TML_SO_SEQ
--							  AND    ( A.VNDR_SEQ, A.YD_CD ) IN ( SELECT DISTINCT VNDR_SEQ, YD_CD
--																  FROM   TES_FILE_IMP_TMP
--																  WHERE  TML_SO_OFC_CTY_CD  = [tml_so_ofc_cty_cd]
--																  AND    TML_SO_SEQ         = [tml_so_seq] )
							  AND    A.VNDR_SEQ 		  = @[vndr_seq]
							  AND	 A.YD_CD			  = @[yd_cd]
							  AND    B.VSL_CD             = SUBSTR(@[vvd],1,4)
							  AND    B.SKD_VOY_NO         = SUBSTR(@[vvd],5,4)
							  AND    B.SKD_DIR_CD         = SUBSTR(@[vvd],9,1)
							  AND    B.IO_BND_CD          = @[io_bnd_cd]
-- [CHM-201640637]MR Invoice에 Calling Sequence추가하고 CNTR List가져올 때 Verify로직 추가 (2016.03.28)
-- [CHM-201642766]INV Booking No.관련 yard calling Sequence검색로직 변경(2016.08.09)
#if(${call_yd_seq_chk} == 'Y')
							  AND	 NVL(B.CLPT_IND_SEQ,'1') = NVL(@[clpt_ind_seq],'1')
							  AND	 NVL(B.CALL_YD_IND_SEQ,'1') = NVL(@[call_yd_ind_seq],'1')
#else
#end
							  AND    NVL(A.DELT_FLG,'N')  <> 'Y'
							  AND    A.TML_INV_TP_CD      = 'TM'
							  GROUP BY B.CNTR_NO
							  ) DB
	 WHERE  L.COP_NO              = R.PCTL_NO(+)
	 AND    R.NOD_LNK_DIV_CD(+)   = 'L'
	 AND    L.R_SEQ               = R.PCTL_SEQ(+)
	 AND    L.COP_NO              = O.PCTL_NO(+)
	 AND    O.NOD_LNK_DIV_CD(+)   = 'L'
	 AND    L.O_SEQ               = O.PCTL_SEQ(+)
	 AND    L.CNTR_NO             = DB.CNTR_NO(+)

#if (${file_import_yn} == 'Y')
		 AND L.CNTR_NO NOT IN (SELECT CNTR_NO
							   FROM  TES_TML_SO_CNTR_LIST
							   WHERE TML_SO_SEQ = @[tml_so_seq]
							   AND   TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
							   AND   VSL_CD||SKD_VOY_NO||SKD_DIR_CD  = ( SELECT DISTINCT VVD_CD
																		 FROM   TES_FILE_IMP_TMP
																		 WHERE  TML_SO_OFC_CTY_CD  = @[tml_so_ofc_cty_cd]
																		 AND    TML_SO_SEQ         = @[tml_so_seq] ))
#else
#end

	 )WHERE IO_BND_CD IN (@[io_bnd_cd],LOWER(@[io_bnd_cd]))

#if (${all_tp} == 'N' and  ${fm_tp_length} == '1')
	AND CNTR_STY_CD = @[fm_tp]
#else
#end

#if (${all_tp} == 'N' and  ${ts_tp_length} == '1')
	AND LOCL_TS_IND_CD = @[ts_tp]
#else
#end

#if (${vrfy_tml_cd} != '' and ${yd_cd} != '' and ${tml_port_cd} != '' and ${vrfy_tml_cd} != ${tml_port_cd}) 
--//invoice의 port(yard의 5자리)와 vrfy_tml_cd가 같다는것은 vrfy_tml_cd가 port라는 의미로 이전 CNTR verify와 동일하므로 yard별로 구별할 필요가 없다.
) X , CTM_MOVEMENT M   --// 구주 털보 아찌 (2014-05-20)
WHERE 1=1
AND     X.CNTR_NO = M.CNTR_NO 
AND     M.ORG_YD_CD LIKE DECODE(@[vrfy_tml_cd],NULL,SUBSTR(@[yd_cd],1,5),'',SUBSTR(@[yd_cd],1,5),@[vrfy_tml_cd])||'%'
AND     M.CRNT_VSL_CD           = X.VSL_CD
AND     M.CRNT_SKD_VOY_NO       = X.SKD_VOY_NO
AND     M.CRNT_SKD_DIR_CD       = X.SKD_DIR_CD
AND     M.FCNTR_FLG             = DECODE(X.CNTR_STY_CD,'F','Y','N')
#else 
#end			]]></sql>
			<params>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="atb_dt" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
				<param name="call_yd_ind_seq" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="fm_tp" type="12" value="" out="N"/>
				<param name="ts_tp" type="12" value="" out="N"/>
				<param name="vrfy_tml_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
