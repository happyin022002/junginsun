<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchSOAmtRSQL">
			<desc><![CDATA[SO Amt 일치 여부를 검사한다.]]></desc>
			<sql><![CDATA[
SELECT  DECODE(NVL(BZC_AMT,0),TO_NUMBER(NVL(@[bzc_amt],0)),'OK','DIFF') BZC_DIFF							
           ,DECODE(NVL(ETC_ADD_AMT,0),TO_NUMBER(NVL(@[etc_add_amt],0)),'OK','DIFF') ETC_DIFF			
           ,DECODE(NVL(NEGO_AMT,0),TO_NUMBER(NVL(@[nego_amt],0)),'OK','DIFF') NEGO_DIFF				
           ,DECODE(NVL(FUEL_SCG_AMT,0),TO_NUMBER(NVL(@[fuel_scg_amt],0)),'OK','DIFF') SCG_DIFF			
           ,DECODE(NVL(SCG_VAT_AMT,0),TO_NUMBER(NVL(@[scg_vat_amt],0)),'OK','DIFF') SCG_VAT_DIFF
	       ,DECODE(NVL(TOLL_FEE_AMT,0),TO_NUMBER(NVL(@[toll_fee_amt],0)),'OK','DIFF') TOLL_FEE_DIFF
 FROM TRS_TRSP_SVC_ORD					
WHERE TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]		
  AND TRSP_SO_SEQ = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="bzc_amt" type="12" value="" out="N"/>
				<param name="etc_add_amt" type="12" value="" out="N"/>
				<param name="nego_amt" type="12" value="" out="N"/>
				<param name="fuel_scg_amt" type="12" value="" out="N"/>
				<param name="scg_vat_amt" type="12" value="" out="N"/>
				<param name="toll_fee_amt" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
