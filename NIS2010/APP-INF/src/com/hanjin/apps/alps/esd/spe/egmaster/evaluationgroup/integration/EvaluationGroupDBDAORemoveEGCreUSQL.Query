<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EvaluationGroupDBDAORemoveEGCreUSQL">
			<desc><![CDATA[EG 데이터를 삭제 한다.]]></desc>
			<sql><![CDATA[
UPDATE SPE_EV_GRP
   SET DELT_FLG        = 'Y'
     , UPD_USR_ID      = @[upd_usr_id]
     , UPD_DT          = SYSDATE
 WHERE EG_ID           = @[eg_id]			]]></sql>
