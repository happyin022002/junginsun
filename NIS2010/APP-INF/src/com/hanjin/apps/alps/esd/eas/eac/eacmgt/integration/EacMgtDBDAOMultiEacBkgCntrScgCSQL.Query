<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOMultiEacBkgCntrScgCSQL">
			<desc><![CDATA[BKG Charge를 Container 단위로 입력한다.
(Hanger Charge등)]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_BKG_CNTR_CHG_CHK(
 BKG_NO
,CNTR_NO
,EAC_SYS_IF_CD
,EAC_NO
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
)
VALUES (
 @[bkg_no]
,@[cntr_no]
,@[eac_sys_if_cd]
,@[eac_no]
,NVL(@[usr_id], 'BKGCOD')
,SYSDATE
,NVL(@[usr_id], 'BKGCOD')
,SYSDATE
)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="eac_sys_if_cd" type="12" value="" out="N"/>
				<param name="eac_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
