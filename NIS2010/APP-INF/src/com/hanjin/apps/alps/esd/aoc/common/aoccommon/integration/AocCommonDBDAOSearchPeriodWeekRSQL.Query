<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AocCommonDBDAOSearchPeriodWeekRSQL">
			<desc><![CDATA[2012.10.04 변종건 [CHM-201220395] Add-on Tariff Management 개선 프로젝트
- Week 조회]]></desc>
			<sql><![CDATA[
SELECT (SELECT SUBSTR(SLS_FM_DT,1,4) || '-' || SUBSTR(SLS_FM_DT,5,2) || '-' || SUBSTR(SLS_FM_DT,7)
          FROM MAS_WK_PRD
         WHERE COST_YR = @[f_fm_yr]
           AND COST_WK = @[f_fm_wk]
        ) AS FM_DATE,
       (SELECT SUBSTR(SLS_TO_DT,1,4) || '-' || SUBSTR(SLS_TO_DT,5,2) || '-' || SUBSTR(SLS_TO_DT,7)
          FROM MAS_WK_PRD
         WHERE COST_YR = @[f_to_yr]
           AND COST_WK = @[f_to_wk]
        ) AS TO_DATE
FROM DUAL			]]></sql>
			<params>
				<param name="f_fm_yr" type="12" value="" out="N"/>
				<param name="f_fm_wk" type="12" value="" out="N"/>
				<param name="f_to_yr" type="12" value="" out="N"/>
				<param name="f_to_wk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
