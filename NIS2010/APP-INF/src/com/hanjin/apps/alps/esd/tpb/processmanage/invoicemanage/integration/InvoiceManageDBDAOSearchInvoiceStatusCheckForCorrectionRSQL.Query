<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOSearchInvoiceStatusCheckForCorrectionRSQL">
			<desc><![CDATA[SearchInvoiceStatusCheckForCorrection]]></desc>
			<sql><![CDATA[
SELECT   NVL(R.N3PTY_INV_STS_CD,'N') VALIDYN
FROM     TPB_INV_RVIS R
       , TPB_INVOICE V
WHERE    1 = 1
AND      R.N3PTY_INV_NO = V.N3PTY_INV_NO
AND      V.N3PTY_DELT_TP_CD = 'N'
AND      R.N3PTY_INV_STS_CD IN ('A','C') /* A : 현재 버전 AR I/F; C : 현재 */
AND      R.N3PTY_INV_NO = @[s_n3pty_inv_no]			]]></sql>
			<params>
				<param name="s_n3pty_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
