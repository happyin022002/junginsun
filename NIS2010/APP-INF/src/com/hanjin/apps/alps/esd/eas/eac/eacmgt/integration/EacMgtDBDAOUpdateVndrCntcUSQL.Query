<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOUpdateVndrCntcUSQL">
			<desc><![CDATA[SP Contact point 를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE EAS_EXPN_AUD_CS_CNTC_PNT
SET    VNDR_CNTC_PNT_NM   = @[vndr_cntc_pnt_nm]
     , PSN_NM             = @[psn_nm]
     , PSN_DESC           = @[psn_desc]
     , STE_NM             = @[ste_nm]
     , CTY_NM             = @[cty_nm]
     , PHN_NO             = @[phn_no]
     , FAX_NO             = @[fax_no]
     , VNDR_EML           = @[vndr_eml]
     , EAC_EML_USE_FLG    = decode(@[eac_eml_use_flg],0,'N',1,'Y',@[eac_eml_use_flg])
     , EAC_FAX_USE_FLG    = decode(@[eac_fax_use_flg],0,'N',1,'Y',@[eac_fax_use_flg])
     , DELT_FLG           = @[delt_flg]
     , UPD_USR_ID         = @[upd_usr_id]
     , UPD_DT             = sysdate
WHERE VNDR_SEQ = @[vndr_seq] 
AND   VNDR_CNTC_PNT_SEQ =  @[vndr_cntc_pnt_seq]
			]]></sql>
			<params>
				<param name="vndr_cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="psn_nm" type="12" value="" out="N"/>
				<param name="psn_desc" type="12" value="" out="N"/>
				<param name="ste_nm" type="12" value="" out="N"/>
				<param name="cty_nm" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="vndr_eml" type="12" value="" out="N"/>
				<param name="eac_eml_use_flg" type="12" value="" out="N"/>
				<param name="eac_fax_use_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vndr_cntc_pnt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
