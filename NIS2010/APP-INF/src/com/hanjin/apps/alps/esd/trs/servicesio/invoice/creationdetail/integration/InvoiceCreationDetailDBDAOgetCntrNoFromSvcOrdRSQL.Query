<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceCreationDetailDBDAOgetCntrNoFromSvcOrdRSQL">
			<desc><![CDATA[Service Order 테이블로부터 Cntr No 를 가져온다]]></desc>
			<sql><![CDATA[
SELECT SO.EQ_NO EqNo,
	   SO.BKG_NO BkgNo,
	   SO.TRSP_BND_CD BndCd,
	   SO.TRSP_COST_DTL_MOD_CD CostDtlModCd 
FROM TRS_TRSP_SVC_ORD SO,
	 TRS_TRSP_WRK_ORD WO
WHERE 1=1
AND SO.TRSP_WO_OFC_CTY_CD = WO.TRSP_WO_OFC_CTY_CD
AND SO.TRSP_WO_SEQ = WO.TRSP_WO_SEQ
AND NVL(SO.DELT_FLG,'N') = 'N'
AND WO.TRSP_WO_OFC_CTY_CD = @[TrspWoOfcCtyCd]
AND WO.TRSP_WO_SEQ = @[TrspWoSeq]
AND WO.WO_VNDR_SEQ = @[WoVndrSeq]
AND NVL(WO.DELT_FLG,'N') = 'N'			]]></sql>
			<params>
				<param name="TrspWoOfcCtyCd" type="12" value="" out="N"/>
				<param name="TrspWoSeq" type="12" value="" out="N"/>
				<param name="WoVndrSeq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
