<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOSearchActualToBeUpdatedTmpVSKUSQL">
			<desc><![CDATA[임시처리를 위해 vsk 정보를 가져오는 부분 조회]]></desc>
			<sql><![CDATA[
SELECT BKG_NO,
  CNTR_NO,
  COP_NO,
  ACT_DT,
  ACT_STS_MAPG_CD,
  COP_DTL_SEQ,
  NOD_CD,
  ACT_RCV_TP_CD,
  VSL_CD,
  SKD_VOY_NO,
  SKD_DIR_CD,
  VPS_PORT_CD,
  EDI_SND_RSLT_FLG,
  USR_ID,
  ACT_UMCH_TP_CD,
  CLPT_IND_SEQ,
  UPD_USR_ID,
  EDI_MSG_TP_CD,
  ACT_DAT_RCV_DT
FROM (
    SELECT --DECODE(MIN(SRC),'0','VVD','1','ORG','2','SPL','3','CMB','4','LCL') SCR,                                      
      MAX(BKG_NO)BKG_NO,
      ACT_RCV_DT,
      ACT_RCV_NO ,
      MAX(CNTR_NO)CNTR_NO,
      MAX(COP_NO)COP_NO,
      MAX(ACT_DT)ACT_DT,
      MAX(ACT_STS_MAPG_CD)ACT_STS_MAPG_CD ,
      MAX(COP_DTL_SEQ) COP_DTL_SEQ ,
      MAX(NOD_CD)NOD_CD,
      MAX(ACT_RCV_TP_CD)ACT_RCV_TP_CD ,
      MAX(VSL_CD)VSL_CD,
      MAX(SKD_VOY_NO)SKD_VOY_NO,
      MAX(SKD_DIR_CD)SKD_DIR_CD,
      MAX(VPS_PORT_CD)VPS_PORT_CD ,
      MAX(EDI_SND_RSLT_FLG)EDI_SND_RSLT_FLG,
      MAX(USR_ID)USR_ID,
      MAX(ACT_UMCH_TP_CD)ACT_UMCH_TP_CD ,
      MAX(CLPT_IND_SEQ)CLPT_IND_SEQ,
      MAX(ACT_DAT_RCV_DT) ACT_DAT_RCV_DT,
      USR_ID AS UPD_USR_ID,
      EDI_MSG_TP_CD
    FROM (
        SELECT '0',
          AD.BKG_NO,
          ACT_RCV_DT,
          ACT_RCV_NO,
          AD.CNTR_NO,
          @[cop_no] as cop_no,
          TO_CHAR(ACT_DT, 'YYYYMMDDHH24MISS')ACT_DT,
          AD.ACT_STS_MAPG_CD,
          V.COP_DTL_SEQ,
          AD.NOD_CD,
          ACT_RCV_TP_CD ,
          AD.VSL_CD,
          AD.SKD_VOY_NO,
          AD.SKD_DIR_CD,
          AD.VPS_PORT_CD,
          EDI_SND_RSLT_FLG,
          'ACT_MAPG_RPLNV' USR_ID ,
          ACT_UMCH_TP_CD,
          AD.CLPT_IND_SEQ,
          TO_CHAR(ACT_DAT_RCV_DT, 'YYYYMMDDHH24MISS') ACT_DAT_RCV_DT,
          AD.EDI_MSG_TP_CD,
          ROW_NUMBER() OVER (PARTITION BY AD.COP_NO, AD.ACT_STS_MAPG_CD, COP_DTL_SEQ, AD.NOD_CD
            ORDER BY ACT_RCV_DT, ACT_RCV_NO) AS INIT_CNT
        FROM SCE_ACT_RCV_IF AD,
          (
            SELECT SUBSTR(NOD_CD, 1, 5) NOD_CD,
              VSL_CD,
              SKD_VOY_NO,
              SKD_DIR_CD,
              'AT'||SUBSTR(ACT_CD, 5, 1) ACT_STS_MAPG_CD,
              CLPT_IND_SEQ,
              COP_DTL_SEQ
            FROM SCE_COP_DTL
            WHERE COP_NO = @[cop_no]
              AND VSL_CD IS NOT NULL
              AND SKD_VOY_NO IS NOT NULL
              AND SKD_DIR_CD IS NOT NULL
              AND SUBSTR(ACT_CD, 5, 1) IN ('A',
                  'B',
                  'D')
            GROUP BY NOD_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, SUBSTR(ACT_CD, 5, 1), CLPT_IND_SEQ, COP_DTL_SEQ)V
        WHERE AD.VSL_CD = V.VSL_CD
          AND AD.SKD_VOY_NO = V.SKD_VOY_NO
          AND AD.SKD_DIR_CD = V.SKD_DIR_CD
          AND AD.VPS_PORT_CD = V.NOD_CD
          AND AD.CLPT_IND_SEQ = V.CLPT_IND_SEQ
          AND AD.BKG_NO IS NULL
          AND AD.ACT_STS_MAPG_CD = V.ACT_STS_MAPG_CD
          AND AD.ACT_UMCH_TP_CD NOT IN ('00',
              'XX') )
    WHERE INIT_CNT = 1
    GROUP BY ACT_RCV_DT, ACT_RCV_NO, USR_ID, EDI_MSG_TP_CD )			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
