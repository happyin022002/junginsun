<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOsearchDwellNofifySendStsRSQL">
			<desc><![CDATA[searchDwellNofifySendSts]]></desc>
			<sql><![CDATA[
SELECT R.DWLL_CUST_CNT_CD||LPAD(R.DWLL_CUST_SEQ,6,0) AS CUST_CD
, SUM(DECODE(R.DWLL_TM_TP_CD, 'T96', 1)) T96_CNT
, SUM(DECODE(R.DWLL_TM_TP_CD, 'E48', 1)) E48_CNT
, SUM(DECODE(R.DWLL_TM_TP_CD, 'D72', 1)) D72_CNT
, SUM(DECODE(R.DWLL_TM_TP_CD, 'V24', 1)) V24_CNT
FROM (
SELECT EML_SND_DT, DWLL_TM_TP_CD, CNTR_NO, BKG_NO, DWLL_CUST_CNT_CD, DWLL_CUST_SEQ
FROM SCE_DWLL_NTFC_EML_SND_RSLT
WHERE 1=1
AND EML_SND_DT BETWEEN TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('USNYC', TO_DATE(@[start_dt] ||'0300', 'YYYYMMDDHH24MI'), 'KRPUS'), 'YYYYMMDD') 
						AND TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC('USNYC', TO_DATE(@[end_dt]   ||'0300', 'YYYYMMDDHH24MI'), 'KRPUS'), 'YYYYMMDD')
#if (${cust_cnt_cd} != '') 
AND DWLL_CUST_CNT_CD = @[cust_cnt_cd]
#end
#if (${cust_seq} != '')
AND DWLL_CUST_SEQ = @[cust_seq]
#end
GROUP BY EML_SND_DT, DWLL_TM_TP_CD, CNTR_NO, BKG_NO, DWLL_CUST_CNT_CD, DWLL_CUST_SEQ) R
GROUP BY DWLL_CUST_CNT_CD, DWLL_CUST_SEQ
ORDER BY DWLL_CUST_CNT_CD, DWLL_CUST_SEQ			]]></sql>
			<params>
				<param name="start_dt" type="12" value="" out="N"/>
				<param name="end_dt" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
