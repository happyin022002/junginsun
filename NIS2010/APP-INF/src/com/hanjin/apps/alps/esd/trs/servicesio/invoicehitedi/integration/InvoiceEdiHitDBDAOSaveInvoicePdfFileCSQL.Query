<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceEdiHitDBDAOSaveInvoicePdfFileCSQL">
			<desc><![CDATA[INVOICE EDI PDF File 정보 저장]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_INV_EDI_RCV_FILE(
	   INV_EDI_RCV_FILE_SEQ
	  ,FILE_NM
	  ,FILE_SAV_ID
	  ,INV_NO
	  ,INV_VNDR_SEQ
	  ,SAV_CFM_FLG
	  ,FILE_RMK
	  ,DELT_FLG
	  ,CRE_USR_ID
	  ,CRE_DT
	  ,UPD_USR_ID
	  ,UPD_DT
	)VALUES(
	  TO_NUMBER(TO_CHAR(SYSDATE,'YYYYMMDD')||LPAD(TRS_INV_EDI_RCV_FILE_SEQ.NEXTVAL,4,'0'))
	 ,@[file_nm]
	 ,@[file_sav_id]
	 ,@[inv_no]
	 ,@[inv_vndr_seq]
 	 ,@[sav_cfm_flg]
	 ,@[file_rmk]
	 ,'N' -- DELT_FLG
	 ,'HIT_INV_PDF' --cre_usr_id
     ,SYSDATE
     ,'HIT_INV_PDF' --upd_usr_id
     ,SYSDATE
	)			]]></sql>
			<params>
				<param name="file_nm" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_vndr_seq" type="12" value="" out="N"/>
				<param name="sav_cfm_flg" type="12" value="" out="N"/>
				<param name="file_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
