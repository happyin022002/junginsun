<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceCreationDetailDBDAOgetCreateOfficeCodeRSQL">
			<desc><![CDATA[Office Code 조회]]></desc>
			<sql><![CDATA[
SELECT inv_ofc_cd 
  FROM trs_trsp_inv_ofc
 WHERE ofc_cd = @[ofc_cd]
   AND ROWNUM = 1			]]></sql>
