<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BiddingCandidateRegistrationDBDAOdelSpotBidCnddtVndrUSQL">
			<desc><![CDATA[delSpotBidCnddtVndr]]></desc>
			<sql><![CDATA[
DELETE 
  FROM TRS_SPOT_BID_CNDDT_VNDR
 WHERE 1=1
   AND SPOT_BID_CNDDT_TERM_SEQ = @[spot_bid_cnddt_term_seq]
#if(${vndr_seq} != '' && ${vndr_seq} != 'null')
   AND VNDR_SEQ = @[vndr_seq]
#end			]]></sql>
			<params>
				<param name="spot_bid_cnddt_term_seq" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
