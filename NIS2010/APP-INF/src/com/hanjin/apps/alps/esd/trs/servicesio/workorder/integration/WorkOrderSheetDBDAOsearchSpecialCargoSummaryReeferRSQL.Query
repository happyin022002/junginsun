<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderSheetDBDAOsearchSpecialCargoSummaryReeferRSQL">
			<desc><![CDATA[search Special Cargo Summary Reefer]]></desc>
			<sql><![CDATA[
SELECT
        ROW_ID AS RF_RNUM,
        CNTR.EQ_NO AS RF_EQ_NO,
        to_char(RF.CDO_TEMP,'990.0') AS RF_TEMPER_C,
        to_char(RF.FDO_TEMP,'990.0') AS RF_TEMPER_F,
        --DECODE(RF.VENT_RTO,0,TO_CHAR(NVL(RF.NEW_UUSD_VENT_NO,0))||'% OPEN',TO_CHAR(RF.NEW_VENT_NO)||'CMH') AS RF_VENT
		TO_CHAR(RF.VENT_RTO)||'CMH'  AS RF_VENT
FROM
  (
  SELECT
          SO.EQ_NO,
          SO.SPCL_CGO_CNTR_TP_CD,
          SO.BKG_NO,
          SO.TRO_SEQ,
          ROWNUM AS ROW_ID
  FROM
          TRS_TRSP_WRK_ORD WO,
          TRS_TRSP_SVC_ORD SO
    WHERE WO.TRSP_WO_OFC_CTY_CD                   = SO.TRSP_WO_OFC_CTY_CD
      AND WO.TRSP_WO_SEQ                          = SO.TRSP_WO_SEQ
	  AND (wo.TRSP_WO_OFC_CTY_CD,wo.TRSP_WO_SEQ)	= ((@[trsp_wo_ofc_cty_cd],@[trsp_wo_seq]))
#if (${wo_vndr_seq} != '')
	and   wo.wo_vndr_seq  =  @[wo_vndr_seq]
#end
  ) CNTR,
BKG_RF_CGO RF
WHERE 1=1
AND   CNTR.BKG_NO                                 = RF.BKG_NO
AND   RF.RC_SEQ                                   = DECODE(SUBSTR(CNTR.TRO_SEQ,2,1)
                                                    ,'E',TO_NUMBER(SUBSTR(CNTR.TRO_SEQ,3,LENGTH(CNTR.TRO_SEQ)-4))
                                                    ,RF.RC_SEQ)
AND   NVL(CNTR.EQ_NO,'-STORMBOY-')                = DECODE(SUBSTR(CNTR.TRO_SEQ,2,1),'E'
                                                    ,NVL(CNTR.EQ_NO,'-STORMBOY-'),RF.CNTR_NO)
AND   NVL(CNTR.SPCL_CGO_CNTR_TP_CD,'-STORMBOY-')             =  DECODE(SUBSTR(CNTR.TRO_SEQ,2,1),'E'
                                                    ,NVL(CNTR.SPCL_CGO_CNTR_TP_CD,'-STORMBOY-'),'RF')			]]></sql>
			<params>
				<param name="trsp_wo_ofc_cty_cd" type="12" value="1111" out="N"/>
				<param name="trsp_wo_seq" type="2" value="11111111" out="N"/>
				<param name="wo_vndr_seq" type="2" value="105292" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
