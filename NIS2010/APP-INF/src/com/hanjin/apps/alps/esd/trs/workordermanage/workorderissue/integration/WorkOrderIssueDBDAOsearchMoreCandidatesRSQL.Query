<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderIssueDBDAOsearchMoreCandidatesRSQL">
			<desc><![CDATA[searchMoreCandidates]]></desc>
			<sql><![CDATA[
SELECT X.TRSP_AGMT_OFC_CTY_CD
	  ,X.TRSP_AGMT_SEQ
      ,X.TRSP_AGMT_OFC_CTY_CD||X.TRSP_AGMT_SEQ AGMT_NO
	  ,X.WAY_TYPE
	  ,X.VNDR_TP_CD
	  ,X.VNDR_SEQ
	  ,X.VNDR_NM
	  ,X.RJT_HIST
	  ,X.CURR_CD
	  ,X.BASIC_RATE
	  ,X.WTR_RCV_TERM_CD
	  ,X.WTR_DE_TERM_CD
	  ,X.TRSP_AGMT_RT_TP_CD
	  ,X.TRSP_AGMT_RT_TP_NM
	  ,X.CUST_NOMI_TRKR_FLG
	  ,X.CUST_CD
	  ,X.FUEL_SCG_RT
	  ,X.VAT_SCG_RT
	  ,X.TOLL_FEE_RT
	  ,X.TRSP_AGMT_RT_SEQ
	  ,X.CFM_FLG
	  ,X.TRSP_AGMT_UPD_DT
      ,(BASIC_RATE+NVL(FUEL_SCG_RT,0)+NVL(VAT_SCG_RT,0)+NVL(TOLL_FEE_RT,0))    TOT_AMOUNT
      ,(TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(CURR_CD, (BASIC_RATE+NVL(FUEL_SCG_RT,0)+NVL(VAT_SCG_RT,0)+NVL(TOLL_FEE_RT,0)))) TOT_USD_AMOUNT
      ,CASE WHEN VNDR_SEQ = @[vndr_seq] THEN 'Preset'
            ELSE 
            TO_CHAR(RANK() OVER(ORDER BY (TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(CURR_CD, (BASIC_RATE+NVL(FUEL_SCG_RT,0)+NVL(VAT_SCG_RT,0)+NVL(TOLL_FEE_RT,0)))) ASC))
       END SEQ
      ,NVL((SELECT HZD_MTRL_FLG
	          FROM TRS_SPCL_CGO_AVAL_SVC_PROV
    	     WHERE VNDR_SEQ = X.VNDR_SEQ
        	   AND SO_CRE_OFC_CD = @[ctrl_ofc_cd]
           	   AND ROWNUM =1 ),'N') HZD_MTRL_FLG
      ,NVL((SELECT OVWT_TRI_AXL_FLG
              FROM TRS_SPCL_CGO_AVAL_SVC_PROV
             WHERE VNDR_SEQ = X.VNDR_SEQ
               AND SO_CRE_OFC_CD = @[ctrl_ofc_cd]
               AND ROWNUM =1 ),'N') OVWT_TRI_AXL_FLG
	  ,X.EFF_FM_DT
	  ,X.EFF_TO_DT
      ,X.AGMT_REF_NO
	  ,NVL((SELECT MAX(USR_NM) FROM COM_USER A WHERE A.USR_ID = X.TRSP_AGMT_UPD_USR_ID), X.TRSP_AGMT_UPD_USR_ID) TRSP_AGMT_UPD_USR_ID
	  ,RANK() OVER(ORDER BY (TRS_COMMON_PKG.GET_CONVERSION_USD_AMT_FNC(CURR_CD, (BASIC_RATE+NVL(FUEL_SCG_RT,0)+NVL(VAT_SCG_RT,0)+NVL(TOLL_FEE_RT,0)))) ASC) RANK_AMT
  FROM TABLE( TRS_AGMT_RATE_MORE_PKG.GET_MORE_CANDIDATES_LIST_FNC(
              @[trsp_so_ofc_cty_cd]
             ,@[trsp_so_seq]
             ,@[ctrl_ofc_cd]
             ,@[vndr_seq]
             ,@[basis_dt]
             ,@[way_tp_cd]
             ,@[eq_knd_cd]
             ,@[eq_tp_sz_cd]
             ,@[cmb_tp_cd]
             ,@[cgo_tp_cd]
             ,@[bound_cd]
             ,@[crr_mod_cd]
             ,@[cost_mod_cd]
             ,NULL
             ,@[cmdt_cd]
             ,@[from_nod_cd]
             ,@[via_nod_cd]
             ,@[door_nod_cd]
             ,@[to_nod_cd]
             ,@[bundle_cnt]
             ,@[wgt_uom]
             ,@[wgt_qty]
             ,@[cust_cnt_cd]||@[cust_seq]
             ,'N'
             )
       ) X
#if(${mlt_mor_ony_flg} == 'Y')
	WHERE X.VNDR_TP_CD = 'HJS'
#end			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="basis_dt" type="12" value="" out="N"/>
				<param name="way_tp_cd" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_tp_sz_cd" type="12" value="" out="N"/>
				<param name="cmb_tp_cd" type="12" value="" out="N"/>
				<param name="cgo_tp_cd" type="12" value="" out="N"/>
				<param name="bound_cd" type="12" value="" out="N"/>
				<param name="crr_mod_cd" type="12" value="" out="N"/>
				<param name="cost_mod_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="from_nod_cd" type="12" value="" out="N"/>
				<param name="via_nod_cd" type="12" value="" out="N"/>
				<param name="door_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="bundle_cnt" type="12" value="" out="N"/>
				<param name="wgt_uom" type="12" value="" out="N"/>
				<param name="wgt_qty" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
