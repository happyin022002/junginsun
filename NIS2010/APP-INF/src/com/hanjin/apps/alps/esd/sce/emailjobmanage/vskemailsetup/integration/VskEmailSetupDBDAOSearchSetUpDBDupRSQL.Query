<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VskEmailSetupDBDAOSearchSetUpDBDupRSQL">
			<desc><![CDATA[VSL E-MAIL 대상 추가 및 변경에 대한 방법 체크]]></desc>
			<sql><![CDATA[
SELECT
    A.EML_JB_ID    
FROM
    SCE_VSL_SKD_EML A,
    SCE_EML_JB_SUBSC B,
    SCE_EML_JB_GRP C
WHERE A.EML_JB_ID = B.EML_JB_ID
AND A.SVC_ST_DT = B.SVC_ST_DT
AND A.SVC_END_DT = B.SVC_END_DT
AND A.EML_GRP_ID = B.EML_GRP_ID
AND A.VSL_SLAN_CD = B.VSL_SLAN_CD
AND A.TO_PORT_CD = B.TO_PORT_CD
AND B.EML_JB_ID = C.EML_JB_ID
AND B.SVC_ST_DT = C.SVC_ST_DT
AND B.SVC_END_DT = C.SVC_END_DT
AND B.EML_GRP_ID = C.EML_GRP_ID
AND A.EML_GRP_ID = @[emlGrpIdHis]
AND A.VSL_SLAN_CD = @[vslSlanCdHis]
AND A.TO_PORT_CD = @[toPortCdHis]
AND A.VSKD_DUR_ID = @[vskdDurIdHis]||'D'
AND C.EML_SND_HR = @[emlSndHrHis]
AND C.EML_SND_DYS_CTNT = @[emlSndDysCtntHis]
AND B.SUBSC_EML = @[subscEmlHis]
AND NVL(A.DELT_FLG, 'N') = 'N'
AND NVL(B.DELT_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="emlGrpIdHis" type="12" value="" out="N"/>
				<param name="vslSlanCdHis" type="12" value="" out="N"/>
				<param name="toPortCdHis" type="12" value="" out="N"/>
				<param name="vskdDurIdHis" type="12" value="" out="N"/>
				<param name="emlSndHrHis" type="12" value="" out="N"/>
				<param name="emlSndDysCtntHis" type="12" value="" out="N"/>
				<param name="subscEmlHis" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
