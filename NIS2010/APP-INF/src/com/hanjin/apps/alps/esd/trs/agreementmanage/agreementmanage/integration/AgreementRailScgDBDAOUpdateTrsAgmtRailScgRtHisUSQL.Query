SET DELT_FLG = @[delt_flg],
UPD_USR_ID = @[UPD_USR_ID],
UPD_DT = SYSDATE,
LOCL_UPD_DT = globaldate_pkg.time_local_ofc_fnc(@[USR_OFC_CD])
WHERE 1=1
AND TRSP_AGMT_SCG_SEQ	 = @[trsp_agmt_scg_seq]
AND TRSP_AGMT_RT_HIS_SEQ = @[trsp_agmt_rt_his_seq]
			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="UPD_USR_ID" type="12" value="" out="N"/>
				<param name="USR_OFC_CD" type="12" value="" out="N"/>
				<param name="trsp_agmt_scg_seq" type="12" value="" out="N"/>
				<param name="trsp_agmt_rt_his_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
