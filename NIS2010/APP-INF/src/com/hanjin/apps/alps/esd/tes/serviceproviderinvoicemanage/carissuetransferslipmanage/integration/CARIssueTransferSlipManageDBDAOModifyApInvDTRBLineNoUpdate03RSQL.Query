<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CARIssueTransferSlipManageDBDAOModifyApInvDTRBLineNoUpdate03RSQL">
			<desc><![CDATA[ModifyApInvDTRBLineNoUpdate03]]></desc>
			<sql><![CDATA[
SELECT ATTR_CTNT1, DTRB_COA_ACCT_CD, DTRB_COA_VVD_CD, FTU_USE_CTNT1, ROWNUM LINE_NO
 FROM (
	--[CHM-201536111]CSR I/F할 때 Line No 반복되지 않도록 로직 변경
	SELECT DISTINCT ATTR_CTNT1, DTRB_COA_ACCT_CD, DTRB_COA_VVD_CD, NVL(FTU_USE_CTNT1,'N') FTU_USE_CTNT1
	FROM   AP_INV_DTRB
	WHERE  CSR_NO = @[csr_no]
	ORDER BY ATTR_CTNT1, DTRB_COA_VVD_CD DESC
 )			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
