<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisDBDAOGetPoolNameRSQL">
			<desc><![CDATA[샤시 Pool 코드에 대한 Pool Name을 가져온다]]></desc>
			<sql><![CDATA[
SELECT CHSS_POOL_NM    
   FROM TRS_TRSP_CHSS_POOL 
  WHERE CHSS_POOL_CD = @[chss_pool_cd]			]]></sql>
			<params>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
