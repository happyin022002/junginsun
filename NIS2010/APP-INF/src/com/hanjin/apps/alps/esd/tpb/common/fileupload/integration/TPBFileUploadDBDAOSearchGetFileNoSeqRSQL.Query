<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TPBFileUploadDBDAOSearchGetFileNoSeqRSQL">
			<desc><![CDATA[SearchGetFileNoSeq]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(file_no_seq),0) + 1 AS file_no_seq
  FROM TPB_TTL_FILE_MGMT
 WHERE file_no = NVL(@[s_file_no],'0')			]]></sql>
			<params>
				<param name="s_file_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
