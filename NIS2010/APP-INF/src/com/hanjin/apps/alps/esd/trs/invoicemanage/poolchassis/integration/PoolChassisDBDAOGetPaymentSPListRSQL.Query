<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PoolChassisDBDAOGetPaymentSPListRSQL">
			<desc><![CDATA[Payment VNDR Info을 가져온다]]></desc>
			<sql><![CDATA[
SELECT CHSS_POOL_CD           
 	    ,INV_VNDR_SEQ           
 	    ,CHSS_POOL_NM             
 	    ,VNDR_CNT_CD            
 	    ,INV_PAY_VNDR_NM        
 FROM TRS_TRSP_CHSS_POOL       
 WHERE CHSS_POOL_CD = @[chss_pool_cd]       
 AND NVL(DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
