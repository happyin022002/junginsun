<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchOdSoByCopRSQL">
			<desc><![CDATA[COP No 로 OD-SO 를 조회한다.]]></desc>
			<sql><![CDATA[
select
trsp_so_ofc_cty_cd,
trsp_so_seq,
cop_no,
cost_act_grp_seq
from 
trs_trsp_svc_ord
where cop_no = @[cop_no]
and cost_act_grp_cd like 'OD%'
and nvl(delt_flg, 'N') = 'N'
and nvl(TRSP_FRST_FLG, 'N') = 'N'

union all

select
trsp_so_ofc_cty_cd,
trsp_so_seq,
cop_no,
cost_act_grp_seq
from 
trs_trsp_rail_bil_ord
where cop_no = @[cop_no]
and cost_act_grp_cd like 'OD%'
and nvl(delt_flg, 'N') = 'N'			]]></sql>
			<params>
				<param name="cop_no" type="12" value="CCHI7506115532" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
