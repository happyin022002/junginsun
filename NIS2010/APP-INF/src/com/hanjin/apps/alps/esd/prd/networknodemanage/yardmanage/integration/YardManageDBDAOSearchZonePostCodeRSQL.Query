<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="YardManageDBDAOSearchZonePostCodeRSQL">
			<desc><![CDATA[SearchZonePostCode]]></desc>
			<sql><![CDATA[
SELECT zn_cd, zn_seq, zip_cd, dstr_nm
FROM   mdm_zn_dtl
WHERE  zn_cd = @[node_code]
			]]></sql>
			<params>
				<param name="node_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
