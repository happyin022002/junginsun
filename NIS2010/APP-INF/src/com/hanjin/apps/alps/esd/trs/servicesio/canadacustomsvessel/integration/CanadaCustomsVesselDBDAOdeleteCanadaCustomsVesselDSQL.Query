<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanadaCustomsVesselDBDAOdeleteCanadaCustomsVesselDSQL">
			<desc><![CDATA[해당 조건의 EDI_CND_CSTMS_VSL 데이타 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM EDI_CND_CSTMS_VSL
WHERE 	VSL_CD  			= @[vsl_cd] 
AND 	SKD_VOY_NO  		= @[skd_voy_no] 
AND 	SKD_DIR_CD  		= @[skd_dir_cd] 
AND 	NVL(EAI_EVNT_DT, to_date(20070505,'yyyymmddhh24miss'))  <= to_date(@[eai_evnt_dt],'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
