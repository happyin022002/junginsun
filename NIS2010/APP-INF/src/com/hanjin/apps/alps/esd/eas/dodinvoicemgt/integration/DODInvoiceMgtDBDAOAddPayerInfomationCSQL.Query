<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DODInvoiceMgtDBDAOAddPayerInfomationCSQL">
			<desc><![CDATA[AddPayerInfomation]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_PAYR_INFO (
	CUST_CNT_CD
,	CUST_SEQ
,	PAYR_NM  
,	CNTC_PNT_NM
,	CUST_ADDR
,	ZIP_CD_CTNT
,	CNTC_PNT_PHN_NO
,	CNTC_PNT_FAX_NO
,	EAS_PAYR_VNDR_FLG
,	N1ST_EML
,	N2ND_EML
,	N3RD_EML
,	EAS_PAYR_OTS_RMK
,	CRE_USR_ID
,	CRE_DT
,	CRE_OFC_CD
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
) VALUES( 
	@[cust_cnt_cd]
,	@[cust_seq]
,	@[payr_nm]
,	@[cntc_pnt_nm]
,	@[cust_addr]
,	@[zip_cd_ctnt]
,	@[cntc_pnt_phn_no]
,	@[cntc_pnt_fax_no]
,	@[eas_payr_vndr_flg]
,	@[n1st_eml]
,	@[n2nd_eml]
,	@[n3rd_eml]
,	@[eas_payr_ots_rmk]
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
,	@[cre_ofc_cd]
,	@[upd_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
,	@[upd_ofc_cd]
)			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="payr_nm" type="12" value="" out="N"/>
				<param name="cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="cust_addr" type="12" value="" out="N"/>
				<param name="zip_cd_ctnt" type="12" value="" out="N"/>
				<param name="cntc_pnt_phn_no" type="12" value="" out="N"/>
				<param name="cntc_pnt_fax_no" type="12" value="" out="N"/>
				<param name="eas_payr_vndr_flg" type="12" value="" out="N"/>
				<param name="n1st_eml" type="12" value="" out="N"/>
				<param name="n2nd_eml" type="12" value="" out="N"/>
				<param name="n3rd_eml" type="12" value="" out="N"/>
				<param name="eas_payr_ots_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
