<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementCorrectionDBDAOCfmAgmtHisUSQL">
			<desc><![CDATA[Agreement History Confirm (Confirm에는 영향이 없으나 결재 history 관리를 위하여 기록..속도에 영향이 있을 경우 제외시켜도 무방)]]></desc>
			<sql><![CDATA[
UPDATE TRS_AGMT_EQ_RT_HIS A
   SET A.CFM_FLG = 'Y'
      ,A.CFM_USR_ID = @[upd_usr_id]
      ,A.CFM_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
      ,A.UPD_USR_ID        = @[upd_usr_id]
      ,A.UPD_DT            = SYSDATE
 WHERE A.TRSP_AGMT_OFC_CTY_CD = @[trsp_agmt_ofc_cty_cd]
   AND A.TRSP_AGMT_SEQ        = @[trsp_agmt_seq]
   AND A.TRSP_AGMT_RT_TP_SER_NO = @[trsp_agmt_rt_tp_ser_no]
   AND (A.TRSP_AGMT_RT_SEQ, A.TRSP_AGMT_RT_HIS_SEQ) IN (
                            SELECT X.TRSP_AGMT_RT_SEQ
                                  ,(SELECT MAX(Y.TRSP_AGMT_RT_HIS_SEQ)
                                      FROM TRS_AGMT_EQ_RT_HIS Y
                                     WHERE Y.TRSP_AGMT_OFC_CTY_CD = X.TRSP_AGMT_OFC_CTY_CD
                                       AND Y.TRSP_AGMT_SEQ        = X.TRSP_AGMT_SEQ
                                       AND Y.TRSP_AGMT_RT_TP_SER_NO = X.TRSP_AGMT_RT_TP_SER_NO
                                       AND Y.TRSP_AGMT_RT_SEQ     = X.TRSP_AGMT_RT_SEQ
                                    )
                              FROM TRS_AGMT_EQ_RT X
                             WHERE 1=1
                               AND X.TRSP_AGMT_OFC_CTY_CD = @[trsp_agmt_ofc_cty_cd]
                               AND X.TRSP_AGMT_SEQ        = @[trsp_agmt_seq]
                               AND X.TRSP_AGMT_RT_TP_SER_NO = @[trsp_agmt_rt_tp_ser_no]
                               AND X.CFM_FLG = 'N'
                            )
			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="trsp_agmt_rt_tp_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
