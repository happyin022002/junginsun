<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchDupChkInvoiceRSQL">
			<desc><![CDATA[invoice 중복 체크]]></desc>
			<sql><![CDATA[
SELECT 							
  A.INV_NO							
  , A.INV_VNDR_SEQ					
  , B.VNDR_LGL_ENG_NM AS INV_VNDR_NM		
  , A.TRSP_INV_AUD_STS_CD				
  , A.GEN_PAY_TERM_CD					
  , TO_CHAR(A.INV_CFM_DT, 'YYYYMMDD') INV_CFM_DT	
  , A.WO_VNDR_SEQ						
  , C.VNDR_LGL_ENG_NM AS WO_VNDR_NM		
  , A.INV_CURR_CD						
  , A.INV_BZC_AMT						
  , A.INV_VAT_AMT						
  , A.INV_SBC_AMT
  , A.INV_WHLD_TAX_AMT				
  , A.INV_TTL_AMT					
  , A.RGST_NO							
  , A.INV_PAY_MZD_CD					
  , A.INV_CHK_TRNS_NO					
  , TO_CHAR(A.PAY_DT, 'YYYYMMDD') PAY_DT		
  , A.INV_RJCT_FLG					
  , TO_CHAR(A.INV_RCV_DT, 'YYYYMMDD') INV_RCV_DT	
  , TO_CHAR(A.INV_EFF_DT, 'YYYYMMDD') INV_EFF_DT	
  , TO_CHAR(A.INV_RJCT_DT, 'YYYYMMDD') INV_RJCT_DT	
  , TO_CHAR(A.INV_ISS_DT, 'YYYYMMDD') INV_ISS_DT	
  , A.IF_SYS_KND_CD
  , A.AP_RVS_CNG_FLG
  , A.IDA_CGST_AMT
  , A.IDA_SGST_AMT
  , A.IDA_IGST_AMT
  , A.IDA_UGST_AMT				
 FROM TRS_TRSP_INV_WRK A			
 , MDM_VENDOR B						
 , MDM_VENDOR C						
 WHERE A.INV_NO = @[inv_no]				
 AND A.INV_VNDR_SEQ = @[paymt_sp_cd]		
 AND A.INV_VNDR_SEQ = B.VNDR_SEQ(+)		
 AND A.WO_VNDR_SEQ = C.VNDR_SEQ(+)			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="paymt_sp_cd" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
