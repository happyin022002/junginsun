<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TesAdvanceAuditDBDAOTesAudCfgUSQL">
			<desc><![CDATA[TesAudCfg]]></desc>
			<sql><![CDATA[
UPDATE EAS_TML_AUTO_AUD_CRTE
   SET EXPN_AUD_TGT_FLG   = DECODE(@[expn_aud_tgt_flg],'0','N','Y')
     , EXPN_MAX_PRMT_RTO  = @[expn_max_prmt_rto]
     , AUD_RMK            = @[aud_rmk]
     , UPD_USR_ID         = @[upd_usr_id]
     , UPD_DT             = SYSDATE
 WHERE AUD_OFC_CD         = @[aud_ofc_cd]
   AND LGS_COST_CD        = @[lgs_cost_cd]			]]></sql>
			<params>
				<param name="expn_aud_tgt_flg" type="12" value="" out="N"/>
				<param name="expn_max_prmt_rto" type="12" value="" out="N"/>
				<param name="aud_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="aud_ofc_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
