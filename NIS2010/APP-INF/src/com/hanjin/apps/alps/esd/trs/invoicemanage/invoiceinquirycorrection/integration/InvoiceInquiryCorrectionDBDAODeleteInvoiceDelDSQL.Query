<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceInquiryCorrectionDBDAODeleteInvoiceDelDSQL">
			<desc><![CDATA[Invoice 삭제]]></desc>
			<sql><![CDATA[
 DELETE FROM TRS_TRSP_INV_WRK 														
 WHERE																				
 INV_NO				= @[inv_no]      								
 AND INV_VNDR_SEQ		= @[inv_vndr_seq]								
		/* 2008.04.29 ETS OPEN */
 AND HJL_NO IS NULL			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
