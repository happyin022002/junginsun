<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WORejectManageDBDAOselectRejectWOSoDataRSQL">
			<desc><![CDATA[selectRejectWOSoData]]></desc>
			<sql><![CDATA[
SELECT																																		
	    	'R'	AS ibflag,																														
			trsp_so_ofc_cty_cd,																													
			trsp_so_seq,																														
	    	NVL(WO_RJCT_FLG,'N') AS wo_cxl_flg,																									
			dtn_use_flg,																														
			wo_bl_no_iss_flg,																													
			vndr_seq,																															
			curr_cd,																															
			bzc_amt,																															
			nego_amt,																															
			etc_add_amt,																														
			fuel_scg_amt,																														
			ovr_wgt_scg_amt,																													
			n3pty_bil_flg																														
	FROM																																		
			TRS_TRSP_SVC_ORD																													
	WHERE   TRSP_WO_OFC_CTY_CD = @[TRSP_WO_OFC_CTY_CD]
	AND		TRSP_WO_SEQ = @[TRSP_WO_SEQ]
	AND		DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="TRSP_WO_OFC_CTY_CD" type="12" value="" out="N"/>
				<param name="TRSP_WO_SEQ" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
