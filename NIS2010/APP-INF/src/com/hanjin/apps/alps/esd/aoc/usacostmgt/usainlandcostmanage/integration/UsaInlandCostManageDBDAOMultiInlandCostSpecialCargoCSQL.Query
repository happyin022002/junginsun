<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInlandCostManageDBDAOMultiInlandCostSpecialCargoCSQL">
			<desc><![CDATA[multiInlandCostSpecialCargo]]></desc>
			<sql><![CDATA[
INSERT INTO AOC_USA_INLND_SPCL_TRF_DTL
        (  COST_TRF_NO
         , CNTR_SZ_CD
         , TRSP_CRR_MOD_CD
         , CURR_CD
         , DG_FX_RT
         , DG_FX_RTO
         , MIN_CGO_WGT
         , MAX_CGO_WGT
         , OVR_WGT_FX_RT
         , OVR_WGT_FX_RTO
         , DCGO_SVC_FLG
         , OVWT_CGO_SVC_FLG
         , CRE_USR_ID
         , CRE_DT
         , UPD_USR_ID
         , UPD_DT
        )
    SELECT HDR.COST_TRF_NO
         , B.CNTR_SZ_CD
         , A.INTG_CD_VAL_CTNT
         , HDR.CURR_CD
         , '0' AS DG_FX_RT
         , '0' AS DG_FX_RTO
         , '0' AS MIN_CGO_WGT
         , '0' AS MAX_CGO_WGT
         , '0' AS OVR_WGT_FX_RT
         , '0' AS OVR_WGT_FX_RTO
         , 'N' AS DCGO_SVC_FLG
         , 'N' AS OVWT_CGO_SVC_FLG
         , @[cre_usr_id]
         , SYSDATE
         , @[upd_usr_id]
         , SYSDATE         
    FROM AOC_USA_INLND_TRF_HDR HDR
       , COM_INTG_CD_DTL A     
       , (    SELECT  '20' CNTR_SZ_CD
              FROM    DUAL
              UNION ALL
              SELECT  '40' CNTR_SZ_CD
              FROM    DUAL
         ) B   
   WHERE 1=1
     AND HDR.COST_TRF_NO = @[in_cost_trf_no]
     AND INTG_CD_ID = DECODE(HDR.IO_BND_CD,'O','CD03067','CD03068')
     AND INTG_CD_VAL_CTNT||CNTR_SZ_CD NOT IN ( SELECT TRSP_CRR_MOD_CD||CNTR_SZ_CD
                                     FROM AOC_USA_INLND_SPCL_TRF_DTL
                                    WHERE COST_TRF_NO = @[in_cost_trf_no]
                                 )			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
