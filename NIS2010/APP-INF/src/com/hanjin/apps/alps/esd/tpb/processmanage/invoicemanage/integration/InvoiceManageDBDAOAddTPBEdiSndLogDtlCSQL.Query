<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOAddTPBEdiSndLogDtlCSQL">
			<desc><![CDATA[AddTPBEdiSndLogDtl]]></desc>
			<sql><![CDATA[
INSERT INTO TPB_EDI_SND_LOG_DTL
(   N3PTY_EDI_SND_IND_CD
   ,SND_DT
   ,FLT_FILE_REF_NO
   ,N3PTY_EDI_SND_SEQ
   ,LOG_DTL_SEQ
   ,N3PTY_NO
   ,N3PTY_INV_NO
   ,EDI_SND_MSG
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
)VALUES(
     @[n3pty_edi_snd_ind_cd]
    ,sysdate
    ,@[flt_file_ref_no]
    ,(  SELECT NVL(MAX(N3PTY_EDI_SND_SEQ),0)+1
        FROM   TPB_EDI_SND_LOG_DTL
        WHERE  1 = 1
        AND    N3PTY_EDI_SND_IND_CD = @[n3pty_edi_snd_ind_cd]
        AND    FLT_FILE_REF_NO = @[flt_file_ref_no]
     )
    ,@[log_dtl_seq]
    ,@[n3pty_no]
    ,@[n3pty_inv_no]
    ,@[edi_snd_msg]
    ,@[cre_usr_id]
    ,SYSDATE
    ,@[upd_usr_id]
    ,SYSDATE
)			]]></sql>
			<params>
				<param name="n3pty_edi_snd_ind_cd" type="12" value="" out="N"/>
				<param name="flt_file_ref_no" type="12" value="" out="N"/>
				<param name="log_dtl_seq" type="12" value="" out="N"/>
				<param name="n3pty_no" type="12" value="" out="N"/>
				<param name="n3pty_inv_no" type="12" value="" out="N"/>
				<param name="edi_snd_msg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
