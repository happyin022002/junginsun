<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOCheckOdRouteDiffRSQL">
			<desc><![CDATA[OD-SO 구간의 route 정보 중 틀린 구간이 존재할 경우 data 가 return 된다.]]></desc>
			<sql><![CDATA[
select N1ST_NOD_CD,
  N2ND_NOD_CD,
  N3RD_NOD_CD,
  N4TH_NOD_CD,
  TRSP_MOD_CD,
  INLND_ROUT_INV_BIL_PATT_CD,
  INV_BIL_PATT_DIV_FLG,
  CTRL_OFC_CD
from sce_pln_so_list
where cop_no = @[fm_cop_no]
  and cost_act_grp_cd like 'OD%'
minus
select N1ST_NOD_CD,
  N2ND_NOD_CD,
  N3RD_NOD_CD,
  N4TH_NOD_CD,
  TRSP_MOD_CD,
  INLND_ROUT_INV_BIL_PATT_CD,
  INV_BIL_PATT_DIV_FLG,
  CTRL_OFC_CD
from sce_pln_so_list
where cop_no = @[to_cop_no]
  and cost_act_grp_cd like 'OD%'			]]></sql>
			<params>
				<param name="fm_cop_no" type="12" value="" out="N"/>
				<param name="to_cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
