<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualRegisterDBDAOSearchVvdCdRSQL">
			<desc><![CDATA[SearchVvdCd]]></desc>
			<sql><![CDATA[
SELECT VSL_CD||SKD_VOY_NO||SKD_DIR_CD AS VVD_CD  
 FROM VSK_VSL_PORT_SKD
WHERE 1=1
  AND VSL_CD = SUBSTR(@[vvd_cd],1,4)
  AND SKD_VOY_NO = SUBSTR(@[vvd_cd],5,4)
  AND SKD_DIR_CD = SUBSTR(@[vvd_cd],9,1)
  AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
