      ,cre_usr_id
      ,upd_usr_id
      ,call_yd_ind_cng_flg
)VALUES(
       TO_CHAR(sysdate,'YYYYMMDD')
      ,sce_act_tml_if_seq1.nextval
      ,@[vsl_cd]
      ,@[skd_voy_no]
      ,@[skd_dir_cd]
      ,@[vps_port_cd]
      ,@[clpt_ind_seq]
      ,@[nod_cd]
