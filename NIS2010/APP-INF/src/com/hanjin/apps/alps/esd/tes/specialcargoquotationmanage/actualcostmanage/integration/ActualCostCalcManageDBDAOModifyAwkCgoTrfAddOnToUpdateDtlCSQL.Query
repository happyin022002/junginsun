<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualCostCalcManageDBDAOModifyAwkCgoTrfAddOnToUpdateDtlCSQL">
			<desc><![CDATA[DTL INSERT]]></desc>
			<sql><![CDATA[
INSERT INTO TES_AWK_CGO_ADON_HDR(
        FM_LOC_CD
        ,FM_NOD_YD_NO
        ,TO_LOC_CD
        ,TO_NOD_YD_NO
        ,COND_NO
        ,TML_AWK_ADON_VER_NO
        ,VNDR_SEQ
        ,VNDR_NM
        ,CALC_RMK
        ,LST_UPD_USR_ID
        ,LST_UPD_DT
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
	) VALUES (
		@[fm_loc_cd],
        CASE WHEN @[fm_nod_yd_no] IS NULL 
        THEN ' '
        ELSE NVL(TRIM(@[fm_nod_yd_no]), ' ')
        END,
        @[to_loc_cd],
        CASE WHEN @[to_nod_yd_no] IS NULL 
        THEN ' '
        ELSE NVL(TRIM(@[to_nod_yd_no]), ' ')
        END,
		@[cond_no],
		@[tml_awk_adon_ver_no],
        '',
        @[vndr_nm],
        @[calc_rmk],
        @[lst_upd_usr_id],
        SYSDATE,
        @[cre_usr_id],
		TO_DATE(@[cre_dt],'YYYYMMDDHH24MISS'), 
        @[upd_usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="fm_loc_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yd_no" type="12" value="" out="N"/>
				<param name="to_loc_cd" type="12" value="" out="N"/>
				<param name="to_nod_yd_no" type="12" value="" out="N"/>
				<param name="cond_no" type="12" value="" out="N"/>
				<param name="tml_awk_adon_ver_no" type="12" value="" out="N"/>
				<param name="vndr_nm" type="12" value="" out="N"/>
				<param name="calc_rmk" type="12" value="" out="N"/>
				<param name="lst_upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
