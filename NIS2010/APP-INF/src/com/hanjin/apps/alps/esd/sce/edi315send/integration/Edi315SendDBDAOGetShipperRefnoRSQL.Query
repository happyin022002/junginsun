<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetShipperRefnoRSQL">
			<desc><![CDATA[for getShipperRefno]]></desc>
			<sql><![CDATA[
select REF_NO SH_REF_NBR
from bkg_customer 
where 1=1 
and BKG_NO        = @[e_bkg_no]
and BKG_CUST_TP_CD  = 'S'			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
