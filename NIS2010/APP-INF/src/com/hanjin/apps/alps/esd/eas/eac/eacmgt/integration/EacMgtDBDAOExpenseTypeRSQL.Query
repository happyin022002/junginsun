<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOExpenseTypeRSQL">
			<desc><![CDATA[ExpenseType Code Search]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_CTNT AS code_cd
     , INTG_CD_VAL_DP_DESC  AS code_nm
FROM COM_INTG_CD_DTL
WHERE INTG_CD_ID =@[code_key]
ORDER BY INTG_CD_VAL_DP_SEQ

			]]></sql>
			<params>
				<param name="code_key" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
