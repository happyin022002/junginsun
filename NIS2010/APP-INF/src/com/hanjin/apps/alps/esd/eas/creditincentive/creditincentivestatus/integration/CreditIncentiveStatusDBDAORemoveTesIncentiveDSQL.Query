<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAORemoveTesIncentiveDSQL">
			<desc><![CDATA[TES Incentive Delete]]></desc>
			<sql><![CDATA[
UPDATE EAS_TML_INCNT
   SET DELT_FLG = 'Y'
      ,UPD_USR_ID = @[usr_id]
      ,UPD_DT = SYSDATE
 WHERE BSE_YR = @[bse_yr]
   AND INCNT_NO = @[incnt_no]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="bse_yr" type="12" value="" out="N"/>
				<param name="incnt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
