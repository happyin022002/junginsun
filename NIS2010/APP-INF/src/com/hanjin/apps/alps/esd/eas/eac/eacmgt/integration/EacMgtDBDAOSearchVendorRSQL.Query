<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchVendorRSQL">
			<desc><![CDATA[S/P명을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ,
       VNDR_LGL_ENG_NM VNDR_NM
  FROM MDM_VENDOR
WHERE 1=1
  AND VNDR_SEQ= @[s_vndr_seq]
  AND NVL(DELT_FLG, 'N') <> 'Y'

			]]></sql>
			<params>
				<param name="s_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
