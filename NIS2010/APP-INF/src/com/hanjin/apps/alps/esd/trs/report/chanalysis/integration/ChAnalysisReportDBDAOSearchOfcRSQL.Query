<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChAnalysisReportDBDAOSearchOfcRSQL">
			<desc><![CDATA[office 검색결과값을 리턴함]]></desc>
			<sql><![CDATA[
SELECT                                            				
		OFC_CD		                                       				
FROM  	MDM_ORGANIZATION                                  		
WHERE 	OFC_KND_CD > 1	                                       	
		AND DELT_FLG = 'N'                                         	
START 	WITH OFC_CD IN 			  								
		(
			SELECT	OFC_CD 
			FROM	MDM_ORGANIZATION			  				
			WHERE	OFC_KND_CD = 3

#if(${SEL_RHQMODE} != 'ALL') 
					AND PRNT_OFC_CD = @[HID_BOUNDMODE]
#end 

		)

CONNECT BY PRIOR OFC_CD = PRNT_OFC_CD			]]></sql>
			<params>
				<param name="HID_BOUNDMODE" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
