<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOSearchDwllNtfcExptCntMastBkgRSQL">
			<desc><![CDATA[Container NO로 Master Bkg_no을 조회]]></desc>
			<sql><![CDATA[
SELECT BKG_NO FROM SCE_COP_HDR A
WHERE 1=1
AND EXISTS (SELECT 'X' FROM SCE_COP_HDR B WHERE 1=1
AND (B.CNTR_NO, B.TRNK_VSL_CD,B.TRNK_SKD_VOY_NO,B.TRNK_SKD_DIR_CD) IN ((A.CNTR_NO, A.TRNK_VSL_CD,A.TRNK_SKD_VOY_NO,A.TRNK_SKD_DIR_CD))
AND B.COP_STS_CD NOT IN ('F','X')
)
AND A.COP_NO = A.MST_COP_NO
AND A.COP_STS_CD NOT IN ('F','X')
AND A.CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
