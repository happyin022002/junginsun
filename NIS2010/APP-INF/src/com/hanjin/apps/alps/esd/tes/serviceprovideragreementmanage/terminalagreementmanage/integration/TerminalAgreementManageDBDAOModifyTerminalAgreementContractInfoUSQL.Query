<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOModifyTerminalAgreementContractInfoUSQL">
			<desc><![CDATA[GroupWare Contract Info Modification]]></desc>
			<sql><![CDATA[
UPDATE TES_TML_AGMT_HDR SET
		  upd_usr_id			= @[upd_usr_id]
		, upd_dt 				= SYSDATE
		, locl_upd_dt			= GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])
#if (${agmt_doc_no} == '')
		, agmt_doc_no			= NULL
		, agmt_doc_desc			= NULL
		, agmt_doc_eff_fm_dt	= NULL
		, agmt_doc_eff_to_dt	= NULL
#else
-- 비용지급 전표 결재 기능 - 3차 추가(4347-09-25)
		, agmt_doc_no			= @[agmt_doc_no]
		, agmt_doc_desc			= @[agmt_doc_desc]
		, agmt_doc_eff_fm_dt	= REPLACE(@[agmt_doc_eff_fm_dt], '-', '')
		, agmt_doc_eff_to_dt	= REPLACE(@[agmt_doc_eff_to_dt], '-', '')
#end
WHERE	tml_agmt_ofc_cty_cd = @[tml_agmt_ofc_cty_cd]
AND		tml_agmt_seq		= @[tml_agmt_seq]
AND		tml_agmt_ver_no		= @[tml_agmt_ver_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="agmt_doc_no" type="12" value="" out="N"/>
				<param name="agmt_doc_desc" type="12" value="" out="N"/>
				<param name="agmt_doc_eff_fm_dt" type="12" value="" out="N"/>
				<param name="agmt_doc_eff_to_dt" type="12" value="" out="N"/>
				<param name="tml_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_agmt_seq" type="12" value="" out="N"/>
				<param name="tml_agmt_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
