<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DODInvoiceMgtDBDAOAddPayerContactPointCSQL">
			<desc><![CDATA[AddPayerContactPoint]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_PAYR_CNTC_PNT (
	CUST_CNT_CD
,	CUST_SEQ
,	CUST_CNTC_PNT_SEQ
,	CNTC_PNT_NM
,	CNTC_PNT_PHN_NO
,	CNTC_PNT_FAX_NO
,	CNTC_PNT_EML
,	CRE_USR_ID
,	CRE_DT
,	CRE_OFC_CD
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
) VALUES( 
	@[cust_cnt_cd]
,	@[cust_seq]
,	(SELECT NVL(MAX(CUST_CNTC_PNT_SEQ),0)+1 
     FROM EAS_PAYR_CNTC_PNT
     WHERE CUST_CNT_CD 	= @[cust_cnt_cd]
    	 AND CUST_SEQ	= @[cust_seq])
,	@[cntc_pnt_nm]
,	@[cntc_pnt_phn_no]
,	@[cntc_pnt_fax_no]
,	@[cntc_pnt_eml]
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
,	@[cre_ofc_cd]
,	@[upd_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
,	@[upd_ofc_cd]
)			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="cntc_pnt_phn_no" type="12" value="" out="N"/>
				<param name="cntc_pnt_fax_no" type="12" value="" out="N"/>
				<param name="cntc_pnt_eml" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
