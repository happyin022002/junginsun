		<query name="MnrAdvanceAuditDBDAOgetMovementEmailSendJobIdRSQL">
			<desc><![CDATA[20160420 HongSeongPill max(job_id) + 1 값을 가져온다.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(SYSDATE,'YYYYMMDD')||LPAD( (NVL(TO_NUMBER(SUBSTR(MAX(JB_ID),9,7)),0) + 1),7,'0' ) AS JB_ID
FROM   EAS_MVMT_EML_SND_HIS 
WHERE  SUBSTR(JB_ID,0,8) = TO_CHAR(SYSDATE,'YYYYMMDD') 			]]></sql>
			<params>
