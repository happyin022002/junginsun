<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairRouteDBDAOSearchPortPairMastersRSQL">
			<desc><![CDATA[Port pair master table 조회한다.]]></desc>
			<sql><![CDATA[
SELECT cust_trd_prnr_id                             
      ,DECODE(use_flg, 'Y', 'N', 'N', 'Y') AS del   	
      ,por_cd                                       
      ,pol_cd                                       
      ,pod_cd                                       
      ,del_cd                                       
      ,cre_usr_id                                   
      ,use_flg                                      	
      ,TO_CHAR(cre_dt, 'YYYYMMDDHH24MI') AS cre_dt  
      ,upd_usr_id                                   
      ,TO_CHAR(upd_dt, 'YYYYMMDDHH24MI') AS upd_dt  
  FROM sce_port_pair_mst                            
 WHERE cust_trd_prnr_id = @[partner_id]         
 
 /* condition - POR */
 #if (${por_port_cd} != '') 
 	AND por_cd = @[por_port_cd]
 #end
 
  /* condition - POL */
  #if (${pol_port_cd} != '') 
 	AND pol_cd = @[pol_port_cd]
 #end
 
  /* condition - POD */
  #if (${pod_port_cd} != '') 
 	AND pod_cd = @[pod_port_cd]
 #end
 
  /* condition - DEL */
  #if (${del_port_cd} != '') 
 	AND del_cd = @[del_port_cd]
 #end
 
  /* condition - USE FLAG */
  #if (${use_flg} != '') 
 	AND use_flg = @[use_flg]
 #end			]]></sql>
			<params>
				<param name="partner_id" type="12" value="" out="N"/>
				<param name="por_port_cd" type="12" value="" out="N"/>
				<param name="pol_port_cd" type="12" value="" out="N"/>
				<param name="pod_port_cd" type="12" value="" out="N"/>
				<param name="del_port_cd" type="12" value="" out="N"/>
				<param name="use_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
