<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchHandleOfficeListRSQL">
			<desc><![CDATA[SearchHandleOfficeList]]></desc>
			<sql><![CDATA[
SELECT ofc_cd, 
       ofc_cd
  FROM TPB_HNDL_OFC A
 WHERE 1=1
   AND n3pty_ofc_tp_cd = 'T'
   AND delt_flg = 'N' 
#if (${s_if_rhq_cd} != '')
   AND rhq_cd = @[s_if_rhq_cd]
#end
#if (${s_if_ctrl_cd} != '')
   AND n3pty_ctrl_ofc_cd = @[s_if_ctrl_cd]
#end
 ORDER BY A.OFC_CD			]]></sql>
			<params>
				<param name="s_if_rhq_cd" type="12" value="" out="N"/>
				<param name="s_if_ctrl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
