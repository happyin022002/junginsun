<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EdiHoldStatusReceiveDBDAOGetCountRSQL">
			<desc><![CDATA[Hold Status 정보가 있는지 확인한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM SCE_RAIL_HLD_STS
 WHERE BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
