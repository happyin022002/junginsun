<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="COPSearchDBDAOSearchBookingVVDDetailRSQL">
			<desc><![CDATA[booking info]]></desc>
			<sql><![CDATA[
SELECT vsl_pre_pst_cd
     , vsl_cd									
     , skd_voy_no							
     , skd_dir_cd 								
  FROM bkg_vvd							 	
 WHERE bkg_no =  @[bkg_no] 				]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
