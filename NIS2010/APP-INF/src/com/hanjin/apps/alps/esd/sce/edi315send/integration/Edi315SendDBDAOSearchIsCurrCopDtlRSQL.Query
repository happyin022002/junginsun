<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchIsCurrCopDtlRSQL">
			<desc><![CDATA[Edi315SendDBDAOSearchIsCurrCopDtlRSQL]]></desc>
			<sql><![CDATA[
select  
      #if(${e_edi_sts} == 'VE' && ${e_org_sts} =='VAT' && ${e_edi_grpcd} =='EUR00190')
        to_char(d.ACT_DT,'yyyymmddhh24miss') CURR_EVENT_DT
      #elseif(${e_edi_sts} == 'VBE' && ${e_org_sts} =='VDL' && 
         (${e_edi_grpcd} =='COM02218' || ${e_edi_grpcd} =='USA00512' || ${e_edi_grpcd} =='USA00607' || ${e_edi_grpcd} =='USA00094'))
		to_char(d.ESTM_DT,'yyyymmddhh24miss') CURR_EVENT_DT
      #else
        to_char(DECODE(@[e_edi_sts] ,'VE',d.ESTM_DT,'AG',d.ESTM_DT ,'AD',d.ESTM_DT,d.ACT_DT),'yyyymmddhh24miss') CURR_EVENT_DT
      #end
      , d.NOD_CD                                      CURR_EVENT_YARD
      , d.COP_DTL_SEQ                                 CURR_COP_DTL_SEQ
	  ,'' CURR_STS
from sce_cop_dtl d, sce_cop_hdr h, bkg_booking b 
where 1=1
and  d.cop_no = @[e_cop_no]
and  d.cop_no = h.cop_no
and  h.bkg_no = b.bkg_no
#if(${e_edi_sts} == 'AG' || ${e_edi_sts} == 'AD')
AND d.act_cd = 'FITZAD'  
#else
 #if(${e_edi_sts} == 'VE' && ${e_org_sts} =='VAT' && ${e_edi_grpcd} =='EUR00190')
and (d.STND_EDI_STS_CD = DECODE(@[e_edi_sts] ,'VE','VAT',@[e_edi_sts])
 #elseif(${e_edi_sts} == 'VBE' && ${e_org_sts} =='VDL' && 
    (${e_edi_grpcd} =='COM02218' || ${e_edi_grpcd} =='USA00512' || ${e_edi_grpcd} =='USA00607' || ${e_edi_grpcd} =='USA00094'))
and (d.STND_EDI_STS_CD = DECODE(@[e_edi_sts] ,'VBE','VBD',@[e_edi_sts])
 #else
and (d.STND_EDI_STS_CD = DECODE(@[e_edi_sts] ,'VE','VAD',@[e_edi_sts])
 #end 
  or d.ACT_CD = @[e_edi_sts]
  or d.ACT_STS_MAPG_CD = @[e_edi_sts]
)
#end
AND  d.NOD_CD = DECODE(@[e_edi_sts] ,'VE',d.NOD_CD
									,'AG',d.NOD_CD
									,'AD',d.NOD_CD
									,'VBE',d.NOD_CD
					  ,@[e_event_yard])			]]></sql>
			<params>
				<param name="e_edi_sts" type="12" value="AD" out="N"/>
				<param name="e_cop_no" type="12" value="CLEH0330753297" out="N"/>
				<param name="e_event_yard" type="12" value="FRLEH20" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
