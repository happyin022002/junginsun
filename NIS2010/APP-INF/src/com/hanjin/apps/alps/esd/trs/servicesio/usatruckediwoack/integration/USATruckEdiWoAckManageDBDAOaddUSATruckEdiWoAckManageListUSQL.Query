<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USATruckEdiWoAckManageDBDAOaddUSATruckEdiWoAckManageListUSQL">
			<desc><![CDATA[EDI_CTRL_SEQ에 따른 TRS_TRSP_SVC_ORD테이블 update]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD SET 
	      EDI_RCV_RSLT_CD				= @[edi_rcv_rslt_cd]		
	    , EDI_RCV_RSLT_DT				= GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
	    , EDI_RCV_MSG_TP_CD				= @[edi_rcv_msg_tp_cd]
WHERE	  EDI_CTRL_SEQ				    = TO_NUMBER(@[edi_ctrl_seq]) 			]]></sql>
			<params>
				<param name="edi_rcv_rslt_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="edi_rcv_msg_tp_cd" type="12" value="" out="N"/>
				<param name="edi_ctrl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
