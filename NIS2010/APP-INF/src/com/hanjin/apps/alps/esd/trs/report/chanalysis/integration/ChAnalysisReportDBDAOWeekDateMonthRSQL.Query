<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChAnalysisReportDBDAOWeekDateMonthRSQL">
			<desc><![CDATA[입력된 월의 첫날과 마지막 날짜를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   
		TO_CHAR(TO_DATE(THEDAY, 'RRRRMM'), 'RRRRMMDD') FM_DT, 
		TO_CHAR(LAST_DAY(TO_DATE(THEDAY, 'RRRRMM')), 'RRRRMMDD') TO_DT
FROM 	(SELECT @[WEEK] THEDAY FROM DUAL)			]]></sql>
			<params>
				<param name="WEEK" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
