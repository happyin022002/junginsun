<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccrualBatchExecuteResultDBDAOSearchAccrualBatchResultFinalManualInputListRSQL">
			<desc><![CDATA[AccrualBatchResultFinalManualInputList 정보 조회]]></desc>
			<sql><![CDATA[
SELECT     S.REV_YRMON
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512073','512075') THEN S.ESTM_COST_AMT
                ELSE 0
           END)                  AS ESTM_COST_AMT_512073
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512073','512075') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512073
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512073','512075') THEN S.ADJ_ACCL_COST_AMT
                ELSE 0
           END)                  AS ADJ_ACCL_COST_AMT_512073
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD = '512361' THEN S.ESTM_COST_AMT
                ELSE 0
           END)                  AS ESTM_COST_AMT_512361
        ,  SUM(CASE WHEN S.ACCT_CD = '512361' THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512361
        ,  SUM(CASE WHEN S.ACCT_CD = '512361' THEN S.ADJ_ACCL_COST_AMT
                ELSE 0
           END)                  AS ADJ_ACCL_COST_AMT_512361
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD = '512074' THEN S.ESTM_COST_AMT
                ELSE 0
           END)                  AS ESTM_COST_AMT_512074
        ,  SUM(CASE WHEN S.ACCT_CD = '512074' THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512074                      
        ,  SUM(CASE WHEN S.ACCT_CD = '512074' THEN S.ADJ_ACCL_COST_AMT
                ELSE 0
           END)                  AS ADJ_ACCL_COST_AMT_512074
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD = '512362' THEN S.ESTM_COST_AMT
                ELSE 0
           END)                  AS ESTM_COST_AMT_512362
        ,  SUM(CASE WHEN S.ACCT_CD = '512362' THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512362
        ,  SUM(CASE WHEN S.ACCT_CD = '512362' THEN S.ADJ_ACCL_COST_AMT
                ELSE 0
           END)                  AS ADJ_ACCL_COST_AMT_512362
           -----------------------------------------------------------------------
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512061', '512062', '512063', '512064', '512065', '512066') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512061
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512151', '512133', '512144', '512152', '512153', '512154', '512155') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512151
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512221', '512222') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512221
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512341', '512342', '512343', '512344', '512345', '512346', '512347') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512341
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512171', '512172') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512171
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512331', '512332', '512333', '512334', '512335', '512336') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512331
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD = '512351' THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512351
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD IN ('512019', '512029', '512039', '512069', '512119', '512229', '512429') THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512019
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD = '512181' THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512181
           -----------------------------------------------------------------------
        ,  SUM(CASE WHEN S.ACCT_CD = '512381' THEN S.PST_ACT_COST_AMT
                ELSE 0
           END)                  AS PST_ACT_COST_AMT_512381
           -----------------------------------------------------------------------
FROM       LEA_ACCL_COST_SMRY   S
WHERE      S.REV_YRMON          BETWEEN REPLACE(@[frm_rev_yrmon_from],'-') AND REPLACE(@[frm_rev_yrmon_to],'-')   -- 변수 BINDING
AND        S.ACCT_CD            IN (  '512073', '512074', '512075'
                                    , '512361', '512362'
                  , '512061', '512062', '512063', '512064', '512065', '512066'
                  , '512151', '512133', '512144', '512152', '512153', '512154', '512155'
                  , '512221', '512222'
                  , '512341', '512342', '512343', '512344', '512345', '512346', '512347'
                  , '512171', '512172'
                  , '512331', '512332', '512333', '512334', '512335', '512336'
                  , '512351'
                  , '512019', '512029', '512039', '512069', '512119', '512229', '512429'
                  , '512181'
                  , '512381'
                  )

GROUP BY   S.REV_YRMON
ORDER BY   S.REV_YRMON             ASC			]]></sql>
			<params>
				<param name="frm_rev_yrmon_from" type="12" value="" out="N"/>
				<param name="frm_rev_yrmon_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
