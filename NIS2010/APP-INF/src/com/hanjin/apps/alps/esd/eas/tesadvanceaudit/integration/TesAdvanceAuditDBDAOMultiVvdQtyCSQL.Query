<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TesAdvanceAuditDBDAOMultiVvdQtyCSQL">
			<desc><![CDATA[VVD Quantity 등록 수정]]></desc>
			<sql><![CDATA[
MERGE INTO EAS_TML_MRN_VVD_QTY K USING(
	SELECT	  @[yd_cd] AS YD_CD
			, @[vsl_cd] AS VSL_CD
			, @[skd_voy_no] AS SKD_VOY_NO
			, @[skd_dir_cd] AS SKD_DIR_CD
			, @[io_bnd_cd] AS IO_BND_CD
			, @[cntr_tpsz_cd] AS CNTR_TPSZ_CD
			, @[ts_flg] AS TS_FLG
			, @[full_mty_cd] AS FULL_MTY_CD
	FROM DUAL 
	) B
	ON		( K.YD_CD		= B.YD_CD
	AND		K.VSL_CD		= B.VSL_CD
	AND		K.SKD_VOY_NO	= B.SKD_VOY_NO
	AND		K.SKD_DIR_CD	= B.SKD_DIR_CD
	AND		K.IO_BND_CD		= B.IO_BND_CD
	AND		K.CNTR_TPSZ_CD	= B.CNTR_TPSZ_CD
	AND		K.TS_FLG		= B.TS_FLG
	AND		K.FULL_MTY_CD	= B.FULL_MTY_CD
	)
	WHEN MATCHED THEN
	UPDATE
	SET		  K.CNTR_TTL_QTY	= @[cntr_ttl_qty]
			, K.DCGO_QTY		= @[dcgo_qty]
			, K.RC_QTY			= @[rc_qty]
			, K.DG_RC_QTY		= @[dg_rc_qty]
			, K.AWK_CGO_QTY		= @[awk_cgo_qty]
			, K.BB_CGO_QTY		= @[bb_cgo_qty]
			, K.AWK_BB_CGO_QTY	= @[awk_bb_cgo_qty]
			, K.HNGR_CGO_QTY	= @[hngr_cgo_qty]
			, K.CFS_CGO_QTY		= @[cfs_cgo_qty]
			, K.UPD_DT			= SYSDATE
			, K.UPD_USR_ID		= 'Batch'
	WHEN NOT MATCHED THEN
	INSERT(
			  YD_CD
			, VSL_CD
			, SKD_VOY_NO
			, SKD_DIR_CD
			, IO_BND_CD
			, CNTR_TPSZ_CD
			, TS_FLG
			, FULL_MTY_CD
			, CNTR_TTL_QTY
			, DCGO_QTY
			, RC_QTY
			, DG_RC_QTY
			, AWK_CGO_QTY
			, BB_CGO_QTY
			, AWK_BB_CGO_QTY
			, HNGR_CGO_QTY
			, CFS_CGO_QTY
			, K.CRE_USR_ID
			, K.CRE_DT
			, K.UPD_USR_ID
			, K.UPD_DT
	) VALUES (
			  @[yd_cd]			-- YD_CD
			, @[vsl_cd]			-- VSL_CD
			, @[skd_voy_no]		-- SKD_VOY_NO
			, @[skd_dir_cd]		-- SKD_DIR_CD
			, @[io_bnd_cd]		-- IO_BND_CD
			, @[cntr_tpsz_cd]	-- CNTR_TPSZ_CD
			, @[ts_flg]			-- TS_FLG
			, @[full_mty_cd]	-- FULL_MTY_CD
			, @[cntr_ttl_qty]	-- CNTR_TTL_QTY
			, @[dcgo_qty]		-- DCGO_QTY
			, @[rc_qty]			-- RC_QTY
			, @[dg_rc_qty]		-- DG_RC_QTY
			, @[awk_cgo_qty]	-- AWK_CGO_QTY
			, @[bb_cgo_qty]		-- BB_CGO_QTY
			, @[awk_bb_cgo_qty]	-- AWK_BB_CGO_QTY
			, @[hngr_cgo_qty]	-- HNGR_CGO_QTY
			, @[cfs_cgo_qty]	-- CFS_CGO_QTY
			, 'Batch'
			, SYSDATE
			, 'Batch'
			, SYSDATE
	)			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="ts_flg" type="12" value="" out="N"/>
				<param name="full_mty_cd" type="12" value="" out="N"/>
				<param name="cntr_ttl_qty" type="12" value="" out="N"/>
				<param name="dcgo_qty" type="12" value="" out="N"/>
				<param name="rc_qty" type="12" value="" out="N"/>
				<param name="dg_rc_qty" type="12" value="" out="N"/>
				<param name="awk_cgo_qty" type="12" value="" out="N"/>
				<param name="bb_cgo_qty" type="12" value="" out="N"/>
				<param name="awk_bb_cgo_qty" type="12" value="" out="N"/>
				<param name="hngr_cgo_qty" type="12" value="" out="N"/>
				<param name="cfs_cgo_qty" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
