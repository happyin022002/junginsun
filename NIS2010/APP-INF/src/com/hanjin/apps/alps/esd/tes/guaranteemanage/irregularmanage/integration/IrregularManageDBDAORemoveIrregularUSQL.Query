<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IrregularManageDBDAORemoveIrregularUSQL">
			<desc><![CDATA[Irregular Header Delete]]></desc>
			<sql><![CDATA[
UPDATE	TES_IRR_HDR SET
		DELT_FLG	= 'Y'
		, UPD_USR_ID	= @[upd_usr_id]
		, UPD_DT		= SYSDATE
WHERE	1	= 1
AND		IRR_NO		= @[irr_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="irr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
