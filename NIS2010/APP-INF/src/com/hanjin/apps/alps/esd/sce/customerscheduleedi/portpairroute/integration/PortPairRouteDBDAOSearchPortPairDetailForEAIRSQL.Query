<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairRouteDBDAOSearchPortPairDetailForEAIRSQL">
			<desc><![CDATA[EAI 로 전달할 정보 조회]]></desc>
			<sql><![CDATA[
SELECT          rout_rcv_dt                                         
		                  ,rout_seq                                            
		                  ,cust_trd_prnr_id                                    
		                  ,por_cd                                              
		                  ,n1st_pol_cd                                         
		                  ,n1st_pod_cd                                         
		                  ,n2nd_pol_cd                                         
		                  ,n2nd_pod_cd                                         
		                  ,n3rd_pol_cd                                         
		                  ,n3rd_pod_cd                                         
		                  ,n4th_pol_cd                                         
		                  ,n4th_pod_cd                                         
		                  ,del_cd                                              
		                  ,upd_ind_cd          	                              
		                  ,use_flg                                             
		                  ,mnl_use_flg                                        
		                  ,(CASE WHEN use_flg = 'N' OR mnl_use_flg = 'N'      		
		                         THEN 'Y'                                      		
		                         ELSE 'N' END) AS delt_flg                     			
		                  ,cre_usr_id                                          
		                  ,TO_CHAR(cre_dt, 'YYYYMMDDHH24MISS') AS cre_dt       
		                  ,upd_usr_id                                          
		                  ,TO_CHAR(upd_dt, 'YYYYMMDDHH24MISS') AS upd_dt       
		                  ,org_loc_cd                                          
		                  ,dest_loc_cd                                         
		
		           FROM sce_port_pair_dtl                                     
		          WHERE 1=1
		          /* condition - rout_rcv_dt */
#if (${rout_rcv_dt} != '') 
	AND rout_rcv_dt IN (
	#foreach($ele IN ${rout_rcv_dt})
		#if($velocityCount == 1 ) 
			('$ele')
		#else 
			,('$ele') 
		#end 
	#end
	)
#end
		          /* condition - rout_seq */
#if (${rout_seq} != '') 
	AND rout_seq IN (
	#foreach($ele IN ${rout_seq})
		#if($velocityCount == 1 ) 
			('$ele')
		#else 
			,('$ele') 
		#end 
	#end
	)
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
