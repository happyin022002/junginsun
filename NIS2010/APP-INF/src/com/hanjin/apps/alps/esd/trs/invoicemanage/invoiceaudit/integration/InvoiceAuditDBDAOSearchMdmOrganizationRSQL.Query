<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOSearchMdmOrganizationRSQL">
			<desc><![CDATA[Office의 Currency Cd를 조회한다]]></desc>
			<sql><![CDATA[
SELECT													
 	BIL_CURR_CD												
   ,(SELECT CNT_CD FROM MDM_LOCATION WHERE 1=1 AND NVL(DELT_FLG, 'N') = 'N' AND LOC_CD = MO.LOC_CD AND ROWNUM = 1) CNT_CD
 FROM										 				
 	MDM_ORGANIZATION MO										
 WHERE														
 	 OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
