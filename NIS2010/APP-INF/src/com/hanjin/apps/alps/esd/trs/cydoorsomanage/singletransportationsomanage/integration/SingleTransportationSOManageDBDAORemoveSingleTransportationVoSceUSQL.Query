<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAORemoveSingleTransportationVoSceUSQL">
			<desc><![CDATA[SCE테이블의 S/O Status를 Delete로 변경]]></desc>
			<sql><![CDATA[
UPDATE SCE_PLN_SO_LIST SET
     TRSP_SO_STS_CD = DECODE(@[trsp_rqst_bkg_flg], 'Y', 'D', 'P')
    ,UPD_DT         = SYSDATE
    ,UPD_USR_ID     = @[form_cre_usr_id]
WHERE COP_NO          = @[cop_no]
AND COST_ACT_GRP_SEQ  = @[cost_act_grp_seq]			]]></sql>
			<params>
				<param name="trsp_rqst_bkg_flg" type="12" value="" out="N"/>
				<param name="form_cre_usr_id" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
