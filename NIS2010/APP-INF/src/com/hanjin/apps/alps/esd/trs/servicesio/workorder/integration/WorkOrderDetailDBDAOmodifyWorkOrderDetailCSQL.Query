<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderDetailDBDAOmodifyWorkOrderDetailCSQL">
			<desc><![CDATA[modify Work Order Detail]]></desc>
			<sql><![CDATA[
INSERT ALL
INTO sce_act_rcv_if	(
		act_rcv_dt
	   ,act_rcv_no
	   ,cntr_no
	   ,act_dt
	   ,act_rcv_tp_cd
	   ,act_umch_tp_cd
	   ,bkg_no
	   ,nod_cd
	   ,act_sts_mapg_cd
	   ,cre_usr_id
	   ,cre_dt
	   ,upd_usr_id
	   ,upd_dt
	   ,vndr_seq
	   ,vndr_nm
	   ,act_gdt
	   ,act_dat_rcv_dt )
	VALUES	(
       to_char(sysdate,'yyyymmdd')
	   ,sce_act_rcv_if_seq1.nextVal
	   ,@[cntr_no]
	   ,TO_DATE(@[act_dt], 'YYYYMMDD HH24:MI:SS')
	   ,'9'
	   ,'00'
	   ,@[bkg_no]
	   ,@[nod_cd]
	   ,DECODE(@[act_sts_mapg_cd],'O','MOTZAD','I','FITZAD')
	   ,@[user_id]
       ,sysdate
       ,@[user_id]
       ,sysdate
	   ,@[vndr_seq]
	   ,@[vndr_nm]
	   ,TO_DATE(@[act_gdt],'yyyymmddhh24miss')
	   ,nvl((select GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',sysdate,LOC_CD)
	    from MDM_vendor where vndr_seq = @[vndr_seq]), SYSDATE)
)
INTO SCE_SVC_PTAL_ACT_IF (
	    act_rcv_dt
	   ,act_rcv_no
	   ,cntr_no
	   ,act_dt
	   ,act_rcv_tp_cd
	   ,act_umch_tp_cd
	   ,bkg_no
	   ,nod_cd
	   ,act_sts_mapg_cd
	   ,cre_usr_id
	   ,cre_dt
	   ,upd_usr_id
	   ,upd_dt
	   ,vndr_seq
	   ,vndr_nm
	   ,act_gdt
	   ,act_dat_rcv_dt ) 
	VALUES	(
       to_char(sysdate,'yyyymmdd')
	   ,sce_svc_ptal_act_if_seq1.nextVal
	   ,@[cntr_no]
	   ,TO_DATE(@[act_dt], 'YYYYMMDD HH24:MI:SS')
	   ,'9'
	   ,'00'
	   ,@[bkg_no]
	   ,@[nod_cd]
	   ,DECODE(@[act_sts_mapg_cd],'O','MOTZAD','I','FITZAD')
	   ,@[user_id]
       ,sysdate
       ,@[user_id]
       ,sysdate
	   ,@[vndr_seq]
	   ,@[vndr_nm]
	   ,TO_DATE(@[act_gdt],'yyyymmddhh24miss')
	   ,nvl((select GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS',sysdate,LOC_CD)
	    from MDM_vendor where vndr_seq = @[vndr_seq]), SYSDATE)
)
SELECT * FROM DUAL			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="act_sts_mapg_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="vndr_nm" type="12" value="" out="N"/>
				<param name="act_gdt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
