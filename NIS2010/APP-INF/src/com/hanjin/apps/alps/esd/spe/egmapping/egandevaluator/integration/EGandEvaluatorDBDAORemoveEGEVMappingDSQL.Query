<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EGandEvaluatorDBDAORemoveEGEVMappingDSQL">
			<desc><![CDATA[EG vs Evalulator Mapping 값을 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM SPE_EV_GRP_EVR   
 WHERE EG_ID       = @[eg_id]
   AND EV_KND_CD   = @[ev_knd_cd]
   AND EV_KND_SEQ  = @[ev_knd_seq]			]]></sql>
			<params>
				<param name="eg_id" type="12" value="" out="N"/>
				<param name="ev_knd_cd" type="12" value="" out="N"/>
				<param name="ev_knd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
