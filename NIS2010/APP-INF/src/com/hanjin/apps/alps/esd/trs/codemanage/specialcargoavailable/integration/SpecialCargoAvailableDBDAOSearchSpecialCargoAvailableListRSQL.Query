<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoAvailableDBDAOSearchSpecialCargoAvailableListRSQL">
			<desc><![CDATA[Special Cargo Available S/P List 조회]]></desc>
			<sql><![CDATA[
SELECT A.TRSP_SPCL_CGO_SP_SEQ
      ,A.VNDR_SEQ
      ,(SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR  WHERE VNDR_SEQ = A.VNDR_SEQ ) AS VNDR_NM
      ,A.SO_CRE_OFC_CD
      ,A.HZD_MTRL_FLG
      ,A.OVWT_TRI_AXL_FLG
      ,A.CRE_USR_ID
      ,(SELECT USR_NM FROM COM_USER WHERE USR_ID = A.CRE_USR_ID )AS CRE_USR_NM
      ,A.CRE_OFC_CD
      ,TO_CHAR(A.CRE_DT,'YYYY-MM-DD') CRE_DT 
      ,A.UPD_USR_ID
      ,TO_CHAR(A.UPD_DT,'YYYY-MM-DD') UPD_DT
  FROM TRS_SPCL_CGO_AVAL_SVC_PROV A
 WHERE 1=1
#if (${vndr_seq} != '' )
   AND A.VNDR_SEQ = @[vndr_seq]
#end			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
