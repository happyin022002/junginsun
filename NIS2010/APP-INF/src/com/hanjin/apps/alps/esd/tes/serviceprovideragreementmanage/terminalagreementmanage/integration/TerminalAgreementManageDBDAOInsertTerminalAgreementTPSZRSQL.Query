<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOInsertTerminalAgreementTPSZRSQL">
			<desc><![CDATA[TMNL Agmt TPSZ 조회]]></desc>
			<sql><![CDATA[
SELECT  cntr_tpsz_cd, cntr_tpsz_savename 
FROM	(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM	MDM_CNTR_TP_SZ                                                       	
		WHERE cntr_tp_cd = 'D' 		
--// MDM_CNTR_TP_SZ에 D3 추가로 인해 TES AGMT에서 사용하지 않는 D3 제외 로직 추가 ( 4342. 01. 22 - 이경한 과장 요청 )										           
		AND CNTR_TPSZ_CD <> 'D3'						    						       
		ORDER BY  cntr_sz_cd )    						        					   
UNION ALL                                                                   
SELECT	cntr_tpsz_cd, cntr_tpsz_savename 
FROM	(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM	MDM_CNTR_TP_SZ                                                         
		WHERE	cntr_tp_cd = 'R' 
		ORDER BY  cntr_sz_cd )                               
UNION ALL                                                                   
SELECT  cntr_tpsz_cd, cntr_tpsz_savename 
FROM                               
		(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM MDM_CNTR_TP_SZ                                                         
		WHERE	cntr_tp_cd = 'F' 
		ORDER BY  cntr_sz_cd ) 							    
UNION ALL                                                                   
SELECT  cntr_tpsz_cd, cntr_tpsz_savename 
FROM	(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM MDM_CNTR_TP_SZ                                                         
		WHERE	cntr_tp_cd = 'O' 
		ORDER BY  cntr_sz_cd )                               
UNION ALL                                                                   
SELECT  cntr_tpsz_cd, cntr_tpsz_savename 
FROM	(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM MDM_CNTR_TP_SZ                                                         
		WHERE	cntr_tp_cd = 'S' 
		ORDER BY  cntr_sz_cd )                               
UNION ALL                                                                   
SELECT  cntr_tpsz_cd, cntr_tpsz_savename 
FROM	(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM MDM_CNTR_TP_SZ                                                         
		WHERE	cntr_tp_cd = 'T' 
		ORDER BY  cntr_sz_cd )                               
UNION ALL                                                                   
SELECT  cntr_tpsz_cd, cntr_tpsz_savename 
FROM	(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM MDM_CNTR_TP_SZ                                                         
		WHERE	cntr_tp_cd = 'A' 
		ORDER BY  cntr_sz_cd )                               
UNION ALL                                                                   
SELECT  cntr_tpsz_cd, cntr_tpsz_savename 
FROM	(SELECT cntr_tpsz_cd, 'tesAgreementManageCommonVO.get'||cntr_tpsz_cd||'()' cntr_tpsz_savename 
		FROM MDM_CNTR_TP_SZ                                                         
		WHERE	cntr_tp_cd = 'P' 
		ORDER BY  cntr_sz_cd )			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
