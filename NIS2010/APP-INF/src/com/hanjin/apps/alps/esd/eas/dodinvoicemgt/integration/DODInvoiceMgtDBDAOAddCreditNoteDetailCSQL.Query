<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DODInvoiceMgtDBDAOAddCreditNoteDetailCSQL">
			<desc><![CDATA[AddCreditNoteDetail]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_DOD_INV_DTL (
 DOD_INV_NO
,CNTR_NO
,CNTR_TPSZ_CD
,DOD_LOC_CD
,BIL_CURR_CD
,BIL_AMT
,ADD_AMT
,TAX_AMT
,INV_AMT
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
) 
SELECT 
  @[new_inv_no]  AS DOD_INV_NO
  ,CNTR_NO
  ,CNTR_TPSZ_CD
  ,DOD_LOC_CD
  ,BIL_CURR_CD
  ,(BIL_AMT * -1) AS BIL_AMT
  ,(NVL(ADD_AMT,0) * -1) AS ADD_AMT
  ,(TAX_AMT * -1) AS TAX_AMT
  ,(INV_AMT * -1) AS INV_AMT
  ,@[cre_usr_id]  AS CRE_USR_ID
  ,NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]),SYSDATE) AS CRE_DT
  ,@[cre_usr_id]  AS UPD_USR_ID
  ,NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]),SYSDATE) AS UPD_DT
FROM EAS_DOD_INV_DTL
WHERE DOD_INV_NO = @[old_inv_no]			]]></sql>
			<params>
				<param name="new_inv_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="old_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
