<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailBillingInquiryDBDAOsearchRailBillingInquiryRSQL">
			<desc><![CDATA[Rail Bill Order Inquiry 화면에 대한 조회]]></desc>
			<sql><![CDATA[
SELECT /*+ ORDERD USE_NL(x b c loc1 cnt1 loc2 cnt2) */                                                                                         
                    TRIM(x.trsp_so_ofc_cty_cd) || TRIM(x.trsp_so_seq) row_idx                                                                               
                   ,'' chk                                                                                                                                  
                   ,x.trsp_so_ofc_cty_cd                                                                                                                    
                   ,x.trsp_so_seq                                                                                                                           
                   ,x.bkg_no                                                                                                                                
                   ,x.cgo_tp_cd                                                                                                                             
                   ,commcode_pkg.get_comdtl_name_fnc ('CD00748', x.cgo_tp_cd) cgo_tp_nm                                                                     
                   ,x.eq_no cntr_no                                                                                                                         
                   ,x.eq_tpsz_cd cntr_tpsz_cd                                                                                                               
                   ,c.cntr_tpsz_rmk cntr_tpsz_nm                                                                                                            
                   ,loc1.loc_nm || ' ' || cnt1.cnt_nm org_splc_loc_nm                                                                                       
                   ,loc2.loc_nm || ' ' || cnt2.cnt_nm dest_loc_nm                                                                                           
                   ,TO_CHAR(x.cre_dt, 'YYYY-MM-DD') req_dt                                                                                                  
                   ,x.cre_dt rail_bill_dt                                                                                                                   
                   ,TO_CHAR(x.cxl_rqst_dt, 'YYYY-MM-DD HH24:MI:SS') cncl_dt                                                                                            
                   ,x.cxl_rqst_rsn                                                                                                                          
                   ,x.cxl_rqst_rjct_rsn                                                                                                                     
                     ,(CASE                                                                                                                                 
                          WHEN x.cxl_rqst_flg = 'N' AND NVL(x.cxl_rqst_rjct_flg, 'N') = 'Y' AND x.delt_flg = 'N' THEN '7' -- 'Cancellation Requested Rejected' 
                          WHEN x.cxl_rqst_flg = 'Y' AND NVL(x.cxl_rqst_rjct_flg, 'N') = 'N' AND x.delt_flg = 'N' THEN '6' -- 'Cancellation Requested'       
                          WHEN x.trsp_so_sts_cd = 'I' AND x.delt_flg = 'N' AND b.bil_edi_rcv_rslt_cd = 'E' THEN '8' -- 'EDI Error'                          
                          WHEN x.trsp_so_sts_cd = 'V' AND x.delt_flg = 'N' AND b.bil_edi_rcv_rslt_cd = 'A' THEN '5' -- 'Acknowledged'                       
                          WHEN x.trsp_so_sts_cd = 'I' AND x.delt_flg = 'N' AND b.bil_edi_rcv_rslt_cd = 'A' THEN '5' -- 'Acknowledged'                       
                          WHEN x.trsp_so_sts_cd = 'I' AND x.spnd_flg = 'N' AND x.delt_flg = 'N' AND b.bil_iss_sts_cd = 'X' THEN '4' -- 'Rail Billing Cancelled' 
                          WHEN x.trsp_so_sts_cd = 'I' AND x.spnd_flg = 'N' AND x.delt_flg = 'N' THEN '3' -- 'Rail Billed'                                   
                          WHEN x.trsp_so_sts_cd = 'C' AND x.delt_flg = 'N' THEN '1' -- 'Requested'                                                          
                          WHEN x.trsp_so_sts_cd = 'R' AND x.spnd_flg = 'N' AND x.delt_flg = 'N' THEN '1' -- 'Requested'                                     
                          WHEN x.trsp_so_sts_cd = 'C' AND x.delt_flg = 'Y' THEN '2' -- 'Request Cancelled'                                                  
                          WHEN x.trsp_so_sts_cd = 'R' AND x.delt_flg = 'Y' THEN '2' -- 'Request Cancelled'		                                            
                          WHEN b.bil_iss_sts_cd = 'X' AND x.delt_flg = 'Y' THEN '10' -- 'Rail Billing Cancelled & Deleted'									
                          WHEN x.trsp_so_sts_cd = 'I' AND x.delt_flg = 'Y' THEN '9' -- 'Rail Billing Deleted'		                                        
                       END) status_cd                                                                                                                       
                    ,(CASE                                                                                                                                  
                         WHEN x.cxl_rqst_flg = 'N' AND NVL(x.cxl_rqst_rjct_flg, 'N') = 'Y' AND x.delt_flg = 'N' THEN 'Cancellation Requested Rejected'      
                         WHEN x.cxl_rqst_flg = 'Y' AND NVL(x.cxl_rqst_rjct_flg, 'N') = 'N' AND x.delt_flg = 'N' THEN 'Cancellation Requested'               
                         WHEN x.trsp_so_sts_cd = 'I' AND x.delt_flg = 'N' AND b.bil_edi_rcv_rslt_cd = 'E' THEN 'EDI Error'                                  
                         WHEN x.trsp_so_sts_cd = 'V' AND x.delt_flg = 'N' AND b.bil_edi_rcv_rslt_cd = 'A' THEN 'Acknowledged'                               
                         WHEN x.trsp_so_sts_cd = 'I' AND x.delt_flg = 'N' AND b.bil_edi_rcv_rslt_cd = 'A' THEN 'Acknowledged'                               
                         WHEN x.trsp_so_sts_cd = 'I' AND x.spnd_flg = 'N' AND x.delt_flg = 'N' AND b.bil_iss_sts_cd = 'X' THEN 'Rail Billing Cancelled'     
                         WHEN x.trsp_so_sts_cd = 'I' AND x.spnd_flg = 'N' AND x.delt_flg = 'N' THEN 'Rail Billed'                                           
                         WHEN x.trsp_so_sts_cd = 'C' AND x.delt_flg = 'N' THEN 'Requested'                                                                  
                         WHEN x.trsp_so_sts_cd = 'R' AND x.spnd_flg = 'N' AND x.delt_flg = 'N' THEN 'Requested'                                             
                         WHEN x.trsp_so_sts_cd = 'C' AND x.delt_flg = 'Y' THEN 'Request Cancelled'                                                          
                         WHEN x.trsp_so_sts_cd = 'R' AND x.delt_flg = 'Y' THEN 'Request Cancelled'                                                          
                         WHEN b.bil_iss_sts_cd = 'X' AND x.delt_flg = 'Y' THEN 'Rail Billing Cancelled & Deleted'		        							
                         WHEN x.trsp_so_sts_cd = 'I' AND x.delt_flg = 'Y' THEN 'Rail Billing Deleted'                                                       
                      END) status_nm                                                                                                                        
                    ,b.bil_iss_knt																															
                    ,b.spcl_instr_rmk                                                                   													
                    ,b.rail_ord_rjct_flg                                                                													
                    ,b.wo_rjct_rsn                                                                      													
                    ,case when b.inter_rmk is null then x.inter_rmk                                     													
                     else b.inter_rmk                                                                   													
                     end inter_rmk                                                                      													
                    ,x.fm_nod_cd                                                                        													
                    ,x.to_nod_cd                                                                        													
                    ,x.bl_no                                                                            													
                    ,x.bkg_cgo_tp_cd                                                                    													
                    ,x.cop_no                                                                           													
                    ,x.cost_act_grp_seq                                                                 													
                    ,x.repo_pln_id                                                                      													
                    ,x.pln_yrwk                                                                         													
                    ,x.ref_id                                                                           													
                    ,x.ref_seq                                                                          													
                    ,vnd.ofc_cd                                                                          													
               FROM (SELECT a.trsp_so_ofc_cty_cd                                                                                                            
                           ,a.trsp_so_seq                                                                                                                   
                           ,a.bkg_no                                                                                                                        
                           ,a.cgo_tp_cd                                                                                                                     
                           ,a.eq_no                                                                                                                         
                           ,a.eq_tpsz_cd                                                                                                                    
                           ,a.trsp_so_sts_cd                                                                                                                
                           ,a.spnd_flg                                                                                                                      
                           ,a.cxl_rqst_flg                                                                                                                  
                           ,a.cxl_rqst_rjct_flg                                                                                                             
                           ,a.delt_flg                                                                                                                      
                           ,a.fm_nod_cd                                                                                                                     
                           ,a.to_nod_cd                                                                                                                     
                           ,a.locl_cre_dt cre_dt                                                                                                                        
                           ,a.cxl_rqst_dt                                                                                                                   
                           ,a.cxl_rqst_rsn                                                                                                                  
                           ,a.cxl_rqst_rjct_rsn                                                                                                             
                           ,(SELECT   MAX(a2.bil_iss_knt)                                                                                                   
                                 FROM trs_trsp_edi_rail_ord a2                                                                                              
                                WHERE a2.trsp_so_ofc_cty_cd = a.trsp_so_ofc_cty_cd                                                                          
                                  AND a2.trsp_so_seq = a.trsp_so_seq                                                                                        
                             GROUP BY a2.trsp_so_ofc_cty_cd                                                                                                 
                                     ,a2.trsp_so_seq) bil_iss_knt                                                                                           
            			   ,a.inter_rmk																														
                           ,a.bl_no                                                                       													
                           ,a.bkg_cgo_tp_cd                                                               													
                           ,a.cop_no                                                                      													
                           ,a.cost_act_grp_seq                                                            													
                           ,a.repo_pln_id                                                                 													
                           ,a.pln_yrwk                                                                    													
                           ,a.ref_id                                                                      													
                           ,a.ref_seq                                                                     													
                           ,a.prov_vndr_seq as vndr_seq                                                                     													
                       FROM trs_trsp_rail_bil_ord a                                                                                                         
                      WHERE 1=1                                                                                                                             
            -- Date 조회이거나 Almighty가 아니면서  && Bkg혹은 Cntr 조회인경우만 Vndr_seq 넣는다.
 			#if (${condType} == '1' || (${userRoleCd} != '1' && ${condType} == '2'))
            	        AND a.prov_vndr_seq = @[provVndrSeq]                                                                                                             
            #end
            #if (${condType} == '1') -- Rail Billing Inquiry 조회구분  ( SEARCH_REQ_DT : 1, SEARCH_BKG_CNTL : 2)
                        AND a.locl_cre_dt BETWEEN TO_DATE(@[reqFromDt] || '000000', 'yyyymmddhh24miss') AND TO_DATE(@[reqToDt] || '235959', 'yyyymmddhh24miss')                      
                #if (${cargoType} != '') 
						AND a.cgo_tp_cd = @[cargoType]  
				#end
			#else 
 				#if ($bkgNoList != "" ) 
                          AND (a.bkg_no) IN (
											#foreach( ${bkgKey} in ${bkgNoList}) 
												#if($velocityCount == 1)						
													'$bkgKey'
												#else
													,'$bkgKey'
												#end
											#end
											)                                                                                 
                #end 
 				#if ($cntrNoList != "" ) 
                          AND a.eq_no IN (
											#foreach( ${cntrKey} in ${cntrNoList}) 
												#if($velocityCount == 1)						
													'$cntrKey'
												#else
													,'$cntrKey'
												#end
											#end
											)                                                                                 
 				#end               

			#end
                    ) x                                                                                                                                     
                   ,trs_trsp_edi_rail_ord b                                                                                                                 
                   ,mdm_cntr_tp_sz c                                                                                                                        
                   ,mdm_location loc1                                                                                                                       
                   ,mdm_location loc2                                                                                                                       
                   ,mdm_country cnt1                                                                                                                        
                   ,mdm_country cnt2                                                                                                                        
                   ,mdm_vendor vnd                                                                                                                        
                WHERE x.trsp_so_ofc_cty_cd = b.trsp_so_ofc_cty_cd(+)                                                                                        
                  AND x.trsp_so_seq        = b.trsp_so_seq(+)                                                                                               
                  AND x.bil_iss_knt        = b.bil_iss_knt(+)                                                                                               
                  AND x.eq_tpsz_cd = c.cntr_tpsz_cd                                                                                                         
                  AND SUBSTR(x.fm_nod_cd, 1, 5) = loc1.loc_cd(+)                                                                                            
                  AND loc1.cnt_cd = cnt1.cnt_cd(+)                                                                                                          
                  AND SUBSTR(x.to_nod_cd, 1, 5) = loc2.loc_cd(+)                                                                                            
                  AND loc2.cnt_cd = cnt2.cnt_cd(+)                                                                                                          
                  AND vnd.vndr_seq = x.vndr_seq                                                                                                          
			#if (${condType} == '1') -- Rail Billing Inquiry 조회구분  ( SEARCH_REQ_DT : 1, SEARCH_BKG_CNTL : 2)
				#if (${statusCd} != '') -- Rail Billing Inquiry Status
					#if (${statusCd} == '2') -- Requested Cancelled 
                              AND x.trsp_so_sts_cd = 'C'                                                                                                    
                              AND x.delt_flg = 'Y'                                                                                                          
					#elseif (${statusCd} == '1') -- Requested
                              AND ((x.trsp_so_sts_cd = 'C') OR (x.trsp_so_sts_cd = 'R' AND x.spnd_flg = 'N'))                                               
                              AND x.delt_flg = 'N'                                                                                                          
					#elseif (${statusCd} == '3') -- Rail Billed
                              AND x.trsp_so_sts_cd = 'I'                                                                                                    
                              AND x.spnd_flg = 'N'                                                                                                          
                              AND x.delt_flg = 'N'                                                                                                          
					#elseif (${statusCd} == '4') -- Rail Billed Cancelled
                              AND x.trsp_so_sts_cd = 'I'                                                                                                    
                              AND x.spnd_flg = 'N'                                                                                                          
                              AND x.delt_flg = 'N'                                                                                                          
                              AND b.bil_iss_sts_cd = 'X'                                                                                                    
					#elseif (${statusCd} == '5') --  Acknowledged
                              AND x.trsp_so_sts_cd IN ('I', 'V')                                                                                            
                              AND x.delt_flg = 'N'                                                                                                          
                              AND b.bil_edi_rcv_rslt_cd = 'A'                                                                                               
					#elseif (${statusCd} == '8') -- EDI Error
                              AND x.trsp_so_sts_cd = 'I'                                                                                                    
                              AND x.delt_flg = 'N'                                                                                                          
                              AND b.bil_edi_rcv_rslt_cd = 'E'                                                                                               
					#elseif (${statusCd} == '6') -- Cancellation Requested
                              AND x.cxl_rqst_flg = 'Y'                                                                                                      
                              AND NVL(x.cxl_rqst_rjct_flg, 'N') = 'N'                                                                                       
                              AND x.delt_flg = 'N'                                                                                                          
					#elseif (${statusCd} == '7') -- Cancellation Request Rejected
                              AND x.cxl_rqst_flg = 'N'                                                                                                      
                              AND NVL(x.cxl_rqst_rjct_flg, 'N') = 'Y'                                                                                       
                              AND x.delt_flg = 'N'                                                                                                          
					#elseif (${statusCd} == '10') -- Rail Billing Cancelled & Deleted
                              AND B.bil_iss_sts_cd = 'X'                                                                                                      
                              AND x.delt_flg = 'Y'                                                                                                          
					#elseif (${statusCd} == '9') -- Rail Billing Deleted
                              AND x.trsp_so_sts_cd = 'I'                                                                                                      
                              AND x.delt_flg = 'Y'                                                                                                          
					#end					

				#end

			#end			]]></sql>
			<params>
				<param name="provVndrSeq" type="12" value="" out="N"/>
				<param name="reqFromDt" type="12" value="" out="N"/>
				<param name="reqToDt" type="12" value="" out="N"/>
				<param name="cargoType" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
