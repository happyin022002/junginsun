<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInlandCostManageDBDAOSearchInlandCostReeferRSQL">
			<desc><![CDATA[2012.10.08 변종건 [CHM-201220395] Add-on Tariff Management 개선 프로젝트 - Reefer 조회]]></desc>
			<sql><![CDATA[
SELECT  SUBSTR(D.PORT_NOD_CD, 1,5) || '-' || SUBSTR(D.LOC_NOD_CD, 1,5) PORT_LOC
      , D.PORT_NOD_CD
      , D.HUB_NOD_CD
      , D.LOC_NOD_CD
      , D.MTY_PKUP_RTN_YD_CD
      , D.RCV_DE_TERM_CD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD01725',D.RCV_DE_TERM_CD) RCV_DE_TERM_NM
      , R.TRSP_CRR_MOD_CD
      , (
          SELECT  USA_COST_TRF_SVC_MOD_CD
          FROM    AOC_USA_IPI_PORT
          WHERE   PORT_CD = SUBSTR(D.PORT_NOD_CD,1,5)
          AND     TRSP_CRR_MOD_CD = D.TRSP_CRR_MOD_CD
        ) LOCL_IPI_SVC_MOD
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03118', (
                                                      SELECT  USA_COST_TRF_SVC_MOD_CD
                                                      FROM    AOC_USA_IPI_PORT
                                                      WHERE   PORT_CD = SUBSTR(D.PORT_NOD_CD,1,5)
                                                      AND     TRSP_CRR_MOD_CD = D.TRSP_CRR_MOD_CD
                                                    )
        ) LOCL_IPI_SVC_MOD_NM
      , D.CURR_CD
      , R.RF_20FT_TTL_COST_AMT
      , R.RF_40FT_TTL_COST_AMT
      , D.INLND_20FT_TTL_AMT
      , D.INLND_40FT_TTL_AMT
      , D.INLND_20FT_TTL_AMT
      , D.INLND_40FT_TTL_AMT
      , D.RAIL_20FT_BZC_COST_TTL_AMT + D.RAIL_20FT_FUEL_SCG_TTL_AMT AS RAIL_20FT_TTL_AMT
      , D.RAIL_40FT_BZC_COST_TTL_AMT + D.RAIL_40FT_FUEL_SCG_TTL_AMT AS RAIL_40FT_TTL_AMT
      , D.TRK_20FT_BZC_COST_TTL_AMT + D.TRK_20FT_FUEL_SCG_TTL_AMT + D.TOLL_FEE_AMT AS TRK_20FT_TTL_AMT
      , D.TRK_40FT_BZC_COST_TTL_AMT + D.TRK_40FT_FUEL_SCG_TTL_AMT + D.TOLL_FEE_AMT AS TRK_40FT_TTL_AMT
      , D.SCC_CD
      , NVL(D.MB_20FT_RTO,0)||'%' MB_20FT_RTO
      , NVL(D.MB_40FT_RTO,0)||'%' MB_40FT_RTO
      
      , D.RAIL_20FT_BZC_COST_SRC_CD
      , D.RAIL_20FT_BZC_COST_AMT
      , D.RAIL_20FT_BZC_COST_ADJ_AMT
      , D.RAIL_20FT_BZC_COST_TTL_AMT
      , CASE WHEN D.RAIL_20FT_BZC_COST_SRC_CD <> 'A' AND D.RAIL_20FT_BZC_AGMT_WGT IS NULL THEN 'MAX'
             WHEN D.RAIL_20FT_BZC_AGMT_WGT = 999999.99 THEN 'MAX'
             ELSE TO_CHAR(D.RAIL_20FT_BZC_AGMT_WGT)
        END RAIL_20FT_BZC_AGMT_WGT
      , D.RAIL_20FT_BZC_AGMT_WY_TP_CD
      
      , D.RAIL_20FT_FUEL_SCG_SRC_CD
      , D.RAIL_20FT_FUEL_SCG_AMT
      , D.RAIL_20FT_FUEL_SCG_ADJ_AMT
      , D.RAIL_20FT_FUEL_SCG_TTL_AMT
      , CASE WHEN D.RAIL_20FT_FUEL_SCG_SRC_CD <> 'A' AND D.RAIL_20FT_FUEL_SCG_AGMT_WGT IS NULL THEN 'MAX'
             WHEN D.RAIL_20FT_FUEL_SCG_AGMT_WGT = 999999.99 THEN 'MAX'
             ELSE TO_CHAR(D.RAIL_20FT_FUEL_SCG_AGMT_WGT)
        END RAIL_20FT_FUEL_SCG_AGMT_WGT
      , D.RAIL_20FT_FUEL_AGMT_WY_TP_CD
      
      , D.RAIL_40FT_BZC_COST_SRC_CD
      , D.RAIL_40FT_BZC_COST_AMT
      , D.RAIL_40FT_BZC_COST_ADJ_AMT
      , D.RAIL_40FT_BZC_COST_TTL_AMT
      , CASE WHEN D.RAIL_40FT_BZC_COST_SRC_CD <> 'A' AND D.RAIL_40FT_BZC_AGMT_WGT IS NULL THEN 'MAX'
             WHEN D.RAIL_40FT_BZC_AGMT_WGT = 999999.99 THEN 'MAX'
             ELSE TO_CHAR(D.RAIL_40FT_BZC_AGMT_WGT)
        END RAIL_40FT_BZC_AGMT_WGT
      , D.RAIL_40FT_BZC_AGMT_WY_TP_CD
      
      , D.RAIL_40FT_FUEL_SCG_SRC_CD
      , D.RAIL_40FT_FUEL_SCG_AMT
      , D.RAIL_40FT_FUEL_SCG_ADJ_AMT
      , D.RAIL_40FT_FUEL_SCG_TTL_AMT
      , CASE WHEN D.RAIL_40FT_FUEL_SCG_SRC_CD <> 'A' AND D.RAIL_40FT_FUEL_SCG_AGMT_WGT IS NULL THEN 'MAX'
             WHEN D.RAIL_40FT_FUEL_SCG_AGMT_WGT = 999999.99 THEN 'MAX'
             ELSE TO_CHAR(D.RAIL_40FT_FUEL_SCG_AGMT_WGT)
        END RAIL_40FT_FUEL_SCG_AGMT_WGT
      , D.RAIL_40FT_FUEL_AGMT_WY_TP_CD
      
      , D.TRK_20FT_BZC_COST_SRC_CD
      , D.TRK_20FT_BZC_COST_AMT
      , D.TRK_20FT_BZC_COST_ADJ_AMT
      , D.TRK_20FT_BZC_COST_TTL_AMT
      , D.TRSP_AGMT_20FT_MTY_YD_CD
      , CASE WHEN NVL(D.MTY_PKUP_RTN_YD_CD, 1) = NVL(D.TRSP_AGMT_20FT_MTY_YD_CD, 1) THEN 'N' ELSE 'Y' END AS TRSP_DIFF_20FT
      , CASE WHEN D.TRK_20FT_BZC_COST_SRC_CD <> 'A' AND D.TRK_20FT_BZC_AGMT_WGT IS NULL THEN 'MAX'
             WHEN D.TRK_20FT_BZC_AGMT_WGT = 999999.99 THEN 'MAX'
             ELSE TO_CHAR(D.TRK_20FT_BZC_AGMT_WGT)
        END TRK_20FT_BZC_AGMT_WGT
      , D.TRK_20FT_BZC_AGMT_WY_TP_CD
      
      , D.TRK_20FT_FUEL_SCG_SRC_CD
      , D.TRK_20FT_FUEL_SCG_AMT
      , D.TRK_20FT_FUEL_SCG_ADJ_AMT
      , D.TRK_20FT_FUEL_SCG_TTL_AMT
      , D.TRK_20FT_FUEL_SCG_AGMT_WGT
      , D.TRK_20FT_FUEL_AGMT_WY_TP_CD
      
      , D.TRK_40FT_BZC_COST_SRC_CD
      , D.TRK_40FT_BZC_COST_AMT
      , D.TRK_40FT_BZC_COST_ADJ_AMT
      , D.TRK_40FT_BZC_COST_TTL_AMT
      , D.TRSP_AGMT_40FT_MTY_YD_CD
      , CASE WHEN NVL(D.MTY_PKUP_RTN_YD_CD, 1) = NVL(D.TRSP_AGMT_40FT_MTY_YD_CD, 1) THEN 'N' ELSE 'Y' END AS TRSP_DIFF_40FT
      , D.TRK_40FT_BZC_AGMT_WGT
      , D.TRK_40FT_BZC_AGMT_WY_TP_CD
      
      , D.TRK_40FT_FUEL_SCG_SRC_CD
      , D.TRK_40FT_FUEL_SCG_AMT
      , D.TRK_40FT_FUEL_SCG_ADJ_AMT
      , D.TRK_40FT_FUEL_SCG_TTL_AMT
      , D.TRK_40FT_FUEL_SCG_AGMT_WGT
      , D.TRK_40FT_FUEL_AGMT_WY_TP_CD
      
      , D.MTY_TRSP_20FT_COST_SRC_CD
      , D.MTY_TRSP_20FT_COST_AMT
      , D.MTY_TRSP_20FT_ADJ_COST_AMT
      , D.MTY_TRSP_20FT_TTL_COST_AMT
      
      , D.MTY_TRSP_40FT_COST_SRC_CD
      , D.MTY_TRSP_40FT_COST_AMT
      , D.MTY_TRSP_40FT_ADJ_COST_AMT
      , D.MTY_TRSP_40FT_TTL_COST_AMT
      
      , D.DMST_20FT_COST_SRC_CD
      , D.DMST_20FT_COST_AMT
      , D.DMST_20FT_ADJ_COST_AMT
      , D.DMST_20FT_TTL_COST_AMT
      
      , D.DMST_40FT_COST_SRC_CD
      , D.DMST_40FT_COST_AMT
      , D.DMST_40FT_ADJ_COST_AMT
      , D.DMST_40FT_TTL_COST_AMT
      
      , D.TML_20FT_COST_SRC_CD
      , D.TML_20FT_COST_AMT
      , D.TML_20FT_ADJ_COST_AMT
      , D.TML_20FT_TTL_COST_AMT
      
      , D.TML_40FT_COST_SRC_CD
      , D.TML_40FT_COST_AMT
      , D.TML_40FT_ADJ_COST_AMT
      , D.TML_40FT_TTL_COST_AMT
      
      , D.N1ST_VNDR_SEQ
      , ( SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR WHERE VNDR_SEQ = D.N1ST_VNDR_SEQ ) N1ST_VNDR_NM
      , D.N1ST_INLND_ROUT_CMB_FLG
      , D.N2ND_VNDR_SEQ
      , ( SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR WHERE VNDR_SEQ = D.N2ND_VNDR_SEQ ) N2ND_VNDR_NM
      , D.N2ND_INLND_ROUT_CMB_FLG
      , D.N3RD_VNDR_SEQ
      , ( SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR WHERE VNDR_SEQ = D.N3RD_VNDR_SEQ ) N3RD_VNDR_NM
      , DECODE(
                GREATEST(NVL(D.N1ST_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N1ST_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_40FT_AGMT_OLD_FLG,'0')),'0',NULL,
                GREATEST(NVL(D.N1ST_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N1ST_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_40FT_AGMT_OLD_FLG,'0'))
        ) AS AGMT_OLD_FLG
      , D.COST_TRF_ROUT_SEQ
      , D.COST_ROUT_GRP_NO
      , H.CNT_CD
      , H.COST_TRF_NO
      , R.COST_TRF_RF_SEQ
      , R.UPD_USR_ID
      , D.TOLL_FEE_AMT AS TOLL_FEE_AMT_20
      , D.TOLL_FEE_AMT AS TOLL_FEE_AMT_40
FROM    AOC_USA_INLND_TRF_HDR H
      , AOC_USA_INLND_TRF_DTL D
      , AOC_USA_INLND_RF_TRF_DTL R
WHERE   H.COST_TRF_NO = D.COST_TRF_NO
AND     D.COST_TRF_NO = R.COST_TRF_NO
AND     D.COST_TRF_ROUT_SEQ = R.COST_TRF_ROUT_SEQ
AND     D.COST_SEL_ROUT_FLG = 'Y'
AND     H.COST_TRF_STS_CD IN ('B','U','C')
AND     D.DELT_FLG = 'N'
AND     R.DELT_FLG = 'N'
AND     H.CNT_CD      = @[in_cnt_cd]
AND     D.COST_TRF_NO = @[in_cost_trf_no]			]]></sql>
			<params>
				<param name="in_cnt_cd" type="12" value="" out="N"/>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
