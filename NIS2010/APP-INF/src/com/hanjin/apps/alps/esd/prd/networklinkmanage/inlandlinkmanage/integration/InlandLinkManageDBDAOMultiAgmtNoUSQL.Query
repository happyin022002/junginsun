<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandLinkManageDBDAOMultiAgmtNoUSQL">
			<desc><![CDATA[MultiAgmtNo]]></desc>
			<sql><![CDATA[
UPDATE prd_inlnd_each_lnk l 
SET upd_usr_id = @[upd_usr_id] , 
  upd_dt = sysdate, 
  trsp_agmt_ofc_cty_cd = @[cty_cd], 
  trsp_agmt_seq = to_number( @[i_agmt_seq] ) 
WHERE l.lnk_org_nod_cd = @[lnk_org_nod_cd]
  AND l.lnk_dest_nod_cd = @[lnk_dest_nod_cd]
  AND SUBSTR (l.lnk_org_nod_cd, 1, 2) IN ('US', 'CA' )    
  AND SUBSTR (l.lnk_dest_nod_cd, 1, 2) IN ('US', 'CA' )   
  AND l.trsp_mod_cd = @[trsp_mod_cd] 
  AND L.VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cty_cd" type="12" value="" out="N"/>
				<param name="i_agmt_seq" type="12" value="" out="N"/>
				<param name="lnk_org_nod_cd" type="12" value="" out="N"/>
				<param name="lnk_dest_nod_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
