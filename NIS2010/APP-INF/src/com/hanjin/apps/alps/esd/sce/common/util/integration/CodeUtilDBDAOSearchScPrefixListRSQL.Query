<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeUtilDBDAOSearchScPrefixListRSQL">
			<desc><![CDATA[SC NO를 가져오기]]></desc>
			<sql><![CDATA[
SELECT SC_PFX_CD   AS CD
      ,SC_PFX_DESC AS NM
  FROM PRI_SC_PFX
 WHERE DELT_FLG = 'N'
 AND SC_PFX_CD IN ('AEF','AEN','ANW','ASE','ASW','AWE','AWN','GLO','SAN','SAS','CEN','ANW','CEF','MME')
 ORDER BY SC_PFX_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
