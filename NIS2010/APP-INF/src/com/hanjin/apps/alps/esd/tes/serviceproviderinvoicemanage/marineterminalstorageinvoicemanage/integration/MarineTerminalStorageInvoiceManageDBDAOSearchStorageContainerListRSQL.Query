<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalStorageInvoiceManageDBDAOSearchStorageContainerListRSQL">
			<desc><![CDATA[SearchStorageContainerList]]></desc>
			<sql><![CDATA[
SELECT    
  BB_CGO_FLG          ,    
  WRK_DT              ,    
  CLM_DT              ,    
  RAIL_BIL_DT         ,    
  DSCR_RSN            ,    
  HNDL_RSLT_RMK       ,    
  CNTR_RMK            ,    
  CRE_USR_ID          ,    
  TO_CHAR(CRE_DT,'YYYY-MM-DD') CRE_DT  ,  
  UPD_USR_ID          ,    
  TO_CHAR(UPD_DT,'YYYY-MM-DD') UPD_DT  ,  
  TML_SO_OFC_CTY_CD   ,    
  TML_SO_SEQ          ,    
  TML_SO_CNTR_LIST_SEQ,    
  VRFY_RSLT_IND_CD    ,    
  MODI_FLG            ,    
  DSCR_IND_CD         ,    
  TML_RVIS_IND_FLG    ,    
  IO_BND_CD           ,    
  IOC_CD              ,    
  LANE_CD             ,    
  ATB_DT              ,    
  CNTR_NO             ,    
  CNTR_TPSZ_CD        ,    
  CNTR_STY_CD         ,    
  LOCL_TS_IND_CD      ,    
  SAM_LOCL_TS_IND_CD  ,    
  RCVDE_TERM_IND_CD   ,    
  PRE_YD_CD           ,    
  TO_CHAR(MVMT_GATE_IN_DT,'YYYY-MM-DD HH24:MI') MVMT_GATE_IN_DT     , 
  TO_CHAR(INV_GATE_IN_DT,'YYYY-MM-DD HH24:MI') INV_GATE_IN_DT      ,  
  GATE_IN_TD_DYS      ,
  TO_CHAR(MVMT_GATE_OUT_DT,'YYYY-MM-DD HH24:MI') MVMT_GATE_OUT_DT    ,
  TO_CHAR(INV_GATE_OUT_DT,'YYYY-MM-DD HH24:MI') INV_GATE_OUT_DT    ,  
  GATE_OUT_TD_DYS     ,    
  MVMT_STAY_DYS       ,    
  INV_STAY_DYS+1 INV_STAY_DYS     ,  -- 2012.07.30 장병용 부장 요청으로 +1 추가함..이유는 Verify시 Stay day 구할때 CMNC_HRMNT를 계산하지 않기 때문임..임시방편 인듯..  
  STAY_DIFF_DYS       ,    
  AWK_CGO_FLG         ,    
  RC_FLG              ,    
  DCGO_CLSS_CD             
FROM TES_TML_SO_CNTR_LIST  
WHERE TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
AND   TML_SO_SEQ = @[tml_so_seq]     
AND   VRFY_RSLT_IND_CD = @[codc_type]
#if (${codc_type} != '' and ${codc_type} == 'DC') 
ORDER BY DSCR_IND_CD ASC, CNTR_TPSZ_CD ASC, CNTR_STY_CD ASC, CNTR_NO ASC
#else 
ORDER BY CNTR_NO ASC, CNTR_TPSZ_CD ASC, CNTR_STY_CD ASC
#end			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="codc_type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
