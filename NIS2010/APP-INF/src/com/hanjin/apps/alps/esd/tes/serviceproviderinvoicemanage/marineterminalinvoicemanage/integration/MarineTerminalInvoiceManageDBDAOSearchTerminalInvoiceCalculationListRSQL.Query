<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOSearchTerminalInvoiceCalculationListRSQL">
			<desc><![CDATA[SearchTerminalInvoiceCalculationList

* 2011.08.08 윤태승 [CHM-201111118-1] MR Invoice Creation & Correction 의 Manual input 보완
* 2013.03.26 이혜민 [CHM-201322399-1] Special Cargo Quotation Project - 조회 시 bkg no 컬럼 추가
* 2015.01.20 김영신 [CHM-201430578] VOL_RT_CHK_FLG, VOL_DUP_CHK_FLG 컬럼 추가]]></desc>
			<sql><![CDATA[
SELECT   BKG_NO
		 ,TML_SO_OFC_CTY_CD
         ,TML_SO_SEQ
         ,TML_SO_DTL_SEQ
         ,CALC_COST_GRP_CD
		 ,DECODE(D.CALC_TP_CD, 'A', D.CALC_TP_CD, (DECODE(D.SEMI_AUTO_CALC_FLG, 'Y','S', D.CALC_TP_CD))) CALC_TP_CD
         ,VSL_CD
         ,SKD_VOY_NO 
         ,SKD_DIR_CD
         ,VSL_CD||SKD_VOY_NO||SKD_DIR_CD 	VVD_NO
         ,IOC_CD
         ,LANE_CD
         ,LANE_CD   LANE_CD2
         ,IO_BND_CD
         ,LGS_COST_CD
         ,LGS_COST_CD LGS_COST_CD2
         ,ACCT_CD
         ,TO_CHAR(ATB_DT,'YYYYMMDD') ATB_DT
         ,CNTR_NO
         ,CNTR_TPSZ_CD
         ,CALC_VOL_QTY
         ,NVL(FM_TR_VOL_VAL,'1')  FM_TR_VOL_VAL
         ,NVL(TO_TR_VOL_VAL,'99999') TO_TR_VOL_VAL
         ,NVL(FM_TR_VOL_VAL,'1')||' ~ '||NVL(TO_TR_VOL_VAL,'99999')  TIER
         ,RVIS_VOL_QTY
         ,DECODE(DCGO_IND_CD,'','N',DCGO_IND_CD) DCGO_IND_CD
         ,STAY_DYS
         ,FREE_DYS
         ,OVR_DYS
         ,TML_WRK_DY_CD
         ,FP_CALC_PRD_CD
         ,WRK_DT
         ,RC_FLG
         ,STK_VOL_QTY
         ,FP_TEU_QTY
         ,INV_VOL_QTY
         ,DIFF_VOL_QTY
         ,OVR_VOL_QTY
         ,VOL_TR_UT_CD
         ,CTRT_RT
         ,REF_VNDR_SEQ
         ,CALC_AMT
         ,INV_AMT
         ,TML_CRR_CD
         ,CALC_RMK
         ,N3PTY_FLG
         ,CURR_CD
         ,INV_XCH_RT
         ,TML_TRNS_MOD_CD
         ,TML_AGMT_OFC_CTY_CD
         ,TML_AGMT_SEQ
         ,TML_AGMT_VER_NO
		 ,RF_MNTR_DYS
		 ,PLG_TERM_DYS PLUG_TERM
		 ,EDI_SO_DTL_ID
		 ,NVL((SELECT COUNT(R.TML_SO_RVIS_LIST_SEQ) FROM TES_TML_SO_RVIS_LIST R 
    	 WHERE R.TML_SO_OFC_CTY_CD = D.TML_SO_OFC_CTY_CD 
    	 AND R.TML_SO_SEQ = D.TML_SO_SEQ 
    	 AND R.TML_SO_DTL_SEQ = D.TML_SO_DTL_SEQ),0) RVIS_KNT
		 ,NVL((SELECT VOL_RT_CHK_FLG FROM TES_TML_SO_COST C WHERE C.LGS_COST_CD = D.LGS_COST_CD),'N') VOL_RT_CHK_FLG
         ,NVL((SELECT VOL_DUP_CHK_FLG FROM TES_TML_SO_COST C WHERE C.LGS_COST_CD = D.LGS_COST_CD),'N') VOL_DUP_CHK_FLG 
         ,NVL((SELECT RMK_CHK_FLG FROM TES_TML_SO_COST C WHERE C.LGS_COST_CD = D.LGS_COST_CD),'N') RMK_CHK_FLG
		 -- [CHM-201642291]Agreement/invoice에서 Cost Code Description 추가
		 ,NVL((SELECT LGS_COST_ABBR_NM FROM TES_LGS_COST T WHERE T.LGS_COST_CD = D.LGS_COST_CD),'') LGS_COST_ABBR_NM   
		 ,IDA_SAC_CD   
		 ,IDA_PAY_TP_CD
		 ,IDA_CGST_RTO 
		 ,IDA_CGST_AMT 
		 ,IDA_SGST_RTO 
		 ,IDA_SGST_AMT 
		 ,IDA_IGST_RTO 
		 ,IDA_IGST_AMT 
	     ,IDA_UGST_RTO 
	     ,IDA_UGST_AMT 
		 ,IDA_GST_RTO  
		 ,IDA_GST_AMT  
 FROM    TES_TML_SO_DTL D
 WHERE   TML_SO_OFC_CTY_CD   = @[tml_so_ofc_cty_cd]
 AND     TML_SO_SEQ          = @[tml_so_seq]
 AND     VSL_CD              = SUBSTR(@[vvd],1,4)
 AND     SKD_VOY_NO          = SUBSTR(@[vvd],5,4)
 AND     SKD_DIR_CD          = SUBSTR(@[vvd],9,1)
 AND     IO_BND_CD           = @[io_bnd_cd]
 ORDER BY  CALC_TP_CD, BKG_NO DESC, LGS_COST_CD			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
