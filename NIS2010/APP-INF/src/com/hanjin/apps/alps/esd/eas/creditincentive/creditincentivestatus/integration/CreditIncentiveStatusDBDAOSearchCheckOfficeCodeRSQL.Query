<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAOSearchCheckOfficeCodeRSQL">
			<desc><![CDATA[Grid에 입력된 Office Code가 유효한지 체크]]></desc>
			<sql><![CDATA[
SELECT A.OFC_CD OFC_CD
  FROM MDM_ORGANIZATION A
 WHERE 1=1
   AND OFC_CD NOT IN ('SELDC', 'SELHO') 
   AND A.DELT_FLG = 'N'
   AND A.OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
