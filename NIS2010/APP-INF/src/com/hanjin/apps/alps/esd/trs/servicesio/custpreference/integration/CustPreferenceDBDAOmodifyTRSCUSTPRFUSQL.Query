<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustPreferenceDBDAOmodifyTRSCUSTPRFUSQL">
			<desc><![CDATA[update]]></desc>
			<sql><![CDATA[
UPDATE  TRS_CUST_PRF  SET                        	   	    
 CUST_CNT_CD = @[cust_cnt_cd],CUST_SEQ = @[cust_seq],TRSP_MOD_CD = @[trsp_mod_cd],ORG_LOC_CD = @[org_loc_cd],      		   	
 DEST_LOC_CD = @[dest_loc_cd],VNDR_CNT_CD = @[vndr_cnt_cd],VNDR_SEQ = @[vndr_seq],CRE_USR_ID = @[cre_usr_id],                 
 UPD_USR_ID = @[upd_usr_id],UPD_DT = to_date(@[upd_dt],'yyyymmddhh24miss')                  		
WHERE CRM_ROW_ID = @[crm_row_id]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="org_loc_cd" type="12" value="" out="N"/>
				<param name="dest_loc_cd" type="12" value="" out="N"/>
				<param name="vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="crm_row_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
