<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOSearchRvisCntrCustTpCdRSQL">
			<desc><![CDATA[SearchRvisCntrCustTpCd]]></desc>
			<sql><![CDATA[
SELECT RVIS_CNTR_CUST_TP_CD
FROM MDM_CUSTOMER
WHERE 1=1
AND CUST_CNT_CD = SUBSTR(@[cust_cd],1,2)
AND CUST_SEQ = TO_NUMBER(SUBSTR(@[cust_cd],3,6))			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
