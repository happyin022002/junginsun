<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetReservedEventDtMinRSQL">
			<desc><![CDATA[GetReservedEventDtMin]]></desc>
			<sql><![CDATA[
select to_char((to_date(@[event_dt],'yyyymmddhh24miss') + to_number(nvl(@[edi_snd_itval_hr],0)/24/60)),'yyyymmddhh24miss') RSV_EVENT_DT
From dual			]]></sql>
			<params>
				<param name="event_dt" type="12" value="" out="N"/>
				<param name="edi_snd_itval_hr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
