<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvOfcAtrtMgmtDBDAORemoveTrsTrspInvOfcDSQL">
			<desc><![CDATA[INVoice Office 컬럼 삭제하는 쿼리]]></desc>
			<sql><![CDATA[
DELETE FROM TRS_TRSP_INV_OFC
WHERE inv_ofc_cd = @[inv_ofc_cd]                                                    
AND   ofc_cd = @[ofc_cd]			]]></sql>
			<params>
				<param name="inv_ofc_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
