<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="COPSearchDBDAOSearchTargetCOPInfoList2CntRSQL">
			<desc><![CDATA[SearchTargetCOPInfoList2Cnt]]></desc>
			<sql><![CDATA[
SELECT MAX(grp_cnt) max_cnt   																	
  FROM ( SELECT a.pctl_no  
              , COUNT(a.pctl_no) OVER(PARTITION BY a.pctl_no) grp_cnt   			
	       FROM prd_prod_ctl_mst a  , prd_prod_ctl_rout_dtl b     									
		  WHERE a.pctl_no = b.pctl_no 
		    AND a.pctl_no LIKE SUBSTR(@[pctl_no] , 1, 16) ||'%'
		    AND nod_lnk_div_cd = 'L'
            AND pctl_io_bnd_cd = @[io_bnd_cd]   
       )			]]></sql>
			<params>
				<param name="pctl_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
