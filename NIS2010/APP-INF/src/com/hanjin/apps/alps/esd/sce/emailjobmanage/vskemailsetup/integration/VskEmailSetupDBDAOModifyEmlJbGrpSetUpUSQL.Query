<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VskEmailSetupDBDAOModifyEmlJbGrpSetUpUSQL">
			<desc><![CDATA[VSL E-MAIL 대상 변경 ( SCE_EML_JB_GRP )]]></desc>
			<sql><![CDATA[
UPDATE SCE_EML_JB_GRP
SET EML_SND_HR = @[emlSndHr],
    EML_SND_DYS_CTNT = @[emlSndDysCtnt]
WHERE EML_JB_ID = @[emlJbId]
AND SVC_ST_DT = @[svcStDt]
AND SVC_END_DT = NVL(@[svcEndDt], '99991231')
AND EML_GRP_ID = @[emlGrpIdHis]
AND EML_SND_HR = @[emlSndHrHis]
AND EML_SND_DYS_CTNT = @[emlSndDysCtntHis]			]]></sql>
			<params>
				<param name="emlSndHr" type="12" value="" out="N"/>
				<param name="emlSndDysCtnt" type="12" value="" out="N"/>
				<param name="emlJbId" type="12" value="" out="N"/>
				<param name="svcStDt" type="12" value="" out="N"/>
				<param name="svcEndDt" type="12" value="" out="N"/>
				<param name="emlGrpIdHis" type="12" value="" out="N"/>
				<param name="emlSndHrHis" type="12" value="" out="N"/>
				<param name="emlSndDysCtntHis" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
