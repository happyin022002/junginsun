<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifyTroMapgByCopUSQL">
			<desc><![CDATA[COP No 로 SCE_TRO_MAPG 에 존재하는 정보를 update 한다.(BOOKING 정보 만을 시도)]]></desc>
			<sql><![CDATA[
UPDATE SCE_TRO_MAPG A
SET
BKG_NO = NVL(@[bkg_no], A.BKG_NO)
WHERE
	COP_NO = @[cop_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
