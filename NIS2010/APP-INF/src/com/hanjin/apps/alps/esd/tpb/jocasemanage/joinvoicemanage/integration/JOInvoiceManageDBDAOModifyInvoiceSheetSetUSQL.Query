<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOInvoiceManageDBDAOModifyInvoiceSheetSetUSQL">
			<desc><![CDATA[TPB -> JO TPB -> JO TPB Invoice Setting
Modify 기능]]></desc>
			<sql><![CDATA[
UPDATE TPB_INV_SH_SET
   SET CO_NM = @[co_nm]
      ,OFC_ADDR = @[ofc_addr]
      ,OFC_PHN_NO = @[ofc_phn_no]
      ,OFC_FAX_NO = @[ofc_fax_no]
      ,BIL_TO_LOC_DIV_CD = @[bil_to_loc_div_cd]
      ,INV_RMK1 = @[inv_rmk1]
      ,INV_RMK2 = @[inv_rmk2]
      ,VAT_XCH_RT = @[vat_xch_rt]
      ,UPD_USR_ID = @[upd_usr_id]
      ,UPD_DT = SYSDATE
 WHERE INV_ISS_OFC_CD = @[s_inv_iss_ofc_cd]			]]></sql>
			<params>
				<param name="co_nm" type="12" value="" out="N"/>
				<param name="ofc_addr" type="12" value="" out="N"/>
				<param name="ofc_phn_no" type="12" value="" out="N"/>
				<param name="ofc_fax_no" type="12" value="" out="N"/>
				<param name="bil_to_loc_div_cd" type="12" value="" out="N"/>
				<param name="inv_rmk1" type="12" value="" out="N"/>
				<param name="inv_rmk2" type="12" value="" out="N"/>
				<param name="vat_xch_rt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="s_inv_iss_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
