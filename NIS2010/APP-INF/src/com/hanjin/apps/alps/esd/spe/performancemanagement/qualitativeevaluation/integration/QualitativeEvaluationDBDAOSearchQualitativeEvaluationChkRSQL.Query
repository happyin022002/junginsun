<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QualitativeEvaluationDBDAOSearchQualitativeEvaluationChkRSQL">
			<desc><![CDATA[S/P Qualitative Evaulation 데이터가 저장된 데이터인지 아닌지 조회한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) ISFLAG
  FROM SPE_SP_QUAL_EV
 WHERE EG_ID      = @[s_eg_id]
   AND EV_YR      = @[s_ev_yr]
   AND EV_MON     = @[s_ev_mon]
   AND SP_KPI_ID  = @[s_sp_kpi_id]
   AND SP_SEQ     = @[s_sp_seq]
   AND EVR_USR_ID = @[cre_usr_id]  			]]></sql>
			<params>
				<param name="s_eg_id" type="12" value="" out="N"/>
				<param name="s_ev_yr" type="12" value="" out="N"/>
				<param name="s_ev_mon" type="12" value="" out="N"/>
				<param name="s_sp_kpi_id" type="12" value="" out="N"/>
				<param name="s_sp_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
