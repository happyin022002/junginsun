<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOAddActualHistoryCSQL">
			<desc><![CDATA[AddActualHistory]]></desc>
			<sql><![CDATA[
INSERT INTO sce_act_rcv_if
                  (       act_rcv_dt
                         ,act_rcv_no
                         ,bkg_no                         
                         ,cre_dt
                         ,act_rcv_tp_cd
                         ,act_sts_mapg_cd
                         ,cntr_no
                         ,nod_cd
                         ,act_dt
                         ,ACT_UMCH_TP_CD
                         ,EDI_MSG_TP_CD
                         ,VNDR_SEQ
                         ,BND_VSKD_SEQ_CD
                         ,ACT_GDT
                         ,CRE_TP_CD
                         ,VSL_CD
                         ,SKD_VOY_NO
                         ,SKD_DIR_CD
                  )
                  VALUES
                  (       TO_CHAR(SYSDATE,'YYYYMMDD')
                         ,SCE_ACT_RCV_IF_SEQ1.NEXTVAL
 						,@[bkgNo]			 		                                    
   						,@[creDt]                                              
   						,@[actRcvTpCd]                                         
   						,@[actStsMapgCd]                                       
   						,@[cntrNo]                                             
   						,@[nodCd]                                              
   						,@[actDt]                                              
   						,@[actUmchTpCd]                                        
   						,@[ediMsgTpCd]                                         
   						,@[vndrSeq]                                            
   						,@[bndVskdSeqCd]                                       
   						,@[actGdt]                                             
   						,@[creTpCd]                                            
   						,@[vslcd]                                              
   						,@[skdVoyNo]                                           
        				,@[skdDirCd]      
                  )			]]></sql>
			<params>
				<param name="bkgNo" type="12" value="" out="N"/>
				<param name="creDt" type="12" value="" out="N"/>
				<param name="actRcvTpCd" type="12" value="" out="N"/>
				<param name="actStsMapgCd" type="12" value="" out="N"/>
				<param name="cntrNo" type="12" value="" out="N"/>
				<param name="nodCd" type="12" value="" out="N"/>
				<param name="actDt" type="12" value="" out="N"/>
				<param name="actUmchTpCd" type="12" value="" out="N"/>
				<param name="ediMsgTpCd" type="12" value="" out="N"/>
				<param name="vndrSeq" type="12" value="" out="N"/>
				<param name="bndVskdSeqCd" type="12" value="" out="N"/>
				<param name="actGdt" type="12" value="" out="N"/>
				<param name="creTpCd" type="12" value="" out="N"/>
				<param name="vslcd" type="12" value="" out="N"/>
				<param name="skdVoyNo" type="12" value="" out="N"/>
				<param name="skdDirCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
