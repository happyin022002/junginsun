<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifySppHisUSQL">
			<desc><![CDATA[ModifySppHis]]></desc>
			<sql><![CDATA[
    UPDATE sce_act_rcv_his
    SET    act_umch_tp_cd = @[v_act_umch_tp_cd]
          ,err_msg = @[v_message]
          ,umch_chk_dt    = SYSDATE
    WHERE  act_rcv_dt    = @[in_act_rcv_dt]
    AND    act_rcv_no    = @[in_act_rcv_no]			]]></sql>
			<params>
				<param name="v_act_umch_tp_cd" type="12" value="" out="N"/>
				<param name="v_message" type="12" value="" out="N"/>
				<param name="in_act_rcv_dt" type="12" value="" out="N"/>
				<param name="in_act_rcv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
