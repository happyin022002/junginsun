<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSelectAutoAudFileAllRSQL">
			<desc><![CDATA[Auto Audit 심사단위로 파일 전체 삭제 대상 조회]]></desc>
			<sql><![CDATA[
SELECT FILE_SAV_ID
  FROM EAS_ATCH_FILE
 WHERE ATCH_FILE_LNK_ID = @[atch_file_lnk_id]			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
