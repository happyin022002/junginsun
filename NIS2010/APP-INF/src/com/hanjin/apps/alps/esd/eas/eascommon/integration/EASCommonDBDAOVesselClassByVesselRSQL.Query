<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EASCommonDBDAOVesselClassByVesselRSQL">
			<desc><![CDATA[VesselClassByVessel]]></desc>
			<sql><![CDATA[
SELECT VSL_CD
     , VSL_ENG_NM AS VSL_NM
     , CNTR_VSL_CLSS_CAPA
  FROM MDM_VSL_CNTR
 WHERE CNTR_VSL_CLSS_CAPA BETWEEN @[from_cntr_vsl_clss_capa] AND @[to_cntr_vsl_clss_capa]
ORDER BY VSL_CD 			]]></sql>
			<params>
				<param name="from_cntr_vsl_clss_capa" type="12" value="" out="N"/>
				<param name="to_cntr_vsl_clss_capa" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
