<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="US204EDISetupDBDAOSearchUSEDISetupVndrRSQL">
			<desc><![CDATA[US EDI Setup List Vendor 조회]]></desc>
			<sql><![CDATA[
SELECT A.VNDR_SEQ AS VNDR_NO
      ,A.VNDR_LGL_ENG_NM AS VNDR_NM_ENG
  FROM MDM_VENDOR A
 WHERE A.VNDR_CNT_CD = 'US'
   AND A.WO_EDI_USE_FLG = 'Y'
   AND A.USA_EDI_CD IS NOT NULL 
   AND A.DELT_FLG = 'N'
   AND A.VNDR_SEQ = @[vndrSeq]			]]></sql>
			<params>
				<param name="vndrSeq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
