<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifyTroMapgByNewSeqVOUSQL">
			<desc><![CDATA[booking 에서 전달한 CombinedNewTroVO 로 SCE_TRO_MAPG 을 정리한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_TRO_MAPG
SET 
	BKG_NO = @[new_bkg_no],
	TRO_SEQ = @[new_tro_seq]
WHERE
	BKG_NO = @[org_bkg_no]
	AND TRO_SEQ = @[org_tro_seq]			]]></sql>
			<params>
				<param name="new_bkg_no" type="12" value="" out="N"/>
				<param name="new_tro_seq" type="12" value="" out="N"/>
				<param name="org_bkg_no" type="12" value="" out="N"/>
				<param name="org_tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
