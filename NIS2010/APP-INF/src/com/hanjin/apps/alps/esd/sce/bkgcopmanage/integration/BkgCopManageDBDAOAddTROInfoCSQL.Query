<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOAddTROInfoCSQL">
			<desc><![CDATA[SCE_TRO_MAPG table 에 mapping 정보 관리]]></desc>
			<sql><![CDATA[
INSERT
INTO SCE_TRO_MAPG (
    COP_NO, 
    IO_BND_CD, 
    AREA_CONTI_CD, 
    BKG_NO, 
    TRO_SEQ, 
    TRO_SUB_SEQ, 
    CRE_USR_ID, 
    CRE_DT, 
    UPD_USR_ID, 
    UPD_DT)
VALUES (
  @[cop_no],
  @[io_bnd_cd],
  @[area_conti_cd],
  @[bkg_no],
  @[tro_seq],
  @[tro_sub_seq],
  'SYSTEM',
  SYSDATE,
  'SYSTEM',
  SYSDATE )			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="area_conti_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="tro_seq" type="12" value="" out="N"/>
				<param name="tro_sub_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
