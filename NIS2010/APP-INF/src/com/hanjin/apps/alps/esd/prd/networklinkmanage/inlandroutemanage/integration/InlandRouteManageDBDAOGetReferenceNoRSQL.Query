<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageDBDAOGetReferenceNoRSQL">
			<desc><![CDATA[GetReferenceNo]]></desc>
			<sql><![CDATA[
SELECT a.AGMT_REF_NO,av.VNDR_SEQ from TRS_AGMT_HDR a ,TRS_AGMT_APLY_VNDR AV   
        	WHERE a.TRSP_AGMT_OFC_CTY_CD = @[cty_cd]
        	AND   a.TRSP_AGMT_SEQ = @[agmt_seq] 
        	AND   a.TRSP_AGMT_OFC_CTY_CD = av.TRSP_AGMT_OFC_CTY_CD 
        	AND   a.TRSP_AGMT_SEQ = av.TRSP_AGMT_SEQ 
        	--AND   NVL(a.DELT_FLG, 'N') = 'N'			]]></sql>
			<params>
				<param name="cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
