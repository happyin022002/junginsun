<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOMultiVolumeAccumulatedMethodUSQL">
			<desc><![CDATA[Method Update]]></desc>
			<sql><![CDATA[
UPDATE	TES_TML_SO_ACCM_MZD SET 
	ctrt_ofc_cd	= @[ctrt_ofc_cd]
	, accm_fm_dt	= @[accm_fm_dt]
	, accm_to_dt	= @[accm_to_dt]
	, tml_accm_ut_cd= @[tml_accm_ut_cd]
	, upd_usr_id	= @[upd_usr_id]
	, upd_dt	= SYSDATE
	, locl_upd_dt	= GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])  
WHERE	1	= 1  
AND		vndr_seq	= @[vndr_seq]
AND		accm_seq	= @[accm_seq]			]]></sql>
			<params>
				<param name="ctrt_ofc_cd" type="12" value="" out="N"/>
				<param name="accm_fm_dt" type="12" value="" out="N"/>
				<param name="accm_to_dt" type="12" value="" out="N"/>
				<param name="tml_accm_ut_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="accm_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
