<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOSearchSceDwllCustSvcListRSQL">
			<desc><![CDATA[정보를 조회를 한다.]]></desc>
			<sql><![CDATA[
SELECT 
DWLL_CUST_CNT_CD
,DWLL_CUST_SEQ
,NTFC_SEQ
,SUBSC_EML
,SUBSC_RMK
,EML_FM_CRM_FLG
,EML_BKG_ORG_FLG
,EML_BKG_DEST_FLG
,EML_BKG_OTR_FLG
,DELT_USR_ID
,DELT_FLG
,TO_CHAR(DELT_DT ,'YYYY-MM-DD HH24:MI:SS') DELT_DT
,CRE_OFC_CD
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
,SND_OPT_CD
FROM SCE_DWLL_CUST_SVC_LIST
WHERE DWLL_CUST_CNT_CD =@[cust_cnr_cd]
AND DWLL_CUST_SEQ =@[cust_seq]
AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="cust_cnr_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
