<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOmodifyAwkCgoShuttleTrfTpSzUSQL">
			<desc><![CDATA[modifyAwkCgoShuttleTrfTpSz]]></desc>
			<sql><![CDATA[
MERGE INTO TRS_AWK_CGO_TRF_TP_SZ V
USING  DUAL
ON (    V.FM_YD_CD = @[fm_loc_cd]||@[fm_nod_yd_no]
        AND V.TO_YD_CD = @[to_loc_cd]||@[to_nod_yd_no]
        AND V.COND_NO = @[cond_no]
        AND V.IO_GA_CD = @[io_ga_cd]
        AND V.TRSP_CRR_MOD_CD = @[trsp_crr_mod_cd]
		AND V.TRSP_AWK_TRF_VER_NO = (SELECT NVL(MAX(TRSP_AWK_TRF_VER_NO),0)
        							FROM TRS_AWK_CGO_TRF_HDR
                                    WHERE FM_YD_CD = @[fm_loc_cd]||@[fm_nod_yd_no]
                                    AND TO_YD_CD = @[to_loc_cd]||@[to_nod_yd_no]
                                    AND COND_NO = @[cond_no]
                                    AND IO_GA_CD = @[io_ga_cd]
                                    AND TRSP_CRR_MOD_CD = @[trsp_crr_mod_cd]
        							)
        AND V.CNTR_SZ_CD = @[cntr_sz_cd]
		AND V.TRSP_AWK_UC_CALC_TP_CD = @[trsp_awk_uc_calc_tp_cd]
)
WHEN MATCHED THEN
    UPDATE SET 
        LOCL_CURR_CD = @[locl_curr_cd],
        LOCL_CURR_AMT = @[locl_curr_amt],
        USD_AMT = @[usd_amt],
        USD_XCH_DT = @[usd_xch_dt],
        UPD_USR_ID = @[upd_usr_id],
        UPD_DT = SYSDATE
WHEN NOT MATCHED THEN
    INSERT (
        FM_YD_CD
        ,TO_YD_CD
        ,TRSP_AWK_CGO_TRF_TP_CD
        ,IO_GA_CD
        ,TRSP_CRR_MOD_CD
        ,COND_NO
        ,TRSP_AWK_TRF_VER_NO
        ,TRSP_AWK_UC_CALC_TP_CD
        ,CNTR_SZ_CD
        ,LOCL_CURR_CD
        ,LOCL_CURR_AMT
        ,USD_AMT
        ,USD_XCH_DT
        ,SPCL_CGO_REF_SEQ
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
	) VALUES (
    	@[fm_loc_cd]||@[fm_nod_yd_no],
        @[to_loc_cd]||@[to_nod_yd_no],
        @[trsp_awk_cgo_trf_tp_cd],
        @[io_ga_cd],
        @[trsp_crr_mod_cd],
		@[cond_no],
        (SELECT NVL(MAX(TRSP_AWK_TRF_VER_NO),0)
        FROM TRS_AWK_CGO_TRF_HDR 
        WHERE FM_YD_CD = @[fm_loc_cd]||@[fm_nod_yd_no]
        AND TO_YD_CD = @[to_loc_cd]||@[to_nod_yd_no]
        AND COND_NO = @[cond_no]
        AND IO_GA_CD = @[io_ga_cd]
        AND TRSP_CRR_MOD_CD = @[trsp_crr_mod_cd]
        ),
        @[trsp_awk_uc_calc_tp_cd],
        @[cntr_sz_cd],
        @[locl_curr_cd],
        @[locl_curr_amt],
        @[usd_amt],
        @[usd_xch_dt],
       	DECODE(@[trsp_awk_uc_calc_tp_cd],'S',TRS_AWK_SPCL_CGO_REF_SEQ.nextval,0),
        (SELECT OFC_CD
		 FROM (
    		 SELECT 
    		 DISTINCT
    		 Y.OFC_CD, 
    		 COUNT(Y.OFC_CD) OVER (PARTITION BY SUBSTR(Y.YD_CD,1,5), Y.OFC_CD) CNT
    		 FROM MDM_YARD Y
    		 WHERE 1=1
    		 AND Y.YD_CD LIKE @[fm_loc_cd]||@[fm_nod_yd_no]||'%' --5자리 이상
    		 ORDER BY CNT DESC
    		  )
		 WHERE ROWNUM = 1), 
        SYSDATE, 
        @[upd_usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="fm_loc_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yd_no" type="12" value="" out="N"/>
				<param name="to_loc_cd" type="12" value="" out="N"/>
				<param name="to_nod_yd_no" type="12" value="" out="N"/>
				<param name="cond_no" type="12" value="" out="N"/>
				<param name="io_ga_cd" type="12" value="" out="N"/>
				<param name="trsp_crr_mod_cd" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
				<param name="trsp_awk_uc_calc_tp_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="locl_curr_amt" type="12" value="" out="N"/>
				<param name="usd_amt" type="12" value="" out="N"/>
				<param name="usd_xch_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_awk_cgo_trf_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
