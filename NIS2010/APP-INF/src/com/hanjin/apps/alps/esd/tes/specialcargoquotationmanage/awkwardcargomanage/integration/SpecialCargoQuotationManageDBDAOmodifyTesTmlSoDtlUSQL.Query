<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOmodifyTesTmlSoDtlUSQL">
			<desc><![CDATA[modifyTesTmlSoDtl]]></desc>
			<sql><![CDATA[
UPDATE TES_TML_SO_DTL
SET LGS_COST_CD    = @[lgs_cost_cd]
	, INV_AMT      = @[inv_amt]
	, CALC_RMK     = @[calc_rmk]
	, UPD_USR_ID   = @[upd_usr_id]
	, UPD_DT       = SYSDATE
	, LOCL_UPD_DT  = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[lg_ofc_cd])
WHERE TML_SO_OFC_CTY_CD	 = @[tml_so_ofc_cty_cd]
AND	  TML_SO_SEQ         = @[tml_so_seq]
AND	  TML_SO_DTL_SEQ     = @[tml_so_dtl_seq]			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="calc_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="lg_ofc_cd" type="12" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="tml_so_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
