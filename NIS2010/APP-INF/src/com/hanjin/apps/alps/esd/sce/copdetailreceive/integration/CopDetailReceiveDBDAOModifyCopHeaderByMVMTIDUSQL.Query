<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopHeaderByMVMTIDUSQL">
			<desc><![CDATA[ModifyCopHeaderByMVMTID]]></desc>
			<sql><![CDATA[
update  sce_cop_hdr
           set     cop_sts_cd  = 'F'
                   ,upd_dt      = SYSDATE
                   ,upd_usr_id  = 'MVMT Close'
                   ,cop_fsh_dt = (select max(ACT_DT) from sce_cop_dtl where substr(ACT_CD, 1,1) <> 'M' and ACT_DT is not null and cop_no  = @[IN_COP_NO] )
            where  cop_no      = @[IN_COP_NO] AND COP_STS_CD != 'X'			]]></sql>
			<params>
				<param name="IN_COP_NO" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
