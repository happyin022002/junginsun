<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailInvoiceauditDBDAOSearchCLMHistoryRSQL">
			<desc><![CDATA[searchCLMHistory SELECT]]></desc>
			<sql><![CDATA[
SELECT
      C.CLM_CRR_NM       ,
      S.CLM_SGHT_ABBR_NM ,
      TO_CHAR(C.ARR_DT   , 'YYYY-MM-DD HH24:MI:SS') ARR_DT ,
      C.ARR_LOC_NM       ,
      C.ARR_STE_CD       ,
      C.DEP_LOC_NM       ,
      C.DEP_STE_CD
FROM
      SCE_CLM      C ,
      SCE_CLM_SGHT S
WHERE C.CNTR_NO     = @[eq_no]
AND   C.CLM_SGHT_CD = S.CLM_SGHT_CD
AND   ARR_DT BETWEEN TO_DATE( @[wbl_dt]||'000001', 'YYYYMMDDHH24MISS') AND TO_DATE( @[wbl_dt]||'235959', 'YYYYMMDDHH24MISS')+30
ORDER BY ARR_DT DESC			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="wbl_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
