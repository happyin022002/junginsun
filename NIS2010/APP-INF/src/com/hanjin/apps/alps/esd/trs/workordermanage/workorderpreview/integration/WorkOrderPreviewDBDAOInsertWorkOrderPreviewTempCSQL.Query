(
	@[trsp_so_ofc_cty_cd]
	, @[trsp_so_seq]
	, @[wo_prv_grp_seq]
	, @[wo_iss_sts_cd]     
	, @[wo_iss_no]      
	, @[vndr_seq]          
	, @[wo_cxl_flg]
	, @[dtn_use_flg]
	, @[wo_bl_no_iss_flg]
	, @[curr_cd]   
	, @[bzc_amt]            
