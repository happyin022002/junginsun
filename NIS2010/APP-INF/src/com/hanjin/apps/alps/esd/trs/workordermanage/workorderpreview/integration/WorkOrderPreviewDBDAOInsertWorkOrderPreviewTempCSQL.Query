 	, cre_usr_id
	, cre_dt
	, upd_usr_id
	, upd_dt
    , wtr_rcv_term_cd
    , wtr_de_term_cd
    , trsp_agmt_ofc_cty_cd
    , trsp_agmt_seq
    , trsp_agmt_cfm_flg
    , trsp_agmt_rt_seq
    , trsp_agmt_upd_dt
    , agmt_mor_cnddt_aply_flg
    , nego_rmk
    , scg_dtl_tmp_seq
    , gline_vndr_seq
    , gline_curr_cd
    , gline_bzc_amt
    , gline_fuel_scg_amt
    , gline_toll_fee_amt
