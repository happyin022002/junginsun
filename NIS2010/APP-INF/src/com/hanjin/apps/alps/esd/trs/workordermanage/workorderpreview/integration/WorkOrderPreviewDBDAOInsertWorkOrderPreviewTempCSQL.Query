	, @[cust_nomi_trkr_flg]
	, @[trsp_agmt_rt_tp_cd] 
	, @[trsp_agmt_wy_tp_cd] 
	, @[trsp_frst_flg] 
	, @[trsp_rjct_rsn_cd]
	, @[trsp_dflt_vndr_flg]
	, TO_DATE(@[n1st_nod_pln_dt],  'YYYYMMDDHH24MISS') 
	, TO_DATE(@[lst_nod_pln_dt],  'YYYYMMDDHH24MISS')	
	, TO_DATE(@[dor_nod_pln_dt],  'YYYYMMDDHH24MISS')    
	, @[inter_rmk]     
	, @[spcl_instr_rmk]
	, @[fctry_nm]     
	, @[dor_pst_cd]
	, @[cntc_pson_phn_no]
	, @[cntc_pson_fax_no]   
	, @[cntc_pson_nm]   
	, @[n3pty_cust_cnt_cd]
	, @[n3pty_cust_seq]  
	, @[n3pty_desc]     
	, @[n3pty_vndr_seq]
	, @[n3pty_ofc_cd]     
	, @[n3pty_bil_bzc_amt]
	, @[n3pty_bil_tp_cd]  
	, @[n3pty_curr_cd]  
	, @[cre_usr_id]
	, sysdate
	, @[upd_usr_id]
	, sysdate
    , @[wtr_rcv_term_cd]
    , @[wtr_de_term_cd]
    , @[trsp_agmt_ofc_cty_cd]
    , @[trsp_agmt_seq]
    , @[trsp_agmt_cfm_flg]
    , @[trsp_agmt_rt_seq]
    , TO_DATE(@[trsp_agmt_upd_dt],  'YYYYMMDDHH24MISS')
    , @[agmt_mor_cnddt_aply_flg]
    , @[nego_rmk]
    , @[scg_grp_seq]
    , @[gline_vndr_seq]
    , @[gline_curr_cd]
    , @[gline_bzc_amt]
    , @[gline_fuel_scg_amt]
    , @[gline_toll_fee_amt]
    , @[gline_scg_vat_amt]
    , NVL(@[gline_bzc_amt], 0)+NVL(@[gline_fuel_scg_amt], 0)+NVL(@[gline_toll_fee_amt], 0)+NVL(@[gline_scg_vat_amt], 0)
	, @[cust_nomi_trkr_ind_cd]
	, @[trsp_sp_cng_rsn_cd]
    , @[trsp_sp_cng_rsn_rmk]
	, (CASE
         WHEN( (SELECT VGM_WGT
                  FROM BKG_CONTAINER BKCN,
                       TRS_TRSP_SVC_ORD TRSO
                 WHERE 1=1
                   AND BKCN.BKG_NO = TRSO.BKG_NO
                   AND BKCN.CNTR_NO = TRSO.EQ_NO
                   AND TRSO.TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd]
                   AND TRSO.TRSP_SO_SEQ = @[trsp_so_seq]
                   AND BKCN.CNTR_DELT_FLG = 'N'
                   AND TRSO.DELT_FLG = 'N') > 0) THEN 'Y'
         ELSE 'N'
       END)
)			]]></sql>
