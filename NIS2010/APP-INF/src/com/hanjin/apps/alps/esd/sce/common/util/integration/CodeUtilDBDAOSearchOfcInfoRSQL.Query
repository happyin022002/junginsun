<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeUtilDBDAOSearchOfcInfoRSQL">
			<desc><![CDATA[SearchOfcInfo]]></desc>
			<sql><![CDATA[
SELECT COUNT(0) CNT
FROM MDM_ORGANIZATION
WHERE 1=1
AND OFC_CD = @[ofc_cd]
AND OFC_KND_CD < 4			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
