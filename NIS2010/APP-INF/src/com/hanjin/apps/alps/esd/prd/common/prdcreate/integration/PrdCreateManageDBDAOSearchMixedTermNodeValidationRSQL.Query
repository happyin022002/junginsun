<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCreateManageDBDAOSearchMixedTermNodeValidationRSQL">
			<desc><![CDATA[rcvT/por 혹은 delT/del 을 매개변수로 쿼리를 실행하여 Y or N 값을 리턴 하게 됩니다]]></desc>
			<sql><![CDATA[
SELECT DECODE(@[term],'D',DECODE(COUNT(*),0,'N','Y'),DECODE(COUNT(*),0,'Y','N')) term
FROM PRD_NODE 
WHERE NOD_CD = @[nodCd]
AND NOD_TP_CD = 'Z'
AND NVL(DELT_FLG,'X') <> 'Y'			]]></sql>
			<params>
				<param name="term" type="12" value="" out="N"/>
				<param name="nodCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
