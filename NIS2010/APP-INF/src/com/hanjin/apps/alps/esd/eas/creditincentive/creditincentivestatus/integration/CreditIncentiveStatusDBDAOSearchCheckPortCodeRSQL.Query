<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAOSearchCheckPortCodeRSQL">
			<desc><![CDATA[TES, VSL Port code Validation]]></desc>
			<sql><![CDATA[
SELECT LOC_CD PORT_CD
  FROM MDM_LOCATION A
  WHERE 1=1
   AND A.PORT_INLND_CD = 'Y'
   AND A.DELT_FLG = 'N'
   AND A.LOC_CD = @[port_cd]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
