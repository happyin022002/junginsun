<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOAddNodeConstraintHistoryCSQL">
			<desc><![CDATA[POD Node History 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_NOD_CNST_MGMT_HIS
     ( NOD_CD
     , NOD_CNST_ITM_CD
     , NOD_CNST_SEQ
     , HIS_CRE_DT
     , PCTL_CNST_ITM_NM
     , NOD_CNST_RMK
     , DELT_FLG
     , CNTR_TP_CD
     , EFF_FM_DT
     , EFF_TO_DT
     , CRE_OFC_CD
     , UPD_OFC_CD
     , SVC_USE_FLG
     , CMDT_CD
     , PORT_PNT_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , VSL_SLAN_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD
     )
SELECT NOD_CD
     , NOD_CNST_ITM_CD
     , NOD_CNST_SEQ
     , SYSDATE
     , PCTL_CNST_ITM_NM
     , NOD_CNST_RMK
     , DELT_FLG
     , CNTR_TP_CD
     , EFF_FM_DT
     , EFF_TO_DT
     , CRE_OFC_CD
     , UPD_OFC_CD
     , SVC_USE_FLG
     , CMDT_CD
     , PORT_PNT_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
     , VSL_SLAN_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD
  FROM PRD_NOD_CNST_MGMT
 WHERE NOD_CD = @[nod_cd]
   AND NOD_CNST_ITM_CD = @[old_nod_cnst_itm_cd]
   AND NOD_CNST_SEQ = TO_NUMBER(@[nod_cnst_seq])			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="old_nod_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
