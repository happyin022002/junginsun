<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderRemarkDAOSearchWorkOrderRemarkListRSQL">
			<desc><![CDATA[SELECT]]></desc>
			<sql><![CDATA[
SELECT
      WO_INSTR_OFC_CD                    ,
      TRSP_BND_CD                        ,
      TRSP_COST_MOD_CD                   ,
      TRSP_CRR_MOD_CD                    ,
      WO_INSTR_RMK                       ,
      TO_CHAR(UPD_DT, 'YYYYMMDD') CRE_DT ,
      UPD_USR_ID CRE_USR_ID              ,
      CRE_OFC_CD
FROM  TRS_TRSP_WRK_ORD_INSTR
WHERE WO_INSTR_OFC_CD	 = @[f_usr_ofc_cd]
#if ( ${f_trsp_cost_mod_cd} != 'ALL' && ${f_trsp_cost_mod_cd} != '' )
AND   TRSP_COST_MOD_CD = @[f_trsp_cost_mod_cd]
#end
#if ( ${f_trsp_crr_mod_cd} != 'ALL' && ${f_trsp_crr_mod_cd} != '' )
AND   TRSP_CRR_MOD_CD  = @[f_trsp_crr_mod_cd]
#end
#if ( ${f_trsp_bnd_cd} != 'ALL' && ${f_trsp_bnd_cd} != '' )
AND   TRSP_BND_CD      = @[f_trsp_bnd_cd]
#end
AND HJL_NO IS NULL			]]></sql>
			<params>
				<param name="f_usr_ofc_cd" type="12" value="" out="N"/>
				<param name="f_trsp_cost_mod_cd" type="12" value="" out="N"/>
				<param name="f_trsp_crr_mod_cd" type="12" value="" out="N"/>
				<param name="f_trsp_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
