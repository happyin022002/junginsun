<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandCostManageDBDAOsearchCntDefaultCurrRSQL">
			<desc><![CDATA[searchCntDefaultCurr]]></desc>
			<sql><![CDATA[
SELECT A.CNT_CD
      ,B.CNT_NM
      ,D.CONTI_NM
      ,C.SCONTI_NM
      ,B.EU_CNT_FLG
      ,A.CURR_CD
      ,(SELECT M.CURR_NM FROM MDM_CURRENCY M WHERE M.CURR_CD = A.CURR_CD) CURR_NM
      ,A.RHQ_CD
      ,(SELECT USR_NM FROM COM_USER
         WHERE USR_ID = A.UPD_USR_ID ) UPD_USR_ID
      ,TO_CHAR(A.UPD_DT, 'YYYY-MM-DD') UPD_DT
  FROM TRS_COST_TRF_CURR A
      ,MDM_COUNTRY       B
      ,MDM_SUBCONTINENT  C
      ,MDM_CONTINENT     D
 WHERE A.CNT_CD = B.CNT_CD(+)
   AND B.SCONTI_CD = C.SCONTI_CD(+)
   AND C.CONTI_CD  = D.CONTI_CD(+)
#if (${rhq_cd} != '') 
AND     A.RHQ_CD IN ($rhq_cd)
#end
#if (${cnt_cd} != '') 
AND     A.CNT_CD IN ($cnt_cd)
#end
#if (${curr_cd} != '') 
AND     A.CURR_CD IN ($curr_cd)
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
