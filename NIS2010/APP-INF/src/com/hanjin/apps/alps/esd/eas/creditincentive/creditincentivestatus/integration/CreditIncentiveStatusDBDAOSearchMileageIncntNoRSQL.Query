<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAOSearchMileageIncntNoRSQL">
			<desc><![CDATA[Mileage Incentive No 조회(Insert시 PK)]]></desc>
			<sql><![CDATA[
SELECT 'MIL'||LPAD(INCNT_NO,4,'0') INCNT_NO
 FROM ( SELECT CASE WHEN SUBSTR(MAX(A.INCNT_NO),4) IS NULL THEN 1
                    ELSE TO_NUMBER(SUBSTR(MAX(A.INCNT_NO),4))+1
               END AS INCNT_NO
          FROM EAS_BNF_MLG A
         WHERE A.BSE_YR = @[bse_yr]
      )
			]]></sql>
			<params>
				<param name="bse_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
