<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOSearchTerminalAgreementManageRSQL">
			<desc><![CDATA[Agreement Header Info 조회]]></desc>
			<sql><![CDATA[
SELECT	A.yd_cd
		, A.ctrt_ofc_cd
		, A.cre_ofc_cd
		, A.vndr_seq
		, B.vndr_abbr_nm
		, TO_CHAR(A.eff_fm_dt	, 'YYYYMMDD')	eff_fm_dt
		, TO_CHAR(A.cre_dt		, 'YYYYMMDD')	cre_dt
		, TO_CHAR(A.upd_dt		, 'YYYYMMDD')	upd_dt
		, A.auto_xtd_flg
		, C.yd_nm
		, TO_CHAR(A.eff_to_dt	, 'YYYYMMDD')	eff_to_dt
		, A.cre_ofc_cd
		, A.cre_usr_id
		, A.upd_usr_id
		, TO_CHAR(A.agmt_apro_dt, 'YYYYMMDD')	agmt_apro_dt
		, agmt_cfm_usr_nm
		, agmt_cfm_usr_id	 							                						
		, agmt_cfm_flg
		-- 비용지급 전표 결재 기능 - 3차 추가(4347-09-25)
		, agmt_doc_no
		, agmt_doc_desc
		, agmt_doc_eff_fm_dt
		, agmt_doc_eff_to_dt
FROM	TES_TML_AGMT_HDR A, MDM_VENDOR B, MDM_YARD C	
WHERE	A.vndr_seq = B.vndr_seq
AND		A.yd_cd = C.yd_cd 
AND   	A.tml_agmt_ofc_cty_cd	= SUBSTR(@[tml_agmt_ofc_cty_cd],1,3)
AND   	A.tml_agmt_seq			= SUBSTR(@[tml_agmt_ofc_cty_cd],4,8)
AND   	A.tml_agmt_ver_no		= @[tml_agmt_ver_no]			]]></sql>
			<params>
				<param name="tml_agmt_ofc_cty_cd" type="12" value="SIN0003" out="N"/>
				<param name="tml_agmt_ver_no" type="2" value="0204" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
