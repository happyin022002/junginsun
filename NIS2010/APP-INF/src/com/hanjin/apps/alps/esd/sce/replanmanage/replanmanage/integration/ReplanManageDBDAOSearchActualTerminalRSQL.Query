<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOSearchActualTerminalRSQL">
			<desc><![CDATA[terminal change 대상을 sce_act_tml_if table 에서 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
	act_rcv_dt, act_rcv_no, vsl_cd, skd_voy_no,     
	skd_dir_cd, vps_port_cd, clpt_ind_seq, nod_cd, clpt_ind_seq call_yd_ind_seq,
	tml_if_sts_cd, cop_evnt_seq, cre_dt, upd_dt, call_yd_ind_cng_flg
 FROM SCE_ACT_TML_IF 
 WHERE 
	 ACT_RCV_DT  > TO_CHAR(SYSDATE-3, 'YYYYMMDD') 
	 AND TML_IF_STS_CD = '00' 
	 AND ROWNUM < 11			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
