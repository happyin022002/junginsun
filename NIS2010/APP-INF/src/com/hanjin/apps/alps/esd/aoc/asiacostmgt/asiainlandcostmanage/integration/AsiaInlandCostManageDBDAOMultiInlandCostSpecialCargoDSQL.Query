   AND DG_FX_RT = '0'
   AND DG_FX_RTO = '0'
   AND MIN_CGO_WGT = '0'
   AND MAX_CGO_WGT = '0'
   AND OVR_WGT_FX_RT = '0'
   AND OVR_WGT_FX_RTO = '0'
   AND DCGO_SVC_FLG = 'N'
   AND OVWT_CGO_SVC_FLG = 'N'			]]></sql>
