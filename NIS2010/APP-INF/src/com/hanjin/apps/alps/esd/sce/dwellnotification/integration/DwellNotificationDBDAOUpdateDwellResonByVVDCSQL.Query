<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOUpdateDwellResonByVVDCSQL">
			<desc><![CDATA[Dwell Reason 데이터를 입력한다]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_DWLL_RSN(
DWLL_RSN_SEQ
,VSL_SLAN_CD
,VSL_CD
,SKD_VOY_NO
,SKD_DIR_CD
,VPS_ETA_DT
,VPS_ETD_DT
,DWLL_RSN
,UPD_OFC_CD
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
)
VALUES(
SCE_DWLL_RSN_SEQ.nextval
,@[slan_cd]
,SUBSTR(@[vvd], 1,4)
,SUBSTR(@[vvd], 5,4)
,SUBSTR(@[vvd], 9)
,TO_DATE(@[eta_dt],'YYYY-MM-DD HH24:MI:SS')
,TO_DATE(@[etd_dt],'YYYY-MM-DD HH24:MI:SS')
,@[dwll_rsn]
,@[upd_ofc_cd1]
,@[upd_usr_id1]
,SYSDATE
,@[upd_usr_id1]
,SYSDATE
)			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="eta_dt" type="12" value="" out="N"/>
				<param name="etd_dt" type="12" value="" out="N"/>
				<param name="dwll_rsn" type="12" value="" out="N"/>
				<param name="upd_ofc_cd1" type="12" value="" out="N"/>
				<param name="upd_usr_id1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
