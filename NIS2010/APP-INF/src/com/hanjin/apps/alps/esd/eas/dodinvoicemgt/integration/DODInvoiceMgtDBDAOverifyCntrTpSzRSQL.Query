<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DODInvoiceMgtDBDAOverifyCntrTpSzRSQL">
			<desc><![CDATA[verifyTpSz 입력된 tpsz_cd의 MDM내 존재여부 확인]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) TPSZ_CNT					
 FROM MDM_CNTR_TP_SZ                		
 WHERE CNTR_TPSZ_CD = @[cntr_tpsz_cd]
 AND NVL(DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
