<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDBDAOsearchCommodityGroupCodeManageListRSQL">
			<desc><![CDATA[searchCommodityGroupCodeManageList]]></desc>
			<sql><![CDATA[
SELECT														
A.trsp_grp_cmdt_cd,													
B.vndr_lgl_eng_nm vndr_nm,											
A.vndr_seq,															
A.trsp_cmdt_grp_nm,													
'Y' save,															
A.vndr_seq||A.trsp_grp_cmdt_cd dupl,
decode(A.delt_flg,'N','LIVE','Y','DEL') delt_flg
FROM trs_trsp_cmdt_grp A, mdm_vendor B 								
WHERE A.vndr_seq=B.vndr_seq
#if (${vndr_cd} != '') 
	AND A.vndr_seq like @[vndr_cd]
#end
#if (${vndr_commoodity_cd} != '') 
	AND A.trsp_grp_cmdt_cd like @[vndr_commoodity_cd]
#end
#if (${vndr_commoodity_nm} != '') 
	AND A.trsp_cmdt_grp_nm like @[vndr_commoodity_nm]
#end			]]></sql>
			<params>
				<param name="vndr_cd" type="12" value="" out="N"/>
				<param name="vndr_commoodity_cd" type="12" value="" out="N"/>
				<param name="vndr_commoodity_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
