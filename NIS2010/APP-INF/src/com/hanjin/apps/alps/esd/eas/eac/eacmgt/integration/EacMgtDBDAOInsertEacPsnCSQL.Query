<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOInsertEacPsnCSQL">
			<desc><![CDATA[Personnel Config 를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_EXPN_AUD_CS_PSON_CFG (EAC_USR_ID
                                    , EAC_USR_NM
                                    , PHN_NO
                                    , FAX_NO
                                    , USR_EML
                                    , NTC_CC_RCV_EML
                                    , EXPN_AUD_SCP_DESC
                                    , EML_SUBJ_CTNT
                                    , EML_CTNT
                                    , DELT_FLG
                                    , CRE_USR_ID
                                    , CRE_DT
                                    , UPD_USR_ID
                                    , UPD_DT)
                                    VALUES(
                                      @[eac_usr_id]
                                    , @[eac_usr_nm]
                                    , @[phn_no]
                                    , @[fax_no]
                                    , @[usr_eml]
                                    , @[ntc_cc_rcv_eml]
                                    , @[expn_aud_scp_desc]
                                    , @[eml_subj_ctnt]
                                    , @[eml_ctnt]
                                    , 'N'
                                    , @[cre_usr_id]
                                    , SYSDATE
                                    , @[upd_usr_id]
                                    , SYSDATE
                                    )

			]]></sql>
			<params>
				<param name="eac_usr_id" type="12" value="" out="N"/>
				<param name="eac_usr_nm" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="usr_eml" type="12" value="" out="N"/>
				<param name="ntc_cc_rcv_eml" type="12" value="" out="N"/>
				<param name="expn_aud_scp_desc" type="12" value="" out="N"/>
				<param name="eml_subj_ctnt" type="12" value="" out="N"/>
				<param name="eml_ctnt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
