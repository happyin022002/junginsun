<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInlandCostManageDBDAOVerifyServiceModeRSQL">
			<desc><![CDATA[verify service mode (0 or minus value)]]></desc>
			<sql><![CDATA[
SELECT C.INTG_CD_VAL_DP_DESC AS LOCL_IPI_SVC_MOD_NM
  FROM COM_INTG_CD_DTL C
     , ( SELECT DISTINCT P.USA_COST_TRF_SVC_MOD_CD
            FROM AOC_USA_INLND_TRF_DTL D
               , AOC_USA_IPI_PORT P
            WHERE 1=1
            AND   D.COST_TRF_NO = @[in_cost_trf_no]
            AND   D.COST_SEL_ROUT_FLG = 'Y'
            AND   D.DELT_FLG = 'N'
            AND   (D.INLND_20FT_TTL_AMT <= 0 OR D.INLND_40FT_TTL_AMT <= 0)
            AND   D.TRSP_CRR_MOD_CD = P.TRSP_CRR_MOD_CD
            AND   P.PORT_CD = SUBSTR(D.PORT_NOD_CD,1,5)
       ) U
WHERE 1=1       
AND   C.INTG_CD_ID = 'CD03118'
AND   C.INTG_CD_VAL_CTNT = U.USA_COST_TRF_SVC_MOD_CD
ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
