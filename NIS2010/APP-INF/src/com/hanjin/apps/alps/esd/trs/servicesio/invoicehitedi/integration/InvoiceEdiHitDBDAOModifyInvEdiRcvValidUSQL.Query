<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceEdiHitDBDAOModifyInvEdiRcvValidUSQL">
			<desc><![CDATA[Invoice EDI Header 정보 Validation 결과 Update]]></desc>
			<sql><![CDATA[
UPDATE TRS_INV_EDI_RCV
   SET VAL_CHK_FLG = @[val_chk_flg]
      ,VAL_RMK = SUBSTR(VAL_RMK||' '||@[val_rmk],1,1000)
      ,UPD_USR_ID = 'HIT_INV_VAL'
      ,UPD_DT = SYSDATE
 WHERE INV_EDI_RCV_SEQ = @[inv_edi_rcv_seq]			]]></sql>
			<params>
				<param name="val_chk_flg" type="12" value="" out="N"/>
				<param name="val_rmk" type="12" value="" out="N"/>
				<param name="inv_edi_rcv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
