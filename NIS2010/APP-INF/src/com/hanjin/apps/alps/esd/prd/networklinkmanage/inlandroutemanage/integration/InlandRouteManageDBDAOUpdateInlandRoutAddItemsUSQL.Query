<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageDBDAOUpdateInlandRoutAddItemsUSQL">
			<desc><![CDATA[UpdateInlandRoutAddItems]]></desc>
			<sql><![CDATA[
UPDATE PRD_INLND_ROUT_MST
SET FULL_RTN_YD_CD = @[full_rtn_yd_cd]
  , FULL_PKUP_YD_CD = @[full_pkup_yd_cd]
  ,TRSP_MOD_CD = @[trsp_mod_cd]
WHERE
  ROUT_ORG_NOD_CD = @[i_rout_org_nod_cd]
  AND
  ROUT_DEST_NOD_CD = @[i_rout_dest_nod_cd]
  AND
  ROUT_SEQ = TO_NUMBER(@[i_rout_seq])			]]></sql>
			<params>
				<param name="full_rtn_yd_cd" type="12" value="" out="N"/>
				<param name="full_pkup_yd_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="i_rout_org_nod_cd" type="12" value="" out="N"/>
				<param name="i_rout_dest_nod_cd" type="12" value="" out="N"/>
				<param name="i_rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
