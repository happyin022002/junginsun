<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetBkgRefNoRSQL">
			<desc><![CDATA[해당 BKG의 REF_NO를   BKG_REFERENCE 에서 BKG_REF_TP_CD 별로 가져오기]]></desc>
			<sql><![CDATA[
SELECT CUST_REF_NO_CTNT
  FROM BKG_REFERENCE
 WHERE 1=1
   AND BKG_NO = @[e_bkg_no]
   AND BKG_REF_TP_CD = @[e_bkg_ref_tp]			]]></sql>
			<params>
				<param name="e_bkg_no" type="12" value="" out="N"/>
				<param name="e_bkg_ref_tp" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
