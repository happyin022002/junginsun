<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KPICodeCreationDBDAOModifyKPICodeCreUSQL">
			<desc><![CDATA[KPI Code Creation 데이터를 수정 한다]]></desc>
			<sql><![CDATA[
UPDATE SPE_SP_SVC_CATE_KPI
   SET EV_SVC_CATE_CD = @[ev_svc_cate_cd]
     , SP_KPI_NM      = @[sp_kpi_nm]
     , SP_KPI_DESC    = @[sp_kpi_desc]
     , SP_KPI_TP_CD   = @[sp_kpi_tp_cd]
     , UPD_USR_ID     = @[upd_usr_id]
     , UPD_DT         = SYSDATE
 WHERE SP_KPI_ID      = @[sp_kpi_id]			]]></sql>
			<params>
				<param name="ev_svc_cate_cd" type="12" value="" out="N"/>
				<param name="sp_kpi_nm" type="12" value="" out="N"/>
				<param name="sp_kpi_desc" type="12" value="" out="N"/>
				<param name="sp_kpi_tp_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="sp_kpi_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
