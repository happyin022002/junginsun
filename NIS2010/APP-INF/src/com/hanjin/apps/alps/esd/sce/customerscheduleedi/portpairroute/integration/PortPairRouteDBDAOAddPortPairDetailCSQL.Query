<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairRouteDBDAOAddPortPairDetailCSQL">
			<desc><![CDATA[Port Pair Detail 테이블에 사용자가 manual로 입력하는 경우]]></desc>
			<sql><![CDATA[
INSERT INTO sce_port_pair_dtl( rout_rcv_dt         
		                               ,rout_seq            
		                               ,cust_trd_prnr_id    
		                               ,por_cd              
		                               ,n1st_pol_cd         
		                               ,n1st_pod_cd         
		                               ,n2nd_pol_cd         
		                               ,n2nd_pod_cd         
		                               ,n3rd_pol_cd         
		                               ,n3rd_pod_cd         
		                               ,n4th_pol_cd         
		                               ,n4th_pod_cd         
		                               ,del_cd              
		                               ,upd_ind_cd          		
		                               ,use_flg             
		                               ,mnl_use_flg         
		                               ,cre_usr_id          
		                               ,cre_dt              
		                               ,upd_usr_id          
		                               ,upd_dt              
		                               ,org_loc_cd          
		                               ,dest_loc_cd         
		                              )                     
		             VALUES( @[rout_rcv_dt]   
		                    ,@[rout_seq]
		                    ,@[cust_trd_prnr_id]                              
		                    ,@[por_cd]                              
		                    ,@[n1st_pol_cd]                              
		                    ,@[n1st_pod_cd]                              
		                    ,@[n2nd_pol_cd]                              
		                    ,@[n2nd_pod_cd]                              
		                    ,@[n3rd_pol_cd]                              
		                    ,@[n3rd_pod_cd]                              
		                    ,@[n4th_pol_cd]                              
		                    ,@[n4th_pod_cd] 
		                    ,@[del_cd]                             
		                    ,'S'                            
		                    ,'Y'                            
		                    ,''
		                    #if (${usr_id} != '')         
									      	  ,'$usr_id'
							#end                                 
		                    ,SYSDATE                        
		                    #if (${usr_id} != '')         
									      	  ,'$usr_id'
							#end                            
		                    ,SYSDATE                           
		                    ,@[pol_cd]                              
		                    ,@[pod_cd]                              
		                  )			]]></sql>
			<params>
				<param name="rout_rcv_dt" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="n1st_pol_cd" type="12" value="" out="N"/>
				<param name="n1st_pod_cd" type="12" value="" out="N"/>
				<param name="n2nd_pol_cd" type="12" value="" out="N"/>
				<param name="n2nd_pod_cd" type="12" value="" out="N"/>
				<param name="n3rd_pol_cd" type="12" value="" out="N"/>
				<param name="n3rd_pod_cd" type="12" value="" out="N"/>
				<param name="n4th_pol_cd" type="12" value="" out="N"/>
				<param name="n4th_pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
