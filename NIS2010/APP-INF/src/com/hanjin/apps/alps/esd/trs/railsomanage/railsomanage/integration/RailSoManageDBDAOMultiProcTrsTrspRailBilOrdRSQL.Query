<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOMultiProcTrsTrspRailBilOrdRSQL">
			<desc><![CDATA[프로시저에 들어갈 파라미터를 대상 조회 SQL]]></desc>
			<sql><![CDATA[
SELECT 
	A.TRSP_SO_OFC_CTY_CD, A.TRSP_SO_SEQ, B.SUB_RAIL_SEQ, B.TRSP_AGMT_OFC_CTY_CD, B.TRSP_AGMT_SEQ, 
	A.CRE_OFC_CD, B.VNDR_SEQ, A.LOCL_CRE_DT, 
	A.CRE_USR_ID, A.UPD_USR_ID, 
	A.EQ_KND_CD, A.EQ_TPSZ_CD, 
	A.CGO_TP_CD, A.TRSP_BND_CD, 
	A.TRSP_COST_DTL_MOD_CD, 
	A.CUST_CNT_CD, NVL(A.CUST_SEQ, 0) CUST_SEQ, 
	B.RAIL_CRR_TP_CD, A.CMDT_CD, 
	B.FM_NOD_CD, B.TO_NOD_CD, 0 BUNDLE_CNT, 
	A.WGT_MEAS_UT_CD, A.CNTR_WGT,
    A.SPCL_CGO_CNTR_TP_CD,
    A.RAIL_CMB_THRU_TP_CD,
    A.TO_NOD_CD AS AL_TO_NOD_CD
FROM 
    TRS_TRSP_RAIL_BIL_ORD A, 
    TRS_TRSP_RAIL_BIL_VNDR_SET B, 
    TRS_TRSP_EDI_RAIL_GLO_TMP C 
WHERE A.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD 
AND   A.TRSP_SO_SEQ = B.TRSP_SO_SEQ 
AND   A.TRSP_SO_OFC_CTY_CD = C.TRSP_SO_OFC_CTY_CD 
AND   A.TRSP_SO_SEQ = C.TRSP_SO_SEQ			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
