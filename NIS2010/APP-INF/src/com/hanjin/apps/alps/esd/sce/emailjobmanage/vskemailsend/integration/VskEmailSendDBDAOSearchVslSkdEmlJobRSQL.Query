<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VskEmailSendDBDAOSearchVslSkdEmlJobRSQL">
			<desc><![CDATA[Vessel Schedule email 송신의 화주 group code 별 송신 조건을 SCE_VSL_SKD_EML 테이블에서 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
	A.EML_JB_ID,
	A.SVC_ST_DT,
	A.SVC_END_DT,
	A.EML_GRP_ID,
	A.VSL_SLAN_CD,
	A.FM_PORT_CD,
	A.TO_PORT_CD,
	A.VSKD_DUR_ID,
	A.DELT_FLG,
	A.CRE_USR_ID,
	A.CRE_DT,
	A.UPD_USR_ID,
	A.UPD_DT,
	C.SUBSC_EML
FROM
	SCE_VSL_SKD_EML A,
	SCE_EML_JB_GRP B,
	SCE_EML_JB_SUBSC C
WHERE A.EML_JB_ID = @[emlJbId]
AND A.EML_JB_ID = B.EML_JB_ID
AND A.SVC_ST_DT = B.SVC_ST_DT
AND A.SVC_END_DT = B.SVC_END_DT
AND A.EML_GRP_ID = B.EML_GRP_ID
AND A.EML_JB_ID = C.EML_JB_ID
AND A.SVC_ST_DT = C.SVC_ST_DT
AND A.SVC_END_DT = C.SVC_END_DT
AND A.EML_GRP_ID = C.EML_GRP_ID
AND A.VSL_SLAN_CD = C.VSL_SLAN_CD
AND A.TO_PORT_CD = C.TO_PORT_CD
AND B.EML_SND_DYS_CTNT LIKE '%' || TO_CHAR(SYSDATE, 'd') || '%' 
AND B.EML_SND_HR = TO_CHAR(SYSDATE, 'hh24')
AND NVL(A.DELT_FLG, 'N' ) = 'N'
AND NVL(C.DELT_FLG, 'N' ) = 'N'
ORDER BY
    EML_GRP_ID ASC,
    SUBSC_EML ASC,
	VSL_SLAN_CD ASC			]]></sql>
			<params>
				<param name="emlJbId" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
