<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOUpdateNodeConstraintUSQL">
			<desc><![CDATA[UpdateNodeConstraint]]></desc>
			<sql><![CDATA[
UPDATE PRD_NOD_CNST_MGMT 
SET   NOD_CNST_ITM_CD = @[nod_cnst_itm_cd], 
	  PCTL_CNST_ITM_NM = (SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL 
						  WHERE INTG_CD_ID = 'CD01386' AND INTG_CD_VAL_CTNT = @[nod_cnst_itm_cd] ), 
	  NOD_CNST_RMK = @[nod_cnst_rmk] , 
	  CNTR_TP_CD = @[cntr_tp_cd] , 
	  SVC_USE_FLG = @[svc_use_flg] , 
	  EFF_FM_DT = REPLACE(@[eff_fm_dt], '-','') , 
	  EFF_TO_DT = REPLACE(@[eff_to_dt], '-','') , 
	  UPD_USR_ID = @[upd_usr_id] , 
	  UPD_OFC_CD = @[upd_ofc_cd] , 
	  UPD_DT = SYSDATE ,
	  PORT_PNT_CD = @[port_pnt_cd],
      VSL_SLAN_CD = @[vsl_slan_cd],
      VSL_CD      = substr(@[vvd], 1,4),
      SKD_VOY_NO  = substr(@[vvd], 5,4),
      SKD_DIR_CD  = substr(@[vvd], 9,1)
WHERE NOD_CD = @[nod_cd]
AND   NOD_CNST_ITM_CD = @[old_nod_cnst_itm_cd]
AND   NOD_CNST_SEQ = TO_NUMBER(@[nod_cnst_seq])			]]></sql>
			<params>
				<param name="nod_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_rmk" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="svc_use_flg" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="port_pnt_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="old_nod_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
