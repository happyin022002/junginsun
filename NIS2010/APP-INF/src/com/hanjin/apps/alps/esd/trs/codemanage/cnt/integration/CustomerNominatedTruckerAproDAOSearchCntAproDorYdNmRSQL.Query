<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerNominatedTruckerAproDAOSearchCntAproDorYdNmRSQL">
			<desc><![CDATA[Door Yard Name 조회]]></desc>
			<sql><![CDATA[
SELECT ZN_NM 
   FROM MDM_ZONE A
  WHERE A.ZN_CD = @[dor_nod_cd]
    AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="dor_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
