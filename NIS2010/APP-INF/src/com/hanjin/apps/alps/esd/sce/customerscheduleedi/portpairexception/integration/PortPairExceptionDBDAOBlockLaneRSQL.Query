<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PortPairExceptionDBDAOBlockLaneRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
  BLK.SLAN_CD,
  LAN.VSL_SLAN_NM,
  '' CUST_TRD_PRNR_ID,
  '' POL_CD,
  '' POD_CD,
  '' UPD_USR_ID,
  '' CRE_USR_ID,
  '' GUBUN
#if (${gubun} == 'A') 
  ,BLK.ROUT_RCV_DT
  ,BLK.ROUT_SEQ
  ,BLK.BLCK_SEQ
#end
FROM SCE_CUST_EDI_BLCK BLK, MDM_VSL_SVC_LANE LAN
#if (${gubun} == 'A') 
WHERE BLK.SLAN_CD = LAN.VSL_SLAN_CD(+)
  AND BLK.ROUT_RCV_DT = @[rout_rcv_dt]
  AND BLK.ROUT_SEQ = @[rout_seq]
  AND BLK.DELT_FLG = 'N'
#else
 	  ,SCE_PORT_PAIR_DTL SPR
WHERE SPR.ROUT_RCV_DT = BLK.ROUT_RCV_DT
  AND SPR.ROUT_SEQ = BLK.ROUT_SEQ
  AND BLK.SLAN_CD = LAN.VSL_SLAN_CD(+)
  AND SPR.CUST_TRD_PRNR_ID = @[cust_trd_prnr_id]
  AND SPR.ORG_LOC_CD = @[pol_cd]
  AND SPR.DEST_LOC_CD = @[pod_cd] 
  AND BLK.DELT_FLG = 'N'
#end			]]></sql>
			<params>
				<param name="rout_rcv_dt" type="12" value="" out="N"/>
				<param name="rout_seq" type="12" value="" out="N"/>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
