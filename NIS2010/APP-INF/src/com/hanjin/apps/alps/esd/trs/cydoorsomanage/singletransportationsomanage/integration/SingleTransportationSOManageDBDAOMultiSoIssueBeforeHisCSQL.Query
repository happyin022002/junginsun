<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOMultiSoIssueBeforeHisCSQL">
			<desc><![CDATA[S/O발행 Before History 기록]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_SVC_ORD_HIS
(
   TRSP_TMP_SEQ
  ,CTRL_OFC_CD
  ,COP_NO
  ,EQ_NO
  ,COST_ACT_GRP_SEQ
  ,COST_ACT_GRP_CD
  ,TRSP_CRR_MOD_CD
  ,TRSP_BND_CD
  ,TRSP_SO_OFC_CTY_CD
  ,TRSP_SO_SEQ
  ,TRSP_SO_STS_CD
  ,TRSP_SO_STS_NM
  ,FM_NOD_CD
  ,VIA_NOD_CD
  ,DOR_NOD_CD
  ,TO_NOD_CD
  ,BKG_NO
  ,UPLN_SO_FLG
  ,INTER_RMK
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
)
VALUES (
   0
  ,@[ctrl_ofc_cd]
  ,@[cop_no]
  ,@[eq_no]
  ,@[cost_act_grp_seq]
  ,@[cost_act_grp_cd]
  ,@[trsp_crr_mod_cd]
  ,@[trsp_bnd_cd]
  ,substr(@[cre_ofc_cd],1,3)
  ,@[trsp_so_seq]
  ,'C'
  ,@[replanSts]
  ,@[fm_nod_cd]||@[fm_nod_yard]
  ,@[via_nod_cd]||@[via_nod_yard]
  ,@[dor_nod_cd]||@[dor_nod_yard]
  ,@[to_nod_cd]||@[to_nod_yard]
  ,@[bkg_no]
  ,@[upln_so_flg]
  ,'T/M:'||@[trsp_crr_mod_cd2]||'|'||
   'FM:'||@[fm_nod_cd2]||@[fm_nod_yard2]||'|'||
   'VIA:'||@[via_nod_cd2]||@[via_nod_yard2]||'|'||
   'DOR:'||@[dor_nod_cd2]||@[dor_nod_yard2]||'|'||
   'TO:'||@[to_nod_cd2]||@[to_nod_yard2]
  ,'LOG_TMP'
  ,SYSDATE
  ,'LOG_TMP'
  ,SYSDATE
)			]]></sql>
			<params>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="" out="N"/>
				<param name="cost_act_grp_cd" type="12" value="" out="N"/>
				<param name="trsp_crr_mod_cd" type="12" value="" out="N"/>
				<param name="trsp_bnd_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="replanSts" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="fm_nod_yard" type="12" value="" out="N"/>
				<param name="via_nod_cd" type="12" value="" out="N"/>
				<param name="via_nod_yard" type="12" value="" out="N"/>
				<param name="dor_nod_cd" type="12" value="" out="N"/>
				<param name="dor_nod_yard" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_yard" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="upln_so_flg" type="12" value="" out="N"/>
				<param name="trsp_crr_mod_cd2" type="12" value="" out="N"/>
				<param name="fm_nod_cd2" type="12" value="" out="N"/>
				<param name="fm_nod_yard2" type="12" value="" out="N"/>
				<param name="via_nod_cd2" type="12" value="" out="N"/>
				<param name="via_nod_yard2" type="12" value="" out="N"/>
				<param name="dor_nod_cd2" type="12" value="" out="N"/>
				<param name="dor_nod_yard2" type="12" value="" out="N"/>
				<param name="to_nod_cd2" type="12" value="" out="N"/>
				<param name="to_nod_yard2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
