<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOInsertSPContactPointCSQL">
			<desc><![CDATA[SP Contact point 를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_EXPN_AUD_CS_CNTC_PNT(VNDR_SEQ
                                  ,  VNDR_CNTC_PNT_SEQ
                                  ,  VNDR_CNTC_PNT_NM
                                  ,  PSN_NM
                                  ,  PSN_DESC
                                  ,  STE_NM
                                  ,  CTY_NM
                                  ,  PHN_NO
                                  ,  FAX_NO
                                  ,  VNDR_EML
                                  ,  EAC_EML_USE_FLG
                                  ,  EAC_FAX_USE_FLG
                                  ,  DELT_FLG
                                  ,  CRE_USR_ID
                                  ,  CRE_DT
                                  ,  UPD_USR_ID
                                  ,  UPD_DT
                                  )VALUES(
                                     @[vndr_seq]
                                  ,  NVL((SELECT /*+ index_desc(A XPKEAS_EXPN_AUD_CS_CNTC_PNT) */ VNDR_CNTC_PNT_SEQ FROM EAS_EXPN_AUD_CS_CNTC_PNT A where VNDR_SEQ = @[vndr_seq] AND rownum=1),0)+1 
                                  ,  @[vndr_cntc_pnt_nm]
                                  ,  @[psn_nm]
                                  ,  @[psn_desc]
                                  ,  @[ste_nm]
                                  ,  @[cty_nm]
                                  ,  @[phn_no]
                                  ,  @[fax_no]
                                  ,  @[vndr_eml]
                                  ,  decode(@[eac_eml_use_flg],0,'N',1,'Y')
                                  ,  decode(@[eac_fax_use_flg],0,'N',1,'Y')
                                  ,  @[delt_flg]
                                  ,  @[cre_usr_id]
                                  ,  sysdate
                                  ,  @[upd_usr_id]
                                  ,  sysdate                                  
                                  )


			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vndr_cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="psn_nm" type="12" value="" out="N"/>
				<param name="psn_desc" type="12" value="" out="N"/>
				<param name="ste_nm" type="12" value="" out="N"/>
				<param name="cty_nm" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="vndr_eml" type="12" value="" out="N"/>
				<param name="eac_eml_use_flg" type="12" value="" out="N"/>
				<param name="eac_fax_use_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
