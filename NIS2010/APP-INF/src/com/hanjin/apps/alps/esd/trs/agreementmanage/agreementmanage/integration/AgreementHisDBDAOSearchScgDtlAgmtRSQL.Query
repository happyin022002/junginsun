    @[rail_svc_tp_cd] ,'OW' ,@[way],@[agmt_trsp_tp_cd] , @[trsp_agmt_ofc_cty_cd]
   ,@[trsp_agmt_seq]  ,@[trsp_agmt_rt_tp_ser_no], @[vndr_seq] ,nvl(globaldate_pkg.time_local_ofc_fnc(@[ctrt_ofc_cd]),TO_DATE(@[effective_date],'YYYY-MM-DD')),@[eq_knd_cd]         
