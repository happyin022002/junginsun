   ,@[trsp_agmt_seq]  ,@[trsp_agmt_rt_tp_ser_no], @[vndr_seq] ,nvl(globaldate_pkg.time_local_ofc_fnc(@[ctrt_ofc_cd]),sysdate),@[eq_knd_cd]         
