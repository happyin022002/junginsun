) TTL_SCG_RT
#end

FROM TRS_AGMT_EQ_TP_RULE T
WHERE TRSP_AGMT_RULE_TP_CD = 'X' 
 AND TRSP_AGMT_EQ_KND_CD = @[eq_knd_cd]
 
#if (${eq_tp} != 'AL') 
 AND TRSP_AGMT_EQ_TP_CD = @[eq_tp]
#end
#if (${eq_sz} != 'AL')
  #if (${eq_knd_cd} != 'G')
  AND TRSP_AGMT_EQ_SZ_CD = @[eq_sz]
  #end
#end			]]></sql>
