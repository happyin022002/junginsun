<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOMultiRehandlingVolumeUpdateUSQL">
			<desc><![CDATA[MultiRehandlingVolumeUpdate]]></desc>
			<sql><![CDATA[
UPDATE TES_TML_SO_RVIS_LIST
 SET
				  CNTR_NO               =  @[cntr_no]
				, BKG_NO                =  @[bkg_no]
				--, BKG_NO_SPLIT          =  [bkg_no_split]
				, CUZ_CNTR_NO           =  @[cuz_cntr_no]
				, RHND_RSN_CD           =  @[rhnd_rsn_cd]
				, RVIS_IND_FLG          =  DECODE(@[rvis_ind_flg],'1','Y','N')
				, UPD_USR_ID            =  @[upd_usr_id]
				, UPD_DT				=  SYSDATE
				, LOCL_UPD_DT           =  GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[locl_upd_dt])
 WHERE   TML_SO_OFC_CTY_CD      =  @[tml_so_ofc_cty_cd]
 AND     TML_SO_SEQ             =  @[tml_so_seq]
 AND     TML_SO_DTL_SEQ         =  @[tml_so_dtl_seq]
 AND     TML_SO_RVIS_LIST_SEQ   =  @[tml_so_rvis_list_seq]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cuz_cntr_no" type="12" value="" out="N"/>
				<param name="rhnd_rsn_cd" type="12" value="" out="N"/>
				<param name="rvis_ind_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="locl_upd_dt" type="12" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="tml_so_dtl_seq" type="12" value="" out="N"/>
				<param name="tml_so_rvis_list_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
