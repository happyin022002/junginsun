<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerNominatedTruckerAproDAOSearchCntAproDorLocNmRSQL">
			<desc><![CDATA[Location Name]]></desc>
			<sql><![CDATA[
SELECT LOC_NM ZN_NM 
   FROM MDM_LOCATION A
  WHERE A.LOC_CD = @[dor_nod_cd]
    AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="dor_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
