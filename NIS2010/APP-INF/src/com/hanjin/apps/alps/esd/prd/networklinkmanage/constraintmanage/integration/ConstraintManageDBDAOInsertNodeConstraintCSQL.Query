<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOInsertNodeConstraintCSQL">
			<desc><![CDATA[InsertNodeConstraint]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_NOD_CNST_MGMT ( 
			NOD_CD, NOD_CNST_ITM_CD, 
            NOD_CNST_SEQ ,
            PCTL_CNST_ITM_NM,
            NOD_CNST_RMK, CNTR_TP_CD, SVC_USE_FLG, EFF_FM_DT, EFF_TO_DT, 
			CRE_DT, CRE_USR_ID, CRE_OFC_CD, UPD_USR_ID, UPD_OFC_CD, UPD_DT, 
			CMDT_CD , PORT_PNT_CD, VSL_SLAN_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD
			) 
VALUES( @[nod_cd], @[nod_cnst_itm_cd], 
        (select max(nod_cnst_seq)+ to_number(@[nod_cnst_seq]) from prd_nod_cnst_mgmt),
        (SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01386' AND INTG_CD_VAL_CTNT = @[nod_cnst_itm_cd]),
        @[nod_cnst_rmk], @[cntr_tp_cd], @[svc_use_flg], REPLACE(@[eff_fm_dt], '-',''), REPLACE(@[eff_to_dt], '-',''),
        SYSDATE, @[cre_usr_id], @[cre_ofc_cd],@[upd_usr_id], @[upd_ofc_cd],SYSDATE,
		@[cmdt_cd] ,  @[port_pnt_cd], @[vsl_slan_cd], SUBSTR(@[vvd], 1,4),SUBSTR(@[vvd], 5,4),SUBSTR(@[vvd], 9,1)
)			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_seq" type="12" value="" out="N"/>
				<param name="nod_cnst_rmk" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="svc_use_flg" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="port_pnt_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
