<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOModifyMarineTerminalInvoiceAccumulatedVolumeUpdateUSQL">
			<desc><![CDATA[ModifyMarineTerminalInvoiceAccumulatedVolumeUpdate]]></desc>
			<sql><![CDATA[
UPDATE TES_TML_SO_ACCM
SET PAY_VOL_QTY = replace(@[pay_vol_qty],',')
 , UPD_USR_ID   = @[upd_usr_id]
 , UPD_DT     	= SYSDATE
WHERE VNDR_SEQ	= @[vndr_seq]
AND ACCM_SEQ  	= @[accm_seq]			]]></sql>
			<params>
				<param name="pay_vol_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="accm_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
