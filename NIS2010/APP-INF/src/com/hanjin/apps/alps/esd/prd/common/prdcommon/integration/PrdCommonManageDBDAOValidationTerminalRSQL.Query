<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PrdCommonManageDBDAOValidationTerminalRSQL">
			<desc><![CDATA[ValidationTerminal]]></desc>
			<sql><![CDATA[
SELECT y.yd_cd yard_code
FROM mdm_yard y
WHERE y.yd_cd = @[check_data] AND NVL( delt_flg,'N') <> 'Y'			]]></sql>
			<params>
				<param name="check_data" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
