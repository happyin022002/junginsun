<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOMultiVolumeAccumulatedYardCSQL">
			<desc><![CDATA[Yard Insert]]></desc>
			<sql><![CDATA[
INSERT INTO TES_TML_SO_ACCM_YD ( 
	vndr_seq
	, accm_seq
	, accm_dtl_seq
	, yd_cd
	, cre_usr_id
	, cre_dt
	, upd_usr_id
	, upd_dt  
	, locl_cre_dt
	) 
SELECT	@[vndr_seq]
	, @[accm_seq]
	, NVL(MAX(accm_dtl_seq), 0) + 1
	, @[yd_cd]
	, @[cre_usr_id]
	, SYSDATE
	, @[upd_usr_id]
	, SYSDATE
	, GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])
FROM	TES_TML_SO_ACCM_YD 
WHERE	1	= 1  
AND		vndr_seq	= @[vndr_seq]
AND		accm_seq	= @[accm_seq]
			]]></sql>
			<params>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="accm_seq" type="2" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
