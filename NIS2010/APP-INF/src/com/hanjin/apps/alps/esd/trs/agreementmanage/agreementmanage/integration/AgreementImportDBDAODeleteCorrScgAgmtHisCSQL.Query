		<query name="AgreementImportDBDAODeleteCorrScgAgmtHisCSQL">
			<desc><![CDATA[Surcharge Rate Delete History 입력]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_AGMT_SCG_RT_HIS (
 TRSP_AGMT_OFC_CTY_CD
,TRSP_AGMT_SEQ
,TRSP_AGMT_RT_TP_SER_NO
,TRSP_AGMT_SCG_NOD_SEQ
,TRSP_AGMT_SCG_RT_SEQ
,TRSP_AGMT_RT_HIS_SEQ
,TRSP_AGMT_EQ_TP_SZ_CD
,EQ_KND_CD
,TO_WGT
,WGT_MEAS_UT_CD
,EFF_FM_DT
,EFF_TO_DT
,CURR_CD
,TRSP_ONE_WY_RT
,TRSP_RND_RT
,DELT_FLG
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
)  
SELECT TRSP_AGMT_OFC_CTY_CD
      ,TRSP_AGMT_SEQ
      ,TRSP_AGMT_RT_TP_SER_NO
      ,TRSP_AGMT_SCG_NOD_SEQ
      ,TRSP_AGMT_SCG_RT_SEQ
      ,TRS_AGMT_SCG_RT_HIS_SEQ1.NEXTVAL
      ,TRSP_AGMT_EQ_TP_SZ_CD
      ,EQ_KND_CD
      ,TO_WGT
      ,WGT_MEAS_UT_CD
      ,EFF_FM_DT
      ,EFF_TO_DT
      ,CURR_CD
      ,TRSP_ONE_WY_RT
      ,TRSP_RND_RT
      ,'Y'
      ,@[fm_account_usr_id]
      ,SYSDATE
      ,@[fm_account_usr_id]
      ,SYSDATE
 FROM TRS_AGMT_SCG_RT
WHERE TRSP_AGMT_OFC_CTY_CD   = @[trsp_agmt_ofc_cty_cd]
  AND TRSP_AGMT_SEQ          = @[trsp_agmt_seq]
  AND TRSP_AGMT_RT_TP_SER_NO = @[trsp_agmt_rt_tp_ser_no]
  AND TRSP_AGMT_SCG_NOD_SEQ  = @[trsp_agmt_scg_seq]
  AND TRSP_AGMT_SCG_RT_SEQ   = @[trsp_agmt_rt_seq]			]]></sql>
			<params>
				<param name="fm_account_usr_id" type="12" value="" out="N"/>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="trsp_agmt_rt_tp_ser_no" type="12" value="" out="N"/>
				<param name="trsp_agmt_scg_seq" type="12" value="" out="N"/>
				<param name="trsp_agmt_rt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
