<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOMultiEacTESAutoAuditCSQL">
			<desc><![CDATA[TES Auto Audit에서 EAC I/F 내역 입력]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_TML_AUD_DTL A (
 INV_NO
,VNDR_SEQ
,INV_CFM_DT
,EXPN_AUD_SEQ
,EXPN_AUD_DTL_SEQ
,CALC_TP_CD
,LGS_COST_CD
,CNTR_TPSZ_CD
,STO_CNTR_SZ_NM
,IO_BND_CD
,DCGO_FLG
,RC_FLG
,TML_WRK_DY_CD
,FP_CALC_PRD_CD
,CALC_COST_GRP_CD 
,EAC_NO
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
)
VALUES (
 @[inv_no]
,@[vndr_seq]
,TO_DATE(@[inv_cfm_dt], 'YYYY-MM-DD HH24:MI:SS')
,@[expn_aud_seq]
,NVL((SELECT EXPN_AUD_DTL_SEQ
        FROM EAS_TML_AUD_DTL X
       WHERE X.INV_NO = A.INV_NO
         AND X.VNDR_SEQ = A.VNDR_SEQ
         AND X.INV_CFM_DT = A.INV_CFM_DT
         AND X.EXPN_AUD_SEQ = A.EXPN_AUD_SEQ
     ),0) + 1
,@[calc_tp_cd]
,@[lgs_cost_cd]
,@[cntr_tpsz_cd]
,@[sto_cntr_sz_nm]
,@[io_bnd_cd]
,@[dcgo_flg]
,@[rc_flg]
,@[tml_wrk_dy_cd]
,@[fp_calc_prd_cd]
,@[calc_cost_grp_cd]
,@[eac_no]
,NVL(@[usr_id], 'TES')
,SYSDATE
,NVL(@[usr_id], 'TES')
,SYSDATE
)
			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="inv_cfm_dt" type="12" value="" out="N"/>
				<param name="expn_aud_seq" type="12" value="" out="N"/>
				<param name="calc_tp_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="sto_cntr_sz_nm" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="dcgo_flg" type="12" value="" out="N"/>
				<param name="rc_flg" type="12" value="" out="N"/>
				<param name="tml_wrk_dy_cd" type="12" value="" out="N"/>
				<param name="fp_calc_prd_cd" type="12" value="" out="N"/>
				<param name="calc_cost_grp_cd" type="12" value="" out="N"/>
				<param name="eac_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
