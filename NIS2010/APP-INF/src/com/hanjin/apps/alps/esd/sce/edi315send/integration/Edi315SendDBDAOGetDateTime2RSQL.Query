<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetDateTime2RSQL">
			<desc><![CDATA[for getting date]]></desc>
			<sql><![CDATA[
SELECT
     to_char((to_date(@[event_dt], 'YYYYMMDDHH24MISS')) + @[edi_snd_itval_hr]/(24),'YYYYMMDDHH24MISS') dateTime
FROM DUAL			]]></sql>
			<params>
				<param name="event_dt" type="12" value="20091122112233" out="N"/>
				<param name="edi_snd_itval_hr" type="12" value="10" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
