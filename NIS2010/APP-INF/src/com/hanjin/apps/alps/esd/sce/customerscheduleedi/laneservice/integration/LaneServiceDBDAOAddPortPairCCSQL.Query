<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LaneServiceDBDAOAddPortPairCCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_PORT_PAIR_DTL(
	ROUT_RCV_DT
	,ROUT_SEQ
	,CUST_TRD_PRNR_ID
	,POR_CD
	,N1ST_POL_CD
	,N1ST_POD_CD
	,N2ND_POL_CD
	,N2ND_POD_CD
	,N3RD_POL_CD
	,N3RD_POD_CD
	,N4TH_POL_CD
	,N4TH_POD_CD
	,DEL_CD
	,UPD_IND_CD
	,USE_FLG
	,MNL_USE_FLG
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
	,ORG_LOC_CD
	,DEST_LOC_CD
	,CUST_RMK
	,VSL_SLAN_CD
) VALUES (
	 @[rout_rcv_dt]
	,(SELECT NVL(MAX(ROUT_SEQ), 0)+1 FROM SCE_PORT_PAIR_DTL WHERE ROUT_RCV_DT = @[rout_rcv_dt])
	,@[cust_trd_prnr_id]
	,@[por_cd]
	,@[n1st_pol_cd]
	,@[n1st_pod_cd]
	,@[n2nd_pol_cd]
	,@[n2nd_pod_cd]
	,@[n3rd_pol_cd]
	,@[n3rd_pod_cd]
	,@[n4th_pol_cd]
	,@[n4th_pod_cd]
	,@[del_cd]
	,@[upd_ind_cd]
	,'Y'
	,'Y'
	,@[cre_usr_id]
	,sysdate
	,@[upd_usr_id]
	,sysdate
	,@[org_loc_cd]
	,@[dest_loc_cd]
	,@[cust_rmk]
	,@[vsl_slan_cd]
)			]]></sql>
			<params>
				<param name="rout_rcv_dt" type="12" value="" out="N"/>
				<param name="cust_trd_prnr_id" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="n1st_pol_cd" type="12" value="" out="N"/>
				<param name="n1st_pod_cd" type="12" value="" out="N"/>
				<param name="n2nd_pol_cd" type="12" value="" out="N"/>
				<param name="n2nd_pod_cd" type="12" value="" out="N"/>
				<param name="n3rd_pol_cd" type="12" value="" out="N"/>
				<param name="n3rd_pod_cd" type="12" value="" out="N"/>
				<param name="n4th_pol_cd" type="12" value="" out="N"/>
				<param name="n4th_pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="upd_ind_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="org_loc_cd" type="12" value="" out="N"/>
				<param name="dest_loc_cd" type="12" value="" out="N"/>
				<param name="cust_rmk" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
