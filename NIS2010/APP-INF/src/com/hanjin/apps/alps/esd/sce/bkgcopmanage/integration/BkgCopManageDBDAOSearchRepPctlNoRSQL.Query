<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOSearchRepPctlNoRSQL">
			<desc><![CDATA[PC No 가 없는 상황일 때 Booking 내의 cop 들 중 대표적으로 사용 가능한 PCTL_NO 를 반환한다.]]></desc>
			<sql><![CDATA[
select SCE_REP_PCTL_NO_FNC(@[bkg_no], @[cntr_tpsz_cd]) as PCTL_NO from dual			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
