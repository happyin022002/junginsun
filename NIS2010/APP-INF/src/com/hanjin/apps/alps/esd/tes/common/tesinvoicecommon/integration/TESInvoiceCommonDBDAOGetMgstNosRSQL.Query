<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESInvoiceCommonDBDAOGetMgstNosRSQL">
			<desc><![CDATA[GetMgstNos]]></desc>
			<sql><![CDATA[
SELECT	EQ_NO
		, EQ_TPSZ_CD 
FROM	CGM_EQUIPMENT 
WHERE	EQ_KND_CD = 'G'
AND		ACIAC_DIV_CD='A'
AND		EQ_NO IN ( ${eq_no} )			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
