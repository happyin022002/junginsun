      ,UPD_USR_ID        = @[upd_usr_id]
      ,UPD_DT            = SYSDATE
 WHERE COST_TRF_NO = @[cost_trf_no]
   AND COST_ROUT_GRP_NO = @[cost_rout_grp_no]
   AND COST_SEL_ROUT_FLG <> 'Y'			]]></sql>
