	  ,RAIL_40FT_BZC_COST_ADJ_AMT = 0
	  ,RAIL_20FT_FUEL_SCG_TTL_AMT = RAIL_20FT_FUEL_SCG_AMT
	  ,RAIL_20FT_FUEL_SCG_ADJ_AMT = 0
	  ,RAIL_40FT_FUEL_SCG_TTL_AMT = RAIL_40FT_FUEL_SCG_AMT
