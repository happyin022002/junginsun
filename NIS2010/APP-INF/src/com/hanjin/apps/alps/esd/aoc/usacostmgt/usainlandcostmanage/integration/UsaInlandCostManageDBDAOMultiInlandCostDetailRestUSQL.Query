	  ,TRK_40FT_BZC_COST_ADJ_AMT  = 0
	  ,TRK_20FT_FUEL_SCG_TTL_AMT  = TRK_20FT_FUEL_SCG_AMT
	  ,TRK_20FT_FUEL_SCG_ADJ_AMT  = 0
	  ,TRK_40FT_FUEL_SCG_TTL_AMT  = TRK_40FT_FUEL_SCG_AMT
	  ,TRK_40FT_FUEL_SCG_ADJ_AMT  = 0
      ,MTY_TRSP_20FT_TTL_COST_AMT = MTY_TRSP_20FT_COST_AMT
      ,MTY_TRSP_40FT_TTL_COST_AMT = MTY_TRSP_40FT_COST_AMT
      ,MTY_TRSP_20FT_ADJ_COST_AMT = 0
      ,MTY_TRSP_40FT_ADJ_COST_AMT = 0
