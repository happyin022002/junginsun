<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDtlActDtUSQL">
			<desc><![CDATA[ModifyCopDtlActDt]]></desc>
			<sql><![CDATA[
update  sce_cop_dtl
                   set  act_dt          = to_date(@[in_cnmv_evnt_dt],'yyyymmddhh24miss')
                        ,act_rcv_tp_cd   = @[P_ACT_RCV_TP_CD]
                        ,upd_usr_id      = @[in_usr_id]
                        ,upd_dt          = sysdate
                        ,edi_msg_tp_cd   = @[v_edi_msg_tp_cd]
                        ,vndr_seq        = @[v_vndr_seq]                  
                        ,act_sts_mapg_cd = @[in_mvmt_sts_cd]
                        ,ACT_DAT_RCV_DT = TO_DATE(@[v_org_act_rcv_dt],'YYYY/MM/DD HH24:MI:SS')
                 where  cop_no      = @[IN_COP_NO]
                   and  cop_dtl_seq = @[v_cop_dtl_seq]
                   and  act_dt is null
                   and  act_sts_cd  = 'F'			]]></sql>
			<params>
				<param name="in_cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="P_ACT_RCV_TP_CD" type="12" value="" out="N"/>
				<param name="in_usr_id" type="12" value="" out="N"/>
				<param name="v_edi_msg_tp_cd" type="12" value="" out="N"/>
				<param name="v_vndr_seq" type="12" value="" out="N"/>
				<param name="in_mvmt_sts_cd" type="12" value="" out="N"/>
				<param name="v_org_act_rcv_dt" type="12" value="" out="N"/>
				<param name="IN_COP_NO" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
