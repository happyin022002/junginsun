<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceNoticeDBDAOsearchInvoiceNoticeListRSQL">
			<desc><![CDATA[searchInvoiceNoticeList]]></desc>
			<sql><![CDATA[
SELECT ROWNUM, A.* from 
( 
SELECT ROWNUM row_num, 
	TO_CHAR(INV.cre_dt,'YYYY-MM-DD') cre_dt, 
	INV.inv_no, 
	INV.trsp_inv_aud_sts_cd, 
	(select intg_cd_val_dp_desc from com_intg_cd_dtl where intg_cd_id = 'CD00824' and intg_cd_val_ctnt = INV.trsp_inv_aud_sts_cd) status 
FROM trs_trsp_inv_wrk INV 
WHERE INV.inv_vndr_seq = @[inv_vndr_seq] 
	AND NVL(INV.delt_flg, 'E') <> 'Y'
 	AND INV.cre_dt >= to_date('20170213', 'yyyyMMdd')
ORDER BY INV.cre_dt desc 
) A 
WHERE ROWNUM < (@[max_count] +1)			]]></sql>
			<params>
				<param name="inv_vndr_seq" type="2" value="195931" out="N"/>
				<param name="max_count" type="12" value="5" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
