<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOSearchNodeConstraintRSQL">
			<desc><![CDATA[SearchNodeConstraint]]></desc>
			<sql><![CDATA[
SELECT
	 NOD_CD, NOD_CNST_ITM_CD, PCTL_CNST_ITM_NM, NOD_CNST_RMK, 
	 CNTR_TP_CD, EFF_FM_DT, EFF_TO_DT, SVC_USE_FLG, 
	 CRE_DT, CRE_OFC_CD, CRE_USR_ID , UPD_DT, UPD_OFC_CD, UPD_USR_ID, 
     NOD_CNST_SEQ , NOD_CNST_ITM_CD OLD_NOD_CNST_ITM_CD, CMDT_CD, CMDT_NM,PORT_PNT_CD, VSL_SLAN_CD, VVD,
     DECODE (CNST_CST_EXPT, 0, '', 'Y') AS CNST_CST_EXPT
FROM (
	SELECT NOD_CD, NOD_CNST_ITM_CD, PCTL_CNST_ITM_NM, NOD_CNST_RMK, 
 		CNTR_TP_CD, EFF_FM_DT, EFF_TO_DT, SVC_USE_FLG, 
		TO_CHAR(CRE_DT, 'YYYY-MM-DD') CRE_DT, CRE_OFC_CD, CRE_USR_ID , NOD_CNST_SEQ, P.CMDT_CD 
       ,(SELECT CMDT_NM FROM MDM_COMMODITY M WHERE M.CMDT_CD  = P.CMDT_CD) CMDT_NM , NVL(PORT_PNT_CD, 'ALL') PORT_PNT_CD 
       , TO_CHAR(UPD_DT, 'YYYY-MM-DD') UPD_DT, UPD_OFC_CD, UPD_USR_ID,
       VSL_SLAN_CD, VSL_CD || SKD_VOY_NO || SKD_DIR_CD VVD,
       (SELECT COUNT (*) AS CNT 
          FROM PRD_NOD_CNST_EXPT U
         WHERE     U.NOD_CD = P.NOD_CD
               AND U.NOD_CNST_ITM_CD = P.NOD_CNST_ITM_CD
               AND U.NOD_CNST_SEQ = P.NOD_CNST_SEQ
               AND U.DELT_FLG = 'N') AS CNST_CST_EXPT
	FROM PRD_NOD_CNST_MGMT P
	WHERE DECODE(NOD_CD,'ALL',@[loc]||'%',NOD_CD) LIKE @[loc]||'%'
    AND NVL(DELT_FLG, 'N') ='N'
    AND NVL(CMDT_CD, '#') = NVL(TRIM(@[cmdt_cd]), NVL(CMDT_CD, '#')) 
    AND NVL(VSL_SLAN_CD,'#') = NVL(TRIM(@[vsl_slan_cd]), NVL(VSL_SLAN_CD,'#'))
    AND NVL(VSL_CD || SKD_VOY_NO || SKD_DIR_CD, '#') LIKE NVL(TRIM(@[vvd]), NVL(VSL_CD || SKD_VOY_NO || SKD_DIR_CD,'%'))
)
WHERE PORT_PNT_CD LIKE DECODE(TRIM(@[point_code]),'ALL' , '%', NULL, '%', @[point_code])
AND NVL(UPPER(CMDT_NM),'#') LIKE DECODE(TRIM(@[cmdt_nm]), NULL, '%', '%' || UPPER(TRIM(@[cmdt_nm])) || '%')			]]></sql>
			<params>
				<param name="loc" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="point_code" type="12" value="" out="N"/>
				<param name="cmdt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
