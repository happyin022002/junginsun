<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualRegisterDBDAOCreateEACRegistrationCSQL">
			<desc><![CDATA[CreateEACRegistration]]></desc>
			<sql><![CDATA[
CALL TPB_CREATE_EAC_PRC(
	 @[s_reg_type]
	,@[n3pty_no]
	,@[s_n3pty_expn_tp_cd]
	,@[s_n3pty_bil_tp_cd]
	,@[s_edn_tp_cd]
	,@[s_if_ofc_cd]
	,@[if_amt]
	,@[s_curr_cd]
	,@[s_bkg_no]
	,@[s_bkg_no_split]
	,@[s_bl_no_chk]
	,@[s_bl_no]
	,@[s_bl_no_tp]

#if (${s_n3pty_expn_tp_cd} =='PSO')
	, SUBSTR(@[eq_no],1,4)
	, SUBSTR(@[eq_no],5,4)
	, SUBSTR(@[eq_no],9,1)
#else
	, @[s_vsl_cd]
	, @[s_skd_voy_no]
	, @[s_skd_dir_cd]
#end
	,@[eq_knd_cd]
	,@[eq_no]
	,@[eq_tpsz_cd]
	,@[s_n3pty_src_no]
	,@[s_so_no]
	,@[s_vndr_cust_div_cd]
	,@[s_vndr_cnt_cd]
	,@[s_vndr_seq]
	,@[s_cust_cnt_cd]
	,@[s_cust_seq]
	,@[s_n3pty_ofc_cd]
	,@[s_if_rmk]
	,@[s_src_vndr_cnt_cd]
	,@[s_src_vndr_seq]
	,@[s_if_dt]
	,@[s_yd_cd]
	,@[s_file_no]
	,@[s_user_id]
	,@[s_ofc_cd]
	,@[s_svr_id]
	,@[s_ofc_cd]
)			]]></sql>
			<params>
				<param name="s_reg_type" type="12" value="" out="N"/>
				<param name="n3pty_no" type="12" value="" out="N"/>
				<param name="s_n3pty_expn_tp_cd" type="12" value="" out="N"/>
				<param name="s_n3pty_bil_tp_cd" type="12" value="" out="N"/>
				<param name="s_edn_tp_cd" type="12" value="" out="N"/>
				<param name="s_if_ofc_cd" type="12" value="" out="N"/>
				<param name="if_amt" type="12" value="" out="N"/>
				<param name="s_curr_cd" type="12" value="" out="N"/>
				<param name="s_bkg_no" type="12" value="" out="N"/>
				<param name="s_bkg_no_split" type="12" value="" out="N"/>
				<param name="s_bl_no_chk" type="12" value="" out="N"/>
				<param name="s_bl_no" type="12" value="" out="N"/>
				<param name="s_bl_no_tp" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="s_vsl_cd" type="12" value="" out="N"/>
				<param name="s_skd_voy_no" type="12" value="" out="N"/>
				<param name="s_skd_dir_cd" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="s_n3pty_src_no" type="12" value="" out="N"/>
				<param name="s_so_no" type="12" value="" out="N"/>
				<param name="s_vndr_cust_div_cd" type="12" value="" out="N"/>
				<param name="s_vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="s_vndr_seq" type="12" value="" out="N"/>
				<param name="s_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="s_cust_seq" type="12" value="" out="N"/>
				<param name="s_n3pty_ofc_cd" type="12" value="" out="N"/>
				<param name="s_if_rmk" type="12" value="" out="N"/>
				<param name="s_src_vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="s_src_vndr_seq" type="12" value="" out="N"/>
				<param name="s_if_dt" type="12" value="" out="N"/>
				<param name="s_yd_cd" type="12" value="" out="N"/>
				<param name="s_file_no" type="12" value="" out="N"/>
				<param name="s_user_id" type="12" value="" out="N"/>
				<param name="s_ofc_cd" type="12" value="" out="N"/>
				<param name="s_svr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
