<sqls>
	<querys>
		<query name="PrdCreateManageDBDAOUpdatePrdMstRoutCnstUSQL">
			<desc><![CDATA[PrdCreateManageDBDAOUpdatePrdMstRoutCnstUSQL]]></desc>
			<sql><![CDATA[
UPDATE PRD_PROD_CTL_MST M																	   
 SET CNST_FLG = ( SELECT MAX(CNST_FLG) FROM PRD_PROD_CTL_ROUT_DTL WHERE PCTL_NO = M.PCTL_NO ) 
 WHERE M.PCTL_NO LIKE @[hd_pctl_no]||'%'																
					]]></sql>
			<params>
				<param name="hd_pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
