<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi214ReceiveDBDAOModifyEDI214ActualDateUSQL">
			<desc><![CDATA[update edi214]]></desc>
			<sql><![CDATA[
update SCE_COP_DTL
SET 	ACT_DT 			=  TO_DATE(@[rcv_dt]||@[rcv_hr]||@[rcv_mnt]||'00','YYYYMMDDHH24MISS')	
		,ACT_RCV_TP_CD 	= '0' 				
		,UPD_USR_ID 	= 'EDI_214_PoD'	
		,UPD_DT 		=  sysdate 				
		,ACT_DAT_RCV_DT =  GLOBALDATE_PKG.TIME_CONV_FNC('KRPUS', SYSDATE, SUBSTR(NOD_CD,1,5))
WHERE COP_NO = (select cop_no from trs_trsp_svc_ord where TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd] and TRSP_SO_SEQ = @[trsp_so_seq] )
AND COP_DTL_SEQ = TO_NUMBER(SUBSTR(@[cop_seq],1,4)) 			]]></sql>
			<params>
				<param name="rcv_dt" type="12" value="" out="N"/>
				<param name="rcv_hr" type="12" value="" out="N"/>
				<param name="rcv_mnt" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
				<param name="cop_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
