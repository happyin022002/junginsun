<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceInquiryDBDAOsearchInvoiceInquiryExcelExtractRSQL">
			<desc><![CDATA[search Invoice Inquiry Excel Extract]]></desc>
			<sql><![CDATA[
SELECT ROWNUM, C.* FROM (
    SELECT   inv_no
    		,wo_no
    		,so_no
    		,eq_no
    		,vndr_nm
    		,inv_curr_cd
    		,inv_iss_dt
    		,inv_rcv_dt
    		,inv_rjct_flg
    		,status
    		,paidDate
    		,paymentMethod
    		,checkTTNumber
    		,(A.inv_bzc_amt + A.inv_surcharge ) inv_ttl_amt
    		,if_sys_knd_name
    		,trsp_kind_nm
    		,trsp_mode_nm
    		,trsp_type_nm
    		,fm_nod_cd
    		,to_nod_cd
    		,eq_tpsz_cd
    		,sp_inv_rmk
			,'' INV_EDI_RSLT_CD
      FROM (
    		SELECT DISTINCT
    				INV.inv_no,
    				SVC.trsp_wo_ofc_cty_cd || SVC.trsp_wo_seq wo_no,
    				SVC.trsp_so_ofc_cty_cd || SVC.trsp_so_seq so_no,
    				NVL(SVC.eq_no,'') eq_no,
    				(	select vndr_lgl_eng_nm
    				from mdm_vendor
    				where vndr_seq = INV.inv_vndr_seq
    				and rownum =1	) 			vndr_nm,
    				INV.inv_curr_cd,
    				NVL(SVC.inv_bzc_amt, 0.0) inv_bzc_amt,
    				(	select nvl(sum(inv_scg_amt),0.0) inv_surcharge
    				from trs_trsp_scg_dtl
    				where (trsp_so_ofc_cty_cd = SVC.trsp_so_ofc_cty_cd
    				and trsp_so_seq = SVC.trsp_so_seq)		) inv_surcharge,
    				TO_CHAR(INV.inv_iss_dt, 'YYYY-MM-DD') inv_iss_dt,
    				TO_CHAR(INV.inv_rcv_dt, 'YYYY-MM-DD') inv_rcv_dt,
    				INV.trsp_inv_aud_sts_cd status,
    				NVL(INV.inv_rjct_flg,'E') inv_rjct_flg,
    				(	select intg_cd_val_dp_desc
    		 			  from com_intg_cd_dtl
    		 			 where intg_cd_id = 'CD00824'
    		 			   and intg_cd_val_ctnt = INV.trsp_inv_aud_sts_cd		) status_nm,
    				TO_CHAR(INV.pay_dt, 'YYYY-MM-DD') paidDate,
    				INV.inv_pay_mzd_cd paymentMethod,
    				INV.inv_chk_trns_no checkTTNumber
    				,DECODE(INV.IF_SYS_KND_CD,'I','EDI','WEB') if_sys_knd_name
    				,SVC.sp_inv_rmk
    				,(select intg_cd_val_dp_desc	from com_intg_cd_dtl
    				where intg_cd_id = 'CD00958'
    				and intg_cd_val_ctnt = SVC.trsp_cost_dtl_mod_cd) trsp_kind_nm
    				,(select intg_cd_val_dp_desc	from com_intg_cd_dtl
    				where intg_cd_id = 'CD00283'
    				and intg_cd_val_ctnt = SVC.trsp_crr_mod_cd) trsp_mode_nm
    				,decode(WRK.wo_fmt_tp_cd,'CC','Combined Case 1',
    				'CM','Combined Case 2','CY','Combined Case 2','IB','Combined Case 2',
    				'NC','Normal','MM','Empty' ) trsp_type_nm
    				,SVC.fm_nod_cd
    				,SVC.to_nod_cd
    				,SVC.eq_tpsz_cd
					,''
    		 FROM trs_trsp_inv_wrk INV, trs_trsp_svc_ord SVC
    			 ,trs_trsp_wrk_ord wrk
    		WHERE INV.inv_no IS NOT NULL
    		  AND INV.inv_no = SVC.inv_no
    		  AND inv.INV_VNDR_SEQ = svc.INV_VNDR_SEQ
    		  AND NVL(SVC.delt_flg,'E') != 'Y'
    		  AND NVL(INV.delt_flg, 'E') <> 'Y'
    #if(${vndr_dvsn} == 'S')
    	AND INV.inv_vndr_seq = (select prnt_vndr_seq from mdm_vendor where vndr_seq = @[vndr_seq])
    #end
    		#if ($inv_no.size() > 0)
    		AND  (1,INV.inv_no) in (
    			#foreach($inv_no_key IN ${inv_no})
    				#if($velocityCount < $inv_no.size())
    					(1,'$inv_no_key') ,
    				#else
    					(1,'$inv_no_key')
    				#end
    			#end
    			)
    		#else
    			AND 1=2
    		#end
    		 AND WRK.TRSP_WO_OFC_CTY_CD = SVC.TRSP_WO_OFC_CTY_CD
    		 AND WRK.TRSP_WO_SEQ = SVC.TRSP_WO_SEQ
    		 AND NVL(WRK.delt_flg, 'N') = 'N'
    		ORDER BY inv_rcv_dt DESC
    ) A
    
    UNION
    
    SELECT   inv_no
    		,wo_no
    		,so_no
    		,eq_no
    		,vndr_nm
    		,inv_curr_cd
    		,inv_iss_dt
    		,inv_rcv_dt
    		,inv_rjct_flg
    		,status
    		,paidDate
    		,paymentMethod
    		,checkTTNumber
    		,inv_amt
    		,if_sys_knd_name
    		,trsp_kind_nm
    		,trsp_mode_nm
    		,trsp_type_nm
    		,fm_nod_cd
    		,to_nod_cd
    		,eq_tpsz_cd
    		,inv_rmk
			,INV_EDI_RSLT_CD
      FROM (
    		SELECT DISTINCT
    				EDI.inv_no,
    				EDI.trsp_wo_ofc_cty_cd || EDI.trsp_wo_seq wo_no,
    				EDI.trsp_so_ofc_cty_cd || EDI.trsp_so_seq so_no,
    				NVL(EDI.eq_no,'') eq_no,
    				(	select vndr_lgl_eng_nm
    				from mdm_vendor
    				where vndr_seq = SVC2.vndr_seq
    				and rownum =1	) 			vndr_nm,
    				EDI.inv_curr_cd,
    				EDI.inv_amt,
    				(	select nvl(sum(inv_scg_amt),0.0) inv_surcharge
    				from trs_trsp_scg_dtl
    				where (trsp_so_ofc_cty_cd = EDI.trsp_so_ofc_cty_cd
    				and trsp_so_seq = EDI.trsp_so_seq)		) inv_surcharge,
    				TO_CHAR(EDI.inv_iss_dt, 'YYYY-MM-DD') inv_iss_dt,
    				TO_CHAR(EDI.CRE_dt, 'YYYY-MM-DD') inv_rcv_dt,
    				'EF' status,
    				'Y' inv_rjct_flg,
    				'EDI transmission failed' status_nm,
    				TO_CHAR('', 'YYYY-MM-DD') paidDate,
    				'' paymentMethod,
    				'' checkTTNumber
    				,'EDI' if_sys_knd_name
    				,EDI.inv_rmk
    				,(select intg_cd_val_dp_desc	from com_intg_cd_dtl
    				where intg_cd_id = 'CD00958'
    				and intg_cd_val_ctnt = SVC2.trsp_cost_dtl_mod_cd) trsp_kind_nm
    				,(select intg_cd_val_dp_desc	from com_intg_cd_dtl
    				where intg_cd_id = 'CD00283'
    				and intg_cd_val_ctnt = SVC2.trsp_crr_mod_cd) trsp_mode_nm
    				,decode(WRK2.wo_fmt_tp_cd,'CC','Combined Case 1',
    				'CM','Combined Case 2','CY','Combined Case 2','IB','Combined Case 2',
    				'NC','Normal','MM','Empty' ) trsp_type_nm
    				,SVC2.fm_nod_cd
    				,SVC2.to_nod_cd
    				,SVC2.eq_tpsz_cd
					,EDI.INV_EDI_RSLT_CD
    		 FROM trs_trsp_inv_EDI EDI, trs_trsp_svc_ord SVC2,trs_trsp_wrk_ord wrk2
    		WHERE EDI.inv_no IS NOT NULL
			  AND SVC2.INV_NO IS NULL
    		  AND NVL(SVC2.delt_flg,'E') != 'Y'
    		  AND ((SVC2.VNDR_SEQ = @[vndr_seq]) OR SVC2.VNDR_SEQ = (select prnt_vndr_seq from mdm_vendor where vndr_seq = @[vndr_seq]))
    		#if ($inv_no.size() > 0)
    		AND  (1,EDI.inv_no) in (
    			#foreach($inv_no_key IN ${inv_no})
    				#if($velocityCount < $inv_no.size())
    					(1,'$inv_no_key') ,
    				#else
    					(1,'$inv_no_key')
    				#end
    			#end
    			)
    		#else
    			AND 1=2
    		#end
    		 AND WRK2.TRSP_WO_OFC_CTY_CD = SVC2.TRSP_WO_OFC_CTY_CD
    		 AND WRK2.TRSP_WO_SEQ = SVC2.TRSP_WO_SEQ
    		 AND EDI.TRSP_WO_OFC_CTY_CD = SVC2.TRSP_WO_OFC_CTY_CD
             AND EDI.TRSP_WO_SEQ = SVC2.TRSP_WO_SEQ
             AND EDI.TRSP_SO_OFC_CTY_CD = SVC2.TRSP_SO_OFC_CTY_CD
             AND EDI.TRSP_SO_SEQ = SVC2.TRSP_SO_SEQ
			 AND EDI.EQ_NO = SVC2.EQ_NO
    		 AND NVL(WRK2.delt_flg, 'N') = 'N'
    ) B
ORDER BY inv_rcv_dt DESC
)C			]]></sql>
			<params>
				<param name="vndr_seq" type="2" value="1111" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
