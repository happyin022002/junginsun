<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceSurchargeDBDAOsearchWorkOrderSurchargeInquiryForTRSRSQL">
			<desc><![CDATA[search WorkOrder Surcharge Inquiry For TRS]]></desc>
			<sql><![CDATA[
SELECT trsp_so_ofc_cty_cd,
	trsp_so_seq,
	lgs_cost_cd,
	scg_amt,
	chss_mgst_tpsz_cd,
	dry_run_rlbl_pty_tp_cd,
	fne_cuz_desc,
	fumg_cost_tp_cd,
	mgst_tpsz_cd,
	insp_rf_pti_cstms_tp_cd,
	lftg_knt,
	lftg_cuz_desc,
	stop_loc_nod_cd,
	grs_wgt,
	TO_CHAR(incrt_dt,'YYYYMMDD')	incrt_dt,
	scl_stop_plc_nod_cd,
	ob_bkg_no,
	otr_rmk,
	n3pty_vndr_seq,
	n3pty_ofc_cd,
	n3pty_amt,
	n3pty_desc,
	sto_dys,
	wt_hrs,
	CHSS_NO,
	TO_CHAR(INCUR_DT, 'YYYYMMDD') INCUR_DT
 FROM trs_trsp_scg_dtl
WHERE trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
  AND trsp_so_seq = @[trsp_so_seq]
  AND SUBSTR(LGS_COST_CD, 3, 2)	<> 'FU'
  --AND scg_amt > 0			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="111" out="N"/>
				<param name="trsp_so_seq" type="2" value="11111111" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
