<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderPreviewDBDAOSearchEdi10514701TroPicRSQL">
			<desc><![CDATA[SearchEdi10514701TroPic]]></desc>
			<sql><![CDATA[
SELECT usr.usr_nm tro_pic_name
      ,usr.xtn_phn_no tro_pic_tel
      ,org.intl_fax_no||org.ofc_fax_no tro_pic_fax
      ,usr.usr_eml tro_pic_email
  FROM com_user usr
 	  ,mdm_organization org
 	  ,trs_trsp_svc_ord so
 WHERE so.delt_flg <> 'Y'			
   AND so.trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
   AND so.trsp_so_seq = @[trsp_so_seq]
   AND so.hjl_no IS NULL
   AND DECODE(LENGTH(TRIM(so.upd_usr_id)),0,so.cre_usr_id,so.upd_usr_id) = usr.usr_id
   AND usr.ofc_cd = org.ofc_cd			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
