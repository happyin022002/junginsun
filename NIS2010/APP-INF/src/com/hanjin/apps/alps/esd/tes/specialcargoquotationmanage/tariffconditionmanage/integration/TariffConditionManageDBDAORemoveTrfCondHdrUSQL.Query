<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffConditionManageDBDAORemoveTrfCondHdrUSQL">
			<desc><![CDATA[CONDITION 삭제 처리]]></desc>
			<sql><![CDATA[
UPDATE TES_TRF_COND C
SET 
C.DELT_FLG = 'Y'
, C.DELT_DT = SYSDATE
, C.UPD_USR_ID = @[usr_id]
, C.UPD_DT = SYSDATE
WHERE C.COND_NO = @[cond_no]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cond_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
