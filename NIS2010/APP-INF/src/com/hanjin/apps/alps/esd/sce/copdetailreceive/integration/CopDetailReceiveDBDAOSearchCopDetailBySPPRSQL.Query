<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchCopDetailBySPPRSQL">
			<desc><![CDATA[SearchCopDetailBySPP]]></desc>
			<sql><![CDATA[
 SELECT  dtl.cop_no as cop_no, 
            dtl.cop_dtl_seq as cop_dtl_seq, 
            dtl.act_sts_cd as act_sts_cd, 
            dtl.nod_cd as nod_cd, 
            vsl_cd, 
            skd_voy_no, 
            skd_dir_cd, 
            stnd_edi_sts_cd
    FROM SCE_COP_DTL dtl,
    (
        SELECT cop_no
        FROM   SCE_COP_HDR
        WHERE   cntr_no = @[p_cntr_no]
        and     bkg_no = @[p_bkg_no] 
        AND     cop_sts_cd IN('C', 'T', 'F')
    ) cntr
    WHERE   dtl.cop_no = cntr.cop_no
    AND     dtl.act_cd = @[p_act_cd]
    AND     ROWNUM     = 1			]]></sql>
			<params>
				<param name="p_cntr_no" type="12" value="" out="N"/>
				<param name="p_bkg_no" type="12" value="" out="N"/>
				<param name="p_act_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
