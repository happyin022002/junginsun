<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOCheckPsNodeRSQL">
			<desc><![CDATA[CheckPsNode]]></desc>
			<sql><![CDATA[
SELECT  'X' 
FROM PRD_NOD_CNST_MGMT 
WHERE NOD_CD = @[nod_cd] 
AND NOD_CNST_ITM_CD = @[nod_cnst_itm_cd] 
AND NVL(CMDT_CD,'N') = NVL(@[cmdt_cd],'N')
AND PORT_PNT_CD = @[port_pnt_cd]
AND NVL(DELT_FLG, 'N') ='N'
AND NVL(VSL_SLAN_CD, ' ') = NVL(@[vsl_slan_cd], ' ')
AND NVL(VSL_CD, ' ')     = NVL(SUBSTR(@[vvd],1,4), ' ')
AND NVL(SKD_VOY_NO, ' ') = NVL(SUBSTR(@[vvd],5,4), ' ')
AND NVL(SKD_DIR_CD, ' ') = NVL(SUBSTR(@[vvd],9,1), ' ')			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="port_pnt_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
