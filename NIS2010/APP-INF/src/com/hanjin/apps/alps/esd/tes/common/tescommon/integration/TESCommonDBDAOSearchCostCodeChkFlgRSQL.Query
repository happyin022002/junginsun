<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchCostCodeChkFlgRSQL">
			<desc><![CDATA[Invoice 의 Manual 입력시 필수 입력 여부 조회]]></desc>
			<sql><![CDATA[
SELECT 
#if (${coid} == 'vol_rt_chk_flg') 
	NVL(VOL_RT_CHK_FLG,'N') VOL_RT_CHK_FLG, 
    NVL(VOL_DUP_CHK_FLG,'N') VOL_DUP_CHK_FLG
#elseif (${coid} == 'rmk_chk_flg') 
	NVL(RMK_CHK_FLG,'N') RMK_CHK_FLG
#end
FROM TES_TML_SO_COST
WHERE 1=1
AND LGS_COST_CD = @[param_lgs_cost_cd]

			]]></sql>
			<params>
				<param name="param_lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
