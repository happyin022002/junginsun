<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailBillingCancelManageDBDAOMultiCancelRailBillingListEqrUSQL">
			<desc><![CDATA[Rail So의 EQR 상태코드 변경]]></desc>
			<sql><![CDATA[
UPDATE EQR_REPO_EXE_SO_IF
SET TRSP_SO_STS_CD = 'P'
WHERE TRSP_SO_STS_CD IN (
                            SELECT 
                                A.TRSP_SO_STS_CD
                            FROM 
                                EQR_REPO_EXE_SO_IF A,
                                TRS_TRSP_RAIL_BIL_ORD B
                            WHERE A.REPO_PLN_ID = B.REPO_PLN_ID
                            AND   A.PLN_YRWK = B.PLN_YRWK
                            AND   A.REF_ID = B.REF_ID
                            AND   A.REF_SEQ = B.REF_SEQ
                            AND   B.TRSP_SO_OFC_CTY_CD = @[trspSoOfcCtyCd]
                            AND   B.TRSP_SO_SEQ = @[trspSoSeq]
                        )			]]></sql>
			<params>
				<param name="trsp_so" type="12" value="" out="N"/>
				<param name="so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
