<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TesAdvanceAuditDBDAOLgsCostSubjCdRSQL">
			<desc><![CDATA[LgsCostSubjCd]]></desc>
			<sql><![CDATA[
SELECT LGS_COST_SUBJ_CD 
FROM (
        SELECT  LC.LGS_COST_SUBJ_CD
              , MIN(LC.LGS_COST_OPT_NO)  
        FROM TES_LGS_COST LC
           , TES_TML_SO_COST SC
        WHERE LGS_COST_SUBJ_CD IS NOT NULL 
        AND   LC.LGS_COST_CD = SC.LGS_COST_CD
        GROUP BY LC.LGS_COST_SUBJ_CD 
        ORDER BY MIN(LC.LGS_COST_OPT_NO)
       )
ORDER BY LGS_COST_SUBJ_CD DESC			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
