<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESInterfaceManageDBDAOUpdateEDInvoiceDTLTmpLGSCostCodeUSQL">
			<desc><![CDATA[UpdateEDInvoiceDTLTmpLGSCostCode]]></desc>
			<sql><![CDATA[
UPDATE TES_EDI_SO_DTL D
SET  D.LGS_COST_CD = (  SELECT DISTINCT C.LGS_COST_CD
                        FROM TES_EDI_SO_COST C
                        WHERE C.VNDR_SEQ = @[vndr_seq]
                        AND C.VNDR_TRF_DESC = D.TRF_DESC 
                        AND ROWNUM = 1 )
WHERE D.TML_EDI_SO_OFC_CTY_CD = @[tml_edi_so_ofc_cty_cd]
AND D.TML_EDI_SO_SEQ = @[tml_edi_so_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="tml_edi_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_edi_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
