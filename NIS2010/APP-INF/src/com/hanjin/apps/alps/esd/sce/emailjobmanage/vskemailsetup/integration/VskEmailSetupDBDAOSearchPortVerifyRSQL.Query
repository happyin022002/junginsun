<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VskEmailSetupDBDAOSearchPortVerifyRSQL">
			<desc><![CDATA[Port 대상 Verify 조회]]></desc>
			<sql><![CDATA[
SELECT
    LOC_CD
FROM
    MDM_LOCATION
WHERE LOC_CD = @[portCd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="portCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
