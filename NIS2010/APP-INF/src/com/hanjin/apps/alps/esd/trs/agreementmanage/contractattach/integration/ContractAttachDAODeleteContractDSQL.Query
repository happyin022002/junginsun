<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContractAttachDAODeleteContractDSQL">
			<desc><![CDATA[Contract Attach 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM TRS_AGMT_CTRT_ATCH
 WHERE TRSP_AGMT_OFC_CTY_CD = @[trsp_agmt_ofc_cty_cd]
   AND TRSP_AGMT_SEQ = @[trsp_agmt_seq]
   AND CTRT_SEQ = @[ctrt_seq]

			]]></sql>
			<params>
				<param name="trsp_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_agmt_seq" type="12" value="" out="N"/>
				<param name="ctrt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
