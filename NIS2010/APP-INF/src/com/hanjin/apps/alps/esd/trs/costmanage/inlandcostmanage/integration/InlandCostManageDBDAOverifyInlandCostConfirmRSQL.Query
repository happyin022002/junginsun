<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandCostManageDBDAOverifyInlandCostConfirmRSQL">
			<desc><![CDATA[verifyInlandCostConfirm]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
  FROM TRS_INLND_COST_TRF_HDR
 WHERE COST_TRF_NO = @[in_cost_trf_no]
   AND COST_TRF_STS_CD = 'C'			]]></sql>
			<params>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
