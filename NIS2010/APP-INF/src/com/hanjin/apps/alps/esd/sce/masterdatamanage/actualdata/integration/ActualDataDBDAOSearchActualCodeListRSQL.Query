		<query name="ActualDataDBDAOSearchActualCodeListRSQL">
			<desc><![CDATA[SearchActualCodeList]]></desc>
			<sql><![CDATA[
SELECT 'ALL' AS ACT_CD
     , '' AS ACT_NM
  FROM DUAL
UNION
SELECT ACT_CD
     , ACT_NM
  FROM MDM_ACTIVITY
 WHERE ACT_TP_CD = 'T'
ORDER BY ACT_CD			]]></sql>
