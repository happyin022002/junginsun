<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDetailInNFBySPPUSQL">
			<desc><![CDATA[ModifyCopDetailInNFBySPP]]></desc>
			<sql><![CDATA[
            UPDATE sce_cop_dtl
            SET    act_sts_cd = 'F'
            WHERE  cop_no = @[v_cop_no]
            AND    cop_no
                ||TO_CHAR(cop_dtl_seq,'0000')
                <  @[v_cop_no]
                   ||TO_CHAR(@[v_cop_dtl_seq],'0000') 
            AND    act_sts_cd  in ('N','C')			]]></sql>
			<params>
				<param name="v_cop_no" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
