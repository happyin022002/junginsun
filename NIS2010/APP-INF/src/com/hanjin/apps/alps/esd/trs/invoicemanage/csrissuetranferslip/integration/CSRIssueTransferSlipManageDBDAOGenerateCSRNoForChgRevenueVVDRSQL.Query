<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOGenerateCSRNoForChgRevenueVVDRSQL">
			<desc><![CDATA[GenerateCSRNoForChgRevenueVVD]]></desc>
			<sql><![CDATA[
SELECT	'14'||	 'S'																																		
			||  'SELCOL'																
			||	(SELECT TO_CHAR	(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[USR_OFC_CD]), 'YYMMDD') FROM DUAL)																	
			||	(SELECT		LPAD(NVL(MAX(SUBSTR(CSR_NO,16,20)),0)+1,5, '0') 																				
				 FROM		AP_CSR_NO																														
				WHERE		SUBSTR(CSR_NO,1,LENGTH(CSR_NO)-5) ='14'	||'S'																					
																	|| 'SELCOL'																				
																	|| (SELECT TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[USR_OFC_CD]), 'YYMMDD')FROM DUAL))			
		NEW_CSR_NO																																				
FROM	DUAL																																									]]></sql>
			<params>
				<param name="USR_OFC_CD" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
