<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpotBiddingManageDBDAOModifySpExistFlgUSQL">
			<desc><![CDATA[TRS_SPOT_BID_CNDDT_VNDR 테이블에 SPP에 Vendor가 존재하는지 유무 업데이트]]></desc>
			<sql><![CDATA[
UPDATE TRS_SPOT_BID_CNDDT_VNDR
   SET SP_PTAL_EXIST_FLG = (SELECT MAX(SP_EXIST_FLG)
                              FROM (SELECT  ROW_NUMBER() OVER(PARTITION BY A.SP_VNDR_SEQ, A.SP_USR_ID ORDER BY A.CRE_DT DESC) RNK
                                           ,A.SP_EXIST_FLG
                                      FROM TRS_SVC_PROV_PTAL_USR_IF A
                                     WHERE A.SP_VNDR_SEQ = @[vndr_seq] )
							  WHERE RNK =1
                            )
       ,UPD_USR_ID = @[cre_usr_id]
       ,UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd])
 WHERE VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
