<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOInsertLinkConstraintCSQL">
			<desc><![CDATA[InsertLinkConstraint]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_LNK_CNST_MGMT (
		LNK_ORG_NOD_CD, LNK_DEST_NOD_CD, TRSP_MOD_CD, 
        LNK_CNST_ITM_CD, PCTL_CNST_ITM_NM,
        LNK_CNST_RMK, CNTR_TP_CD, EFF_FM_DT, EFF_TO_DT, 
		SVC_USE_FLG, CRE_DT, CRE_USR_ID, CRE_OFC_CD,
        UPD_DT, UPD_USR_ID,UPD_OFC_CD,
		CMDT_CD , LNK_CNST_SEQ,
        VSL_SLAN_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD
	)
VALUES (@[lnk_org_nod_cd], @[lnk_dest_nod_cd],@[trsp_mod_cd],
       @[lnk_cnst_itm_cd], (SELECT INTG_CD_VAL_DP_DESC FROM COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD01386' AND INTG_CD_VAL_CTNT = @[lnk_cnst_itm_cd] ),
       @[lnk_cnst_rmk], @[cntr_tp_cd] , REPLACE(@[eff_fm_dt], '-', ''), REPLACE(@[eff_to_dt], '-', ''),
       @[svc_use_flg] , SYSDATE, @[cre_usr_id], @[cre_ofc_cd],
       SYSDATE, @[upd_usr_id], @[upd_ofc_cd],
	   @[cmdt_cd] ,(select max(lnk_cnst_seq)+1 from PRD_LNK_CNST_MGMT ),
       @[vsl_slan_cd], SUBSTR(@[vvd], 1,4),SUBSTR(@[vvd], 5,4),SUBSTR(@[vvd], 9,1)
)			]]></sql>
			<params>
				<param name="lnk_org_nod_cd" type="12" value="" out="N"/>
				<param name="lnk_dest_nod_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="lnk_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="lnk_cnst_rmk" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
				<param name="svc_use_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
