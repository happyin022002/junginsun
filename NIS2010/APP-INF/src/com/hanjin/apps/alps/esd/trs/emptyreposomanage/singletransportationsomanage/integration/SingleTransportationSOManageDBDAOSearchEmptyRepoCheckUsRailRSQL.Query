<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSearchEmptyRepoCheckUsRailRSQL">
			<desc><![CDATA[입력받은 From Node가 US Rail인지 확인하고
US Rail일 경우 에러 메시지를 보낸다]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(*) = 0 THEN 'N'
            ELSE 'Y'
        END AS CHK_US_RAIL
  FROM MDM_COUNTRY CNTY 
      ,MDM_LOCATION LOC
 WHERE LOC.CNT_CD = CNTY.CNT_CD
   AND CNTY.CNT_CD != 'MX' 
   AND LOC.CONTI_CD = 'M'
   AND 'R' = @[trsp_mod_cd]
   AND LOC.LOC_CD = @[fm_loc_cd]			]]></sql>
			<params>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="fm_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
