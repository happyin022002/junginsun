<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TesAdvanceAuditDBDAOCreateTesInvoiceAuditBatchCSQL">
			<desc><![CDATA[Manual(5분) Batch 등록]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_AUTO_AUD_BAT
(
	  AUTO_AUD_BAT_SEQ
	, BAT_PROG_STS_CD
	, SUB_SYS_CD
	, INV_NO
	, INV_VNDR_SEQ
	, INV_CFM_DT
	, EXPN_AUD_SEQ
	, VSL_CD
	, SKD_VOY_NO
	, SKD_DIR_CD
	, IO_BND_CD
	, CRE_OFC_CD
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT 
) VALUES (
	NVL((SELECT /*+ INDEX_DESC(A XPKEAS_AUTO_AUD_BAT) */ AUTO_AUD_BAT_SEQ
		FROM EAS_AUTO_AUD_BAT A
		WHERE ROWNUM = 1
		), 0) + 1
	, 'P'
	, 'TES'
	, @[inv_no]
	, @[vndr_seq]
	, TO_DATE(@[inv_cfm_dt], 'YYYYMMDDHH24MISS')
	, @[expn_aud_seq]
	, @[vsl_cd]
	, @[skd_voy_no]
	, @[skd_dir_cd]
	, @[io_bnd_cd]
	, @[cre_ofc_cd]
	, 'Batch'
	, SYSDATE
	, 'Batch'
	, SYSDATE
)			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="inv_cfm_dt" type="12" value="" out="N"/>
				<param name="expn_aud_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
