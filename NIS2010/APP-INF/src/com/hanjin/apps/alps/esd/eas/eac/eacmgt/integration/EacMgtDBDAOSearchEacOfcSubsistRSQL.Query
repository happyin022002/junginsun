<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchEacOfcSubsistRSQL">
			<desc><![CDATA[Office Config 를 저장전 이미 등록된 offce 인지 조회한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS CNT
FROM   EAS_EXPN_AUD_CS_OFC_CFG
WHERE  EAC_OFC_CD = @[eac_ofc_cd]

			]]></sql>
			<params>
				<param name="eac_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
