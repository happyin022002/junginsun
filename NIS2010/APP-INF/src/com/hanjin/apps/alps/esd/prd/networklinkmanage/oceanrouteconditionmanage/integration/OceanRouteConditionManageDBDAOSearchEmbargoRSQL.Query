<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanRouteConditionManageDBDAOSearchEmbargoRSQL">
			<desc><![CDATA[SearchEmbargo]]></desc>
			<sql><![CDATA[
SELECT e.fm_cnt_cd, e.to_cnt_cd,
        to_char(e.cre_dt,'yyyy-mm-dd') cre_dt, e.upd_usr_id,
        to_char(e.upd_dt,'yyyy-mm-dd') upd_dt, e.mbgo_rmk
   FROM prd_mbgo_mgmt e  
  WHERE 1=1


#if(${i_from} != '')
  	 AND e.fm_cnt_cd = @[i_from]
#end

#if(${i_to} != '')
    AND e.to_cnt_cd = @[i_to]      
#end			]]></sql>
			<params>
				<param name="i_from" type="12" value="" out="N"/>
				<param name="i_to" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
