<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoAvailableDBDAOMultiSpecialCargoAvailableSpUSQL">
			<desc><![CDATA[Special Cargo Available S/P Update]]></desc>
			<sql><![CDATA[
UPDATE TRS_SPCL_CGO_AVAL_SVC_PROV
   SET VNDR_SEQ = @[vndr_seq]
      ,SO_CRE_OFC_CD = @[so_cre_ofc_cd]
 	  ,HZD_MTRL_FLG  = DECODE(@[hzd_mtrl_flg],'1','Y','N')
      ,OVWT_TRI_AXL_FLG = DECODE(@[ovwt_tri_axl_flg],'1','Y','N')
      ,UPD_USR_ID = @[login_usr_id]
      ,UPD_DT = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[login_ofc_cd])
 WHERE 1=1
   AND TRSP_SPCL_CGO_SP_SEQ = @[trsp_spcl_cgo_sp_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="so_cre_ofc_cd" type="12" value="" out="N"/>
				<param name="hzd_mtrl_flg" type="12" value="" out="N"/>
				<param name="ovwt_tri_axl_flg" type="12" value="" out="N"/>
				<param name="login_usr_id" type="12" value="" out="N"/>
				<param name="login_ofc_cd" type="12" value="" out="N"/>
				<param name="trsp_spcl_cgo_sp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
