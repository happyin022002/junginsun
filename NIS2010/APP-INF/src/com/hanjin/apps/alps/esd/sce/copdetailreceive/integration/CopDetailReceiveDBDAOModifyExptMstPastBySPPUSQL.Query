<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyExptMstPastBySPPUSQL">
			<desc><![CDATA[ModifyExptMstPastBySPP]]></desc>
			<sql><![CDATA[
           UPDATE SCE_EXPT_MST 
           SET    COP_EXPT_STS_CD = 'R'
                 ,EXPT_CLR_TP_CD  = '2'
                 ,EXPT_RSOLV_DT   = SYSDATE
                 ,upd_usr_id      = 'SPP'
                 ,upd_dt          = SYSDATE
           WHERE cop_no = @[v_cop_no] 
           AND   (cop_no || TRIM(TO_CHAR(cop_dtl_seq,'0000'))
                          < @[v_cop_no] || TRIM(TO_CHAR(@[v_cop_dtl_seq],'0000')))
           AND   cop_expt_sts_cd = 'O'			]]></sql>
			<params>
				<param name="v_cop_no" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
