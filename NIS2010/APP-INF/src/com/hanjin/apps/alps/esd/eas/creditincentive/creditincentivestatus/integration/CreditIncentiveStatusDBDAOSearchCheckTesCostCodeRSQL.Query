<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAOSearchCheckTesCostCodeRSQL">
			<desc><![CDATA[Grid에 입력된 Terminal Cost Code Validation]]></desc>
			<sql><![CDATA[
SELECT A.LGS_COST_CD
  FROM TES_LGS_COST A
      ,TES_TML_SO_COST B
 WHERE 1=1
  AND A.LGS_COST_CD = B.LGS_COST_CD
  AND A.LGS_COST_CD = @[lgs_cost_cd]			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
