<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanadaCCTManageDBDAOSearchCCTManageRSQL">
			<desc><![CDATA[CanadaCCTManageDBDAOSearchCCTManage]]></desc>
			<sql><![CDATA[
SELECT POR_CD,POL_NOD_CD
	  ,POR_NOD_CD
      ,ARR_SUN_DYS
      ,ARR_MON_DYS
      ,ARR_TUE_DYS
      ,ARR_WED_DYS
      ,ARR_THU_DYS
      ,ARR_FRI_DYS
      ,ARR_SAT_DYS
      ,CCT_HRMNT
      ,UPD_USR_ID
      ,UPD_DT
  FROM PRD_CND_TML_CCT_MGMT
 WHERE DELT_FLG = 'N'

#if (${por_cd} != '') 
  AND POR_CD = @[por_cd]
#end
#if (${pol_nod_cd} != '') 
  AND POL_NOD_CD = @[pol_nod_cd]
#end
#if (${por_nod_cd} != '') 
  AND POR_NOD_CD = @[por_nod_cd]
#end

ORDER BY POL_NOD_CD,POR_CD			]]></sql>
			<params>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_nod_cd" type="12" value="" out="N"/>
				<param name="por_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
