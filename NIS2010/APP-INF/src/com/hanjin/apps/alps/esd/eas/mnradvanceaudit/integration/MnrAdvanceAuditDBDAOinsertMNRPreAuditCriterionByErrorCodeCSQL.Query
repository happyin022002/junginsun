<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MnrAdvanceAuditDBDAOinsertMNRPreAuditCriterionByErrorCodeCSQL">
			<desc><![CDATA[MNR Pre-Audit Criterion By Error Code 에서 새로운 Office Code 추가]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_MNR_PRE_AUD_VRFY_CFG
(AUD_OFC_CD, EXPN_AUD_MNR_VRFY_TP_CD, MNR_VRFY_TP_CD, MNR_VRFY_TP_AUD_FLG, VRFY_RMK, CRE_USR_ID, CRE_DT, UPD_OFC_CD, UPD_USR_ID, UPD_DT)
    SELECT @[i_ofc_cd],
       INTG_CD_VAL_CTNT,
       MNR_CD_ID,
       NVL(C.MNR_VRFY_TP_AUD_FLG, 'N'),
       '',
       @[cre_usr_id],  
       sysdate, 
       @[ofc_cd],
       @[upd_usr_id],
       sysdate
  FROM (SELECT B.INTG_CD_VAL_CTNT,
               A.MNR_CD_ID
          FROM MNR_GEN_CD A,
               COM_INTG_CD_DTL B
         WHERE A.PRNT_CD_ID = 'CD00004'
           AND B.INTG_CD_ID = 'CD03412'
           AND A.MNR_CD_ID NOT IN ('SS', 'SL', 'OF') ) A, 
       EAS_MNR_PRE_AUD_VRFY_CFG C
 WHERE C.EXPN_AUD_MNR_VRFY_TP_CD (+) = A.INTG_CD_VAL_CTNT
   AND C.MNR_VRFY_TP_CD (+)= A.MNR_CD_ID
   AND C.AUD_OFC_CD (+)= MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(@[i_ofc_cd])			]]></sql>
			<params>
				<param name="i_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
