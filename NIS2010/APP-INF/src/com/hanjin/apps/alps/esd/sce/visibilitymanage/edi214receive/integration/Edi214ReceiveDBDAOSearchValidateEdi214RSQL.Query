<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi214ReceiveDBDAOSearchValidateEdi214RSQL">
			<desc><![CDATA[select validate]]></desc>
			<sql><![CDATA[
SELECT d.COP_DTL_SEQ COP_DTL_SEQ, d.COP_NO COP_NO
FROM sce_cop_dtl d, (
	select cop_no,DOR_NOD_CD  from trs_trsp_svc_ord
	where TRSP_SO_OFC_CTY_CD = @[trsp_so_ofc_cty_cd] 
	and TRSP_SO_SEQ = @[trsp_so_seq] 
	and BKG_RCVDE_TERM_CD = 'D'
  ) v
WHERE d.COP_NO = v.cop_no
and d.act_cd = 'FITZAD'
and d.NOD_CD = v.DOR_NOD_CD			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
