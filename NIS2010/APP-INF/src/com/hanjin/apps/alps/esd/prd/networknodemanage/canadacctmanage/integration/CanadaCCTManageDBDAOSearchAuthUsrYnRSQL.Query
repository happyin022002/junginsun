<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanadaCCTManageDBDAOSearchAuthUsrYnRSQL">
			<desc><![CDATA[Search User Auth Check]]></desc>
			<sql><![CDATA[
SELECT DECODE (COUNT (*), 0, 'N', 'Y') AS auth_usr_yn
  FROM COM_USER A, COM_USR_ROLE B, COM_USR_ROLE_MTCH C
 WHERE     1 = 1
       AND A.USR_ID = C.USR_ID
       AND B.USR_ROLE_CD = C.USR_ROLE_CD
       AND C.USR_ROLE_CD = 'PRD09'
       AND DECODE (A.USE_FLG, NULL, 'N', A.USE_FLG) = 'Y'
       --AND A.USR_AUTH_TP_CD IN ('A', 'S')
       AND A.USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
