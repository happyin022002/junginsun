<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifySceCopDetailEstmDtByBoundUSQL">
			<desc><![CDATA[ModifySceCopDetailEstmDtByBound]]></desc>
			<sql><![CDATA[
UPDATE sce_cop_dtl
SET    estm_dt    = estm_dt + TO_NUMBER(@[rcv_evnt_gap_desc])
     ,estm_gdt   = estm_gdt + TO_NUMBER(@[rcv_evnt_gap_desc])
     ,upd_dt     = SYSDATE
     ,upd_usr_id = SUBSTR(@[cre_usr_id]||'SKD',1,20)
WHERE cop_no = @[cop_no]
AND    (((COP_NO||COP_DTL_SEQ)  >= (@[cop_no]||@[fm_cop_dtl_seq])  AND  SUBSTR(@[rcv_evnt_proc_flg],2,1) = 'E')
  OR   ((COP_NO||COP_DTL_SEQ)  >  (@[cop_no]||@[fm_cop_dtl_seq])  AND  SUBSTR(@[rcv_evnt_proc_flg],2,1) = 'A'))
AND    COP_DTL_SEQ  <  @[to_cop_dtl_seq]			]]></sql>
			<params>
				<param name="rcv_evnt_gap_desc" type="12" value="2.0000000000" out="N"/>
				<param name="cre_usr_id" type="12" value="TEST" out="N"/>
				<param name="cop_no" type="12" value="CSLC7506108111" out="N"/>
				<param name="fm_cop_dtl_seq" type="12" value="6051" out="N"/>
				<param name="rcv_evnt_proc_flg" type="12" value="1E" out="N"/>
				<param name="to_cop_dtl_seq" type="12" value="6063" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
