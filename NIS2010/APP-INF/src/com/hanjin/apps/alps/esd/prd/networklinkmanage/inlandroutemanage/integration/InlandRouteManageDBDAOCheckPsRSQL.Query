<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageDBDAOCheckPsRSQL">
			<desc><![CDATA[CheckPs]]></desc>
			<sql><![CDATA[
SELECT 'X'
        	  FROM DUAL 

         
#if(${r_inbound} == 'I')

				 WHERE EXISTS (SELECT 'X'
            	                 FROM prd_node n 
            		                WHERE n.nod_cd = @[i_rout_org_nod_cd] AND n.nod_tp_cd IN ('M', 'B'))  
    
	#if(${nod_tp_cd1} == 'Z')
                		   AND EXISTS (SELECT 'X' 
                		                 FROM prd_node n 
                		                WHERE n.nod_cd = @[i_rout_dest_nod_cd] AND n.nod_tp_cd = 'Z')   
    #else               
                  
                    	   AND EXISTS (SELECT 'X' 
                		                 FROM prd_node n
                		               WHERE n.nod_cd = @[i_rout_dest_nod_cd] AND n.nod_tp_cd != 'Z')    
    #end
#elseif(${r_inbound} == 'O')        	                                       
        			 WHERE EXISTS (SELECT 'X' 
            		                FROM prd_node n 
            		                WHERE n.nod_cd = @[i_rout_dest_nod_cd] AND n.nod_tp_cd IN ('M', 'B'))   
	    #if(${nod_tp_cd1} == 'Z')    
	                	   AND EXISTS (SELECT 'X' 
	            		                 FROM prd_node n 
	            		                WHERE n.nod_cd = @[i_rout_org_nod_cd] AND n.nod_tp_cd = 'Z')  
	    #else  
	                         
	                	   AND EXISTS (SELECT 'X' 
	            		               FROM prd_node n 
	            		               WHERE n.nod_cd = @[i_rout_org_nod_cd] AND n.nod_tp_cd != 'Z')   
	    #end         
#elseif(${r_inbound} == 'M')    
 
        			WHERE  
        			EXISTS 
        	
        			    SELECT 'X' FROM mdm_yard 
        			   WHERE yd_fcty_tp_rail_rmp_flg='Y' AND 
        			    yd_cd = @[i_rout_org_nod_cd]
        			)
        			AND EXISTS
        			( 
        			    SELECT 'X' FROM mdm_yard 
        			    WHERE yd_fcty_tp_rail_rmp_flg='Y' AND 
        			    yd_cd = @[i_rout_dest_nod_cd] 
        			) 

#end       			]]></sql>
			<params>
				<param name="i_rout_org_nod_cd" type="12" value="" out="N"/>
				<param name="i_rout_dest_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
