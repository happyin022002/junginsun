<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOUpdateRouteConstraintUSQL">
			<desc><![CDATA[UpdateRouteConstraint]]></desc>
			<sql><![CDATA[
UPDATE PRD_ROUT_CNST 
SET   SVC_USE_FLG = @[svc_use_flg] 
	, ROUT_CNST_RMK = @[rout_cnst_rmk] 
	, UPD_USR_ID = @[upd_usr_id]  
	, UPD_DT = SYSDATE 
	, UPD_OFC_CD = @[upd_ofc_cd]
    , VSL_CD = SUBSTR(@[vvd],1,4)
    , SKD_VOY_NO = SUBSTR(@[vvd],5,4)
    , SKD_DIR_CD = SUBSTR(@[vvd],9,1)
    , CNTR_TP_CD = @[cntr_tp_cd]
    , CMDT_CD = @[cmdt_cd]
WHERE TRNK_LANE_CD = @[trnk_lane_cd] 
AND   POL_NOD_CD = @[pol_cd] || @[pol_nod_cd]
AND   POD_NOD_CD = @[pod_cd] || @[pod_nod_cd] 
AND   ROUT_CNST_SEQ = TO_NUMBER(@[rout_cnst_seq])			]]></sql>
			<params>
				<param name="svc_use_flg" type="12" value="" out="N"/>
				<param name="rout_cnst_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="trnk_lane_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pol_nod_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="pod_nod_cd" type="12" value="" out="N"/>
				<param name="rout_cnst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
