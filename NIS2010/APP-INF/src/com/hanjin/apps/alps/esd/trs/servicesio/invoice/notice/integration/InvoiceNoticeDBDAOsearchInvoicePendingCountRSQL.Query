<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceNoticeDBDAOsearchInvoicePendingCountRSQL">
			<desc><![CDATA[Invoice 건수 조회]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) pendingCount
  FROM trs_trsp_inv_wrk INV
 WHERE INV.trsp_inv_aud_sts_cd IN ('RC')
   AND INV.inv_vndr_seq = @[inv_vndr_seq]
   AND NVL(INV.delt_flg, 'E') <> 'Y'			]]></sql>
			<params>
				<param name="inv_vndr_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
