<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDBDAOSearchCommodityRSQL">
			<desc><![CDATA[commodity 코드와 이름을 MDM에서 조회]]></desc>
			<sql><![CDATA[
SELECT												
		cmdt_cd, 												
		cmdt_nm 												
FROM	mdm_commodity											
WHERE	cmdt_cd = @[cmdt_cd]			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
