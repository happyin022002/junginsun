<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOInsertAPCsrNoCSQL">
			<desc><![CDATA[CSR 번호를 AP_CSR_NO에 등록]]></desc>
			<sql><![CDATA[
INSERT INTO	AP_CSR_NO	(											
							CSR_NO									
						,	CRE_USR_ID								
						,	CRE_DT									
						)											
VALUES					(											
							@[CSR_NO]										
						,	@[COST_OFC_CD]
						,	GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[COST_OFC_CD])	
						)			]]></sql>
			<params>
				<param name="CSR_NO" type="12" value="" out="N"/>
				<param name="COST_OFC_CD" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
