<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualCostCalcManageDBDAOCompleteActCostCalcUSQL">
			<desc><![CDATA[ACT COST CALC 완료 처리]]></desc>
			<sql><![CDATA[
UPDATE TES_ACT_COST_HDR A
SET A.EXE_STS_CD = 'C', A.EXE_TO_DT = SYSDATE, A.UPD_DT = SYSDATE, A.UPD_USR_ID = 'CALC_DONE'
WHERE 1=1
AND A.TML_ACT_COST_SEQ = @[tml_act_cost_seq]			]]></sql>
			<params>
				<param name="tml_act_cost_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
