<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsCommonDBDAOChangeAttachFileUSQL">
			<desc><![CDATA[TrsCommonDBDAOChangeAttachFileDSQL]]></desc>
			<sql><![CDATA[
UPDATE TRS_CUST_NOMI_TRKR
   SET ATCH_FILE_LNK_ID =
          DECODE ( (SELECT COUNT (*)
                      FROM TRS_ATCH_FILE
                     WHERE ATCH_FILE_LNK_ID = @[atch_file_lnk_id]),
                  0, NULL,
                  @[atch_file_lnk_id])
 WHERE 1 = 1 AND APRO_NO = @[key1]			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="key1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
