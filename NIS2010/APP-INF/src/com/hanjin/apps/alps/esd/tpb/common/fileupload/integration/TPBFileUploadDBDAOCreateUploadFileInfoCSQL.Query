<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TPBFileUploadDBDAOCreateUploadFileInfoRSQL">
			<desc><![CDATA[CreateUploadFileInfo]]></desc>
			<sql><![CDATA[
INSERT INTO TPB_TTL_FILE_MGMT (
	file_no, 
	file_no_seq, 
	delt_flg,
	file_lgc_nm, 
	file_phys_nm, 
	file_path_nm,
	cre_usr_id, 
	cre_dt, 
	upd_usr_id, 
	upd_dt
) VALUES (
	@[s_file_no], 
	@[s_file_no_seq], 
	'N',
	REPLACE(@[s_file_lgc_nm],'''',''), 
	@[s_file_phys_nm], 
	@[s_file_path_nm],
	@[s_cre_usr_id], 
	SYSDATE, 
	@[s_upd_usr_id], 
	SYSDATE
)			]]></sql>
			<params>
				<param name="s_file_no" type="12" value="" out="N"/>
				<param name="s_file_no_seq" type="12" value="" out="N"/>
				<param name="s_file_lgc_nm" type="12" value="" out="N"/>
				<param name="s_file_phys_nm" type="12" value="" out="N"/>
				<param name="s_file_path_nm" type="12" value="" out="N"/>
				<param name="s_cre_usr_id" type="12" value="" out="N"/>
				<param name="s_upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
