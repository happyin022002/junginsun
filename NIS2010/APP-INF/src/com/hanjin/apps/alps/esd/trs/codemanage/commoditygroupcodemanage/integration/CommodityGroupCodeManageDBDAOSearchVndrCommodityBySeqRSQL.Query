<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityGroupCodeManageDBDAOSearchVndrCommodityBySeqRSQL">
			<desc><![CDATA[search_vndr_commodity]]></desc>
			<sql><![CDATA[
SELECT 															
		vndr_seq vndr_no,														
		VNDR_LGL_ENG_NM vndr_nm_eng												
FROM	mdm_vendor															
WHERE	vndr_seq = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
