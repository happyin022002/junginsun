<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpotBiddingManageDBDAOAddSpotBidCnddtVndrHisCSQL">
			<desc><![CDATA[Candidate Vendor History Insert]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_SPOT_BID_CNDDT_VNDR_HIS
  	  (SPOT_BID_CNDDT_TERM_SEQ
      ,VNDR_SEQ
      ,CNDDT_VNDR_HIS_SEQ
      ,SPOT_BID_VNDR_EML
      ,SP_PTAL_EXIST_FLG
      ,LOCL_CRE_DT
      ,CRE_OFC_CD
      ,DELT_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
)
SELECT SPOT_BID_CNDDT_TERM_SEQ,
       VNDR_SEQ,
       (SELECT NVL(MAX(CNDDT_VNDR_HIS_SEQ)+1, 1)
          FROM TRS_SPOT_BID_CNDDT_VNDR_HIS
         WHERE SPOT_BID_CNDDT_TERM_SEQ = SBCV.SPOT_BID_CNDDT_TERM_SEQ
           AND VNDR_SEQ = SBCV.VNDR_SEQ) CNDDT_VNDR_HIS_SEQ,
       SPOT_BID_VNDR_EML,
       SP_PTAL_EXIST_FLG,
       GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]) LOCL_CRE_DT,
       @[cre_ofc_cd] CRE_OFC_CD,
       DELT_FLG,
       @[cre_usr_id] CRE_USR_ID,
       SYSDATE CRE_DT,
       @[cre_usr_id] UPD_USR_ID,
       SYSDATE UPD_DT
  FROM TRS_SPOT_BID_CNDDT_VNDR SBCV
 WHERE VNDR_SEQ = @[vndr_seq]
   			]]></sql>
			<params>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
