<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOSearchCostCodeListRSQL">
			<desc><![CDATA[Lgs Cost Code List Inquiry]]></desc>
			<sql><![CDATA[
SELECT	lgs_cost_cd
		, lgs_cost_full_nm
		, thrp_flg
FROM	TES_LGS_COST
WHERE	LENGTH(lgs_cost_cd)	= 6
AND		thrp_flg			= 'Y'
ORDER BY lgs_cost_opt_no			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
