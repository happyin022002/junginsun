<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOValidateSacCdRSQL">
			<desc><![CDATA[ValidateSacCd]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(IDA_SAC_CD),1,'Y','N') VALID_IDA_SAC_CD 
FROM BKG_IDA_SAC_MST
WHERE IDA_SAC_CD = @[ida_sac_cd]			]]></sql>
			<params>
				<param name="ida_sac_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
