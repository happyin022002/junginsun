<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LogisticsRevenueDBDAOCheckTrdPartyRSQL">
			<desc><![CDATA[CheckTrdParty]]></desc>
			<sql><![CDATA[
SELECT  VNDR_LGL_ENG_NM
FROM	MDM_VENDOR
WHERE 	1=1 
       	AND VNDR_SEQ = TO_NUMBER(@[s_vndr_seq])
       	AND DELT_FLG = 'N' 
       	AND RFND_PSDO_CUST_CD IS NOT NULL
       	AND ROWNUM = 1    			]]></sql>
			<params>
				<param name="s_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
