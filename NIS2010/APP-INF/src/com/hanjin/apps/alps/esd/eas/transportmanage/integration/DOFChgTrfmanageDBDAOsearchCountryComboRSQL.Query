<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DOFChgTrfmanageDBDAOsearchCountryComboRSQL">
			<desc><![CDATA[선택된 Customer 정보로 Effect date List Search]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(MAX(SYS_CONNECT_BY_PATH(CNT_NM,'|')),2) CNT_NM,
        SUBSTR(MAX(SYS_CONNECT_BY_PATH(CNT_CD,'|')),2) CNT_CD
FROM( SELECT 'GROUP' GP,
        ROW_NUMBER() OVER(ORDER BY  M.CNT_NM) RM,
        M.CNT_NM, 
        M.CNT_CD
FROM MDM_COUNTRY M
WHERE 1=1
AND M.SCONTI_CD LIKE 'E%'
)
 START WITH RM=1
 CONNECT BY PRIOR RM = RM -1 AND PRIOR GP = GP
 GROUP BY GP			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
