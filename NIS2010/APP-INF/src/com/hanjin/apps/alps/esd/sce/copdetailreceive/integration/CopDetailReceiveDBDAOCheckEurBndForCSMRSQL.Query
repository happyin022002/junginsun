<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOCheckEurBndForCSMRSQL">
			<desc><![CDATA[check Europe Bound for CSM]]></desc>
			<sql><![CDATA[
SELECT 'EU_BOUND'
FROM BKG_BOOKING B, MDM_COUNTRY M
WHERE 1=1
AND B.BKG_NO = @[bkg_no]
AND B.BKG_STS_CD <> 'X'
AND M.CNT_CD IN (SUBSTR(B.POR_CD,1,2), SUBSTR(B.POL_CD,1,2), SUBSTR(B.POD_CD,1,2), SUBSTR(B.DEL_CD,1,2))
AND EU_CNT_FLG = 'Y'

UNION

SELECT 'EU_BOUND'
FROM BKG_BOOKING B, BKG_VVD V, MDM_COUNTRY M
WHERE 1=1
AND B.BKG_NO = @[bkg_no]
AND B.BKG_STS_CD <> 'X'
AND B.BKG_NO = V.BKG_NO
AND M.CNT_CD IN (SUBSTR(V.POL_CD,1,2), SUBSTR(V.POD_CD,1,2))
AND EU_CNT_FLG = 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
