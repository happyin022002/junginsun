<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOAddInvoiceFaxEmailSentHistoryCSQL">
			<desc><![CDATA[Fax Email 전송이력을 저장합니다.]]></desc>
			<sql><![CDATA[
INSERT INTO TPB_FAX_EML_SND_HIS
(   N3PTY_INV_NO         
   ,SND_SEQ
   ,N3PTY_INV_RVIS_CD              
   ,N3PTY_INV_SND_TP_CD  
   ,SND_DT               
   ,FAX_EML_SND_NO       
   ,VNDR_CUST_EML        
   ,FAX_NO               
   ,CRE_USR_ID           
   ,CRE_DT               
   ,UPD_USR_ID           
   ,UPD_DT 
)VALUES(
     @[s_n3pty_inv_no]
    ,(  SELECT NVL(MAX(SND_SEQ),0)+1
        FROM   TPB_FAX_EML_SND_HIS
        WHERE  1 = 1
        AND    N3PTY_INV_NO = @[s_n3pty_inv_no]
     )
    ,@[s_n3pty_inv_rvis_cd]
    ,@[s_n3pty_inv_if_tp_cd]
    ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[user_ofc_cd])
    ,@[fax_eml_snd_no]
    ,@[vndr_cust_eml]
    ,@[fax_no]
    ,@[user_id]
    ,SYSDATE
    ,@[user_id]
    ,SYSDATE
)			]]></sql>
			<params>
				<param name="s_n3pty_inv_no" type="12" value="" out="N"/>
				<param name="s_n3pty_inv_rvis_cd" type="12" value="" out="N"/>
				<param name="s_n3pty_inv_if_tp_cd" type="12" value="" out="N"/>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="fax_eml_snd_no" type="12" value="" out="N"/>
				<param name="vndr_cust_eml" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
