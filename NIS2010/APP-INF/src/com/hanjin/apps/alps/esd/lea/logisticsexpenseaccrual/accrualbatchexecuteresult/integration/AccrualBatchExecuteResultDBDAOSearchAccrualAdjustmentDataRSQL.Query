<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccrualBatchExecuteResultDBDAOSearchAccrualAdjustmentDataRSQL">
			<desc><![CDATA[조정계수 존재여부 조회]]></desc>
			<sql><![CDATA[
SELECT A.EXE_YRMON, A.REV_YRMON, A.ACCL_LGC_TP_CD FROM LEA_ACCL_ADJ_FCTR A
WHERE 1=1
AND A.EXE_YRMON = REPLACE(@[exe_yrmon],'-','')
AND A.REV_YRMON = REPLACE(@[rev_yrmon],'-','')
AND A.ACCL_LGC_TP_CD = @[accl_lgc_tp_cd]			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="accl_lgc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
