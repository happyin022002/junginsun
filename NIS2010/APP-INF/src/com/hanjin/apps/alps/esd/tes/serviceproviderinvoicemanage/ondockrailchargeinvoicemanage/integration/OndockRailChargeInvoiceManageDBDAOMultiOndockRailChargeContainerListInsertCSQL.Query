		<query name="OndockRailChargeInvoiceManageDBDAOMultiOndockRailChargeContainerListInsertCSQL">
			<desc><![CDATA[MultiOndockRailChargeContainerListInsert]]></desc>
			<sql><![CDATA[
INSERT INTO TES_TML_SO_CNTR_LIST(
						 TML_SO_OFC_CTY_CD
						,TML_SO_SEQ
						,TML_SO_CNTR_LIST_SEQ
						,VRFY_RSLT_IND_CD
						,MODI_FLG
						,DSCR_IND_CD
						,DSCR_DTL_IND_CD
						,RVIS_IND_FLG
						,VSL_CD
						,SKD_VOY_NO
						,SKD_DIR_CD
						,FINC_VSL_CD
						,FINC_SKD_VOY_NO
						,FINC_SKD_DIR_CD
						,IO_BND_CD
						,IOC_CD
						,LANE_CD
						,ATB_DT
						,CNTR_NO
						,CNTR_TPSZ_CD
						,CNTR_STY_CD
						,LOCL_TS_IND_CD
						,SAM_LOCL_TS_IND_CD
						,RCVDE_TERM_IND_CD
						,PRE_YD_CD
						,MVMT_GATE_IN_DT
						,INV_GATE_IN_DT
						,GATE_IN_TD_DYS
						,MVMT_GATE_OUT_DT
						,INV_GATE_OUT_DT
						,GATE_OUT_TD_DYS
						,MVMT_STAY_DYS
						,INV_STAY_DYS
						,STAY_DIFF_DYS
						,DCGO_CLSS_CD
						,BB_CGO_FLG
						,AWK_CGO_FLG
						,RC_FLG
						,WRK_DT
						,CLM_DT
						,RAIL_BIL_DT
						,BKG_NO
						--,BKG_NO_SPLIT
						,BL_NO
						--,BL_NO_TP
						--,BL_NO_CHK
						,DSCR_RSN
						,HNDL_RSLT_RMK
						,CNTR_RMK
						,CRE_USR_ID
						,CRE_DT
						,LOCL_CRE_DT
						,UPD_USR_ID
						,UPD_DT
						,LOCL_UPD_DT
)VALUES(			      @[tml_so_ofc_cty_cd]  							-- tml_so_ofc_cty_cd
						, @[tml_so_seq] 									-- tml_so_seq
						, @[tml_so_cntr_list_seq]  							-- tml_so_cntr_list_seq
						, @[vrfy_rslt_ind_cd]  								-- vrfy_rslt_ind_cd
						, @[modi_flg]  										-- modi_flg
						, SUBSTR(@[dscr_ind_cd],0,2)						-- dscr_ind_cd
						, @[dscr_dtl_ind_cd]  								-- dscr_dtl_ind_cd
						, @[rvis_ind_flg]  									-- rvis_ind_flg
						, @[vsl_cd]  										-- vsl_cd
						, @[skd_voy_no]  									-- skd_voy_no
						, @[skd_dir_cd]  									-- skd_dir_cd
						, @[finc_vsl_cd]  									-- finc_vsl_cd
						, @[finc_skd_voy_no]  								-- finc_skd_voy_no
						, @[finc_skd_dir_cd]  								-- finc_skd_dir_cd
						, @[io_bnd_cd]  									-- io_bnd_cd
						, @[ioc_cd]  										-- ioc_cd
						, @[lane_cd]  										-- lane_cd
						, TO_DATE(SUBSTR(REPLACE(@[atb_dt],'-'),0,8),'YYYYMMDD')  	-- atb_dt
						, @[cntr_no]														-- cntr_no
						, @[cntr_tpsz_cd]													-- cntr_tpsz_cd
						, @[cntr_sty_cd]													-- cntr_sty_cd
						, @[locl_ts_ind_cd]													-- locl_ts_ind_cd
						, @[sam_locl_ts_ind_cd]												-- sam_locl_ts_ind_cd
						, REPLACE(@[rcvde_term_ind_cd],'/')									-- rcvde_term_ind_cd
						, @[pre_yd_cd]														-- pre_yd_cd
						, TO_DATE(SUBSTR(REPLACE(@[mvmt_gate_in_dt],'-'),0,8),'YYYYMMDD')  	-- mvmt_gate_in_dt
						, TO_DATE(SUBSTR(REPLACE(@[inv_gate_in_dt],'-'),0,8),'YYYYMMDD')  	-- inv_gate_in_dt
						, @[gate_in_td_dys]  												-- gate_in_td_dys
						, TO_DATE(SUBSTR(REPLACE(@[mvmt_gate_out_dt],'-'),0,8),'YYYYMMDD')  -- mvmt_gate_out_dt
						, TO_DATE(SUBSTR(REPLACE(@[inv_gate_out_dt],'-'),0,8),'YYYYMMDD')  	-- inv_gate_out_dt
						, @[gate_out_td_dys]												-- gate_out_td_dys
						, @[mvmt_stay_dys]													-- mvmt_stay_dys
						, @[inv_stay_dys]													-- inv_stay_dys
						, @[stay_diff_dys]													-- stay_diff_dys
						, @[dcgo_clss_cd]													-- dcgo_clss_cd
						, @[bb_cgo_flg]														-- bb_cgo_flg
						, @[awk_cgo_flg]													-- awk_cgo_flg
						, @[rc_flg]															-- rc_flg
						, TO_DATE(SUBSTR(REPLACE(@[wrk_dt],'-'),0,8),'YYYYMMDD')  			-- wrk_dt
						, TO_DATE(SUBSTR(REPLACE(@[clm_dt],'-'),0,8),'YYYYMMDD')  			-- clm_dt
						, TO_DATE(SUBSTR(REPLACE(@[rail_bil_dt],'-'),0,8),'YYYYMMDD')  		-- rail_bil_dt
						, @[bkg_no]  									-- bkg_no
						--, NVL(RTRIM([bkg_no_split],'  '),'  ')			-- bkg_no_split
						, @[bl_no]  									-- bl_no
						--, [bl_no_tp]  									-- bl_no_tp
						--, [bl_no_chk]  								-- bl_no_chk
						, @[dscr_rsn]  									-- dscr_rsn
						, @[hndl_rslt_rmk]  							-- hndl_rslt_rmk
						, @[cntr_rmk]  									-- cntr_rmk
						, @[cre_usr_id]  								-- cre_usr_id
						, SYSDATE										-- cre_dt	
						, GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[locl_cre_dt])	-- locl_cre_dt
						, @[cre_usr_id]  									-- cre_usr_id
						, SYSDATE											-- upd_dt
						, GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[locl_cre_dt])	-- locl_upd_dt
)			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="tml_so_cntr_list_seq" type="12" value="" out="N"/>
