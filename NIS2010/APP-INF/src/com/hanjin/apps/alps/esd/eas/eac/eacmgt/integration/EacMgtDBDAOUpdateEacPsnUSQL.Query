<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOUpdateEacPsnUSQL">
			<desc><![CDATA[Personnel Config 를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE EAS_EXPN_AUD_CS_PSON_CFG
SET    EAC_USR_NM        = @[eac_usr_nm]       
     , PHN_NO            = @[phn_no]           
     , FAX_NO            = @[fax_no]           
     , USR_EML           = @[usr_eml]          
     , NTC_CC_RCV_EML    = @[ntc_cc_rcv_eml]   
     , EXPN_AUD_SCP_DESC = @[expn_aud_scp_desc]
     , EML_SUBJ_CTNT     = @[eml_subj_ctnt]    
     , EML_CTNT          = @[eml_ctnt]         
     , DELT_FLG          = 'N'         
     , UPD_USR_ID        = @[upd_usr_id]       
     , UPD_DT            = SYSDATE
WHERE  EAC_USR_ID        = @[eac_usr_id]

			]]></sql>
			<params>
				<param name="eac_usr_nm" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
				<param name="usr_eml" type="12" value="" out="N"/>
				<param name="ntc_cc_rcv_eml" type="12" value="" out="N"/>
				<param name="expn_aud_scp_desc" type="12" value="" out="N"/>
				<param name="eml_subj_ctnt" type="12" value="" out="N"/>
				<param name="eml_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eac_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
