<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffConditionManageDBDAOSearchTariffObjectListRSQL">
			<desc><![CDATA[Object 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
O.OBJ_LIST_NO
, O.OBJ_LIST_NM
, O.OBJ_LIST_ABBR_NM
, O.TML_OBJ_LIST_TP_CD
, O.TML_MEAS_UT_CD
, O.TML_VAL_TP_CD
, O.DFLT_CTNT
, O.DFLT_VAL
, O.MAX_VAL
, O.MIN_VAL
, O.OBJ_RMK
, O.INTER_USE_FLG
, O.DELT_FLG
FROM TES_OBJ_LIST O
WHERE 1=1
#if (${cond_tp_cd} != '') 
AND O.TML_OBJ_LIST_TP_CD = @[cond_tp_cd]
#else 
#end
AND NVL(O.DELT_FLG,'N') <> 'Y'
AND NVL(O.INTER_USE_FLG,'N') <> 'Y'
ORDER BY O.TML_OBJ_LIST_TP_CD, O.OBJ_LIST_NO			]]></sql>
			<params>
				<param name="cond_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
