<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SceAdminManageDBDAOaddSceMngRplnCSQL">
			<desc><![CDATA[manual replan table에 대상을 INSERT 한다.]]></desc>
			<sql><![CDATA[
MERGE INTO SCE_MNL_RPLN A
USING (SELECT COP_NO FROM SCE_COP_HDR WHERE COP_NO = @[cop_no]) B
   ON (A.COP_NO = B.COP_NO)
 WHEN MATCHED THEN
 UPDATE SET
  A.IO_BND_CD = @[io_bnd_cd]
, A.RPLN_SCS_FLG = @[rpln_scs_flg]
, A.CRE_USR_ID = @[cre_usr_id]
, A.CRE_DT = SYSDATE
, A.UPD_USR_ID = 'SYSTEM'
, A.UPD_DT = SYSDATE
, A.RPLN_JB_TP_CD = @[rpln_jb_tp_cd]
 WHEN NOT MATCHED THEN
 INSERT (
  COP_NO
, IO_BND_CD
, RPLN_SCS_FLG
, PCTL_NO
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
, RPLN_JB_TP_CD
) VALUES (
  @[cop_no]
, @[io_bnd_cd]
, @[rpln_scs_flg]
, @[pctl_no]
, @[cre_usr_id]
, SYSDATE
, 'SYSTEM'
, SYSDATE
, @[rpln_jb_tp_cd]
)			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="rpln_scs_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="rpln_jb_tp_cd" type="12" value="" out="N"/>
				<param name="pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
