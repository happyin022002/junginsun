<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DoNotificationSettingDBDAOModifyDoNoticeSettingUSQL">
			<desc><![CDATA[D/O Notification Setting 수정]]></desc>
			<sql><![CDATA[
UPDATE TRS_DO_NTFC_SET
   SET NTFC_ACT_FLG  = @[act_flg]
      ,LOCL_UPD_DT  = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd])
      ,DELT_FLG = NVL(@[delt_flg],'N')
      ,UPD_USR_ID  = @[usr_id]
      ,UPD_DT  = SYSDATE
WHERE 1=1
  AND DO_NTFC_SET_SEQ = @[do_ntfc_set_seq]			]]></sql>
			<params>
				<param name="act_flg" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="do_ntfc_set_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
