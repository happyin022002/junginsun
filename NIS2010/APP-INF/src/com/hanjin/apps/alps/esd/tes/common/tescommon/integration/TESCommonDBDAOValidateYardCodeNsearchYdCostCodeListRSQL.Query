<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOValidateYardCodeNsearchYdCostCodeListRSQL">
			<desc><![CDATA[Yard Code, Yard Cost Code Inquiry]]></desc>
			<sql><![CDATA[
SELECT
		CASE WHEN COUNT(YD_CD)>0 THEN 'Y' ELSE 'N' END YD_CD_EXISTING
		, YD_CD
		, YD_NM
		, YD_CHR_CD
		, YD_FCTY_TP_MRN_TML_FLG
		, YD_FCTY_TP_CY_FLG
		, YD_FCTY_TP_CFS_FLG
		, YD_FCTY_TP_RAIL_RMP_FLG
		, YD_OSHP_CD
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD,'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE	DECODE('MT','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_MT
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD,'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE  DECODE('ON','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				--##ON-Dock Manual Cost Code 수정
				--##Manual Cost Code에 TMNDRF, TMNDRM을 불러오기 위해 일시적으로 SQL막아줌. 2007.07.17
				--##AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_ON
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD,'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE	DECODE('OT','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_OT
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD,'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE	DECODE('OS','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_OS
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD,'|')),'|')
		FROM	(
				SELECT C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE  DECODE('ST','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_ST
		-- // [CHM-201642291]Agreement/invoice에서 Cost Code Description 추가 2016-06-28
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD||'\t'||TES_GET_LGS_COST_ABBR_NM_FNC(LGS_COST_CD),'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE	DECODE('MT','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_NM_MT
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD||'\t'||TES_GET_LGS_COST_ABBR_NM_FNC(LGS_COST_CD),'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE  DECODE('ON','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				--##ON-Dock Manual Cost Code 수정
				--##Manual Cost Code에 TMNDRF, TMNDRM을 불러오기 위해 일시적으로 SQL막아줌. 2007.07.17
				--##AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_NM_ON
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD||'\t'||TES_GET_LGS_COST_ABBR_NM_FNC(LGS_COST_CD),'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE	DECODE('OT','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_NM_OT
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD||'\t'||TES_GET_LGS_COST_ABBR_NM_FNC(LGS_COST_CD),'|')),'|')
		FROM	(
				SELECT	C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE	DECODE('OS','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_NM_OS
		, (
		SELECT	LTRIM(MAX(SYS_CONNECT_BY_PATH(LGS_COST_CD||'\t'||TES_GET_LGS_COST_ABBR_NM_FNC(LGS_COST_CD),'|')),'|')
		FROM	(
				SELECT C.LGS_COST_CD
					, ROWNUM ROW_ID
				FROM	TES_TML_SO_COST C
					, MDM_YARD Y
				WHERE  DECODE('ST','MT',MRN_TML_FLG,'ON',ODCK_RAIL_CHG_FLG,'OT',FDCK_CY_TML_FLG,
									'OS',FDCK_CY_STO_FLG,'ST',STO_INV_FLG)  = 'Y'
				AND		CFS_FLG      IN (DECODE(Y.YD_FCTY_TP_CFS_FLG,'N','N','Y'),'N')
				AND		RAIL_RMP_FLG IN (DECODE(Y.YD_FCTY_TP_RAIL_RMP_FLG,'N','N','Y'),'N')
				AND		XCLD_TML_FLG IN (DECODE(Y.YD_OSHP_CD,'O','Y','N'),'N')
				AND		Y.YD_CD		= @[yd_cd]
				-- // CHM-201432861 TMRFGR 사용국가제한_ US, CA 북미 2국가에서만 발생가능하도록 조치 - 4347-11-20
				#if (${genset_flg} != 'Y') 
				AND		C.LGS_COST_CD	<> @[lgs_cost_cd]
				#end
				ORDER BY C.LGS_COST_CD ASC
				)
		CONNECT BY PRIOR ROW_ID = ROW_ID - 1
		START WITH ROW_ID = 1
		) COST_CD_NM_ST
FROM	MDM_YARD
WHERE	YD_CD	= @[yd_cd]
AND		DELT_FLG = 'N'
GROUP BY YD_CD
	, YD_NM
	, YD_CHR_CD
	, YD_OSHP_CD
	, YD_FCTY_TP_MRN_TML_FLG
	, YD_FCTY_TP_CY_FLG
	, YD_FCTY_TP_CFS_FLG, YD_FCTY_TP_RAIL_RMP_FLG			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
