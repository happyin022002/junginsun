<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChAnalysisReportDBDAOWeekDateWeekRSQL">
			<desc><![CDATA[입력된 주의 첫날과 마지막 날짜를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT   
		WK_ST_DT FM_DT,  
		WK_END_DT TO_DT
FROM  	EQR_WK_PRD                   	
WHERE 	PLN_YR||PLN_WK = @[WEEK]
			]]></sql>
			<params>
				<param name="WEEK" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
