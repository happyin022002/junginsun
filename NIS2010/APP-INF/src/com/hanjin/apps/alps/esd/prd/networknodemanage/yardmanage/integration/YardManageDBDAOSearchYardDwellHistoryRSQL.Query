<sqls>
	<querys>
		<query name="YardManageDBDAOSearchYardDwellHistoryRSQL">
			<desc><![CDATA[YardManageDBDAOSearchYardDwellHistoryRSQL]]></desc>
			<sql><![CDATA[
select NOD_CD,substr(HIS_CRE_DT,1,6) his_month,HIS_CRE_DT,
IB_DRY_AVG_DWLL_HRS,IB_RF_AVG_DWLL_HRS,OB_DRY_AVG_DWLL_HRS,OB_RF_AVG_DWLL_HRS,
COP_IB_DRY_AVG_DWLL_HRS,COP_IB_RF_AVG_DWLL_HRS,COP_OB_DRY_AVG_DWLL_HRS,COP_OB_RF_AVG_DWLL_HRS,
IB_DRY_MIN_DWLL_HRS,IB_RF_MIN_DWLL_HRS,OB_DRY_MIN_DWLL_HRS,OB_RF_MIN_DWLL_HRS
from PRD_DWLL_TM_HIS
where nod_cd=@[search_nod_cd]
order by HIS_CRE_DT desc			]]></sql>
			<params>
				<param name="search_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
