<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AdjustmentManageDBDAOSearchNextProcessCheckRSQL">
			<desc><![CDATA[다음 단계 진행 여부 체크]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(G.N3PTY_NO),0,'N','Y') TPB_NXT_PRC_CHK
  FROM TPB_OTS_GRP G
 WHERE G.N3PTY_NO = @[n3pty_no]
   AND G.OTS_STS_CD NOT IN ('R','O','J')			]]></sql>
			<params>
				<param name="n3pty_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
