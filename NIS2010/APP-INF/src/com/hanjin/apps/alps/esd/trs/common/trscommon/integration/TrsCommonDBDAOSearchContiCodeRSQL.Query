<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TrsCommonDBDAOSearchContiCodeRSQL">
			<desc><![CDATA[오피스의 대륙 코드를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT	NVL(C.CONTI_CD, '') CONTI_CD /* CONTINENT CODE */ 
FROM	MDM_ORGANIZATION B,                                              
		MDM_LOCATION C                                                   
WHERE	B.OFC_CD = @[OFC_CD]
		AND B.LOC_CD = C.LOC_CD                                              
		AND B.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="OFC_CD" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
