<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOAddPlnSoListCSQL">
			<desc><![CDATA[List<ScePlnSoListVO> 에서 data 를 받아 단순 insert 만을 수행한다.]]></desc>
			<sql><![CDATA[
INSERT INTO SCE_PLN_SO_LIST
        (
            COP_NO,
            COST_ACT_GRP_SEQ,
            COST_ACT_GRP_CD,
            CTRL_OFC_CD,
            N1ST_NOD_PLN_DT,
            N1ST_NOD_CD,
            N2ND_NOD_CD,
            N3RD_NOD_CD,
            N4TH_NOD_CD,
            PCTL_IO_BND_CD,
            PCTL_COST_MOD_CD,
            N1ST_VNDR_SEQ,
            TRSP_SO_STS_CD,
            TRSP_MOD_CD,
            INLND_ROUT_INV_BIL_PATT_CD,
            INV_BIL_PATT_DIV_FLG,
            CRE_USR_ID,
            CRE_DT,
            UPD_USR_ID,
            UPD_DT,
			DOR_ARR_DT,
			LST_NOD_PLN_DT
        )
values
(
    @[cop_no],
	@[cost_act_grp_seq],
	@[cost_act_grp_cd],
	@[ctrl_ofc_cd],
	TO_DATE(@[n1st_nod_pln_dt], 'YYYYMMDDHH24MISS'),
	@[n1st_nod_cd],
	@[n2nd_nod_cd],
	@[n3rd_nod_cd],
	@[n4th_nod_cd],
	@[pctl_io_bnd_cd],
	@[pctl_cost_mod_cd],
	@[n1st_vndr_seq],
	@[trsp_so_sts_cd],
	@[trsp_mod_cd],
	@[inlnd_rout_inv_bil_patt_cd],
	@[inv_bil_patt_div_flg],
	@[cre_usr_id],
	NVL(TO_DATE(@[cre_dt], 'YYYYMMDDHH24MISS'), sysdate),
	@[upd_usr_id],
	NVL(TO_DATE(@[upd_dt], 'YYYYMMDDHH24MISS'), sysdate),
	TO_DATE(@[dor_arr_dt], 'YYYYMMDDHH24MISS'),
	TO_DATE(@[lst_nod_pln_dt], 'YYYYMMDDHH24MISS')
)			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="" out="N"/>
				<param name="cost_act_grp_cd" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="n1st_nod_pln_dt" type="12" value="" out="N"/>
				<param name="n1st_nod_cd" type="12" value="" out="N"/>
				<param name="n2nd_nod_cd" type="12" value="" out="N"/>
				<param name="n3rd_nod_cd" type="12" value="" out="N"/>
				<param name="n4th_nod_cd" type="12" value="" out="N"/>
				<param name="pctl_io_bnd_cd" type="12" value="" out="N"/>
				<param name="pctl_cost_mod_cd" type="12" value="" out="N"/>
				<param name="n1st_vndr_seq" type="12" value="" out="N"/>
				<param name="trsp_so_sts_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="inlnd_rout_inv_bil_patt_cd" type="12" value="" out="N"/>
				<param name="inv_bil_patt_div_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="dor_arr_dt" type="12" value="" out="N"/>
				<param name="lst_nod_pln_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
