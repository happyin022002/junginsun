<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EASCommonDBDAOVendorNameRSQL">
			<desc><![CDATA[VendorName]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ
     , VNDR_LGL_ENG_NM
  FROM MDM_VENDOR
 WHERE VNDR_SEQ= @[vndr_seq]
   AND NVL(DELT_FLG, 'N') <> 'Y' 			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
