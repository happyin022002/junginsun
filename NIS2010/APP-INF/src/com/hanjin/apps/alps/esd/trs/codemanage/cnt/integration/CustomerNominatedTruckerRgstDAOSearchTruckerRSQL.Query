<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerNominatedTruckerRgstDAOSearchTruckerRSQL">
			<desc><![CDATA[SearchTrucker]]></desc>
			<sql><![CDATA[
SELECT A.VNDR_SEQ
       ,A.VNDR_LGL_ENG_NM AS VNDR_NM
       ,A.USA_EDI_CD
  FROM MDM_VENDOR A
 WHERE 1=1
--   AND A.VNDR_CNT_CD = 'US'
--   AND A.WO_EDI_USE_FLG = 'Y'
--   AND A.USA_EDI_CD IS NOT NULL 
   AND A.DELT_FLG = 'N'
   AND A.VNDR_SEQ = TO_NUMBER(@[as_vndr_seq])
			]]></sql>
			<params>
				<param name="as_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
