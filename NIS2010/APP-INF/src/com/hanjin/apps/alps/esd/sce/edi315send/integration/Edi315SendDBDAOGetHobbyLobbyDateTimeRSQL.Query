<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetHobbyLobbyDateTimeRSQL">
			<desc><![CDATA[GetHobbyLobbyDateTime]]></desc>
			<sql><![CDATA[
SELECT  to_char(to_date(substr(@[event_dt], 1, 8)||@[postfix]  
            , 'yyyymmddhh24miss') - 1, 'yyyymmddhh24miss')
FROM DUAL			]]></sql>
			<params>
				<param name="event_dt" type="12" value="" out="N"/>
				<param name="postfix" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
