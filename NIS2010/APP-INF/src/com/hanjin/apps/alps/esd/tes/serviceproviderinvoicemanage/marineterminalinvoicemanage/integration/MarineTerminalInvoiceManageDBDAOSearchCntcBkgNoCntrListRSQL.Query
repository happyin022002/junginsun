<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MarineTerminalInvoiceManageDBDAOSearchCntcBkgNoCntrListRSQL">
			<desc><![CDATA[CNTC항차이면서 NH인 BKG NO. 없는 Container List 조회]]></desc>
			<sql><![CDATA[
SELECT    C.TML_SO_OFC_CTY_CD
		, C.TML_SO_SEQ
		, C.TML_SO_CNTR_LIST_SEQ
		, C.VRFY_RSLT_IND_CD
		, C.MODI_FLG
		, C.DSCR_IND_CD
		, C.DSCR_DTL_IND_CD
		, C.RVIS_IND_FLG
		, C.TML_RVIS_IND_FLG
		, C.STO_RVIS_IND_FLG
		, C.STV_RVIS_IND_FLG
		, C.CGO_RVIS_IND_FLG
		, C.RVIS_GATE_IN_FLG
		, C.RVIS_GATE_OUT_FLG
		, C.TML_IF_SEQ
		, C.VSL_CD
		, C.SKD_VOY_NO
		, C.SKD_DIR_CD
		, C.FINC_VSL_CD
		, C.FINC_SKD_VOY_NO
		, C.FINC_SKD_DIR_CD
		, C.IO_BND_CD
		, C.IOC_CD
		, C.LANE_CD
		, C.TML_TRNS_MOD_CD
		, C.ATB_DT
		, C.CNTR_NO
		, C.CNTR_TPSZ_CD
		, C.CNTR_STY_CD
		, C.LOCL_TS_IND_CD
		, C.SAM_LOCL_TS_IND_CD
		, C.RCVDE_TERM_IND_CD
		, C.PRE_YD_CD
		, C.MVMT_GATE_IN_DT
		, C.INV_GATE_IN_DT
		, C.GATE_IN_TD_DYS
		, C.MVMT_GATE_OUT_DT
		, C.INV_GATE_OUT_DT
		, C.GATE_OUT_TD_DYS
		, C.MVMT_STAY_DYS
		, C.INV_STAY_DYS
		, C.STAY_DIFF_DYS
		, C.DCGO_CLSS_CD
		, C.BB_CGO_FLG
		, C.AWK_CGO_FLG
		, C.RC_FLG
		, C.WRK_DT
		, C.CLM_DT
		, C.RAIL_BIL_DT
		, C.BKG_NO
		, C.BL_NO
		, C.DSCR_RSN
		, C.HNDL_RSLT_RMK
		, C.CNTR_RMK
		, C.CLPT_IND_SEQ
		, C.CALL_YD_IND_SEQ 
		, C.LOCL_CRE_DT
		, C.LOCL_UPD_DT
		, C.CRE_USR_ID
		, C.CRE_DT
		, C.UPD_USR_ID
		, C.UPD_DT
		, H.YD_CD
FROM	TES_TML_SO_HDR H
		, TES_TML_SO_CNTR_LIST C
WHERE	1	= 1
AND		H.TML_SO_OFC_CTY_CD = C.TML_SO_OFC_CTY_CD 
AND		H.TML_SO_SEQ 		= C.TML_SO_SEQ
AND		C.TML_SO_OFC_CTY_CD	= @[tml_so_ofc_cty_cd]
AND		C.TML_SO_SEQ		= @[tml_so_seq]
AND		VSL_CD				= @[vsl_cd]
AND		DSCR_IND_CD			= @[dscr_ind_cd]
AND		CNTR_STY_CD			= @[cntr_sty_cd]
AND		NVL(BKG_NO, 'N')	= 'N'			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="dscr_ind_cd" type="12" value="" out="N"/>
				<param name="cntr_sty_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
