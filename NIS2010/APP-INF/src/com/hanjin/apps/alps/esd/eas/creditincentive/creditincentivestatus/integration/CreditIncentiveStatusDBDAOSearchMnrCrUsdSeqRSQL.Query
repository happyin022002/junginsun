<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CreditIncentiveStatusDBDAOSearchMnrCrUsdSeqRSQL">
			<desc><![CDATA[M&R Credit Used Seq Search]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN MAX(A.CR_USD_SEQ) IS NULL THEN 1
            ELSE MAX(A.CR_USD_SEQ) + 1
        END CR_USD_SEQ
FROM EAS_MNR_CR_USD A
WHERE A.CR_ISS_NO = @[cr_iss_no]			]]></sql>
			<params>
				<param name="cr_iss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
