<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchContactPointRSQL">
			<desc><![CDATA[Contact point를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ
     , VNDR_CNTC_PNT_SEQ
     , VNDR_CNTC_PNT_NM
     , PSN_NM
     , PSN_DESC
     , STE_NM
     , CTY_NM
     , PHN_NO
     , FAX_NO
     , VNDR_EML
     , EAC_EML_USE_FLG
     , EAC_FAX_USE_FLG
     , DELT_FLG
     , (SELECT USR_NM FROM COM_USER WHERE USR_ID = A.UPD_USR_ID) AS CRE_USR_NM
     , UPD_DT AS CRE_DT
FROM   EAS_EXPN_AUD_CS_CNTC_PNT A
WHERE  VNDR_SEQ = @[g_vndr_seq]
AND    DELT_FLG IS NULL
ORDER BY VNDR_CNTC_PNT_SEQ ASC

			]]></sql>
			<params>
				<param name="g_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
