<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AsiaOceanFeederCostManageDBDAOSearchFeederCostTariffNoRSQL">
			<desc><![CDATA[searchFeederCostTariffNo]]></desc>
			<sql><![CDATA[
SELECT COST_TRF_NO
  FROM AOC_CHN_FDR_TRF_HDR
 WHERE RHQ_CD = @[in_ofc_cd]
   AND COST_TRF_STS_CD IN ('B', 'U', 'C')
ORDER BY CRE_DT DESC 			]]></sql>
			<params>
				<param name="in_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
