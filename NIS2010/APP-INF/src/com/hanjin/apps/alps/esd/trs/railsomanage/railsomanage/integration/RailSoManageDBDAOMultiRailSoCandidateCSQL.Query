<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RailSoManageDBDAOMultiRailSoCandidateCSQL">
			<desc><![CDATA[EQR_REPO_EXE_SO_IF INSERT]]></desc>
			<sql><![CDATA[
INSERT INTO EQR_REPO_EXE_SO_IF(
			 REPO_PLN_ID
			,PLN_YRWK
			,PLN_SEQ
			,REF_ID
			,REF_SEQ
			,CNTR_TPSZ_CD
			,CO_CD
			,SO_IF_DIV_CD
			,TRSP_MOD_CD
			,FM_YD_CD
			,FM_DT
			,TO_YD_CD
			,TO_DT
			,VSL_LANE_CD
			,VSL_CD
			,SKD_VOY_NO
			,SKD_DIR_CD
			,REPO_PURP_RMK
			,WO_EXE_FLG
			,REPO_COST_AMT
			,EQ_CTRL_OFC_CD
			,SO_RQST_DT
			,WO_RQST_FLG
			,TRSP_SO_STS_CD
			,CRE_USR_ID
			,CRE_DT
			,UPD_USR_ID
			,UPD_DT
		)VALUES(
			 @[repo_pln_id]       		-- REPO_PLN_ID
			,@[pln_yrwk]          		-- PLN_YRWK
			,1           		  		-- PLN_SEQ
			,@[ref_id]            		-- REF_ID
			,@[ref_seq]           		-- REF_SEQ
			,@[eq_tpsz_cd]        		-- CNTR_TPSZ_CD
			,'H'             	  		-- CO_CD H:SM LINE
			,@[so_if_div_cd]      		-- SO_IF_DIV_CD
			,@[trsp_mod_cd]       		-- TRSP_MOD_CD
			,@[fm_loc_cd]||@[fm_nod_cd] -- FM_YD_CD
			,@[fm_dt]             		-- FM_DT
			,@[to_loc_cd]||@[to_nod_cd] -- TO_YD_CD
			,@[to_dt]             		-- TO_DT
			,@[slan_cd]          		-- VSL_LANE_CD
			,SUBSTR(@[vvd_cd],1,4)      -- VSL_CD
			,SUBSTR(@[vvd_cd],5,4)  	-- SKD_VOY_NO
			,SUBSTR(@[vvd_cd],9,1)		-- SKD_DIR_CD
			,@[repo_purp_rmk]     		-- REPO_PURP_RMK
			,'N'        		  		-- WO_EXE_FLG
			,0				      		-- REPO_COST_AMT
			,@[ofc_cd]    		  		-- EQ_CTRL_OFC_CD
			,@[so_rqst_dt]        		-- SO_RQST_DT
            ,'Y'				  		-- WO_RQST_FLG
			,'P'  				  		-- TRSP_SO_STS_CD
			,@[usr_id]            		-- CRE_USR_ID
			,SYSDATE	          		-- CRE_DT
			,@[usr_id]        	  		-- UPD_USR_ID
			,SYSDATE              		-- UPD_DT
		)			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="ref_id" type="12" value="" out="N"/>
				<param name="ref_seq" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="so_if_div_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="fm_loc_cd" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_loc_cd" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="repo_purp_rmk" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="so_rqst_dt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
