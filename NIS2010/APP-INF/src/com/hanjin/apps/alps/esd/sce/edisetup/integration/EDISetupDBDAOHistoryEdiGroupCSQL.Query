<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EDISetupDBDAOHistoryEdiGroupCSQL">
			<desc><![CDATA[EdiGroup 테이블에 모든 사항을 히스토리로 기록한다 (Insert, Update, Delete)]]></desc>
			<sql><![CDATA[
insert into edi_grp_his (
							EDI_SND_HIS_SEQ
                            ,EDI_GRP_CD
                            ,CO_DIV_CD
                            ,EDI_GRP_DESC
                            ,CUST_TRD_PRNR_ID
                            ,PROV_TRD_PRNR_ID
                            ,DELT_FLG
                            ,CRE_USR_ID
                            ,CRE_DT
                            ,EAI_EVNT_DT
                            )

       values (
				EDI_GROUP_HIS_SEQ1.NEXTVAL,
                @[group_cd],
                @[company_cd],
                @[group_nm],
                @[cust_tp_id],
                @[host_tp_id],
                @[del_ind],
                @[cre_id],
                sysdate,
                to_date(@[eai_date],'yyyy/mm/dd hh24:mi:ss')
       )			]]></sql>
			<params>
				<param name="group_cd" type="12" value="" out="N"/>
				<param name="company_cd" type="12" value="" out="N"/>
				<param name="group_nm" type="12" value="" out="N"/>
				<param name="cust_tp_id" type="12" value="" out="N"/>
				<param name="host_tp_id" type="12" value="" out="N"/>
				<param name="del_ind" type="12" value="" out="N"/>
				<param name="cre_id" type="12" value="" out="N"/>
				<param name="eai_date" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
