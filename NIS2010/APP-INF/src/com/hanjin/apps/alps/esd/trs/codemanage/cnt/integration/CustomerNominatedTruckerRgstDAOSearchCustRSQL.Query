<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerNominatedTruckerRgstDAOSearchCustRSQL">
			<desc><![CDATA[Customer Code로 Customer Name 검색]]></desc>
			<sql><![CDATA[
SELECT CUST_LGL_ENG_NM
  FROM MDM_CUSTOMER
 WHERE DELT_FLG = 'N'
     AND CUST_CNT_CD       = substr(@[as_cust_seq],1,2)
     AND CUST_SEQ          = substr(@[as_cust_seq],3)
			]]></sql>
			<params>
				<param name="as_cust_seq" type="12" value="JP000484" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
