<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContractAttachDAOSearchContractRSQL">
			<desc><![CDATA[Service Proivder의 계약서 첨부내역을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT TRSP_AGMT_OFC_CTY_CD
      ,TRSP_AGMT_SEQ
      ,CTRT_SEQ
      ,CTRT_MN_FLG
      ,AGMT_DOC_NO
      ,AGMT_DOC_DESC
      ,AGMT_DOC_EFF_FM_DT
      ,AGMT_DOC_EFF_TO_DT
      ,CASE WHEN AGMT_DOC_NO IS NULL THEN '' ELSE 'O' END CONTRACT_VIEW
  FROM TRS_AGMT_CTRT_ATCH
 WHERE TRSP_AGMT_OFC_CTY_CD = SUBSTR(@[agmt_no],1,3)
   AND TRSP_AGMT_SEQ = SUBSTR(@[agmt_no],4)
   #if (${f_ctrt_mn_flg} == 'Y' )
      AND CTRT_MN_FLG = 'Y'
   #else
      AND CTRT_MN_FLG = 'N'
   #end
			]]></sql>
			<params>
				<param name="agmt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
