<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InlandRouteManageDBDAORowSearchInlandRouteManageRSQL">
			<desc><![CDATA[RowSearchInlandRouteManage]]></desc>
			<sql><![CDATA[
SELECT lnk_org_nod_cd, lnk_dest_nod_cd, l.vndr_seq, v.VNDR_LGL_ENG_NM vndr_name, 
        	        TRIM (TO_CHAR (TRUNC (l.tztm_hrs / 24, 0), '00'))                      
        	        || TRIM (TO_CHAR (MOD (l.tztm_hrs, 24), '00')) fmt_tztm_hrs,           
        	        tztm_hrs, lnk_dist, dist_ut_cd, rail_crr_tp_cd, trsp_mod_cd            
        	   FROM prd_inlnd_each_lnk l, mdm_vendor v                     
        	  WHERE l.lnk_org_nod_cd = @[from_nod]                                                   
        	    AND l.lnk_dest_nod_cd = @[to_nod]                                                 
        	    AND l.trsp_mod_cd = @[trsp_mod]                                                      
        	    AND nvl(l.delt_flg,'N') <> 'Y'                                             
        	    AND l.vndr_seq = v.vndr_seq(+)			]]></sql>
			<params>
				<param name="from_nod" type="12" value="" out="N"/>
				<param name="to_nod" type="12" value="" out="N"/>
				<param name="trsp_mod" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
