<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOExptCustDupChkRSQL">
			<desc><![CDATA[Dup Check]]></desc>
			<sql><![CDATA[
SELECT COUNT (*) AS DUP_CNT
  FROM PRD_NOD_CNST_EXPT
 WHERE     1 = 1
       AND DELT_FLG = 'N'
       AND NOD_CD = @[nod_cd]
       AND NOD_CNST_ITM_CD = @[nod_cnst_itm_cd]
       AND NOD_CNST_SEQ = @[nod_cnst_seq]
       AND CNST_EXPT_TP_CD = @[cnst_expt_tp_cd]
       AND CNST_EXPT_NO = @[cnst_expt_no]			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_seq" type="12" value="" out="N"/>
				<param name="cnst_expt_tp_cd" type="12" value="" out="N"/>
				<param name="cnst_expt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
