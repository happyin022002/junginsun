<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchScheduleInformationByActRSQL">
			<desc><![CDATA[SearchScheduleInformationByAct]]></desc>
			<sql><![CDATA[
SELECT act_sts_mapg_cd skd_mapg_cd
, nod_cd skd_nod_cd
, act_rcv_tp_cd skd_rcv_tp_cd
, act_umch_tp_cd call_yd_ind_seq
, err_msg
FROM   SCE_ACT_RCV_IF
WHERE  ACT_RCV_DT = @[skd_rcv_dt]
AND    ACT_RCV_NO = @[act_rcv_no]
			]]></sql>
			<params>
				<param name="skd_rcv_dt" type="12" value="20091027" out="N"/>
				<param name="act_rcv_no" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
