<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceEdiHitDBDAOSaveInvoiceCSQL">
			<desc><![CDATA[Invoice Save]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_INV_WRK (
	   INV_NO
      ,INV_VNDR_SEQ
	  ,TRSP_INV_AUD_STS_CD
	  ,WO_VNDR_SEQ
	  ,INV_CURR_CD
	  ,INV_BZC_AMT
	  ,INV_VAT_AMT
      ,INV_WHLD_TAX_AMT
	  ,INV_TTL_AMT
	  ,INV_RCV_DT
	  ,INV_ISS_DT
	  ,IF_SYS_KND_CD
      ,INV_HLD_FLG
	  ,DELT_FLG
	  ,CRE_OFC_CD
	  ,CRE_USR_ID
	  ,CRE_DT
	  ,UPD_USR_ID
	  ,UPD_DT
	  ,LOCL_CRE_DT
      ,LOCL_UPD_DT
	)
    SELECT A.INV_NO
          ,A.INV_VNDR_SEQ
  	      ,'SV' TRSP_INV_AUD_STS_CD
	      ,A.INV_VNDR_SEQ WO_VNDR_SEQ
          ,A.INV_CURR_CD
          ,A.INV_BZC_AMT
      	  ,A.INV_VAT_AMT
          ,A.INV_WHLD_TAX_AMT
     	  ,A.INV_TTL_AMT
   	      ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC((SELECT OFC_CD FROM MDM_VENDOR WHERE VNDR_SEQ = A.INV_VNDR_SEQ)) INV_RCV_DT
  	      ,A.INV_ISS_DT
 	      ,'I' IF_SYS_KND_CD
          ,'N' INV_HLD_FLG
 	      ,'N' DELT_FLG 
	      ,(SELECT OFC_CD FROM MDM_VENDOR WHERE VNDR_SEQ = A.INV_VNDR_SEQ) CRE_OFC_CD
	      ,'HIT_INV_EDI'
	      ,SYSDATE
	      ,'HIT_INV_EDI'
	      ,SYSDATE
	      ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC((SELECT OFC_CD FROM MDM_VENDOR WHERE VNDR_SEQ = A.INV_VNDR_SEQ)) LOCL_CRE_DT
	      ,GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC((SELECT OFC_CD FROM MDM_VENDOR WHERE VNDR_SEQ = A.INV_VNDR_SEQ)) LOCL_UPD_DT 
     FROM TRS_INV_EDI_RCV  A
    WHERE A.INV_EDI_RCV_SEQ = @[inv_edi_rcv_seq]
      AND A.VAL_CHK_FLG = 'Y'			]]></sql>
			<params>
				<param name="inv_edi_rcv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
