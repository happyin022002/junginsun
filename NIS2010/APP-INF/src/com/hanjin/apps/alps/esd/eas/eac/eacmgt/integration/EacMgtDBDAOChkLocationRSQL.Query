<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOChkLocationRSQL">
			<desc><![CDATA[Location 에 값이 존재하는지 체크 한다]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) AS isflag
   FROM MDM_LOCATION
  WHERE LOC_CD = @[yd_cd]

			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
