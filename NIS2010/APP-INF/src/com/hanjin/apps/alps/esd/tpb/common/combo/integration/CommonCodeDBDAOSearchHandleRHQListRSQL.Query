<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonCodeDBDAOSearchHandleRHQListRSQL">
			<desc><![CDATA[SearchHandleRHQList]]></desc>
			<sql><![CDATA[
#if (${s_office_level} == 'H')

SELECT   OFC_CD
       , OFC_CD
FROM     TPB_HNDL_OFC A
WHERE    1 = 1
AND      NVL(DELT_FLG,'N') = 'N'
AND      N3PTY_OFC_TP_CD = 'R'

#elseif (${s_office_level} == 'R' || ${s_office_level} == 'S')

SELECT   OFC_CD
       , OFC_CD
FROM     TPB_HNDL_OFC A
WHERE    1 = 1
AND      NVL(DELT_FLG,'N') = 'N'
AND      N3PTY_OFC_TP_CD = 'R'
AND      OFC_CD LIKE SUBSTR(@[s_ofc_cd_for_rhq],1,3)||'%'

#elseif (${s_office_level} == 'T' || ${s_office_level} == 'G')

SELECT   RHQ_CD AS OFC_CD
       , RHQ_CD AS OFC_CD
FROM     TPB_HNDL_OFC A
WHERE    1 = 1
AND      NVL(DELT_FLG,'N') = 'N'
AND      N3PTY_OFC_TP_CD = 'T'
AND      OFC_CD = @[s_ofc_cd_for_rhq]

#else

SELECT   'X'
FROM     DUAL 
WHERE    1 = 0

#end			]]></sql>
			<params>
				<param name="s_ofc_cd_for_rhq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
