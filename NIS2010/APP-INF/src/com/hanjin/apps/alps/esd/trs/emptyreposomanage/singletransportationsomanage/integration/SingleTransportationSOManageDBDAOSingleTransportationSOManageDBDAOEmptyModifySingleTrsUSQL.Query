<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSingleTransportationSOManageDBDAOEmptyModifySingleTrsUSQL">
			<desc><![CDATA[f_cmd : MODIFY]]></desc>
			<sql><![CDATA[
UPDATE trs_trsp_svc_ord
   SET eq_no = @[eq_no]
      ,inter_rmk = @[inter_rmk]
      ,spcl_instr_rmk = @[spcl_instr_rmk]
      ,trsp_so_cmb_tp_cd = @[trsp_so_cmb_tp_cd]
#if (${cbstatus} == 'CF') 
      ,trsp_so_cmb_seq = @[trsp_so_cmb_seq]
      ,cmb_so_rlt_sts_flg = 'F'
      ,trsp_so_cmb_srt_no = @[cmbSeq]
#end
       upd_dt = globaldate_pkg.time_local_ofc_fnc (@[soffice_cd])
      ,upd_usr_id = @[upd_usr_id]
 WHERE trsp_so_ofc_cty_cd = @[trsp_so_ofc_cty_cd]
   AND trsp_so_seq = @[trsp_so_seq]
   AND delt_flg = 'N'
   AND hjl_no IS NULL			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="inter_rmk" type="12" value="" out="N"/>
				<param name="spcl_instr_rmk" type="12" value="" out="N"/>
				<param name="trsp_so_cmb_tp_cd" type="12" value="" out="N"/>
				<param name="trsp_so_cmb_seq" type="2" value="" out="N"/>
				<param name="cmbSeq" type="2" value="" out="N"/>
				<param name="soffice_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
