<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualCustomerPopupDBDAOSearchActualCustomerRSQL">
			<desc><![CDATA[Actual Customer Popup에서 Customer 조회]]></desc>
			<sql><![CDATA[
#if (${sCustInfoIndicator} == 'EUR')
    SELECT  A.ACT_CUST_ADDR_SEQ
           ,A.FCTRY_NM
           ,A.CNTC_PSON_NM
           ,A.FCTRY_ADDR
           ,A.ACT_CUST_PST_CD
           ,A.CNTC_PSON_PHN_NO
           ,A.CNTC_PSON_FAX_NO
           ,A.CNTC_PSON_REQ_RMK
    FROM    TRS_TRSP_ACT_CUST_ADDR A
    WHERE   A.ACT_CUST_CNT_CD  = @[acCustCd]
    AND     A.ACT_CUST_SEQ     = @[acCustSeq]
#elseif (${sCustInfoIndicator} == 'XEUR')
    SELECT   D.DFLT_ACT_CUST_FLG     IBCHECK
           , D.TRSP_ACT_CUST_SEQ     ACT_CUST_ADDR_SEQ
           , D.ACT_CUST_NM       FCTRY_NM
           , D.CNTC_PSON_NM        CNTC_PSON_NM
           , D.ACT_CUST_ADDR       FCTRY_ADDR
           , D.ACT_CUST_ZIP_CD     ACT_CUST_PST_CD
           , D.ACT_CUST_PHN_NO     CNTC_PSON_PHN_NO
           , D.ACT_CUST_FAX_NO     CNTC_PSON_FAX_NO
           , D.ACT_CUST_RMK        CNTC_PSON_REQ_RMK
    FROM     TRS_TRSP_USA_ACT_CUST_DTL   D
    WHERE    D.TRSP_ACT_CUST_NO  = @[sUSACustNo]
    AND      D.DELT_FLG          = 'N'
#end			]]></sql>
			<params>
				<param name="acCustCd" type="12" value="" out="N"/>
				<param name="acCustSeq" type="12" value="" out="N"/>
				<param name="sUSACustNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
