<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchisVvdView2RSQL">
			<desc><![CDATA[Edi315SendDBDAOSearchisVvdView2RSQL]]></desc>
			<sql><![CDATA[
select count(@[e_cop_no])  
           from    SCE_COP_DTL  
           
where 1=1  
	and cop_no = @[e_cop_no]  
	and cop_dtl_seq > @[e_cop_dtl_seq]  
	and (  
		STND_EDI_STS_CD = DECODE(@[e_edi_sts], 'VE', 'VAD', @[e_edi_sts])  
		OR  
		ACT_CD = @[e_edi_sts]  
		)     			]]></sql>
			<params>
				<param name="e_cop_no" type="12" value="CSZP9A21012247" out="N"/>
				<param name="e_cop_dtl_seq" type="12" value="6071" out="N"/>
				<param name="e_edi_sts" type="12" value="URN" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
