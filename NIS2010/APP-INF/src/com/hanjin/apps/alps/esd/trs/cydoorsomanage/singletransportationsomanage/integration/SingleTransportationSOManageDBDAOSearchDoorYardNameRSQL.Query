<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOSearchDoorYardNameRSQL">
			<desc><![CDATA[USA CY & Door Creation화면에서 그리드의 Door Yard변경시 Door Yard name 조회]]></desc>
			<sql><![CDATA[
SELECT ZN_NM
  FROM MDM_ZONE A
 WHERE 1=1
   AND A.ZN_CD = @[zn_cd]
   AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="zn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
