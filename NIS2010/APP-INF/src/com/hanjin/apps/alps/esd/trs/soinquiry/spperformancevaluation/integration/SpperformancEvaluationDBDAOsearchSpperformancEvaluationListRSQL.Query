<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpperformancEvaluationDBDAOsearchSpperformancEvaluationListRSQL">
			<desc><![CDATA[Performance Evaluation 조회]]></desc>
			<sql><![CDATA[
SELECT WO.TRSP_WO_OFC_CTY_CD||WO.TRSP_WO_SEQ  AS TRS_WO_KEY
      ,WO.TRSP_WO_OFC_CTY_CD AS TRSP_WO_OFC_CTY_CD
      ,WO.TRSP_WO_SEQ AS TRSP_WO_SEQ
      ,MAX(WO.WO_VNDR_SEQ) AS WO_VNDR_SEQ
      ,MAX(MV.VNDR_LGL_ENG_NM) AS VNDR_LGL_ENG_NM
      ,TO_CHAR(MAX(WO.LOCL_CRE_DT),'YYYYMMDD') CRE_DT
      ,MAX(VE.WO_EV_GRD_CD) AS WO_EV_GRD_CD
      ,MAX(VE.SHPR_EV_GRD_CD) AS SHPR_EV_GRD_CD
      ,MAX(VE.EV_CTNT) AS EV_CTNT
      ,TO_CHAR(MAX(VE.CRE_DT),'YYYYMMDD') CRE_DT_1
      ,MAX(VE.CRE_USR_ID)  AS CRE_USR_ID
  FROM TRS_TRSP_WRK_ORD      WO
      ,TRS_TRSP_SVC_ORD      SO
      ,TRS_TRSP_VNDR_PERF_EV VE
      ,MDM_VENDOR            MV
      ,TRS_TRSP_INV_WRK      INV
 WHERE 1 = 1
   AND WO.TRSP_WO_OFC_CTY_CD   = SO.TRSP_WO_OFC_CTY_CD
   AND WO.TRSP_WO_SEQ          = SO.TRSP_WO_SEQ
   AND SO.TRSP_SO_STS_CD      IN ('I' ,'E')
   AND SO.INV_NO               = INV.INV_NO
   AND SO.INV_VNDR_SEQ         = INV.INV_VNDR_SEQ
   AND WO.WO_VNDR_SEQ          = MV.VNDR_SEQ(+)
   AND WO.TRSP_WO_OFC_CTY_CD   = VE.TRSP_WO_OFC_CTY_CD(+)
   AND WO.TRSP_WO_SEQ          = VE.TRSP_WO_SEQ(+)
   AND INV.TRSP_INV_AUD_STS_CD = 'IF'

#if ($tempWoNo.size() > 0)
   AND   WO.TRSP_WO_OFC_CTY_CD||WO.TRSP_WO_SEQ  IN (
#foreach($tempWoNokey IN ${tempWoNo})
	#if($velocityCount < $tempWoNo.size())
		'$tempWoNokey',
	#else
		'$tempWoNokey'
	#end
#end
  )
#end

#if (${r_period} == 'I')
	#if ((${r_from_date} != '') && (${r_to_date} != ''))
        AND WO.LOCL_CRE_DT BETWEEN TO_DATE(@[hid_from_date],'YYYYMMDD') AND TO_DATE(@[hid_to_date],'YYYYMMDD')+0.99999
	#end
#elseif(${r_period} == 'E')
	#if ((${r_from_date} != '') && (${r_to_date} != ''))
        AND VE.UPD_DT BETWEEN TO_DATE(@[hid_from_date],'YYYYMMDD') AND TO_DATE(@[hid_to_date],'YYYYMMDD')+0.99999
	#end
#end

#if ((${r_provider} != '') && (${r_to_date} != 'ALL'))
		AND WO.WO_VNDR_SEQ = @[hid_provider]
#end

GROUP BY WO.TRSP_WO_OFC_CTY_CD
        ,WO.TRSP_WO_SEQ
UNION ALL
SELECT EDI.TRSP_SO_OFC_CTY_CD||EDI.TRSP_SO_SEQ AS TRS_WO_KEY
      ,EDI.TRSP_SO_OFC_CTY_CD
      ,EDI.TRSP_SO_SEQ
      ,EDI.VNDR_SEQ
      ,MV.VNDR_LGL_ENG_NM
      ,TO_CHAR(EDI.LOCL_CRE_DT,'YYYYMMDD') CRE_DT
      ,VE.WO_EV_GRD_CD
      ,VE.SHPR_EV_GRD_CD
      ,VE.EV_CTNT
      ,TO_CHAR(VE.CRE_DT,'YYYYMMDD') CRE_DT_1
      ,VE.CRE_USR_ID
  FROM TRS_TRSP_VNDR_PERF_EV VE
      ,MDM_VENDOR MV
      ,TRS_TRSP_EDI_RAIL_ORD EDI          
      ,(SELECT VNDR_S.TRSP_SO_OFC_CTY_CD
              ,VNDR_S.TRSP_SO_SEQ
              ,MAX(EDI_S.BIL_ISS_KNT) BIL_ISS_KNT
          FROM TRS_TRSP_EDI_RAIL_ORD       EDI_S
              ,TRS_TRSP_RAIL_BIL_VNDR_SET  VNDR_S
              ,TRS_TRSP_RAIL_BIL_ORD       ORD_S
              ,TRS_TRSP_VNDR_PERF_EV       VE_S
              ,TRS_TRSP_RAIL_INV_WRK       INV_S
         WHERE EDI_S.TRSP_SO_OFC_CTY_CD    = VNDR_S.TRSP_SO_OFC_CTY_CD
           AND EDI_S.TRSP_SO_SEQ           = VNDR_S.TRSP_SO_SEQ
           AND VNDR_S.TRSP_SO_OFC_CTY_CD   = ORD_S.TRSP_SO_OFC_CTY_CD
           AND VNDR_S.TRSP_SO_SEQ          = ORD_S.TRSP_SO_SEQ    
           AND VNDR_S.INV_NO               = INV_S.INV_NO
           AND VNDR_S.INV_VNDR_SEQ         = INV_S.INV_VNDR_SEQ           
           AND VNDR_S.TRSP_SO_OFC_CTY_CD   = VE_S.TRSP_WO_OFC_CTY_CD(+)
           AND VNDR_S.TRSP_SO_SEQ          = VE_S.TRSP_WO_SEQ(+)
           AND ORD_S.TRSP_SO_STS_CD       IN ('I','E')
           AND INV_S.TRSP_INV_AUD_STS_CD   = 'IF'
           
           #if (${r_wonumber} != '')
               AND  1 = 2
           #end

           #if (${r_period} == 'I')
               #if ((${r_from_date} != '') && (${r_to_date} != ''))
                   AND EDI_S.LOCL_CRE_DT BETWEEN TO_DATE(@[hid_from_date],'YYYYMMDD') AND TO_DATE(@[hid_to_date],'YYYYMMDD')+0.99999
               #end
           #elseif(${r_period} == 'E')
               #if ((${r_from_date} != '') && (${r_to_date} != ''))
                   AND VE_S.UPD_DT BETWEEN TO_DATE(@[hid_from_date],'YYYYMMDD') AND TO_DATE(@[hid_to_date],'YYYYMMDD')+0.99999
               #end
           #end

           #if ((${r_provider} != '') && (${r_to_date} != 'ALL'))
               AND VNDR_S.VNDR_SEQ = @[hid_provider]
           #end
         GROUP BY VNDR_S.TRSP_SO_OFC_CTY_CD, VNDR_S.TRSP_SO_SEQ 
       ) X
 WHERE 1 = 1
   AND EDI.VNDR_SEQ             = MV.VNDR_SEQ(+)
   AND EDI.TRSP_SO_OFC_CTY_CD   = VE.TRSP_WO_OFC_CTY_CD(+)
   AND EDI.TRSP_SO_SEQ          = VE.TRSP_WO_SEQ(+)
   AND EDI.TRSP_SO_OFC_CTY_CD   = X.TRSP_SO_OFC_CTY_CD
   AND EDI.TRSP_SO_SEQ          = X.TRSP_SO_SEQ
   AND EDI.BIL_ISS_KNT          = X.BIL_ISS_KNT
   #if (${r_wonumber} != '')
      AND  1 = 2
   #end
 ORDER BY TRSP_WO_OFC_CTY_CD, TRSP_WO_SEQ			]]></sql>
			<params>
				<param name="hid_from_date" type="12" value="20090404" out="N"/>
				<param name="hid_to_date" type="12" value="20090404" out="N"/>
				<param name="hid_provider" type="12" value="111" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
