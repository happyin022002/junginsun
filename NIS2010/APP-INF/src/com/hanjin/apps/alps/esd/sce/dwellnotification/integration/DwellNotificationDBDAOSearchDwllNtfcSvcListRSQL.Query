<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DwellNotificationDBDAOSearchDwllNtfcSvcListRSQL">
			<desc><![CDATA[email 조회]]></desc>
			<sql><![CDATA[
SELECT 
  SC_NO
, CUST_CD
, CTRT_PTY_NM
, PROP_MQC_QTY
, UNIT_NM
, PROP_OFC_CD
, EFF_DT
, EXP_DT
, EMAIL_LIST_CNT
FROM (
SELECT 
    A.SC_NO
,   SEXP.DWLL_CUST_CNT_CD || LPAD(SEXP.DWLL_CUST_SEQ, 6, '0') AS CUST_CD
,   A.CTRT_PTY_NM
,   A.FNL_MQC_QTY AS PROP_MQC_QTY
,   A.INTG_CD_VAL_DP_DESC UNIT_NM
,	A.PROP_OFC_CD
,   TO_CHAR(CTRT_EFF_DT,'YYYY-MM-DD') EFF_DT
,   TO_CHAR(CTRT_EXP_DT,'YYYY-MM-DD') EXP_DT
,   'Edit (Registered : '|| (SELECT COUNT(1) FROM SCE_DWLL_CUST_SVC_LIST S WHERE S.DWLL_CUST_CNT_CD = SEXP.DWLL_CUST_CNT_CD AND S.DWLL_CUST_SEQ = SEXP.DWLL_CUST_SEQ AND S.DELT_FLG = 'N' ) || ' )' EMAIL_LIST_CNT
FROM 
(
    SELECT
    *
    FROM
    (
        SELECT
            ROW_NUMBER() OVER ( PARTITION BY CUST_CNT_CD, CUST_SEQ ORDER BY HDR.CRE_DT DESC, PTY.AMDT_SEQ DESC) SEL_SEQ
        ,   PTY.PROP_NO
        ,   PTY.AMDT_SEQ
        ,   HDR.SC_NO
        ,   PTY.CUST_CNT_CD
        ,   PTY.CUST_SEQ
        ,   PTY.CTRT_PTY_NM
        ,   PTY.PRC_CTRT_PTY_TP_CD
		, MN.PROP_OFC_CD
        , CTRT_EFF_DT
        , CTRT_EXP_DT
        , MQC.FNL_MQC_QTY
        ,   UNIT.INTG_CD_VAL_DP_DESC
        FROM 
            PRI_SP_CTRT_PTY PTY
        ,   PRI_SP_HDR HDR
        ,   PRI_SP_DUR DUR
        ,   PRI_SP_MQC MQC
		,   PRI_SP_MN MN
        ,   (SELECT INTG_CD_VAL_CTNT, INTG_CD_VAL_DP_DESC from COM_INTG_CD_DTL WHERE INTG_CD_ID = 'CD00897' ) UNIT
        WHERE 1=1
        AND PTY.PRC_CTRT_PTY_TP_CD='C'
        AND SUBSTR(HDR.SC_NO, 1,3) IN ('AEF', 'AEN', 'ANW','ASE','AWE','ASW', 'AWN', 'GLO', 'SAN', 'SAS', 'CEN','ANW','CEF','MME')
        AND HDR.PROP_NO = PTY.PROP_NO    
        AND PTY.PROP_NO = DUR.PROP_NO
        AND PTY.AMDT_SEQ = DUR.AMDT_SEQ
		AND PTY.PROP_NO = MN.PROP_NO
		AND PTY.AMDT_SEQ = MN.AMDT_SEQ
        AND MQC.PROP_NO = DUR.PROP_NO
        AND MQC.AMDT_SEQ = DUR.AMDT_SEQ
        AND MQC.CNTR_LOD_UT_CD = UNIT.INTG_CD_VAL_CTNT
            #if($scpfxcdArr.size() > 0)
    AND ( 
         #foreach( ${key} in ${scpfxcdArr}) 
             #if($velocityCount < $scpfxcdArr.size()) 
             HDR.SC_NO LIKE '$key' || @[sc_no] || DECODE(@[mtch_flg], 'Y', '', '%') OR
             #else
             HDR.SC_NO LIKE '$key' || @[sc_no] || DECODE(@[mtch_flg], 'Y', '', '%')
             #end
         #end
        )
        #end
        #if (${ctrt_exp_dt} != '')
        AND DUR.CTRT_EXP_DT >= TO_DATE(@[ctrt_exp_dt], 'yyyy-MM-dd')
        #end 
        #if (${ctrt_eff_dt} != '') 
        AND DUR.CTRT_EFF_DT <= TO_DATE(@[ctrt_eff_dt], 'yyyy-MM-DD')
        #end
		
    )
    WHERE SEL_SEQ=1
    ) A
,   SCE_DWLL_NTFC_CUST_EXPT SEXP
WHERE 1=1
AND SEXP.DWLL_CUST_CNT_CD = A.CUST_CNT_CD
    AND SEXP.DWLL_CUST_SEQ = A.CUST_SEQ
	AND SEXP.DELT_FLG = 'N'
    #if (${cust_cnt_cd} != '' )
    AND SEXP.DWLL_CUST_CNT_CD LIKE '%'||@[cust_cnt_cd]||'%'
    #end
    #if (${cust_seq} != '' )
    AND SEXP.DWLL_CUST_SEQ LIKE '%'||TO_NUMBER(@[cust_seq])||'%'
    #end
UNION
    SELECT
        '' SC_NO
    ,   SEXP.DWLL_CUST_CNT_CD || LPAD(SEXP.DWLL_CUST_SEQ, 6, '0') AS CUST_CD
    ,   CUST_LGL_ENG_NM AS CTRT_PTY_NM
    ,   0 PROP_MQC_QTY
    ,   '' UNIT_NM
    ,   '' PROP_OFC_CD
    ,   '' EFF_DT
    ,   '' EXP_DT
    ,   'Edit (Registered : '|| (SELECT COUNT(1) FROM SCE_DWLL_CUST_SVC_LIST S WHERE S.DWLL_CUST_CNT_CD = SEXP.DWLL_CUST_CNT_CD AND S.DWLL_CUST_SEQ = SEXP.DWLL_CUST_SEQ AND S.DELT_FLG = 'N' ) || ' )' EMAIL_LIST_CNT
    FROM SCE_DWLL_NTFC_CUST_EXPT SEXP, MDM_CUSTOMER CUST
    WHERE 1=1
    AND SEXP.DELT_FLG = 'N'
	AND SEXP.DWLL_CUST_CNT_CD = CUST.CUST_CNT_CD
	AND SEXP.DWLL_CUST_SEQ = CUST.CUST_SEQ
    #if (${cust_cnt_cd} != '' )
    AND SEXP.DWLL_CUST_CNT_CD LIKE '%'||@[cust_cnt_cd]||'%'
    #end
    #if (${cust_seq} != '' )
    AND SEXP.DWLL_CUST_SEQ LIKE '%'||TO_NUMBER(@[cust_seq])||'%'
    #end
    AND NOT EXISTS
    (
            SELECT
               'X'
            FROM 
                PRI_SP_CTRT_PTY PTY
            ,   PRI_SP_HDR HDR
            ,   PRI_SP_DUR DUR
            ,   PRI_SP_MQC MQC
                    ,   PRI_SP_MN MN
            WHERE 1=1
            AND PTY.PRC_CTRT_PTY_TP_CD='C'
            AND SUBSTR(HDR.SC_NO, 1,3) IN ('AEF', 'AEN', 'ANW','ASE','AWE','ASW', 'AWN', 'GLO', 'SAN', 'SAS', 'CEN','ANW','CEF','MME')
            AND HDR.PROP_NO = PTY.PROP_NO    
            AND PTY.PROP_NO = DUR.PROP_NO
            AND PTY.AMDT_SEQ = DUR.AMDT_SEQ
                    AND PTY.PROP_NO = MN.PROP_NO
                    AND PTY.AMDT_SEQ = MN.AMDT_SEQ
            AND MQC.PROP_NO = DUR.PROP_NO
            AND MQC.AMDT_SEQ = DUR.AMDT_SEQ
            AND SEXP.DWLL_CUST_CNT_CD = PTY.CUST_CNT_CD
            AND SEXP.DWLL_CUST_SEQ = PTY.CUST_SEQ
                       #if($scpfxcdArr.size() > 0)
    AND ( 
         #foreach( ${key} in ${scpfxcdArr}) 
             #if($velocityCount < $scpfxcdArr.size()) 
             HDR.SC_NO LIKE '$key' || @[sc_no] || DECODE(@[mtch_flg], 'Y', '', '%') OR
             #else
             HDR.SC_NO LIKE '$key' || @[sc_no] || DECODE(@[mtch_flg], 'Y', '', '%')
             #end
         #end
        )
        #end
            )
)




ORDER BY SC_NO, CUST_CD			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="mtch_flg" type="12" value="" out="N"/>
				<param name="ctrt_exp_dt" type="12" value="" out="N"/>
				<param name="ctrt_eff_dt" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
