			,SEP_CNTR_VOL_QTY = @[sep_cntr_vol_qty]
			,OCT_CNTR_VOL_QTY = @[oct_cntr_vol_qty]
			,NOV_CNTR_VOL_QTY = @[nov_cntr_vol_qty]
			,DEC_CNTR_VOL_QTY = @[dec_cntr_vol_qty]
			,TTL_CNTR_VOL_QTY = @[ttl_cntr_vol_qty]
			,ESTM_CNTR_VOL_QTY = @[estm_cntr_vol_qty]
			,CURR_CD = @[curr_cd]
			,CNTR_UT_INCNT_AMT = @[cntr_ut_incnt_amt]
