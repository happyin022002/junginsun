			,DEC_CNTR_VOL_QTY
			,TTL_CNTR_VOL_QTY
			,ESTM_CNTR_VOL_QTY
			,CURR_CD
			,CNTR_UT_INCNT_AMT
			,JAN_INCNT_AMT
			,FEB_INCNT_AMT
			,MAR_INCNT_AMT
			,APR_INCNT_AMT
			,MAY_INCNT_AMT
			,JUN_INCNT_AMT
			,JUL_INCNT_AMT
			,AUG_INCNT_AMT
			,SEP_INCNT_AMT
			,OCT_INCNT_AMT
			,NOV_INCNT_AMT
			,DEC_INCNT_AMT
			,TTL_INCNT_AMT
			,TTL_RCV_AMT
			,TTL_RMN_AMT
