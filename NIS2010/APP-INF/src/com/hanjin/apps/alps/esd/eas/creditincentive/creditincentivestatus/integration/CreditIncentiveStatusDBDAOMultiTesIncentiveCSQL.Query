			,EFF_TO_DT = TO_DATE(@[eff_to_dt],'YYYYMMDD')
			,INV_CYC_CD = @[inv_cyc_cd]
			,INV_ISS_DT_RMK = @[inv_iss_dt_rmk]
			,FULL_MTY_CD = @[full_mty_cd] 
