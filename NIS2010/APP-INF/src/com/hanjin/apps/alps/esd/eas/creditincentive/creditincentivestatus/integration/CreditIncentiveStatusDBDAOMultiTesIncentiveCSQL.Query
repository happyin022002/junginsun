			,@[oct_cntr_vol_qty]
			,@[nov_cntr_vol_qty]
			,@[dec_cntr_vol_qty]
			,@[ttl_cntr_vol_qty]
			,@[estm_cntr_vol_qty]
			,@[curr_cd]
			,@[cntr_ut_incnt_amt]
			,@[jan_incnt_amt]
			,@[feb_incnt_amt]
			,@[mar_incnt_amt]
			,@[apr_incnt_amt]
			,@[may_incnt_amt]
			,@[jun_incnt_amt]
			,@[jul_incnt_amt]
			,@[aug_incnt_amt]
			,@[sep_incnt_amt]
			,@[oct_incnt_amt]
			,@[nov_incnt_amt]
			,@[dec_incnt_amt]
			,@[ttl_incnt_amt]
			,@[ttl_rcv_amt]
			,@[ttl_rmn_amt]
			,@[ttl_incnt_usd_amt]
			,@[ttl_rcv_usd_amt]
			,@[ttl_rmn_usd_amt]
			,@[rct_dt_rmk]
			,@[incnt_desc]
			,@[incnt_rmk]
			,@[atch_file_lnk_id]
            ,@[atch_n2nd_file_lnk_id]
			,@[usr_id]
			,SYSDATE
			,@[usr_id]
			,SYSDATE
		)			]]></sql>
