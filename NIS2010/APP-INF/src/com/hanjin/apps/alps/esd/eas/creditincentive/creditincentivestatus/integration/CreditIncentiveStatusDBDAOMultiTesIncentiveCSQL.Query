			,APR_INCNT_AMT = @[apr_incnt_amt]
			,MAY_INCNT_AMT = @[may_incnt_amt]
			,JUN_INCNT_AMT = @[jun_incnt_amt]
			,JUL_INCNT_AMT = @[jul_incnt_amt]
			,AUG_INCNT_AMT = @[aug_incnt_amt]
			,SEP_INCNT_AMT = @[sep_incnt_amt]
			,OCT_INCNT_AMT = @[oct_incnt_amt]
			,NOV_INCNT_AMT = @[nov_incnt_amt]
			,DEC_INCNT_AMT = @[dec_incnt_amt]
			,TTL_INCNT_AMT = @[ttl_incnt_amt]
			,TTL_RCV_AMT = @[ttl_rcv_amt]
			,TTL_RMN_AMT = @[ttl_rmn_amt]
