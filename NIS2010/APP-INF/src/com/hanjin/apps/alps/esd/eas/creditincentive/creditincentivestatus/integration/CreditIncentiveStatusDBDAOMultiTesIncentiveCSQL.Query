			,TTL_RCV_USD_AMT = @[ttl_rcv_usd_amt]
			,TTL_RMN_USD_AMT = @[ttl_rmn_usd_amt]
			,RCT_DT_RMK = @[rct_dt_rmk]
			,INCNT_DESC = @[incnt_desc]
			,INCNT_RMK = @[incnt_rmk]
			,ATCH_FILE_LNK_ID = @[atch_file_lnk_id]
            ,ATCH_N2ND_FILE_LNK_ID = @[atch_n2nd_file_lnk_id]
			,UPD_USR_ID = @[usr_id]
			,UPD_DT = SYSDATE
WHEN NOT MATCHED THEN
    INSERT(	 BSE_YR
		    ,INCNT_NO
			,RHQ_CD
			,INV_OFC_CD
			,PORT_CD
			,YD_CD
			,VNDR_SEQ
			,EFF_FM_DT
			,EFF_TO_DT
			,INV_CYC_CD
			,INV_ISS_DT_RMK
			,FULL_MTY_CD
			,LOCL_TS_STS_CD
			,LGS_COST_CD
			,INCNT_UT_CD
			,JAN_CNTR_VOL_QTY
			,FEB_CNTR_VOL_QTY
			,MAR_CNTR_VOL_QTY
			,APR_CNTR_VOL_QTY
			,MAY_CNTR_VOL_QTY
