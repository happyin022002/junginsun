                            WHERE INTG_CD_ID = 'CD01860'
                              AND (INSTR(@[cntr_tpsz], INTG_CD_VAL_CTNT) > 0
                               OR @[cntr_tpsz] = 'ALL'
                                  )
                          )
#end
    GROUP BY
    		TO_CHAR(A.CRE_DT, 'YYYYMM'),
#if( ${f_chkprd} == 'W' )
            A.WEEK,
#end
  		    B.VNDR_SEQ,
#if( ${agmt_chk} == 'Y' )
			X.AGMT_REF_NO,
#end

			A.LANE_CD,
    		A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD,
    		B.FM_YD_CD,
    		B.TO_YD_CD,
            A.DELT_FLG,
    		LOCTO.LOC_CD,
    		LOCTO.EQ_CTRL_OFC_CD,
