            TR.DELT_FLG,
            TR.EQ_TPSZ_CD,
            YY.VNDR_SEQ,
            YY.CURR_CD CURR_CD ,
            YY.INV_CURR_CD,
            YY.ETC_ADD_AMT ,
            YY.HZD_MTRL_SCG_AMT ,
            YY.OVR_WGT_SCG_AMT ,
            YY.FUEL_SCG_AMT ,
            YY.BZC_AMT ,
            YY.INV_BZC_AMT,
            YY.INV_ETC_ADD_AMT,
