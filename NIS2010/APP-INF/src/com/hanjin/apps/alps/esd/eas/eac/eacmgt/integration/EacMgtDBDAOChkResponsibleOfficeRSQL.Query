<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOChkResponsibleOfficeRSQL">
			<desc><![CDATA[Responsible Office 에 값이 존재하는지 체크 한다]]></desc>
			<sql><![CDATA[
SELECT count(1) AS isflag
      ,TRS_COMMON_PKG.TRS_GET_RHQ_OFC_CD(@[respb_ofc_cd]) RHQ_OFC_CD
  FROM MDM_ORGANIZATION
 WHERE DELT_FLG = 'N'
   AND OFC_CD = @[respb_ofc_cd]     -- Office Code
   #if (${n3tpy_if_ofc_flg} == 'Y')	-- TPB Office Y/N
   AND OFC_KND_CD != 1				
   #end			]]></sql>
			<params>
				<param name="respb_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
