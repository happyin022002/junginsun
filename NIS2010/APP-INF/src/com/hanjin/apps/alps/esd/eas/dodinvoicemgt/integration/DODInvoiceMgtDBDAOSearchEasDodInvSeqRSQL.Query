<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DODInvoiceMgtDBDAOSearchEasDodInvSeqRSQL">
			<desc><![CDATA[SearchEasDodInvSeq]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(@[ofc_cd],0,2)||'D'||LPAD(EAS_DOD_INV_SEQ.NEXTVAL, 6, 0) AS DOD_INV_NO
  FROM DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
