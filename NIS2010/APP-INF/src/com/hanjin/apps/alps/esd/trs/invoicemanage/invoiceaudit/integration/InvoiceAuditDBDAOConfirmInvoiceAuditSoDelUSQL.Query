<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceAuditDBDAOConfirmInvoiceAuditSoDelUSQL">
			<desc><![CDATA[Invoice 취소시 취소내역을 S/O 테이블에 반영]]></desc>
			<sql><![CDATA[
UPDATE  TRS_TRSP_SVC_ORD
SET	    TRSP_INV_ACT_STS_CD		= NULL	
      ,	INV_NO					= NULL	
      ,	INV_VNDR_SEQ			= NULL	
      ,	INV_BZC_AMT				= NULL	
      ,	INV_ETC_ADD_AMT			= NULL	
      ,	INV_CURR_CD				= NULL	
      ,	INV_RMK					= NULL	
      ,	TRSP_INV_CALC_LGC_TP_CD	= NULL	
      ,	INV_XCH_RT				= NULL	
      , UPD_DT                  = SYSDATE
      , UPD_USR_ID	            = @[FORM_CRE_USR_ID]
      , LOCL_UPD_DT             = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[FORM_USR_OFC_CD])
WHERE   TRSP_SO_OFC_CTY_CD		= @[trsp_so_ofc_cty_cd]		
AND     TRSP_SO_SEQ			    = @[trsp_so_seq]			]]></sql>
			<params>
				<param name="FORM_CRE_USR_ID" type="12" value="" out="N"/>
				<param name="FORM_USR_OFC_CD" type="12" value="" out="N"/>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
