<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualRegisterDBDAOSearchVndrCntCdRSQL">
			<desc><![CDATA[SearchVndrCntCd]]></desc>
			<sql><![CDATA[
SELECT VNDR_CNT_CD VNDR_CNT_CD
FROM MDM_VENDOR
WHERE VNDR_SEQ = @[s_src_vndr_seq]
AND DELT_FLG = 'N'
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="s_src_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
