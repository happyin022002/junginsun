<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOPutEDIOriginInvoiceCSQL">
			<desc><![CDATA[원본 EDI invoice file 정보를 DB에 입력]]></desc>
			<sql><![CDATA[
INSERT INTO TES_EDI_SO_FILE(
	FILE_SEQ
	, ORG_FILE_NM
	, SAV_FILE_NM
	, SAV_PATH_NM
	, TML_EDI_SO_OFC_CTY_CD
	, TML_EDI_SO_SEQ
	, TML_SO_OFC_CTY_CD
	, TML_SO_SEQ
	, SAV_CFM_FLG
	, DELT_FLG
	, FILE_RMK
	, FILE_SAV_ID
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
	) 
VALUES (
	TES_EDI_SO_FILE_SEQ.NEXTVAL
	, @[org_file_nm]
	, @[sav_file_nm]
	, @[sav_path_nm]
	, @[tml_edi_so_ofc_cty_cd]
	, @[tml_edi_so_seq]
	, @[tml_so_ofc_cty_cd]
	, @[tml_so_seq]
	, @[sav_cfm_flg]
	, 'N'
	, @[file_rmk]
	, @[file_sav_id]
	, @[cre_usr_id]
	, SYSDATE
	, @[upd_usr_id]
	, SYSDATE
	)			]]></sql>
			<params>
				<param name="org_file_nm" type="12" value="" out="N"/>
				<param name="sav_file_nm" type="12" value="" out="N"/>
				<param name="sav_path_nm" type="12" value="" out="N"/>
				<param name="tml_edi_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_edi_so_seq" type="2" value="" out="N"/>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="2" value="" out="N"/>
				<param name="sav_cfm_flg" type="12" value="" out="N"/>
				<param name="file_rmk" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
