<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USARailWoAckManageDBDAOselectUSARailWoAckManageRSQL">
			<desc><![CDATA[TRS_TRSP_EDI_RAIL_ORD 와 TRS_TRSP_RAIL_BIL_ORD 테이블 조인한 데이타]]></desc>
			<sql><![CDATA[
SELECT B.TRSP_RAIL_BIL_TP_CD 
       , A.TRSP_SO_SEQ
  , A.TRSP_SO_OFC_CTY_CD  
FROM TRS_TRSP_EDI_RAIL_ORD A, TRS_TRSP_RAIL_BIL_ORD B
WHERE A.TRSP_SO_SEQ = B.TRSP_SO_SEQ 
AND   A.TRSP_SO_OFC_CTY_CD = B.TRSP_SO_OFC_CTY_CD 
AND   A.BIL_EDI_CTRL_SEQ = TO_NUMBER(@[bil_edi_ctrl_seq])			]]></sql>
			<params>
				<param name="bil_edi_ctrl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
