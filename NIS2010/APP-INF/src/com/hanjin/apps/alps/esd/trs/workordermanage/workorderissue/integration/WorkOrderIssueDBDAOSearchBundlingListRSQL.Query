<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WorkOrderIssueDBDAOSearchBundlingListRSQL">
			<desc><![CDATA[SearchBundlingList]]></desc>
			<sql><![CDATA[
SELECT '' IBCHECK,
  A.MCNTR_BDL_GRP_SEQ,
  A.MCNTR_BDL_SEQ,
  A.EQ_NO,
  A.EQ_TPSZ_CD,
  DECODE(NVL(A.MCNTR_BDL_SEQ,'N'),'N','N','Bundled') BUNDL_STS,
  A.TRSP_CRR_MOD_CD,
  A.FM_NOD_CD,
  A.TO_NOD_CD,
  A.TRSP_SO_OFC_CTY_CD, 
  A.TRSP_SO_SEQ,
  A.MTY_BDL_CNTR_QTY
FROM TRS_TRSP_SVC_ORD A
WHERE 1=1
#if($so_no_a.size() > 0)  
AND (A.TRSP_SO_OFC_CTY_CD,A.TRSP_SO_SEQ) IN (
	#foreach($code IN ${so_no_a})  
		#if($velocityCount == 1)  
 			(SUBSTR('$code', 1, 3),SUBSTR('$code', 4))
		#else  
		   ,(SUBSTR('$code', 1, 3),SUBSTR('$code', 4))  
		#end  
	#end
)
#end 

#if($so_no_a.size() < 1)  
 AND 1=2
#end
ORDER BY A.EQ_TPSZ_CD, A.EQ_NO			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
