<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOSearchCostCodeInfoRSQL">
			<desc><![CDATA[SearchCostCodeInfo]]></desc>
			<sql><![CDATA[
SELECT
  LGS_COST_CD
  , LGS_COST_FULL_NM
  , LGS_COST_SUBJ_CD
  , LGS_COST_DTL_CD
  , LGS_COST_CD_CLSS_LVL
  , LGS_COST_OPT_NO
  , LGS_COST_ABBR_NM
  , ACCT_CD
  , LGS_COST_RMK
  , THRP_FLG
  , CRR_ACCT_CD
  , DELT_FLG
  , CRE_USR_ID
  , TO_CHAR(CRE_DT, 'YYYY-MM-DD') CRE_DT
  , UPD_USR_ID
  , TO_CHAR(UPD_DT, 'YYYY-MM-DD') UPD_DT
FROM TES_LGS_COST
WHERE LGS_COST_CD = @[lgs_cost_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="lgs_cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
