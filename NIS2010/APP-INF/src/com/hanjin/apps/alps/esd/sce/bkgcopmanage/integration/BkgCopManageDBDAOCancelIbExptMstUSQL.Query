<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOCancelIbExptMstUSQL">
			<desc><![CDATA[Inbound 의 Exception 정보를 cancel 하고 notify 대상에서 제외한다.]]></desc>
			<sql><![CDATA[
UPDATE SCE_EXPT_MST 
SET    COP_EXPT_STS_CD = 'X'  
,NTFD_FLG        = 'N'
,UPD_USR_ID = 'resolveTRO/I'    
,UPD_DT = SYSDATE 
WHERE  COP_NO = @[cop_no]
AND    COP_DTL_SEQ >= 6000
AND    COP_EXPT_STS_CD in ('O','R')			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
