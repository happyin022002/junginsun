<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGenEventDtFFFormRSQL">
			<desc><![CDATA[GenEventDtFFForm]]></desc>
			<sql><![CDATA[
select DECODE(SUBSTR(@[event_yard], 1, 5), NULL, '', 
            DECODE(TO_DATE(@[event_dt], 'YYYYMMDDHH24MISS'), NULL, '', 
                    TO_CHAR(GLOBALDATE_PKG.TIME_CONV_FNC(SUBSTR (@[event_yard],1,5)
                                                        ,TO_DATE(@[event_dt]
                                                        ,'RRRRMMDDHH24MISS')
                                                        ,'GMT'),'YYYYMMDDHH24MI'))) FF_EVENT_DT_GMT
      ,substr(@[event_dt],1,12) FF_EVENT_DT
from dual			]]></sql>
			<params>
				<param name="event_yard" type="12" value="" out="N"/>
				<param name="event_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
