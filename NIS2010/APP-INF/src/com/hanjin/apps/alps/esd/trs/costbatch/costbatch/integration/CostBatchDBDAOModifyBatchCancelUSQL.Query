<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CostBatchDBDAOModifyBatchCancelUSQL">
			<desc><![CDATA[2012.06.04 변종건 [CHM-201217633] Inland Cost Batch Creation - Batch 취소 기능 추가]]></desc>
			<sql><![CDATA[
UPDATE  TRS_COST_TRF_BAT
SET     BAT_PROG_STS_CD     = 'X'
      , UPD_USR_ID          = @[upd_usr_id]
      , UPD_DT              = SYSDATE
WHERE   COST_TRF_BAT_SEQ    = @[cost_trf_bat_seq]
AND     BAT_PROG_STS_CD     = 'W'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_trf_bat_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
