<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="COPUpdateDBDAOSearchCOPDetailActDtRSQL">
			<desc><![CDATA[SearchCOPDetailActDt]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(ACT_DT, 'YYYYMMDD') ACT_DT
  FROM SCE_COP_DTL
 WHERE cop_no = @[cop_no]
   AND COP_DTL_SEQ = @[cop_dtl_seq]			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
