<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchEacStatusRSQL">
			<desc><![CDATA[EAC Status 콤보 조회]]></desc>
			<sql><![CDATA[
SELECT A.EAC_STS_CD AS DIS_EAC_STS_CD
      ,B.INTG_CD_VAL_DP_DESC AS DIS_EAC_STS_NM
      ,C.USR_NM AS APRO_USR_NM
  FROM EAS_EXPN_AUD_CS_APRO_STEP A
      ,COM_INTG_CD_DTL B
      ,COM_USER C
 WHERE B.INTG_CD_ID = 'CD03337'
   AND A.EAC_STS_CD = B.INTG_CD_VAL_CTNT
   AND A.APRO_USR_ID = C.USR_ID(+)
   AND EAC_NO = @[eac_no]
ORDER BY A.APRO_STEP_SEQ			]]></sql>
			<params>
				<param name="eac_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
