<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifySceCopDetailEstmDtToVLByOceanUSQL">
			<desc><![CDATA[ModifySceCopDetailEstmDtToVLByOcean]]></desc>
			<sql><![CDATA[
update sce_cop_dtl 
set    estm_dt     = estm_dt + @[rcv_evnt_gap_desc] 
      ,estm_gdt    = globaldate_pkg.time_conv_fnc(substr(nod_cd,1,5), estm_dt + + @[rcv_evnt_gap_desc], 'GMT')  
      ,upd_usr_id  = 'VPS_TS' 
      ,upd_dt      = sysdate 
where  cop_no      = @[cop_no] 
and    (cop_no||cop_dtl_seq) > (@[cop_no]||@[fm_cop_dtl_seq]) 
and    (cop_no||cop_dtl_seq) < @[trnk_cop]
			]]></sql>
			<params>
				<param name="rcv_evnt_gap_desc" type="12" value="" out="N"/>
				<param name="cop_no" type="12" value="" out="N"/>
				<param name="fm_cop_dtl_seq" type="12" value="" out="N"/>
				<param name="trnk_cop" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
