<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CanadaCCTManageDBDAOSearchPolNodeRSQL">
			<desc><![CDATA[CanadaCCTManageDBDAOSearchPolNodeRSQL]]></desc>
			<sql><![CDATA[
select   P.LOC_CD
		,P.SCC_CD
		,P.LOC_NM
		,P.RGN_CD
		,P.CNT_CD
		,P.STE_CD
		,P.CONTI_CD
		,P.PORT_INLND_CD
		,P.LOC_CHR_CD
		,P.BLK_PORT_FLG
		,P.HUB_LOC_CD
		,P.SLS_OFC_CD
		,P.LOC_GRD_NO
		,P.GMT_HRS
		,P.BKG_BL_PFX_CD
		,P.CALL_PORT_FLG
		,P.LOC_AMS_PORT_CD
		,P.FINC_CTRL_OFC_CD
		,P.EQ_CTRL_OFC_CD
		,P.MTY_PKUP_YD_CD
		,P.SEN_EQ_OFC_CD
		,P.EQ_RTN_YD_CD
		,P.UN_LOC_IND_CD
		,P.UN_LOC_CD
		,P.CML_ZN_FLG
		,P.CSTMS_CD
		,P.LOC_TP_CD
		,P.BFR_FINC_CTRL_OFC_CD
		,P.BFR_EQ_CTRL_OFC_CD
		,P.BFR_SLS_OFC_CD
		,P.BFR_OFC_CNG_DT
		,P.REP_ZN_CD
		,P.ZIP_CD
		,P.SCONTI_CD
		,P.EXPT_LGS_OFC_CD
		,P.EXPT_CUST_SVC_OFC_CD
		,P.VOP_PORT_RHQ_CD
		,P.VOP_PORT_CTRL_OFC_CD
		,P.VOP_PORT_MNTR_FLG
		,P.VOP_LOC_URL
		,P.VOP_PORT_FLG
		,P.VOP_BNK_PORT_FLG
		,P.LOC_LOCL_LANG_NM
		,P.CRE_USR_ID
		,P.CRE_DT
		,P.UPD_USR_ID
		,P.UPD_DT
		,P.DELT_FLG
		,P.EAI_EVNT_DT
		,P.VSKD_PORT_RHQ_CD
		,P.LOC_LAT
		,P.LAT_UT_CD
		,P.LOC_LON
		,P.LON_UT_CD
		,P.EAI_IF_ID
from prd_node n, mdm_location p
where n.nod_cd= @[loc_cd]
and p.loc_cd = substr(n.nod_cd,1,5)
AND P.CNT_CD IN ('CA', 'US')  -- POL 도 US 가능 20130930 --POR 은 US,CA/ POL 은 CA
and nvl(n.delt_flg,'N') = 'N'
and nvl(p.delt_flg,'N') = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
