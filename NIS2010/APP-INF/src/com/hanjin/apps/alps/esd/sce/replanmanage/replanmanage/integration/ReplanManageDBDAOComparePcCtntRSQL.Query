<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOComparePcCtntRSQL">
			<desc><![CDATA[P/C 의 주요 내역을 비교하여 diff 를 찾는다. diff 발생시에만 booking replan 이 수행된다.]]></desc>
			<sql><![CDATA[
SELECT PCTL_SEQ,
  ORG_NOD_CD,
  DEST_NOD_CD,
  NOD_LNK_DIV_CD,
  PCTL_IO_BND_CD,
  TRSP_MOD_CD,
  ARR_ST_DT,
  DEP_FSH_DT,
  VSL_CD,
  SKD_VOY_NO,
  SKD_DIR_CD
FROM PRD_PROD_CTL_ROUT_DTL
WHERE PCTL_NO = @[old_pctl_no]
MINUS
SELECT PCTL_SEQ,
  ORG_NOD_CD,
  DEST_NOD_CD,
  NOD_LNK_DIV_CD,
  PCTL_IO_BND_CD,
  TRSP_MOD_CD,
  ARR_ST_DT,
  DEP_FSH_DT,
  VSL_CD,
  SKD_VOY_NO,
  SKD_DIR_CD
FROM PRD_PROD_CTL_ROUT_DTL
WHERE PCTL_NO = @[crnt_pctl_no]			]]></sql>
			<params>
				<param name="old_pctl_no" type="12" value="" out="N"/>
				<param name="crnt_pctl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
