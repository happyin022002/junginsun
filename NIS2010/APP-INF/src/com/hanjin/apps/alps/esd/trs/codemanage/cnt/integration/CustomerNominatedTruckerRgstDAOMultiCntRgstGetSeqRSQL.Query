<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerNominatedTruckerRgstDAOMultiCntRgstGetSeqRSQL">
			<desc><![CDATA[Approval No 채번]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(@[cre_ofc_cd],1,3) -- 세션 오피스 코드
     ||TO_CHAR(SYSDATE, 'YYMMDD')
     ||NVL((SELECT LPAD(MAX(SUBSTR(APRO_NO, 10))+1, 4, '0')
              FROM TRS_CUST_NOMI_TRKR
             WHERE APRO_NO LIKE SUBSTR(@[cre_ofc_cd],1, 3)||TO_CHAR(SYSDATE, 'YYMMDD') || '%'
               AND LENGTH(APRO_NO) >= 12 -- 12자리에서 한자리증가.
     ), '0001') APRO_NO
 FROM DUAL			]]></sql>
			<params>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
