<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOModifyOfficeSingleTransportationVoPrdUSQL">
			<desc><![CDATA[S/O Trans Office PRD Update]]></desc>
			<sql><![CDATA[
UPDATE PRD_PROD_CTL_ACT_GRP_DTL
   SET CTRL_OFC_CD = @[ctrl_ofc_cd]
 WHERE PCTL_NO         = (
       SELECT PCTL_NO 
         FROM SCE_COP_HDR
        WHERE COP_NO   = @[cop_no]
       )
  AND COST_ACT_GRP_SEQ = @[cost_act_grp_seq]			]]></sql>
			<params>
				<param name="ctrl_ofc_cd" type="12" value="1" out="N"/>
				<param name="cop_no" type="12" value="1" out="N"/>
				<param name="cost_act_grp_seq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
