<sqls>
	<querys>
		<query name="DefaultCurrDBDAOSearchRHQComboRSQL">
			<desc><![CDATA[searchRHQCombo]]></desc>
			<sql><![CDATA[
SELECT B.INTG_CD_VAL_CTNT     RHQ_CD
      ,B.INTG_CD_VAL_DP_DESC  RHQ_NM
  FROM COM_INTG_CD A
      ,COM_INTG_CD_DTL B
 WHERE A.INTG_CD_ID = B.INTG_CD_ID
   AND A.INTG_CD_USE_FLG = 'Y'
   AND A.INTG_CD_ID = 'CD00961'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
