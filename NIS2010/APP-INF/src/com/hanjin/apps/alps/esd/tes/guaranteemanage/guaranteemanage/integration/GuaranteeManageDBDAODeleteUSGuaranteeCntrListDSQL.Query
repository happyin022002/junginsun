<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeManageDBDAODeleteUSGuaranteeCntrListDSQL">
			<desc><![CDATA[Guarantee CNTR 목록 삭제]]></desc>
			<sql><![CDATA[
DELETE	FROM	TES_GNTE_CNTR_LIST
WHERE	1	= 1
AND		GNTE_NO	= @[gnte_no]
AND		TML_GNTE_CNTR_LIST_SEQ	= @[tml_gnte_cntr_list_seq]
AND		CNTR_NO	= @[cntr_no]
			]]></sql>
			<params>
				<param name="gnte_no" type="12" value="" out="N"/>
				<param name="tml_gnte_cntr_list_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
