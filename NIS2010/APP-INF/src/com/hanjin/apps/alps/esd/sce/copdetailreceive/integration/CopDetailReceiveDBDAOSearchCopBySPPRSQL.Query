<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchCopBySPPRSQL">
			<desc><![CDATA[SearchCopBySPP]]></desc>
			<sql><![CDATA[
SELECT cop_no
    FROM   SCE_COP_HDR
    WHERE  cntr_no = @[in_cntr_no]
    and     bkg_no = @[in_bkg_no] 
    AND     cop_sts_cd IN ('C', 'T', 'F')
    AND     ROWNUM = 1			]]></sql>
			<params>
				<param name="in_cntr_no" type="12" value="" out="N"/>
				<param name="in_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
