<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AwkwardCargoCommonDBDAOLogAwkCgoCommErrMsgCSQL">
			<desc><![CDATA[공통 ERR LOG]]></desc>
			<sql><![CDATA[
INSERT INTO TES_AWK_CGO_ERR_LOG (
TML_AWK_CGO_ERR_LOG_SEQ
, TML_AWK_CGO_ERR_TP_CD
, ERR_LOG_RMK
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
) VALUES (
TES_AWK_CGO_ERR_LOG_SEQ.NEXTVAL 
, DECODE(@[tml_awk_cgo_err_tp_cd],NULL,'EC','','EC',@[tml_awk_cgo_err_tp_cd]) --TML_AWK_CGO_ERR_TP_CD
, SUBSTRB(@[err_log_rmk],1,1000) --ERR_LOG_RMK    
, 'SYSTEM' --CRE_USR_ID     
, SYSDATE --CRE_DT         
, 'SYSTEM' --UPD_USR_ID     
, SYSDATE --UPD_DT        
)			]]></sql>
			<params>
				<param name="tml_awk_cgo_err_tp_cd" type="12" value="" out="N"/>
				<param name="err_log_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
