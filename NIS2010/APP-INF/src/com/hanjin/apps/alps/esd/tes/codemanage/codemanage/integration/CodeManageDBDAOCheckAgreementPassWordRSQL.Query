<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOCheckAgreementPassWordRSQL">
			<desc><![CDATA[Agreement Password Check]]></desc>
			<sql><![CDATA[
SELECT               
		@[usr_id]		usr_id           
		, @[usr_pwd]	usr_pwd          
FROM	COM_USER        
--WHERE	'WeWorld' = [usr_pwd]  
--Register Password 변경
WHERE	'smline' = @[usr_pwd]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="usr_pwd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
