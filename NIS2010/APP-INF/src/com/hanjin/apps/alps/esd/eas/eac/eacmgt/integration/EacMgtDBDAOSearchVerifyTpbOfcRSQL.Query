<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOSearchVerifyTpbOfcRSQL">
			<desc><![CDATA[Responsible Office 가 TPB 에 등록된 office 인지 확인한다. 이미 I/F한 EAC_NO 인지 확인한다.]]></desc>
			<sql><![CDATA[
SELECT TPB_GET_N3PTY_OFC_CD_FNC(@[respb_ofc_cd]) AS OFC_CD
     , N3PTY_NO
FROM   EAS_EXPN_AUD_CS_N3RD_PTY
WHERE  EAC_NO = @[eac_no]

			]]></sql>
			<params>
				<param name="respb_ofc_cd" type="12" value="" out="N"/>
				<param name="eac_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
