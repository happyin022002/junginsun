<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifyOdSoByNewVOUSQL">
			<desc><![CDATA[TRS_TRSP_SVC_ORD 의 TRO_SEQ, BKG_NO 를  combine 결과로 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD 
SET 
	TRO_SEQ = @[new_tro_seq], BKG_NO = @[new_bkg_no], BL_NO = (SELECT BL_NO FROM BKG_BOOKING WHERE BKG_NO=@[new_bkg_no])
WHERE BKG_NO = @[org_bkg_no] AND TRO_SEQ = @[org_tro_seq]
			]]></sql>
			<params>
				<param name="new_tro_seq" type="12" value="" out="N"/>
				<param name="new_bkg_no" type="12" value="" out="N"/>
				<param name="org_bkg_no" type="12" value="" out="N"/>
				<param name="org_tro_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
