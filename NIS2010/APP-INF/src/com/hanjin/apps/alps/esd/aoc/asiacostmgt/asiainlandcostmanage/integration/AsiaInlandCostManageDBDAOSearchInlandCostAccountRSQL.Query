<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AsiaInlandCostManageDBDAOSearchInlandCostAccountRSQL">
			<desc><![CDATA[2012.05.22 변종건 [CHM-201217633] Inland Cost Management - Cost Detail]]></desc>
			<sql><![CDATA[
SELECT  A.COST_TRF_NO                                                           AS TRF_NO
      , A.COST_TRF_ROUT_SEQ                                                     AS TRF_ROUT_SEQ
      , A.COST_TRF_ROUT_DESC                                                    AS TRF_ROUT_DESC
      , MAS_GET_COM_NM_FNC('cost_act_grp_cd', A.COST_ACT_GRP_CD)                AS ACT_GRP
      , A.STND_COST_CD                                                          AS STND_COST_CD
      , V.STND_COST_NM                                                          AS STND_COST_NM
      , A.COA_COST_SRC_CD                                                       AS SRC_CD
      , MAS_GET_COM_NM_FNC('mas_cost_src_cd', A.COA_COST_SRC_CD)                AS SRC_NM  
      , A.COST_ACT_GRP_CD                                                       AS ACT_GRP_CD
      , A.INLND_20FT_COST_TRF_AMT                                               AS TRF_AMT_20FT
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', INLND_20FT_COST_SYS_SRC_CD) AS SYS_SRC_CD_20FT
      , A.INLND_20FT_COST_CALC_RMK                                              AS CALC_RMK_20FT
      , A.INLND_40FT_COST_TRF_AMT                                               AS TRF_AMT_40FT
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03050', INLND_40FT_COST_SYS_SRC_CD) AS SYS_SRC_CD_40FT
      , A.INLND_40FT_COST_CALC_RMK                                              AS CALC_RMK_40FT
FROM    AOC_CHN_INLND_TRF_ACCT	A
      , MAS_STND_ACCT_V			V
WHERE   1 = 1
AND     A.STND_COST_CD      = V.STND_COST_CD
AND     V.PA_VW             = 'BKG'
AND     A.COST_TRF_NO       = @[trf_no]
AND     A.COST_TRF_ROUT_SEQ = @[trf_rout_seq]
ORDER BY COST_ACT_GRP_SEQ, COA_COST_SRC_CD			]]></sql>
			<params>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="trf_rout_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
