<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvOfcAtrtMgmtDBDAOSearchOfficeCodeRSQL">
			<desc><![CDATA[Invoice Office Code 입력시 Office  Code 의 이름을 가져오는 쿼리]]></desc>
			<sql><![CDATA[
SELECT OFC_CD AS OFC_CD
      ,OFC_ENG_NM AS OFC_ENG_NM
      ,@[row] AS S_ROW
  FROM MDM_ORGANIZATION
 WHERE OFC_CD=@[inv_ofc_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="row" type="12" value="" out="N"/>
				<param name="inv_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
