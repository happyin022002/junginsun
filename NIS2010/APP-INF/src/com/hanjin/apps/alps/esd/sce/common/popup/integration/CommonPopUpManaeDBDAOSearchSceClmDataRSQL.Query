<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonPopUpManaeDBDAOSearchSceClmDataRSQL">
			<desc><![CDATA[select data]]></desc>
			<sql><![CDATA[
SELECT CNTR_NO
	,TO_CHAR(DEP_DT,'YYYY/MM/DD HH24:MI:SS') ETA_DT
	,CLM_CRR_NM RR
	,TO_CHAR(CLM_RCV_DT,'YYYY/MM/DD HH24:MI:SS') UPD_DT
	,DECODE(CNMV_YR,'IETA','RR ETA at Interchange','RR ETA at DEST') ETA_TP
FROM  SCE_CLM
WHERE CNTR_NO = @[cntr_no]
AND   CNMV_YR = @[cnmv_yr]
AND   ROWNUM <= 5
ORDER BY CLM_RCV_DT DESC			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
