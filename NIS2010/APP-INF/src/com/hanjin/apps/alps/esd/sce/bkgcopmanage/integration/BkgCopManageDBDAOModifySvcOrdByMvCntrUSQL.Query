<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BkgCopManageDBDAOModifySvcOrdByMvCntrUSQL">
			<desc><![CDATA[move container 시 Cy/DOOR S/O 의 bkg_no, bl_no, cop_no 를 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE TRS_TRSP_SVC_ORD
SET BKG_NO = @[new_bkg_no],
  BL_NO = @[new_bl_no],
  COP_NO = @[new_cop_no]
WHERE (TRSP_SO_OFC_CTY_CD,
      TRSP_SO_SEQ) IN (
    SELECT TRSP_SO_OFC_CTY_CD,
      TRSP_SO_SEQ
    FROM TRS_TRSP_SVC_ORD
    WHERE COP_NO = @[old_cop_no] AND NVL(DELT_FLG, 'N') = 'N' 
	AND COST_ACT_GRP_CD NOT LIKE 'OD%'
	)			]]></sql>
			<params>
				<param name="new_bkg_no" type="12" value="" out="N"/>
				<param name="new_bl_no" type="12" value="" out="N"/>
				<param name="new_cop_no" type="12" value="" out="N"/>
				<param name="old_cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
