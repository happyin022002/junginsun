<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOInsertExptCustCSQL">
			<desc><![CDATA[Constrainte Exception Customer Insert]]></desc>
			<sql><![CDATA[
INSERT INTO PRD_NOD_CNST_EXPT (
   NOD_CD, NOD_CNST_ITM_CD, NOD_CNST_SEQ, 
   CNST_EXPT_SEQ, CNST_EXPT_TP_CD, CNST_EXPT_NO, 
   CNST_EXPT_RMK, DELT_FLG, CRE_OFC_CD, 
   CRE_USR_ID, CRE_DT, UPD_USR_ID, 
   UPD_DT) 
VALUES (
  @[nod_cd],
  @[nod_cnst_itm_cd],
  @[nod_cnst_seq],
  (SELECT TO_NUMBER(NVL(MAX(CNST_EXPT_SEQ), 0)) + 1
     FROM PRD_NOD_CNST_EXPT
    WHERE NOD_CD = @[nod_cd]
          AND NOD_CNST_ITM_CD = @[nod_cnst_itm_cd]
          AND NOD_CNST_SEQ = @[nod_cnst_seq]),
  @[cnst_expt_tp_cd],
  @[cnst_expt_no],
  @[cnst_expt_rmk],
  'N',
  @[cre_ofc_cd],
  @[cre_usr_id],
  SYSDATE,
  @[upd_usr_id],
  SYSDATE
  )			]]></sql>
			<params>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_itm_cd" type="12" value="" out="N"/>
				<param name="nod_cnst_seq" type="12" value="" out="N"/>
				<param name="cnst_expt_tp_cd" type="12" value="" out="N"/>
				<param name="cnst_expt_no" type="12" value="" out="N"/>
				<param name="cnst_expt_rmk" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
