<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GuaranteeCommonDBDAOCheckNonTPBRSQL">
			<desc><![CDATA[NON TPB 확인]]></desc>
			<sql><![CDATA[
SELECT  
		GNTE_NO
		, TML_GNTE_CNTR_LIST_SEQ
		, CNTR_NO
		, CNTR_TPSZ_CD
		, BKG_NO
		, BL_NO
		, VVD_CD
		, SC_NO
		, FM_DT
		, TO_DT
		, GNTE_AMT
		, CRE_USR_ID
		, CRE_DT
		, UPD_USR_ID
		, UPD_DT
		, GNTE_PROC_TP_CD
		, IRR_NO
		, TML_IF_SEQ
		, TML_IF_OFC_CD
FROM    TES_GNTE_CNTR_LIST
WHERE   1   = 1
AND     GNTE_NO = @[gnte_no]			]]></sql>
			<params>
				<param name="gnte_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
