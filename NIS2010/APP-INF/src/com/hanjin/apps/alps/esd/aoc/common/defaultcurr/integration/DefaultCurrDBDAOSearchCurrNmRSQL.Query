<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DefaultCurrDBDAOSearchCurrNmRSQL">
			<desc><![CDATA[searchCurrNm]]></desc>
			<sql><![CDATA[
SELECT CURR_NM
  FROM MDM_CURRENCY
 WHERE NVL(DELT_FLG, 'N') <> 'Y'
   AND CURR_CD IN @[curr_cd]			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
