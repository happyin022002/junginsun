<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOSearchGapEdiSndRultRSQL">
			<desc><![CDATA[기존에 발송 내역이 URN, OAN이 A가 있는지 확인을 한다.]]></desc>
			<sql><![CDATA[
SELECT nvl(count(EDI_GRP_CD), 0)  
  FROM SCE_EDI_SND_RSLT
 WHERE 1=1
   AND EDI_GRP_CD = @[edi_group_cd]
   AND EDI_STS_CD IN ('URN','OAN')
   AND EDI_SUB_STS_CD = 'A'
   AND CNTR_NO = @[cntr_no]
   AND BKG_NO = @[bkg_no]
   --AND EDI_SND_TP_CD='Y'
   --AND EDI_SND_RMK = 'SUCCESS(SENT)'			]]></sql>
			<params>
				<param name="edi_group_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
