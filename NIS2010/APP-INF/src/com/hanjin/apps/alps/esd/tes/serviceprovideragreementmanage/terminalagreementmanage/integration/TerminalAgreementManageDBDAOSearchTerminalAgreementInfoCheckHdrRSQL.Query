<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOSearchTerminalAgreementInfoCheckHdrRSQL">
			<desc><![CDATA[Header Select]]></desc>
			<sql><![CDATA[
SELECT  tml_agmt_ofc_cty_cd
        , LPAD(tml_agmt_seq, 5, '0') tml_agmt_seq
        --, agmt_ver_n1st_no ||'.'||agmt_ver_n2nd_no	ver_no		
		, CASE 		WHEN LENGTH(tml_agmt_ver_no) = 3 
					THEN LPAD(SUBSTR( tml_agmt_ver_no, 0, 1 ), 2, '0') || '.' || SUBSTR( tml_agmt_ver_no, 2, 2 )
					ELSE SUBSTR( tml_agmt_ver_no, 0, 2 ) || '.' || SUBSTR( tml_agmt_ver_no, 3, 2 )
		  END tml_agmt_ver_no
FROM    TES_TML_AGMT_HDR  											
WHERE   delt_flg    = 'N'  												
AND     yd_cd       = @[yd_cd]
AND     vndr_seq    = @[vndr_seq]
AND     TO_CHAR(eff_fm_dt, 'YYYY-MM-DD') = @[eff_fm_dt]
AND     TO_CHAR(eff_to_dt, 'YYYY-MM-DD') = @[eff_to_dt]			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="eff_fm_dt" type="12" value="" out="N"/>
				<param name="eff_to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
