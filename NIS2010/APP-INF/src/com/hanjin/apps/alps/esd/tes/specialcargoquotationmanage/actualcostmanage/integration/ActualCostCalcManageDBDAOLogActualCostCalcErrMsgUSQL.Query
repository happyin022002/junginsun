<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ActualCostCalcManageDBDAOLogActualCostCalcErrMsgUSQL">
			<desc><![CDATA[error log처리]]></desc>
			<sql><![CDATA[
UPDATE TES_ACT_COST_HDR A
SET A.EXE_STS_CD = 'E', A.EXE_TO_DT = SYSDATE, A.ERR_RMK = SUBSTRB(@[err_rmk]||DECODE(TRIM(A.ERR_RMK),NULL,'','','',' -- ')||A.ERR_RMK,1,4000)
WHERE 1=1
AND A.TML_ACT_COST_SEQ = @[tml_act_cost_seq]			]]></sql>
			<params>
				<param name="err_rmk" type="12" value="" out="N"/>
				<param name="tml_act_cost_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
