<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ConstraintManageDBDAOSearchCategoryCodeRSQL">
			<desc><![CDATA[SearchCategoryCode]]></desc>
			<sql><![CDATA[
SELECT pctl_cnst_cate_cd category_code
FROM prd_prod_ctl_cnst_cate c  
WHERE nod_lnk_div_cd = @[div_code]     
GROUP BY pctl_cnst_cate_cd
			]]></sql>
			<params>
				<param name="div_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
