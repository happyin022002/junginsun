<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCntrTareWeightRSQL">
			<desc><![CDATA[Container Tare Weight]]></desc>
			<sql><![CDATA[
SELECT S.TARE_WGT CNTR_TWEIGHT
FROM MST_CONTAINER C, MST_CNTR_SPEC S
WHERE 1=1
AND C.CNTR_SPEC_NO = S.CNTR_SPEC_NO
AND C.CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
