<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SingleTransportationSOManageDBDAOMultiSingleTransportationHisCSQL">
			<desc><![CDATA[EmptyRepo S/O History정보를 입력]]></desc>
			<sql><![CDATA[
INSERT INTO TRS_TRSP_HIS  (               
	 TRSP_SO_HIS_SEQ                       ,               
	 TRSP_SO_OFC_CTY_CD                    ,               
	 TRSP_SO_SEQ                           ,               
	 FM_NOD_CD                             ,               
	 TO_NOD_CD                             ,               
	 CRE_OFC_CD                            ,               
	 CRE_DT                                ,               
	 CRE_USR_ID                            ,
	 UPD_DT								   ,
	 UPD_USR_ID							   ,
	 LOCL_CRE_DT						   ,
	 LOCL_UPD_DT        			       
	  ) VALUES (                                           
	 TRS_TRSP_HIS_SEQ1.NEXTVAL			,               
	 @[trsp_so_ofc_cty_cd]				,               
	 @[trsp_so_seq]						,               
	 @[fm_nod_cd]						,               
	 @[to_nod_cd]						,               
	 @[ctrl_ofc_cd]						,     
	 SYSDATE							,
     @[cre_usr_id] 						,    
	 SYSDATE							,
	 @[cre_usr_id] 						,  
	 GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ctrl_ofc_cd])	,               
	 GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ctrl_ofc_cd])                                                    
	 )			]]></sql>
			<params>
				<param name="trsp_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="trsp_so_seq" type="2" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
