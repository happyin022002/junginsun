<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOModifyCopDtlStsFUSQL">
			<desc><![CDATA[ModifyCopDtlStsF]]></desc>
			<sql><![CDATA[
update  sce_cop_dtl
                   set  act_sts_cd = 'F'
                 where  cop_no || to_char(cop_dtl_seq,'0000') < @[IN_COP_NO] || to_char(@[v_cop_dtl_seq],'0000')
                   and  act_sts_cd in ('N','C')
                   and  cop_no = @[IN_COP_NO]			]]></sql>
			<params>
				<param name="IN_COP_NO" type="12" value="" out="N"/>
				<param name="v_cop_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
