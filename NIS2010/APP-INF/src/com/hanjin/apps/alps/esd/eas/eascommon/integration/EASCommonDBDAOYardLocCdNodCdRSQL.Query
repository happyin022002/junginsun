<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EASCommonDBDAOYardLocCdNodCdRSQL">
			<desc><![CDATA[YardLocCdNodCd]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SUBSTR(YD_CD, 6, 7) NOD_CD
      , YD_NM
   FROM MDM_YARD
  WHERE SUBSTR(YD_CD, 1, 5) = @[loc_cd]
    AND DELT_FLG = 'N'
ORDER BY SUBSTR(YD_CD, 6, 7) DESC 			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
