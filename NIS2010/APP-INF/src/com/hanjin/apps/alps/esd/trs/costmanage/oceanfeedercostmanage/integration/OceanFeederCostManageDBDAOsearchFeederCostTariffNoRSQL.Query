<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OceanFeederCostManageDBDAOsearchFeederCostTariffNoRSQL">
			<desc><![CDATA[searchFeederCostTariffNo]]></desc>
			<sql><![CDATA[
SELECT COST_TRF_NO
  FROM TRS_FDR_COST_TRF_HDR
 WHERE RHQ_CD = TRS_COMMON_PKG.TRS_GET_RHQ_OFC_CD(@[in_ofc_cd])
   AND COST_TRF_STS_CD IN ('B', 'U', 'C')
ORDER BY CRE_DT DESC			]]></sql>
			<params>
				<param name="in_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
