<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CopDetailReceiveDBDAOSearchPodArrVslSkdHisTgtRSQL">
			<desc><![CDATA[TPS, TAS TRADE GROUP 에 대해서 미주로 출발하는 마지막 PORT 일 경우, 미주 지역의 SKD를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT BB.VSL_CD,
  BB.SKD_VOY_NO,
  BB.SKD_DIR_CD,
  BB.VPS_PORT_CD,
  BB.CLPT_IND_SEQ,
  C.TRD_CD,
  TO_CHAR(BB.VPS_ETA_DT, 'YYYY/MM/DD HH24:MI:SS') AS VPS_ETA_DT,
  TO_CHAR(BB.VPS_ETB_DT, 'YYYY/MM/DD HH24:MI:SS') AS VPS_ETB_DT,
  TO_CHAR(BB.VPS_ETD_DT, 'YYYY/MM/DD HH24:MI:SS') AS VPS_ETD_DT,
  A.ACT_RCV_DT,
  A.ACT_RCV_NO,
  TO_CHAR(A.ACT_DT, 'YYYY/MM/DD HH24:MI:SS') AS ACT_DEP_DT,
  A.VPS_PORT_CD AS LST_POL_CD
FROM (
    SELECT /*+ INDEX_DESC (SCE_ACT_RCV_IF XPKSCE_ACT_RCV_IF) */
      ACT_RCV_DT,
      ACT_RCV_NO,
      BKG_NO,
      CNTR_NO,
      ACT_DT,
      ACT_STS_MAPG_CD,
      NOD_CD,
      ACT_RCV_TP_CD,
      COP_RLT_FLG,
      ACT_UMCH_TP_CD,
      UMCH_CHK_DT,
      VSL_CD,
      SKD_VOY_NO,
      SKD_DIR_CD,
      VPS_PORT_CD,
      CLPT_IND_SEQ
    FROM SCE_ACT_RCV_IF
    WHERE ACT_RCV_TP_CD = '2'
      AND ACT_RCV_DT = TO_CHAR(SYSDATE, 'YYYYMMDD')
      AND VSL_CD = @[vsl_cd]
      AND SKD_VOY_NO = @[skd_voy_no]
      AND SKD_DIR_CD = @[skd_dir_cd]
      AND ACT_STS_MAPG_CD = 'ATD'
      AND NOD_CD = @[nod_cd]
      AND CLPT_IND_SEQ = @[clpt_ind_seq]
      AND ROWNUM = 1 ) A,
  (
    SELECT VSL_CD,
      SKD_VOY_NO,
      SKD_DIR_CD,
      VPS_PORT_CD,
      CLPT_IND_SEQ,
      CLPT_SEQ,
      SLAN_CD,
      YD_CD,
      LEAD(YD_CD) OVER (
        ORDER BY CLPT_SEQ) AS NEXT_PORT
    FROM (
            SELECT VSL_CD,
              SKD_VOY_NO,
              SKD_DIR_CD,
              VPS_PORT_CD,
              CLPT_IND_SEQ,
              CLPT_SEQ,
              SLAN_CD,
              YD_CD
            FROM VSK_VSL_PORT_SKD SKD
            WHERE VSL_CD = @[vsl_cd]
              AND SKD_VOY_NO = @[skd_voy_no]
              AND SKD_DIR_CD = @[skd_dir_cd]
              AND EXISTS (
                    SELECT '1'
                    FROM BKG_BOOKING
                    WHERE VSL_CD = @[vsl_cd]
                      AND SKD_VOY_NO = @[skd_voy_no]
                      AND SKD_DIR_CD = @[skd_dir_cd]
                      AND SKD.VPS_PORT_CD IN (POL_CD, POD_CD)
                    ) 
	) VSK
    WHERE VSL_CD = @[vsl_cd]
      AND SKD_VOY_NO = @[skd_voy_no]
      AND SKD_DIR_CD = @[skd_dir_cd] ) B,
  VSK_VSL_PORT_SKD BB,
  MDM_DTL_REV_LANE C,
  MDM_LOCATION EVNT_LOC,
  MDM_LOCATION DEST_LOC,
  MDM_LOCATION CK_LOC
WHERE A.VSL_CD = B.VSL_CD
  AND A.SKD_VOY_NO = B.SKD_VOY_NO
  AND A.SKD_DIR_CD = B.SKD_DIR_CD
  AND A.NOD_CD = B.YD_CD
  AND A.CLPT_IND_SEQ = B.CLPT_IND_SEQ
  AND B.VSL_CD = BB.VSL_CD
  AND B.SKD_VOY_NO = BB.SKD_VOY_NO
  AND B.SKD_DIR_CD = BB.SKD_DIR_CD
  AND B.CLPT_SEQ < BB.CLPT_SEQ
  AND B.SLAN_CD = SUBSTR(C.RLANE_CD, 0, 3)
  AND A.SKD_DIR_CD = C.VSL_SLAN_DIR_CD
  AND C.TO_CONTI_CD = 'M'
  AND C.TRD_CD IN ('TPS', 'TAS')
  AND A.VPS_PORT_CD = EVNT_LOC.LOC_CD
  AND BB.VPS_PORT_CD = DEST_LOC.LOC_CD
  AND CK_LOC.LOC_CD = SUBSTR(B.NEXT_PORT, 0, 5)
  AND CK_LOC.CONTI_CD = 'M'
  AND EVNT_LOC.CONTI_CD != 'M'
  AND DEST_LOC.CONTI_CD = 'M'
  AND NOT EXISTS (SELECT '1' FROM SCE_POD_ARR_VSL_SKD_HIS WHERE VSL_CD = BB.VSL_CD AND SKD_VOY_NO = BB.SKD_VOY_NO
			AND SKD_DIR_CD = BB.SKD_DIR_CD AND VPS_PORT_CD = BB.VPS_PORT_CD AND CLPT_IND_SEQ = BB.CLPT_IND_SEQ)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="nod_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
