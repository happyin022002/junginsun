								,	NULL                           				FTU_USE_CTNT3																					
								,	NULL                           				FTU_USE_CTNT4																					
								,	NULL                           				FTU_USE_CNTR5																					
								,	SYSDATE                        				CRE_DT																							
								,	@[CRE_USR_ID]          		   				CRE_USR_ID																						
								,	''                             				EAI_EVNT_DT                      																
								,	'F'											TRSP_SO_TP_CD																					
								,	NULL										TRSP_SO_OFC_CTY_CD																				
								,	-6											TRSP_SO_SEQ																						
						FROM		TRS_TRSP_INV_WRK 							INV_WRK																							
