	     , NULL                  REF_BKG_NO                                                                 
	     , NULL                  CGO_TP_CD                                                                  
	     , NULL                  EQ_KND_CD                                                                   
	    FROM    TRS_TRSP_INV_WRK              INV_WRK                                                        
