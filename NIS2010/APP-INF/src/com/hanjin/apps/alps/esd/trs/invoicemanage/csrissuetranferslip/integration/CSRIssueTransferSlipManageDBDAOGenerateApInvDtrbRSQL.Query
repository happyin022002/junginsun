<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CSRIssueTransferSlipManageDBDAOGenerateApInvDtrbRSQL">
			<desc><![CDATA[CSR 생성내역을 조회]]></desc>
			<sql><![CDATA[
SELECT		CSR_NO																										
			,	ROWNUM			LINE_SEQ																					
			,	LINE_NO																										
			,	LINE_TP_LU_CD																								
			,	CSR_AMT AS INV_AMT																										
			,	INV_DESC																									
			,	INV_TAX_CD																									
			,	DTRB_COA_CO_CD																								
			,	DTRB_COA_RGN_CD																								
			,	DTRB_COA_CTR_CD																								
			,	DTRB_COA_ACCT_CD																							
			,	DTRB_COA_VVD_CD																								
			,	DTRB_COA_INTER_CO_CD																						
			,	DTRB_COA_FTU_N1ST_CD																						
			,	DTRB_COA_FTU_N2ND_CD																						
			,	ATTR_CATE_NM																								
			,	ATTR_CTNT1																									
			,	ATTR_CTNT2																									
			,	ATTR_CTNT3																									
			,	ATTR_CTNT4																									
			,	ATTR_CTNT5																									
			,	ATTR_CTNT6																									
			,	ATTR_CTNT7																									
			,	ATTR_CTNT8																									
			,	ATTR_CTNT9																									
			,	ATTR_CTNT10																									
			,	ATTR_CTNT11																									
			,	ATTR_CTNT12																									
			,	ATTR_CTNT13																									
			,	ATTR_CTNT14																									
			,	ATTR_CTNT15																									
			,	CASE 	WHEN TRSP_SO_TP_CD = 'O' AND EQ_KND_CD = 'U' AND CGO_TP_CD = 'F' AND REF_BKG_NO IS NOT NULL 
						AND (SELECT B.BKG_STS_CD FROM BKG_BOOKING B WHERE B.BKG_NO = REF_BKG_NO) <> 'X' THEN REF_BKG_NO			
						ELSE BKG_NO																							
				END		BKG_NO																								
			,	CNTR_TPSZ_CD																								
			,	ACT_VVD_CD																									
			,	PLN_SCTR_DIV_CD																								
			,	SO_CRR_CD																									
			,	YD_CD																										
			,	FTU_USE_CTNT1																								
			,	FTU_USE_CTNT2																								
			,	FTU_USE_CTNT3																								
			,	FTU_USE_CTNT4																								
			,	FTU_USE_CNTR5																								
			,	CRE_DT																										
			,	CRE_USR_ID																									
			,	EAI_EVNT_DT																									
			,	TRSP_SO_TP_CD																					 			
			,	TRSP_SO_OFC_CTY_CD AS SO_OFC_CTY_CD
			,	TRSP_SO_SEQ AS SO_SEQ																									
	FROM	(																												
			SELECT		CSR_NO																								
					,	LINE_SEQ																							
					,	LINE_NO																								
					,	LINE_TP_LU_CD																						
					,	CSR_AMT																								
					,	INV_DESC																							
					,	INV_TAX_CD																							
					,	DTRB_COA_CO_CD																						
					,	DTRB_COA_RGN_CD																						
					,	DTRB_COA_CTR_CD																						
					,	DTRB_COA_ACCT_CD																					
					,	DTRB_COA_VVD_CD																						
					,	DTRB_COA_INTER_CO_CD																				
					,	DTRB_COA_FTU_N1ST_CD																				
					,	DTRB_COA_FTU_N2ND_CD																				
					,	ATTR_CATE_NM																						
					,	ATTR_CTNT1																							
					,	ATTR_CTNT2																							
					,	ATTR_CTNT3																							
					,	ATTR_CTNT4																							
					,	ATTR_CTNT5																							
					,	ATTR_CTNT6																							
					,	ATTR_CTNT7																							
					,	ATTR_CTNT8																							
					,	ATTR_CTNT9																							
					,	ATTR_CTNT10																							
					,	ATTR_CTNT11																							
					,	ATTR_CTNT12																							
					,	ATTR_CTNT13																							
					,	ATTR_CTNT14																							
					,	ATTR_CTNT15																							
					,	BKG_NO																								
					,	CNTR_TPSZ_CD																						
					,	ACT_VVD_CD																							
					,	PLN_SCTR_DIV_CD																						
					,	SO_CRR_CD																							
					,	YD_CD																								
					,	FTU_USE_CTNT1																						
					,	FTU_USE_CTNT2																						
					,	FTU_USE_CTNT3																						
					,	FTU_USE_CTNT4																						
					,	FTU_USE_CNTR5																						
					,	CRE_DT																								
					,	CRE_USR_ID																							
					,	EAI_EVNT_DT        																					
					,	TRSP_SO_TP_CD																					 	
					,	TRSP_SO_OFC_CTY_CD																					
					,	TRSP_SO_SEQ																							
					,	REF_BKG_NO																							
					,	CGO_TP_CD																							
					,	EQ_KND_CD																							
			FROM	(                                                                                                 		
					SELECT		@[CSR_NO]	CSR_NO																					
							,	'1'	LINE_SEQ													    
							,	@[LINE_NO]      LINE_NO							                                
							,	'ITEM'		LINE_TP_LU_CD													
							,	SUM(CSR_AMT)        		CSR_AMT															
							,	INV_DESC																					
							,	INV_TAX_CD																					
							,	DTRB_COA_CO_CD																				
							,	DTRB_COA_RGN_CD																				
							,	DTRB_COA_CTR_CD																				
							,	DTRB_COA_ACCT_CD																			
							,	DTRB_COA_VVD_CD		                                                                        
							,	DTRB_COA_INTER_CO_CD																		
							,	DTRB_COA_FTU_N1ST_CD																		
							,	DTRB_COA_FTU_N2ND_CD																		
							,	ATTR_CATE_NM																				
							,	ATTR_CTNT1																					
							,	ATTR_CTNT2																			        
							,	ATTR_CTNT3																			        
							,	ATTR_CTNT4																					
							,	ATTR_CTNT5																					
							,	ATTR_CTNT6																					
							,	ATTR_CTNT7																					
							,	ATTR_CTNT8																					
							,	ATTR_CTNT9																					
							,	ATTR_CTNT10																					
							,	ATTR_CTNT11																					
							,	ATTR_CTNT12																					
							,	ATTR_CTNT13																					
							,	ATTR_CTNT14																					
							,	ATTR_CTNT15																					
							,	BKG_NO																						
							,	CNTR_TPSZ_CD																				
							,	ACT_VVD_CD																					
							,	PLN_SCTR_DIV_CD																				
							,	SO_CRR_CD																					
							,	YD_CD																						
							,	FTU_USE_CTNT1																				
							,	TO_CHAR(COUNT(FTU_USE_CTNT2)) 	FTU_USE_CTNT2						                        
							,	FTU_USE_CTNT3																				
							,	FTU_USE_CTNT4																				
							,	FTU_USE_CNTR5																				
							,	CRE_DT																						
							,	CRE_USR_ID																					
							,	EAI_EVNT_DT                                                                                 
							,	TRSP_SO_TP_CD																					 			
							,	TRSP_SO_OFC_CTY_CD																							
							,	TRSP_SO_SEQ																									
							,	REF_BKG_NO																							
							,	CGO_TP_CD																							
							,	EQ_KND_CD																							
					FROM     	(                                                                                           
					SELECT																									
								CASE	WHEN @[CURR_CD] IN ('KRW','JPY','TWD') THEN																																			
											CASE 	WHEN SVC_ORD.TRSP_INV_CALC_LGC_TP_CD = 'TM' THEN SUM( ROUND( ( (NVL(SVC_ORD.BZC_AMT,0) + NVL(SVC_ORD.NEGO_AMT,0) ) * NVL(SVC_ORD.INV_XCH_RT,1) ), 0) + NVL(SVC_ORD.CSR_BZC_CORR_AMT,0) ) 		
													WHEN SVC_ORD.TRSP_INV_CALC_LGC_TP_CD = 'DV' THEN SUM( ROUND( ( (NVL(SVC_ORD.BZC_AMT,0) + NVL(SVC_ORD.NEGO_AMT,0) ) / NVL(SVC_ORD.INV_XCH_RT,1) ), 0) + NVL(SVC_ORD.CSR_BZC_CORR_AMT,0) ) 		
													ELSE SUM( ROUND( (NVL(SVC_ORD.BZC_AMT,0) + NVL(SVC_ORD.NEGO_AMT,0) ), 0) + NVL(SVC_ORD.CSR_BZC_CORR_AMT,0) ) 																					
											END																																									
										ELSE																																									
											CASE 	WHEN SVC_ORD.TRSP_INV_CALC_LGC_TP_CD = 'TM' THEN SUM( ROUND( ( (NVL(SVC_ORD.BZC_AMT,0) + NVL(SVC_ORD.NEGO_AMT,0) ) * NVL(SVC_ORD.INV_XCH_RT,1) ), 2) + NVL(SVC_ORD.CSR_BZC_CORR_AMT,0) )		
													WHEN SVC_ORD.TRSP_INV_CALC_LGC_TP_CD = 'DV' THEN SUM( ROUND( ( (NVL(SVC_ORD.BZC_AMT,0) + NVL(SVC_ORD.NEGO_AMT,0) ) / NVL(SVC_ORD.INV_XCH_RT,1) ), 2) + NVL(SVC_ORD.CSR_BZC_CORR_AMT,0) )		
													ELSE SUM( ROUND( (NVL(SVC_ORD.BZC_AMT,0) + NVL(SVC_ORD.NEGO_AMT,0) ), 2) + NVL(SVC_ORD.CSR_BZC_CORR_AMT,0) )																					
											END																																									
								END												CSR_AMT																																									
							,	ACCT_NM.ACCT_ENG_NM								INV_DESC																								
							,	@[INV_TAX_CD]									INV_TAX_CD																								
							,	'01'											DTRB_COA_CO_CD																							
							,	FINC_RGN_CD										DTRB_COA_RGN_CD																							
							,	AP_CTR_CD										DTRB_COA_CTR_CD																							
							,	SVC_ORD.ACCT_CD									DTRB_COA_ACCT_CD																						
							,	SVC_ORD.FINC_VVD_CD								DTRB_COA_VVD_CD																							
							,	NVL(SUBS_CO_CD,'00')    						DTRB_COA_INTER_CO_CD																					
							,	'000000'                						DTRB_COA_FTU_N1ST_CD																					
							,	'000000'                						DTRB_COA_FTU_N2ND_CD																					
							,	SVC_ORD.ACCT_CD         						ATTR_CATE_NM																							
							,	SVC_ORD.INV_NO          						ATTR_CTNT1																								
							,	TO_CHAR(INV_WRK.INV_ISS_DT,'YYYYMMDD')      	ATTR_CTNT2																			
							,	(	SELECT   A.LOC_CD																												
									FROM     MDM_ORGANIZATION A																										
									WHERE    SVC_ORD.CRE_OFC_CD	= A.OFC_CD																						
								)     											ATTR_CTNT3																			
							,	NULL											ATTR_CTNT4																						
							,	NULL											ATTR_CTNT5																						
							,	NULL											ATTR_CTNT6																						
							,	NULL											ATTR_CTNT7																						
							,	NULL   											ATTR_CTNT8																						
							,	NULL											ATTR_CTNT9																						
							,	NULL											ATTR_CTNT10																						
							,	NULL											ATTR_CTNT11																						
							,	NULL											ATTR_CTNT12																						
							,	NULL											ATTR_CTNT13																						
							,	INV_WRK.AP_RVS_CNG_FLG							ATTR_CTNT14																						
							,	NULL											ATTR_CTNT15																						
							,	SVC_ORD.ORG_BKG_NO								BKG_NO																							
							,	SVC_ORD.EQ_TPSZ_CD								CNTR_TPSZ_CD																					
							,	SVC_ORD.FINC_VVD_CD								ACT_VVD_CD																												
							,	'C'												PLN_SCTR_DIV_CD																					
							,	NULL											SO_CRR_CD																						
							,	NULL											YD_CD																							
							,	SVC_ORD.LGS_COST_CD								FTU_USE_CTNT1																					
							,	COUNT(SVC_ORD.EQ_TPSZ_CD)  						FTU_USE_CTNT2			 																								
							,	'C'												FTU_USE_CTNT3																					
							,	NULL											FTU_USE_CTNT4																					
							,	NULL											FTU_USE_CNTR5																					
							,	SYSDATE											CRE_DT																							
							,	@[CRE_USR_ID]                   				CRE_USR_ID																						
							,	''												EAI_EVNT_DT              																		
							,	INV_WRK.INV_NO																																	
							,	INV_WRK.INV_VNDR_SEQ                                                                                                                            
							,	SVC_ORD.TRSP_SO_TP_CD																					 			
							,	SVC_ORD.TRSP_SO_OFC_CTY_CD																							
							,	SVC_ORD.TRSP_SO_SEQ																									
							,	SVC_ORD.REF_BKG_NO																									
							,	SVC_ORD.CGO_TP_CD																							
							,	SVC_ORD.EQ_KND_CD																							
					FROM		TRS_TRSP_INV_WRK 								INV_WRK		            																				
							,	MDM_ACCOUNT       								ACCT_NM		            																				
							,	TRS_TRSP_SVC_ORD 								SVC_ORD			            																			
							,	MDM_ORGANIZATION  								MDM_ORG		            																				
							,	TRS_TRSP_WRK_ORD 								WRK_ORD            																						
							,	MDM_VENDOR 										MDM_VNDR                                         														
					WHERE		ACCT_NM.ACCT_CD				= SVC_ORD.ACCT_CD           																			
					AND			INV_WRK.INV_NO				= SVC_ORD.INV_NO              																			
					AND			INV_WRK.INV_VNDR_SEQ		= SVC_ORD.INV_VNDR_SEQ        																			
					AND			MDM_ORG.OFC_CD				= @[COST_OFC_CD]          																			
					AND			MDM_VNDR.VNDR_SEQ			= SVC_ORD.INV_VNDR_SEQ        																			
					AND			SVC_ORD.TRSP_WO_OFC_CTY_CD	= WRK_ORD.TRSP_WO_OFC_CTY_CD  																			
					AND			SVC_ORD.TRSP_WO_SEQ			= WRK_ORD.TRSP_WO_SEQ         																			
--					AND			SVC_ORD.WO_ISS_KNT			= WRK_ORD.WO_ISS_KNT            																		
					AND			INV_WRK.INV_NO				= @[INV_NO]
					AND			INV_WRK.INV_VNDR_SEQ		= @[INV_VNDR_SEQ]
					AND			TRSP_INV_AUD_STS_CD			= @[TRSP_INV_AUD_STS_CD]                           																			
					AND			NVL(SVC_ORD.DELT_FLG,'N')   = 'N'                           																			
					AND			NVL(INV_HLD_FLG,'N')        = 'N'                 																					
					AND			INV_WRK.DELT_FLG			= 'N'                     																				
					GROUP BY	ACCT_NM.ACCT_ENG_NM		            																								
							,	INV_WRK.INV_BZC_AMT	            																									
							,	FINC_RGN_CD                 																										
							,	AP_CTR_CD                        																									
							,	SVC_ORD.ACCT_CD                   																									
							,	SVC_ORD.FINC_VSL_CD		               																								
							,	SVC_ORD.FINC_SKD_VOY_NO               																								
							,	SVC_ORD.SKD_DIR_CD               																									
							,	SVC_ORD.FINC_SKD_DIR_CD               																								
							,	NVL(SUBS_CO_CD,'00')               																									
							,	SVC_ORD.ACCT_CD               																										
							,	SVC_ORD.INV_NO               																										
							,	INV_WRK.INV_ISS_DT 
							,   INV_WRK.AP_RVS_CNG_FLG                                            																		
							,	SUBSTR(SVC_ORD.FM_NOD_CD,1,5)                                             															
							,	SVC_ORD.ORG_BKG_NO                                             																			
							,	SVC_ORD.EQ_TPSZ_CD                                             																		
							,	NVL(SVC_ORD.VSL_CD||SVC_ORD.SKD_VOY_NO||SVC_ORD.SKD_DIR_CD,'CNTC'||TO_CHAR(WRK_ORD.LOCL_CRE_DT,'YYMM')||'MM')                            
							,	SVC_ORD.LGS_COST_CD                                             																	
							,	MDM_VNDR.LOC_CD                                               																		
							,	WRK_ORD.LOCL_CRE_DT                                              																		
							,	SVC_ORD.CRE_OFC_CD                                          																		
							, 	SVC_ORD.INV_XCH_RT                                          																		
							, 	SVC_ORD.TRSP_INV_CALC_LGC_TP_CD                                       																
						    , 	INV_WRK.INV_NO																		
						    , 	INV_WRK.INV_VNDR_SEQ                                                                  
						    , 	SVC_ORD.FINC_VVD_CD                                                                   
						    , 	SVC_ORD.TRSP_SO_TP_CD                                                                   
						    , 	SVC_ORD.TRSP_SO_OFC_CTY_CD                                                                   
						    , 	SVC_ORD.TRSP_SO_SEQ                                                                   
							,	SVC_ORD.REF_BKG_NO																									
							,	SVC_ORD.CGO_TP_CD																							
							,	SVC_ORD.EQ_KND_CD																							
						    )                                                                                       
				GROUP BY	INV_NO                                                                                  
						,	INV_VNDR_SEQ                                                                            
						,	INV_DESC																				
						,	INV_TAX_CD																				
						,	DTRB_COA_CO_CD																			
						,	DTRB_COA_RGN_CD																			
						,	DTRB_COA_CTR_CD																			
						,	DTRB_COA_ACCT_CD																		
						,	DTRB_COA_VVD_CD		                                                                    
						,	DTRB_COA_INTER_CO_CD																	
						,	DTRB_COA_FTU_N1ST_CD																	
						,	DTRB_COA_FTU_N2ND_CD																	
						,	ATTR_CATE_NM																			
						,	ATTR_CTNT1																				
						,	ATTR_CTNT2																			    
						,	ATTR_CTNT3																			    
						,	ATTR_CTNT4																				
						,	ATTR_CTNT5																				
						,	ATTR_CTNT6																				
						,	ATTR_CTNT7																				
						,	ATTR_CTNT8																				
						,	ATTR_CTNT9																				
						,	ATTR_CTNT10																				
						,	ATTR_CTNT11																				
						,	ATTR_CTNT12																				
						,	ATTR_CTNT13																				
						,	ATTR_CTNT14																				
						,	ATTR_CTNT15																				
						,	BKG_NO																					
						,	CNTR_TPSZ_CD																			
						,	ACT_VVD_CD																				
						,	PLN_SCTR_DIV_CD																			
						,	SO_CRR_CD																				
						,	YD_CD																					
						,	FTU_USE_CTNT1																			
						,	FTU_USE_CTNT3																			
						,	FTU_USE_CTNT4																			
						,	FTU_USE_CNTR5																			
						,	CRE_DT																					
						,	CRE_USR_ID																				
						,	EAI_EVNT_DT                                                                             
						, 	TRSP_SO_TP_CD                                                                   		
						, 	TRSP_SO_OFC_CTY_CD                                                                   	
						, 	TRSP_SO_SEQ                                                                   			
						,	REF_BKG_NO																				
						, 	CGO_TP_CD                                                                   			
						, 	EQ_KND_CD                                                                   			
					UNION ALL 																						
					SELECT		@[CSR_NO]	CSR_NO																					
							,	'6'	LINE_SEQ													    
							,	@[LINE_NO]      LINE_NO							                                
							,	'ITEM'		LINE_TP_LU_CD													
							,	SUM(CSR_AMT)        		CSR_AMT															
							,	INV_DESC																					
							,	INV_TAX_CD																					
							,	DTRB_COA_CO_CD																				
							,	DTRB_COA_RGN_CD																				
							,	DTRB_COA_CTR_CD																				
							,	DTRB_COA_ACCT_CD																			
							,	DTRB_COA_VVD_CD		                                                                        
							,	DTRB_COA_INTER_CO_CD																		
							,	DTRB_COA_FTU_N1ST_CD																		
							,	DTRB_COA_FTU_N2ND_CD																		
							,	ATTR_CATE_NM																				
							,	ATTR_CTNT1																					
							,	ATTR_CTNT2																			        
							,	ATTR_CTNT3																			        
							,	ATTR_CTNT4																					
							,	ATTR_CTNT5																					
							,	ATTR_CTNT6																					
							,	ATTR_CTNT7																					
							,	ATTR_CTNT8																					
							,	ATTR_CTNT9																					
							,	ATTR_CTNT10																					
							,	ATTR_CTNT11																					
							,	ATTR_CTNT12																					
							,	ATTR_CTNT13																					
							,	ATTR_CTNT14																					
							,	ATTR_CTNT15																					
							,	BKG_NO																						
							,	CNTR_TPSZ_CD																				
							,	ACT_VVD_CD																					
							,	PLN_SCTR_DIV_CD																				
							,	SO_CRR_CD																					
							,	YD_CD																						
							,	FTU_USE_CTNT1																				
							,	TO_CHAR(COUNT(FTU_USE_CTNT2)) 	FTU_USE_CTNT2						                        
							,	FTU_USE_CTNT3																				
							,	FTU_USE_CTNT4																				
							,	FTU_USE_CNTR5																				
							,	CRE_DT																						
							,	CRE_USR_ID																					
							,	EAI_EVNT_DT                                                                                 
							,	TRSP_SO_TP_CD																					 			
							,	TRSP_SO_OFC_CTY_CD																							
							,	TRSP_SO_SEQ																									
							,	REF_BKG_NO																							
							,	CGO_TP_CD																							
							,	EQ_KND_CD																							
					FROM     	(                                                                                           
					SELECT																									
								SUM(NVL(SVC_ORD.INV_SBC_AMT, 0))				CSR_AMT																																									
							,	DECODE(SVC_ORD.CGO_TP_CD, 'F', 'SBC Tax Full', 'SBC Tax Empty')								INV_DESC																								
							,	''												INV_TAX_CD																								
							,	'01'											DTRB_COA_CO_CD																							
							,	FINC_RGN_CD										DTRB_COA_RGN_CD																							
							,	AP_CTR_CD										DTRB_COA_CTR_CD																							
							,	DECODE(SVC_ORD.CGO_TP_CD, 'F', '512396', '512347') DTRB_COA_ACCT_CD																						
							,	SVC_ORD.FINC_VVD_CD								DTRB_COA_VVD_CD																							
