<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="QualitativeEvaluationCreationDBDAOSearchQECreRSQL">
			<desc><![CDATA[Qualitative Evaluation 테이블에서 데이터를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 	 EV_YR
		,EV_SVC_CATE_CD
		,QUAL_EV_SEQ
		,EV_AREA_CTNT
		,EV_FCTR_CTNT
		,EV_WGT_RT
		,N1ST_EV_GRD_CTNT
		,N2ND_EV_GRD_CTNT
		,N3RD_EV_GRD_CTNT
		,CRE_USR_ID
		,CRE_DT
		,UPD_USR_ID
		,UPD_DT
FROM SPE_QUAL_EV
WHERE EV_YR = @[ev_yr]
AND EV_SVC_CATE_CD = @[ev_svc_cate_cd]			]]></sql>
			<params>
				<param name="ev_yr" type="12" value="" out="N"/>
				<param name="ev_svc_cate_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
