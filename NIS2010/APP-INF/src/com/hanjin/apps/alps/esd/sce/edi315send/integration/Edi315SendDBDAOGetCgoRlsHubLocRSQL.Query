<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetCgoRlsHubLocRSQL">
			<desc><![CDATA[US AMS:B/L Inquiry의 HUB Loc cd를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
HUB_LOC_CD
FROM
BKG_CSTMS_ADV_BL
WHERE 1=1
AND CNT_CD = SUBSTR(@[event_yard], 1, 2)
AND BL_NO = @[bl_no]			]]></sql>
			<params>
				<param name="event_yard" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
