<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EDISetupDBDAOSearchEdiCgoStndStsCntRSQL">
			<desc><![CDATA[SearchEdiCgoStndStsCnt]]></desc>
			<sql><![CDATA[
select count(EDI_STND_STS_CD)
            from EDI_CGO_STND_STS
            where
            1=1 
#if(${status_std_cd} != '')
            and EDI_STND_STS_CD = @[status_std_cd]
#end
#if(${company_cd} != '')
            and CO_DIV_CD = @[company_cd]
#end			]]></sql>
			<params>
				<param name="status_std_cd" type="12" value="" out="N"/>
				<param name="company_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
