<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeManageDBDAOMultiCodeUSQL">
			<desc><![CDATA[MultiCodeU]]></desc>
			<sql><![CDATA[
UPDATE   TPB_N3RD_PTY_BIL_TP
SET      N3PTY_EXPN_TP_CD = @[n3pty_expn_tp_cd]
       , N3PTY_BIL_TP_NM = @[n3pty_bil_tp_nm]
       , N3PTY_BIL_TP_DESC = @[n3pty_bil_tp_desc]
	   , COP_EXPT_CS_CD =@[cop_expt_cs_cd]
	   , CML_SYS_IF_CD = @[cml_sys_if_cd]
	   , ACT_FLG = @[act_flg]
	   , UPD_USR_ID = @[upd_usr_id]
	   , UPD_DT = SYSDATE
	   , N3PTY_IF_TP_CD = @[n3pty_if_tp_cd]
       , CHG_CD = @[chg_cd]
       , REV_ACCT_CD = ( SELECT NVL(MAX(M.HJS_CHG_ACCT_CD),@[rev_acct_cd]) FROM MDM_CHARGE M WHERE M.CHG_CD = @[chg_cd] )
       , IDA_SAC_CD = @[ida_sac_cd]
WHERE    1 = 1
AND      N3PTY_BIL_TP_CD = @[n3pty_bil_tp_cd]			]]></sql>
			<params>
				<param name="n3pty_expn_tp_cd" type="12" value="" out="N"/>
				<param name="n3pty_bil_tp_nm" type="12" value="" out="N"/>
				<param name="n3pty_bil_tp_desc" type="12" value="" out="N"/>
				<param name="cop_expt_cs_cd" type="12" value="" out="N"/>
				<param name="cml_sys_if_cd" type="12" value="" out="N"/>
				<param name="act_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="n3pty_if_tp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="rev_acct_cd" type="12" value="" out="N"/>
				<param name="ida_sac_cd" type="12" value="" out="N"/>
				<param name="n3pty_bil_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
