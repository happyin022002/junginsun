<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PodManageDBDAOSearchLocationStateRSQL">
			<desc><![CDATA[SearchLocationState]]></desc>
			<sql><![CDATA[
SELECT ste_cd location_state 
 FROM mdm_location  
 WHERE 1 = 1 
 	AND loc_cd = @[del_code] 
 	AND NVL(DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="del_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
