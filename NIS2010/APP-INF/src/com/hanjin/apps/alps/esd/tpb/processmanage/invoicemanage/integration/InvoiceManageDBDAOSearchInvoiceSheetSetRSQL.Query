<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceManageDBDAOSearchInvoiceSheetSetRSQL">
			<desc><![CDATA[searchInvoiceSheetSet]]></desc>
			<sql><![CDATA[
SELECT   TS.INV_ISS_OFC_CD AS S_INV_ISS_OFC_CD
       , TS.CO_NM
       , TS.OFC_ADDR
       , TS.OFC_PHN_NO
       , TS.OFC_FAX_NO
       , TS.BIL_TO_LOC_DIV_CD
       , TS.INV_RMK1
       , TS.INV_RMK2
       , TS.VAT_XCH_RT
       , TS.VAT_XCH_RT / 100 AS VAT_XCH_RT_DIV
	   , TS.EUR_VAT_REF_NO
       , S.IDA_STE_CD
       , C.IDA_GST_RGST_NO
       , C.IDA_PAN_NO
       , ( SELECT ATTR_CTNT1 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'IDA_TAX_INFO' ) AS IDA_BANK_ACCT_NO
       , ( SELECT ATTR_CTNT2 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'IDA_TAX_INFO' ) AS IDA_IFSC_CD
       , ( SELECT ATTR_CTNT3 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'IDA_TAX_INFO' ) AS IDA_TAX_CIN_NO
FROM     TPB_INV_SH_SET TS
       , MDM_ORGANIZATION O
       , MDM_CUSTOMER C
       , MDM_LOCATION L
       , MDM_STATE S
WHERE    1 = 1
AND      TS.INV_ISS_OFC_CD = O.OFC_CD
AND      O.REP_CUST_CNT_CD = C.CUST_CNT_CD
AND      O.REP_CUST_SEQ = C.CUST_SEQ
AND      C.LOC_CD = L.LOC_CD
AND      L.CNT_CD = S.CNT_CD(+)
AND      L.STE_CD = S.STE_CD(+)
AND      TS.INV_ISS_OFC_CD = @[s_inv_iss_ofc_cd]			]]></sql>
			<params>
				<param name="s_inv_iss_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
