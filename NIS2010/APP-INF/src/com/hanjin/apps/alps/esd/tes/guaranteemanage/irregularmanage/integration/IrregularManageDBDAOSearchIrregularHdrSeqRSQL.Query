<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IrregularManageDBDAOSearchIrregularHdrSeqRSQL">
			<desc><![CDATA[Irregular Header Max Seq 조회]]></desc>
			<sql><![CDATA[
SELECT	(SUBSTR(@[ofc_cd], 1, 3) || TO_CHAR(SYSDATE, 'YYMM') ||
		(SELECT	LPAD((NVL(MAX(CASE WHEN IRR_NO IS NOT NULL AND LENGTH(IRR_NO) > 7 
									THEN SUBSTR(IRR_NO, 8) 
									ELSE NULL 
								END), 0) + 1), 5, 0)
		FROM	TES_IRR_HDR IH2
		WHERE	IH2.OFC_CD LIKE SUBSTR(@[ofc_cd], 1, 3) || '%' ) ) IRR_NO 
FROM DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
