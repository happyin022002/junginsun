<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="USALastCityManageDBDAOSearchLocalCodeManageListRSQL">
			<desc><![CDATA[USA LAST CITY에 대한 LOCATION 정보 확인]]></desc>
			<sql><![CDATA[
SELECT 
    LOC_CD
FROM 
    MDM_LOCATION
WHERE LOC_CD = @[searchStr]			]]></sql>
			<params>
				<param name="searchStr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
