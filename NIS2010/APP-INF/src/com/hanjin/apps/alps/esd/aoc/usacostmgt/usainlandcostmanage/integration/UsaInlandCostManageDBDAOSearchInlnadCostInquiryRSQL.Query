<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UsaInlandCostManageDBDAOSearchInlnadCostInquiryRSQL">
			<desc><![CDATA[searchInlnadCostInquiry]]></desc>
			<sql><![CDATA[
SELECT  H.CNT_CD
      , H.IO_BND_CD
      , H.COST_TRF_NO
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03051',H.COST_TRF_STS_CD) COST_TRF_STS_NM
      , TO_CHAR(H.EFF_FM_DT, 'YYYY-MM-DD') EFF_FM_DT 

      , SUBSTR(D.PORT_NOD_CD, 1,5) || '-' || SUBSTR(D.LOC_NOD_CD, 1,5) PORT_LOC
      , D.PORT_NOD_CD
      , D.HUB_NOD_CD
      , D.LOC_NOD_CD
      , D.MTY_PKUP_RTN_YD_CD
      #if (${bnt_flg} == 'N')   
      , D.RCV_DE_TERM_CD
      #end
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD01725',D.RCV_DE_TERM_CD) RCV_DE_TERM_NM
      , D.TRSP_CRR_MOD_CD
      #if (${bnt_flg} == 'N')   
      , (
          SELECT  USA_COST_TRF_SVC_MOD_CD
          FROM    AOC_USA_IPI_PORT
          WHERE   PORT_CD = SUBSTR(D.PORT_NOD_CD,1,5)
          AND     TRSP_CRR_MOD_CD = D.TRSP_CRR_MOD_CD
        ) LOCL_IPI_SVC_MOD
        #end
      , COMMCODE_PKG.GET_COMDTL_NAME_FNC('CD03118', (
                                                      SELECT  USA_COST_TRF_SVC_MOD_CD
                                                      FROM    AOC_USA_IPI_PORT
                                                      WHERE   PORT_CD = SUBSTR(D.PORT_NOD_CD,1,5)
                                                      AND     TRSP_CRR_MOD_CD = D.TRSP_CRR_MOD_CD
                                                    )
        ) LOCL_IPI_SVC_MOD_NM
      , D.CURR_CD
      , D.INLND_20FT_TTL_AMT
      , D.INLND_40FT_TTL_AMT
      , D.RAIL_20FT_BZC_COST_TTL_AMT + D.RAIL_20FT_FUEL_SCG_TTL_AMT AS RAIL_20FT_TTL_AMT
      , D.RAIL_40FT_BZC_COST_TTL_AMT + D.RAIL_40FT_FUEL_SCG_TTL_AMT AS RAIL_40FT_TTL_AMT
      , D.TRK_20FT_BZC_COST_TTL_AMT + D.TRK_20FT_FUEL_SCG_TTL_AMT + D.TOLL_FEE_AMT AS TRK_20FT_TTL_AMT
      , D.TRK_40FT_BZC_COST_TTL_AMT + D.TRK_40FT_FUEL_SCG_TTL_AMT + D.TOLL_FEE_AMT AS TRK_40FT_TTL_AMT
      , D.SCC_CD
      , NVL(D.MB_20FT_RTO,0)||'%' MB_20FT_RTO
      , NVL(D.MB_40FT_RTO,0)||'%' MB_40FT_RTO
      
      , D.RAIL_20FT_BZC_COST_SRC_CD
      , D.RAIL_20FT_BZC_COST_AMT
      , D.RAIL_20FT_BZC_COST_ADJ_AMT
      , D.RAIL_20FT_BZC_COST_TTL_AMT
      , D.RAIL_20FT_BZC_AGMT_WGT
      , D.RAIL_20FT_BZC_AGMT_WY_TP_CD
      
      , D.RAIL_20FT_FUEL_SCG_SRC_CD
      , D.RAIL_20FT_FUEL_SCG_AMT
      , D.RAIL_20FT_FUEL_SCG_ADJ_AMT
      , D.RAIL_20FT_FUEL_SCG_TTL_AMT
      , D.RAIL_20FT_FUEL_SCG_AGMT_WGT
      , D.RAIL_20FT_FUEL_AGMT_WY_TP_CD
      
      , D.RAIL_40FT_BZC_COST_SRC_CD
      , D.RAIL_40FT_BZC_COST_AMT
      , D.RAIL_40FT_BZC_COST_ADJ_AMT
      , D.RAIL_40FT_BZC_COST_TTL_AMT
      , D.RAIL_40FT_BZC_AGMT_WGT
      , D.RAIL_40FT_BZC_AGMT_WY_TP_CD
      
      , D.RAIL_40FT_FUEL_SCG_SRC_CD
      , D.RAIL_40FT_FUEL_SCG_AMT
      , D.RAIL_40FT_FUEL_SCG_ADJ_AMT
      , D.RAIL_40FT_FUEL_SCG_TTL_AMT
      , D.RAIL_40FT_FUEL_SCG_AGMT_WGT
      , D.RAIL_40FT_FUEL_AGMT_WY_TP_CD
      
      , D.TRK_20FT_BZC_COST_SRC_CD
      , D.TRK_20FT_BZC_COST_AMT
      , D.TRK_20FT_BZC_COST_ADJ_AMT
      , D.TRK_20FT_BZC_COST_TTL_AMT
      , D.TRSP_AGMT_20FT_MTY_YD_CD
      , CASE WHEN NVL(D.MTY_PKUP_RTN_YD_CD, 1) = NVL(D.TRSP_AGMT_20FT_MTY_YD_CD, 1) THEN 'N' ELSE 'Y' END AS TRSP_DIFF_20FT
      , D.TRK_20FT_BZC_AGMT_WGT
      , D.TRK_20FT_BZC_AGMT_WY_TP_CD
      
      , D.TRK_20FT_FUEL_SCG_SRC_CD
      , D.TRK_20FT_FUEL_SCG_AMT
      , D.TRK_20FT_FUEL_SCG_ADJ_AMT
      , D.TRK_20FT_FUEL_SCG_TTL_AMT
      , D.TRK_20FT_FUEL_SCG_AGMT_WGT
      , D.TRK_20FT_FUEL_AGMT_WY_TP_CD
      , D.TOLL_FEE_AMT AS TOLL_FEE_AMT_20 
      
      , D.TRK_40FT_BZC_COST_SRC_CD
      , D.TRK_40FT_BZC_COST_AMT
      , D.TRK_40FT_BZC_COST_ADJ_AMT
      , D.TRK_40FT_BZC_COST_TTL_AMT
      , D.TRSP_AGMT_40FT_MTY_YD_CD
      , CASE WHEN NVL(D.MTY_PKUP_RTN_YD_CD, 1) = NVL(D.TRSP_AGMT_40FT_MTY_YD_CD, 1) THEN 'N' ELSE 'Y' END AS TRSP_DIFF_40FT
      , D.TRK_40FT_BZC_AGMT_WGT
      , D.TRK_40FT_BZC_AGMT_WY_TP_CD
      
      , D.TRK_40FT_FUEL_SCG_SRC_CD
      , D.TRK_40FT_FUEL_SCG_AMT
      , D.TRK_40FT_FUEL_SCG_ADJ_AMT
      , D.TRK_40FT_FUEL_SCG_TTL_AMT
      , D.TRK_40FT_FUEL_SCG_AGMT_WGT
      , D.TRK_40FT_FUEL_AGMT_WY_TP_CD
      , D.TOLL_FEE_AMT AS TOLL_FEE_AMT_40     
  
      , D.MTY_TRSP_20FT_COST_SRC_CD
      , D.MTY_TRSP_20FT_COST_AMT
      , D.MTY_TRSP_20FT_ADJ_COST_AMT
      , D.MTY_TRSP_20FT_TTL_COST_AMT
      
      , D.MTY_TRSP_40FT_COST_SRC_CD
      , D.MTY_TRSP_40FT_COST_AMT
      , D.MTY_TRSP_40FT_ADJ_COST_AMT
      , D.MTY_TRSP_40FT_TTL_COST_AMT
      
      , D.DMST_20FT_COST_SRC_CD
      , D.DMST_20FT_COST_AMT
      , D.DMST_20FT_ADJ_COST_AMT
      , D.DMST_20FT_TTL_COST_AMT
      
      , D.DMST_40FT_COST_SRC_CD
      , D.DMST_40FT_COST_AMT
      , D.DMST_40FT_ADJ_COST_AMT
      , D.DMST_40FT_TTL_COST_AMT
      
      , D.TML_20FT_COST_SRC_CD
      , D.TML_20FT_COST_AMT
      , D.TML_20FT_ADJ_COST_AMT
      , D.TML_20FT_TTL_COST_AMT
      
      , D.TML_40FT_COST_SRC_CD
      , D.TML_40FT_COST_AMT
      , D.TML_40FT_ADJ_COST_AMT
      , D.TML_40FT_TTL_COST_AMT
      
      , D.N1ST_VNDR_SEQ
      , ( SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR WHERE VNDR_SEQ = D.N1ST_VNDR_SEQ ) N1ST_VNDR_NM
      , D.N1ST_INLND_ROUT_CMB_FLG
      , D.N2ND_VNDR_SEQ
      , ( SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR WHERE VNDR_SEQ = D.N2ND_VNDR_SEQ ) N2ND_VNDR_NM
      , D.N2ND_INLND_ROUT_CMB_FLG
      , D.N3RD_VNDR_SEQ
      , ( SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR WHERE VNDR_SEQ = D.N3RD_VNDR_SEQ ) N3RD_VNDR_NM
      , DECODE(
                GREATEST(NVL(D.N1ST_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N1ST_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_40FT_AGMT_OLD_FLG,'0')),'0',NULL,
                GREATEST(NVL(D.N1ST_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N1ST_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N2ND_VNDR_40FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_20FT_AGMT_OLD_FLG,'0'), NVL(D.N3RD_VNDR_40FT_AGMT_OLD_FLG,'0'))
        ) AS AGMT_OLD_FLG


      , TO_CHAR(H.LOCL_CRE_DT, 'YYYY-MM-DD HH24:MI:SS') LOCL_CRE_DT
      , ( SELECT USR_NM FROM COM_USER WHERE USR_ID = H.CRE_USR_ID ) CRE_USR_ID
      , H.CRE_OFC_CD
      , TO_CHAR(H.LOCL_UPD_DT, 'YYYY-MM-DD HH24:MI:SS') LOCL_UPD_DT
      , ( SELECT USR_NM FROM COM_USER WHERE USR_ID = H.UPD_USR_ID) UPD_USR_ID
      , H.UPD_OFC_CD
      
#if (${bnt_flg} == 'N')   
      , D.COST_TRF_ROUT_SEQ
#end
FROM    AOC_USA_INLND_TRF_HDR H
      , AOC_USA_INLND_TRF_DTL D  
WHERE   H.COST_TRF_NO = D.COST_TRF_NO
AND     D.COST_SEL_ROUT_FLG = 'Y'
AND     H.COST_TRF_STS_CD = 'C'
AND     D.DELT_FLG    = 'N'

-- Effective as of
#if(${eff_to_dt} != '')
AND     TO_DATE(REPLACE(@[eff_to_dt],'-',''), 'YYYYMMDD') BETWEEN EFF_FM_DT AND EFF_TO_DT + 0.999999
#end
--Location
#if (${loc_nod_cd} != '')
AND     (  
            SUBSTR(D.LOC_NOD_CD, 1,2) IN (
#foreach ($user_locNodCds IN ${locNodCds})
  #if($velocityCount < $locNodCds.size())
    '$user_locNodCds',
  #else
    '$user_locNodCds'
  #end
#end
                                         )
         OR SUBSTR(D.LOC_NOD_CD, 1,5) IN (
#foreach ($user_locNodCds IN ${locNodCds})
  #if($velocityCount < $locNodCds.size())
    '$user_locNodCds',
  #else
    '$user_locNodCds'
  #end
#end
                                         )
         OR D.LOC_NOD_CD IN (
#foreach ($user_locNodCds IN ${locNodCds})
  #if($velocityCount < $locNodCds.size())
    '$user_locNodCds',
  #else
    '$user_locNodCds'
  #end
#end
                                         )
    )
#end

-- Hub
#if (${hub_nod_cd} != '')
AND     (
SUBSTR(D.HUB_NOD_CD, 1,2) IN (
#foreach ($user_hubNodCds IN ${hubNodCds})
  #if($velocityCount < $hubNodCds.size())
    '$user_hubNodCds',
  #else
    '$user_hubNodCds'
  #end
#end
)
OR SUBSTR(D.HUB_NOD_CD, 1,5) IN (
#foreach ($user_hubNodCds IN ${hubNodCds})
  #if($velocityCount < $hubNodCds.size())
    '$user_hubNodCds',
  #else
    '$user_hubNodCds'
  #end
#end
)
OR D.HUB_NOD_CD IN (
#foreach ($user_hubNodCds IN ${hubNodCds})
  #if($velocityCount < $hubNodCds.size())
    '$user_hubNodCds',
  #else
    '$user_hubNodCds'
  #end
#end
)
    )
#end

-- Port
#if (${port_nod_cd} != '')
AND (
SUBSTR(D.PORT_NOD_CD, 1,2) IN (
#foreach ($user_portNodCds IN ${portNodCds})
  #if($velocityCount < $portNodCds.size())
    '$user_portNodCds',
  #else
    '$user_portNodCds'
  #end
#end
)
OR SUBSTR(D.PORT_NOD_CD, 1,5) IN (
#foreach ($user_portNodCds IN ${portNodCds})
  #if($velocityCount < $portNodCds.size())
    '$user_portNodCds',
  #else
    '$user_portNodCds'
  #end
#end
)
OR D.PORT_NOD_CD IN (
#foreach ($user_portNodCds IN ${portNodCds})
  #if($velocityCount < $portNodCds.size())
    '$user_portNodCds',
  #else
    '$user_portNodCds'
  #end
#end
)
    )
#end

-- Trans Mode
#if (${trsp_crr_mod_cd} != 'ALL')
AND D.TRSP_CRR_MOD_CD IN (
#foreach ($user_trspCrrModCds IN ${trspCrrModCds})
  #if($velocityCount < $trspCrrModCds.size())
    '$user_trspCrrModCds',
  #else
    '$user_trspCrrModCds'
  #end
#end
)
#end

-- Bound
#if (${io_bnd_cd} != 'ALL')
AND H.IO_BND_CD = @[io_bnd_cd]
#end

-- R/D Term
#if (${rcv_de_term_cd} != 'ALL')
AND D.RCV_DE_TERM_CD = @[rcv_de_term_cd]
#end

-- Cost Tariff No
#if (${cost_trf_no} != '')
AND H.COST_TRF_NO IN (
#foreach ($user_costTrfNos IN ${costTrfNos})
  #if($velocityCount < $costTrfNos.size())
    '$user_costTrfNos',
  #else
    '$user_costTrfNos'
  #end
#end
)
#end

-- Cost Factor / System Source (CD03050)
#if (${sys_src_cd} != 'ALL')
AND (1=2
     --Full Rail Basic 20'
#if (${cost_factor_cd} == 'ALL' || ${fullRailBasic20} == 'Y')
     OR D.RAIL_20FT_BZC_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Full Rail FSC 20'
#if (${cost_factor_cd} == 'ALL' || ${fullRailFSC20} == 'Y')
     OR D.RAIL_20FT_FUEL_SCG_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Full Rail Basic 40'
#if (${cost_factor_cd} == 'ALL' || ${fullRailBasic40} == 'Y')
     OR D.RAIL_40FT_BZC_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Full Rail FSC 40'
#if (${cost_factor_cd} == 'ALL' || ${fullRailFSC40} == 'Y')
     OR D.RAIL_40FT_FUEL_SCG_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Truck Basic 20'
#if (${cost_factor_cd} == 'ALL' || ${truckBasic20} == 'Y')
     OR D.TRK_20FT_BZC_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Truck FSC 20'
#if (${cost_factor_cd} == 'ALL' || ${truckFSC20} == 'Y')
     OR D.TRK_20FT_FUEL_SCG_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Truck Basic 40'
#if (${cost_factor_cd} == 'ALL' || ${truckBasic40} == 'Y')
     OR D.TRK_40FT_BZC_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Truck FSC 40'
#if (${cost_factor_cd} == 'ALL' || ${truckFSC40} == 'Y')
     OR D.TRK_40FT_FUEL_SCG_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --TMNL 20'
#if (${cost_factor_cd} == 'ALL' || ${tmnl20} == 'Y')
     OR D.TML_20FT_COST_SYS_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --TMNL 40'
#if (${cost_factor_cd} == 'ALL' || ${tmnl40} == 'Y')
     OR D.TML_40FT_COST_SYS_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Domestic 20'
#if (${cost_factor_cd} == 'ALL' || ${domestic20} == 'Y')
     OR D.DMST_20FT_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Domestic 40'
#if (${cost_factor_cd} == 'ALL' || ${domestic40} == 'Y')
     OR D.DMST_40FT_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end
 
     --Empty 20'
#if (${cost_factor_cd} == 'ALL' || ${empty20} == 'Y')
     OR D.MTY_TRSP_20FT_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end

     --Empty 40'
#if (${cost_factor_cd} == 'ALL' || ${empty40} == 'Y')
     OR D.MTY_TRSP_40FT_COST_SRC_CD IN (
      #foreach ($user_sysSrcCds IN ${sysSrcCds})
        #if($velocityCount < $sysSrcCds.size())
          '$user_sysSrcCds',
        #else
          '$user_sysSrcCds'
        #end
      #end
     )
#end
   )
#end

     --Service Mode Code
#if (${svc_mod_cd} != 'ALL')
AND     ( SUBSTR(D.PORT_NOD_CD, 1,5), D.TRSP_CRR_MOD_CD ) IN (
                                                               SELECT  PORT_CD
                                                                     , TRSP_CRR_MOD_CD
                                                               FROM    AOC_USA_IPI_PORT
                                                               WHERE   USA_COST_TRF_SVC_MOD_CD IN (
	#foreach ($user_svcModCds IN ${svcModCds})
  		#if($velocityCount < $svcModCds.size())
   		 '$user_svcModCds',
 		#else
   		 '$user_svcModCds'
  		#end
	#end
		)
	)
#end


-- Cost Factor / Adjustment
#if (${adjustment_cd} != 'ALL')
AND (1=2
     --Full Rail Basic 20' 
#if (${cost_factor_cd} == 'ALL' || ${fullRailBasic20} == 'Y')
  #if (${adjustment_cd} == 'E')
     OR NVL(D.RAIL_20FT_BZC_COST_ADJ_AMT, 0) = 0
  #end
  #if (${adjustment_cd} == 'N')
     OR NVL(D.RAIL_20FT_BZC_COST_ADJ_AMT, 0) <> 0
  #end
#end
     --Full Rail FSC 20' 
#if (${cost_factor_cd} == 'ALL' || ${fullRailFSC20} == 'Y')
  #if (${adjustment_cd} == 'E')
     OR NVL(D.RAIL_20FT_FUEL_SCG_ADJ_AMT, 0) = 0
  #end
  #if (${adjustment_cd} == 'N')
     OR NVL(D.RAIL_20FT_FUEL_SCG_ADJ_AMT, 0) <> 0
  #end
#end
     --Full Rail Basic 40' 
#if (${cost_factor_cd} == 'ALL' || ${fullRailBasic40} == 'Y')
  #if (${adjustment_cd} == 'E')
     OR NVL(D.RAIL_40FT_BZC_COST_ADJ_AMT, 0) = 0
  #end
  #if (${adjustment_cd} == 'N')
     OR NVL(D.RAIL_40FT_BZC_COST_ADJ_AMT, 0) <> 0
  #end
#end
     --Full Rail FSC 40' 
#if (${cost_factor_cd} == 'ALL' || ${fullRailFSC40} == 'Y')
  #if (${adjustment_cd} == 'E')
     OR NVL(D.RAIL_40FT_FUEL_SCG_ADJ_AMT, 0) = 0
  #end
  #if (${adjustment_cd} == 'N')
     OR NVL(D.RAIL_40FT_FUEL_SCG_ADJ_AMT, 0) <> 0
  #end
#end
     --Truck Basic 20' 
#if (${cost_factor_cd} == 'ALL' || ${truckBasic20} == 'Y')
  #if (${adjustment_cd} == 'E')
     OR NVL(D.TRK_20FT_BZC_COST_ADJ_AMT, 0) = 0
  #end
  #if (${adjustment_cd} == 'N')
     OR NVL(D.TRK_20FT_BZC_COST_ADJ_AMT, 0) <> 0
  #end
#end
