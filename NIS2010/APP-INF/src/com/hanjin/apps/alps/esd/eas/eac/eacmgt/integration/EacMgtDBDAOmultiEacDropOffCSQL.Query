<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOmultiEacDropOffCSQL">
			<desc><![CDATA[Drop Off Charge를 EAC로 I/F한다.]]></desc>
			<sql><![CDATA[
INSERT INTO EAS_DRP_OFF_CHG_CHK(
 BKG_NO
,CNTR_NO
,MTY_RTN_YD_CD
,EAC_NO
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
)
VALUES (
 @[bkg_no]
,@[cntr_no]
,@[mty_rtn_yd_cd]
,@[eac_no]
,NVL(@[usr_id], 'DROPOFF')
,SYSDATE
,NVL(@[usr_id], 'DROPOFF')
,SYSDATE
)
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="mty_rtn_yd_cd" type="12" value="" out="N"/>
				<param name="eac_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
