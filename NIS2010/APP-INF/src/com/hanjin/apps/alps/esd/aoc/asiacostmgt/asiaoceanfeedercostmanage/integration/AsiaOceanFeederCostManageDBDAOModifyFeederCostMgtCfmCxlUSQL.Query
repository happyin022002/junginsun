<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AsiaOceanFeederCostManageDBDAOModifyFeederCostMgtCfmCxlUSQL">
			<desc><![CDATA[modifyFeederCostMgtCfmCxl]]></desc>
			<sql><![CDATA[
UPDATE  AOC_CHN_FDR_TRF_HDR
SET     COST_TRF_STS_CD = 'U'
      , UPD_USR_ID      = @[upd_usr_id]
      , UPD_DT          = SYSDATE
      , LOCL_UPD_DT     = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd])
      , EFF_FM_DT       = NULL
      , EFF_TO_DT       = NULL
WHERE   COST_TRF_STS_CD = 'C'
AND     COST_TRF_NO = @[in_cost_trf_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="in_cost_trf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
