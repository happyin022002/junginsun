<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OndockRailChargeInvoiceManageDBDAOSearchOndockRailChargeCostCalculationListNewRSQL">
			<desc><![CDATA[SearchOndockRailChargeCostCalculationListNew]]></desc>
			<sql><![CDATA[
SELECT I.COST_CODE                                                          LGS_COST_CD,
		 I.TPSZ                                                               CNTR_TPSZ_CD,
		 I.COST_CODE                                                          LGS_COST_CD2,
		 I.TPSZ                                                               CNTR_TPSZ_CD2,
		 I.IO                                                                 IO_BND_CD,
		 I.TML_WRK_DY_CD                                                      TML_WRK_DY_CD,
		 I.FM                                                                 CNTR_STY_CD,
		 I.DCGO_CLSS_CD                                                                 DCGO_IND_CD,
		 SUM(DECODE(SIGN(TO_TR_VOL_VAL-CAL_VOL),-1,CAL_VOL-TO_TR_VOL_VAL,CAL_VOL)) CALC_VOL_QTY,
		 SUM(DECODE(SIGN(TO_TR_VOL_VAL-CAL_VOL),-1,CAL_VOL-TO_TR_VOL_VAL,CAL_VOL)) RVIS_VOL_QTY,
		 D.TML_AGMT_VOL_UT_CD                                                 VOL_TR_UT_CD,
		 DECODE(D.TML_AGMT_VOL_UT_CD,'C',C.AGMT_RT,D.AGMT_UT_RT)              CTRT_RT,
		 SUM(DECODE(SIGN(TO_TR_VOL_VAL-CAL_VOL),-1,CAL_VOL-TO_TR_VOL_VAL,CAL_VOL)) *
		   DECODE(D.TML_AGMT_VOL_UT_CD,'C',C.AGMT_RT,D.AGMT_UT_RT)            INV_AMT,
		 SUM(DECODE(SIGN(TO_TR_VOL_VAL-CAL_VOL),-1,CAL_VOL-TO_TR_VOL_VAL,CAL_VOL)) *
		   DECODE(D.TML_AGMT_VOL_UT_CD,'C',C.AGMT_RT,D.AGMT_UT_RT)            CALC_AMT,
		 D.FM_TR_VOL_VAL                                                      FM_TR_VOL_VAL,
		 D.TO_TR_VOL_VAL                                                      TO_TR_VOL_VAL,
		 D.FM_TR_VOL_VAL||' ~ '||D.TO_TR_VOL_VAL                              TIER,
--		20061208 삭제 On-Dock은 VVD별 data는 의미없음.
--		         I.VSL_CD                                                             VSL_CD,
--		         I.VOY_NO                                                             SKD_VOY_NO,
--		         I.DIR_CD                                                             SKD_DIR_CD,
--		         I.VSL_CD||I.VOY_NO||I.DIR_CD                                         VVD_NO,
		 'ON'                                                                 CALC_COST_GRP_CD,
		 'A'                                                                  CALC_TP_CD,
		 L.ACCT_CD                                                            ACCT_CD,
		 ''                                                                   CALC_RMK,
		 ''                                                                   TML_CRR_CD,
		 ''                                                                   N3PTY_FLG,
		 @[tml_so_ofc_cty_cd]                                                   			        TML_SO_OFC_CTY_CD,
		 @[tml_so_seq]                                                          		    TML_SO_SEQ,
		 0                                                                    TML_SO_DTL_SEQ,
		 CTY              													TML_AGMT_OFC_CTY_CD,
		 SEQ              													TML_AGMT_SEQ,
		 NO               													TML_AGMT_VER_NO,
		 D.CURR_CD                                                            CURR_CD,
		 '1'                                                            		INV_XCH_RT,
		 DECODE(@[curr_cd],D.CURR_CD,'Y','N')                                  		CURR_CHK
  FROM ( SELECT DECODE(L.CNTR_STY_CD,'F',DECODE(SUBSTR(C.COST_CODE,6,1),'F',C.COST_CODE),
				  DECODE(SUBSTR(C.COST_CODE,6,1),'M',C.COST_CODE)) COST_CODE,
				L.CNTR_TPSZ_CD   TPSZ,
				L.WRK_DT		   WRK_DT,
				DECODE(TO_CHAR(L.WRK_DT,'DY'),'SAT','SA','SUN','SU','HOL','HO','WD') TML_WRK_DY_CD,
				L.CNTR_STY_CD    FM,
				L.DCGO_CLSS_CD   DCGO_CLSS_CD,
				COUNT(CNTR_NO)   CAL_VOL,
				L.RC_FLG   	   RC_FLG,
				L.IO_BND_CD      IO,
				CTY              CTY,
				SEQ              SEQ,
				NO               NO --,
--		20061208 삭제 On-Dock은 VVD별 data는 의미없음.
--		                L.VSL_CD         VSL_CD,
--		                L.SKD_VOY_NO     VOY_NO,
--		                L.SKD_DIR_CD     DIR_CD
		 FROM   TES_TML_SO_CNTR_LIST L,
				( SELECT DISTINCT DECODE(1,0,C.LGS_COST_CD,DECODE(C.LGS_COST_CD,CD,TP,C.LGS_COST_CD)) COST_CODE,
						 CTY,
						 SEQ,
						 NO
				  FROM ( SELECT COUNT(T.LGS_COST_CD) CNT,
								T.LGS_COST_CD TP,
								T.THRP_LGS_COST_CD CD,
								H.TML_AGMT_OFC_CTY_CD CTY,
								H.TML_AGMT_SEQ SEQ,
								H.TML_AGMT_VER_NO NO
						 FROM   TES_TML_AGMT_HDR H, TES_TML_AGMT_THRP_COST T
						 WHERE  H.YD_CD            = @[yd_cd]
						 AND    H.VNDR_SEQ         = @[vndr_seq]
						 AND    H.TML_AGMT_STS_CD = 'C'
						 AND    H.DELT_FLG        = 'N'
						 AND    TO_CHAR(H.EFF_FM_DT,'YYYYMMDD') <= REPLACE(@[min_wrk_dt],'-')
						 AND    TO_CHAR(H.EFF_TO_DT,'YYYYMMDD') >= REPLACE(@[max_wrk_dt],'-')
						 AND    H.TML_AGMT_VER_NO = ( SELECT MAX(M.TML_AGMT_VER_NO)
													  FROM   TES_TML_AGMT_HDR M
													  WHERE  M.YD_CD               = @[yd_cd]
													  AND    M.VNDR_SEQ            = @[vndr_seq]
													  AND    M.TML_AGMT_STS_CD     = 'C'
													  AND    M.DELT_FLG            = 'N'
													  AND    TO_CHAR(M.EFF_FM_DT,'YYYYMMDD') <= REPLACE(@[min_wrk_dt],'-')
													  AND    TO_CHAR(M.EFF_TO_DT,'YYYYMMDD') >= REPLACE(@[max_wrk_dt],'-') )
						 AND    H.TML_AGMT_OFC_CTY_CD = T.TML_AGMT_OFC_CTY_CD(+)
						 AND    H.TML_AGMT_SEQ        = T.TML_AGMT_SEQ(+)
						 AND    H.TML_AGMT_VER_NO     = T.TML_AGMT_VER_NO(+)
						 GROUP BY H.TML_AGMT_OFC_CTY_CD, H.TML_AGMT_SEQ, H.TML_AGMT_VER_NO,
								  T.LGS_COST_CD, T.THRP_LGS_COST_CD ) A, TES_TML_SO_COST C
				  WHERE  C.COST_CALC_MZD_CD  = 'A'
				  AND    C.ODCK_RAIL_CHG_FLG = 'Y'
				  AND    DECODE(A.CNT,0,DECODE(SUBSTR(C.LGS_COST_CD,1,2),'TP','N','Y'),'Y') = 'Y' ) C
		 WHERE  VRFY_RSLT_IND_CD    = 'CO'
		 AND    L.TML_SO_OFC_CTY_CD = @[tml_so_ofc_cty_cd]
		 AND    L.TML_SO_SEQ        = @[tml_so_seq]
		 AND    DECODE(L.CNTR_STY_CD,'F','F','M') = SUBSTR(C.COST_CODE,6,1)
		 GROUP BY C.COST_CODE, CNTR_TPSZ_CD, L.WRK_DT, DECODE(TO_CHAR(L.WRK_DT,'DY'),'SAT','SA','SUN','SU','HOL','HO','WD'),
				  CNTR_STY_CD, L.DCGO_CLSS_CD, L.IO_BND_CD, L.RC_FLG, CTY, SEQ, NO ) I,
		 TES_TML_AGMT_DTL D, TES_TML_AGMT_TP_SZ C, TES_TML_AGMT_APLY_DY A, TES_LGS_COST L, TES_TML_AGMT_DG_CGO_CLSS G
  WHERE  I.CTY       = D.TML_AGMT_OFC_CTY_CD
  AND    I.SEQ       = D.TML_AGMT_SEQ
  AND    I.NO        = D.TML_AGMT_VER_NO
  AND    I.COST_CODE = D.LGS_COST_CD
  AND    D.TML_AGMT_TP_CD     = 'T'
  AND    D.AUTO_CALC_FLG      = 'Y'
  AND    NVL(D.IO_BND_CD,'N')  = DECODE(NVL(D.IO_BND_CD,'N'),'N','N','S','S',I.IO)
  AND    TO_NUMBER(NVL(D.FM_TR_VOL_VAL,'1')) 	    <= CAL_VOL
  AND    TO_NUMBER(NVL(D.TO_TR_VOL_VAL,'9999999'))  >= CAL_VOL
  AND    D.TML_AGMT_OFC_CTY_CD = C.TML_AGMT_OFC_CTY_CD(+)
  AND    D.TML_AGMT_SEQ        = C.TML_AGMT_SEQ(+)
  AND    D.TML_AGMT_VER_NO     = C.TML_AGMT_VER_NO(+)
  AND    D.TML_AGMT_DTL_SEQ    = C.TML_AGMT_DTL_SEQ(+)
  AND    DECODE(D.TML_AGMT_VOL_UT_CD,'C',C.CNTR_TPSZ_CD,'N')
			= DECODE(D.TML_AGMT_VOL_UT_CD,'C',DECODE(I.RC_FLG,'Y',DECODE(SUBSTR(I.TPSZ,1,1),'R','D'||SUBSTR(I.TPSZ,2,1),I.TPSZ),I.TPSZ),'N')
  AND    D.TML_AGMT_OFC_CTY_CD  = A.TML_AGMT_OFC_CTY_CD(+)
  AND    D.TML_AGMT_SEQ         = A.TML_AGMT_SEQ(+)
  AND    D.TML_AGMT_VER_NO      = A.TML_AGMT_VER_NO(+)
  AND    D.TML_AGMT_DTL_SEQ     = A.TML_AGMT_DTL_SEQ(+)
  AND    ( DECODE(D.TML_DY_APLY_TP_CD,'P',I.TML_WRK_DY_CD,'S') = DECODE(D.TML_DY_APLY_TP_CD,'P',DECODE(A.WKDY_FLG,'Y','WD'),'S')
  OR       DECODE(D.TML_DY_APLY_TP_CD,'P',I.TML_WRK_DY_CD,'S') = DECODE(D.TML_DY_APLY_TP_CD,'P',DECODE(A.SAT_FLG,'Y','SA'),'S')
  OR       DECODE(D.TML_DY_APLY_TP_CD,'P',I.TML_WRK_DY_CD,'S') = DECODE(D.TML_DY_APLY_TP_CD,'P',DECODE(A.SUN_FLG,'Y','SU'),'S')
  OR       DECODE(D.TML_DY_APLY_TP_CD,'P',I.TML_WRK_DY_CD,'S') = DECODE(D.TML_DY_APLY_TP_CD,'P',DECODE(A.HOL_FLG,'Y','HO'),'S') )
  AND    D.TML_AGMT_OFC_CTY_CD = G.TML_AGMT_OFC_CTY_CD(+)
  AND    D.TML_AGMT_SEQ        = G.TML_AGMT_SEQ(+)
  AND    D.TML_AGMT_VER_NO     = G.TML_AGMT_VER_NO(+)
--		   -- 수정(20070109)
   AND    D.TML_AGMT_DTL_SEQ   = G.TML_AGMT_DTL_SEQ(+)
   AND    DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'N','N',G.DCGO_APLY_TP_CD) = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'N','N','D')
   AND    DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'A',DECODE(I.DCGO_CLSS_CD,'N','N',G.DCGO_SAM_CLSS_FLG),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'A',DECODE(I.DCGO_CLSS_CD,'N','N','Y'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'A',DECODE(I.DCGO_CLSS_CD,'N',G.DCGO_NON_CLSS_FLG,'N'),'S',DECODE(I.DCGO_CLSS_CD,'N',G.DCGO_NON_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'A',DECODE(I.DCGO_CLSS_CD,'N','Y','N'),'S',DECODE(I.DCGO_CLSS_CD,'N','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'1',G.DCGO_N1ST_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'1','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'2',G.DCGO_N2ND_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'2','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'3',G.DCGO_N3RD_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'3','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'4',G.DCGO_N4TH_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'4','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'5',G.DCGO_N5TH_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'5','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'6',G.DCGO_N6TH_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'6','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'7',G.DCGO_N7TH_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'7','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'8',G.DCGO_N8TH_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'8','Y','N'),'N')
   AND     DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'9',G.DCGO_N9TH_CLSS_FLG,'N'),'N')
		  = DECODE(NVL(D.DCGO_APLY_TP_CD,'N'),'S',DECODE(I.DCGO_CLSS_CD,'9','Y','N'),'N')

  AND    I.COST_CODE = L.LGS_COST_CD(+)
  GROUP BY I.COST_CODE, I.TPSZ, I.IO, I.TML_WRK_DY_CD, I.FM, I.DCGO_CLSS_CD, D.FM_TR_VOL_VAL, D.TO_TR_VOL_VAL,
		   D.TML_AGMT_VOL_UT_CD, DECODE(D.TML_AGMT_VOL_UT_CD,'C',C.AGMT_RT,D.AGMT_UT_RT),
		   L.ACCT_CD, CTY, SEQ, NO, D.CURR_CD			]]></sql>
			<params>
				<param name="tml_so_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_so_seq" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="min_wrk_dt" type="12" value="" out="N"/>
				<param name="max_wrk_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
