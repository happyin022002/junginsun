<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustomerNominatedTruckerRgstDAOSearchSpNameByScacCdRSQL">
			<desc><![CDATA[SCAC CD로 S/P 정보 조회]]></desc>
			<sql><![CDATA[
 SELECT A.VNDR_SEQ
       ,A.VNDR_LGL_ENG_NM AS VNDR_NM
       ,A.USA_EDI_CD
  FROM MDM_VENDOR A
 WHERE 1=1
   AND A.USA_EDI_CD = @[usa_edi_cd]
   AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="usa_edi_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
