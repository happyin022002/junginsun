<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TerminalAgreementManageDBDAOSearchMaxAgmtVerRSQL">
			<desc><![CDATA[Agreement Ver No Max값 조회]]></desc>
			<sql><![CDATA[
SELECT  LPAD(MAX(tml_agmt_ver_no), 4, '0') tml_agmt_ver_no 
FROM    TES_TML_AGMT_HDR
WHERE   tml_agmt_ofc_cty_cd = @[tml_agmt_ofc_cty_cd]
AND     tml_agmt_seq		= @[tml_agmt_seq]
GROUP BY tml_agmt_ver_no			]]></sql>
			<params>
				<param name="tml_agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="tml_agmt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
