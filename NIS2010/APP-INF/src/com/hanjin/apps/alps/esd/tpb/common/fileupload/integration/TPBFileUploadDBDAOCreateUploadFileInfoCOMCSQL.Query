		<query name="TPBFileUploadDBDAOCreateUploadFileInfoCOMRSQL">
			<desc><![CDATA[CreateUploadFileInfoCOM]]></desc>
			<sql><![CDATA[
INSERT INTO COM_UPLD_FILE (
    FILE_SAV_ID,
    FILE_UPLD_NM,
    FILE_SZ_CAPA,
    FILE_PATH_URL,
    PGM_SUB_SYS_CD,
    DELT_FLG,
    CRE_USR_ID,
    CRE_DT,
    UPD_USR_ID,
    UPD_DT
) VALUES (
    @[s_file_phys_nm],
    REPLACE(@[s_file_lgc_nm],'''',''),
    '100000',
    @[s_file_path_nm],
    'TPB',
    'N',
    @[s_cre_usr_id],
    SYSDATE,
    @[s_cre_usr_id],
    SYSDATE
)			]]></sql>
			<params>
				<param name="s_file_phys_nm" type="12" value="" out="N"/>
				<param name="s_file_lgc_nm" type="12" value="" out="N"/>
				<param name="s_file_path_nm" type="12" value="" out="N"/>
				<param name="s_cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
