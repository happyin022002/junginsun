<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccrualBatchExecuteResultDBDAOAddAccrualAdjustmentCSQL">
			<desc><![CDATA[조정계수를 입력한다.]]></desc>
			<sql><![CDATA[
INSERT INTO LEA_ACCL_ADJ_FCTR (
	EXE_YRMON				,                      
	REV_YRMON               ,       
	ACCL_LGC_TP_CD          ,       
	ACCL_ADJ_FCTR_RT        ,       
	ACCL_ADJ_FCTR_CFM_FLG   ,      
	ACCL_ADJ_FCTR_APLY_PROC_CD	,
	DELT_FLG                ,       
	CRE_USR_ID              ,       
	CRE_DT                  ,       
	UPD_USR_ID              ,       
	UPD_DT               	,          
    ACCL_ADJ_FCTR_FNL_FLG
)
SELECT
 REPLACE(@[exe_yrmon],'-','') EXE_YRMON
,REPLACE(@[rev_yrmon],'-','') REV_YRMON
,@[accl_lgc_tp_cd] ACCL_LGC_TP_CD
,@[accl_adj_fctr_rt] ACCL_ADJ_FCTR_RT
,@[accl_adj_fctr_cfm_flg] ACCL_ADJ_FCTR_CFM_FLG
,'N' ACCL_ADJ_FCTR_APLY_PROC_CD
,NVL(@[delt_flg],'N') DELT_FLG
,@[cre_usr_id] CRE_USR_ID
,SYSDATE CRE_DT
,@[upd_usr_id] UPD_USR_ID
,SYSDATE UPD_DT
,'N' ACCL_ADJ_FCTR_FNL_FLG
FROM DUAL			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="accl_lgc_tp_cd" type="12" value="" out="N"/>
				<param name="accl_adj_fctr_rt" type="12" value="" out="N"/>
				<param name="accl_adj_fctr_cfm_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
