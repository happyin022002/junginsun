<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetBkgRouteSccCdRSQL">
			<desc><![CDATA[GetBkgRouteSccCd]]></desc>
			<sql><![CDATA[
SELECT (SELECT SCC_CD FROM MDM_LOCATION WHERE LOC_CD = A.POD_CD) POD_SCC_CD
     , (SELECT SCC_CD FROM MDM_LOCATION WHERE LOC_CD = A.DEL_CD) DEL_SCC_CD
  FROM BKG_BOOKING A
 WHERE BKG_NO = @[bkg_no]
			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
