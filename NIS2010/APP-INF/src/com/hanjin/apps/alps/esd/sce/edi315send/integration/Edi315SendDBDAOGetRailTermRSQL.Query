<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Edi315SendDBDAOGetRailTermRSQL">
			<desc><![CDATA[GetRailTerm]]></desc>
			<sql><![CDATA[
SELECT SUM(rail_cnt) AS rail_cnt, SUM(oan_term_seq) AS oan_term_seq
  FROM(
       SELECT COUNT(*) rail_cnt, 0 as oan_term_seq
         FROM sce_cop_dtl a, sce_cop_hdr b
        WHERE a.cop_no = b.cop_no
          AND b.cop_no = @[cop_no]
          AND cop_dtl_seq>6000
          AND act_cd LIKE '__R%'
          AND (pod_nod_cd LIKE 'US%' OR pod_nod_cd LIKE 'CA%')
        UNION ALL
       SELECT 0 AS rail_cnt, COP_DTL_SEQ AS oan_term_seq
         FROM sce_cop_dtl
        WHERE cop_no = @[cop_no]
          AND cop_dtl_seq>6000
          AND stnd_edi_sts_cd = 'OAN'
          AND ACT_STS_MAPG_CD = 'ID'
      )			]]></sql>
			<params>
				<param name="cop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
