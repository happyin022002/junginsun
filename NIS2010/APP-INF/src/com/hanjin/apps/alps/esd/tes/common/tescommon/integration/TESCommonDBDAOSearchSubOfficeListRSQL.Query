<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TESCommonDBDAOSearchSubOfficeListRSQL">
			<desc><![CDATA[SearchSubOfficeListRSQL]]></desc>
			<sql><![CDATA[
SELECT 
    (SELECT LTRIM(MAX(SYS_CONNECT_BY_PATH(OFC_CD,',')),',')
    FROM (
          SELECT ROWNUM ROW_ID,OFC_CD 
          FROM MDM_ORGANIZATION
          WHERE DELT_FLG = 'N'
          CONNECT BY NOCYCLE PRIOR OFC_CD = PRNT_OFC_CD
          START WITH OFC_CD = @[ofc_cd]
         )
  CONNECT BY PRIOR ROW_ID = ROW_ID - 1
    START WITH ROW_ID = 1    
    ) SUB_OFC_CD
FROM DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
