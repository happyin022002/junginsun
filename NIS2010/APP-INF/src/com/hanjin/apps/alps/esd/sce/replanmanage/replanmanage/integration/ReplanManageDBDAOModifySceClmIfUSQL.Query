<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReplanManageDBDAOModifySceClmIfUSQL">
			<desc><![CDATA[대상을 UPDATE]]></desc>
			<sql><![CDATA[
UPDATE SCE_CLM_IF
   SET SO_MAPG_STS_CD = '00'
   WHERE CNTR_NO = @[cntr_no]
   AND CNMV_YR =  @[cnmv_yr]
   AND CNMV_ID_NO = @[cnmv_id_no]
   AND CLM_SEQ = @[clm_seq]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
				<param name="clm_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
