<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DistanceCreationDBDAOmultiTrsAgmtDistUSQL">
			<desc><![CDATA[multiTrsAgmtDist]]></desc>
			<sql><![CDATA[
UPDATE TRS_AGMT_DIST  SET                                              
dist_meas_ut_cd     =@[dist_meas_ut_cd],                                             
bzc_dist            =@[bzc_dist],                                             
conv_meas_ut_cd     =@[conv_meas_ut_cd],                                             
conv_dist           =@[conv_dist],                                             
fm_nod_zip_cd_ctnt  =@[fm_nod_zip_cd_ctnt], 					
to_nod_zip_cd_ctnt  =@[to_nod_zip_cd_ctnt],          
upd_usr_id          = @[upd_usr_id],                                             
upd_dt              = SYSDATE
 WHERE fm_nod_cd = @[fm_nod_cd]                                                    
 AND   to_nod_cd = @[to_nod_cd]                                                    
 AND   HJL_NO IS NULL			]]></sql>
			<params>
				<param name="dist_meas_ut_cd" type="12" value="" out="N"/>
				<param name="bzc_dist" type="12" value="" out="N"/>
				<param name="conv_meas_ut_cd" type="12" value="" out="N"/>
				<param name="conv_dist" type="12" value="" out="N"/>
				<param name="fm_nod_zip_cd_ctnt" type="12" value="" out="N"/>
				<param name="to_nod_zip_cd_ctnt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="fm_nod_cd" type="12" value="" out="N"/>
				<param name="to_nod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
