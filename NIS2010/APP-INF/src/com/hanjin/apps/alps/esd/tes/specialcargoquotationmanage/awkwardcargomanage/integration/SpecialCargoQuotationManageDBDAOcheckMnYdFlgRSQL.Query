<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SpecialCargoQuotationManageDBDAOcheckMnYdFlgRSQL">
			<desc><![CDATA[checkMnYdFlg]]></desc>
			<sql><![CDATA[
SELECT YD_CD
FROM   TES_AWK_CGO_TRF_HDR
WHERE  YD_CD LIKE  @[yd_cd]||'%' --5자리 Port Code
AND    MN_YD_FLG = 'Y'			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
