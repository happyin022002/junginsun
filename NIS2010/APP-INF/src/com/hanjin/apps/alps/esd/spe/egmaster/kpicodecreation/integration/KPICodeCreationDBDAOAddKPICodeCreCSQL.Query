<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="KPICodeCreationDBDAOAddKPICodeCreCSQL">
			<desc><![CDATA[KPI Code Creation 데이터를 입력 한다]]></desc>
			<sql><![CDATA[
INSERT INTO SPE_SP_SVC_CATE_KPI(SP_KPI_ID
                              , EV_SVC_CATE_CD
                              , SP_KPI_NM
                              , SP_KPI_DESC
                              , SP_KPI_TP_CD
                              , DELT_FLG
                              , CRE_USR_ID
                              , CRE_DT
                              , UPD_USR_ID
                              , UPD_DT
                              )VALUES(
                                @[ev_svc_cate_cd]||(SELECT LPAD(COUNT(1)+1,3,0) FROM SPE_SP_SVC_CATE_KPI WHERE EV_SVC_CATE_CD LIKE @[ev_svc_cate_cd]||'%')
                              , @[ev_svc_cate_cd]
                              , @[sp_kpi_nm]
                              , @[sp_kpi_desc]
                              , @[sp_kpi_tp_cd]
                              , 'N'
                              , @[cre_usr_id]
                              , SYSDATE
                              , @[upd_usr_id]
                              , SYSDATE
                              )			]]></sql>
			<params>
				<param name="ev_svc_cate_cd" type="12" value="" out="N"/>
				<param name="sp_kpi_nm" type="12" value="" out="N"/>
				<param name="sp_kpi_desc" type="12" value="" out="N"/>
				<param name="sp_kpi_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
