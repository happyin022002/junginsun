<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EacMgtDBDAOAddAutoAuditFileNoRSQL">
			<desc><![CDATA[EAS_ATCH_FILE 저장시 사용 할 ATCH_FILE_LNK_ID 채번]]></desc>
			<sql><![CDATA[
SELECT @[mdl_tp_cd]||LPAD(TO_CHAR(NVL((
        SELECT /*+ INDEX_DESC(A XPKEAS_ATCH_FILE) */
               TO_NUMBER(SUBSTR(ATCH_FILE_LNK_ID,4)) AS ATCH_FILE_LNK_ID
          FROM EAS_ATCH_FILE A
         WHERE ATCH_FILE_LNK_ID LIKE @[mdl_tp_cd]||'%'
           AND ROWNUM = 1
       ),0)+1),7,'0') ATCH_FILE_LNK_ID
  FROM DUAL
			]]></sql>
			<params>
				<param name="mdl_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
