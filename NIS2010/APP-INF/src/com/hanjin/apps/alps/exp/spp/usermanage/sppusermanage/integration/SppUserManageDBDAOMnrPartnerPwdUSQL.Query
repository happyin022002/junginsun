<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SppUserManageDBDAOMnrPartnerPwdUSQL">
			<desc><![CDATA[mnr user change password]]></desc>
			<sql><![CDATA[
update mnr_partner
set 
	sp_ptal_pwd             =	@[sp_ptal_pwd]         ,
	upd_usr_id              =	@[upd_usr_id]           ,
	upd_dt                  =	sysdate                 
where mnr_prnr_cre_seq			=	(select mnr_prnr_cre_seq from mnr_partner where sp_ptal_id = @[sp_ptal_id])			]]></sql>
			<params>
				<param name="sp_ptal_pwd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="sp_ptal_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
