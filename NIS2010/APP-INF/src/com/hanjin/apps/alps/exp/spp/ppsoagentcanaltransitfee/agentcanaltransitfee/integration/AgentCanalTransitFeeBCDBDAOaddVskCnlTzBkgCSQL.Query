<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgentCanalTransitFeeBCDBDAOaddVskCnlTzBkgCSQL">
			<desc><![CDATA[AgentCanalTransitFeeBCDBDAOaddVskCnlTzBkg]]></desc>
			<sql><![CDATA[
INSERT INTO VSK_CNL_TZ_BKG(
	CNL_TZ_BKG_YRMON
	,VSL_CD
	,SKD_VOY_NO
	,SKD_DIR_CD
	,CNL_PORT_CD
	,SVC_SCP_BND_CD
	,CNL_TZ_BKG_PRD_CD
	,CNL_TZ_BKG_PROC_STS_CD
	,CNL_BKG_TZ_DT
	,CNL_OT_SVC_APRO_FLG
	,CNL_OT_SVC_ARR_DT
	,CNL_BKG_AMT
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
)VALUES(
	TO_CHAR(TO_DATE(@[cnl_tz_bkg_yrmon],'YYYY-MM-DD HH24:MI'),'YYYYMM')
	,@[vsl_cd]
	,@[skd_voy_no]
	,@[skd_dir_cd]
	,'PAPAC'
	,DECODE(@[svc_scp_bnd_cd],'South','S','North','N','West','W','East','E','')
	,@[cnl_tz_bkg_prd_cd]
	,@[cnl_tz_bkg_proc_sts_cd]
	,TO_DATE(@[cnl_bkg_tz_dt],'YYYY/MM/DD HH24:MI')
	,@[cnl_ot_svc_apro_flg]
	,TO_DATE(@[cnl_ot_svc_arr_dt],'YYYY/MM/DD HH24:MI')
	,@[cnl_bkg_amt]
	,@[cre_usr_id]
	,SYSDATE
	,@[upd_usr_id]
	,SYSDATE
)			]]></sql>
			<params>
				<param name="cnl_tz_bkg_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="svc_scp_bnd_cd" type="12" value="" out="N"/>
				<param name="cnl_tz_bkg_prd_cd" type="12" value="" out="N"/>
				<param name="cnl_tz_bkg_proc_sts_cd" type="12" value="" out="N"/>
				<param name="cnl_bkg_tz_dt" type="12" value="" out="N"/>
				<param name="cnl_ot_svc_apro_flg" type="12" value="" out="N"/>
				<param name="cnl_ot_svc_arr_dt" type="12" value="" out="N"/>
				<param name="cnl_bkg_amt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
