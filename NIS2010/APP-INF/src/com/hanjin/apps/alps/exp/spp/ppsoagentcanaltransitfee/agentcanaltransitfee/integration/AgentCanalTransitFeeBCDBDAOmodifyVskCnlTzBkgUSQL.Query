<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgentCanalTransitFeeBCDBDAOmodifyVskCnlTzBkgUSQL">
			<desc><![CDATA[AgentCanalTransitFeeBCDBDAOmodifyVskCnlTzBkg]]></desc>
			<sql><![CDATA[
UPDATE VSK_CNL_TZ_BKG
SET CNL_TZ_BKG_PRD_CD = @[cnl_tz_bkg_prd_cd]
	,CNL_TZ_BKG_PROC_STS_CD = @[cnl_tz_bkg_proc_sts_cd]
	,CNL_BKG_TZ_DT = TO_DATE(@[cnl_bkg_tz_dt],'YYYY/MM/DD HH24:MI')
	,CNL_OT_SVC_APRO_FLG = @[cnl_ot_svc_apro_flg]
	,CNL_OT_SVC_ARR_DT = TO_DATE(@[cnl_ot_svc_arr_dt],'YYYY/MM/DD HH24:MI')
	,CNL_BKG_AMT = @[cnl_bkg_amt]
	,UPD_USR_ID = @[upd_usr_id]
	,UPD_DT = sysdate
WHERE 1=1
AND CNL_TZ_BKG_YRMON = TO_CHAR(TO_DATE(@[cnl_tz_bkg_yrmon],'YYYY-MM-DD HH24:MI'),'YYYYMM')
AND VSL_CD = @[vsl_cd]
AND SKD_VOY_NO = @[skd_voy_no]
AND SKD_DIR_CD = @[skd_dir_cd]
AND CNL_PORT_CD = 'PAPAC'			]]></sql>
			<params>
				<param name="cnl_tz_bkg_prd_cd" type="12" value="" out="N"/>
				<param name="cnl_tz_bkg_proc_sts_cd" type="12" value="" out="N"/>
				<param name="cnl_bkg_tz_dt" type="12" value="" out="N"/>
				<param name="cnl_ot_svc_apro_flg" type="12" value="" out="N"/>
				<param name="cnl_ot_svc_arr_dt" type="12" value="" out="N"/>
				<param name="cnl_bkg_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cnl_tz_bkg_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
