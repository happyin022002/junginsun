<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOAddChargeDeletionPathSetupCSQL">
			<desc><![CDATA[ChargeCalculationDBDAOAddChargeDeletionPathSetupCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_CHG_DELT_PATH_STUP(
	 CHG_DELT_PATH_STUP_SEQ 
	,EFF_DT
	,EXP_DT
	,CHG_DELT_RHQ_CD
	,DMDT_BRNC_OFC_OP_MGR_FLG
	,DMDT_BRNC_FLG
	,DMDT_RHQ_FLG
	,DMDT_HO_FLG
	,CRE_USR_ID
	,CRE_DT
	,CRE_OFC_CD
	,UPD_USR_ID
	,UPD_DT
	,UPD_OFC_CD
)
VALUES(
	 @[chg_delt_path_stup_seq]
	,TO_DATE(@[eff_dt], 'YYYYMMDD') + .0
	,TO_DATE(@[exp_dt], 'YYYYMMDD') + .99999
	,@[chg_delt_rhq_cd]
	,DECODE(@[dmdt_brnc_ofc_op_mgr_flg], '0', 'N', '1', 'Y')
	,DECODE(@[dmdt_brnc_flg], '0', 'N', '1', 'Y')
	,DECODE(@[dmdt_rhq_flg],  '0', 'N', '1', 'Y')
	,DECODE(@[dmdt_ho_flg],   '0', 'N', '1', 'Y')
	,@[cre_usr_id]
	,NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
	,@[cre_ofc_cd]
	,@[upd_usr_id]
	,NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), SYSDATE)
	,@[upd_ofc_cd]
)			]]></sql>
			<params>
				<param name="chg_delt_path_stup_seq" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="chg_delt_rhq_cd" type="12" value="" out="N"/>
				<param name="dmdt_brnc_ofc_op_mgr_flg" type="12" value="" out="N"/>
				<param name="dmdt_brnc_flg" type="12" value="" out="N"/>
				<param name="dmdt_rhq_flg" type="12" value="" out="N"/>
				<param name="dmdt_ho_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
