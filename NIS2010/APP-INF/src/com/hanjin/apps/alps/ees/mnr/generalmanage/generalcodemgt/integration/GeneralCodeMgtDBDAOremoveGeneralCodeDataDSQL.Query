<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeMgtDBDAOremoveGeneralCodeDataDSQL">
			<desc><![CDATA[삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MNR_GEN_CD A
WHERE   1=1
   AND  (
        MNR_CD_SEQ = @[mnr_cd_seq]
        )

#if (${mnr_cd_grp_no} == '1') 
   OR   (
        A.MNR_CD_GRP_NO = '2'
        AND @[mnr_cd_id] = A.PRNT_CD_ID
        )
   OR   (
        A.MNR_CD_GRP_NO = '3'
        AND A.PRNT_CD_ID IN ( SELECT MNR_CD_ID
                               FROM MNR_GEN_CD
                              WHERE MNR_CD_GRP_NO = '2'
                                AND PRNT_CD_ID = @[mnr_cd_id]
                                 )
        )
#elseif(${mnr_cd_grp_no} == '2')
   OR   (
        A.MNR_CD_GRP_NO = '3'
        AND @[mnr_cd_id] = A.PRNT_CD_ID
        )

#end
			]]></sql>
			<params>
				<param name="mnr_cd_seq" type="2" value="1" out="N"/>
				<param name="mnr_cd_id" type="12" value="2" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
