<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOSearchCHSCpsAgreementAgmtSeqDataRSQL">
			<desc><![CDATA[신규입력시 NP(CP) Agreement No 생성.]]></desc>
			<sql><![CDATA[
SELECT 
	CASE WHEN NVL(MAX(AGMT_SEQ),50000) + 1 = 999998 
	THEN 1000000 
	ELSE NVL(MAX(AGMT_SEQ),50000) + 1 END 	AS AGMT_SEQ
FROM CGM_AGREEMENT 
WHERE AGMT_SEQ NOT IN (999999, 999998)
AND AGMT_SEQ > 50000			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
