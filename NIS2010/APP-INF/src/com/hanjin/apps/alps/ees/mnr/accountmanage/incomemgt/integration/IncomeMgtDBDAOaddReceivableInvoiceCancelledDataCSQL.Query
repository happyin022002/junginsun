<sqls>
	<querys>
		<query name="IncomeMgtDBDAOaddReceivableInvoiceCancelledDataCSQL">
			<desc><![CDATA[IncomeMgtDBDAOaddReceivableInvoiceCancelledDataCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_CXL_RCV_INV_WRK(
	RCV_INV_SEQ
	, CXL_INV_SEQ  
	, MNR_GRP_TP_CD
	, MNR_PRNR_TP_CD
	, MNR_PRNR_CNT_CD
	, MNR_PRNR_SEQ
	, INV_NO
	, MNR_INV_REF_NO
	, MNR_INV_STS_CD
	, CFM_DT
	, CXL_USR_ID 
	, CXL_DT      
	, CURR_CD
	, BZC_AMT
	, VAT_AMT
	, VAT_XCH_RT
	, WHLD_TAX_AMT
	, TTL_AMT
	, INV_XCH_RT
	, INV_PAY_MZD_CD
	, CHK_TRNS_NO
	, GL_DT
	, ISS_DT
	, ISS_OFC_CD
	, INV_DUE_DT
	, BANK_NM
	, BANK_ACCT_NO
	, MNR_SWIFT_NO
	, MNR_BIL_TO_NM
	, MNR_INV_RMK
	, CLT_STL_FLG
	, CHG_CURR_CD
	, CHG_XCH_RT
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
)(
SELECT
	RCV_INV_SEQ
	, (SELECT NVL(MAX(CXL_INV_SEQ), 0)+1 
		FROM MNR_CXL_RCV_INV_WRK
		WHERE RCV_INV_SEQ = @[rcv_inv_seq])
	, MNR_GRP_TP_CD
	, MNR_PRNR_TP_CD
	, MNR_PRNR_CNT_CD
	, MNR_PRNR_SEQ
	, INV_NO
	, MNR_INV_REF_NO
	, MNR_INV_STS_CD
	, CFM_DT
	, @[cre_usr_id]
	, SYSDATE      
	, CURR_CD
	, BZC_AMT
	, VAT_AMT
	, VAT_XCH_RT
	, WHLD_TAX_AMT
	, TTL_AMT
	, INV_XCH_RT
	, INV_PAY_MZD_CD
	, CHK_TRNS_NO
	, GL_DT
	, ISS_DT
	, ISS_OFC_CD
	, INV_DUE_DT
	, BANK_NM
	, BANK_ACCT_NO
	, MNR_SWIFT_NO
	, MNR_BIL_TO_NM
	, MNR_INV_RMK
	, CLT_STL_FLG
	, CHG_CURR_CD
	, CHG_XCH_RT
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
FROM MNR_RCV_INV_WRK
WHERE RCV_INV_SEQ = @[rcv_inv_seq] 
)			]]></sql>
			<params>
				<param name="rcv_inv_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
