<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyReleaseRedeliveryOrderMgtDBDAOSearchBkgAwkCgoforEdiRSQL">
			<desc><![CDATA[BKG_AWK_CGO 테이블에서 EDI로 전송할 정보를 조회한다]]></desc>
			<sql><![CDATA[
SELECT OVR_BKWD_LEN
      ,OVR_FWRD_LEN
      ,OVR_HGT
      ,OVR_LF_LEN
      ,OVR_RT_LEN
      ,OVR_VOID_SLT_QTY
      ,TTL_DIM_LEN
      ,TTL_DIM_WDT
      ,TTL_DIM_HGT
      ,GRS_WGT || WGT_UT_CD OVWGT
      ,STWG_RQST_DESC
  FROM BKG_AWK_CGO
 WHERE BKG_NO = @[bkg_no]
#if (${cntr_no} != '')
   AND CNTR_NO = @[cntr_no]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
