<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOAddChargeDeletionOfficeSetupHistoryCSQL">
			<desc><![CDATA[ChargeCalculationDBDAOAddChargeDeletionOfficeSetupHistoryCSQL]]></desc>
			<sql><![CDATA[
insert into DMT_CHG_DELT_OFC_STUP_HIS
select  CHG_DELT_PATH_STUP_SEQ
       ,@[chg_delt_path_stup_his_seq]
       ,CHG_DELT_OFC_CD
	   ,CRE_USR_ID
	   ,CRE_DT
	   ,CRE_OFC_CD
	   ,@[upd_usr_id]
	   ,nvl(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), sysdate)
	   ,@[upd_ofc_cd]	   
  from  DMT_CHG_DELT_OFC_STUP 
 where  CHG_DELT_PATH_STUP_SEQ = to_number(@[chg_delt_path_stup_seq])			]]></sql>
			<params>
				<param name="chg_delt_path_stup_his_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="chg_delt_path_stup_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
