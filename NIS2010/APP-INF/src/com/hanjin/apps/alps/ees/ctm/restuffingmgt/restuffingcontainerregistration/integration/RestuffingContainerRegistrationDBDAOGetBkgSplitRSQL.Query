<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RestuffingContainerRegistrationDBDAOGetBkgSplitRSQL">
			<desc><![CDATA[BKG_CONTAINER 에서 SPLIT된 부킹을 찾아온다]]></desc>
			<sql><![CDATA[
SELECT BKG.BKG_NO,
       BKG.BL_NO
  FROM BKG_CONTAINER CNTR,
       BKG_BOOKING BKG
 WHERE CNTR_NO = @[cntr_no]
   AND CNMV_CYC_NO = (SELECT MAX (CNMV_CYC_NO)
                        FROM BKG_CONTAINER
                       WHERE CNTR_NO = @[cntr_no]
                         AND CNMV_CYC_NO != '9999')
   AND CNTR.BKG_NO = BKG.BKG_NO			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
