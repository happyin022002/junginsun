<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchWeekDatePeriodRSQL">
			<desc><![CDATA[week date period (start date, to date) 검색]]></desc>
			<sql><![CDATA[
#if(${division} == 'WIDE')
	 -- 해당주의 시작일-시작일+7주
	SELECT 
		WK_ST_DT                     	
	    ,TO_CHAR(TO_DATE(WK_END_DT, 'YYYYMMDD')+49 , 'YYYYMMDD') WK_END_DT   
	FROM 
		EQR_WK_PRD                   		
	WHERE 
		PLN_YR||PLN_WK = @[week]         		
	
#else
	-- 해당주의 시작일-끝날짜
	SELECT 
		WK_ST_DT                     	
	    ,WK_END_DT                     	
	FROM 
		EQR_WK_PRD                   		
	WHERE 
		PLN_YR||PLN_WK = @[week]         		
#end			]]></sql>
			<params>
				<param name="week" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
