<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOSearchMovementListByContainerVORSQL">
			<desc><![CDATA[EES_CTM_0408 : 컨테이너 번호에 의한 Each Container LIST 조회]]></desc>
			<sql><![CDATA[
SELECT CNMV_CYC_NO,
       CNMV_CO_CD,
       MVMT_STS_CD,
       MVMT_CRE_TP_CD,
       ORG_YD_CD,
       ORG_YD_NM,
       DEST_YD_CD,
       DEST_YD_NM,
       CNMV_EVNT_DT,
       VVD_CD,
       BKG_KNT,
       BKG_NO,
       BL_NO,
       DECODE (FCNTR_FLG, 'Y', 'F', 'M') AS FCNTR_FLG,
       DECODE (OB_CNTR_FLG, 'Y', 'O', 'I') AS OB_CNTR_FLG,
       MVMT_EDI_MSG_TP_ID,
       BKG_CGO_TP_CD,
       CNTR_DMG_FLG,
       CNTR_HNGR_RCK_CD,
       CNTR_HNGR_BAR_ATCH_KNT,
       CNTR_DISP_FLG,
       IMDT_EXT_FLG,
       CNTR_RFUB_FLG,
       SPCL_CGO_FLG,
       VNDR_SEQ,
       VNDR_ABBR_NM,
       MVMT_TRSP_MOD_CD,
       CHSS_NO,
       MGST_NO,
       CNTR_SEAL_NO,
       WBL_NO,
       PKUP_NO,
       UPD_LOCL_DT,
       CRE_LOCL_DT,
       UPD_DT,
       CRE_DT,
       OFC_CD,
       USR_NM,
       CNMV_RMK,
       CNMV_YR,
       CNMV_SEQ,
       CNMV_SPLIT_NO,
       LST_BKG_NO,
       DECODE(MODI_TP_FLG, 'U', 'Update','I','Insert','') AS MODI_TP,
       CNMV_COL_NM AS CNMV_HIS_COL_NM,
       VGM
  FROM (SELECT /*+ INDEX CTM_MOVEMENT XAK2CTM_MOVEMENT */
               A.CNMV_CYC_NO,
               A.CNMV_CO_CD,
               A.MVMT_STS_CD,
               A.MVMT_CRE_TP_CD,
               A.ORG_YD_CD,
               (SELECT YD_NM FROM MDM_YARD WHERE YD_CD = A.ORG_YD_CD) AS ORG_YD_NM,
               A.DEST_YD_CD,
               (SELECT YD_NM FROM MDM_YARD WHERE YD_CD = A.DEST_YD_CD) AS DEST_YD_NM,
               TO_CHAR (A.CNMV_EVNT_DT, 'YYYY-MM-DD HH24:MI') AS CNMV_EVNT_DT,
               A.CRNT_VSL_CD||A.CRNT_SKD_VOY_NO||A.CRNT_SKD_DIR_CD AS VVD_CD,
               A.BKG_KNT,
               A.BKG_NO,
               A.BL_NO,
               A.FCNTR_FLG,
               A.OB_CNTR_FLG,
               A.MVMT_EDI_MSG_TP_ID,
               A.BKG_CGO_TP_CD,
               A.CNTR_DMG_FLG,
               DECODE (A.CNMV_YR||LPAD (A.CNMV_SEQ, 4, 0)||A.CNMV_SPLIT_NO, TEMP_CNMV_CD, CTM.CNTR_HNGR_RCK_CD, '') AS CNTR_HNGR_RCK_CD,
               DECODE (A.CNMV_YR||LPAD (A.CNMV_SEQ, 4, 0)||A.CNMV_SPLIT_NO, TEMP_CNMV_CD, CTM.CNTR_HNGR_BAR_ATCH_KNT, '') AS CNTR_HNGR_BAR_ATCH_KNT,
               A.CNTR_DISP_FLG,
               A.IMDT_EXT_FLG,
               A.CNTR_RFUB_FLG,
               A.SPCL_CGO_FLG,
               C.VNDR_SEQ,
               C.VNDR_ABBR_NM,
               A.MVMT_TRSP_MOD_CD,
               TRIM (A.CHSS_NO) AS CHSS_NO,
               TRIM (A.MGST_NO) AS MGST_NO,
               TRIM (A.CNTR_SEAL_NO) AS CNTR_SEAL_NO,
               TRIM (A.WBL_NO) AS WBL_NO,
               TRIM (A.PKUP_NO) AS PKUP_NO,
               TO_CHAR (A.UPD_LOCL_DT, 'YYYY-MM-DD HH24:MI') AS UPD_LOCL_DT,
               TO_CHAR (A.CRE_LOCL_DT, 'YYYY-MM-DD HH24:MI') AS CRE_LOCL_DT,
               TO_CHAR (A.UPD_DT, 'YYYY-MM-DD HH24:MI') AS UPD_DT,
               TO_CHAR (A.CRE_DT, 'YYYY-MM-DD HH24:MI') AS CRE_DT,
               A.OFC_CD,
               TRIM(A.USR_NM) AS USR_NM,
               TRIM(A.CNMV_RMK) AS CNMV_RMK,
               A.CNMV_YR,
               A.CNMV_SEQ,
               A.CNMV_SPLIT_NO,
               A.LST_BKG_NO,
               HIS.MODI_TP_FLG,
               HIS.CNMV_COL_NM,
               A.VGM_WGT_QTY || ' ' || DECODE(A.VGM_WGT_UT_CD, 'KGM', 'KGS', A.VGM_WGT_UT_CD) AS VGM
          FROM CTM_MOVEMENT A, MDM_VENDOR C,
               (SELECT /*+ INDEX(CTM XAK2CTM_MOVEMENT) */
                       CTM.CNTR_NO,
                       MAX (CTM.CNMV_YR||LPAD (CTM.CNMV_SEQ, 4, 0)||CTM.CNMV_SPLIT_NO) AS TEMP_CNMV_CD,
                       NVL2 (MST.CNTR_HNGR_RCK_CD, 'Y', 'N') AS CNTR_HNGR_RCK_CD,
                       MST.CNTR_HNGR_BAR_ATCH_KNT
                  FROM CTM_MOVEMENT CTM, MST_CONTAINER MST
                 WHERE CTM.CNTR_NO = MST.CNTR_NO
                   AND CTM.CNTR_NO = @[p_cntrno]||@[check_digit]
                   AND CTM.CNMV_EVNT_DT BETWEEN TO_DATE (@[p_date1], 'YYYY-MM-DD') AND TO_DATE (@[p_date2], 'YYYY-MM-DD') + 0.99999
              GROUP BY CTM.CNTR_NO, MST.CNTR_HNGR_RCK_CD, MST.CNTR_HNGR_BAR_ATCH_KNT) CTM,
               (SELECT A.CNTR_NO
                      ,A.CNMV_YR
                      ,A.CNMV_ID_NO
                      ,A.CNMV_HIS_SEQ
                      ,(SELECT NLS_LOWER(SUBSTR (XMLAGG (XMLELEMENT (B, ':', B.CNMV_COL_NM) ORDER BY B.CNMV_COL_NM).EXTRACT ('//text()'), 2)) AS CNMV_COL_NM
                        FROM   CTM_MVMT_MNL_HIS_COL B
                        WHERE  B.CNTR_NO = A.CNTR_NO 
                        AND    B.CNMV_YR = A.CNMV_YR 
                        AND    B.CNMV_ID_NO = A.CNMV_ID_NO 
                        AND    B.CNMV_HIS_SEQ = A.CNMV_HIS_SEQ
                       ) AS CNMV_COL_NM
                      ,(SELECT H.MODI_TP_FLG 
                        FROM   CTM_MVMT_MNL_HIS H 
                        WHERE  DAT_DIV_FLG = 'T' 
                        AND    H.CNTR_NO = A.CNTR_NO 
                        AND    H.CNMV_YR= A.CNMV_YR 
                        AND    H.CNMV_ID_NO = A.CNMV_ID_NO 
                        AND    H.CNMV_HIS_SEQ = A.CNMV_HIS_SEQ
                        AND    ROWNUM = 1) AS MODI_TP_FLG
                FROM   (SELECT CMH.CNTR_NO,CMH.CNMV_YR,CMH.CNMV_ID_NO, MAX(CMH.CNMV_HIS_SEQ) AS CNMV_HIS_SEQ
                        FROM   CTM_MVMT_MNL_HIS CMH
                        WHERE  CMH.DAT_DIV_FLG = 'T'
                        AND    CMH.CNTR_NO = @[p_cntrno]||@[check_digit]
                        GROUP BY CMH.CNTR_NO,CMH.CNMV_YR,CMH.CNMV_ID_NO
                       ) A
               ) HIS
         WHERE A.CNTR_NO = @[p_cntrno]||@[check_digit]
           AND A.CNTR_NO = CTM.CNTR_NO
           AND A.CNMV_EVNT_DT BETWEEN TO_DATE (@[p_date1], 'YYYY-MM-DD') AND TO_DATE (@[p_date2], 'YYYY-MM-DD') + 0.99999
           AND A.VNDR_SEQ = C.VNDR_SEQ(+)
           AND A.CNTR_NO = HIS.CNTR_NO(+)
           AND A.CNMV_YR = HIS.CNMV_YR(+)
           AND A.CNMV_ID_NO = HIS.CNMV_ID_NO(+))
ORDER BY CNMV_YR, CNMV_SEQ, CNMV_SPLIT_NO			]]></sql>
			<params>
				<param name="p_cntrno" type="12" value="" out="N"/>
				<param name="check_digit" type="12" value="" out="N"/>
				<param name="p_date1" type="12" value="" out="N"/>
				<param name="p_date2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
