<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOInsertYearSubleasePlanCSQL">
			<desc><![CDATA[eqr_sublease  테이블 데이터 입력]]></desc>
			<sql><![CDATA[
INSERT INTO EQR_SUBLEASE
(
    PLN_YRWK, 
    FM_ECC_CD, 
    TO_ECC_CD, 
    CNTR_TPSZ_CD, 
    CNTR_VOL_QTY,
    CRE_USR_ID, 
    CRE_DT, 
    UPD_USR_ID, 
    UPD_DT
)
SELECT 
    @[yrWk], 
    FM_ECC_CD, 
    TO_ECC_CD, 
    CNTR_TPSZ_CD, 
    ROUND(@[rsCount] * SLSE_RTO / 100),
    @[upd_usr_id], 
    SYSDATE, 
    @[upd_usr_id], 
    SYSDATE
FROM EQR_SLSE_PERF
WHERE FM_ECC_CD IN (SELECT ECC_CD FROM EQR_ECC_MST WHERE RCC_CD = @[rccCd] )			]]></sql>
			<params>
				<param name="yrWk" type="12" value="" out="N"/>
				<param name="rsCount" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rccCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
