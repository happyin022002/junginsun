<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOOnhireApprovalNumberHistorySeqRSQL">
			<desc><![CDATA[Onhire 될 장비의 Lift On Charge  내용을 조회]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(ONH_APRO_SEQ), 0)+1 ONH_APRO_SEQ
  FROM LSE_ONH_APRO_HIS
 WHERE CNTR_ONH_AUTH_NO = @[cntr_onh_auth_no]			]]></sql>
			<params>
				<param name="cntr_onh_auth_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
