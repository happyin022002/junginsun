<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationReportDBDAOUsLfdEdiAudidtListRSQL">
			<desc><![CDATA[ChargeCalculationReportDBDAOUsLfdEdiAudidtListVORSQL]]></desc>
			<sql><![CDATA[
SELECT  CNTR_NO,CNTR_TPSZ_CD,YD_CD,GNTE_FLG,DECODE(GNTE_FLG,'N','',GNTE_DT) GNTE_DT,
       EVNT_DT,RECE_DT,BKG_NO,ALPS_BKG_NO,FT_END_DT,DMDT_TRF_CD,VVD,BKG_FLG,DAYS,RESULT,GND_TOTAL
FROM (
SELECT SIF.EQ_NO AS CNTR_NO,
    ( SELECT CNTR_TPSZ_CD FROM MST_CONTAINER WHERE CNTR_NO = SIF.EQ_NO ) AS CNTR_TPSZ_CD,
    SIF.NOD_CD AS YD_CD,

    ( SELECT DECODE(COUNT(*),1,'N','Y') FROM SCE_EDI_DIR_IF A
         WHERE EQ_NO = SIF.EQ_NO
           AND NOD_CD = SIF.NOD_CD
           AND BL_EDI_322_NO = SIF.BL_EDI_322_NO
           AND EDI_STS_CD = SIF.EDI_STS_CD
           AND CRE_DT <= SIF.CRE_DT ) GNTE_FLG,
           
    TO_CHAR(SIF.EVNT_DT, 'YYYY-MM-DD') GNTE_DT,
      
    ( SELECT TO_CHAR(A.EVNT_DT, 'YYYY-MM-DD') FROM SCE_EDI_DIR_IF A
         WHERE EQ_NO = SIF.EQ_NO
           AND NOD_CD = SIF.NOD_CD
           AND BL_EDI_322_NO = SIF.BL_EDI_322_NO
           AND EDI_STS_CD = SIF.EDI_STS_CD
           AND CRE_DT = ( SELECT MIN(CRE_DT) FROM SCE_EDI_DIR_IF 
                           WHERE EQ_NO = A.EQ_NO
                             AND NOD_CD = A.NOD_CD
                             AND BL_EDI_322_NO = A.BL_EDI_322_NO
                             AND EDI_STS_CD = A.EDI_STS_CD ) ) EVNT_DT,
      
    TO_CHAR(SIF.CRE_DT, 'YYYY-MM-DD hh24:mm') RECE_DT,

    SIF.BL_EDI_322_NO AS BKG_NO,
    MAIN.ALPS_BKG_NO,
    TO_CHAR(MAIN.FT_END_DT, 'YYYY-MM-DD') FT_END_DT,
    MAIN.DMDT_TRF_CD,
    MAIN.VVD,
	CASE WHEN SIF.BL_EDI_322_NO != MAIN.ALPS_BKG_NO THEN 'Y' ELSE 'N' END BKG_FLG,
    NVL((TO_DATE(TO_CHAR(SIF.EVNT_DT,'YYYYMMDD'),'YYYYMMDD') - TO_DATE(TO_CHAR(MAIN.FT_END_DT,'YYYYMMDD'),'YYYYMMDD')),'') AS Days,
    DECODE(SIGN((TO_DATE(TO_CHAR(SIF.EVNT_DT,'YYYYMMDD'),'YYYYMMDD') - TO_DATE(TO_CHAR(MAIN.FT_END_DT,'YYYYMMDD'),'YYYYMMDD'))),0,'Coincidence','Descripency') AS Result,
       ( SELECT COUNT(*) FROM SCE_EDI_DIR_IF AAA
          WHERE 1=1
#if( ${event_receive} == 'RECEIVE')
			AND CRE_DT > TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD')
            AND CRE_DT < TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + .99999
#else
            AND EVNT_DT > TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD')
            AND EVNT_DT < TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + .99999
#end

            AND EDI_STS_CD = 'NF'
            AND EXISTS ( SELECT /*+ INDEX(A XPKMDM_YARD) */ 'Y' FROM MDM_YARD A WHERE AAA.NOD_CD = YD_CD AND YD_FCTY_TP_MRN_TML_FLG = 'Y' )
		) AS GND_TOTAL
FROM (
SELECT DISTINCT SF.EQ_NO AS CNTR_NO,
       SF.NOD_CD AS YD_CD, 
       SF.BL_EDI_322_NO AS BKG_NO,
       B.BKG_NO AS ALPS_BKG_NO,
       A.FT_END_DT,
       A.DMDT_TRF_CD,
       B.VSL_CD||B.SKD_VOY_NO||B.SKD_DIR_CD AS VVD
FROM DMT_CHG_CALC A, DMT_CHG_BKG_CNTR B, SCE_EDI_DIR_IF SF
WHERE 1=1
#if( ${event_receive} == 'RECEIVE')
AND SF.CRE_DT > TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD')
AND SF.CRE_DT < TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + .99999
#else
AND SF.EVNT_DT > TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD')
AND SF.EVNT_DT < TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + .99999
#end
#if( ${loce_cd} != '')
AND SF.NOD_CD LIKE NVL(@[yd_cd],@[loce_cd]||'%')
#end
#if( ${bkg_no} != '')
AND SF.BL_EDI_322_NO IN (
        		#foreach( $an_bkg_no in ${bkg_no_list} )
        			#if($velocityCount < $bkg_no_list.size()) '$an_bkg_no', #else '$an_bkg_no' #end
        		#end
        		) 
#end
#if( ${cntr_no} != '')
AND SF.EQ_NO IN (
        		#foreach( $an_cntr_no in ${cntr_no_list} )
        			#if($velocityCount < $cntr_no_list.size()) '$an_cntr_no', #else '$an_cntr_no' #end
        		#end
        		) 
#end
AND SF.EDI_STS_CD = 'NF'
AND EXISTS ( SELECT /*+ INDEX(A XPKMDM_YARD) */ 'Y' FROM MDM_YARD A WHERE SF.NOD_CD = YD_CD AND YD_FCTY_TP_MRN_TML_FLG = 'Y' )
AND B.SYS_AREA_GRP_ID = 'USA'
AND B.BKG_NO LIKE SUBSTR(SF.BL_EDI_322_NO,1,10)||'%'
AND B.CNTR_NO = SF.EQ_NO
AND A.FM_MVMT_YD_CD = SF.NOD_CD
AND A.SYS_AREA_GRP_ID = B.SYS_AREA_GRP_ID
AND A.CNTR_NO = B.CNTR_NO
AND A.CNTR_CYC_NO = B.CNTR_CYC_NO
AND A.DMDT_TRF_CD IN ( 'DMIF','CTIC' )
AND A.FM_MVMT_STS_CD = 'VD'
#if (${cntr_tpsz_cd} != 'All' && ${cntr_tpsz_cd} != '')
	AND B.CNTR_TPSZ_CD IN (
        		#foreach( $an_cntr_tpsz_cd in ${cntr_tpsz_cd_list} )
        			#if($velocityCount < $cntr_tpsz_cd_list.size()) '$an_cntr_tpsz_cd', #else '$an_cntr_tpsz_cd' #end
        		#end
        		) 
#end
#if (${result} != 'A' && ${result} != '')
	AND DECODE(SIGN((TO_DATE(TO_CHAR(SF.EVNT_DT,'YYYYMMDD'),'YYYYMMDD') - TO_DATE(TO_CHAR(A.FT_END_DT,'YYYYMMDD'),'YYYYMMDD'))),0,'C','D') IN (
        		#foreach( $an_result in ${result_list} )
        			#if($velocityCount < $result_list.size()) '$an_result', #else '$an_result' #end
        		#end
        		) 
#end
) MAIN, SCE_EDI_DIR_IF SIF
WHERE 1=1
#if( ${event_receive} == 'RECEIVE')
AND SIF.CRE_DT > TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD')
AND SIF.CRE_DT < TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + .99999
#else
AND SIF.EVNT_DT > TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD')
AND SIF.EVNT_DT < TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + .99999
#end
#if( ${loce_cd} != '')
AND SIF.NOD_CD LIKE NVL(@[yd_cd],@[loce_cd]||'%')
#end
#if( ${bkg_no} != '')
AND SIF.BL_EDI_322_NO IN (
        		#foreach( $an_bkg_no in ${bkg_no_list} )
        			#if($velocityCount < $bkg_no_list.size()) '$an_bkg_no', #else '$an_bkg_no' #end
        		#end
        		) 
#end
#if( ${cntr_no} != '')
AND SIF.EQ_NO IN (
        		#foreach( $an_cntr_no in ${cntr_no_list} )
        			#if($velocityCount < $cntr_no_list.size()) '$an_cntr_no', #else '$an_cntr_no' #end
        		#end
        		) 
#end
AND SIF.EDI_STS_CD = 'NF'
AND EXISTS ( SELECT /*+ INDEX(A XPKMDM_YARD) */  'Y' FROM MDM_YARD A WHERE SIF.NOD_CD = YD_CD AND YD_FCTY_TP_MRN_TML_FLG = 'Y' )

AND MAIN.BKG_NO(+) = SIF.BL_EDI_322_NO
AND MAIN.CNTR_NO(+) = SIF.EQ_NO
AND MAIN.YD_CD(+) = SIF.NOD_CD

#if (${latest} == 'Y')
      AND SIF.CRE_DT = ( SELECT CRE_DT FROM SCE_EDI_DIR_IF A
                          WHERE EQ_NO = SIF.EQ_NO
                            AND NOD_CD = SIF.NOD_CD
                            AND BL_EDI_322_NO = SIF.BL_EDI_322_NO
                            AND EDI_STS_CD = SIF.EDI_STS_CD
                            AND CRE_DT = ( SELECT MAX(CRE_DT) FROM SCE_EDI_DIR_IF 
                                            WHERE EQ_NO = A.EQ_NO
                                              AND NOD_CD = A.NOD_CD
                                              AND BL_EDI_322_NO = A.BL_EDI_322_NO
                                              AND EDI_STS_CD = A.EDI_STS_CD ) ) 
#end

)
WHERE 1=1

#if (${cntr_tpsz_cd} != 'All' && ${cntr_tpsz_cd} != '')
	AND CNTR_TPSZ_CD IN (
        		#foreach( $an_cntr_tpsz_cd in ${cntr_tpsz_cd_list} )
        			#if($velocityCount < $cntr_tpsz_cd_list.size()) '$an_cntr_tpsz_cd', #else '$an_cntr_tpsz_cd' #end
        		#end
        		) 
#end
#if (${result} != 'A' && ${result} != '')
	AND DECODE(Result,'Coincidence','C','D') IN (
        		#foreach( $an_result in ${result_list} )
        			#if($velocityCount < $result_list.size()) '$an_result', #else '$an_result' #end
        		#end
        		) 
#end


#if( ${event_receive} == 'RECEIVE')
ORDER BY 1,7
#else
ORDER BY 1,6
#end			]]></sql>
			<params>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="loce_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
