<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOSearchCPSCHSSPoolListDataRSQL">
			<desc><![CDATA[CPS Chassis Pool List를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CHSS_POOL_CD CODE1,
       CHSS_POOL_NM DESC1,
       VNDR_SEQ CODE2
FROM CGM_CPS_POOL_LIST
#if(${chss_pool_cd} != '')
WHERE CHSS_POOL_CD = @[chss_pool_cd]
#end
ORDER BY CHSS_POOL_CD			]]></sql>
			<params>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
