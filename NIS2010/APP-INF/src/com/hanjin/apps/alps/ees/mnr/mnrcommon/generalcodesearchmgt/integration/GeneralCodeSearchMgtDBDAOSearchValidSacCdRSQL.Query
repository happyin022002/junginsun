<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOSearchValidSacCdRSQL">
			<desc><![CDATA[SAC_CD Validate Check]]></desc>
			<sql><![CDATA[
SELECT   CASE WHEN COUNT(IDA_SAC_CD) = 1 THEN 'N' ELSE 'Y' END AS ERR_FLG
FROM     BKG_IDA_SAC_MST
WHERE    1 = 1
AND      IDA_SAC_CD = @[ida_sac_cd]			]]></sql>
			<params>
				<param name="ida_sac_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
