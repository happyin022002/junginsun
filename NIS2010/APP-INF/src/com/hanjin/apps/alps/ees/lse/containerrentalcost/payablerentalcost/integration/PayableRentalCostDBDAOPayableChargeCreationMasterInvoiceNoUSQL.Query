<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOPayableChargeCreationMasterInvoiceNoUSQL">
			<desc><![CDATA[Payable Rental Charge Creation : Master Data Charge Invoice No Update]]></desc>
			<sql><![CDATA[
UPDATE LSE_PAY_RNTL_CHG
SET    ( INV_NO
       , LR_COST_AMT
       , UPD_USR_ID
       , UPD_DT)
     = ( SELECT @[inv_no] AS INV_NO
              , SUM(TTL_COST_AMT) AS LR_COST_AMT
              , @[usr_id] AS UPD_USR_ID
              , SYSDATE   AS UPD_DT
         FROM   LSE_PAY_RNTL_CHG_CO
         WHERE  CO_COST_YRMON = @[chg_cost_yrmon]
         AND    AGMT_CTY_CD = @[agmt_cty_cd]
         AND    AGMT_SEQ = @[agmt_seq] )
WHERE  1 = 1
#if ( ${chg_seq}!= '' )
AND    CHG_SEQ = @[chg_seq]
#end
#if ( ${chg_cost_yrmon}!= '' )
AND    CHG_COST_YRMON = @[chg_cost_yrmon]
#end
AND    AGMT_CTY_CD = @[agmt_cty_cd]
AND    AGMT_SEQ = @[agmt_seq]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="chg_cost_yrmon" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="chg_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
