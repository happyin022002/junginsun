<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReeferSparePartMgtDBDAOsearchRFsparePartCodeListDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT   A.SPR_PRT_NO
        ,A.SPR_PRT_SPL_TP_CD 
        ,DECODE(A.SPR_PRT_TP_FLG1,'N','0','1') AS SPR_PRT_TP_FLG1
        ,DECODE(A.SPR_PRT_TP_FLG2,'N','0','1') AS SPR_PRT_TP_FLG2  
        ,DECODE(A.SPR_PRT_TP_FLG3,'N','0','1') AS SPR_PRT_TP_FLG3  
        ,A.SPR_UT_TP_NM
        ,A.SPR_PRT_NM
        ,A.SPR_PRT_QTY
        ,A.SPR_PRT_RMK
        ,A.CRE_USR_ID
        ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
        ,A.UPD_USR_ID
        ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
 FROM MNR_RF_SPR_PRT_CD A
 WHERE 1 = 1
#if (${spr_prt_no} != '') 
    AND A.SPR_PRT_NO = @[spr_prt_no]
	#if (${spr_work_type} != 'check') 
    AND A.SPR_PRT_SPL_TP_CD = @[spr_prt_spl_tp_cd]
	#end
#end
#if (${spr_ut_tp_nm} != '') 
   AND A.SPR_UT_TP_NM = @[spr_ut_tp_nm]
#end
#if (${spr_prt_spl_tp_cd} != '') 
   AND A.SPR_PRT_SPL_TP_CD = @[spr_prt_spl_tp_cd]
#end
#if (${spr_tp_cd} == 'TA') 
   AND A.SPR_PRT_TP_FLG1 = 'Y'
#elseif (${spr_tp_cd} == 'TB') 
   AND A.SPR_PRT_TP_FLG2 = 'Y'
#elseif (${spr_tp_cd} == 'TC') 
   AND A.SPR_PRT_TP_FLG3 = 'Y'
#end
ORDER BY A.SPR_PRT_NO			]]></sql>
			<params>
				<param name="spr_prt_no" type="12" value="" out="N"/>
				<param name="spr_prt_spl_tp_cd" type="12" value="" out="N"/>
				<param name="spr_ut_tp_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
