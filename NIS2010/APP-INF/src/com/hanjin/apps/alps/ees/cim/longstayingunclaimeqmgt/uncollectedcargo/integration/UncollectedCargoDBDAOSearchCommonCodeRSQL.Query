<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOSearchCommonCodeRSQL">
			<desc><![CDATA[CommonCode Combo 데이터 조회 ]]></desc>
			<sql><![CDATA[
SELECT	INTG_CD_VAL_CTNT	AS CODE,
		INTG_CD_VAL_DP_DESC AS CODE_DESC
FROM	COM_INTG_CD_DTL
WHERE	INTG_CD_ID = @[code]
		AND	APLY_ST_DT <= TO_CHAR(SYSDATE, 'YYYYMMDD')
		AND	APLY_END_DT >= TO_CHAR(SYSDATE, 'YYYYMMDD')
ORDER BY INTG_CD_VAL_DP_SEQ ASC			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
