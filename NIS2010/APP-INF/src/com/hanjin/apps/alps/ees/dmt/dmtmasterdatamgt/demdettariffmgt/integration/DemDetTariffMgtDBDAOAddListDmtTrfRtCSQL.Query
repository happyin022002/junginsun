<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAOAddListDmtTrfRtCSQL">
			<desc><![CDATA[Copy Basic Tariff]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_TRF_RT
(
	SYS_AREA_GRP_ID
	, DMDT_TRF_CD
	, TRF_SEQ
	, DMDT_DE_TERM_CD
	, TRF_GRP_SEQ
	, TRF_RT_SEQ
	, FT_OVR_DYS
	, FT_UND_DYS
	, CNTR_20FT_RT_AMT
	, CNTR_40FT_RT_AMT
	, CNTR_HC_RT_AMT
	, CNTR_45FT_RT_AMT
    , CNTR_R9_RT_AMT 
	, CRE_USR_ID
	, CRE_DT
	, CRE_OFC_CD
	, UPD_USR_ID
	, UPD_DT
	, UPD_OFC_CD
)
SELECT 
	SYS_AREA_GRP_ID
	, DMDT_TRF_CD
	, @[trf_seq]
	, @[dmdt_de_term_cd]
	, @[trf_grp_seq]
	, TRF_RT_SEQ
	, FT_OVR_DYS
	, FT_UND_DYS
	, CNTR_20FT_RT_AMT
	, CNTR_40FT_RT_AMT
	, CNTR_HC_RT_AMT
	, CNTR_45FT_RT_AMT
	, CNTR_R9_RT_AMT
	, @[usr_id]
	, SYSDATE
	, @[ofc_cd]
	, @[usr_id]
	, SYSDATE
	, @[ofc_cd]
FROM    DMT_TRF_RT
WHERE   SYS_AREA_GRP_ID = @[svr_id]
AND     DMDT_TRF_CD     = @[dmdt_trf_cd]
AND     TRF_SEQ         = @[from_trf_seq]
AND     DMDT_DE_TERM_CD = @[from_dmdt_de_term_cd]
AND     TRF_GRP_SEQ     = @[from_trf_grp_seq]			]]></sql>
			<params>
				<param name="trf_seq" type="12" value="" out="N"/>
				<param name="dmdt_de_term_cd" type="12" value="" out="N"/>
				<param name="trf_grp_seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="from_trf_seq" type="12" value="" out="N"/>
				<param name="from_dmdt_de_term_cd" type="12" value="" out="N"/>
				<param name="from_trf_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
