<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DualTypeExceptionMgtDBDAODualTypeCustomerCSQL">
			<desc><![CDATA[지역 Calculation Type과 무관하게 Combined Tariff 적용이 필요한 화주 입력용 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_DUL_TP_EXPT (
		CUST_CNT_CD
	,	CUST_SEQ
	,	CUST_EXPT_SEQ
	,	DUL_EXPT_EFF_DT
	,	DUL_EXPT_EXP_DT
	,	DUL_EXPT_RMK
	,	DMDT_CNTR_TP_CD
	,	DMDT_CGO_TP_CD
	,	DMDT_CNTR_CGO_TP_CD
	,	IO_BND_CD
	,	DMDT_CTRT_EXPT_TP_CD
	,	CVRG_CNT_CD
	,	CVRG_RGN_STE_CD
	,	CVRG_LOC_CD
	,	CRE_USR_ID
	,	CRE_DT
	,	CRE_OFC_CD
	,	UPD_USR_ID
	,	UPD_DT
	,	UPD_OFC_CD
	,	PROP_NO
) VALUES (
		SUBSTR(@[cust_cd], 1, 2)
	,	SUBSTR(@[cust_cd], 3)
	,	@[cust_expt_seq]
	,	TO_DATE(@[dul_expt_eff_dt], 'YYYYMMDD')
	,	TO_DATE(@[dul_expt_exp_dt], 'YYYYMMDD')
	,	@[dul_expt_rmk]
	,	DECODE(@[dmdt_ctrt_expt_tp_cd], 'B', SUBSTR(@[dmdt_cntr_cgo_tp_cd], 0, 1), '')
	,	DECODE(@[dmdt_ctrt_expt_tp_cd], 'B', SUBSTR(@[dmdt_cntr_cgo_tp_cd], 3), '')
	,	DECODE(@[dmdt_ctrt_expt_tp_cd], 'S', @[dmdt_cntr_cgo_tp_cd], '')
	,	@[io_bnd_cd]
	,	@[dmdt_ctrt_expt_tp_cd]
	,	NVL(@[cvrg_cnt_cd], ' ')
	,	NVL(@[cvrg_rgn_ste_cd], ' ')
	,	NVL(@[cvrg_loc_cd], ' ')
	,	@[cre_usr_id]
	,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
	,	@[cre_ofc_cd]
	,	@[cre_usr_id]
	,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
	,	@[cre_ofc_cd]
	,	TRIM(@[prop_no])
)			]]></sql>
			<params>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="cust_expt_seq" type="12" value="" out="N"/>
				<param name="dul_expt_eff_dt" type="12" value="" out="N"/>
				<param name="dul_expt_exp_dt" type="12" value="" out="N"/>
				<param name="dul_expt_rmk" type="12" value="" out="N"/>
				<param name="dmdt_ctrt_expt_tp_cd" type="12" value="" out="N"/>
				<param name="dmdt_cntr_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="cvrg_cnt_cd" type="12" value="" out="N"/>
				<param name="cvrg_rgn_ste_cd" type="12" value="" out="N"/>
				<param name="cvrg_loc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
