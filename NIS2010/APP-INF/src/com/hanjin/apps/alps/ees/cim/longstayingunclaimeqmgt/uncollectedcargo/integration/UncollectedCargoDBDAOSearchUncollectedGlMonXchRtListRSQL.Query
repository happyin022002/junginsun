<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOSearchUncollectedGlMonXchRtListRSQL">
			<desc><![CDATA[년월 / 통화코드별 환율조회 ]]></desc>
			<sql><![CDATA[
SELECT GL.ACCT_XCH_RT_YRMON
        , GL.CURR_CD
        , MDM.CNT_CD
        , GL.USD_LOCL_XCH_RT
        , GL.LOCL_KRW_XCH_RT
FROM GL_MON_XCH_RT GL
    , MDM_CURRENCY MDM
WHERE GL.ACCT_XCH_RT_YRMON = @[acct_xch_rt_yrmon]
AND GL.ACCT_XCH_RT_LVL = '1'
AND GL.CURR_CD = MDM.CURR_CD
ORDER BY GL.CURR_CD			]]></sql>
			<params>
				<param name="acct_xch_rt_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
