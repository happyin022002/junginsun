<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DisposalMgtDBDAOsearchDispContractMailListDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT A.MNR_PRNR_EML AS RECIPIENT,
       'Disposal Notice [' || @[disp_no] || ']  - ' ||  TO_CHAR(sysdate, 'yyyy.mm.dd hh24:mi') AS SUBJECT,
       'Disposal Notice [' || @[disp_no] || ']  - ' ||  TO_CHAR(sysdate, 'yyyy.mm.dd hh24:mi') AS TEXTCONTENT,
       @[mnr_prnr_eml] AS SENDER,
	   A.MNR_PRNR_LGL_ENG_NM AS RECIPIENT_NM,
	   @[apro_ofc_cd] AS SENDER_OFC_CD,
	   (SELECT USR_NM FROM COM_USER WHERE USR_ID = @[apro_usr_id]) AS SENDER_NM,
	   (SELECT OFC_ENG_NM FROM MDM_ORGANIZATION WHERE OFC_CD = @[apro_ofc_cd]) AS SENDER_OFC_NM
  FROM MNR_PARTNER A
 WHERE A.MNR_GRP_TP_CD = 'DSP'
   AND (A.MNR_PRNR_CNT_CD, A.MNR_PRNR_SEQ) IN (SELECT MNR_PRNR_CNT_CD,
                                                      MNR_PRNR_SEQ
                                                 FROM MNR_DISP_BUYR_PART
                                                WHERE DISP_NO = @[disp_no]
                                                  AND MNR_PRNR_EML = 'Y'
                                              )
   AND SYSDATE BETWEEN A.EFF_DT AND A.EXP_DT
   AND A.MNR_PRNR_EML IS NOT NULL			]]></sql>
			<params>
				<param name="disp_no" type="12" value="" out="N"/>
				<param name="mnr_prnr_eml" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="apro_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
