<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOAddOwnCntrStatusHistorysDataCSQL">
			<desc><![CDATA[Own Container 에 대해 S/N Range 만큼 Status History 를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MST_CNTR_STS_HIS
(
  CNTR_NO
, CNTR_STS_SEQ
, CO_CD
, YD_CD
, LOC_CD
, SCC_CD
, LCC_CD
, ECC_CD
, RCC_CD
, AGMT_CTY_CD
, AGMT_SEQ
, CNTR_STS_CD
, OFC_CD
, CNTR_STS_EVNT_DT
, CNTR_STS_RMK
, CNMV_STS_CD
, CNTR_FULL_FLG
, CNTR_OLD_VAN_FLG
, CRE_USR_ID
, CRE_DT
, UPD_USR_ID
, UPD_DT
)
SELECT 
  CNTR_NO
, LST_STS_SEQ	AS CNTR_STS_SEQ
, OWNR_CO_CD	AS CO_CD
, CRNT_YD_CD	AS YD_CD
, LOC_CD
, SCC_CD
, LCC_CD
, ECC_CD
, RCC_CD
, AGMT_CTY_CD
, AGMT_SEQ
, CNTR_STS_CD
, '1'		AS OFC_CD
, ONH_DT	AS CNTR_STS_EVNT_DT
, CNTR_RMK	AS CNTR_STS_RMK
, CNMV_STS_CD
, FULL_FLG	AS CNTR_FULL_FLG
, '1'		AS CNTR_OLD_VAN_FLG
, CRE_USR_ID
, SYSDATE
, UPD_USR_ID
, SYSDATE
FROM MST_CONTAINER
WHERE CNTR_NO BETWEEN SUBSTR(@[fm_ser_no],0,9) AND SUBSTR(@[to_ser_no],4,9);			]]></sql>
			<params>
				<param name="fm_ser_no" type="12" value="" out="N"/>
				<param name="to_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
