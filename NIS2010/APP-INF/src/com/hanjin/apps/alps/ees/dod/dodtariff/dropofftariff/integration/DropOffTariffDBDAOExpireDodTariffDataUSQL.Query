<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DropOffTariffDBDAOExpireDodTariffDataUSQL">
			<desc><![CDATA[DodTariff 만료 수정]]></desc>
			<sql><![CDATA[
update DOD_DRP_OFF_CHG_TRF
   set 
       drp_off_chg_trf_exp_flg = 'Y',
	   DRP_OFF_CHG_TRF_EXP_DT = TO_CHAR(SYSDATE, 'YYYYMMDD'),
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = SYSDATE
 where 1=1
   and DRP_OFF_CHG_TRF_SEQ = @[drp_off_chg_trf_seq]
   and drp_off_chg_trf_exp_flg != 'Y'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="drp_off_chg_trf_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
