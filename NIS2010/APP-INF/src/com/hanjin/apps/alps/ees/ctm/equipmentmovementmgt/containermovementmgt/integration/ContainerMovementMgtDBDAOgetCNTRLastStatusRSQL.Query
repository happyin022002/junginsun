<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOgetCNTRLastStatusRSQL">
			<desc><![CDATA[컨테이너의 마지막 상태값을 반환한다]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_desc(CTM_MOVEMENT XUK1CTM_MOVEMENT) */
       MVMT_STS_CD
  FROM CTM_MOVEMENT
 WHERE CNTR_NO = @[cntr_no]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
