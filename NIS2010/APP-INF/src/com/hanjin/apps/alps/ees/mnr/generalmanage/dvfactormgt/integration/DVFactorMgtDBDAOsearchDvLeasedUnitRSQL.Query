		<query name="DVFactorMgtDBDAOsearchDvLeasedUnitRSQL">
			<desc><![CDATA[MNR-Total-DV 화면조회]]></desc>
			<sql><![CDATA[
SELECT
LPAD(B.VNDR_SEQ, 6, '0') VNDR_SEQ
,B.MNR_CNTC_PRNR_NM
,B.INTL_PHN_NO
,B.PHN_NO
,B.INTL_FAX_NO
,B.FAX_NO
,B.MNR_PRNR_EML
,B.MNR_PRNR_ADDR
,B.MNR_PRNR_RMK
,B.CRE_USR_ID
,TO_CHAR(B.CRE_DT, 'YYYY-MM-DD') CRE_DT
,B.UPD_USR_ID
,TO_CHAR(B.UPD_DT, 'YYYY-MM-DD') UPD_DT
FROM MNR_DPC_VAL_CNTC_PNT B
ORDER BY B.VNDR_SEQ			]]></sql>
