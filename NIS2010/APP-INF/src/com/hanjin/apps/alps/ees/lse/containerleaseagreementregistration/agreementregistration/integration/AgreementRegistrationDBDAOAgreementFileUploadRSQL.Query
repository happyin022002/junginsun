<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRegistrationDBDAOAgreementFileUploadRSQL">
			<desc><![CDATA[Lease Agreement File Upload Search]]></desc>
			<sql><![CDATA[
SELECT A.AGMT_CTY_CD
     , A.AGMT_SEQ
     , A.FILE_DTL_SEQ
     , A.FILE_PATH_NM
     , A.ORG_FILE_NM
     , B.FILE_UPLD_NM
     , B.FILE_SZ_CAPA
     , B.FILE_PATH_URL
FROM   LSE_AGMT_FILE_ATCH A, COM_UPLD_FILE B
WHERE  A.AGMT_CTY_CD = @[agmt_cty_cd]
AND    A.AGMT_SEQ = @[agmt_seq]
AND    A.ORG_FILE_NM = B.FILE_SAV_ID
#if (${file_dtl_seq} != '')
AND    A.FILE_DTL_SEQ = @[file_dtl_seq]
#end
ORDER  BY A.AGMT_CTY_CD, A.AGMT_SEQ			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="file_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
