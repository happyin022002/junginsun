<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchBookingContainerQuantityRSQL">
			<desc><![CDATA[SearchBookingContainerQuantity]]></desc>
			<sql><![CDATA[
SELECT COUNT (CNTR_NO) BKGQTY
  FROM BKG_CONTAINER
 WHERE BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
