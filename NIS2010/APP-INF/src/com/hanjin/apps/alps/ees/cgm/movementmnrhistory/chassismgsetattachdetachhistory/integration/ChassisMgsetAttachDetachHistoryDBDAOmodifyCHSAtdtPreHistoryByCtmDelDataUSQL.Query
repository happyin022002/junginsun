<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAttachDetachHistoryDBDAOmodifyCHSAtdtPreHistoryByCtmDelDataUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE CGM_EQ_ATCH_DTCH_HIS
/*+ INDEX_DESC(A XPKCGM_EQ_ATCH_DTCH_HIS) */
SET (DTCH_YD_CD  ,DTCH_DT)
               = (select /*+ INDEX(C XAK2CTM_MOVEMENT)   */
            ORG_YD_CD , CNMV_EVNT_DT
            FROM CTM_MOVEMENT C
            WHERE   C.CNTR_NO =@[cntr_no]
            AND C.CNMV_EVNT_DT > TO_DATE( @[atch_dt] ,'YYYYMMDDHH24MISS')
            AND ROWNUM =1)
   ,UPD_USR_ID = @[upd_usr_id]
   ,DTCH_INP_TP_CD = 'A'
   ,UPD_DT = SYSDATE
WHERE  CNTR_NO =@[cntr_no]
AND ATCH_DT < TO_DATE( @[atch_dt] ,'YYYYMMDDHH24MISS')
AND EXISTS ( select /*+ INDEX(C XAK2CTM_MOVEMENT)   */
            'X'
            FROM CTM_MOVEMENT C
            WHERE   C.CNTR_NO = @[cntr_no]
            AND C.CNMV_EVNT_DT > TO_DATE( @[atch_dt] ,'YYYYMMDDHH24MISS')
            AND ROWNUM =1) 
AND ROWNUM=1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="atch_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
