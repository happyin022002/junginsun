<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOModifyChargeDeletionPathSetupUSQL">
			<desc><![CDATA[ChargeCalculationDBDAOModifyChargeDeletionPathSetupUSQL]]></desc>
			<sql><![CDATA[
UPDATE  DMT_CHG_DELT_PATH_STUP 
   SET  EXP_DT                   = to_date(@[exp_dt], 'YYYYMMDD') + 0.99999
       ,DMDT_BRNC_OFC_OP_MGR_FLG = decode(@[dmdt_brnc_ofc_op_mgr_flg], '0', 'N', '1', 'Y')
       ,DMDT_BRNC_FLG            = decode(@[dmdt_brnc_flg], '0', 'N', '1', 'Y')
       ,DMDT_RHQ_FLG             = decode(@[dmdt_rhq_flg],  '0', 'N', '1', 'Y')
       ,DMDT_HO_FLG              = decode(@[dmdt_ho_flg],   '0', 'N', '1', 'Y')
       ,UPD_USR_ID               = @[upd_usr_id]
       ,UPD_DT                   = nvl(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), sysdate)
       ,UPD_OFC_CD               = @[upd_ofc_cd]
 WHERE  CHG_DELT_PATH_STUP_SEQ = to_number(@[chg_delt_path_stup_seq]) 			]]></sql>
			<params>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="dmdt_brnc_ofc_op_mgr_flg" type="12" value="" out="N"/>
				<param name="dmdt_brnc_flg" type="12" value="" out="N"/>
				<param name="dmdt_rhq_flg" type="12" value="" out="N"/>
				<param name="dmdt_ho_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="chg_delt_path_stup_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
