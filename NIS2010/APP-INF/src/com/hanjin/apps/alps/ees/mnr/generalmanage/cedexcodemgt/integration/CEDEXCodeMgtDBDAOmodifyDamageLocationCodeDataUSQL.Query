<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CEDEXCodeMgtDBDAOmodifyDamageLocationCodeDataUSQL">
			<desc><![CDATA[modifyDamageLocationCodeData]]></desc>
			<sql><![CDATA[
UPDATE MNR_EQ_LOC_CD
   SET EQ_LOC_CD = @[eq_loc_cd],
       EQ_LOC_NM = @[eq_loc_nm],
       EQ_LESS_20FT_FLG = DECODE(NVL(@[eq_less_20ft_flg],'0'),'0','N','Y'),
       EQ_KND_CD = @[eq_knd_cd],
       EQ_LOC_PRNT_CD = @[eq_loc_prnt_cd],
       UPD_USR_ID = @[upd_usr_id],
       UPD_DT = sysdate
 WHERE EQ_LOC_CD_LVL = @[eq_loc_cd_lvl] 
   AND EQ_LOC_CD = @[eq_loc_cd]			]]></sql>
			<params>
				<param name="eq_loc_cd" type="12" value="" out="N"/>
				<param name="eq_loc_nm" type="12" value="" out="N"/>
				<param name="eq_less_20ft_flg" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_loc_prnt_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eq_loc_cd_lvl" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
