<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DVFactorMgtDBDAOmodifyDvLeasedUnitRSQL">
			<desc><![CDATA[DV Leased Unit 화면 수정]]></desc>
			<sql><![CDATA[
UPDATE MNR_DPC_VAL_CNTC_PNT
SET MNR_CNTC_PRNR_NM = @[mnr_cntc_prnr_nm],
    PHN_NO = @[phn_no],
    MNR_PRNR_EML = @[mnr_prnr_eml],
    MNR_PRNR_ADDR = @[mnr_prnr_addr],
    MNR_PRNR_RMK = @[mnr_prnr_rmk],
    CRE_USR_ID = @[cre_usr_id],
    CRE_DT = SYSDATE,
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT = SYSDATE
WHERE VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="mnr_cntc_prnr_nm" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="mnr_prnr_eml" type="12" value="" out="N"/>
				<param name="mnr_prnr_addr" type="12" value="" out="N"/>
				<param name="mnr_prnr_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
