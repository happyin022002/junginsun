<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FreeTimeDBDAOAddIfFtHisCSQL">
			<desc><![CDATA[FreeTimeDBDAOAddIfFtHisCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_IF_FT_HIS
    (
        BKG_NO
       ,CNTR_NO
       ,IF_FT_HIS_SEQ
       ,DMDT_CHG_LOC_DIV_CD
       ,FM_MVMT_STS_CD
       ,FM_MVMT_DT
       ,FM_MVMT_YD_CD
       ,FT_CMNC_DT
       ,FT_END_DT
       ,FT_DYS
       ,SYS_AREA_GRP_ID
       ,DMDT_TRF_CD
       ,TRF_SEQ
       ,DMDT_DE_TERM_CD
       ,TRF_GRP_SEQ
       ,FT_RMK
       ,CRE_USR_ID
       ,CRE_DT
    )
VALUES
	(
        @[bkg_no]
       ,@[cntr_no]
       ,(
			SELECT  NVL(MAX(IF_FT_HIS_SEQ), 0) + 1
			  FROM  DMT_IF_FT_HIS
			 WHERE  BKG_NO  = @[bkg_no]
			   AND  CNTR_NO = @[cntr_no]
	    )
       ,@[dmdt_chg_loc_div_cd]
       ,@[fm_mvmt_sts_cd]
       ,TO_DATE(@[fm_mvmt_dt], 'YYYYMMDDHH24MISS')
       ,@[fm_mvmt_yd_cd]
       ,TO_DATE(@[ft_cmnc_dt], 'YYYYMMDDHH24MISS')
       ,TO_DATE(@[ft_end_dt], 'YYYYMMDDHH24MISS')
       ,@[ft_dys]
       ,@[sys_area_grp_id]
       ,@[dmdt_trf_cd]
       ,@[trf_seq]
       ,@[dmdt_de_term_cd]
       ,@[trf_grp_seq]
       ,@[ft_rmk]
       ,'SCE_BATCH'
       ,SYSDATE
	)			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="fm_mvmt_sts_cd" type="12" value="" out="N"/>
				<param name="fm_mvmt_dt" type="12" value="" out="N"/>
				<param name="fm_mvmt_yd_cd" type="12" value="" out="N"/>
				<param name="ft_cmnc_dt" type="12" value="" out="N"/>
				<param name="ft_end_dt" type="12" value="" out="N"/>
				<param name="ft_dys" type="12" value="" out="N"/>
				<param name="sys_area_grp_id" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="trf_seq" type="12" value="" out="N"/>
				<param name="dmdt_de_term_cd" type="12" value="" out="N"/>
				<param name="trf_grp_seq" type="12" value="" out="N"/>
				<param name="ft_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
