<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQFlagMgtDBDAOsearchHangerRackReportListDataRSQL">
			<desc><![CDATA[--------------------------------------------------------
History
2011.12.20 김상수 [CHM-201115062-01] ALPS MNR-Hanger-hanger rack and Bar History에 Report 보턴 추가 및 처리
                                     - [UI_MNR_0257] Hanger Rack/Bar Using Report 신규 개발
2012.01.04 신혜정 [CHM-201215407-01] Detail EQ no 내역 팝업 조회 추가
                                     - YEAR_MONTH, MNR_HNGR_RCK_CD, MNR_HNGR_TRF_CD, P_LOC_TP 조회 추가]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(YEAR_MONTH, 1, 4) AS YEAR,
       TO_CHAR(TO_DATE(YEAR_MONTH, 'YYYY-MM'), 'MON', 'NLS_DATE_LANGUAGE=ENGLISH') AS MONTH_NM,
       LOCATION,
       (SELECT A.MNR_CD_DP_DESC
          FROM MNR_GEN_CD A
         WHERE A.PRNT_CD_ID = 'CD00021'
           AND A.MNR_CD_ID = MNR_HNGR_RCK_CD) AS MNR_HNGR_RCK_NM,
       (SELECT A.MNR_CD_DP_DESC
          FROM MNR_GEN_CD A
         WHERE A.PRNT_CD_ID = 'CD00092'
           AND A.MNR_CD_ID = MNR_HNGR_TRF_CD) AS MNR_HNGR_TRF_NM,
       SALES_20FT,
       SALES_40FT,
       SALES_TOTAL,
       INSTALL_QTY,
       COLLECT_SOUND,
       COLLECT_REPAIR,
       COLLECT_MISSING,
       COLLECT_DISPOSAL,
       COLLECT_TOTAL,
       TO_CHAR(DECODE(INSTALL_QTY, 0, 0, ROUND(COLLECT_TOTAL / INSTALL_QTY * 100, 2)), 'FM999,990.00')||' %' AS RATIO,
       YEAR_MONTH,
	   MNR_HNGR_RCK_CD,
       MNR_HNGR_TRF_CD,
#if (${p_loc_cd} != '')
	   ${p_loc_tp} AS P_LOC_TP,
	   ${p_loc_cd} AS P_LOC_CD,
#else
	   '' AS P_LOC_TP,
	   '' AS P_LOC_CD,
#end
	   @[from_date] AS FROM_DATE,
	   @[to_date] AS TO_DATE,
	   SUB_EQ_TPSZ_CD AS EQ_TPSZ_CD
  FROM (SELECT SUBSTR(MNR_FLG_INP_DT, 1, 7) AS YEAR_MONTH,
               SUBSTR(MNR_FLG_YD_CD, 1, 5) AS LOCATION,
               MNR_HNGR_RCK_CD,
               MNR_HNGR_TRF_CD,
               SUM(DECODE(SUBSTR(EQ_TPSZ_CD, 2, 1), '2', 1, 0)) AS SALES_20FT,
               SUM(DECODE(SUBSTR(EQ_TPSZ_CD, 2, 1), '2', 0, 1)) AS SALES_40FT,
               COUNT(EQ_TPSZ_CD) AS SALES_TOTAL,
               SUM(HNGR_BAR_AMD_QTY) AS INSTALL_QTY,
               SUM(ACT_INVT_QTY) AS COLLECT_SOUND,
               SUM(MNR_HNGR_DMG_QTY) AS COLLECT_REPAIR,
               SUM(MNR_LOST_HNGR_QTY) AS COLLECT_MISSING,
               SUM(MNR_DISP_HNGR_QTY) AS COLLECT_DISPOSAL,
               (SUM(ACT_INVT_QTY) + SUM(MNR_HNGR_DMG_QTY) + SUM(MNR_LOST_HNGR_QTY) + SUM(MNR_DISP_HNGR_QTY)) AS COLLECT_TOTAL,
			   DECODE(SUBSTR(EQ_TPSZ_CD, 2, 1), '2', 1, 0) AS SUB_EQ_TPSZ_CD
          FROM (SELECT TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.MNR_FLG_INP_DT, @[user_ofc_cd]), 'YYYY-MM-DD') AS MNR_FLG_INP_DT,
                       A.MNR_FLG_YD_CD,
                       A.MNR_HNGR_RCK_CD,
                       A.MNR_HNGR_TRF_CD,
                       A.EQ_TPSZ_CD,
                       DECODE(A.HNGR_BAR_AMD_QTY, '', 0, A.HNGR_BAR_AMD_QTY) AS HNGR_BAR_AMD_QTY,
                       DECODE(A.MNR_STS_FLG, 'N', DECODE(B.ACT_INVT_QTY, '', 0, B.ACT_INVT_QTY), 0)  AS ACT_INVT_QTY,
                       DECODE(A.MNR_STS_FLG, 'N', DECODE(B.MNR_HNGR_DMG_QTY, '', 0, B.MNR_HNGR_DMG_QTY), 0) AS MNR_HNGR_DMG_QTY,
                       DECODE(A.MNR_STS_FLG, 'N', DECODE(B.MNR_LOST_HNGR_QTY, '', 0, B.MNR_LOST_HNGR_QTY), 0) AS MNR_LOST_HNGR_QTY,
                       DECODE(A.MNR_STS_FLG, 'N', DECODE(B.MNR_DISP_HNGR_QTY, '', 0, B.MNR_DISP_HNGR_QTY), 0) AS MNR_DISP_HNGR_QTY
                  FROM MNR_FLG_HIS A,
#if (${p_loc_cd} != '' && ${p_loc_tp} != 'YARD')
                       (SELECT A.LOC_CD,
                               A.SCC_CD,
                               C.LCC_CD,
                               C.RCC_CD
                          FROM MDM_LOCATION A,
                               MDM_EQ_ORZ_CHT C
                         WHERE A.SCC_CD = C.SCC_CD) C,
#end
                       MNR_EQ_STS_V B
                 WHERE A.EQ_NO = B.EQ_NO
                   AND A.MNR_FLG_TP_CD = 'HGR'
                   AND A.MNR_FLG_INP_DT BETWEEN GLOBALDATE_PKG.TIME_CONV_OFC_FNC(@[user_ofc_cd], TO_DATE(@[from_date], 'YYYY-MM-DD'), MNR_COMMON_PKG.MNR_GET_HOOFC_FNC())
                                            AND GLOBALDATE_PKG.TIME_CONV_OFC_FNC(@[user_ofc_cd], TO_DATE(@[to_date], 'YYYY-MM-DD') + 0.99999, MNR_COMMON_PKG.MNR_GET_HOOFC_FNC())
                   AND A.MNR_HNGR_BAR_TP_CD = 'S'
#if (${mnr_hngr_rck_cd} != '' && ${mnr_hngr_rck_cd} != 'ALL')
                   AND A.MNR_HNGR_RCK_CD = @[mnr_hngr_rck_cd]
#end
#if (${mnr_hngr_trf_cd} != '' && ${mnr_hngr_trf_cd} != 'ALL')
                   AND A.MNR_HNGR_TRF_CD = @[mnr_hngr_trf_cd]
#end
#if (${p_loc_cd} != '')
     #if (${p_loc_tp} == 'RCC')
                   AND SUBSTR(A.MNR_FLG_YD_CD,1,5) = C.LOC_CD
                   AND C.RCC_CD = @[p_loc_cd]
     #elseif (${p_loc_tp} == 'LCC')
                   AND SUBSTR(A.MNR_FLG_YD_CD,1,5) = C.LOC_CD
                   AND C.LCC_CD = @[p_loc_cd]
     #elseif (${p_loc_tp} == 'SCC')
                   AND SUBSTR(A.MNR_FLG_YD_CD,1,5) = C.LOC_CD
                   AND C.SCC_CD = @[p_loc_cd]
     #else
                   AND SUBSTR(A.MNR_FLG_YD_CD,1,5) = @[p_loc_cd]
     #end
#end
                 ORDER BY A.EQ_NO,
                          A.MNR_FLG_SEQ ASC)
         GROUP BY SUBSTR(MNR_FLG_INP_DT, 1, 7),
                  SUBSTR(MNR_FLG_YD_CD, 1, 5),
                  MNR_HNGR_RCK_CD,
                  MNR_HNGR_TRF_CD,
                  DECODE(SUBSTR(EQ_TPSZ_CD, 2, 1), '2', 1, 0)
         ORDER BY SUBSTR(MNR_FLG_INP_DT, 1, 7),
                  SUBSTR(MNR_FLG_YD_CD, 1, 5),
                  MNR_HNGR_RCK_CD,
                  MNR_HNGR_TRF_CD)			]]></sql>
			<params>
				<param name="from_date" type="12" value="" out="N"/>
				<param name="to_date" type="12" value="" out="N"/>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="mnr_hngr_rck_cd" type="12" value="" out="N"/>
				<param name="mnr_hngr_trf_cd" type="12" value="" out="N"/>
				<param name="p_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
