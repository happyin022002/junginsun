<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerTypeSizeDBDAOContainerTypeSizeDynamicHeaderRSQL">
			<desc><![CDATA[Container Type Size의 가변적 Header를 조회한다.
2011.06.13 남궁진호 [CHM-201111467-01] [LSE] MDM에 R9 등록에 따른 추가 업무 진행 요청]]></desc>
			<sql><![CDATA[
SELECT    '|'||TPSZ_DP01||'|'||TPSZ_DP02||'|'||TPSZ_DP03||'|'||TPSZ_DP04||'|'||TPSZ_DP05
        ||'|'||TPSZ_DP06||'|'||TPSZ_DP07||'|'||TPSZ_DP08||'|'||TPSZ_DP09||'|'||TPSZ_DP10
        ||'|'||TPSZ_DP11||'|'||TPSZ_DP12||'|'||TPSZ_DP13||'|'||TPSZ_DP14||'|'||TPSZ_DP15
        ||'|'||TPSZ_DP16||'|'||TPSZ_DP17||'|'||TPSZ_DP18||'|'||TPSZ_DP19||'|'||TPSZ_DP20
        ||'|'||TPSZ_DP21||'|'||TPSZ_DP22||'|'||TPSZ_DP23||'|'||TPSZ_DP24||'|'||TPSZ_DP25
        ||'|'||TPSZ_DP26||'|'||TPSZ_DP27||'|'||TPSZ_DP28||'|'||TPSZ_DP29||'|'||TPSZ_DP30 AS CNTR_TPSZ_HD        
FROM   (SELECT  MAX(CASE WHEN RPT_DP_SEQ = 1  THEN CNTR_TPSZ_CD END) TPSZ_DP01,
                MAX(CASE WHEN RPT_DP_SEQ = 2  THEN CNTR_TPSZ_CD END) TPSZ_DP02,
                MAX(CASE WHEN RPT_DP_SEQ = 3  THEN CNTR_TPSZ_CD END) TPSZ_DP03,
                MAX(CASE WHEN RPT_DP_SEQ = 4  THEN CNTR_TPSZ_CD END) TPSZ_DP04,
                MAX(CASE WHEN RPT_DP_SEQ = 5  THEN CNTR_TPSZ_CD END) TPSZ_DP05,
                MAX(CASE WHEN RPT_DP_SEQ = 6  THEN CNTR_TPSZ_CD END) TPSZ_DP06,
                MAX(CASE WHEN RPT_DP_SEQ = 7  THEN CNTR_TPSZ_CD END) TPSZ_DP07,
                MAX(CASE WHEN RPT_DP_SEQ = 8  THEN CNTR_TPSZ_CD END) TPSZ_DP08,
                MAX(CASE WHEN RPT_DP_SEQ = 9  THEN CNTR_TPSZ_CD END) TPSZ_DP09,
                MAX(CASE WHEN RPT_DP_SEQ = 10 THEN CNTR_TPSZ_CD END) TPSZ_DP10,
                MAX(CASE WHEN RPT_DP_SEQ = 11 THEN CNTR_TPSZ_CD END) TPSZ_DP11,
                MAX(CASE WHEN RPT_DP_SEQ = 12 THEN CNTR_TPSZ_CD END) TPSZ_DP12,
                MAX(CASE WHEN RPT_DP_SEQ = 13 THEN CNTR_TPSZ_CD END) TPSZ_DP13,
                MAX(CASE WHEN RPT_DP_SEQ = 14 THEN CNTR_TPSZ_CD END) TPSZ_DP14,
                MAX(CASE WHEN RPT_DP_SEQ = 15 THEN CNTR_TPSZ_CD END) TPSZ_DP15,
                MAX(CASE WHEN RPT_DP_SEQ = 16 THEN CNTR_TPSZ_CD END) TPSZ_DP16,
                MAX(CASE WHEN RPT_DP_SEQ = 17 THEN CNTR_TPSZ_CD END) TPSZ_DP17,
                MAX(CASE WHEN RPT_DP_SEQ = 18 THEN CNTR_TPSZ_CD END) TPSZ_DP18,
                MAX(CASE WHEN RPT_DP_SEQ = 19 THEN CNTR_TPSZ_CD END) TPSZ_DP19,
                MAX(CASE WHEN RPT_DP_SEQ = 20 THEN CNTR_TPSZ_CD END) TPSZ_DP20,
                MAX(CASE WHEN RPT_DP_SEQ = 21 THEN CNTR_TPSZ_CD END) TPSZ_DP21,
                MAX(CASE WHEN RPT_DP_SEQ = 22 THEN CNTR_TPSZ_CD END) TPSZ_DP22,
                MAX(CASE WHEN RPT_DP_SEQ = 23 THEN CNTR_TPSZ_CD END) TPSZ_DP23,
                MAX(CASE WHEN RPT_DP_SEQ = 24 THEN CNTR_TPSZ_CD END) TPSZ_DP24,
                MAX(CASE WHEN RPT_DP_SEQ = 25 THEN CNTR_TPSZ_CD END) TPSZ_DP25,
                MAX(CASE WHEN RPT_DP_SEQ = 26 THEN CNTR_TPSZ_CD END) TPSZ_DP26,
                MAX(CASE WHEN RPT_DP_SEQ = 27 THEN CNTR_TPSZ_CD END) TPSZ_DP27,
                MAX(CASE WHEN RPT_DP_SEQ = 28 THEN CNTR_TPSZ_CD END) TPSZ_DP28,
                MAX(CASE WHEN RPT_DP_SEQ = 29 THEN CNTR_TPSZ_CD END) TPSZ_DP29,
                MAX(CASE WHEN RPT_DP_SEQ = 30 THEN CNTR_TPSZ_CD END) TPSZ_DP30
        FROM   (                
                SELECT  NVL(A.RPT_DP_SEQ, B.LEVEL_SEQ) AS RPT_DP_SEQ,
                        CASE WHEN A.DELT_FLG = 'N' AND A.ACIAC_DIV_CD = 'A'
                             THEN A.CNTR_TPSZ_CD ELSE NULL END CNTR_TPSZ_CD, 
                        A.DELT_FLG, A.ACIAC_DIV_CD
                FROM   (SELECT  ROW_NUMBER() OVER(ORDER BY RPT_DP_SEQ) AS RPT_DP_SEQ, 
                                CNTR_TPSZ_CD, DELT_FLG, ACIAC_DIV_CD
                        FROM    MDM_CNTR_TP_SZ
                        WHERE DELT_FLG = 'N' AND ACIAC_DIV_CD = 'A') A,
                       (SELECT  LEVEL AS LEVEL_SEQ
                        FROM    DUAL
                        CONNECT BY LEVEL <= 30) B        
                WHERE   A.RPT_DP_SEQ(+) = B.LEVEL_SEQ                                
        )
)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
