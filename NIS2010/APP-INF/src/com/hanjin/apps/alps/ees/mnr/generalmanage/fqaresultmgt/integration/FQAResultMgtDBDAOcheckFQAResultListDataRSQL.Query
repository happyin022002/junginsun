<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FQAResultMgtDBDAOcheckFQAResultListDataRSQL">
			<desc><![CDATA[FQAResultMgtDBDAOcheckFQAResultListDataRSQL]]></desc>
			<sql><![CDATA[
SELECT COUNT(A.VNDR_SEQ) CNT 
FROM MNR_FLD_QLTY_AUD_RSLT A 
WHERE A.VNDR_SEQ = TO_NUMBER(@[vndr_seq])
AND A.YD_CD = @[yd_cd]	
AND TO_CHAR(A.FLD_AUD_DT,'YYYY-MM-DD') = @[fld_aud_dt]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="Y"/>
				<param name="yd_cd" type="12" value="" out="Y"/>
				<param name="fld_aud_dt" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
