<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLocYardInitialInfoRSQL">
			<desc><![CDATA[VSL, INLAND(WATER)의 EXECUTE ROW 생성하면서 YARD 정보를 검색 item --> VSL(059)           = 'V' , INLAND(WATER)(080) = 'W']]></desc>
			<sql><![CDATA[
SELECT MIN(DISTINCT TML_CD) AS LOC_YARD            
FROM PRD_PORT_TML_MTX   			                
WHERE PORT_CD     = @[ecc]                  				
AND   VSL_SLAN_CD = @[vsl]			]]></sql>
			<params>
				<param name="ecc" type="12" value="" out="N"/>
				<param name="vsl" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
