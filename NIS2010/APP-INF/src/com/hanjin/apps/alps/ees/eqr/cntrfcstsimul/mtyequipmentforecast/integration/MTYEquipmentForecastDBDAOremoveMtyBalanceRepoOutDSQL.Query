<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MTYEquipmentForecastDBDAOremoveMtyBalanceRepoOutDSQL">
			<desc><![CDATA[EQR_CTRL_BAL_RPT_REPO_OUT 테이블 정보 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM EQR_CTRL_BAL_RPT_REPO_OUT
 WHERE LOC_GRP_CD = @[loc_grp_cd]
   AND LOC_CD     = @[loc_cd]
   AND INP_YRWK   = @[inp_yrwk]
   AND FCAST_YRWK = @[fcast_yrwk]
   AND CRE_SEQ    = @[cre_seq]			]]></sql>
			<params>
				<param name="loc_grp_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="inp_yrwk" type="12" value="" out="N"/>
				<param name="fcast_yrwk" type="12" value="" out="N"/>
				<param name="cre_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
