<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EqInterchangeDBDAOMasOffhBkgListUSQL">
			<desc><![CDATA[2016-04-11]]></desc>
			<sql><![CDATA[
MERGE INTO MAS_OFFH_BKG_LIST A
USING DUAL ON (A.CNTR_NO      = @[cntr_no]
               AND A.BKG_NO = @[bkg_no]
)
WHEN MATCHED
THEN UPDATE SET 
             A.UPD_DT   = SYSDATE
            ,A.UPD_USR_ID = @[upd_usr_id]              
     WHERE  1=1
       AND A.CNTR_NO = @[cntr_no]
       AND A.BKG_NO = @[bkg_no]
WHEN NOT MATCHED
THEN INSERT (A.CNTR_NO
            ,A.BKG_NO
            ,A.TPSZ_CD
            ,A.UPD_DT
            ,A.UPD_USR_ID
            ,A.CRE_DT
            ,A.CRE_USR_ID
            )
     VALUES (@[cntr_no]           
            ,@[bkg_no]
            ,@[cntr_tpsz_cd]                
            ,SYSDATE        
            ,@[upd_usr_id]                    
            ,SYSDATE              
            ,@[upd_usr_id]                 
     )			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
