<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRegistrationDBDAOAgreementDropOfficeDescRSQL">
			<desc><![CDATA[Lease Agreement Drop Office Description Search]]></desc>
			<sql><![CDATA[
SELECT A.AGMT_CTY_CD
     , A.AGMT_SEQ
     , A.LOC_CD
     , A.DPT_DESC
     , A.ADDR_DESC
     , A.CTRT_PSON_DESC
     , A.CTRT_NO_DESC
     , A.TURN_REF_NO_DESC
     , A.LSE_PSON_CTRT_EML
FROM   LSE_DRP_OFF_DESC A
WHERE  A.AGMT_CTY_CD = @[agmt_cty_cd]
AND    A.AGMT_SEQ = @[agmt_seq]
ORDER  BY A.LOC_CD			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
