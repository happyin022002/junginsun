<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanOptiExecuteDBDAOGetMaxPlanIDRSQL">
			<desc><![CDATA[repo plan의 Max ID 조회]]></desc>
			<sql><![CDATA[
SELECT MAX(TO_NUMBER (SUBSTR (REPO_PLN_ID, 12, 3)) + 1) 
	  	FROM EQR_EQ_REPO_PLN 
        WHERE SUBSTR (REPO_PLN_ID, 5, 6) = @[scnr_yrwk]			]]></sql>
			<params>
				<param name="scnr_yrwk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
