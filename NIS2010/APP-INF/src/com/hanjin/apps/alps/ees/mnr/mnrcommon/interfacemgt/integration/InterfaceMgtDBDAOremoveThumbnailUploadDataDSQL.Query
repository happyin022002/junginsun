<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOremoveThumbnailUploadDataDSQL">
			<desc><![CDATA[MNR_FILE_ATCH_EXTR의 파일을 삭제한다.(썸네일용 파일들로 COM_UPLD_FILE에 삭제 되기 전에 선행되어 삭제되어야 함)]]></desc>
			<sql><![CDATA[
DELETE FROM MNR_FILE_ATCH_EXTR A
WHERE FILE_SEQ = @[file_seq]
AND   FILE_DTL_SEQ = @[file_dtl_seq]			]]></sql>
			<params>
				<param name="file_seq" type="12" value="" out="N"/>
				<param name="file_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
