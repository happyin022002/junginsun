<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchSCExceptionInfoRSQL">
			<desc><![CDATA[SC Exception]]></desc>
			<sql><![CDATA[
SELECT   
D.PROP_NO			PROP_NO,
D.SC_EXPT_VER_SEQ	VER_SEQ,
D.SC_EXPT_GRP_SEQ  	GRP_SEQ,
D.FT_FLG		FTIME_MK,	
D.XCLD_SAT_FLG	EXCL_SAT,
D.XCLD_SUN_FLG	EXCL_SUN,	
D.XCLD_HOL_FLG	EXCL_HOLI,
D.FT_ADD_FLG	FT_ADD_MK,	
D.FT_ADD_DYS	FT_ADD_DAY,
D.FT_ADJ_FLG	FT_ADJ_MK,	
D.RT_ADJ_FLG	RT_ADJ_MK,
D.CURR_CD 		CUR_CD
FROM  DMT_SC_EXPT_GRP D, 
      PRI_SP_HDR P
WHERE D.PROP_NO = P.PROP_NO
AND P.SC_NO = @[sc_no]
AND D.SC_EXPT_VER_SEQ =  @[sc_expt_ver_seq]
AND D.SC_EXPT_GRP_SEQ =  @[sc_expt_grp_seq]			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
