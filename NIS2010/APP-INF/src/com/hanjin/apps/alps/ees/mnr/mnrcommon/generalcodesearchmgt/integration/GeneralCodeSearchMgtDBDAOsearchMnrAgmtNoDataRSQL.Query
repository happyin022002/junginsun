<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchMnrAgmtNoDataRSQL">
			<desc><![CDATA[Agreement No 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT AGMT_OFC_CTY_CD||LTRIM(TO_CHAR(AGMT_SEQ, '000000')) AGMT_NO
FROM MNR_AGMT_HDR
WHERE AGMT_OFC_CTY_CD  = SUBSTR(@[agmt_no], 1, 3)
AND   AGMT_SEQ         = TO_NUMBER(SUBSTR(@[agmt_no], 4))
#if (${ctrt_ofc_cd} != '')
AND   AGMT_OFC_CD      = @[ctrt_ofc_cd]
#end
AND   AGMT_LST_VER_FLG = 'Y'
AND   DELT_FLG         = 'N'
AND   ROWNUM = 1			]]></sql>
			<params>
				<param name="agmt_no" type="12" value="" out="N"/>
				<param name="ctrt_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
