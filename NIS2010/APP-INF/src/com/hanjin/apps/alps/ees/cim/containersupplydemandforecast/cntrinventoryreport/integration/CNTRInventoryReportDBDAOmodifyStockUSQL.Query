<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CNTRInventoryReportDBDAOmodifyStockUSQL">
			<desc><![CDATA[modifyStock]]></desc>
			<sql><![CDATA[
UPDATE CIM_STK_OPMZ SET  
       CNTR_QTY   = @[cntr_qty]
     , UPD_USR_ID = @[upd_usr_id]
     , UPD_DT	  = SYSDATE
WHERE YD_CD = @[loc_cd]
AND CNTR_TPSZ_CD = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="cntr_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
