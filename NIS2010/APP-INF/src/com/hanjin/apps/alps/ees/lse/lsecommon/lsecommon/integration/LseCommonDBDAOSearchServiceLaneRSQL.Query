<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LseCommonDBDAOSearchServiceLaneRSQL">
			<desc><![CDATA[Vessel SVC Lane 목록을 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT  VSL_SLAN_CD, VSL_SLAN_NM,
        VSL_SVC_TP_CD, CO_CD
FROM    MDM_VSL_SVC_LANE
WHERE   1 = 1
AND     NVL(DELT_FLG, 'N') <> 'Y'
AND     VSL_SLAN_CD = @[slan_cd]			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
