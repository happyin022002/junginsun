<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAOSearchApproSetupValidationRSQL">
			<desc><![CDATA[DemDetTariffMgtDBDAOSearchApproSetupValidationRSQL]]></desc>
			<sql><![CDATA[
SELECT DECODE(COUNT(*),0,'N','Y') VAL_FLG
  FROM DMT_APRO_STUP
 WHERE DMDT_EXPT_APRO_TP_CD = @[dmdt_expt_apro_tp_cd]
   AND OFC_LVL              = @[ofc_lvl]
   AND DMDT_OFC_CD          = @[dmdt_ofc_cd]
   AND CUST_CNT_CD          = CASE WHEN TRIM(UPPER(@[cust_cd])) = 'ALL' THEN 'AL'
  		                           ELSE SUBSTR(@[cust_cd],1,2) END
   AND CUST_SEQ             = CASE WHEN TRIM(UPPER(@[cust_cd])) = 'ALL' THEN 0
		                           ELSE TO_NUMBER(SUBSTR(@[cust_cd],3)) END			]]></sql>
			<params>
				<param name="dmdt_expt_apro_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_lvl" type="12" value="" out="N"/>
				<param name="dmdt_ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
