<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOSearchCHSCpsAgreementRateDataRSQL">
			<desc><![CDATA[CPS Agreement 에 해당하는 Rate 정보를 CGM_AGMT_CPS_RT 에서 조회한다.]]></desc>
			<sql><![CDATA[
SELECT LOC_CD,
       AMT_AUD_FLG,
       CHSS_POOL_RT_AMT,
       CHSS_POOL_TAX_RT,
       AGMT_RMK
FROM CGM_AGMT_CPS_RT
WHERE 1=1
AND AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
AND AGMT_SEQ = @[agmt_seq]
#if (${agmt_ver_no} != '')
	  AND AGMT_VER_NO = @[agmt_ver_no]
#end
ORDER BY AGMT_VER_NO			]]></sql>
			<params>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
