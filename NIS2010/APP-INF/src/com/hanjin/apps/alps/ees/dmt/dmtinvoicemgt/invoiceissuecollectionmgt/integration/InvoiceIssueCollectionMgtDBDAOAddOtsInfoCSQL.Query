<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOAddOtsInfoCSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOAddOtsInfoCSQL]]></desc>
			<sql><![CDATA[
insert into DMT_INV_OTS_PAY_RCV
(
    INV_PAY_RCV_SEQ
   ,DMDT_INV_NO
   ,BL_NO
   ,DMDT_INV_PAY_TP_CD
   ,IO_BND_CD
   ,INV_PAY_DT
   ,INV_PAY_AMT
   ,INV_CURR_CD
   ,INV_PAY_COFF_DT
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
   ,INV_PAY_OFC_CD
   ,EAI_IF_ID
   ,BKG_NO
   ,INV_BAL_AMT
   ,INV_PAY_LOCL_DT
   ,UPD_LOCL_DT
)
values
(
    @[inv_pay_rcv_seq]
   ,@[dmdt_inv_no]
   ,@[bl_no]
   ,@[dmdt_inv_pay_tp_cd]
   ,@[io_bnd_cd]
   ,TO_DATE(@[inv_pay_dt], 'YYYYMMDDHH24MISS')
   ,@[inv_pay_amt]
   ,@[inv_curr_cd]
   ,to_date(@[inv_pay_coff_dt], 'YYYYMMDD')
   ,@[cre_usr_id]
   ,sysdate
   ,@[upd_usr_id]
   ,sysdate
   ,@[inv_pay_ofc_cd]
   ,@[eai_if_id]
   ,@[bkg_no]
   ,@[inv_bal_amt]
   ,NVL(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('SELSC', TO_DATE(@[inv_pay_dt], 'YYYYMMDDHH24MISS'), @[inv_pay_ofc_cd]), TO_DATE(@[inv_pay_dt], 'YYYYMMDDHH24MISS'))
   ,NVL(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('SELSC', SYSDATE, @[inv_pay_ofc_cd]), SYSDATE)
)			]]></sql>
			<params>
				<param name="inv_pay_rcv_seq" type="12" value="" out="N"/>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="dmdt_inv_pay_tp_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="inv_pay_dt" type="12" value="" out="N"/>
				<param name="inv_pay_amt" type="12" value="" out="N"/>
				<param name="inv_curr_cd" type="12" value="" out="N"/>
				<param name="inv_pay_coff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="inv_pay_ofc_cd" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="inv_bal_amt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
