<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrMtyBkgCreateDBDAOSearchBkgLocCodeRSQL">
			<desc><![CDATA[YardCode 의 Loc Code 조회]]></desc>
			<sql><![CDATA[
SELECT A.LOC_CD
FROM MDM_LOCATION   A
    ,MDM_YARD       B
WHERE A.LOC_CD = B.LOC_CD
AND   B.YD_CD  = @[yd_cd]			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
