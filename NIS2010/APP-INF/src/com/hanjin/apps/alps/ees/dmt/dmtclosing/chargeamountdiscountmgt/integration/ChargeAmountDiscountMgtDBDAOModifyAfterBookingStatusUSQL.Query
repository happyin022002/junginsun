<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAOModifyAfterBookingStatusUSQL">
			<desc><![CDATA[등록된 After Booking DAR 의 상태를 변경하기 위한 쿼리]]></desc>
			<sql><![CDATA[
UPDATE	DMT_AFT_BKG_ADJ_RQST SET
	DMDT_EXPT_RQST_STS_CD = @[dmdt_expt_rqst_sts_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
,	UPD_OFC_CD = @[upd_ofc_cd]
#if(${dmdt_expt_rqst_sts_cd} == 'O' || ${dmdt_expt_rqst_sts_cd} == 'J')
,	AFT_BKG_APRO_NO = ''
#elseif(${dmdt_expt_rqst_sts_cd} == 'A')
,	AFT_BKG_APRO_NO = @[aft_bkg_apro_no]
,	APRO_USR_ID = @[upd_usr_id]
,	APRO_DT = NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
#end

WHERE	AFT_EXPT_DAR_NO = @[aft_expt_dar_no]
			]]></sql>
			<params>
				<param name="dmdt_expt_rqst_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="aft_bkg_apro_no" type="12" value="" out="N"/>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
