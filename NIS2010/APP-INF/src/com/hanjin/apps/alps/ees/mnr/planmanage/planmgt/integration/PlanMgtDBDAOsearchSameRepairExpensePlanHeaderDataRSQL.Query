<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanMgtDBDAOsearchSameRepairExpensePlanHeaderDataRSQL">
			<desc><![CDATA[searchSameRepairExpensePlanHeaderData select]]></desc>
			<sql><![CDATA[
SELECT 
	A.MNR_PLN_YR
,	A.MNR_PLN_OFC_CD
,   A.MNR_PLN_SEQ
FROM MNR_PLN_HDR A 
WHERE	A.MNR_GRP_TP_CD = 'RPR'
AND A.MNR_PLN_YR = @[mnr_pln_yr]
AND A.MNR_PLN_OFC_CD = @[mnr_pln_ofc_cd]			]]></sql>
			<params>
				<param name="mnr_pln_yr" type="12" value="" out="N"/>
				<param name="mnr_pln_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
