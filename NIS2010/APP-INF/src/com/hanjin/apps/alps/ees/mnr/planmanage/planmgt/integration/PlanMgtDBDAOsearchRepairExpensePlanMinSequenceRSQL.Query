<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanMgtDBDAOsearchRepairExpensePlanMinSequenceRSQL">
			<desc><![CDATA[Header의 데이터로 MNR_PLN_DTL_SEQ 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT MIN(MNR_PLN_DTL_SEQ) - 1
  FROM MNR_PLN_DTL
 WHERE 1=1
   AND CTRL_OFC_CD = @[ctrl_ofc_cd]
   AND MNR_PLN_SEQ = (SELECT MNR_PLN_SEQ
            	  		FROM MNR_PLN_HDR
            	 		WHERE 1=1
                   		AND MNR_PLN_YR  = @[mnr_pln_yr]
            			AND MNR_PLN_OFC_CD = @[ctrl_ofc_cd]
                	)
   AND OFC_TP_CD   = @[ofc_tp_cd] 			]]></sql>
			<params>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="mnr_pln_yr" type="12" value="" out="N"/>
				<param name="ofc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
