<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAORemoveDMTUserRoleInfoListDSQL">
			<desc><![CDATA[DMT User별 Role 정보를 삭제 합니다.]]></desc>
			<sql><![CDATA[
DELETE FROM DMT_USR_ROLE_MTCH
WHERE  1=1
AND    USR_ID      = @[usr_id]
AND    USR_ROLE_CD = @[usr_role_cd]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="usr_role_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
