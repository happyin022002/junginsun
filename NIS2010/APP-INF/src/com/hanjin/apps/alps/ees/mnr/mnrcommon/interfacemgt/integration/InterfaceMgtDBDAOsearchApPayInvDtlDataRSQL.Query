<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchApPayInvDtlDataRSQL">
			<desc><![CDATA[InterfaceMgtDBDAOsearchApPayInvDtlDataRSQL]]></desc>
			<sql><![CDATA[
SELECT  '' INV_RGST_NO,
        '' INV_RGST_SEQ,
        OD.COST_CD LGS_COST_CD,
        OD.ACCT_CD ACCT_CD,
        'CNTC' VSL_CD,
        TO_CHAR(OD.CRE_DT, 'YYMM') SKD_VOY_NO,
        'M' SKD_DIR_CD,
        'M' REV_DIR_CD,
        'CNT' SLAN_CD,
        '' ACT_VVD_CD,
        '' PORT_CD,
        '' YD_CD,
        OD.EQ_TPSZ_CD CNTR_TPSZ_CD,
        MNR_COMMON_PKG.MNR_CONV_DECIMAL_FNC(IW.CURR_CD,SUM(NVL(OD.INV_AMT,0))) INV_AMT,
        0 SO_20FT_QTY,
        0 SO_40FT_QTY,
        0 SO_TEU_QTY,
        0 SO_UT_QTY,
        '' SO_OFC_CTY_CD,
        0 SO_SEQ,
        'N' DELT_FLG
FROM   MNR_ORD_DTL OD, MNR_PAY_INV_WRK IW
WHERE  IW.PAY_INV_SEQ = OD.PAY_INV_SEQ
AND IW.PAY_INV_SEQ = @[pay_inv_seq]
GROUP BY OD.COST_CD, OD.ACCT_CD, OD.EQ_TPSZ_CD, IW.CURR_CD, OD.CRE_DT			]]></sql>
			<params>
				<param name="pay_inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
