<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EqInterchangeDBDAOUpdateEqInterchangeReqNoCancelUSQL">
			<desc><![CDATA[EQ interChange Update & Cancel 화면에서 Rre No를 Cancel 한다.]]></desc>
			<sql><![CDATA[
UPDATE LSE_EQ_ITCHG_RQST
SET LSE_ITCHG_AUTH_NO = NULL
   ,LSE_ITCHG_AUTH_SEQ = NULL
   ,UPD_USR_ID = @[upd_usr_id]
   ,UPD_DT = SYSDATE
WHERE 1=1 
AND LSE_ITCHG_RQST_NO = @[lse_itchg_rqst_no]
AND LSE_ITCHG_RQST_SEQ =@[lse_itchg_rqst_seq]
			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="lse_itchg_rqst_no" type="12" value="" out="N"/>
				<param name="lse_itchg_rqst_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
