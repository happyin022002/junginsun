<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAODeleteYearSubleasePlanDSQL">
			<desc><![CDATA[eqr_sublease  테이블 데이터 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM EQR_SUBLEASE
WHERE PLN_YRWK    = @[pln_yrwk]
AND FM_ECC_CD     = @[fm_ecc_cd]
AND TO_ECC_CD     = @[to_ecc_cd]
AND CNTR_TPSZ_CD  = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
