<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAOAddTrfGrpCfmHisCSQL">
			<desc><![CDATA[AddTrfGrpCfmHis]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_TRF_GRP_CFM_HIS
(
  SYS_AREA_GRP_ID
, DMDT_TRF_CD
, TRF_SEQ
, RGN_CFM_SEQ
, GRP_CFM_SEQ
, DMDT_DE_TERM_CD
, TRF_GRP_SEQ
, CRNT_CFM_FLG
, CRE_USR_ID
, CRE_DT
, CRE_OFC_CD
, UPD_USR_ID
, UPD_DT
)
VALUES
( 
  @[svr_id]
, @[dmdt_trf_cd]
, @[trf_seq]
, @[rgn_cfm_seq]
, (
  SELECT NVL(MAX(GRP_CFM_SEQ), 0) + 1
  FROM   DMT_TRF_GRP_CFM_HIS
  WHERE  SYS_AREA_GRP_ID    = @[svr_id]
  AND    DMDT_TRF_CD        = @[dmdt_trf_cd]
  AND    TRF_SEQ            = @[trf_seq]
  AND    RGN_CFM_SEQ        = @[rgn_cfm_seq]
  )
, @[dmdt_de_term_cd]
, @[trf_grp_seq]
, @[crnt_cfm_flg]
, @[usr_id]
, SYSDATE
, @[ofc_cd]
, @[usr_id]
, SYSDATE
)			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="trf_seq" type="2" value="" out="N"/>
				<param name="rgn_cfm_seq" type="2" value="" out="N"/>
				<param name="dmdt_de_term_cd" type="12" value="" out="N"/>
				<param name="trf_grp_seq" type="2" value="" out="N"/>
				<param name="crnt_cfm_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
