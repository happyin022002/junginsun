<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VslResidualSpaceManageDBDAOUpdateBSAInfoUSQL">
			<desc><![CDATA[EQR_SCNR_BSA_VVD 테이블 정보 수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_SCNR_BSA_VVD SET 
    VSL_SPC = @[vsl_spc] ,			
    VSL_WGT = @[vsl_wgt] , 			
    VSL_BSA_DRY_SUB_SPC = @[vsl_bsa_dry_sub_spc] , 
    VSL_BSA_RF_SUB_SPC = @[vsl_bsa_rf_sub_spc] ,  
    UPD_USR_ID = @[user_id],			
    UPD_DT = sysdate        
WHERE
    SCNR_ID = @[scnr_id]				
    AND FM_YRWK = @[fm_yrwk]					
    AND TO_YRWK = @[to_yrwk]				
    AND VSL_CD  = @[vsl_cd]						
    AND SKD_VOY_NO = @[skd_voy_no]			
    AND SKD_DIR_CD = @[skd_dir_cd]			
    AND CO_CD = @[co_cd]				
    AND TRD_CD = @[trd_cd]			
--    AND SUB_TRD_CD =   /sub_trd_cd/			
    AND VSL_LANE_CD = @[vsl_lane_cd]			]]></sql>
			<params>
				<param name="vsl_spc" type="2" value="" out="N"/>
				<param name="vsl_wgt" type="2" value="" out="N"/>
				<param name="vsl_bsa_dry_sub_spc" type="2" value="" out="N"/>
				<param name="vsl_bsa_rf_sub_spc" type="2" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="fm_yrwk" type="12" value="" out="N"/>
				<param name="to_yrwk" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="vsl_lane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
