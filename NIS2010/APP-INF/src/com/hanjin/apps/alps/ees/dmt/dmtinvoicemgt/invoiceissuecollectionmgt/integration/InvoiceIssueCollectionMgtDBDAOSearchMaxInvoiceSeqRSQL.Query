<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchMaxInvoiceSeqRSQL">
			<desc><![CDATA[Invoice 채번 테이블 max값 조회]]></desc>
			<sql><![CDATA[
SELECT 	DMDT_INV_SEQ + 1 AS DMDT_INV_SEQ
FROM    DMT_INV_NO
WHERE   INV_OFC_PFX_CD = @[inv_ofc_pfx_cd]
AND     DMDT_INV_TP_CD = @[dmdt_inv_tp_cd]
FOR UPDATE WAIT 10			]]></sql>
			<params>
				<param name="inv_ofc_pfx_cd" type="12" value="" out="N"/>
				<param name="dmdt_inv_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
