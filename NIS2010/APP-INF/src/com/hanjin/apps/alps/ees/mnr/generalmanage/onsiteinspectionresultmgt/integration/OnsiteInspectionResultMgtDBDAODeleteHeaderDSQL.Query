<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnsiteInspectionResultMgtDBDAODeleteHeaderDSQL">
			<desc><![CDATA[사용자가 지정한 Yard, Vendor, Date에 해당하는 Inspection Result를 Header에서 삭제]]></desc>
			<sql><![CDATA[
DELETE MNR_ONSITE_INSP_RSLT
WHERE VNDR_SEQ = @[vndr_seq]
AND YD_CD = @[yd_cd]
AND TO_CHAR(FLD_INSP_DT, 'YYYY-MM-DD') = @[fld_insp_dt]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="fld_insp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
