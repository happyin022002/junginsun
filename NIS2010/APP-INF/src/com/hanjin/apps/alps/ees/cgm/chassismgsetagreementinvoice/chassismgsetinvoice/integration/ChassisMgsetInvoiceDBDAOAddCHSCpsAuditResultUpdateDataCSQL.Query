<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOAddCHSCpsAuditResultUpdateDataCSQL">
			<desc><![CDATA[데이터를 CGM_LSE_INV_TMP에 insert.
2014.06.19 PAY_LSE_CHG_STS_CD_VARCHAR2(12)에 cost_yrmon_seq를 넣는부분 추가
]]></desc>
			<sql><![CDATA[
INSERT INTO CGM_LSE_INV_TMP 
(
 CHG_CRE_SEQ
,COST_YRMON
,INV_NO
,AGMT_OFC_CTY_CD
,AGMT_SEQ
,AGMT_VER_NO
,EQ_NO
,INV_REF_NO
,INV_EQ_NO
,CHG_CD
,INV_LSE_USE_DYS -- 임시로 INV_LSE_USE_DYS, 차후 CHG_SEQ 컬럼 생성 후 변경
,INV_EQ_ONH_LOC_NM
,INV_EQ_ONH_DT
,INV_EQ_OFFH_DT
,INV_BIL_UT_DYS
,INV_LSE_CHG_AMT
,INV_TAX_AMT
,INV_CR_AMT
,INV_SMRY_AMT
,LSE_CHG_AUD_STS_CD
,CRE_USR_ID
,CRE_DT
,UPD_USR_ID
,UPD_DT
,COST_YRMON_SEQ
)
values
(
 @[chg_cre_seq]
,@[cost_yrmon]
,@[inv_no]
,@[agmt_ofc_cty_cd]
,@[agmt_seq]
,@[agmt_ver_no]
,@[eq_no]
,@[seq]
,@[inv_cust_eq_no]
,@[chg_cd]
,@[chg_seq]
,@[inv_eq_onh_loc_nm]
,to_date(@[inv_eq_onh_dt], 'yyyyMMdd')
,to_date(@[inv_eq_offh_dt], 'yyyyMMdd')
,@[lse_bil_ut_dys]
,@[inv_lse_chg_amt]
,@[inv_tax_amt]
,@[inv_cr_amt]
,@[inv_lse_chg_ttl_amt]
,@[pay_lse_chg_sts_cd]
,@[cre_usr_id]
,sysdate
,@[upd_usr_id]
,sysdate
,@[cost_yrmon_seq]
)			]]></sql>
			<params>
				<param name="chg_cre_seq" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="seq" type="12" value="" out="N"/>
				<param name="inv_cust_eq_no" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
				<param name="inv_eq_onh_loc_nm" type="12" value="" out="N"/>
				<param name="inv_eq_onh_dt" type="12" value="" out="N"/>
				<param name="inv_eq_offh_dt" type="12" value="" out="N"/>
				<param name="lse_bil_ut_dys" type="12" value="" out="N"/>
				<param name="inv_lse_chg_amt" type="12" value="" out="N"/>
				<param name="inv_tax_amt" type="12" value="" out="N"/>
				<param name="inv_cr_amt" type="12" value="" out="N"/>
				<param name="inv_lse_chg_ttl_amt" type="12" value="" out="N"/>
				<param name="pay_lse_chg_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
