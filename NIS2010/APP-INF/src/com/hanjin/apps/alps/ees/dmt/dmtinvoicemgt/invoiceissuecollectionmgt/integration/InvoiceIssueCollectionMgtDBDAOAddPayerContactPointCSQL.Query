<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOAddPayerContactPointCSQL">
			<desc><![CDATA[DEM/DET Payer Info & Fax/E-mail]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_PAYR_CNTC_PNT (
	SYS_AREA_GRP_ID
,	CUST_CNT_CD
,	CUST_SEQ
,	CUST_CNTC_PNT_SEQ
,	DMDT_PAYR_CNTC_PNT_NM
,	PAYR_CNTC_PNT_PHN_NO
,	PAYR_CNTC_PNT_FAX_NO
,	PAYR_CNTC_PNT_EML
,	OTS_SND_FLG
,	CRE_USR_ID
,	CRE_DT
,	CRE_OFC_CD
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
) VALUES( 
	@[svr_id]
,	@[cust_cnt_cd]
,	@[cust_seq]
,	(SELECT NVL(MAX(CUST_CNTC_PNT_SEQ),0)+1 
     FROM DMT_PAYR_CNTC_PNT
     WHERE SYS_AREA_GRP_ID 	= @[svr_id]
	 AND CUST_CNT_CD 		= @[cust_cnt_cd]
	 AND CUST_SEQ			= @[cust_seq])
,	@[dmdt_payr_cntc_pnt_nm]
,	@[payr_cntc_pnt_phn_no]
,	@[payr_cntc_pnt_fax_no]
,	@[payr_cntc_pnt_eml]
,	DECODE(NVL(@[ots_snd_flg], 1), 0, 'N', 'Y')
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
,	@[cre_ofc_cd]
,	@[upd_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
,	@[upd_ofc_cd]
)			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="dmdt_payr_cntc_pnt_nm" type="12" value="" out="N"/>
				<param name="payr_cntc_pnt_phn_no" type="12" value="" out="N"/>
				<param name="payr_cntc_pnt_fax_no" type="12" value="" out="N"/>
				<param name="payr_cntc_pnt_eml" type="12" value="" out="N"/>
				<param name="ots_snd_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
