<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchCHSStatusHistoryLsiDataRSQL">
			<desc><![CDATA[CGM_EQ_STS_HIS 테이블에서 History 정보 가져오기 ]]></desc>
			<sql><![CDATA[


SELECT /*+ index_desc(a xpkCGM_EQ_sts_his) */ 
       A.STS_EVNT_YD_CD 
      ,A.STS_EVNT_LOC_CD
      ,A.STS_EVNT_OFC_CD
      ,to_char(A.STS_EVNT_DT,'yyyy-mm-dd HH24:MI:SS') STS_EVNT_DT
      ,A.AGMT_OFC_CTY_CD
      ,A.AGMT_SEQ
      ,A.AGMT_VER_NO
      ,A.EQ_ASET_STS_CD
FROM  CGM_EQ_STS_HIS A
WHERE A.EQ_NO = @[eq_no]
#if ( ${eq_aset_sts_cd} != '' )
 AND A.EQ_ASET_STS_CD ='LSI'
#end
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
