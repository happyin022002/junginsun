<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrMtyRouteSettingDBDAOSearchLocationByRccCodeRSQL">
			<desc><![CDATA[CntrMtyRouteSettingDBDAOSearchLocationByRccCode]]></desc>
			<sql><![CDATA[
SELECT A.RCC_CD RCC
   FROM MDM_EQ_ORZ_CHT A, MDM_LOCATION B
  WHERE A.SCC_CD = B.SCC_CD
    AND B.LOC_CD = @[loccd]			]]></sql>
			<params>
				<param name="loccd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
