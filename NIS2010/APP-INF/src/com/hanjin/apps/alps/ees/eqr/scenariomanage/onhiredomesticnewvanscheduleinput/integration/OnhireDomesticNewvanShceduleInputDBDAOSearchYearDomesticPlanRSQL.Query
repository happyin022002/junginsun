<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireDomesticNewvanShceduleInputDBDAOSearchYearDomesticPlanRSQL">
			<desc><![CDATA[eqr_scnr_dmst 테이블의 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT
    MAX (DECODE (PLN_MON, 01, CNTR_VOL_QTY)) S1_01_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 02, CNTR_VOL_QTY)) S1_02_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 03, CNTR_VOL_QTY)) S1_03_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 04, CNTR_VOL_QTY)) S1_04_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 05, CNTR_VOL_QTY)) S1_05_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 06, CNTR_VOL_QTY)) S1_06_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 07, CNTR_VOL_QTY)) S1_07_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 08, CNTR_VOL_QTY)) S1_08_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 09, CNTR_VOL_QTY)) S1_09_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 10, CNTR_VOL_QTY)) S1_10_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 11, CNTR_VOL_QTY)) S1_11_CNTR_VOL_QTY,
    MAX (DECODE (PLN_MON, 12, CNTR_VOL_QTY)) S1_12_CNTR_VOL_QTY
FROM (
    SELECT	 B.PLN_MON
			,SUM (A.CNTR_VOL_QTY) CNTR_VOL_QTY
    FROM	EQR_SCNR_DMST A, EQR_WK_PRD B
    WHERE	A.PLN_YRWK	= B.PLN_YR || B.PLN_WK
    	AND A.PLN_YRWK BETWEEN @[fmPlnYrwk] AND @[toPlnYrwk]
    	AND A.SCNR_ID	=  @[scnr_id]
    GROUP BY B.PLN_MON
) C			]]></sql>
			<params>
				<param name="fmPlnYrwk" type="12" value="" out="N"/>
				<param name="toPlnYrwk" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
