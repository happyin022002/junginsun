<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchDummyInvSrcNoRSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOSearchDummyInvSrcNoRSQL]]></desc>
			<sql><![CDATA[
SELECT  ATTR_CTNT2 AS INV_SRC_NO    -- DUMMY INVOICE SRC NO.
  FROM  DMT_HRD_CDG_CTNT
 WHERE  HRD_CDG_ID = 'AR_IF_DMY_INV_SRC_NO'
   AND  ATTR_CTNT1 = @[invoice_no]  -- INVOICE SRC NO.			]]></sql>
			<params>
				<param name="invoice_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
