<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InputDataRLAExamineDBDAOUpdateRLAEccInfo_str20QueryUSQL">
			<desc><![CDATA[EQR_SCNR_ECC  테이블에 대한 STO 20feet amount  수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_SCNR_ECC	SET 
  	STO_20FT_COST_AMT 	= @[sto_20ft_cost_amt] 
,	UPD_USR_ID         	= @[upd_usr_id] 
,	UPD_DT             	= SYSDATE                        
WHERE 
 	SCNR_ID          	= @[scnr_id]                             	
AND ECC_CD	   			= @[ecc_cd]			]]></sql>
			<params>
				<param name="sto_20ft_cost_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
