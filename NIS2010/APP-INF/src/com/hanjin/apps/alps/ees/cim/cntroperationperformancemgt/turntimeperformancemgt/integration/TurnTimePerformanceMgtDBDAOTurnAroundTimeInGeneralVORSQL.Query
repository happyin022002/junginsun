<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TurnTimePerformanceMgtDBDAOTurnAroundTimeInGeneralVORSQL">
			<desc><![CDATA[...........]]></desc>
			<sql><![CDATA[
SELECT
        /*+ INDEX (D XAK1CIM_SHP_TURN_TM_SMRY) */
		TRD_CD||'-'||TRNK_DIR_CD loccode1, 
		SLAN_CD loccode2, 
		ROUND(SUM( 
			OB_FULL_LAND_TT_DYS	+ 
			FULL_SEA_TT_DYS	+ 
			TS_LAND_TT_DYS	+ 
			TS_SEA_TT_DYS	+ 
			IB_FULL_LAND_TT_DYS	+ 
			MTY_LAND_TT_DYS	+ 
			MTY_SEA_TT_DYS 
		)/SUM(CNTR_KNT),1)  total,											
		ROUND(SUM(OB_FULL_LAND_TT_DYS)/SUM(CNTR_KNT),1) obFullLandTime,		
		ROUND(SUM(FULL_SEA_TT_DYS)/SUM(CNTR_KNT),1)  fullSeaTime,		
		ROUND(SUM(TS_LAND_TT_DYS)/SUM(CNTR_KNT),1)  tsLandTime,		
		ROUND(SUM(TS_SEA_TT_DYS)/SUM(CNTR_KNT),1)  tsSeaTime,		
		ROUND(SUM(IB_FULL_LAND_TT_DYS)/SUM(CNTR_KNT),1)  ibFullLandTime,		
		ROUND(SUM(MTY_LAND_TT_DYS)/SUM(CNTR_KNT),1)  mtyLandTime,		
		ROUND(SUM(MTY_SEA_TT_DYS)/SUM(CNTR_KNT),1)  mtySeaTime,	
		ROUND(365/(SUM( 
			OB_FULL_LAND_TT_DYS	+ 
			FULL_SEA_TT_DYS	+ 
			TS_LAND_TT_DYS	+ 
			TS_SEA_TT_DYS	+ 
			IB_FULL_LAND_TT_DYS	+ 
			MTY_LAND_TT_DYS	+ 
			MTY_SEA_TT_DYS 
		    )/	SUM(CNTR_KNT)),1) eq,
		SUM(CNTR_KNT) cntrVolume								
FROM	CIM_SHP_TURN_TM_SMRY		D

#if (${subtrade} != '' && ${subtrade} != 'ALL') 
         ,(SELECT DISTINCT M.TRD_CD||SUBSTR(M.RLANE_CD, 1, 3)||M.SUB_TRD_CD AS SUBT
           FROM MDM_DTL_REV_LANE M
          ) M
#end

#if (${period} == 'M') 
WHERE	TGT_MVMT_DT	BETWEEN    @[from]	AND @[to]
#elseif (${period} == 'W') 
WHERE	TGT_YRWK	BETWEEN    @[from]	AND @[to]	
#end

#if (${trade} != '') 
AND		TRD_CD			=	@[trade]
#end
#if (${bound} != '') 
AND		TRNK_DIR_CD		=	@[bound]
#end
#if (${lane} != '')  
AND		SLAN_CD			=	@[lane]
#end

#if (${tpsz} != '') 
AND     CNTR_TPSZ_CD		IN	(
#foreach($cntrtpszcd in ${vel_tpsz_cd})  
	'$cntrtpszcd',  
	#end  
	'') 
#end 



#if (${tscntr} == 'E') 
AND		TS_FLG		=	'N' 
#elseif (${tscntr} == 'O') 
AND		TS_FLG		=	'Y' 
#end 
#if (${soc} == 'E') 
AND		SOC_FLG		=	'N' 
#elseif (${soc} == 'O') 
AND		SOC_FLG		=	'Y' 
#end 

#if (${subtrade} != '' && ${subtrade} != 'ALL') 
AND   TRD_CD  = SUBSTR(M.SUBT, 1, 3)
AND   SLAN_CD = SUBSTR(M.SUBT, 4, 3)
AND   SUBSTR(M.SUBT, 7, 2) = @[subtrade]
#end 

GROUP BY 
		RollUp(TRD_CD||'-'||TRNK_DIR_CD, SLAN_CD)
ORDER BY
		TRD_CD||'-'||TRNK_DIR_CD,
		SLAN_CD			]]></sql>
			<params>
				<param name="from" type="12" value="" out="N"/>
				<param name="to" type="12" value="" out="N"/>
				<param name="trade" type="12" value="" out="N"/>
				<param name="bound" type="12" value="" out="N"/>
				<param name="lane" type="12" value="" out="N"/>
				<param name="subtrade" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
