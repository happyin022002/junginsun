<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAODeleteEccTurnTimeDSQL">
			<desc><![CDATA[Turn Time 삭제]]></desc>
			<sql><![CDATA[
DELETE  FROM    EQR_ECC_TURN_TM
WHERE   FM_ECC_CD       = @[fm_ecc_cd]
    AND   TO_ECC_CD     = @[to_ecc_cd]
    AND   CNTR_TPSZ_CD  = @[cntr_tpsz_cd]
    AND   IO_BND_CD     = @[io_bnd_cd]
    AND   CO_CD         = @[co_cd]			]]></sql>
			<params>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
