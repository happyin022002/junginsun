<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOCheckUCCaseNoRSQL">
			<desc><![CDATA[Uncollected Cargo Case Number 생성 ]]></desc>
			<sql><![CDATA[
SELECT	TO_CHAR(NVL(MAX(SUBSTR(uc_cs_no,10,12)),0) + 1,'009') UC_CS_NO
FROM    CIM_UC_CGO
WHERE   SUBSTR(uc_cs_no,1,9) = @[uc_cd_hd]			]]></sql>
			<params>
				<param name="uc_cd_hd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
