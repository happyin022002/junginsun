<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOSearchCntrSpecRSQL">
			<desc><![CDATA[SearchCntrSpec]]></desc>
			<sql><![CDATA[
SELECT
A.CNTR_SPEC_NO
,	A.OWN_CNTR_FLG
,	A.SPEC_YR
,	A.CNTR_TPSZ_CD
,	A.CNTR_MTRL_CD
,	A.LOD_CAPA
,	A.CNTR_GRS_WGT
,	A.TARE_WGT
,	A.INTER_LEN
,	A.INTER_WDT
,	A.INTER_HGT
,	A.XTER_LEN
,	A.XTER_WDT
,	A.XTER_HGT
,	A.OPN_DOR_WDT
,	A.OPN_DOR_HGT
,	A.RC_LDB_CAPA
,	A.RC_LDB_HGT
,	A.TNK_CAPA
,	A.DIFF_RMK
,	A.CRE_USR_ID
,	A.CRE_DT
,	A.UPD_USR_ID
,	A.UPD_DT
,   LTRIM(TO_CHAR(A.VNDR_SEQ,'000000')) AS VNDR_SEQ2
,   MDM.VNDR_ABBR_NM
FROM MST_CNTR_SPEC A, MDM_VENDOR MDM 
WHERE	A.CNTR_SPEC_NO = @[cntr_spec_no]
AND A.VNDR_SEQ = MDM.VNDR_SEQ(+)			]]></sql>
			<params>
				<param name="cntr_spec_no" type="12" value="1986-D2-025" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
