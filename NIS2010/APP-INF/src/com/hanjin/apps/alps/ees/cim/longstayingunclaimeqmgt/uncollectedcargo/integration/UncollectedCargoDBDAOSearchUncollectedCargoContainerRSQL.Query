<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOSearchUncollectedCargoContainerRSQL">
			<desc><![CDATA[UncollectedCargoDBDAOSearchUncollectedCargoContainerRSQL]]></desc>
			<sql><![CDATA[
#if (${flag} == 'Y')
	SELECT  C.CNTR_NO, C.BL_NO,
            (SELECT G.HNDL_BRNC_CD
              FROM CIM_UC_CGO G
              WHERE C.UC_CS_NO = G.UC_CS_NO
            ) HNDL_BRNC_CD,
            (SELECT G.HNDL_HDLR_USR_ID
              FROM CIM_UC_CGO G
              WHERE C.UC_CS_NO = G.UC_CS_NO
             ) HNDL_HDLR_USR_ID	
	FROM	CIM_UC_CGO_CNTR C
	WHERE	1 = 1
	AND C.UC_CS_NO = @[uc_cs_no]
#else
	SELECT  CNTR_NO, C.BL_NO,
            (SELECT G.HNDL_BRNC_CD
              FROM CIM_UC_CGO G
              WHERE C.UC_CS_NO = G.UC_CS_NO
            ) HNDL_BRNC_CD,
            (SELECT G.HNDL_HDLR_USR_ID
              FROM CIM_UC_CGO G
              WHERE C.UC_CS_NO = G.UC_CS_NO
             ) HNDL_HDLR_USR_ID	
	FROM	CIM_UC_CGO_CNTR C
	WHERE	1 = 1
	AND C.UC_CS_NO = @[uc_cs_no]
	AND C.BL_NO = @[bl_no]
#end			]]></sql>
			<params>
				<param name="uc_cs_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
