<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LeaseInfoManageDBDAOUpdateLTOffHireInfoUSQL">
			<desc><![CDATA[EQR_SCNR_LONG_TERM_OFFH_COND테이블 데이터 수정]]></desc>
			<sql><![CDATA[
UPDATE  EQR_SCNR_LONG_TERM_OFFH_COND          	
SET AVAL_OFFH_QTY    = @[aval_offh_qty],        	
	LTOF_CHG_AMT     = @[ltof_chg_amt],         	
	DRYG_AMT         = @[dryg_amt],
	DRFF_CHG_CR_AMT  = @[drff_chg_cr_amt],         	
	UPD_USR_ID       = @[upd_usr_id],         	
	UPD_DT           = sysdate    	
WHERE       SCNR_ID          = @[scnr_id]          	
		 AND ECC_CD           = @[ecc_cd]        	
		 AND CTRT_OFC_CTY_CD  = @[ctrt_ofc_cty_cd]          	
   		 AND CTRT_SEQ         = @[ctrt_seq]           	
   		 AND CNTR_TPSZ_CD     = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="aval_offh_qty" type="12" value="" out="N"/>
				<param name="ltof_chg_amt" type="12" value="" out="N"/>
				<param name="dryg_amt" type="12" value="" out="N"/>
				<param name="drff_chg_cr_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="ctrt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="ctrt_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
