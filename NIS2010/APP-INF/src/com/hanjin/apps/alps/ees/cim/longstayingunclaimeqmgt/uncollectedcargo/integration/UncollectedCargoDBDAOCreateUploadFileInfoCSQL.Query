<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOCreateUploadFileInfoCSQL">
			<desc><![CDATA[CreateUploadFileInfo ]]></desc>
			<sql><![CDATA[
INSERT INTO CIM_UC_CGO_FILE (
     UC_CS_NO
    , UC_CGO_FILE_ID
    , FILE_NO_SEQ
    , FILE_SAV_ID
    , FILE_NM
    , FILE_DESC
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT
)VALUES(
      @[s_uc_cs_no]
	, @[s_uc_cgo_file_id]
	, @[s_file_no_seq]
    , @[s_file_sav_id]
    , REPLACE(@[s_file_nm],'''','')
    , @[s_file_desc]
    , @[s_cre_usr_id]
    , SYSDATE
    , @[s_upd_usr_id]
    , SYSDATE
)			]]></sql>
			<params>
				<param name="s_uc_cs_no" type="12" value="" out="N"/>
				<param name="s_uc_cgo_file_id" type="12" value="" out="N"/>
				<param name="s_file_no_seq" type="12" value="" out="N"/>
				<param name="s_file_sav_id" type="12" value="" out="N"/>
				<param name="s_file_nm" type="12" value="" out="N"/>
				<param name="s_file_desc" type="12" value="" out="N"/>
				<param name="s_cre_usr_id" type="12" value="" out="N"/>
				<param name="s_upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
