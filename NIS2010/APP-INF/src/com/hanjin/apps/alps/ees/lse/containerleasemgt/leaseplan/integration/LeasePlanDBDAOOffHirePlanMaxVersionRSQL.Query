<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LeasePlanDBDAOOffHirePlanMaxVersionRSQL">
			<desc><![CDATA[Search Off-Hire Plan Max Version Sequence]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(A.OFFH_VER_SEQ),1) AS OFFH_VER_MAX_SEQ
FROM   LSE_OFFH_PLN A
WHERE  A.PLN_YR = @[pln_yr]
AND    A.OFFH_PLN_TP_CD = @[offh_pln_tp_cd]
AND    A.OFFH_LOC_TP_CD = @[offh_loc_tp_cd]			]]></sql>
			<params>
				<param name="pln_yr" type="12" value="" out="N"/>
				<param name="offh_pln_tp_cd" type="12" value="" out="N"/>
				<param name="offh_loc_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
