<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOCheckCalcTypeRSQL">
			<desc><![CDATA[선택한 Tariff Type 과 Coverage 가 Calculation Type 인지를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC( DMT_CALC_TP XPKDMT_CALC_TP) */ 
        DECODE(DMDT_CALC_TP_CD, @[dmdt_calc_tp_cd], 1, 0) AS CNT 
FROM    DMT_CALC_TP
WHERE   ( CNT_CD  =  @[cnt_cd]    OR  CNT_CD = ' ' )
AND ( RGN_CD  =  @[rgn_cd]    OR  RGN_CD = ' ' )
AND ( STE_CD  =  @[ste_cd]    OR  STE_CD = ' ' )
AND ( LOC_CD  =  @[loc_cd]    OR  LOC_CD = ' ' )
AND IO_BND_CD =  @[io_bnd_cd]
AND CALC_TP_SEQ =    
  (
   SELECT  /*+ INDEX_DESC( D XPKDMT_CALC_TP) */ D.CALC_TP_SEQ
   FROM DMT_CALC_TP D
   WHERE   ( D.CNT_CD  =  @[cnt_cd]  OR D.CNT_CD = ' ' )
    AND ( D.RGN_CD  =  @[rgn_cd]  OR D.RGN_CD = ' ' )
    AND ( D.STE_CD  =  @[ste_cd]  OR D.STE_CD = ' ' )
    AND ( D.LOC_CD  =  @[loc_cd]  OR D.LOC_CD = ' ' )
    AND D.IO_BND_CD =  @[io_bnd_cd]
    AND (
                       ( NVL(@[eff_dt], 'N') = 'N' )
                    OR ( NVL(@[eff_dt], 'N') <> 'N'
                  AND EFF_DT <= TO_DATE (Replace(@[eff_dt], '-', ''), 'YYYYMMDD')
      )
     )
                AND (
                       ( NVL(@[exp_dt], 'N') = 'N' )
                    OR ( NVL(@[exp_dt], 'N') <> 'N'
                  AND NVL(EXP_DT, TO_DATE (Replace(@[exp_dt], '-', ''), 'YYYYMMDD')) >= TO_DATE (Replace(@[exp_dt], '-', ''), 'YYYYMMDD') 
      )
     )
    AND ROWNUM = 1
  )
AND (
                       ( NVL(@[eff_dt], 'N') = 'N' )
                    OR ( NVL(@[eff_dt], 'N') <> 'N'
                  AND EFF_DT <= TO_DATE (Replace(@[eff_dt], '-', ''), 'YYYYMMDD')
      )
 )
AND (
                       ( NVL(@[exp_dt], 'N') = 'N' )
                    OR ( NVL(@[exp_dt], 'N') <> 'N'
                  AND NVL(EXP_DT, TO_DATE (Replace(@[exp_dt], '-', ''), 'YYYYMMDD')) >= TO_DATE (Replace(@[exp_dt], '-', ''), 'YYYYMMDD') 
      )
 )
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="dmdt_calc_tp_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
