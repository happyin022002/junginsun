<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchCreditNoteYnRSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOSearchCreditNoteYnRSQL]]></desc>
			<sql><![CDATA[
SELECT  DECODE(DMDT_INV_STS_CD, 'C', 'Y', 'N')
  FROM  DMT_INV_MN
 WHERE  DMDT_INV_NO = @[dmdt_inv_no]
   AND  CRE_OFC_CD = @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
