<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOcheckBareMGSetDataRSQL">
			<desc><![CDATA[MgSetNo조회]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(1) >= 1 THEN 'T'  -- T: 사용가능
                               ELSE 'F'  -- F: 입력불가 
       END CHK_MGSET                        
FROM CGM_EQUIPMENT
WHERE EQ_NO        = @[mgst_no]  -- 검색조건(mgset)
AND   EQ_KND_CD    = 'G' -- M.G.Set (Hard Coding)
AND   ACIAC_DIV_CD = 'A' -- Active(Hard Coding) 			]]></sql>
			<params>
				<param name="mgst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
