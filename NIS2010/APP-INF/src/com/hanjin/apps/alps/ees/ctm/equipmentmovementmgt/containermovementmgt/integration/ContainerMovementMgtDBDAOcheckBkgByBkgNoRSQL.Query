<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOcheckBkgByBkgNoRSQL">
			<desc><![CDATA[부킹 번호의 DE_TERM 유무를 조회한다]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
FROM   BKG_BOOKING
WHERE  BKG_NO       =  @[bkg_no]
AND    POD_CD      = DEL_CD
AND    DE_TERM_CD = 'Y'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
