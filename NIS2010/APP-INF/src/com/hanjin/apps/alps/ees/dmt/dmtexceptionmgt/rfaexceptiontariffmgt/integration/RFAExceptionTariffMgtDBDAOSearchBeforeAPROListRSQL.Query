<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchBeforeAPROListRSQL">
			<desc><![CDATA[DEM/DET Adjustment Request - Before Booking Request 의 DAR No. 의 VER No. 별 승인번호 정보를 조회하기 위한 쿼리]]></desc>
			<sql><![CDATA[
SELECT 	RFA_EXPT_APRO_NO
FROM 	DMT_RFA_EXPT_TRF
WHERE	
	#if(${prop_no} != '')
		PROP_NO = @[prop_no]
	AND	(RFA_EXPT_APRO_NO IS NOT NULL OR RFA_EXPT_APRO_NO <> '')
	#else
		RFA_EXPT_DAR_NO 	= @[rfa_expt_dar_no]
	AND	RFA_EXPT_MAPG_SEQ 	= @[rfa_expt_mapg_seq]
	AND	RFA_EXPT_VER_SEQ 	= @[rfa_expt_ver_seq]
	#end			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
