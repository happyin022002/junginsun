<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOSearchUncollectedCargoFileRSQL">
			<desc><![CDATA[UC Activity 리스트 조회 ]]></desc>
			<sql><![CDATA[
SELECT UC_CS_NO
        , UC_CGO_FILE_ID
        , FILE_NO_SEQ
        , FILE_DESC
        , (SELECT OFC_CD FROM COM_USER WHERE USR_ID = A.UPD_USR_ID) AS OFC_CD
        , UPD_USR_ID
        , NVL2(UC_CGO_FILE_ID,'FILE ATTACH','') IMG_FILE_NO
        , UPD_DT
        , (SELECT COUNT(*) FROM COM_UPLD_FILE B WHERE FILE_SAV_ID = A.FILE_SAV_ID AND PGM_SUB_SYS_CD = 'CIM') AS FILE_CNT
        , FILE_SAV_ID
		, '' as s_uc_cs_no
		, '' as s_uc_cgo_file_id
		, '' as cre_usr_id
FROM CIM_UC_CGO_FILE A
WHERE UC_CS_NO = @[s_uc_cs_no]
AND   UC_CGO_FILE_ID = @[s_uc_cgo_file_id]
ORDER BY FILE_NO_SEQ			]]></sql>
			<params>
				<param name="s_uc_cs_no" type="12" value="" out="N"/>
				<param name="s_uc_cgo_file_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
