<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepoThresholdManageDBDAOSearchRepoPlanRLAThresholdRSQL">
			<desc><![CDATA[EQR_REPO_PLN_RED_LGT_ALT_MST  테이블의 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT
    A.RCC_CD
    ,B.PERF_DUR_WKS
    ,B.TRSP_CAPA_RTO
    ,B.LS_RTO
    ,'' ADHERENCE
FROM
(
	-- RCC CODE
    SELECT 
        INTG_CD_VAL_CTNT
        ,INTG_CD_VAL_DP_DESC RCC_CD
        ,INTG_CD_VAL_DP_SEQ
    FROM COM_INTG_CD_DTL
    WHERE INTG_CD_ID = 'CD00255'
    ORDER BY INTG_CD_VAL_DP_SEQ
) A,
EQR_REPO_PLN_RED_LGT_ALT_MST B
WHERE A.RCC_CD = B.RCC_CD(+)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
