		<query name="ChassisMgsetInvoiceDBDAOAddCHSCpsAuditInvoiceOnlyDetailDataCSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
INSERT INTO CGM_LSE_CHG_DTL
(
	INV_NO,
	AGMT_OFC_CTY_CD,
	AGMT_SEQ,
	AGMT_VER_NO,
	INV_REF_NO,
	INV_EQ_NO,
	EQ_NO,
	INV_CUST_EQ_NO,
	EQ_TPSZ_CD,
	CHG_CD,
	CHG_SEQ,
	INV_EQ_ONH_LOC_NM,
	INV_EQ_ONH_DT,
	INV_EQ_OFFH_DT,
	INV_BIL_ST_DT,
	INV_BIL_END_DT,
	INV_LSE_USE_DYS,
	INV_BIL_UT_DYS,
	INV_LSE_RT_AMT,
	INV_TAX_AMT,
	INV_LSE_CHG_AMT,
	LSE_CHG_AUD_STS_CD,
	PAY_LSE_CHG_STS_CD,
	LSE_CHG_AUD_RSLT_RSN_CD,
	EQ_FM_MVMT_DT,
	EQ_FM_YD_CD,
	EQ_FM_MVMT_CD,
	EQ_TO_MVMT_DT,
	EQ_TO_YD_CD,
	EQ_TO_MVMT_CD,
	PAY_LSE_USE_DYS,
	PAY_BIL_UT_DYS,
	PAY_LSE_RT_AMT,
	PAY_TAX_AMT,
	PAY_LSE_CHG_AMT,
	INV_SC_NO,
	INV_BKG_NO,
	INV_BKG_TERM_CD,
	COST_YRMON,
	COST_CD,
	ACCT_CD,
	EQ_KND_CD,
	INV_CR_AMT,
	INV_TAX_RT_AMT,
	INV_LSE_CHG_AMT_TTL,
	PAY_TAX_RT_AMT,
	PAY_LSE_CHG_AMT_TTL,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
) 
SELECT
	@[inv_no],
	@[agmt_ofc_cty_cd],
	@[agmt_seq],
	@[agmt_ver_no],
	@[inv_ref_no],
	CASE WHEN @[chg_cd] = 'CRD' THEN
            (SELECT DECODE(MAX(EQ_NO),NULL,  
                        		'CRD' || SUBSTR(@[cost_yrmon],3,6) || '001',  
                        	    SUBSTR(MAX(EQ_NO),1,7) || LPAD( SUBSTR(MAX(EQ_NO),-3)+1,3,'0'))
             FROM CGM_LSE_CHG_DTL
             WHERE COST_YRMON = @[cost_yrmon]
                   AND EQ_KND_CD = @[eq_knd_cd]
                   AND EQ_NO LIKE ('CRD' || '%'))
		 WHEN @[chg_cd] = 'TAX' THEN
            (SELECT DECODE(MAX(EQ_NO),NULL,  
                        		'TAX' || SUBSTR(@[cost_yrmon],3,6) || '001',  
                        	    SUBSTR(MAX(EQ_NO),1,7) || LPAD( SUBSTR(MAX(EQ_NO),-3)+1,3,'0'))
             FROM CGM_LSE_CHG_DTL
             WHERE COST_YRMON = @[cost_yrmon]
                   AND EQ_KND_CD = @[eq_knd_cd]
                   AND EQ_NO LIKE ('TAX' || '%'))
         ELSE 
			@[eq_no] 
	END,
	CASE WHEN @[chg_cd] = 'CRD' THEN
            (SELECT DECODE(MAX(EQ_NO),NULL,  
                        		'CRD' || SUBSTR(@[cost_yrmon],3,6) || '001',  
                        	    SUBSTR(MAX(EQ_NO),1,7) || LPAD( SUBSTR(MAX(EQ_NO),-3)+1,3,'0'))
             FROM CGM_LSE_CHG_DTL
             WHERE COST_YRMON = @[cost_yrmon]
                   AND EQ_KND_CD = @[eq_knd_cd]
                   AND EQ_NO LIKE ('CRD' || '%'))
		 WHEN @[chg_cd] = 'TAX' THEN
            (SELECT DECODE(MAX(EQ_NO),NULL,  
                        		'TAX' || SUBSTR(@[cost_yrmon],3,6) || '001',  
                        	    SUBSTR(MAX(EQ_NO),1,7) || LPAD( SUBSTR(MAX(EQ_NO),-3)+1,3,'0'))
             FROM CGM_LSE_CHG_DTL
             WHERE COST_YRMON = @[cost_yrmon]
                   AND EQ_KND_CD = @[eq_knd_cd]
                   AND EQ_NO LIKE ('TAX' || '%'))
         ELSE 
			@[eq_no] 
	END,
	@[inv_cust_eq_no],
	@[eq_tpsz_cd],
	@[chg_cd],
	(SELECT NVL(MAX(CHG_SEQ),0)+1 FROM CGM_LSE_CHG_DTL 
	WHERE 
    AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
    AND AGMT_SEQ = @[agmt_seq]
    AND AGMT_VER_NO = @[agmt_ver_no]
    AND COST_YRMON = @[cost_yrmon]
    AND EQ_NO = (	CASE WHEN @[chg_cd] = 'CRD' THEN
            (SELECT DECODE(MAX(EQ_NO),NULL,  
                        		'CRD' || SUBSTR(@[cost_yrmon],3,6) || '001',  
                        	    SUBSTR(MAX(EQ_NO),1,7) || LPAD( SUBSTR(MAX(EQ_NO),-3)+1,3,'0'))
             FROM CGM_LSE_CHG_DTL
             WHERE COST_YRMON = @[cost_yrmon]
                   AND EQ_KND_CD = @[eq_knd_cd]
                   AND EQ_NO LIKE ('CRD' || '%'))
		 WHEN @[chg_cd] = 'TAX' THEN
            (SELECT DECODE(MAX(EQ_NO),NULL,  
                        		'TAX' || SUBSTR(@[cost_yrmon],3,6) || '001',  
                        	    SUBSTR(MAX(EQ_NO),1,7) || LPAD( SUBSTR(MAX(EQ_NO),-3)+1,3,'0'))
             FROM CGM_LSE_CHG_DTL
             WHERE COST_YRMON = @[cost_yrmon]
                   AND EQ_KND_CD = @[eq_knd_cd]
                   AND EQ_NO LIKE ('TAX' || '%'))
         ELSE 
			@[eq_no] 
	END)
    AND CHG_CD = @[chg_cd])
	,
	@[inv_eq_onh_loc_nm],
	DECODE(@[inv_eq_onh_dt],'','',TO_DATE(@[inv_eq_onh_dt],'YYYYMMDD')),
	DECODE(@[inv_eq_offh_dt],'','',TO_DATE(@[inv_eq_offh_dt],'YYYYMMDD')),
	DECODE(@[inv_bil_st_dt],'','',TO_DATE(@[inv_bil_st_dt],'YYYYMMDD')),
	DECODE(@[inv_bil_end_dt],'','',TO_DATE(@[inv_bil_end_dt],'YYYYMMDD')),
	@[inv_lse_use_dys],
	@[inv_bil_ut_dys],
	@[inv_lse_rt_amt],
	@[inv_tax_amt],
	@[inv_lse_chg_amt],
	@[lse_chg_aud_sts_cd],
	@[pay_lse_chg_sts_cd],
	@[lse_chg_aud_rslt_rsn_cd],
	DECODE(@[eq_fm_mvmt_dt],'','',TO_DATE(@[eq_fm_mvmt_dt],'YYYYMMDD')),
	@[eq_fm_yd_cd],
	@[eq_fm_mvmt_cd],
	DECODE(@[eq_to_mvmt_dt],'','',TO_DATE(@[eq_to_mvmt_dt],'YYYYMMDD')),
	@[eq_to_yd_cd],
	@[eq_to_mvmt_cd],
	@[pay_lse_use_dys],
	@[pay_bil_ut_dys],
	@[pay_lse_rt_amt],
	@[pay_tax_amt],
	@[pay_lse_chg_amt],
	@[inv_sc_no],
	@[inv_bkg_no],
	@[inv_bkg_term_cd],
	@[cost_yrmon],
	@[cost_cd],
	@[acct_cd],
	@[eq_knd_cd],
	-1 * ABS(@[inv_cr_amt]),
	@[inv_tax_rt_Amt],
	@[inv_lse_chg_amt_ttl],
	@[pay_tax_rt_amt],
	@[pay_lse_chg_amt_ttl],
	@[cre_usr_id],
	SYSDATE,
	@[upd_usr_id],
	SYSDATE
FROM	
	DUAL			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
				<param name="inv_ref_no" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="inv_cust_eq_no" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="inv_eq_onh_loc_nm" type="12" value="" out="N"/>
				<param name="inv_eq_onh_dt" type="12" value="" out="N"/>
				<param name="inv_eq_offh_dt" type="12" value="" out="N"/>
				<param name="inv_bil_st_dt" type="12" value="" out="N"/>
				<param name="inv_bil_end_dt" type="12" value="" out="N"/>
				<param name="inv_lse_use_dys" type="12" value="" out="N"/>
				<param name="inv_bil_ut_dys" type="12" value="" out="N"/>
				<param name="inv_lse_rt_amt" type="12" value="" out="N"/>
				<param name="inv_tax_amt" type="12" value="" out="N"/>
				<param name="inv_lse_chg_amt" type="12" value="" out="N"/>
				<param name="lse_chg_aud_sts_cd" type="12" value="" out="N"/>
				<param name="pay_lse_chg_sts_cd" type="12" value="" out="N"/>
				<param name="lse_chg_aud_rslt_rsn_cd" type="12" value="" out="N"/>
				<param name="eq_fm_mvmt_dt" type="12" value="" out="N"/>
				<param name="eq_fm_yd_cd" type="12" value="" out="N"/>
				<param name="eq_fm_mvmt_cd" type="12" value="" out="N"/>
				<param name="eq_to_mvmt_dt" type="12" value="" out="N"/>
				<param name="eq_to_yd_cd" type="12" value="" out="N"/>
				<param name="eq_to_mvmt_cd" type="12" value="" out="N"/>
				<param name="pay_lse_use_dys" type="12" value="" out="N"/>
				<param name="pay_bil_ut_dys" type="12" value="" out="N"/>
				<param name="pay_lse_rt_amt" type="12" value="" out="N"/>
				<param name="pay_tax_amt" type="12" value="" out="N"/>
				<param name="pay_lse_chg_amt" type="12" value="" out="N"/>
				<param name="inv_sc_no" type="12" value="" out="N"/>
				<param name="inv_bkg_no" type="12" value="" out="N"/>
				<param name="inv_bkg_term_cd" type="12" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="inv_cr_amt" type="12" value="" out="N"/>
				<param name="inv_tax_rt_Amt" type="12" value="" out="N"/>
				<param name="inv_lse_chg_amt_ttl" type="12" value="" out="N"/>
				<param name="pay_tax_rt_amt" type="12" value="" out="N"/>
				<param name="pay_lse_chg_amt_ttl" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
