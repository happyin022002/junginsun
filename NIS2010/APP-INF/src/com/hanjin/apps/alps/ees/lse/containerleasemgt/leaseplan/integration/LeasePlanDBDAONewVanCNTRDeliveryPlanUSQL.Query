<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LeasePlanDBDAONewVanCNTRDeliveryPlanUSQL">
			<desc><![CDATA[신조장비(OW/LP/OL) 계획자료를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE  LSE_NEW_VAN_DE_PLN
SET     NEW_VAN_YRMON = @[new_van_yrmon],
        MFT_VNDR_SEQ = @[mft_vndr_seq],
        DEL_CD       = @[del_cd],
        CNTR_TPSZ_CD = @[cntr_tpsz_cd],
        AGMT_CTY_CD  = @[agmt_cty_cd],
        AGMT_SEQ     = @[agmt_seq],
        LSTM_CD      = @[lstm_cd],
        DE_QTY       = @[de_qty],
        UPD_USR_ID   = @[upd_usr_id],
        UPD_DT       = SYSDATE ,
		PLN_RMK      = @[pln_rmk]
WHERE	PLN_YRMON  	 = @[pln_yrmon]
AND	    PLN_SEQ 	 = @[pln_seq]			]]></sql>
			<params>
				<param name="new_van_yrmon" type="12" value="" out="N"/>
				<param name="mft_vndr_seq" type="2" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="de_qty" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="pln_rmk" type="12" value="" out="N"/>
				<param name="pln_yrmon" type="12" value="" out="N"/>
				<param name="pln_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
