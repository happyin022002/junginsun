<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchInvoiceCancelRmkRSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOSearchInvoiceCancelRmkRSQL]]></desc>
			<sql><![CDATA[
SELECT  INTG_CD_VAL_DP_DESC || ' : Invoice nbr.' || @[dmdt_inv_no] || ' was cancelled automatically due to ' || decode(@[dmdt_expt_rqst_sts_cd], 'A', 'approval', 'J', 'reject', 'O', 'counter offer') || ' for After BKG DAR No. ' || @[aft_expt_dar_no]
  FROM  COM_INTG_CD_DTL
 WHERE  INTG_CD_ID = 'CD01962'
   AND  INTG_CD_VAL_CTNT = @[intg_cd_val_ctnt]
   AND  APLY_ST_DT  <= TO_CHAR(SYSDATE, 'YYYYMMDD')
   AND  APLY_END_DT >= TO_CHAR(SYSDATE, 'YYYYMMDD')			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="dmdt_expt_rqst_sts_cd" type="12" value="" out="N"/>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
				<param name="intg_cd_val_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
