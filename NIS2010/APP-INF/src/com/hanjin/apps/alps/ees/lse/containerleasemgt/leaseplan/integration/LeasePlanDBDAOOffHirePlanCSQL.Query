<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LeasePlanDBDAOOffHirePlanCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_OFFH_PLN (
       PLN_YR
     , PLN_SEQ
     , OFFH_VER_SEQ
     , OFFH_YRMON
     , OFFH_PLN_TP_CD
     , OFFH_LOC_TP_CD
     , OFFH_RGN_LOC_CD
     , CNTR_TPSZ_CD
     , OFFH_LOC_CD
     , OFFH_QTY
     , LSTM_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT )
SELECT @[pln_yr]                AS PLN_YR
     , NVL(MAX(C.PLN_SEQ), 0)+1 AS PLN_SEQ
     , @[offh_ver_seq]          AS OFFH_VER_SEQ
     , @[offh_yrmon]            AS OFFH_YRMON
     , @[offh_pln_tp_cd]        AS OFFH_PLN_TP_CD
     , @[offh_loc_tp_cd]        AS OFFH_LOC_TP_CD
     , @[offh_rgn_loc_cd]       AS OFFH_RGN_LOC_CD
     , @[cntr_tpsz_cd]          AS CNTR_TPSZ_CD
     , @[offh_loc_cd]           AS OFFH_LOC_CD
     , @[offh_qty]              AS OFFH_QTY
     , @[lstm_cd]               AS LSTM_CD
     , @[cre_usr_id]            AS CRE_USR_ID
     , SYSDATE                  AS CRE_DT
     , @[cre_usr_id]            AS UPD_USR_ID
     , SYSDATE                  AS UPD_DT
FROM   LSE_OFFH_PLN C
WHERE  C.PLN_YR = @[pln_yr]
AND    C.OFFH_VER_SEQ = @[offh_ver_seq]			]]></sql>
			<params>
				<param name="pln_yr" type="12" value="" out="N"/>
				<param name="offh_ver_seq" type="2" value="" out="N"/>
				<param name="offh_yrmon" type="12" value="" out="N"/>
				<param name="offh_pln_tp_cd" type="12" value="" out="N"/>
				<param name="offh_loc_tp_cd" type="12" value="" out="N"/>
				<param name="offh_rgn_loc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="offh_loc_cd" type="12" value="" out="N"/>
				<param name="offh_qty" type="2" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
