<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOModifyChargeStatusByInvoiceUSQL">
			<desc><![CDATA[DMT_CHG_CALC]]></desc>
			<sql><![CDATA[
UPDATE	DMT_CHG_CALC
SET		DMDT_CHG_STS_CD =	@[dmdt_chg_sts_cd]
		, DMDT_INV_NO	=	@[dmdt_inv_no]
		, UPD_USR_ID	=	@[upd_usr_id]
		, UPD_DT		= 	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
		, UPD_OFC_CD	= 	@[upd_ofc_cd]
WHERE	SYS_AREA_GRP_ID	=	@[svr_id]
AND		CNTR_NO			=	@[cntr_no]
AND		CNTR_CYC_NO		=	@[cntr_cyc_no]
AND		DMDT_TRF_CD		=	@[dmdt_trf_cd]
AND		DMDT_CHG_LOC_DIV_CD	=	@[dmdt_chg_loc_div_cd]
AND		CHG_SEQ			=	@[chg_seq]			]]></sql>
			<params>
				<param name="dmdt_chg_sts_cd" type="12" value="" out="N"/>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
