<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EccLinkInfoManageDBDAODeleteEccLinkInfoDSQL">
			<desc><![CDATA[EQR_SCNR_ECC_LNK 테이블 데이터 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM EQR_SCNR_ECC_LNK
WHERE SCNR_ID         = @[scnr_id]
AND   FM_ECC_CD		  = @[fm_ecc_cd]
AND   TO_ECC_CD       = @[to_ecc_cd]
AND   TRSP_MOD_CD     = @[trsp_mod_cd]			]]></sql>
			<params>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
