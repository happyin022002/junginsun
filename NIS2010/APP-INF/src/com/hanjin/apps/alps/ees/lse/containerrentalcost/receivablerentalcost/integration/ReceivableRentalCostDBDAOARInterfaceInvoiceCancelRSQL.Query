<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceivableRentalCostDBDAOARInterfaceInvoiceCancelRSQL">
			<desc><![CDATA[Invoice Cancel 자료의 AR Interface Main 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT  A.BL_SRC_NO, A.INV_SRC_NO, A.CUST_CNT_CD, A.CUST_SEQ,                         
        A.OFC_CD, A.IF_SRC_CD, A.VSL_CD, A.SKD_VOY_NO, A.SKD_DIR_CD,                 
        A.SVC_SCP_CD, A.SAIL_DT, A.SAIL_ARR_DT, A.DUE_DT, A.GL_EFF_DT,
        A.SLAN_CD, A.IO_BND_CD, A.TRNK_VSL_CD, A.TRNK_SKD_VOY_NO,
        A.TRNK_SKD_DIR_CD, A.POR_CD, A.POL_CD, A.POD_CD, A.DEL_CD,    
        A.BKG_TEU_QTY, A.BKG_FEU_QTY, A.INV_REF_NO,
        @[cre_usr_id] AS CRE_USR_ID, SYSDATE AS CRE_DT,
        @[cre_usr_id] AS UPD_USR_ID, SYSDATE AS UPD_DT
FROM    INV_AR_IF_MN A
WHERE   A.SRC_IF_DT = @[src_if_dt]
AND     A.SRC_IF_SEQ = @[src_if_seq]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="src_if_dt" type="12" value="" out="N"/>
				<param name="src_if_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
