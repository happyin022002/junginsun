<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOCheckCPSCHSSPoolVndrNameDataRSQL">
			<desc><![CDATA[입력된 Vendor Code로부터 Vendor Name을 체크한다.]]></desc>
			<sql><![CDATA[
SELECT LPAD(VNDR_SEQ, 6, '0') VNDR_SEQ,
       VNDR_LGL_ENG_NM
FROM MDM_VENDOR
WHERE VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
