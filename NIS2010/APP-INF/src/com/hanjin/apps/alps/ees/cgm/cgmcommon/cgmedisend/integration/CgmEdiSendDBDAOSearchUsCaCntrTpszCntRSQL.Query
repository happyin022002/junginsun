<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmEdiSendDBDAOSearchUsCaCntrTpszCntRSQL">
			<desc><![CDATA[CgmEdiSendDBDAOSearchUsCaCntrTpszCnt]]></desc>
			<sql><![CDATA[
SELECT COUNT(B.CNTR_NO) T_CNT
      ,B.CNTR_TPSZ_CD T_CNTR_TPSZ_CD
 FROM BKG_BOOKING   A
     ,BKG_CONTAINER B
WHERE A.BKG_NO = B.BKG_NO
  AND A.BKG_NO = @[bkg_no]
GROUP BY CNTR_TPSZ_CD
ORDER BY CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
