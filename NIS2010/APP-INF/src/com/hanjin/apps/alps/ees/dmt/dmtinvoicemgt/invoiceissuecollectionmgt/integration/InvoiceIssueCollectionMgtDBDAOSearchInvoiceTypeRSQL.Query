<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchInvoiceTypeRSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOSearchInvoiceTypeRSQL]]></desc>
			<sql><![CDATA[
SELECT  CASE
			WHEN MNL_INP_FLG = 'Y' THEN
				'M'
			ELSE
				CASE 
					WHEN SUBSTR(DMDT_TRF_CD, 1, 2) = 'DM' THEN
						'R'
					ELSE
						'T'
				END
		END AS INV_TP_CD
			
  FROM  DMT_INV_MN
 WHERE  DMDT_INV_NO = @[dmdt_inv_no]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
