<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RateMgtDBDAOsearchAGMTHDRDataRSQL">
			<desc><![CDATA[헤더 데이타 조회
-- 2014.11 10만불 결제관련
-- [CHM-201433304]
  CSR 승인 강화에 따른 ALPS MNR-AGMT에 GW Document Contract Link 관련 추가 요청 사항]]></desc>
			<sql><![CDATA[
SELECT 
		MNR_COMMON_PKG.MNR_CONV_AGMT_NO_FNC(A.AGMT_OFC_CTY_CD, A.AGMT_SEQ) AS AGMT_NO
   		,A.AGMT_OFC_CTY_CD
        ,A.AGMT_SEQ
        ,A.AGMT_VER_NO
        ,A.EQ_KND_CD
        ,A.AGMT_LST_VER_FLG
        ,MNR_COMMON_PKG.MNR_CONV_PARTNER_CD_FNC(A.VNDR_SEQ) AS VNDR_SEQ
        ,A.PAY_TERM_DYS
        ,A.AGMT_REF_NO
        ,TO_CHAR(A.AGMT_DT, 'yyyy-mm-dd') AGMT_DT
        ,TO_CHAR(A.EFF_DT, 'yyyy-mm-dd') EFF_DT
        ,TO_CHAR(A.EXP_DT, 'yyyy-mm-dd') EXP_DT
        ,A.CURR_CD
        ,A.DELT_FLG
        ,A.AGMT_RMK
        ,A.AGMT_OFC_CD
        ,A.TRF_NO
        ,A.CRE_USR_ID
        ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
        ,A.UPD_USR_ID
        ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
        ,B.VNDR_LGL_ENG_NM AS VNDR_NM
        ,nvl(A.FILE_ATCH_FLG, 'N') AS FILE_ATCH_FLG
FROM MNR_AGMT_HDR A,MDM_VENDOR B
WHERE A.AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
AND   A.AGMT_SEQ = @[agmt_seq] 
AND   A.AGMT_VER_NO = @[agmt_ver_no]
AND   A.VNDR_SEQ = B.VNDR_SEQ			]]></sql>
			<params>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="agmt_ver_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
