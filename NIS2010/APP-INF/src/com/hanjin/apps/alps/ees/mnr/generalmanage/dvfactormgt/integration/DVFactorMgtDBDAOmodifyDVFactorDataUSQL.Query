<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DVFactorMgtDBDAOmodifyDVFactorDataUSQL">
			<desc><![CDATA[수정]]></desc>
			<sql><![CDATA[
MERGE INTO MNR_EQ_DPC A
USING DUAL
   ON (EQ_DPC_YR = @[eq_dpc_yr]
       AND EQ_TPSZ_CD = @[cd_id]
       AND EQ_KND_CD = @[eq_knd_cd]
       )
 WHEN MATCHED THEN
      UPDATE
         SET CURR_CD = @[curr_cd],
             EQ_INIT_PRC = @[eq_init_prc],
             EQ_DPC_RT = @[eq_dpc_rt],
             EQ_MTRL_CD = @[eq_mtrl_cd],
             UPD_USR_ID = @[upd_usr_id],
             UPD_DT = SYSDATE 
       WHERE EQ_DPC_YR = @[eq_dpc_yr]
         AND EQ_TPSZ_CD = @[cd_id]
         AND EQ_KND_CD = @[eq_knd_cd]
 WHEN NOT MATCHED THEN
      INSERT (
        	 EQ_DPC_YR,
        	 EQ_TPSZ_CD,
        	 EQ_KND_CD,
             CURR_CD,
        	 EQ_INIT_PRC,
        	 EQ_DPC_RT,
             EQ_MTRL_CD,
        	 CRE_USR_ID,
        	 CRE_DT,
        	 UPD_USR_ID,
        	 UPD_DT
            ) VALUES( 
        	 @[eq_dpc_yr],
        	 @[cd_id],
        	 @[eq_knd_cd],
             @[curr_cd],
        	 @[eq_init_prc],
        	 @[eq_dpc_rt],
             @[eq_mtrl_cd],
        	 @[cre_usr_id],
        	 SYSDATE,
        	 @[upd_usr_id],
        	 SYSDATE
            )			]]></sql>
			<params>
				<param name="eq_dpc_yr" type="12" value="1" out="N"/>
				<param name="cd_id" type="12" value="1" out="N"/>
				<param name="eq_knd_cd" type="12" value="1" out="N"/>
				<param name="curr_cd" type="12" value="1" out="N"/>
				<param name="eq_init_prc" type="2" value="1" out="N"/>
				<param name="eq_dpc_rt" type="2" value="1" out="N"/>
				<param name="eq_mtrl_cd" type="12" value="1" out="N"/>
				<param name="upd_usr_id" type="12" value="1" out="N"/>
				<param name="cre_usr_id" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
