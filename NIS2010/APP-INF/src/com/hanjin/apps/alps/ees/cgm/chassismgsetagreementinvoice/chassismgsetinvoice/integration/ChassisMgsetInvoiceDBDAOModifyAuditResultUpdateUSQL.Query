<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOModifyAuditResultUpdateUSQL">
			<desc><![CDATA[데이터를 CGM_LSE_CHG_DTL에 update]]></desc>
			<sql><![CDATA[
UPDATE CGM_LSE_CHG_DTL
SET
	PAY_LSE_CHG_STS_CD = 'C',
	INV_LSE_CHG_AMT             = @[inv_lse_chg_amt],
	INV_TAX_AMT                 = @[inv_tax_amt],
	INV_CR_AMT                  = @[inv_cr_amt],
	INV_LSE_CHG_TTL_AMT         = @[inv_lse_chg_ttl_amt],
	PAY_LSE_CHG_AMT             = @[inv_lse_chg_amt],
	PAY_TAX_AMT                 = @[inv_tax_amt],
	PAY_CR_AMT                  = @[inv_cr_amt],
	PAY_LSE_CHG_TTL_AMT         = @[inv_lse_chg_ttl_amt],
	VNDR_PAY_CHG_RMK            = @[vndr_pay_chg_rmk],
	VNDR_PAY_CHG_CMT_CTNT       = @[vndr_pay_chg_cmt_ctnt],
	VNDR_PAY_CHG_CR_STY_CTNT    = @[vndr_pay_chg_cr_sty_ctnt],
	VNDR_PAY_CHG_CR_DUE_CTNT    = @[vndr_pay_chg_cr_due_ctnt]
WHERE
	ROWID = @[updt_key]			]]></sql>
			<params>
				<param name="inv_lse_chg_amt" type="12" value="" out="N"/>
				<param name="inv_tax_amt" type="12" value="" out="N"/>
				<param name="inv_cr_amt" type="12" value="" out="N"/>
				<param name="inv_lse_chg_ttl_amt" type="12" value="" out="N"/>
				<param name="vndr_pay_chg_rmk" type="12" value="" out="N"/>
				<param name="vndr_pay_chg_cmt_ctnt" type="12" value="" out="N"/>
				<param name="vndr_pay_chg_cr_sty_ctnt" type="12" value="" out="N"/>
				<param name="vndr_pay_chg_cr_due_ctnt" type="12" value="" out="N"/>
				<param name="updt_key" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
