<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOCheckAgentRSQL">
			<desc><![CDATA[유효한 Branch/Agent 인지 Check  ]]></desc>
			<sql><![CDATA[
SELECT	o.ofc_cd agent
FROM 	MDM_ORGANIZATION O 
WHERE 	1 = 1
   		AND	o.ofc_cd = @[agent]
		AND o.delt_flg = 'N'			]]></sql>
			<params>
				<param name="agent" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
