<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchDualExceptionDeleteRSQL">
			<desc><![CDATA[DMTCalculationDBDAOSearchDualExceptionDeleteRSQL]]></desc>
			<sql><![CDATA[
SELECT DUL_TP_EXPT_FLG
 FROM DMT_CHG_CALC A, DMT_CHG_BKG_CNTR B
WHERE 1=1
AND A.SYS_AREA_GRP_ID = B.SYS_AREA_GRP_ID
AND A.CNTR_NO = B.CNTR_NO
AND A.CNTR_CYC_NO = B.CNTR_CYC_NO
and b.cntr_no = @[cntr_no]
AND A.DMDT_TRF_CD = @[trf_cd]
AND A.CNTR_CYC_NO = @[cyc_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="trf_cd" type="12" value="" out="N"/>
				<param name="cyc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
