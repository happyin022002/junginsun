<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchLoCalDateInfoDataRSQL">
			<desc><![CDATA[searchLoCalDateInfoData]]></desc>
			<sql><![CDATA[
SELECT
	TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(( SELECT MNR_CD_ID FROM MNR_GEN_CD WHERE PRNT_CD_ID = 'HOOFC' ),SYSDATE,@[ofc_cd]),'YYYY-MM-DD') AS LOC_TIME,
	CASE WHEN TO_NUMBER(TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(( SELECT MNR_CD_ID FROM MNR_GEN_CD WHERE PRNT_CD_ID = 'HOOFC' ),SYSDATE,@[ofc_cd]),'HH24')) < 8 THEN 'N'   --8시부터
		 WHEN TO_NUMBER(TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(( SELECT MNR_CD_ID FROM MNR_GEN_CD WHERE PRNT_CD_ID = 'HOOFC' ),SYSDATE,@[ofc_cd]),'HH24')) > 19 THEN 'N'       --7까지
		 ELSE 'Y' END AS WORKENABLE
FROM DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
