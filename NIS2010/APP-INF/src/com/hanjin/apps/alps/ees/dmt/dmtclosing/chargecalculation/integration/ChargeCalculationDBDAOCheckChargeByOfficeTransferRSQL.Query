<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOCheckChargeByOfficeTransferRSQL">
			<desc><![CDATA[해당 DmtChgBkgCntr 정보에 대해서 Charge가 존재하는지 체크한다.]]></desc>
			<sql><![CDATA[
SELECT 'Y' AS EXIST_YN
FROM DMT_CHG_CALC
WHERE	SYS_AREA_GRP_ID	= @[svr_id]
AND	CNTR_NO				= @[cntr_no]
AND	CNTR_CYC_NO			= @[cntr_cyc_no]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
