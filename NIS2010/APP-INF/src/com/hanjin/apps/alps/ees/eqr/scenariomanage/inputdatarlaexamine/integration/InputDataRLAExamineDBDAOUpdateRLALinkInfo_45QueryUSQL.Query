<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InputDataRLAExamineDBDAOUpdateRLALinkInfo_45QueryUSQL">
			<desc><![CDATA[EQR_SCNR_ECC_LNK 테이블에 대한 45feet amount  수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_SCNR_ECC_LNK SET 
  	TZ_45FT_COST_AMT   	= @[tz_45ft_cost_amt] 
,	UPD_USR_ID         	= @[upd_usr_id] 
,	UPD_DT             	= SYSDATE                        
WHERE 
  	SCNR_ID            	= @[scnr_id]                             	
AND FM_ECC_CD	 		= @[fm_ecc_cd]                              
AND TO_ECC_CD	 		= @[to_ecc_cd]                              
AND TRSP_MOD_CD  		= @[trsp_mod_cd]			]]></sql>
			<params>
				<param name="tz_45ft_cost_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
