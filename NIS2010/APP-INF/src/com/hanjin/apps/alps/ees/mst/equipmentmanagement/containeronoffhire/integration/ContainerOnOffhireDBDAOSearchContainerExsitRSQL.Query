<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOSearchContainerExsitRSQL">
			<desc><![CDATA[입력된 container no가 존재하는 여부를 체크를 한다.]]></desc>
			<sql><![CDATA[
SELECT  CNTR_NO FROM MST_CONTAINER
WHERE 1=1
#if (${chk_dgt} == '')
AND CNTR_NO = @[cntr_no] 
#end 
#if (${chk_dgt} != '')
AND CNTR_NO = @[cntr_no] || @[chk_dgt]
#end			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="chk_dgt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
