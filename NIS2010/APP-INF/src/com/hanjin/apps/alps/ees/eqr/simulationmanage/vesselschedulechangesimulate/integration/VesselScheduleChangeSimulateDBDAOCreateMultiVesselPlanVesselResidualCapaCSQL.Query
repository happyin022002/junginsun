<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselScheduleChangeSimulateDBDAOCreateMultiVesselPlanVesselResidualCapaCSQL">
			<desc><![CDATA[EQR_SCNR_VSL_RSDL_CAPA 테이블에 SCNR Vessel Residual Capa 입력]]></desc>
			<sql><![CDATA[
INSERT INTO EQR_SCNR_VSL_RSDL_CAPA (
    SCNR_ID      
    , CO_CD 
    , FCAST_YRWK
    , ECC_CD
    , VSL_LANE_CD 
    , VSL_CD 
    , SKD_VOY_NO   
    , SKD_DIR_CD 
    , VSL_BSA_SPC
    , VSL_BSA_WGT  
    , VSL_FULL_SPC 
    , VSL_FULL_WGT 
    , VSL_DEAD_SPC 
    , VSL_SPC 
    , TTL_RSDL_SPC 
    , TTL_RSDL_WGT 
    , CRE_USR_ID 
    , CRE_DT 
    , UPD_USR_ID 
    , UPD_DT 
) VALUES ( 
    @[maxScnrId] 
    , @[co_cd]
    ,(
        SELECT 
            PLN_YR||PLN_WK 
        FROM 
            EQR_WK_PRD  
        WHERE   
            TO_CHAR(TO_DATE( @[vsl_etb_dt] , 'YYYY-MM-DD HH24:MI'),'YYYYMMDD') 
                BETWEEN WK_ST_DT AND WK_END_DT 
    ) 
    , @[vsl_loc_cd] 
    , @[vsl_slan_cd] 
    , @[vsl_cd] 
    , @[skd_voy_no] 
    , @[skd_dir_cd] 
    , 0 
    , 0 
    , 0 
    , 0 
    , 0 
    , 0 
    , 10000 
    , 0 
    , @[usrId] 
    , sysdate 
    , @[usrId] 
    , sysdate
)  																											]]></sql>
			<params>
				<param name="maxScnrId" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
				<param name="vsl_etb_dt" type="12" value="" out="N"/>
				<param name="vsl_loc_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="usrId" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
