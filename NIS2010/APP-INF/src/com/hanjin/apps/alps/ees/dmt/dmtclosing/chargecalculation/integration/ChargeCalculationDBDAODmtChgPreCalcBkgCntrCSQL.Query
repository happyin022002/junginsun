<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAODmtChgPreCalcBkgCntrCSQL">
			<desc><![CDATA[ChargeCalculationDBDAODmtChgPreCalcBkgCntrDSQL.Query]]></desc>
			<sql><![CDATA[
INSERT	INTO DMT_CHG_PRE_CALC_BKG_CNTR
SELECT	*
FROM	DMT_CHG_BKG_CNTR
WHERE	SYS_AREA_GRP_ID	= @[svr_id]	
AND		CNTR_NO			= @[cntr_no]
AND		CNTR_CYC_NO		= @[cntr_cyc_no]

			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
