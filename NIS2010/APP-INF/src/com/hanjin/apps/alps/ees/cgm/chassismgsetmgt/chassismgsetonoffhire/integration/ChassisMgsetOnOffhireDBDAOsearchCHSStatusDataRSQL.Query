<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchCHSStatusDataRSQL">
			<desc><![CDATA[Chassis Status Inquiry/Correction 조회   ]]></desc>
			<sql><![CDATA[
SELECT A.EQ_NO 
      ,A.EQ_STS_SEQ
      ,A.EQ_ASET_STS_CD
      ,to_char(A.STS_EVNT_DT,'yyyy-mm-dd HH24:MI') STS_EVNT_DT
      ,to_char(A.CRE_DT,'yyyy-mm-dd HH24:MI') CRE_DT
      ,A.STS_EVNT_OFC_CD
      ,A.STS_EVNT_YD_CD
      ,A.AGMT_OFC_CTY_CD
      ,LPAD(A.AGMT_SEQ  , 6, '0'    ) AGMT_SEQ
      , A.AGMT_OFC_CTY_CD || LPAD(A.AGMT_SEQ  , 6, '0'    ) AGREEEMENT
      ,B.AGMT_REF_NO
	  ,C.VNDR_SEQ
      ,C.VNDR_LGL_ENG_NM
      ,A.CRE_USR_ID
      ,D.ACIAC_DIV_CD
      ,DECODE(D.ACIAC_DIV_CD,'A','active','I','In-active') ACIAC_DIV_NM
      ,D.EQ_TPSZ_CD
      ,D.AGMT_LSTM_CD
      ,A.TERM_CNG_SEQ
      ,D.CRNT_YD_CD
      ,D.CRNT_LOC_CD
	  ,D.ONH_OFC_CD
	  ,D.ONH_DT
	  ,D.ONH_YD_CD
	  ,D.AGMT_OFC_CTY_CD
      ,D.AGMT_VER_NO
      ,'' NO
      ,case WHEN (sysdate - A.STS_EVNT_DT) >180 then
       'Y'
       else
       'N'
       end chk_val
FROM CGM_EQ_STS_HIS A
    ,CGM_AGREEMENT  B
    ,MDM_VENDOR     C
    ,CGM_EQUIPMENT D
WHERE A.EQ_KND_CD       ='Z'
AND A.AGMT_SEQ        = B.AGMT_SEQ(+)
AND A.AGMT_OFC_CTY_CD = B.AGMT_OFC_CTY_CD(+)
AND A.AGMT_VER_NO     = B.AGMT_VER_NO(+)
AND B.VNDR_SEQ        = C.VNDR_SEQ(+)
  AND A.EQ_NO           = D.EQ_NO
  AND A.EQ_KND_CD       = B.EQ_KND_CD(+)
  AND A.EQ_NO = @[eq_no]
--ORDER BY A.EQ_NO ,A.EQ_STS_SEQ
ORDER BY A.STS_EVNT_DT, A.EQ_STS_SEQ			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
