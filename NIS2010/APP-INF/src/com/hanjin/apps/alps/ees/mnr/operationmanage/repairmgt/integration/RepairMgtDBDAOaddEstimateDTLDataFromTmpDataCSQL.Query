<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOaddEstimateDTLDataFromTmpDataCSQL">
			<desc><![CDATA[addEstimateDTLDataFromTmpData]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_RPR_RQST_DTL(
         RQST_EQ_NO
        ,RPR_RQST_SEQ
        ,RPR_RQST_VER_NO
        ,RPR_RQST_DTL_SEQ
        ,RPR_RQST_LST_VER_FLG
        ,COST_CD
        ,EQ_LOC_CD
        ,EQ_LOC_CD_CHK_FLG
        ,EQ_CMPO_CD
        ,EQ_CMPO_CD_CHK_FLG
        ,EQ_DMG_CD
        ,EQ_DMG_CD_CHK_FLG
        ,EQ_RPR_CD
        ,EQ_RPR_CD_CHK_FLG
        ,TRF_DIV_CD
        ,TRF_OPT_CD
        ,VOL_TP_CD
        ,RPR_QTY
        ,RPR_SZ_NO
        ,RPR_LEN_NO
        ,RPR_WDT_NO
        ,RPR_LBR_HRS
        ,RPR_LBR_RT
        ,RPR_LBR_BZC_HRS
        ,RPR_LBR_BZC_RT
        ,MNR_LBR_BZC_AMT
        ,LBR_MTRL_BZC_AMT
        ,LBR_COST_AMT
        ,MTRL_COST_AMT
        ,XCH_MTRL_COST_AMT
        ,MTRL_RECO_AMT
        ,MNR_LR_ACCT_FLG
        ,N3PTY_FLG
        ,N3PTY_BIL_LBR_HRS
        ,N3PTY_BIL_LBR_RT
        ,N3PTY_BIL_LBR_COST_AMT
        ,N3PTY_BIL_MTRL_COST_AMT
        ,MNR_AGMT_AMT
        ,MNR_WRK_AMT
        ,N3PTY_BIL_AMT
        ,RPR_DTL_RMK
        ,MNR_VRFY_TP_CD
        ,CRE_USR_ID
        ,CRE_DT
        ,UPD_USR_ID
        ,UPD_DT
)
SELECT
   		 @[new_rqst_eq_no]
        ,@[new_rpr_rqst_seq]
        ,@[new_rpr_rqst_ver_no]
        ,A.RPR_RQST_TMP_DTL_SEQ
        ,A.RPR_RQST_LST_VER_FLG
        ,A.COST_CD
        ,A.EQ_LOC_CD
        ,A.EQ_LOC_CD_CHK_FLG
        ,A.EQ_CMPO_CD
        ,A.EQ_CMPO_CD_CHK_FLG
        ,A.EQ_DMG_CD
        ,A.EQ_DMG_CD_CHK_FLG
        ,A.EQ_RPR_CD
        ,A.EQ_RPR_CD_CHK_FLG
        ,A.TRF_DIV_CD
        ,A.TRF_OPT_CD
        ,A.VOL_TP_CD
        ,A.RPR_QTY
        ,A.RPR_SZ_NO
        ,A.RPR_LEN_NO
        ,A.RPR_WDT_NO
        ,A.RPR_LBR_HRS
        ,A.RPR_LBR_RT
        ,A.RPR_LBR_BZC_HRS
        ,A.RPR_LBR_BZC_RT
        ,A.MNR_LBR_BZC_AMT
        ,A.LBR_MTRL_BZC_AMT
        ,A.LBR_COST_AMT
        ,A.MTRL_COST_AMT
        ,A.XCH_MTRL_COST_AMT
        ,A.MTRL_RECO_AMT
        ,A.MNR_LR_ACCT_FLG
        ,A.N3PTY_FLG
        ,A.N3PTY_BIL_LBR_HRS
        ,A.N3PTY_BIL_LBR_RT
        ,A.N3PTY_BIL_LBR_COST_AMT
        ,A.N3PTY_BIL_MTRL_COST_AMT
        ,A.MNR_AGMT_AMT
        ,A.MNR_WRK_AMT
        ,A.N3PTY_BIL_AMT
        ,A.RPR_DTL_RMK
        ,A.MNR_VRFY_TP_CD
        ,A.CRE_USR_ID
        ,A.CRE_DT
        ,A.UPD_USR_ID
        ,A.UPD_DT
FROM MNR_RPR_RQST_TMP_DTL A
WHERE A.RQST_EQ_NO = @[rqst_eq_no]
AND   A.RPR_RQST_TMP_SEQ = @[rpr_rqst_tmp_seq]
AND   A.RPR_RQST_TMP_VER_NO = @[rpr_rqst_tmp_ver_no]			]]></sql>
			<params>
				<param name="new_rqst_eq_no" type="12" value="" out="N"/>
				<param name="new_rpr_rqst_seq" type="12" value="" out="N"/>
				<param name="new_rpr_rqst_ver_no" type="12" value="" out="N"/>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
				<param name="rpr_rqst_tmp_seq" type="12" value="" out="N"/>
				<param name="rpr_rqst_tmp_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
