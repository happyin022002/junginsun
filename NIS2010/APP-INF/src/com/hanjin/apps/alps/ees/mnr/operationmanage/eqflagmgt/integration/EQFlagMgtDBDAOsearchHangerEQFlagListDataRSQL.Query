<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQFlagMgtDBDAOsearchHangerEQFlagListDataRSQL">
			<desc><![CDATA[searchHangerEQFlagListData]]></desc>
			<sql><![CDATA[
SELECT
 B.EQ_NO
,B.EQ_TYPE EQ_KND_CD
,B.EQ_TPSZ_CD
,NVL(A.MNR_HNGR_FLG,'N') MNR_HNGR_FLG
,B.MVMT_CD
,B.CRNT_YD_CD MNR_HNGR_FLG_YD_CD
,NVL(B.HNGR_RCK_CD,'R') AS MNR_HNGR_RCK_CD
,DECODE(NVL(B.HNGR_RCK_CD,'R'),'O','S', NVL(B.BAR_TP_CD,'S')) AS MNR_HNGR_BAR_TP_CD
,NVL(A.MNR_HNGR_TRF_CD,'CB') AS MNR_HNGR_TRF_CD
,DECODE(A.MNR_HNGR_TRF_CD, 'OT', A.MNR_HNGR_TRF_OTR_DESC, '') AS MNR_HNGR_TRF_OTR_DESC
,NVL(A.HNGR_BAR_ATCH_KNT,0) HNGR_BAR_ATCH_KNT
,NVL(A.ACT_INVT_QTY,0) ACT_INVT_QTY
,NVL(A.MNR_HNGR_DMG_QTY,0) MNR_HNGR_DMG_QTY
,NVL(A.MNR_LOST_HNGR_QTY,0) MNR_LOST_HNGR_QTY
,NVL(A.MNR_DISP_HNGR_QTY,0) MNR_DISP_HNGR_QTY
,A.MNR_STS_RMK
FROM MNR_EQ_STS A, MNR_EQ_STS_V B
WHERE 1 = 1
AND A.EQ_NO(+) = B.EQ_NO
#if (${eq_list} != '')
	AND B.EQ_NO = @[eq_list]
#end			]]></sql>
			<params>
				<param name="eq_list" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
