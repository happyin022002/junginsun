<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EqInterchangeDBDAOUpdateEqInterchangeUpdateUSQL">
			<desc><![CDATA[EQ interChange Update &  Cancel 화면의 save 시에 저장을 한다.]]></desc>
			<sql><![CDATA[
UPDATE LSE_EQ_ITCHG
SET AGMT_SEQ = @[agmt_seq]
   ,LSE_ITCHG_AUTH_QTY = @[lse_itchg_auth_qty]
   ,LSE_FREE_DYS = @[lse_free_dys]
   ,PKUP_UT_AMT =  @[pkup_ut_amt]
   ,PKUP_CR_AMT =  @[pkup_cr_amt]
WHERE 1=1 
AND LSE_ITCHG_AUTH_NO = @[lse_itchg_auth_no]
AND LSE_ITCHG_AUTH_SEQ = @[lse_itchg_auth_seq]
			]]></sql>
			<params>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="lse_itchg_auth_qty" type="12" value="" out="N"/>
				<param name="lse_free_dys" type="12" value="" out="N"/>
				<param name="pkup_ut_amt" type="12" value="" out="N"/>
				<param name="pkup_cr_amt" type="12" value="" out="N"/>
				<param name="lse_itchg_auth_no" type="12" value="" out="N"/>
				<param name="lse_itchg_auth_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
