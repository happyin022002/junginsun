<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOSearchSCNoCustTpCdProposalNoRSQL">
			<desc><![CDATA[SCExceptionTariffMgtDBDAOSearchSCNoCustTpCdProposalNoRSQL]]></desc>
			<sql><![CDATA[
SELECT	/*+ INDEX_DESC(A XPKPRI_SP_CTRT_CUST_TP) */ 
        CASE prc_ctrt_cust_tp_cd WHEN 'N' THEN 'NVO'
                                 WHEN 'I' THEN 'BCO'
                                 WHEN 'A' THEN 'BCO'
                                 WHEN 'B' THEN 'BCO'
                                 ELSE ' ' END prc_ctrt_cust_tp_cd
FROM	PRI_SP_CTRT_CUST_TP A
WHERE	PROP_NO = @[prop_no]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
