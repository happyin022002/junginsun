<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchFAEqNoDataRSQL">
			<desc><![CDATA[FA 전송시 EQ No 로 FA EQ No 조회]]></desc>
			<sql><![CDATA[
SELECT DECODE(LSTM_CD,'OW',NVL(FA_EQ_NO,EQ_NO),'NOSEND') AS FA_EQ_NO
  FROM MNR_EQ_STS_V
 WHERE EQ_NO = @[eq_no]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
