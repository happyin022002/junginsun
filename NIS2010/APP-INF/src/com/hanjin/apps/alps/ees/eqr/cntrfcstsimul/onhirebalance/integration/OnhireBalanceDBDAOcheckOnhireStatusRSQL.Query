<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireBalanceDBDAOcheckOnhireStatusRSQL">
			<desc><![CDATA[EQR_CTRL_ONH_ORD 테이블의 데이터 중복을 체크한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) DUP_CHK 
FROM   EQR_CTRL_ONH_ORD
WHERE  ONH_ORD_YR  = @[onh_ord_yr]               
AND    LCC_CD      = @[lcc_cd]                   
AND    EQ_LSTM_CD  = @[eq_lstm_cd]               
AND    LSE_PRD_SEQ = @[lse_prd_seq] 			]]></sql>
			<params>
				<param name="onh_ord_yr" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="eq_lstm_cd" type="12" value="" out="N"/>
				<param name="lse_prd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
