<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongstayingUnclaimEQFlaggingDBDAOSubContiCdRSQL">
			<desc><![CDATA[Setting Value for Sub-Conti Combo]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.SCONTI_CD, N.SCONTI_NM
  FROM MDM_COUNTRY A
  , MDM_EQ_ORZ_CHT B
  , CIM_EQ_SPLS_DFCT_RHQ C
  , MDM_SUBCONTINENT N
 WHERE A.CNT_CD = SUBSTR(B.LCC_CD,1,2)
   AND A.SCONTI_CD = N.SCONTI_CD
   AND B.RCC_CD = C.RCC_CD
   AND B.DELT_FLG = 'N'
#if (${rhq_cd} != 'ALL' && ${rhq_cd} != '')
   AND C.RHQ_CD = @[rhq_cd]
#end
ORDER BY A.SCONTI_CD			]]></sql>
			<params>
				<param name="rhq_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
