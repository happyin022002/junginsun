               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(P.TS25, PD.EQ_TPSZ_CD, PD.EQ_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(P.TS25, PD.EQ_TPSZ_CD, PD.EQ_QTY))*MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) WHEN C.DCOL = 'Z' THEN MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) END TS25_Q,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(P.TS26, PD.EQ_TPSZ_CD, PD.EQ_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(P.TS26, PD.EQ_TPSZ_CD, PD.EQ_QTY))*MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) WHEN C.DCOL = 'Z' THEN MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) END TS26_Q,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(P.TS27, PD.EQ_TPSZ_CD, PD.EQ_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(P.TS27, PD.EQ_TPSZ_CD, PD.EQ_QTY))*MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) WHEN C.DCOL = 'Z' THEN MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) END TS27_Q,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(P.TS28, PD.EQ_TPSZ_CD, PD.EQ_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(P.TS28, PD.EQ_TPSZ_CD, PD.EQ_QTY))*MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) WHEN C.DCOL = 'Z' THEN MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) END TS28_Q,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(P.TS29, PD.EQ_TPSZ_CD, PD.EQ_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(P.TS29, PD.EQ_TPSZ_CD, PD.EQ_QTY))*MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) WHEN C.DCOL = 'Z' THEN MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) END TS29_Q,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(P.TS30, PD.EQ_TPSZ_CD, PD.EQ_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(P.TS30, PD.EQ_TPSZ_CD, PD.EQ_QTY))*MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) WHEN C.DCOL = 'Z' THEN MAX(MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(SYSDATE, 'YYYYMM'), TRF_CURR, DECODE(@[usd_only], 'Y', 'USD', TRF_CURR), TRF_AMT)) END TS30_Q
        FROM  PARAM P, DUMMY_COL C, MNR_PLN_HDR PH, MNR_PLN_DTL PD,
              (
                  SELECT TD.CTRL_OFC_CD TRF_OFC, TD.EQ_TPSZ_CD TRF_TPSZ, MAX(TD.CURR_CD) TRF_CURR, MAX(TD.MNR_DISP_TRF_AMT) TRF_AMT
                  FROM MNR_DISP_TRF_HDR TH, MNR_DISP_TRF_DTL TD
                  WHERE TH.MNR_DISP_TRF_SEQ = TD.MNR_DISP_TRF_SEQ
                  AND   TH.MNR_DISP_TRF_STS_CD = 'HC'
                  AND   TH.MNR_DISP_TRF_LST_VER_FLG = 'Y'
                  GROUP BY TD.CTRL_OFC_CD, TD.EQ_TPSZ_CD
              )            
        WHERE PH.MNR_PLN_SEQ = PD.MNR_PLN_SEQ
        AND   PH.MNR_GRP_TP_CD = 'DSP'
        AND   PH.MNR_PLN_FLG = 'Y'
        AND   PH.EQ_KND_CD = 'U'  
        AND   PH.MNR_PLN_YR||LTRIM(TO_CHAR(PH.MNR_PLN_GRP_NO, '00')) BETWEEN REPLACE(@[from_mon],'-','') AND REPLACE(@[to_mon],'-','') 
		#if (${ofc_cd} != 'A') 
        AND   PD.CTRL_OFC_CD = @[ofc_cd]
		#end
        AND   PD.CTRL_OFC_CD = TRF_OFC(+)
        AND   PD.EQ_TPSZ_CD  = TRF_TPSZ(+)
        GROUP BY PD.CTRL_OFC_CD,C.DCOL
    )
SELECT  RHQ, OFC_CD, CURR_CD,DECODE(TYPE, 'Q', 'QTY', 'S', 'AMT', 'Z', 'AVG') TYPE,
                     DECODE(TYPE, 'Q', '1', 'S', '2', 'Z', '3') TYPE2,
        DECODE(RHQ,'RHQ',             
        TS01_Q || '|' || TS01_P || '|' || TS02_Q || '|' || TS02_P || '|' ||
        TS03_Q || '|' || TS03_P || '|' || TS04_Q || '|' || TS04_P || '|' ||
        TS05_Q || '|' || TS05_P || '|' || TS06_Q || '|' || TS06_P || '|' ||
        TS07_Q || '|' || TS07_P || '|' || TS08_Q || '|' || TS08_P || '|' ||
        TS09_Q || '|' || TS09_P || '|' || TS10_Q || '|' || TS10_P || '|' ||
        TS11_Q || '|' || TS11_P || '|' || TS12_Q || '|' || TS12_P || '|' ||
        TS13_Q || '|' || TS13_P || '|' || TS14_Q || '|' || TS14_P || '|' ||
        TS15_Q || '|' || TS15_P || '|' || TS16_Q || '|' || TS16_P || '|' ||
        TS17_Q || '|' || TS17_P || '|' || TS18_Q || '|' || TS18_P || '|' ||
        TS19_Q || '|' || TS19_P || '|' || TS20_Q || '|' || TS20_P || '|' ||
        TS21_Q || '|' || TS21_P || '|' || TS22_Q || '|' || TS22_P || '|' ||
        TS23_Q || '|' || TS23_P || '|' || TS24_Q || '|' || TS24_P || '|' ||
        TS25_Q || '|' || TS25_P || '|' || TS26_Q || '|' || TS26_P || '|' ||
        TS27_Q || '|' || TS27_P || '|' || TS28_Q || '|' || TS28_P || '|' ||
        TS29_Q || '|' || TS29_P || '|' || TS30_Q || '|' || TS30_P,'') AS TITLE,
        TS01_Q, TS01_P, TS02_Q, TS02_P, TS03_Q, TS03_P, TS04_Q, TS04_P, TS05_Q, TS05_P,
        TS06_Q, TS06_P, TS07_Q, TS07_P, TS08_Q, TS08_P, TS09_Q, TS09_P, TS10_Q, TS10_P,
        TS11_Q, TS11_P, TS12_Q, TS12_P, TS13_Q, TS13_P, TS14_Q, TS14_P, TS15_Q, TS15_P,
        TS16_Q, TS16_P, TS17_Q, TS17_P, TS18_Q, TS18_P, TS19_Q, TS19_P, TS20_Q, TS20_P,
        TS21_Q, TS21_P, TS22_Q, TS22_P, TS23_Q, TS23_P, TS24_Q, TS24_P, TS25_Q, TS25_P,
        TS26_Q, TS26_P, TS27_Q, TS27_P, TS28_Q, TS28_P, TS29_Q, TS29_P, TS30_Q, TS30_P    
FROM        
    (    
        SELECT 'RHQ' RHQ, 'OFFICE' OFC_CD, 'CURR' CURR_CD, 'TYPE' TYPE,                              
               NVL(P.TS01,'N') TS01_Q, NVL(P.TS01,'N') TS01_P, NVL(P.TS02,'N') TS02_Q, NVL(P.TS02,'N') TS02_P,
               NVL(P.TS03,'N') TS03_Q, NVL(P.TS03,'N') TS03_P, NVL(P.TS04,'N') TS04_Q, NVL(P.TS04,'N') TS04_P,
               NVL(P.TS05,'N') TS05_Q, NVL(P.TS05,'N') TS05_P, NVL(P.TS06,'N') TS06_Q, NVL(P.TS06,'N') TS06_P,
               NVL(P.TS07,'N') TS07_Q, NVL(P.TS07,'N') TS07_P, NVL(P.TS08,'N') TS08_Q, NVL(P.TS08,'N') TS08_P,
               NVL(P.TS09,'N') TS09_Q, NVL(P.TS09,'N') TS09_P, NVL(P.TS10,'N') TS10_Q, NVL(P.TS10,'N') TS10_P,
               NVL(P.TS11,'N') TS11_Q, NVL(P.TS11,'N') TS11_P, NVL(P.TS12,'N') TS12_Q, NVL(P.TS12,'N') TS12_P,
               NVL(P.TS13,'N') TS13_Q, NVL(P.TS13,'N') TS13_P, NVL(P.TS14,'N') TS14_Q, NVL(P.TS14,'N') TS14_P,
               NVL(P.TS15,'N') TS15_Q, NVL(P.TS15,'N') TS15_P, NVL(P.TS16,'N') TS16_Q, NVL(P.TS16,'N') TS16_P,
               NVL(P.TS17,'N') TS17_Q, NVL(P.TS17,'N') TS17_P, NVL(P.TS18,'N') TS18_Q, NVL(P.TS18,'N') TS18_P,
               NVL(P.TS19,'N') TS19_Q, NVL(P.TS19,'N') TS19_P, NVL(P.TS20,'N') TS20_Q, NVL(P.TS20,'N') TS20_P,
               NVL(P.TS21,'N') TS21_Q, NVL(P.TS21,'N') TS21_P, NVL(P.TS22,'N') TS22_Q, NVL(P.TS22,'N') TS22_P,
               NVL(P.TS23,'N') TS23_Q, NVL(P.TS23,'N') TS23_P, NVL(P.TS24,'N') TS24_Q, NVL(P.TS24,'N') TS24_P,
               NVL(P.TS25,'N') TS25_Q, NVL(P.TS25,'N') TS25_P, NVL(P.TS26,'N') TS26_Q, NVL(P.TS26,'N') TS26_P,
               NVL(P.TS27,'N') TS27_Q, NVL(P.TS27,'N') TS27_P, NVL(P.TS28,'N') TS28_Q, NVL(P.TS28,'N') TS28_P,
               NVL(P.TS29,'N') TS29_Q, NVL(P.TS29,'N') TS29_P, NVL(P.TS30,'N') TS30_Q, NVL(P.TS30,'N') TS30_P                 
        FROM PARAM P
        UNION ALL  
        SELECT MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(DQ.OFC_CD) RHQ, DQ.OFC_CD OFC_CD, DQ.CURR_CD, DQ.DCOL TYPE,
               TO_CHAR(QQ.TS01_Q) TS01_Q, TO_CHAR(DQ.TS01_P) TS01_P, TO_CHAR(QQ.TS02_Q) TS02_Q, TO_CHAR(DQ.TS02_P) TS02_P, 
			   TO_CHAR(QQ.TS03_Q) TS03_Q, TO_CHAR(DQ.TS03_P) TS03_P, TO_CHAR(QQ.TS04_Q) TS04_Q, TO_CHAR(DQ.TS04_P) TS04_P, 
	           TO_CHAR(QQ.TS05_Q) TS05_Q, TO_CHAR(DQ.TS05_P) TS05_P, TO_CHAR(QQ.TS06_Q) TS06_Q, TO_CHAR(DQ.TS06_P) TS06_P, 
	           TO_CHAR(QQ.TS07_Q) TS07_Q, TO_CHAR(DQ.TS07_P) TS07_P, TO_CHAR(QQ.TS08_Q) TS08_Q, TO_CHAR(DQ.TS08_P) TS08_P,
               TO_CHAR(QQ.TS09_Q) TS09_Q, TO_CHAR(DQ.TS09_P) TS09_P, TO_CHAR(QQ.TS10_Q) TS10_Q, TO_CHAR(DQ.TS10_P) TS10_P,
               TO_CHAR(QQ.TS11_Q) TS11_Q, TO_CHAR(DQ.TS11_P) TS11_P, TO_CHAR(QQ.TS12_Q) TS12_Q, TO_CHAR(DQ.TS12_P) TS12_P, 
			   TO_CHAR(QQ.TS13_Q) TS13_Q, TO_CHAR(DQ.TS13_P) TS13_P, TO_CHAR(QQ.TS14_Q) TS14_Q, TO_CHAR(DQ.TS14_P) TS14_P,
               TO_CHAR(QQ.TS15_Q) TS15_Q, TO_CHAR(DQ.TS15_P) TS15_P, TO_CHAR(QQ.TS16_Q) TS16_Q, TO_CHAR(DQ.TS16_P) TS16_P,
               TO_CHAR(QQ.TS17_Q) TS17_Q, TO_CHAR(DQ.TS17_P) TS17_P, TO_CHAR(QQ.TS18_Q) TS18_Q, TO_CHAR(DQ.TS18_P) TS18_P,
               TO_CHAR(QQ.TS19_Q) TS19_Q, TO_CHAR(DQ.TS19_P) TS19_P, TO_CHAR(QQ.TS20_Q) TS20_Q, TO_CHAR(DQ.TS20_P) TS20_P,       
               TO_CHAR(QQ.TS21_Q) TS21_Q, TO_CHAR(DQ.TS21_P) TS21_P, TO_CHAR(QQ.TS22_Q) TS22_Q, TO_CHAR(DQ.TS22_P) TS22_P, 
			   TO_CHAR(QQ.TS23_Q) TS23_Q, TO_CHAR(DQ.TS23_P) TS23_P, TO_CHAR(QQ.TS24_Q) TS24_Q, TO_CHAR(DQ.TS24_P) TS24_P,
               TO_CHAR(QQ.TS25_Q) TS25_Q, TO_CHAR(DQ.TS25_P) TS25_P, TO_CHAR(QQ.TS26_Q) TS26_Q, TO_CHAR(DQ.TS26_P) TS26_P,
               TO_CHAR(QQ.TS27_Q) TS27_Q, TO_CHAR(DQ.TS27_P) TS27_P, TO_CHAR(QQ.TS28_Q) TS28_Q, TO_CHAR(DQ.TS28_P) TS28_P,
               TO_CHAR(QQ.TS29_Q) TS29_Q, TO_CHAR(DQ.TS29_P) TS29_P, TO_CHAR(QQ.TS30_Q) TS30_Q, TO_CHAR(DQ.TS30_P) TS30_P
        FROM  DISP_QTY DQ, QUOTA_QTY QQ
        WHERE DQ.OFC_CD = QQ.OFC_CD(+)
		#if (${rhq} != 'A') 		
        AND   MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(DQ.OFC_CD) =  @[rhq]
		#end
        AND   DQ.DCOL = QQ.DCOL(+)
    ) 
ORDER BY TITLE, RHQ, OFC_CD, CURR_CD, TYPE2			]]></sql>
			<params>
				<param name="eq_kind" type="12" value="" out="N"/>
				<param name="usd_only" type="12" value="" out="N"/>
				<param name="from_mon" type="12" value="" out="N"/>
				<param name="to_mon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="disp_tp_cd" type="12" value="" out="N"/>
				<param name="rhq" type="12" value="" out="N"/>
