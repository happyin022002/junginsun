    ), DUMMY_COL AS (
       SELECT 'Q' DCOL FROM DUAL  --QTY
       UNION ALL
       SELECT 'S' DCOL FROM DUAL  --AMOUNT
       UNION ALL 
       SELECT 'Z' DCOL FROM DUAL  --AVG
