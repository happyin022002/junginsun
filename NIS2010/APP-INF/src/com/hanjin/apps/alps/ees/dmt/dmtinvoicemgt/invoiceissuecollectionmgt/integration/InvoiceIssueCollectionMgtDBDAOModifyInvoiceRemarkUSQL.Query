<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyInvoiceRemarkUSQL">
			<desc><![CDATA[INVOICE 생성 및 징수관리
EES_DMT_4105
Remark(s)]]></desc>
			<sql><![CDATA[
UPDATE
		DMT_INV_MN
SET
		 INV_RMK    = @[rmrk]
		,UPD_USR_ID = @[usid]
		,UPD_DT     = SYSDATE
		,UPD_OFC_CD = @[usof]
WHERE
		DMDT_INV_NO = @[invno]			]]></sql>
			<params>
				<param name="rmrk" type="12" value="" out="N"/>
				<param name="usid" type="12" value="" out="N"/>
				<param name="usof" type="12" value="" out="N"/>
				<param name="invno" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
