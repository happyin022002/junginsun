			<desc><![CDATA[VERMAS EDI 수신 데이터 저장]]></desc>
			<sql><![CDATA[
INSERT INTO CTM_VRFD_GRS_MASS_EDI_MSG (
  REF_NO,
  CNTR_NO,
  VGM_SEQ,
  CNTR_TPSZ_CD,
  VGM_ID,
  FUNC_CD,
  ISS_DT,
  CUT_OFF_DT,
  POR_NM,
  POR_CD,
  POR_YD_CD,
  POL_NM,
  POL_CD,
  POL_YD_CD,
  SMT_NM,
  SMT_ADDR,
  SMT_CTY_CTNT,
  SMT_STE_CTNT,
  SMT_ZIP_CTNT,
  SMT_CNT_CD,
  SMT_CNTC_DTL_CTNT,
  SMT_CNTC_EML,
  SMT_CNTC_PHN_CTNT,
  VSL_CD,
  SKD_VOY_NO,
  SKD_DIR_CD,
  VSL_NM,
  VSL_LLOYD_NO,
  BKG_REF_NO,
  SI_REF_NO,
  BKG_NO,
  TRSP_CTRL_NO_CTNT,
  VGM_WGT_QTY,
  VRFD_WGT_CD,
  VGM_MZD_TP_CD,
  DOC_ID,
  VGM_DTMN_DT,
  VGM_VRFY_DT,
  SEAL_NO_CTNT,
  PTY_FUNC_CD,
  PTY_NM,
  PTY_ADDR,
  PTY_CTY_CTNT,
  PTY_STE_CTNT,
  PTY_ZIP_CTNT,
  PTY_CNT_CD,
  PTY_PSON_NM,
  CMMC_CNTC_PHN_CTNT,
  CMMC_CNTC_EML,
  CMMC_CNTC_FAX_CTNT,
  CRE_USR_ID,
  CRE_DT,
  UPD_USR_ID,
  UPD_DT
)
VALUES (
  @[ref_no],
  @[cntr_no],
  NVL((SELECT MAX(VGM_SEQ)
       FROM CTM_VRFD_GRS_MASS_EDI_MSG
       WHERE CNTR_NO = @[cntr_no]
        AND BKG_NO = @[bkg_no]
      ), 0
  ) + 1,
  @[cntr_tpsz_cd],
  @[vgm_id],
  @[func_cd],
  TO_DATE(@[iss_dt], 'YYYYMMDDHH24MI'),
  TO_DATE(@[cut_off_dt], 'YYYYMMDDHH24MI'),
  @[por_nm],
  @[por_cd],
  @[por_yd_cd],
  @[pol_nm],
  @[pol_cd],
  @[pol_yd_cd],
  @[smt_nm],
  @[smt_addr],
  @[smt_cty_ctnt],
  @[smt_ste_ctnt],
  @[smt_zip_ctnt],
  @[smt_cnt_cd],
  @[smt_cntc_dtl_ctnt],
  @[smt_cntc_eml],
  @[smt_cntc_phn_ctnt],
  @[vsl_cd],
  @[skd_voy_no],
  @[skd_dir_cd],
  @[vsl_nm],
  @[vsl_lloyd_no],
  @[bkg_ref_no],
  @[si_ref_no],
  @[bkg_no],
  @[trsp_ctrl_no_ctnt],
  @[vgm_wgt_qty],
  @[vrfd_wgt_cd],
  @[vgm_mzd_tp_cd],
  @[doc_id],
  TO_DATE(@[vgm_dtmn_dt], 'YYYYMMDDHH24MI'),
  TO_DATE(@[vgm_vrfy_dt], 'YYYYMMDDHH24MI'),
  @[seal_no_ctnt],
  @[pty_func_cd],
  @[pty_nm],
  @[pty_addr],
  @[pty_cty_ctnt],
  @[pty_ste_ctnt],
  @[pty_zip_ctnt],
  @[pty_cnt_cd],
  @[pty_pson_nm],
  @[cmmc_cntc_phn_ctnt],
  @[cmmc_cntc_eml],
  @[cmmc_cntc_fax_ctnt],
  @[cre_usr_id],
  SYSDATE,
  @[upd_usr_id],
  SYSDATE
)			]]></sql>
