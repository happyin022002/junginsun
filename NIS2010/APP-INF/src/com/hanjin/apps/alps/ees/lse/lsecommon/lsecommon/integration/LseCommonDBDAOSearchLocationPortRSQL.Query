<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LseCommonDBDAOSearchLocationPortRSQL">
			<desc><![CDATA[Port 코드조회]]></desc>
			<sql><![CDATA[
SELECT  T1.LOC_CD, T1.SCC_CD, T1.LOC_NM, T1.VOP_PORT_FLG
FROM    MDM_LOCATION T1, 
        MDM_COUNTRY T2
WHERE   1 = 1
AND     T1.CNT_CD=T2.CNT_CD
AND     T1.CALL_PORT_FLG='Y'
AND     T1.DELT_FLG = 'N'
AND     T2.DELT_FLG = 'N'
AND     T1.LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
