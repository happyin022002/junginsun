<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOAddRfaExptTrfProgByMstRfaVerCSQL">
			<desc><![CDATA[RFAExceptionTariffMgtDBDAOAddRfaExptTrfProgByMstRfaVerCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_RFA_EXPT_TRF_PROG
	(
		RFA_EXPT_DAR_NO
	   ,RFA_EXPT_MAPG_SEQ
	   ,RFA_EXPT_VER_SEQ	
	   ,PROG_SEQ
	   ,DMDT_EXPT_RQST_STS_CD
	   ,PROG_RMK
	   ,PROG_DT
	   ,PROG_USR_ID
	   ,PROG_OFC_CD
	   ,CRE_USR_ID
	   ,CRE_DT
	   ,CRE_OFC_CD
	   ,UPD_USR_ID
	   ,UPD_DT
	   ,UPD_OFC_CD	   
	)
SELECT  @[rfa_expt_dar_no]
	   ,1
	   ,(
			SELECT  MAX(RFA_EXPT_VER_SEQ)
			  FROM  DMT_RFA_EXPT_TRF
			 WHERE  PROP_NO = @[bzc_prop_no]
	    )
	   ,1
	   ,'T'
	   ,@[prog_rmk]
	   ,SYSDATE
	   ,@[cre_usr_id]
	   ,@[cre_ofc_cd]
	   ,@[cre_usr_id]
	   ,SYSDATE
	   ,@[cre_ofc_cd]
	   ,@[cre_usr_id]
	   ,SYSDATE
	   ,@[cre_ofc_cd]	   
  FROM  DUAL			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="1" out="N"/>
				<param name="bzc_prop_no" type="12" value="1" out="N"/>
				<param name="prog_rmk" type="12" value="1" out="N"/>
				<param name="cre_usr_id" type="12" value="1" out="N"/>
				<param name="cre_ofc_cd" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
