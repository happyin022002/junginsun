<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOInactiveListRSQL">
			<desc><![CDATA[InactiveListVO]]></desc>
			<sql><![CDATA[
SELECT  MAX(AA.DMDT_DELT_RQST_STS_CD) DMDT_DELT_RQST_STS_CD
       ,MAX(AA.STS_CD)STS_CD
       ,MAX(AA.INACT_RQST_NO) INACT_RQST_NO
       ,MAX(CASE WHEN AA.INACT_RQST_NO IS NULL THEN TO_CHAR(ROWNUM) ELSE AA.INACT_RQST_NO END) GROUP_RQST_NO
       ,MAX(AA.INACT_APRO_NO) INACT_APRO_NO
	    #if (${group_by} == 'BKG' )
       ,AA.BKG_NO
	    #end	
	    #if (${group_by} == 'CNTR' ) 
       ,AA.CNTR_NO
	    #end
       ,MAX(TO_DATE(TO_CHAR(SYSDATE, 'YYYYMMDD'), 'YYYYMMDD') - TO_DATE(TO_CHAR(CC.RQST_DT, 'YYYYMMDD'), 'YYYYMMDD')) AS OVR_DYS
       ,MAX(TO_CHAR(CC.RQST_DT,'YYYYMMDD')) RQST_DT
       ,MAX(CC.RQST_OFC_CD) RQST_OFC
       ,MAX( CASE WHEN CC.RQST_OFC_CD = @[usr_ofc_cd] THEN 'Y' ELSE 'N' END ) RQST_OFC_FLG
       ,MAX(( SELECT USR_NM FROM COM_USER WHERE USR_ID = CC.RQST_USR_ID )) AS RQST_NM
	   --// OOM ( OFC Operation Manager )
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'OOM' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN TO_CHAR(BB.UPD_DT,'YYYYMMDD') ELSE '' END) OOM_DT
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'OOM' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN BB.UPD_OFC_CD ELSE '' END) OOM_OFC
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'OOM' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN ( SELECT USR_NM FROM COM_USER WHERE USR_ID = BB.UPD_USR_ID ) ELSE '' END) OOM_NM	   
	   --// BBG ( Branch Office )
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'BBG' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN TO_CHAR(BB.UPD_DT,'YYYYMMDD') ELSE '' END) BBG_DT
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'BBG' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN BB.UPD_OFC_CD ELSE '' END) BBG_OFC
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'BBG' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN ( SELECT USR_NM FROM COM_USER WHERE USR_ID = BB.UPD_USR_ID ) ELSE '' END) BBG_NM
	   --// RHQ
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'RHQ' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN TO_CHAR(BB.UPD_DT,'YYYYMMDD') ELSE '' END) RHQ_DT
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'RHQ' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN BB.UPD_OFC_CD ELSE '' END) RHQ_OFC
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'RHQ' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN ( SELECT USR_NM FROM COM_USER WHERE USR_ID = BB.UPD_USR_ID ) ELSE '' END) RHQ_NM
       --// HDO
	   ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'HDO' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN TO_CHAR(BB.UPD_DT,'YYYYMMDD') ELSE '' END) HDO_DT
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'HDO' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN BB.UPD_OFC_CD ELSE '' END) HDO_OFC
       ,MAX(CASE WHEN BB.CHG_DELT_PATH_CD = 'HDO' AND TRIM(BB.CHG_DELT_STS_CD) IS NOT NULL THEN ( SELECT USR_NM FROM COM_USER WHERE USR_ID = BB.UPD_USR_ID ) ELSE '' END) HDO_NM
       ,AA.INACTIV_INFO INACTIV_INFO
       ,MAX(CASE WHEN AA.DMDT_DELT_RQST_STS_CD = 'R' AND @[tab_cd] = 'S' THEN 'N' ELSE AA.CHG_DELT_USR_YN END) CHG_DELT_USR_YN
	   ,MAX(( SELECT ATTR_CTNT2 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'CHG_DELT_RSN_CD' AND ATTR_CTNT1 = CC.DMDT_CHG_DELT_RSN_CD AND ROWNUM = 1)) INACT_RSN
	   ,MAX(( SELECT ATTR_CTNT4 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'CHG_DELT_RSN_CD' AND ATTR_CTNT3 = CC.DMDT_CHG_DELT_SPEC_RSN_CD AND ROWNUM = 1)) SPEC_RSN
	   
  FROM  (
			SELECT  T1.INACT_RQST_NO
                   ,T1.INACT_APRO_NO
                   ,T1.DMDT_DELT_RQST_STS_CD
                   ,(
						SELECT  INTG_CD_VAL_DP_DESC 
						  FROM  COM_INTG_CD_DTL 
						 WHERE  INTG_CD_ID = 'CD03382' 
						   AND  INTG_CD_VAL_CTNT = T1.DMDT_DELT_RQST_STS_CD 
					) AS STS_CD
                   ,T1.SYS_AREA_GRP_ID||'|'||T1.CNTR_NO||'|'||T1.CNTR_CYC_NO||'|'||T1.DMDT_TRF_CD||'|'||T1.DMDT_CHG_LOC_DIV_CD||'|'||T1.CHG_SEQ||'|'||T1.CHG_OFC_CD||'|'||T1.DELT_SEQ||'|' INACTIV_INFO
        	       ,MAX((
								
							CASE 
								WHEN DECODE(@[chg_delt_path_cd], 'OOM', 1, 'BBG', 2, 'RHQ', 3, 'HDO', 4, 0) >= 
									(
										SELECT  MAX(CHG_DELT_PATH_LVL)
										  FROM  DMT_CHG_DELT_PATH
										 WHERE  SYS_AREA_GRP_ID        = T1.SYS_AREA_GRP_ID
										   AND  CNTR_NO                = T1.CNTR_NO
										   AND  CNTR_CYC_NO            = T1.CNTR_CYC_NO
										   AND  DMDT_TRF_CD            = T1.DMDT_TRF_CD
										   AND  DMDT_CHG_LOC_DIV_CD    = T1.DMDT_CHG_LOC_DIV_CD
										   AND  CHG_SEQ                = T1.CHG_SEQ
										   AND  CHG_OFC_CD             = T1.CHG_OFC_CD
										   AND  DELT_SEQ               = T1.DELT_SEQ
										   AND  (CHG_DELT_PATH_CPLS_FLG = 'Y' OR CHG_DELT_STS_CD IN ('A', 'J'))
										   AND  CHG_OFC_CD             = T4.OFC_N8TH_LVL_CD
									) 
									THEN 
									(
										CASE 
											WHEN @[chg_delt_path_cd] = 'RHQ' AND T1.RQST_OFC_CD IN ('ATLSC', 'CHISC', 'HOUSC', 'LGBSC', 'SFOBS', 'NYCSC', 'OREBS', 'PDXSO', 'SEASC', 'PHXSA') 
												THEN
													CASE WHEN @[usr_id] IN ( SELECT ATTR_CTNT1 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INACTIV_APPL_USER' ) THEN 'Y' ELSE 'N' END
											ELSE 'Y'
										END
									)
								ELSE
									NVL((
											SELECT  CASE
														WHEN SYS_AREA_GRP_ID IS NOT NULL THEN
															CASE 
																WHEN @[chg_delt_path_cd] = 'RHQ' AND T1.RQST_OFC_CD in ('ATLSC', 'CHISC', 'HOUSC', 'LGBSC', 'SFOBS', 'NYCSC', 'OREBS', 'PDXSO', 'SEASC', 'PHXSA') THEN
																	CASE WHEN @[usr_id] in ( SELECT ATTR_CTNT1 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INACTIV_APPL_USER' ) THEN 'Y' ELSE 'N' END
																ELSE
																	'Y'
															END								
														ELSE
															'N'
													END
											  FROM  DMT_CHG_DELT_PATH
											 WHERE  SYS_AREA_GRP_ID        = T1.SYS_AREA_GRP_ID
											   AND  CNTR_NO                = T1.CNTR_NO
											   AND  CNTR_CYC_NO            = T1.CNTR_CYC_NO
											   AND  DMDT_TRF_CD            = T1.DMDT_TRF_CD
											   AND  DMDT_CHG_LOC_DIV_CD    = T1.DMDT_CHG_LOC_DIV_CD
											   AND  CHG_SEQ                = T1.CHG_SEQ
											   AND  CHG_OFC_CD             = T1.CHG_OFC_CD
											   AND  DELT_SEQ               = T1.DELT_SEQ
											   AND  CHG_DELT_PATH_CD       = @[chg_delt_path_cd]
											   AND  CHG_DELT_PATH_LVL     >= DECODE(T1.DMDT_DELT_RQST_STS_CD, 'R', 1, 'O', 1, 'P', 1, 'B', 2, 'E', 2, 'Q', 3, 'F', 3, 'H', 4, 'G', 4, 0)
											   AND  CHG_DELT_PATH_CPLS_FLG = 'Y'
											   AND  CHG_OFC_CD             = T4.OFC_N8TH_LVL_CD
											   AND  ROWNUM                 = 1
									),'N')
							END
					))													AS CHG_DELT_USR_YN		--// Charge Deletion ?붿껌????빐 ?뱀씤沅뚰븳?먯씤吏??щ?瑜??섑??몃떎.

	               #if (${group_by} == 'BKG' )
                   ,T3.BKG_NO
	               #elseif (${group_by} == 'CNTR' )
                   ,T3.CNTR_NO
	               #end
				   
			  FROM  DMT_CHG_DELT_RQST_APRO 	T1
		           ,DMT_CHG_BKG_CNTR 		T3
		           ,(
						SELECT  /*+NO_MERGE*/ OFC_N8TH_LVL_CD 
						  FROM  DMT_OFC_LVL_V  
						 WHERE  DECODE(@[usr_ofc_cd], 'SELCON', 'SELHO', SUBSTR(@[usr_ofc_cd], 1, 5)) IN 
								( OFC_N1ST_LVL_CD, OFC_N2ND_LVL_CD, OFC_N3RD_LVL_CD, OFC_N4TH_LVL_CD, OFC_N5TH_LVL_CD ) 
					) T4 
					
			 WHERE  1 = 1
               AND  T1.INACT_RQST_NO IS NULL
		       AND  T1.SYS_AREA_GRP_ID = T3.SYS_AREA_GRP_ID
               AND  T1.CNTR_NO = T3.CNTR_NO
               AND  T1.CNTR_CYC_NO = T3.CNTR_CYC_NO
			   
			   #if (${inact_no} == '' && ${apvl_no} == '' && ${bkg_no} == '' && ${bl_no} == '')
				#if (${dt_tp} == 'R')
               AND T1.RQST_DT BETWEEN TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD') AND TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
				#else
               AND T1.UPD_DT BETWEEN TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD') AND TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
				#end
	           #end
			   
			   #if (${inact_sts_cd} != '')
               AND  T1.DMDT_DELT_RQST_STS_CD IN (
			   		#foreach( $inact_sts_cd in ${inact_sts_cd_list} )
			   			#if($velocityCount < $inact_sts_cd_list.size()) '$inact_sts_cd', #else '$inact_sts_cd' #end
			   		#end
			   		)
			   #end
			   
			   #if (${ofc_cd} != '')
               AND  T1.CHG_OFC_CD IN (
					#foreach( $ofc_cd in ${ofc_cd_list} )
						#if($velocityCount < $ofc_cd_list.size()) '$ofc_cd', #else '$ofc_cd' #end
					#end
					)
			   #end
			   
			   #if (${trf_cd} != '')
               AND  T1.DMDT_TRF_CD IN (
					#foreach( $trf_cd in ${trf_cd_list} )
						#if($velocityCount < $trf_cd_list.size()) '$trf_cd', #else '$trf_cd' #end
					#end
					)
			   #end

			   #if (${inact_no} != '')
               AND  T1.INACT_RQST_NO IN (
					#foreach( $inact_no in ${inact_no_list} )
						#if($velocityCount < $inact_no_list.size()) '$inact_no', #else '$inact_no' #end
					#end
					)
			   #end
	
			   #if (${apvl_no} != '')
               AND  T1.INACT_APRO_NO IN (
					#foreach( $apvl_no in ${apvl_no_list} )
						#if($velocityCount < $apvl_no_list.size()) '$apvl_no', #else '$apvl_no' #end
					#end
					)
               #end
	
			   #if (${bkg_no} != '')
               AND  T3.BKG_NO IN (
					#foreach( $bkg_no in ${bkg_no_list} )
						#if($velocityCount < $bkg_no_list.size()) '$bkg_no', #else '$bkg_no' #end
					#end
					)
               #end
	
			   #if (${bl_no} != '')
               AND T3.BL_NO IN (
					#foreach( $bl_no in ${bl_no_list} )
						#if($velocityCount < $bl_no_list.size()) '$bl_no', #else '$bl_no' #end
					#end
					)
			   #end
			   
			   #if (${inact_rsn_cd} != '' )
               AND  T1.DMDT_CHG_DELT_RSN_CD IN (
					#foreach( $inact_rsn_cd in ${inact_rsn_cd_list} )
						#if($velocityCount < $inact_rsn_cd_list.size()) '$inact_rsn_cd', #else '$inact_rsn_cd' #end
					#end
					)
			   #end
	
			   #if (${spec_rsn_cd} != '' )
               AND  T1.DMDT_CHG_DELT_SPEC_RSN_CD IN (
					#foreach( $spec_rsn_cd in ${spec_rsn_cd_list} )
						#if($velocityCount < $spec_rsn_cd_list.size()) '$spec_rsn_cd', #else '$spec_rsn_cd' #end
					#end
					)
			   #end

			GROUP BY T1.INACT_RQST_NO
                    ,T1.INACT_APRO_NO
                    ,T1.DMDT_DELT_RQST_STS_CD
                    ,T1.DMDT_DELT_RQST_STS_CD
                    ,T1.SYS_AREA_GRP_ID
                    ,T1.CNTR_NO
                    ,T1.CNTR_CYC_NO
                    ,T1.DMDT_TRF_CD
                    ,T1.DMDT_CHG_LOC_DIV_CD
                    ,T1.CHG_SEQ
                    ,T1.CHG_OFC_CD
                    ,T1.DELT_SEQ
				    #if (${group_by} == 'BKG' )
                    ,T3.BKG_NO
					#elseif (${group_by} == 'CNTR' )
                    ,T3.CNTR_NO
					#end
             
        UNION ALL
        
			SELECT  T1.INACT_RQST_NO
				   ,T1.INACT_APRO_NO
                   ,T1.DMDT_DELT_RQST_STS_CD
                   ,( 
						SELECT  INTG_CD_VAL_DP_DESC 
						  FROM  COM_INTG_CD_DTL 
						 WHERE  INTG_CD_ID = 'CD03382' 
						   AND  INTG_CD_VAL_CTNT = T1.DMDT_DELT_RQST_STS_CD 
					) AS STS_CD
                   ,MAX(T1.SYS_AREA_GRP_ID||'|'||T1.CNTR_NO||'|'||T1.CNTR_CYC_NO||'|'||T1.DMDT_TRF_CD||'|'||T1.DMDT_CHG_LOC_DIV_CD||'|'||T1.CHG_SEQ||'|'||T1.CHG_OFC_CD||'|'||T1.DELT_SEQ)||'|' INACTIV_INFO
                   ,MAX((CASE
                            WHEN DECODE(@[chg_delt_path_cd], 'OOM', 1, 'BBG', 2, 'RHQ', 3, 'HDO', 4, 0) >= 
								(
									SELECT  MAX(CHG_DELT_PATH_LVL)
									  FROM  DMT_CHG_DELT_PATH
									 WHERE  SYS_AREA_GRP_ID        = T1.SYS_AREA_GRP_ID
									   AND  CNTR_NO                = T1.CNTR_NO
									   AND  CNTR_CYC_NO            = T1.CNTR_CYC_NO
									   AND  DMDT_TRF_CD            = T1.DMDT_TRF_CD
									   AND  DMDT_CHG_LOC_DIV_CD    = T1.DMDT_CHG_LOC_DIV_CD
									   AND  CHG_SEQ                = T1.CHG_SEQ
									   AND  CHG_OFC_CD             = T1.CHG_OFC_CD
									   AND  DELT_SEQ               = T1.DELT_SEQ
									   AND  (CHG_DELT_PATH_CPLS_FLG = 'Y' OR CHG_DELT_STS_CD IN ('A', 'J'))
									   AND  CHG_OFC_CD             =  T4.OFC_N8TH_LVL_CD
								) 
								 THEN 
								 (
									CASE 
										WHEN @[chg_delt_path_cd] = 'RHQ' AND T1.RQST_OFC_CD IN ('ATLSC', 'CHISC', 'HOUSC', 'LGBSC', 'SFOBS', 'NYCSC', 'OREBS', 'PDXSO', 'SEASC', 'PHXSA') THEN
											CASE WHEN @[usr_id] IN ( SELECT ATTR_CTNT1 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INACTIV_APPL_USER' ) THEN 'Y' ELSE 'N' END
										ELSE
											'Y'
									END
								 )
							ELSE
								NVL((
									SELECT  CASE
												WHEN SYS_AREA_GRP_ID IS NOT NULL THEN
													CASE 
														WHEN @[chg_delt_path_cd] = 'RHQ' AND T1.RQST_OFC_CD IN ('ATLSC', 'CHISC', 'HOUSC', 'LGBSC', 'SFOBS', 'NYCSC', 'OREBS', 'PDXSO', 'SEASC', 'PHXSA') THEN
															CASE 
																WHEN @[usr_id] IN ( SELECT ATTR_CTNT1 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INACTIV_APPL_USER' ) THEN 'Y' ELSE 'N' 
															END
														ELSE
															'Y'
													END								
												ELSE
													'N'
											END
									  FROM  DMT_CHG_DELT_PATH
									 WHERE  SYS_AREA_GRP_ID        = T1.SYS_AREA_GRP_ID
									   AND  CNTR_NO                = T1.CNTR_NO
									   AND  CNTR_CYC_NO            = T1.CNTR_CYC_NO
									   AND  DMDT_TRF_CD            = T1.DMDT_TRF_CD
									   AND  DMDT_CHG_LOC_DIV_CD    = T1.DMDT_CHG_LOC_DIV_CD
									   AND  CHG_SEQ                = T1.CHG_SEQ
									   AND  CHG_OFC_CD             = T1.CHG_OFC_CD
									   AND  DELT_SEQ               = T1.DELT_SEQ
									   AND  CHG_DELT_PATH_CD       = @[chg_delt_path_cd]
									   AND  CHG_DELT_PATH_LVL     >= DECODE(T1.DMDT_DELT_RQST_STS_CD, 'R', 1, 'O', 1, 'P', 1, 'B', 2, 'E', 2, 'Q', 3, 'F', 3, 'H', 4, 'G', 4, 0)
									   AND  CHG_DELT_PATH_CPLS_FLG = 'Y'
									   AND  CHG_OFC_CD =  T4.OFC_N8TH_LVL_CD
									   AND  ROWNUM = 1
								), 'N')
					END))													AS CHG_DELT_USR_YN		--// Charge Deletion ?붿껌????빐 ?뱀씤沅뚰븳?먯씤吏??щ?瑜??섑??몃떎.

				   #if (${group_by} == 'BKG' )
				   ,T3.BKG_NO
				   #elseif (${group_by} == 'CNTR' )
				   ,T3.CNTR_NO
				   #end
				   
			  FROM  DMT_CHG_DELT_RQST_APRO T1
				   ,DMT_CHG_BKG_CNTR T3
				   ,( 
						SELECT  /*+NO_MERGE*/ OFC_N8TH_LVL_CD 
						  FROM  DMT_OFC_LVL_V  
						 WHERE  DECODE(@[usr_ofc_cd], 'SELCON', 'SELHO', SUBSTR(@[usr_ofc_cd] ,1,5)) IN 
								(OFC_N1ST_LVL_CD, OFC_N2ND_LVL_CD, OFC_N3RD_LVL_CD, OFC_N4TH_LVL_CD, OFC_N5TH_LVL_CD) 
					) T4 
					
			 WHERE  1 = 1
               AND  T1.INACT_RQST_NO IS NOT NULL
			   AND  T1.SYS_AREA_GRP_ID = T3.SYS_AREA_GRP_ID
			   AND  T1.CNTR_NO         = T3.CNTR_NO
			   AND  T1.CNTR_CYC_NO     = T3.CNTR_CYC_NO

			   #if (${inact_no} == '' && ${apvl_no} == '' && ${bkg_no} == '' && ${bl_no} == '')
				#if (${dt_tp} == 'R')
               AND T1.RQST_DT BETWEEN TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD') AND TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
				#else
               AND T1.UPD_DT BETWEEN TO_DATE(REPLACE(@[fm_dt],'-',''),'YYYYMMDD') AND TO_DATE(REPLACE(@[to_dt],'-',''),'YYYYMMDD') + 0.99999
				#end
	           #end			   
			   
			   #if (${inact_sts_cd} != '')
               AND  T1.DMDT_DELT_RQST_STS_CD IN (
			   		#foreach( $inact_sts_cd in ${inact_sts_cd_list} )
			   			#if($velocityCount < $inact_sts_cd_list.size()) '$inact_sts_cd', #else '$inact_sts_cd' #end
			   		#end
			   		)
			   #end

			   #if (${ofc_cd} != '')
               AND  T1.CHG_OFC_CD IN (
					#foreach( $ofc_cd in ${ofc_cd_list} )
						#if($velocityCount < $ofc_cd_list.size()) '$ofc_cd', #else '$ofc_cd' #end
					#end
					)
			   #end
			   
			   #if (${trf_cd} != '')
               AND  T1.DMDT_TRF_CD IN (
					#foreach( $trf_cd in ${trf_cd_list} )
						#if($velocityCount < $trf_cd_list.size()) '$trf_cd', #else '$trf_cd' #end
					#end
					)
			   #end
			   
			   #if (${inact_no} != '')
               AND  T1.INACT_RQST_NO IN (
					#foreach( $inact_no in ${inact_no_list} )
						#if($velocityCount < $inact_no_list.size()) '$inact_no', #else '$inact_no' #end
					#end
					)
			   #end
	
			   #if (${apvl_no} != '')
               AND  T1.INACT_APRO_NO IN (
					#foreach( $apvl_no in ${apvl_no_list} )
						#if($velocityCount < $apvl_no_list.size()) '$apvl_no', #else '$apvl_no' #end
					#end
					)
               #end
	
			   #if (${bkg_no} != '')
               AND  T3.BKG_NO IN (
					#foreach( $bkg_no in ${bkg_no_list} )
						#if($velocityCount < $bkg_no_list.size()) '$bkg_no', #else '$bkg_no' #end
					#end
					)
               #end

			   #if (${bl_no} != '')
               AND T3.BL_NO IN (
					#foreach( $bl_no in ${bl_no_list} )
						#if($velocityCount < $bl_no_list.size()) '$bl_no', #else '$bl_no' #end
					#end
					)
			   #end
			   
			   #if (${inact_rsn_cd} != '' )
               AND  T1.DMDT_CHG_DELT_RSN_CD IN (
					#foreach( $inact_rsn_cd in ${inact_rsn_cd_list} )
						#if($velocityCount < $inact_rsn_cd_list.size()) '$inact_rsn_cd', #else '$inact_rsn_cd' #end
					#end
					)
			   #end
	
			   #if (${spec_rsn_cd} != '' )
               AND  T1.DMDT_CHG_DELT_SPEC_RSN_CD IN (
					#foreach( $spec_rsn_cd in ${spec_rsn_cd_list} )
						#if($velocityCount < $spec_rsn_cd_list.size()) '$spec_rsn_cd', #else '$spec_rsn_cd' #end
					#end
					)
			   #end
			   
			GROUP BY T1.INACT_RQST_NO
                    ,T1.INACT_APRO_NO
                    ,T1.DMDT_DELT_RQST_STS_CD
                    ,T1.DMDT_DELT_RQST_STS_CD 
					#if (${group_by} == 'BKG' )
                    ,T3.BKG_NO
					#elseif (${group_by} == 'CNTR' )
                    ,T3.CNTR_NO
					#end
		) 						AA
       ,DMT_CHG_DELT_RQST_APRO 	CC
       ,DMT_CHG_DELT_PATH 		BB
	   
 WHERE  1 = 1
   AND  AA.DMDT_DELT_RQST_STS_CD IS NOT NULL
   AND  SUBSTR(AA.INACTIV_INFO,0,INSTR(AA.INACTIV_INFO,'|',1,1)-1) = CC.SYS_AREA_GRP_ID
   AND  SUBSTR(AA.INACTIV_INFO,INSTR(AA.INACTIV_INFO,'|',1,1)+1,INSTR(AA.INACTIV_INFO,'|',1,2)-INSTR(AA.INACTIV_INFO,'|',1,1)-1) = CC.CNTR_NO
   AND  SUBSTR(AA.INACTIV_INFO,INSTR(AA.INACTIV_INFO,'|',1,2)+1,INSTR(AA.INACTIV_INFO,'|',1,3)-INSTR(AA.INACTIV_INFO,'|',1,2)-1) = CC.CNTR_CYC_NO
   AND  SUBSTR(AA.INACTIV_INFO,INSTR(AA.INACTIV_INFO,'|',1,3)+1,INSTR(AA.INACTIV_INFO,'|',1,4)-INSTR(AA.INACTIV_INFO,'|',1,3)-1) = CC.DMDT_TRF_CD
   AND  SUBSTR(AA.INACTIV_INFO,INSTR(AA.INACTIV_INFO,'|',1,4)+1,INSTR(AA.INACTIV_INFO,'|',1,5)-INSTR(AA.INACTIV_INFO,'|',1,4)-1) = CC.DMDT_CHG_LOC_DIV_CD
   AND  SUBSTR(AA.INACTIV_INFO,INSTR(AA.INACTIV_INFO,'|',1,5)+1,INSTR(AA.INACTIV_INFO,'|',1,6)-INSTR(AA.INACTIV_INFO,'|',1,5)-1) = CC.CHG_SEQ
   AND  SUBSTR(AA.INACTIV_INFO,INSTR(AA.INACTIV_INFO,'|',1,6)+1,INSTR(AA.INACTIV_INFO,'|',1,7)-INSTR(AA.INACTIV_INFO,'|',1,6)-1) = CC.CHG_OFC_CD
   AND  SUBSTR(AA.INACTIV_INFO,INSTR(AA.INACTIV_INFO,'|',1,7)+1,INSTR(AA.INACTIV_INFO,'|',1,8)-INSTR(AA.INACTIV_INFO,'|',1,7)-1) = CC.DELT_SEQ
   AND  BB.SYS_AREA_GRP_ID(+)     = CC.SYS_AREA_GRP_ID
   AND  BB.CNTR_NO(+)             = CC.CNTR_NO
   AND  BB.CNTR_CYC_NO(+)         = CC.CNTR_CYC_NO
   AND  BB.DMDT_TRF_CD(+)         = CC.DMDT_TRF_CD
   AND  BB.DMDT_CHG_LOC_DIV_CD(+) = CC.DMDT_CHG_LOC_DIV_CD
   AND  BB.CHG_SEQ(+)             = CC.CHG_SEQ
   AND  BB.CHG_OFC_CD(+)          = CC.CHG_OFC_CD
   AND  BB.DELT_SEQ(+)            = CC.DELT_SEQ

#if (${tab_cd} == 'P' )
   AND  (
			(
					 CHG_DELT_USR_YN           = 'Y' 
				AND  AA.DMDT_DELT_RQST_STS_CD != 'C'
				AND  ( 
						SELECT  MIN(CHG_DELT_PATH_LVL) 
						  FROM  DMT_CHG_DELT_PATH T2
						 WHERE  1 = 1
						   AND  BB.SYS_AREA_GRP_ID        = T2.SYS_AREA_GRP_ID
						   AND  BB.CNTR_NO                = T2.CNTR_NO
						   AND  BB.CNTR_CYC_NO            = T2.CNTR_CYC_NO
						   AND  BB.DMDT_TRF_CD            = T2.DMDT_TRF_CD
						   AND  BB.DMDT_CHG_LOC_DIV_CD    = T2.DMDT_CHG_LOC_DIV_CD
						   AND  BB.CHG_SEQ                = T2.CHG_SEQ
						   AND  BB.CHG_OFC_CD             = T2.CHG_OFC_CD
						   AND  BB.DELT_SEQ               = T2.DELT_SEQ
						   AND  T2.CHG_DELT_PATH_CPLS_FLG = 'Y'
						   AND  T2.CHG_DELT_STS_CD       != 'A' 
					 ) = CASE @[chg_delt_path_cd] 
							WHEN 'HDO' THEN 4
							WHEN 'RHQ' THEN 3
							WHEN 'BBG' THEN 2
							WHEN 'OOM' THEN 1
							ELSE 0 
						 END 

				AND  SUBSTR(INACTIV_INFO,INSTR(INACTIV_INFO,'|',1,6)+1,INSTR(INACTIV_INFO,'|',1,7)-INSTR(INACTIV_INFO,'|',1,6)-1) IN 
					 ( 
						SELECT  OFC_N8TH_LVL_CD 
						  FROM  DMT_OFC_LVL_V 
						 WHERE  DECODE(@[usr_ofc_cd], 'SELCON', 'SELHO', SUBSTR(@[usr_ofc_cd], 1, 5)) IN 
								(OFC_N1ST_LVL_CD, OFC_N2ND_LVL_CD, OFC_N3RD_LVL_CD, OFC_N4TH_LVL_CD, OFC_N5TH_LVL_CD)
					 )
                AND  AA.DMDT_DELT_RQST_STS_CD NOT IN ('P', 'E', 'F', 'G')
			)
         OR ( 
				( 	@[chg_delt_path_cd] IS NULL 
				OR  ( 
						CASE 
							WHEN @[chg_delt_path_cd] = 'RHQ' AND CC.RQST_OFC_CD IN ('ATLSC', 'CHISC', 'HOUSC', 'LGBSC', 'SFOBS', 'NYCSC', 'OREBS', 'PDXSO', 'SEASC', 'PHXSA') 
								THEN
									CASE 
										WHEN @[usr_id] IN ( SELECT ATTR_CTNT1 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INACTIV_APPL_USER' ) THEN 'Y' ELSE 'N' 
									END
							ELSE 'Y' 
						END 
					) = 'N' )
				AND  AA.DMDT_DELT_RQST_STS_CD = 'R'
				AND  SUBSTR(INACTIV_INFO,INSTR(INACTIV_INFO,'|',1,6)+1,INSTR(INACTIV_INFO,'|',1,7)-INSTR(INACTIV_INFO,'|',1,6)-1) = DECODE(@[usr_ofc_cd], 'SELCON', 'SELHO', SUBSTR(@[usr_ofc_cd],1,5)) 
			)
		)

#elseif (${tab_cd} == 'S' )
   AND  (
			(
				(
					CASE 
						WHEN @[chg_delt_path_cd] = 'OOM' THEN 1 
						WHEN @[chg_delt_path_cd] = 'BBG' THEN 2 
						WHEN @[chg_delt_path_cd] = 'RHQ' THEN 3 
						WHEN @[chg_delt_path_cd] = 'HDO' THEN 4
					END = 
					CASE 
						WHEN @[chg_delt_path_cd] = 'OOM' AND AA.DMDT_DELT_RQST_STS_CD IN ('O', 'P') THEN 1
						WHEN @[chg_delt_path_cd] = 'BBG' AND AA.DMDT_DELT_RQST_STS_CD IN ('B', 'E') THEN 2
						WHEN @[chg_delt_path_cd] = 'RHQ' AND AA.DMDT_DELT_RQST_STS_CD IN ('Q', 'F') THEN 3
						WHEN @[chg_delt_path_cd] = 'HDO' AND AA.DMDT_DELT_RQST_STS_CD IN ('H', 'G') THEN 4                       
					END
				)
				AND  SUBSTR(INACTIV_INFO,INSTR(INACTIV_INFO,'|',1,6)+1,INSTR(INACTIV_INFO,'|',1,7)-INSTR(INACTIV_INFO,'|',1,6)-1) IN 
					 ( 
						SELECT  OFC_N8TH_LVL_CD 
						  FROM  DMT_OFC_LVL_V 
						 WHERE  DECODE(@[usr_ofc_cd], 'SELCON', 'SELHO', SUBSTR(@[usr_ofc_cd], 1, 5)) IN 
								(OFC_N1ST_LVL_CD, OFC_N2ND_LVL_CD, OFC_N3RD_LVL_CD, OFC_N4TH_LVL_CD, OFC_N5TH_LVL_CD)
					 )
			)

         OR (
				( 
					@[chg_delt_path_cd] IS NULL OR
					( 
						CASE 
							WHEN @[chg_delt_path_cd] = 'RHQ' AND CC.RQST_OFC_CD IN ('ATLSC', 'CHISC', 'HOUSC', 'LGBSC', 'SFOBS', 'NYCSC', 'OREBS', 'PDXSO', 'SEASC', 'PHXSA') 
								THEN
									CASE 
										WHEN @[usr_id] IN ( SELECT ATTR_CTNT1 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'INACTIV_APPL_USER' ) THEN 'Y' ELSE 'N' 
									END
							ELSE 'Y' 
						END 
					) = 'N'
				)
				AND  AA.DMDT_DELT_RQST_STS_CD IN ('R', 'C')
				AND  SUBSTR(INACTIV_INFO,INSTR(INACTIV_INFO,'|',1,6)+1,INSTR(INACTIV_INFO,'|',1,7)-INSTR(INACTIV_INFO,'|',1,6)-1) = DECODE(@[usr_ofc_cd], 'SELCON', 'SELHO', SUBSTR(@[usr_ofc_cd],1,5)) 
			)
		)
#end

GROUP BY AA.INACTIV_INFO
		#if (${group_by} == 'BKG' )
        ,AA.BKG_NO
		#end
		#if (${group_by} == 'CNTR' )
        ,AA.CNTR_NO
		#end
		
ORDER BY 
		#if (${group_by} == 'INACT' )
		INACT_RQST_NO
		#end
		#if (${group_by} == 'BKG' )
		AA.BKG_NO
		#end
		#if (${group_by} == 'CNTR' )
		AA.CNTR_NO
		#end			]]></sql>
			<params>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="tab_cd" type="12" value="" out="N"/>
				<param name="chg_delt_path_cd" type="12" value=" " out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
