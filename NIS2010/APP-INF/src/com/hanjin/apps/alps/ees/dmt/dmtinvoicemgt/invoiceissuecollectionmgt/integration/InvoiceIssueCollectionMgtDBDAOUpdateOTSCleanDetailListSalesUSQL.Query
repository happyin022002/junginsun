<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOUpdateOTSCleanDetailListSalesUSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOUpdateOTSCleanDetailListSalesUSQL]]></desc>
			<sql><![CDATA[
UPDATE DMT_OTS_DTL
SET IB_SLS_OFC_CD = @[ib_sls_ofc_cd] ,
  IB_SREP_CD = @[ib_srep_cd] ,
  UPD_USR_ID = @[usid] ,
  UPD_DT = SYSDATE
WHERE ( BKG_NO,
      CNTR_NO,
      DMDT_TRF_CD ) IN (
    SELECT B.BKG_NO,
      B.CNTR_NO,
      A.DMDT_TRF_CD
    FROM DMT_CHG_CALC A,
      DMT_CHG_BKG_CNTR B
    WHERE 1=1
      AND A.SYS_AREA_GRP_ID = B.SYS_AREA_GRP_ID
      AND A.CNTR_NO = B.CNTR_NO
      AND A.CNTR_CYC_NO = B.CNTR_CYC_NO
      AND A.DMDT_INV_NO = @[invnoo] )			]]></sql>
			<params>
				<param name="ib_sls_ofc_cd" type="12" value="" out="N"/>
				<param name="ib_srep_cd" type="12" value="" out="N"/>
				<param name="usid" type="12" value="" out="N"/>
				<param name="invnoo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
