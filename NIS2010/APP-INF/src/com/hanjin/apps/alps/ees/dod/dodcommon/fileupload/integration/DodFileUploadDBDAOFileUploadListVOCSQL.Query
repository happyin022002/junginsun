<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodFileUploadDBDAOFileUploadListVOCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO DOD_ATCH_FILE
( ATCH_FILE_LNK_ID
 ,ATCH_FILE_LNK_SEQ
 ,FILE_SAV_ID
 ,CRE_DT
 ,CRE_USR_ID
 ,UPD_DT
 ,UPD_USR_ID
)
SELECT
  @[atch_file_lnk_id] AS ATCH_FILE_LNK_ID
 ,(SELECT NVL(MAX(ATCH_FILE_LNK_SEQ),0)+1 FROM DOD_ATCH_FILE WHERE ATCH_FILE_LNK_ID = @[atch_file_lnk_id]) AS ATCH_FILE_LNK_SEQ
 ,@[file_sav_id] 	AS FILE_SAV_ID
 ,SYSDATE			AS CRE_DT
 ,@[upd_usr_id]		AS CRE_USR_ID
 ,SYSDATE			AS UPD_DT
 ,@[upd_usr_id]		AS UPD_USR_ID
FROM DUAL			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
				<param name="file_sav_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
