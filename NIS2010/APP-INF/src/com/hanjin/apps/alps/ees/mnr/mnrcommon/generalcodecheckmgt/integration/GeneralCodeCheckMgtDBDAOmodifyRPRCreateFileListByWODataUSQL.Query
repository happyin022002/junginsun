<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyRPRCreateFileListByWODataUSQL">
			<desc><![CDATA[modifyRPRCreateFileListByWOData]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
SET INP_MSG4 = 'NW'
WHERE TMP_SEQ = @[tmp_seq]
AND   INP_MSG4 = 'SS'
AND NOT EXISTS 
( 
	SELECT B.MNR_ORD_SEQ 
	  FROM MNR_RPR_RQST_HDR B
	 WHERE B.VNDR_SEQ = @[vndr_seq] 
	   AND A.INP_MSG1 = B.RQST_EQ_NO
	   AND B.RPR_RQST_LST_VER_FLG ='Y'
	   AND B.MNR_ORD_SEQ IS NOT NULL 
		
 )			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
