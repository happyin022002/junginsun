<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrMtyBkgCreateDBDAOCheckNotPermitTypeSizeRSQL">
			<desc><![CDATA[TYPE SIZE 가 양하 혹은 선적 허용된 것인지 확인]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(*) > 0 THEN 'Y' 
                              ELSE 'N'
       END CHK_RESULT  
FROM EQR_CTRL_ROUT_SET A
    ,EQR_CTRL_ROUT_SET_TP_SZ B
WHERE A.LODG_DCHG_DIV_CD = B.LODG_DCHG_DIV_CD
AND   A.LOC_CD           = B.LOC_CD
AND   A.LODG_DCHG_DIV_CD = @[load_disc]
AND   B.CNTR_TPSZ_CD     = @[tpsz]
#if(${load_div} == 'Y') -- YARD
AND   A.LOC_CD = ( SELECT LOC_CD FROM MDM_YARD WHERE YD_CD = @[yard_cd] AND ROWNUM=1 )
#end
#if(${load_div} == 'L') -- LOAD
AND   A.LOC_CD = @[yard_cd]
#end

#if(${load_disc} == 'L') -- LOAD
AND   A.MTY_BKG_DSABIL_FLG = 'Y'  
#end
#if(${load_disc} == 'D') -- DISC
AND   A.MTY_SPLIT_BKG_DSABIL_FLG ='Y'
#end			]]></sql>
			<params>
				<param name="load_disc" type="12" value="" out="N"/>
				<param name="tpsz" type="12" value="" out="N"/>
				<param name="yard_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
