<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOModifyEccLinkInfoUSQL">
			<desc><![CDATA[특정 ecc 의 EQR_ECC_LNK  테이블의 정보 수정]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_ECC_LNK I      	                                                     		
USING 
(
    SELECT 
        @[fm_ecc_cd]    AS FM_ECC_CD    	                                                       
        , @[to_ecc_cd]  AS TO_ECC_CD    	                                                       
        , @[trsp_mod_cd] AS TRSP_MOD_CD    	                                                     
    FROM DUAL
) M              	                                                                   
ON 
(
    I.FM_ECC_CD         = M.FM_ECC_CD 	                                            
    AND I.TO_ECC_CD     = M.TO_ECC_CD   	                                      
    AND I.TRSP_MOD_CD   = M.TRSP_MOD_CD
)  	                
WHEN MATCHED THEN 	                                        
    UPDATE SET 
        I.TZ_DYS             = @[tz_dys]                            
        ,I.TZ_20FT_COST_AMT  = @[tz_20ft_cost_amt]
        ,I.TZ_40FT_COST_AMT  = @[tz_40ft_cost_amt]  	                                                
        ,I.TZ_45FT_COST_AMT  = @[tz_45ft_cost_amt] 	                   
        ,I.CNTR_MAX_CAPA_QTY = @[cntr_max_capa_qty]   	                                             
        ,I.CNTR_VOL_UT_CD    = @[cntr_vol_ut_cd] 	                                               
        ,I.TRSP_FREQ_KNT     = @[trsp_freq_knt]                                                                                         
        ,I.EXPT_FM_YRWK      = @[expt_fm_yrwk]  	                                                
        ,I.EXPT_TO_YRWK 	 = @[expt_to_yrwk]   	                                               
        ,I.EXPT_MAX_CAPA_QTY = @[expt_max_capa_qty]   	                                               
        ,I.EXPT_VOL_UT_CD    = @[expt_vol_ut_cd]   	                        
        ,I.DELT_FLG          = 'N'   	                             
        ,I.UPD_USR_ID        = @[user_id]   	                                               
        ,I.UPD_DT            = SYSDATE   	                                                                
WHEN NOT MATCHED THEN    	                                                                   
    INSERT (
        I.FM_ECC_CD    	                                                                 
        ,I.TO_ECC_CD      	                                                               
        ,I.TRSP_MOD_CD      	                                                         
        ,I.TZ_DYS             	                                                       
        ,I.TZ_20FT_COST_AMT   	                                                       
        ,I.TZ_40FT_COST_AMT    	                                                  
        ,I.TZ_45FT_COST_AMT    	                                                  
        ,I.CNTR_MAX_CAPA_QTY    	                                                 
        ,I.CNTR_VOL_UT_CD     	                                                       
        ,I.TRSP_FREQ_KNT      	                                                       
        ,I.EXPT_FM_YRWK        	                                                  
        ,I.EXPT_TO_YRWK        	                                                  
        ,I.EXPT_MAX_CAPA_QTY    	                                                 
        ,I.EXPT_VOL_UT_CD       	                     
        ,I.CRE_USR_ID          	                                                  
        ,I.CRE_DT              	                                                  
        ,I.UPD_USR_ID          	                                                  
        ,I.UPD_DT               	                                                 
    ) VALUES ( 
        @[fm_ecc_cd] 
        , @[to_ecc_cd]
        , @[trsp_mod_cd]
        , @[tz_dys]
        , @[tz_20ft_cost_amt]
        , @[tz_40ft_cost_amt]
        , @[tz_45ft_cost_amt]
        , @[cntr_max_capa_qty]
        , @[cntr_vol_ut_cd]
        , @[trsp_freq_knt]
        , @[expt_fm_yrwk]
        , @[expt_to_yrwk]
        , @[expt_max_capa_qty]
        , @[expt_vol_ut_cd]
        , @[user_id] 
        , SYSDATE 
        , @[user_id] 
        , SYSDATE
    )			]]></sql>
			<params>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="tz_dys" type="12" value="" out="N"/>
				<param name="tz_20ft_cost_amt" type="12" value="" out="N"/>
				<param name="tz_40ft_cost_amt" type="12" value="" out="N"/>
				<param name="tz_45ft_cost_amt" type="12" value="" out="N"/>
				<param name="cntr_max_capa_qty" type="12" value="" out="N"/>
				<param name="cntr_vol_ut_cd" type="12" value="" out="N"/>
				<param name="trsp_freq_knt" type="12" value="" out="N"/>
				<param name="expt_fm_yrwk" type="12" value="" out="N"/>
				<param name="expt_to_yrwk" type="12" value="" out="N"/>
				<param name="expt_max_capa_qty" type="12" value="" out="N"/>
				<param name="expt_vol_ut_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
