<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmEdiSendDBDAOChssExceptFlagRSQL">
			<desc><![CDATA[CHSS_EXCEPT 조회
변수
:bkg_no
:cntr_no
:ie_ind    - IP : 미국IMPORT, XP : 미국EXPORT
:tpsz_cd
:loc_cd    - IMPORT 일때는 DEL_CD, EXPORT 일때는 POR_CD  
:sc_no
:eff_dt    - BKG 의 OC_DT 없으면 BKG_CRE_DT]]></desc>
			<sql><![CDATA[
SELECT   
           -- RCV_TERM : 출발지 운송, DE_TERM : 도착지 운송
           CASE WHEN  DECODE(@[ie_ind],'XP',B.RCV_TERM_CD, B.DE_TERM_CD) <> 'D' THEN -- XP : 
                      (
                       CASE WHEN      SUBSTR(@[tpsz_cd],1,1) = 'R'  
                                 AND  B.DEL_CD             = 'USOAK' 
                                 AND @[ie_ind]               = 'IP'  -- us import
                                 AND (
                                      SELECT C.RD_CGO_FLG 
                                      FROM BKG_CONTAINER C 
                                      WHERE @[bkg_no] = C.BKG_NO
                                      AND   @[cntr_no] = C.CNTR_NO
                                     ) ='N' 
                                 THEN 'Y'
                            WHEN      B.SC_NO    = 'AEN110235' 
                                  AND @[ie_ind]               = 'IP'  -- us import
                                  AND B.POD_CD IN ('CAPRR','CAVAN','USSEA','USTIW','USPDX','USOAK','USLAX','USLGB')  
                                THEN 'Y' 
                            WHEN      B.SC_NO      = 'AEN110235' 
                                  AND @[ie_ind]               = 'IP'  -- us import 
                                  AND B.POD_CD NOT IN ('CAPRR','CAVAN','USSEA','USTIW','USPDX','USOAK','USLAX','USLGB')
                                  AND B.DEL_CD IN ('USADV','USCSF','USCHS','USZGB','USGSA','USHGR','USJAX','USISM','USMVP','USNYC','USORF','USNWB','USPFP','USPPY','USPFD','USPWN','USSAV','USSVH','USTHM','USVLD','USBAF','USILM')
                                THEN 'N'
                            ELSE      
                                DECODE( (
                                        SELECT 1
                                        FROM  CGM_SC_EXPT_VER V
                                             ,CGM_SC_EXPT_LIST L
                                             ,BKG_BOOKING B 
                                        WHERE V.PROP_NO = (SELECT A.PROP_NO 
                                                           FROM  PRI_SP_HDR A 
                                                           WHERE A.SC_NO = B.SC_NO
                                                          ) 
                                        AND   V.CHSS_EXPT_VER_STS_CD = 'L' -- 하드코딩 
                                        AND   TO_DATE(@[eff_dt], 'YYYYMMDD') BETWEEN V.EFF_DT AND V.EXP_DT               
                                        AND   V.PROP_NO              = L.PROP_NO 
                                        AND   V.SC_EXPT_VER_SEQ      = L.SC_EXPT_VER_SEQ 
                                        AND   V.DELT_FLG             = 'N'                                         
                                        AND   L.LOC_CD               = @[loc_cd]  -- del(import), por(export)
                                        AND   B.BKG_NO               = @[bkg_no]
                                        AND   NVL(L.CUST_CNT_CD,'*')         IN ('*',B.AGMT_ACT_CNT_CD) 
                                        AND   NVL(L.CUST_SEQ,-1)             IN (-1,B.AGMT_ACT_CUST_SEQ) 
                                        AND   NVL(L.CHSS_CNTR_CGO_TP_CD,'*') IN ('*',DECODE(SUBSTR(@[tpsz_cd],1,1),'R','R','D','D','S')) 
                                        AND   NVL(L.CMDT_CD,'*')             IN ('*',B.CMDT_CD) 
                                        AND   ROWNUM=1
                                     ),1, 'Y', 'N'
                                     )
                       END
                       )
                ELSE 'N' -- N : exception 아님, Y : EXCEPTION
           END  AS CHZ_EXCEPT

    FROM  BKG_BOOKING  B        
    WHERE B.BKG_NO  = @[bkg_no]			]]></sql>
			<params>
				<param name="ie_ind" type="12" value="" out="N"/>
				<param name="tpsz_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
