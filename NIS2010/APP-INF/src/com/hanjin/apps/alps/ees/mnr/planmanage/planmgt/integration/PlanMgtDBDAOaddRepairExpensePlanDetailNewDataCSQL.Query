<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanMgtDBDAOaddRepairExpensePlanDetailNewDataCSQL">
			<desc><![CDATA[SELOPB일때 최초 입력 데이터의 Detail 정보를 저장한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_PLN_DTL (
    		MNR_PLN_SEQ
    		,	MNR_PLN_DTL_SEQ
    		,	CTRL_OFC_CD
    		,	EQ_TPSZ_CD
    		,	EQ_QTY
    		,	ACCT_CD
    		,	MNR_PLN_AMT
    		,	MNR_PLN_DTL_RMK
    		,   OFC_TP_CD
    		,	CRE_USR_ID 
    		,	CRE_DT
    		,	UPD_USR_ID
    		,	UPD_DT
            ) VALUES(
               (SELECT MNR_PLN_SEQ
            	  FROM MNR_PLN_HDR
            	 WHERE 1=1
                   AND MNR_PLN_YR  = @[mnr_pln_yr]
            	AND MNR_PLN_OFC_CD = @[ctrl_ofc_cd]
                )
    		,	@[mnr_pln_dtl_seq]
    		,	@[ctrl_ofc_cd]
    		,	'D2' 
    		,	'1' 
    		,	@[acct_cd]
    		,	 @[mnr_pln_amt] 
    		,	''   
    		,   @[ofc_tp_cd]
    		,	@[cre_usr_id]
    		,	SYSDATE
    		,	@[cre_usr_id]
    		,	SYSDATE
    		) 			]]></sql>
			<params>
				<param name="mnr_pln_yr" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="mnr_pln_dtl_seq" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="mnr_pln_amt" type="12" value="" out="N"/>
				<param name="ofc_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
