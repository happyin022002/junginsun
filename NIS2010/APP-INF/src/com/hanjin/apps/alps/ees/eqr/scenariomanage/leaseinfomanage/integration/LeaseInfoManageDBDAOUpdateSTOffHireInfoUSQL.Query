<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LeaseInfoManageDBDAOUpdateSTOffHireInfoUSQL">
			<desc><![CDATA[EQR_SCNR_SHRT_TERM_OFFH_COND 테이블 데이터 수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_SCNR_SHRT_TERM_OFFH_COND  SET         
    AVAL_CNTR_QTY    = @[aval_cntr_qty] ,          
    LFT_CHG_AMT      = @[lft_chg_amt] ,          
    DRYG_AMT         = @[dryg_amt] ,  
    DFLT_USD_DYS     = @[dflt_usd_dys] ,          
    DRFF_CHG_CR_AMT  = @[drff_chg_cr_amt] ,          
    UPD_USR_ID       = @[user_id] ,          
    UPD_DT           = SYSDATE     
WHERE       
    SCNR_ID          = @[scnr_id]           
    AND  ECC_CD           = @[ecc_cd]           
    AND  CTRT_OFC_CTY_CD  = @[ctrt_ofc_cty_cd03]           
    AND  CTRT_SEQ         = @[ctrt_ofc_cty_cd]           
    AND  CNTR_TPSZ_CD     = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="aval_cntr_qty" type="12" value="" out="N"/>
				<param name="lft_chg_amt" type="12" value="" out="N"/>
				<param name="dryg_amt" type="12" value="" out="N"/>
				<param name="dflt_usd_dys" type="12" value="" out="N"/>
				<param name="drff_chg_cr_amt" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="ctrt_ofc_cty_cd03" type="12" value="" out="N"/>
				<param name="ctrt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
