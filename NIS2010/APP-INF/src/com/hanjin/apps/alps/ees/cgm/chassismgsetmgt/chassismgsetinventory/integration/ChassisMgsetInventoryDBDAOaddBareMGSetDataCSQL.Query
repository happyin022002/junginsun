<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOaddBareMGSetDataCSQL">
			<desc><![CDATA[addBareMGSetData]]></desc>
			<sql><![CDATA[
INSERT INTO CGM_BARE_MGST_REPO
(
     MGST_NO
    ,MGST_BARE_STS_SEQ
    ,MGST_BARE_STS_CD  
    ,CHSS_NO
    ,CNTR_NO
    ,VNDR_SEQ
    ,EQ_TRSP_MOD_CD
    ,RTN_YD_CD 
    ,BARE_MGST_RMK
    ,MGST_BARE_EVNT_DT 
    ,CRE_OFC_CD
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT   
    
)
VALUES
(
     @[mgst_no]
	,(
        SELECT NVL(MAX(MGST_BARE_STS_SEQ),0) + 1 as MGST_BARE_STS_SEQ
        FROM CGM_BARE_MGST_REPO
        WHERE MGST_NO = @[mgst_no]   
     )
    ,@[mgst_bare_sts_cd]   
    ,@[chss_no]
    ,@[cntr_no]
    ,@[vndr_seq]
    ,@[eq_trsp_mod_cd]
	,@[rtn_yd_cd]
    ,@[bare_mgst_rmk]
    ,TO_DATE(@[mgst_bare_evnt_dt], 'YYYYMMDD')
    ,@[cre_ofc_cd]
    ,@[upd_usr_id]
    ,SYSDATE
    ,@[upd_usr_id]
    ,SYSDATE
)			]]></sql>
			<params>
				<param name="mgst_no" type="12" value="" out="N"/>
				<param name="mgst_bare_sts_cd" type="12" value="" out="N"/>
				<param name="chss_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="eq_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="rtn_yd_cd" type="12" value="" out="N"/>
				<param name="bare_mgst_rmk" type="12" value="" out="N"/>
				<param name="mgst_bare_evnt_dt" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
