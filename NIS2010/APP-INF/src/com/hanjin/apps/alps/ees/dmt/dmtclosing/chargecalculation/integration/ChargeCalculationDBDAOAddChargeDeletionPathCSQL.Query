<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOAddChargeDeletionPathCSQL">
			<desc><![CDATA[ChargeCalculationDBDAOAddChargeDeletionPathCSQL]]></desc>
			<sql><![CDATA[
insert into DMT_CHG_DELT_PATH(
	SYS_AREA_GRP_ID 
   ,CNTR_NO
   ,CNTR_CYC_NO
   ,DMDT_TRF_CD
   ,DMDT_CHG_LOC_DIV_CD
   ,CHG_SEQ
   ,CHG_OFC_CD
   ,DELT_SEQ
   ,CHG_DELT_PATH_CD
   ,CHG_DELT_PATH_LVL
   ,CHG_DELT_STS_CD
   ,CHG_DELT_PATH_CPLS_FLG
   ,CRE_USR_ID
   ,CRE_DT
   ,CRE_OFC_CD
   ,UPD_USR_ID
   ,UPD_DT
   ,UPD_OFC_CD
)
values(
	@[sys_area_grp_id]
   ,@[cntr_no]
   ,@[cntr_cyc_no]
   ,@[dmdt_trf_cd]
   ,@[dmdt_chg_loc_div_cd]
   ,@[chg_seq]
   ,@[chg_ofc_cd]
   ,@[delt_seq]
   ,@[chg_delt_path_cd]
   ,@[chg_delt_path_lvl]
   ,@[chg_delt_sts_cd]
   ,@[chg_delt_path_cpls_flg]
   ,@[cre_usr_id]
   ,nvl(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), sysdate)
   ,@[cre_ofc_cd]
   ,@[upd_usr_id]
   ,nvl(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), sysdate)
   ,@[upd_ofc_cd]
)			]]></sql>
			<params>
				<param name="sys_area_grp_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
				<param name="chg_ofc_cd" type="12" value="" out="N"/>
				<param name="delt_seq" type="12" value="" out="N"/>
				<param name="chg_delt_path_cd" type="12" value="" out="N"/>
				<param name="chg_delt_path_lvl" type="12" value="" out="N"/>
				<param name="chg_delt_sts_cd" type="12" value="" out="N"/>
				<param name="chg_delt_path_cpls_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
