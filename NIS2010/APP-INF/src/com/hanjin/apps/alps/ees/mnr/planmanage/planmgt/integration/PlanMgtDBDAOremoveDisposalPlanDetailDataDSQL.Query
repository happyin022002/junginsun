<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PlanMgtDBDAOremoveDisposalPlanDetailDataDSQL">
			<desc><![CDATA[Disposal Planning Detail 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MNR_PLN_DTL A
     WHERE A.MNR_PLN_SEQ = @[mnr_pln_seq]
       AND A.CTRL_OFC_CD = @[ctrl_ofc_cd]
       AND A.EQ_TPSZ_CD  = @[eq_tpsz_cd]			]]></sql>
			<params>
				<param name="mnr_pln_seq" type="12" value="" out="N"/>
				<param name="ctrl_ofc_cd" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
