<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FQAResultMgtDBDAOsearchFQAResultListAllDataRSQL">
			<desc><![CDATA[search all]]></desc>
			<sql><![CDATA[
SELECT
@[vndr_seq] AS VNDR_SEQ,
@[yd_cd] AS YD_CD,
@[ofc_cd] AS OFC_CD,
C.FLD_AUD_DT,
C.FLD_AUD_DTL_SEQ,
B.MNR_CD_DESC EV_DESC,
B.MNR_CD_DFLT_PNT_NO MAX_PNT_NO,
C.PNT_NO,
C.PNT_CALC_FLG,
C.FLD_AUD_RMK,
NVL(C.FILE_SEQ,'0') FILE_SEQ,
C.CRE_USR_ID,
C.CRE_DT,
C.UPD_USR_ID,
C.UPD_DT
FROM
(
SELECT
TRIM(A.VNDR_SEQ) VNDR_SEQ,
@[yd_cd] AS YD_CD,
A.OFC_CD,
TO_CHAR(A.FLD_AUD_DT,'YYYY-MM-DD') FLD_AUD_DT,
TRIM(A.FLD_AUD_DTL_SEQ) FLD_AUD_DTL_SEQ,
A.EV_DESC,
TRIM(A.MAX_PNT_NO) MAX_PNT_NO,
TRIM(A.PNT_NO) PNT_NO,
DECODE(A.PNT_CALC_FLG,NULL,'N',A.PNT_CALC_FLG)  PNT_CALC_FLG,
A.FLD_AUD_RMK,
A.FILE_SEQ,
A.CRE_USR_ID,
TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.CRE_DT, @[ofc_cd]), 'yyyy-mm-dd') CRE_DT,
A.UPD_USR_ID,
TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.UPD_DT, @[ofc_cd]), 'yyyy-mm-dd') UPD_DT
FROM MNR_FLD_QLTY_AUD_RSLT A
WHERE  
A.OFC_CD = @[ofc_cd]
AND TO_CHAR(A.FLD_AUD_DT,'YYYY-MM-DD') = @[fld_aud_dt]
AND A.VNDR_SEQ = TO_NUMBER(@[vndr_seq])
#if (${gubn} == '2') 
AND A.YD_CD = @[yd_cd]
#end 
)C, MNR_GEN_CD B
WHERE
C.EV_DESC(+) = B.MNR_CD_DESC
AND B.PRNT_CD_ID = 'CD00003'
ORDER BY B.MNR_CD_DP_SEQ			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="1" out="N"/>
				<param name="yd_cd" type="12" value="DD" out="N"/>
				<param name="ofc_cd" type="12" value="DD" out="N"/>
				<param name="fld_aud_dt" type="12" value="20091020" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
