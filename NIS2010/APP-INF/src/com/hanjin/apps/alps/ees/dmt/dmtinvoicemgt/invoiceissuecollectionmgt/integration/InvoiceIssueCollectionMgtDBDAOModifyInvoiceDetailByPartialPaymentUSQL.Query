<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyInvoiceDetailByPartialPaymentUSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
UPDATE DMT_INV_DTL
SET	SYS_AREA_GRP_ID	= @[svr_id]
,	CNTR_NO			= @[cntr_no]
,	CNTR_CYC_NO 	= @[cntr_cyc_no]
,	DMDT_TRF_CD 	= @[dmdt_trf_cd]
,	DMDT_CHG_LOC_DIV_CD = @[dmdt_chg_loc_div_cd]
,	CHG_SEQ			= @[chg_seq]
,	FM_MVMT_DT		= TO_DATE(@[fm_mvmt_dt],'YYYYMMDDHH24MI')
,	TO_MVMT_DT		= TO_DATE(@[to_mvmt_dt],'YYYYMMDDHH24MI')
,	FT_DYS			= @[ft_dys]
,	FT_CMNC_DT		= TO_DATE(@[ft_cmnc_dt],'YYYYMMDDHH24MI')
,	FT_END_DT		= TO_DATE(@[ft_end_dt],'YYYYMMDDHH24MI')
,	FX_FT_OVR_DYS 	= @[fx_ft_ovr_dys]
,	ORG_CHG_AMT		= @[org_chg_amt]
,	SC_RFA_EXPT_AMT = @[sc_rfa_expt_amt]
,	AFT_EXPT_DC_AMT = @[aft_expt_dc_amt]
,	BIL_AMT			= @[bil_amt]
,	INV_PRT_FLG		= @[inv_prt_flg]
,	UPD_USR_ID		= @[upd_usr_id]
,	UPD_DT			= NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
,	UPD_OFC_CD		= @[upd_ofc_cd]
WHERE	DMDT_INV_NO = @[dmdt_inv_no]
AND		CRE_OFC_CD	= @[cre_ofc_cd]
AND		INV_DTL_SEQ = @[inv_dtl_seq]			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="2" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="2" value="" out="N"/>
				<param name="fm_mvmt_dt" type="12" value="" out="N"/>
				<param name="to_mvmt_dt" type="12" value="" out="N"/>
				<param name="ft_dys" type="2" value="" out="N"/>
				<param name="ft_cmnc_dt" type="12" value="" out="N"/>
				<param name="ft_end_dt" type="12" value="" out="N"/>
				<param name="fx_ft_ovr_dys" type="2" value="" out="N"/>
				<param name="org_chg_amt" type="6" value="" out="N"/>
				<param name="sc_rfa_expt_amt" type="6" value="" out="N"/>
				<param name="aft_expt_dc_amt" type="6" value="" out="N"/>
				<param name="bil_amt" type="6" value="" out="N"/>
				<param name="inv_prt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="inv_dtl_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
