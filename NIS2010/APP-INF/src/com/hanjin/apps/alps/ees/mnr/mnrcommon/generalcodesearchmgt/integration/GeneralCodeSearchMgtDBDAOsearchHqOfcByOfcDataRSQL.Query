<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchHqOfcByOfcDataRSQL">
			<desc><![CDATA[searchHqOfcByOfcData]]></desc>
			<sql><![CDATA[
SELECT AR_HD_QTR_OFC_CD AS HQ_OFC 
FROM MDM_ORGANIZATION 
WHERE OFC_CD = @[ofc_cd]

			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
