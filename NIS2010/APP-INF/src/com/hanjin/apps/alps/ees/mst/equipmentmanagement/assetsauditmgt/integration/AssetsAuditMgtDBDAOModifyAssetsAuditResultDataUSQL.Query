<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AssetsAuditMgtDBDAOModifyAssetsAuditResultDataUSQL">
			<desc><![CDATA[ModifyAssetsAuditResultData]]></desc>
			<sql><![CDATA[
UPDATE MST_EQ_ASET_AUD_RSLT
SET
   DIFF_RMK = @[diff_rmk]
WHERE 1 = 1
AND JB_EXE_YRMON = REPLACE(@[yr_mon], '-','')
AND VER_NO       = @[ver_no]
AND EQ_KND_CD    = @[eq_type]
AND EQ_NO        = @[eq_no]			]]></sql>
			<params>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="yr_mon" type="12" value="" out="N"/>
				<param name="ver_no" type="12" value="" out="N"/>
				<param name="eq_type" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
