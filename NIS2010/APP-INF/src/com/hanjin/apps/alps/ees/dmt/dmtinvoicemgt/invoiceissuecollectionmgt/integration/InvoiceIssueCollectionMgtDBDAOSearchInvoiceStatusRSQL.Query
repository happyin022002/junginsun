<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchInvoiceStatusRSQL">
			<desc><![CDATA[Invoice Status  조회]]></desc>
			<sql><![CDATA[
SELECT DMDT_INV_STS_CD 
FROM DMT_INV_MN
WHERE DMDT_INV_NO 	= @[dmdt_inv_no]
AND CRE_OFC_CD 		= @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
