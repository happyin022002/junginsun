<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EqInterchangeDBDAOEqInterchangeRequestDataUSQL">
			<desc><![CDATA[EQ Interchange DAta Update   ]]></desc>
			<sql><![CDATA[
UPDATE LSE_EQ_ITCHG_RQST  SET 
  LSTM_CD            = @[lstm_cd],        
  LR_VNDR_SEQ        = @[vndr_seq],
  FM_LOC_CD          = @[loc_fm],  
  CNTR_TPSZ_CD       = @[tpsz_cd], 
  LSE_LOC_GRP_CD     = @[loc_grp], 
  TO_LOC_CD          = @[loc_to],  
  FM_COST_AMT        = Decode(@[lstm_cd],'OF',@[por_cost],@[sbo_cost]),
  TO_COST_AMT        = decode(@[lstm_cd],'OF',@[del_cost],@[sbi_cost]),
  LSE_ITCHG_RQST_QTY = @[rqst_qty],
  LSE_FREE_DYS       = @[free_day],
  PKUP_UT_AMT        = Decode(@[lstm_cd],'OF',@[puc_amt],@[gto_amt]), 
  ADD_TTL_COST_AMT =   @[add_cost],
  PKUP_CR_AMT        = @[pcr_credit],
  TTL_SAV_AMT        = @[ttl_sav], 
  LSE_ITCHG_AUTH_NO  = @[auth_no], 
  LSE_ITCHG_AUTH_SEQ = @[auth_seq], 
  UPD_USR_ID         = @[upd_usr_id],
  UPD_DT             = SYSDATE
 WHERE LSE_ITCHG_RQST_NO = @[req_no]
   AND LSE_ITCHG_RQST_SEQ = @[req_seq]
			]]></sql>
			<params>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="loc_fm" type="12" value="" out="N"/>
				<param name="tpsz_cd" type="12" value="" out="N"/>
				<param name="loc_grp" type="12" value="" out="N"/>
				<param name="loc_to" type="12" value="" out="N"/>
				<param name="por_cost" type="12" value="" out="N"/>
				<param name="sbo_cost" type="12" value="" out="N"/>
				<param name="del_cost" type="12" value="" out="N"/>
				<param name="sbi_cost" type="12" value="" out="N"/>
				<param name="rqst_qty" type="12" value="" out="N"/>
				<param name="free_day" type="12" value="" out="N"/>
				<param name="puc_amt" type="12" value="" out="N"/>
				<param name="gto_amt" type="12" value="" out="N"/>
				<param name="add_cost" type="12" value="" out="N"/>
				<param name="pcr_credit" type="12" value="" out="N"/>
				<param name="ttl_sav" type="12" value="" out="N"/>
				<param name="auth_no" type="12" value="" out="N"/>
				<param name="auth_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="req_no" type="12" value="" out="N"/>
				<param name="req_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
