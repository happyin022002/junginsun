<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongstayingUnclaimEQFlaggingDBDAOSrchCondByOfcCdRSQL">
			<desc><![CDATA[로긴 OFC_CD의 본부코드]]></desc>
			<sql><![CDATA[
SELECT O.AR_HD_QTR_OFC_CD
  FROM MDM_ORGANIZATION O
 WHERE 1= 1
   AND O.DELT_FLG = 'N'
   AND O.OFC_CD = @[ofc_cd]   -- 로긴 OFFICE CODE			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
