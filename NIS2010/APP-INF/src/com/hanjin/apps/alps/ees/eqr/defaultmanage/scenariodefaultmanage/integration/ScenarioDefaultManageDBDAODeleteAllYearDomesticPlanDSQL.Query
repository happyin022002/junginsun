<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAODeleteAllYearDomesticPlanDSQL">
			<desc><![CDATA[eqr_dmst_pln 테이블 데이터 삭제(특정년도 데이터)]]></desc>
			<sql><![CDATA[
DELETE FROM 
	EQR_DMST_PLN
WHERE	
	SUBSTR(pln_yrwk,1,4) = @[pln_yrwk]			]]></sql>
			<params>
				<param name="pln_yrwk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
