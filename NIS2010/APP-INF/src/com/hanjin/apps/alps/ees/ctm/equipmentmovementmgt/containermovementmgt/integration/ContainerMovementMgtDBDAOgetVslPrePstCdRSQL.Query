<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOgetVslPrePstCdRSQL">
			<desc><![CDATA[VD의 다음 상태가 T일때 다음배를 찾아 옴]]></desc>
			<sql><![CDATA[
SELECT vsl_pre_pst_cd
  FROM bkg_vvd
 WHERE bkg_no = @[bkg_no] AND vsl_pre_pst_cd = 'U'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
