<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOaddOnhireApprovalNumberCSQL">
			<desc><![CDATA[LSE_ONH_APRO를 생성한다.
2013.08.13 채창호 [CHM-201325598]Split 02-EQR T/F 관련 LEGACY 연계 방안 및 일정
reqno 추가]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_ONH_APRO (
	CNTR_ONH_AUTH_NO
,	AGMT_CTY_CD
,	AGMT_SEQ
,	LSTM_CD
,	ONH_LOC_CD
,	PKUP_DUE_DT
,	MFT_YR
,	FREE_DYS
,	PKUP_CHG_AMT
,	PKUP_CHG_CR_AMT
,	MIN_ONH_DYS
,	APRO_RMK
,   DELT_FLG
,	CRE_USR_ID
,	CRE_DT
,   UPD_USR_ID
,   UPD_DT
,   LSE_RQST_NO
,   ONH_ORD_YR
,   LOC_CD
,   ECC_CD
)VALUES(
    @[cntr_onh_auth_no] 
,	@[agmt_cty_cd]
,	@[agmt_seq]
,	@[lstm_cd]
,	@[onh_loc_cd]
,	TO_DATE(@[pkup_due_dt],'YYYY-MM-DD')
,	@[mft_yr]
,	@[free_dys]
,	@[pkup_chg_amt]
,	@[pkup_chg_cr_amt]
,	@[min_onh_dys]
,	@[apro_rmk]
,   'N'
,	@[cre_usr_id]
,	SYSDATE
,	@[upd_usr_id]
,	SYSDATE
,   @[reqno]
,   @[onh_ord_yr]
,   @[loc_cod]
,   @[ecc_cd]
)			]]></sql>
			<params>
				<param name="cntr_onh_auth_no" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="onh_loc_cd" type="12" value="" out="N"/>
				<param name="pkup_due_dt" type="12" value="" out="N"/>
				<param name="mft_yr" type="12" value="" out="N"/>
				<param name="free_dys" type="12" value="" out="N"/>
				<param name="pkup_chg_amt" type="12" value="" out="N"/>
				<param name="pkup_chg_cr_amt" type="12" value="" out="N"/>
				<param name="min_onh_dys" type="12" value="" out="N"/>
				<param name="apro_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="reqno" type="12" value="" out="N"/>
				<param name="onh_ord_yr" type="12" value="" out="N"/>
				<param name="loc_cod" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
