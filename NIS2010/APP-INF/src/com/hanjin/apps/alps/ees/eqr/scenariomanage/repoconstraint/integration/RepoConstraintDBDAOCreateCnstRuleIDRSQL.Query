<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepoConstraintDBDAOCreateCnstRuleIDRSQL">
			<desc><![CDATA[eqr_scnr_repo_cnst 테이블 에서 const rule id 최대값 생성]]></desc>
			<sql><![CDATA[
SELECT 
    NVL(MAX(TO_NUMBER(SUBSTR(cnst_rule_id,3,4)))+1,1) cnst_rule_id 
FROM 
    EQR_SCNR_REPO_CNST 	 
WHERE 
    SUBSTR(cnst_rule_id,0,2) = @[nationCode]
    AND repo_cnst_tp_cd = @[cnstTypeCode]
    AND scnr_id = @[scnrId]			]]></sql>
			<params>
				<param name="nationCode" type="12" value="" out="N"/>
				<param name="cnstTypeCode" type="12" value="" out="N"/>
				<param name="scnrId" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
