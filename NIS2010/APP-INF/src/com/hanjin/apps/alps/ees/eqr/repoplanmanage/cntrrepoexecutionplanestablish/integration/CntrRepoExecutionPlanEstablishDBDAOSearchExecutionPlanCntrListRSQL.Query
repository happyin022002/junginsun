<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOSearchExecutionPlanCntrListRSQL">
			<desc><![CDATA[EQR_EXE_PLN_CNTR &  BKG_BKG_CNTR 테이블에서  컨테이너-MOVEMENT정보 조회

- 20100510 SQL QUERY 수정, 신용찬 수석(신범철 확인)
1. CNMV_YR, CNMV_ID_NO --> CM.CNMV_YR, CM.CNMV_SEQ, CM.CNMV_SPLIT_NO 으로 변경
2. XPKCTM_MOVEMENT --> XUK1CTM_MOVEMENT 으로 변경]]></desc>
			<sql><![CDATA[
SELECT 
    MC.CNTR_NO,                                                               
    MC.CNTR_TPSZ_CD,                                                          
    MC.LSTM_CD,                                                                 
    CM.MVMT_STS_CD , --CNMV_STS_CD,      	                                                     
    MC.VNDR_ABBR_NM ,                                                           
    MC.CNTR_USE_CO_CD ,                                                         
    MC.DMG_FLG,                                                                 
    MC.CNTR_HNGR_RCK_CD, -- CNTR_HNGR_RCK_FLG,                                                       
    MC.CNTR_HNGR_BAR_ATCH_KNT, -- CNTR_HNGR_BAR_FLG,                                                       
    MC.RFUB_FLG,                                                                
    MC.DISP_FLG,                                                                
    MC.PLST_FLR_FLG ,                                                           
    MC.IMDT_EXT_FLG,                                                            
    MC.RF_TP_CD_C ,                                                             
    MC.RF_TP_CD_H                                                               
FROM  
    CTM_MOVEMENT  CM,                                                       
	( 
	    with cntr_info as 
	        (                                                            
	        SELECT 
                MC.CNTR_NO,                                                       
                MC.CNTR_TPSZ_CD,                                                         
                MC.LSTM_CD,                                                              
                MC.CNMV_STS_CD,      	                                                   
                MV.VNDR_ABBR_NM ,                                                        
                DECODE(MC.CNTR_USE_CO_CD,'H','SML','SEN') CNTR_USE_CO_CD ,               
                MC.DMG_FLG,                                                              
                MC.CNTR_HNGR_RCK_CD,                                                    
                MC.CNTR_HNGR_BAR_ATCH_KNT,                                                    
                MC.RFUB_FLG,                                                             
                MC.DISP_FLG,                                                             
                MC.PLST_FLR_FLG ,                                                        
                MC.IMDT_EXT_FLG,                                                         
                DECODE(MC.RF_TP_CD, 'C', 'Y') RF_TP_CD_C ,                               
                DECODE(MC.RF_TP_CD, 'H', 'Y') RF_TP_CD_H ,                               
                MC.CRNT_YD_CD                                  						   
            FROM 
                MST_CONTAINER MC
                , MDM_VENDOR MV                                       
--    		    	WHERE MC.VNDR_CNT_CD = MV.VNDR_CNT_CD                                      
-- VENDOR CODE Join 시 CNT_CD 제외하고 SEQ만 하게 함. - by.JH Kwon
            WHERE 
                MC.VNDR_SEQ = MV.VNDR_SEQ                                            
            #if(${trans_mode} != 'F')  --Off-Hire의 경우 INACTIVE 한 것도 가능하게 함.
                AND MC.ACIAC_DIV_CD <> 'I'                       --Active한 것만 가져온다 
            #end

--ref_id가 있는경우 EQR_EXE_PLN_CNTR에서, 없는 경우 MST_CONTAINER 테이블에서 데이터 가져온다.
            #if(${ref_check} != '')
                #if(${bkgno} == '')
                AND MC.CNTR_NO IN (  							
                    SELECT DISTINCT CNTR_NO 	
		            FROM EQR_EXE_PLN_CNTR  	
	                WHERE REF_ID = @[ref_check]  			
 		        ) 							
                #else
	            AND MC.CNTR_NO IN (                           
                    SELECT DISTINCT CNTR_NO   
                    FROM BKG_CONTAINER			
                    WHERE BKG_NO = @[bkgno] 			
                        --AND   BKG_NO_SPLIT =  
	            )    						
                #end
            #else
                #if(${locType} == '')
                    #if(${userLcc} != '')
	    	    AND MC.LCC_CD = @[userLcc]                   --로그인한 user가 속한 LCC만 기준으로 한다.
                    #end
                #end
	            AND MC.CNTR_NO NOT IN 
                (   -- 동일한 repo_id 에 중복되는 cntr_no가 없게 함.
	                SELECT DISTINCT CNTR_NO 
	                FROM EQR_EXE_PLN_CNTR  
                    WHERE PLN_YRWK = @[pln_yrwk]  
                	AND NVL(FM_ECC_CD,'XXXXX') = @[fm_ecc]  
                	AND NVL(TO_ECC_CD,'XXXXX') = @[to_ecc]  
                	AND TRSP_MOD_CD = DECODE(@[trans_mode] ,'S','X',@[trans_mode]) 
                ) 	--trans_mode = 'S'는 ecc_internal임. ECC Internal은 cntr 제한 없이 가능하게 함. 	 
                -- LOC Type
        		-- RCC
            	#if(${locType} == 'R')
            		 	AND MC.RCC_CD IN( ${arrStrLocation} )  
            	#end    	
            	
            	-- LCC
            	#if(${locType} == 'L')
            		 	AND MC.LCC_CD IN( ${arrStrLocation} )
            	#end    	
            	-- ECC
            	#if(${locType} == 'E')
            		 	AND MC.SCC_CD IN
                       ( 
										SELECT DISTINCT SCC_CD 
										FROM MDM_EQ_ORZ_CHT 
										WHERE ECC_CD IN ( ${arrStrLocation} )
						)  
            	#end    	
            	#if(${tpszs} != '')
            	--Type Size
            		 	AND MC.CNTR_TPSZ_CD IN( ${arrStrTpsz} )   
            	#end    	
            	#if(${lease} != '')
            	--Lease Term
            		 	AND MC.LSTM_CD IN( ${arrStrLease} )
            	#end
            	
            	#if(${dm} == 'Y')
            	--DM
            		 	AND MC.DMG_FLG = 'Y'  
            	#end
            	#if(${hr} == 'Y')
            	--HR
            		 	AND MC.CNTR_HNGR_RCK_CD = 'O'     
            	#end
            	#if(${hb} == 'Y')
            	--HB
            		 	AND MC.CNTR_HNGR_BAR_ATCH_KNT > 0 -- = 'Y'  	   
            	#end
            	#if(${rb} == 'Y')
            	--RB
            		 	AND MC.RFUB_FLG = 'Y'  	   
            	#end
            	#if(${dp} == 'Y')
            	--DP
            		 	AND MC.DISP_FLG = 'Y'  
            	#end
            	#if(${pf} == 'Y') 
            	--PF
            		 	AND MC.PLST_FLR_FLG = 'Y'  	   
            	#end
            	#if(${im} == 'Y')
            	--IM
            		 	AND MC.IMDT_EXT_FLG = 'Y'
            	#end
            	#if(${ma} == 'Y')
            	--MA
            		 	AND MC.RF_TP_CD = 'C'
            	#end
            	#if(${hm} == 'Y') 
            	--HM
            		 	AND MC.RF_TP_CD = 'H'
            	#end
            	
        		#if(${yard} != '')
        		--Yard
        			 	AND MC.CRNT_YD_CD  IN( ${arrStrYard} )
        		#end
        		
        		#if(${agmt_no} != '')
        		-- AGMT_NO
        			 	AND MC.AGMT_CTY_CD||MC.AGMT_SEQ  IN( ${arrStrAgmtNo} )   
        		#end
        		 
            #end

	        )  -- width end                                                                          
	        SELECT 
	            CNTR_NO,                                                             
                CNTR_TPSZ_CD,                                                            
                LSTM_CD,                                                                 
                CNMV_STS_CD,      	                                                     
                VNDR_ABBR_NM ,                                                           
                CNTR_USE_CO_CD ,                                                         
                DMG_FLG,                                                                 
                CNTR_HNGR_RCK_CD,                                                       
                CNTR_HNGR_BAR_ATCH_KNT,                                                       
                RFUB_FLG,                                                                
                DISP_FLG,                                                                
                PLST_FLR_FLG ,                                                           
                IMDT_EXT_FLG,                                                            
                RF_TP_CD_C ,                                                             
                RF_TP_CD_H ,                                                             
                CRNT_YD_CD                                                               
	        FROM 
	            cntr_info
    ) MC                                                          
WHERE CM.CNTR_NO = MC.CNTR_NO                                                    
-- CNMV_YR, CNMV_ID_NO --> CM.CNMV_YR, CM.CNMV_SEQ, CM.CNMV_SPLIT_NO 으로 변경,  20100510 신용찬 수정(신범철 확인)                                                
--AND (CM.CNMV_YR, CM.CNMV_ID_NO) = (       
AND (CM.CNMV_YR, CM.CNMV_SEQ, CM.CNMV_SPLIT_NO) = 
								(                                               
                					--SELECT /*+index_desc(a XPKCTM_MOVEMENT) */ CNMV_YR, CNMV_ID_NO 
                                    -- 20100510 신용찬 수정(신범철 확인)
                                    -- XPKCTM_MOVEMENT --> XUK1CTM_MOVEMENT 으로 변경   
                                    -- CNMV_YR, CNMV_ID_NO --> CNMV_YR, CNMV_SEQ, CNMV_SPLIT_NO 으로 변경                                        
               						SELECT /*+index_desc(a XUK1CTM_MOVEMENT) */ CNMV_YR, CNMV_SEQ, CNMV_SPLIT_NO                                    
        							FROM CTM_MOVEMENT A                                            
        							WHERE A.CNTR_NO = MC.CNTR_NO                                     
        							AND ROWNUM = 1
        						)                
	#if(${move} != '')                                 
    --Movement Status
        AND CM.MVMT_STS_CD IN( ${arrStrMove} )   
	#end			]]></sql>
			<params>
				<param name="ref_check" type="12" value="" out="N"/>
				<param name="bkgno" type="12" value="" out="N"/>
				<param name="userLcc" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="fm_ecc" type="12" value="" out="N"/>
				<param name="to_ecc" type="12" value="" out="N"/>
				<param name="trans_mode" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
