<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VslResidualSpaceManageDBDAOSearchVslRsdlSpaceVvdRSQL">
			<desc><![CDATA[Vessel R.Capa. [ EES_EQR_0060 ]
EQR_SCNR_VSL_RSDL_CAPA 테이블의 특정 vvd의 vsl space 정보 조회]]></desc>
			<sql><![CDATA[
SELECT	DISTINCT VSL_SPC
		,'VVD' AS GUBUN
FROM	EQR_SCNR_VSL_RSDL_CAPA
WHERE	VSL_LANE_CD = @[vsl_lane_cd]
	AND VSL_CD		= @[vsl_cd]
	AND	SKD_VOY_NO	= @[skd_voy_no]
	AND SKD_DIR_CD	= @[skd_dir_cd]
	AND ROWNUM		= 1			]]></sql>
			<params>
				<param name="vsl_lane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
