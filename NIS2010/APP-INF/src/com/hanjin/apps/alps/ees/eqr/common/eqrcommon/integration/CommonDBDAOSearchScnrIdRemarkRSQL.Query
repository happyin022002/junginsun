<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchScnrIdRemarkRSQL">
			<desc><![CDATA[SCNR_ID 기준으로 해서 REMARK 정보와 기타 정보를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT              						
    CASE WHEN REPO_PLN_CRE_FLG = 'Y' or REPO_PLN_DTRB_FLG ='Y' or SCNR_AUTO_GEN_FLG ='Y'  THEN 
        'FALSE'                			          
    ELSE 
        'TRUE'							                 
    END  STATUS,        						
    SCNR_RMK        							
FROM 
    EQR_SCNR_MST  						
WHERE 
    SCNR_ID =@[scnr_id]   								
			]]></sql>
			<params>
				<param name="scnr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
