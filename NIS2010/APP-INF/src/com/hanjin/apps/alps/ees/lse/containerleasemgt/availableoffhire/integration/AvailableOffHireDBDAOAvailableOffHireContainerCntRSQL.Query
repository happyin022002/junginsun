<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AvailableOffHireDBDAOAvailableOffHireContainerCntRSQL">
			<desc><![CDATA[LSO 메뉴얼 작업으로 인한 비정상 data 삭제를 위한 임시쿼리]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM   LSE_AVAL_OFFH
WHERE  1=1
AND    CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
