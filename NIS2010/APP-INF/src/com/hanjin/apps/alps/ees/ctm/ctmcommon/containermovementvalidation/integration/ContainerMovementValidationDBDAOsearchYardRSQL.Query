<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOsearchYardRSQL">
			<desc><![CDATA[Yard가 존재하는지 Check]]></desc>
			<sql><![CDATA[
SELECT YD_CD
  FROM MDM_YARD
#if (${p_yard1} != '')
 WHERE YD_CD = @[p_yard1]
   AND DELT_FLG = 'N'
#end			]]></sql>
			<params>
				<param name="p_yard1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
