<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSupplyDemandPlanDBDAOMergeProcurementPlanDataUSQL">
			<desc><![CDATA[MergeProcurementPlanData]]></desc>
			<sql><![CDATA[
MERGE INTO MST_CNTR_PROCU_PLN A
USING DUAL
ON (
    A.PLN_YR = @[pln_yr]
AND A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
AND A.CNTR_PROCU_PLN_CD = @[cntr_procu_pln_cd]
)
WHEN MATCHED THEN
UPDATE SET  CNTR_JAN_QTY = @[n1],
            CNTR_FEB_QTY = @[n2],
            CNTR_MAR_QTY = @[n3],
            CNTR_APR_QTY = @[n4],
            CNTR_MAY_QTY = @[n5],
            CNTR_JUN_QTY = @[n6],
            CNTR_JUL_QTY = @[n7],
            CNTR_AUG_QTY = @[n8],
            CNTR_SEP_QTY = @[n9],
            CNTR_OCT_QTY = @[n10],
            CNTR_NOV_QTY = @[n11],
            CNTR_DEC_QTY = @[n12],
            UPD_USR_ID   = @[upd_usr_id],
            UPD_DT       = SYSDATE
WHEN NOT MATCHED THEN
INSERT (    PLN_YR, CNTR_TPSZ_CD, CNTR_PROCU_PLN_CD,
            CNTR_JAN_QTY ,
            CNTR_FEB_QTY ,
            CNTR_MAR_QTY ,
            CNTR_APR_QTY ,
            CNTR_MAY_QTY ,
            CNTR_JUN_QTY ,
            CNTR_JUL_QTY ,
            CNTR_AUG_QTY ,
            CNTR_SEP_QTY ,
            CNTR_OCT_QTY ,
            CNTR_NOV_QTY ,
            CNTR_DEC_QTY ,                        
            CRE_USR_ID   ,
            CRE_DT       ,
            UPD_USR_ID   ,
            UPD_DT
       ) VALUES (      
            @[pln_yr],@[cntr_tpsz_cd],@[cntr_procu_pln_cd],
            @[n1],@[n2],@[n3],@[n4],@[n5],@[n6],
            @[n7],@[n8],@[n9],@[n10],@[n11],@[n12],
            @[cre_usr_id],SYSDATE,
            @[upd_usr_id],SYSDATE            
       )			]]></sql>
			<params>
				<param name="pln_yr" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_procu_pln_cd" type="12" value="" out="N"/>
				<param name="n1" type="12" value="" out="N"/>
				<param name="n2" type="12" value="" out="N"/>
				<param name="n3" type="12" value="" out="N"/>
				<param name="n4" type="12" value="" out="N"/>
				<param name="n5" type="12" value="" out="N"/>
				<param name="n6" type="12" value="" out="N"/>
				<param name="n7" type="12" value="" out="N"/>
				<param name="n8" type="12" value="" out="N"/>
				<param name="n9" type="12" value="" out="N"/>
				<param name="n10" type="12" value="" out="N"/>
				<param name="n11" type="12" value="" out="N"/>
				<param name="n12" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
