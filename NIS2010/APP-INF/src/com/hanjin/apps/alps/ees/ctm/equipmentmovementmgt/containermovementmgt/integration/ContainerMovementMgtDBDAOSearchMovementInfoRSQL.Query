<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchMovementInfoRSQL">
			<desc><![CDATA[Movement row 단위 조회.]]></desc>
			<sql><![CDATA[
SELECT NVL(DECODE(FCNTR_FLG, 'Y', 'F', 'N', 'M', FCNTR_FLG), 'N') AS FCNTR_FLG,
  DECODE(OB_CNTR_FLG, 'Y', 'O', 'N', 'I', OB_CNTR_FLG) AS OB_CNTR_FLG,
  VNDR_SEQ,
  CNMV_RMK,
  USR_NM,
  WBL_NO,
  PKUP_NO,
  CNMV_CO_CD,
  CNMV_SPLIT_NO,
  MVMT_STS_CD,
  BKG_NO,
  BL_NO,
  CNMV_CYC_NO,
  BKG_CGO_TP_CD,
  DEST_YD_CD,
  INP_YD_CD,
  ORG_YD_CD,
  CHSS_NO,
  MGST_NO,
  CRNT_VSL_CD || CRNT_SKD_VOY_NO || CRNT_SKD_DIR_CD AS CNTR_ID,
  CNTR_SEAL_NO,
  CNTR_DMG_FLG,
  CNTR_DISP_FLG,
  IMDT_EXT_FLG,
  CNTR_XCH_CD,
  SPCL_CGO_FLG,
  TO_CHAR (CNMV_EVNT_DT, 'YYYYMMDDHH24MI') AS CNMV_EVNT_DT,
  MVMT_TRSP_MOD_CD,
  MVMT_EDI_MSG_TP_ID,
  MVMT_CRE_TP_CD,
  OFC_CD,
  CNT_CD
FROM CTM_MOVEMENT
WHERE CNTR_NO = @[cntr_no]
  AND CNMV_YR = @[cnmv_yr]
  AND CNMV_ID_NO = @[cnmv_id_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
