<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOsearchEccListDataRSQL">
			<desc><![CDATA[LCC에 따른 ECC List를 조회해온다.[ECC를 SCC가져오는 걸로 변경]]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
      A.SCC_CD ECC_CD
    --  A.ECC_CD
    , A.LCC_CD
FROM  MDM_EQ_ORZ_CHT A
WHERE DELT_FLG = 'N'
#if (${scc_cd} != '') 
AND A.SCC_CD = @[scc_cd]
#end
#if (${lcc_cd} != '') 
AND A.LCC_CD = @[lcc_cd]
#end
#if (${rcc_cd} != '') 
AND A.RCC_CD = @[rcc_cd]
#end
#if (${ecc_cd} != '') 
AND A.ECC_CD = @[ecc_cd]
#end			]]></sql>
			<params>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
