<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmValidationDBDAOsearchChssPoolDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.CHSS_POOL_CD
      ,A.CHSS_POOL_NM
      ,A.POOL_MGMT_CO_CD 
      ,A.POOL_MGMT_CO_NM
      ,A.CRE_USR_ID
      ,A.CRE_DT
      ,A.UPD_USR_ID
      ,A.UPD_DT
FROM CGM_CHSS_POOL A
WHERE A.CHSS_POOL_CD = @[chss_pool_cd] 			]]></sql>
			<params>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
