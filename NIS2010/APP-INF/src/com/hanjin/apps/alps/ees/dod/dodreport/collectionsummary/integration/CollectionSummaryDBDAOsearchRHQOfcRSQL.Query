<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CollectionSummaryDBDAOsearchRHQOfcRSQL">
			<desc><![CDATA[Combo Box용 RHQ Offcie 조회]]></desc>
			<sql><![CDATA[
SELECT 
 A.OFC_CD AS OFFICE
FROM 
(
    SELECT    DISTINCT
              DECODE(X.AR_HD_QTR_OFC_CD,'SELIB',1,'TYOIB',2,'HAMRU',3,'NYCRA',4,'SHARC',5,'SINRS',6,'VVOIA',7,9) AS RUM
             ,X.AR_HD_QTR_OFC_CD AS OFC_CD    
    FROM      MDM_ORGANIZATION   X
    WHERE     X.OFC_KND_CD        = '2'
    AND       X.AR_HD_QTR_OFC_CD  IS NOT NULL
    AND       X.DELT_FLG = 'N'
) A
WHERE 1=1
ORDER BY RUM ASC 			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
