<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSupplyDemandPlanDBDAOEqPriceDetailVOUSQL">
			<desc><![CDATA[Container Purchasing Trend by Year & input & update]]></desc>
			<sql><![CDATA[
UPDATE MST_EQ_PUR_PRC SET 
	BSE_YRMON = REPLACE(@[bse_yrmon],'-','')
,	LOC_CD = @[loc_cd]
,	EQ_TPSZ_CD = @[eq_tpsz_cd]
,	VNDR_SEQ = @[vndr_seq]
,	EQ_KND_CD = @[eq_knd_cd]
,	EQ_QTY = @[eq_qty]
,	PUR_PRC = @[pur_prc]
,	PUR_UT_PRC = @[pur_ut_prc]
,	DRYG_AMT = @[dryg_amt]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
,   DIFF_RMK = @[diff_rmk]
WHERE	BSE_YRMON = REPLACE(@[bse_yrmon],'-','')
AND	LOC_CD = @[loc_cd]
AND	EQ_TPSZ_CD = @[eq_tpsz_cd]
AND	VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="bse_yrmon" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_qty" type="2" value="" out="N"/>
				<param name="pur_prc" type="6" value="" out="N"/>
				<param name="pur_ut_prc" type="6" value="" out="N"/>
				<param name="dryg_amt" type="6" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
