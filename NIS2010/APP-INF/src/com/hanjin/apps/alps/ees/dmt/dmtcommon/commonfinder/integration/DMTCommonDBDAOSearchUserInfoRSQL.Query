<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchUserInfoRSQL">
			<desc><![CDATA[사용자의 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT	OFC_N3RD_LVL_CD AS CNG_RHQ_OFC_CD,
        LST_LGIN_OFC_CD AS LST_LGIN_OFC_CD
FROM 	COM_USER T1, DMT_OFC_LVL_V T2
WHERE	1=1
AND     T1.LST_LGIN_OFC_CD = T2.OFC_N8TH_LVL_CD
AND     T1.USR_ID          = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
