<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchRFAExceptionInfoRSQL">
			<desc><![CDATA[RFA Exception Info]]></desc>
			<sql><![CDATA[
SELECT D.ADD_DYS  AS ADD_DAY
	  ,D.TTL_DYS  AS TTL_DAY
	  ,D.XCLD_SAT_FLG  AS EXCL_SAT
	  ,D.XCLD_SUN_FLG  AS EXCL_SUN
	  ,D.XCLD_HOL_FLG  AS EXCL_HOLI
      ,D.RT_USE_FLG    AS RATE_MK
	  ,D.CURR_CD       AS CURR_CD
      ,D.FT_USE_FLG    AS FTIME_MK
      ,D.CVRG_CMB_SEQ  AS CMB_SEQ
 FROM	DMT_RFA_EXPT_TRF_DTL D
WHERE	D.RFA_EXPT_DAR_NO		= @[dar_no]
AND		D.RFA_EXPT_MAPG_SEQ		= @[mapg_seq]
AND		D.RFA_EXPT_VER_SEQ		= @[ver_seq]
AND     D.RFA_RQST_DTL_SEQ		= @[dtl_seq]			]]></sql>
			<params>
				<param name="dar_no" type="12" value="" out="N"/>
				<param name="mapg_seq" type="12" value="" out="N"/>
				<param name="ver_seq" type="12" value="" out="N"/>
				<param name="dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
