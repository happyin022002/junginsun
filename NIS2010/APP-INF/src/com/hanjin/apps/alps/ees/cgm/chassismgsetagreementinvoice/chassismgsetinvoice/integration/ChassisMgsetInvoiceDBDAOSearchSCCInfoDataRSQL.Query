<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOSearchSCCInfoDataRSQL">
			<desc><![CDATA[Sc No. 데이터를 찾는다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SCC_CD AS LOC_CD
FROM(SELECT ML.SCC_CD 
     FROM MDM_LOCATION ML
     WHERE 1 = 1
     AND NVL(ML.DELT_FLG, 'N') <> 'Y'
     )
WHERE 1=1
  AND SCC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
