<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanSensitivityAnalysisDBDAOUpdateSensitivityEccMst_40QueryUSQL">
			<desc><![CDATA[EQR_ECC_MST 테이블에 대한 STV 40feet amount  수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_ECC_MST
       SET    STV_40FT_COST_AMT  = ${curr_cost}          	
             ,UPD_USR_ID         = @[user_id]          	
             ,UPD_DT             = sysdate    			
WHERE ECC_CD = @[fm_loc]  			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="fm_loc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
