<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DropOffCreationDBDAOModifyARInterfaceUSQL">
			<desc><![CDATA[AR Interface 전송을 완료하고 DOD_DRP_OFF_CHG 테이블의 AR_IF_NO컬럼값을 업데이트한다.]]></desc>
			<sql><![CDATA[
UPDATE DOD_DRP_OFF_CHG
   SET AR_IF_NO = @[ar_if_no]
 WHERE 1 = 1
   AND BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]
   AND DRP_OFF_CHG_SEQ = @[drp_off_chg_seq]
   AND INV_SRC_NO = @[inv_src_no]
			]]></sql>
			<params>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="drp_off_chg_seq" type="12" value="" out="N"/>
				<param name="inv_src_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
