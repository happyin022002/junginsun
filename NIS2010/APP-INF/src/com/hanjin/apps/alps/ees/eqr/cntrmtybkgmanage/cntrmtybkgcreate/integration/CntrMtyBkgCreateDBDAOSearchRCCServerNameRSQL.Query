<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrMtyBkgCreateDBDAOSearchRCCServerNameRSQL">
			<desc><![CDATA[COM_INTG_CD_DTL 테이블에서 특정 RCC 의 해당 서버네임 검색]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_DESC SEVERNAME
FROM COM_INTG_CD_DTL
WHERE INTG_CD_ID = 'CD00949'
AND	  INTG_CD_VAL_CTNT = @[rcc_name]			]]></sql>
			<params>
				<param name="rcc_name" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
