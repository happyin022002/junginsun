<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EqInterchangeDBDAOEqInterchangeAuthNoMakeRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(@[loc_cd],3,3) || @[lstm_cd] ||
 TO_CHAR(SYSDATE,'YYYYMMDD') || '-' || SUBSTR(TO_CHAR(( COUNT(*) + 1 ), '000'),2,3) AUTH_NO
FROM LSE_EQ_ITCHG
WHERE 1=1
AND TO_CHAR(CRE_DT,'YYYYMMDD') = TO_CHAR(SYSDATE,'YYYYMMDD')


			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
