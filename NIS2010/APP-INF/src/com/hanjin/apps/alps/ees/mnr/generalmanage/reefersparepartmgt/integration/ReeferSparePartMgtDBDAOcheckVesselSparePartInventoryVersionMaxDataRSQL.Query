<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReeferSparePartMgtDBDAOcheckVesselSparePartInventoryVersionMaxDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT NVL((
            SELECT MAX(SPR_PRT_INVT_VER_SEQ) AS SPR_PRT_INVT_VER_SEQ
            FROM MNR_VSL_SPR_PRT_INVT
            WHERE SPR_PRT_INVT_NO = @[spr_prt_invt_no]
            ),0) AS SPR_PRT_INVT_VER_SEQ
FROM DUAL 			]]></sql>
			<params>
				<param name="spr_prt_invt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
