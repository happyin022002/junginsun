<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnsiteInspectionResultMgtDBDAOInsertSurveyItemsCSQL">
			<desc><![CDATA[사용자가 새로 작성한 설문 조사 항목들을 입력]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_ONSITE_INSP_RSLT_ITM
(
EV_ITM_SEQ, 
EV_ITM_NM, 
EV_ITM_ORD_NO, 
EG_ITM_FLG, 
DELT_FLG, 
CRE_USR_ID, 
CRE_DT, 
UPD_USR_ID, 
UPD_DT
)
SELECT NVL(MAX(EV_ITM_SEQ), 0)+1, 
NVL(@[ev_itm_nm], ' '), 
LPAD(@[seq], 4, '0'), 
'N', 
'N', 
@[usr_id], 
SYSDATE,  
@[usr_id], 
SYSDATE
FROM MNR_ONSITE_INSP_RSLT_ITM			]]></sql>
			<params>
				<param name="ev_itm_nm" type="12" value="" out="N"/>
				<param name="seq" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
