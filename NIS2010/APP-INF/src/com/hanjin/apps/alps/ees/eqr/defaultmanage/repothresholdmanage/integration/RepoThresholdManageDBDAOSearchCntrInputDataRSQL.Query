<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepoThresholdManageDBDAOSearchCntrInputDataRSQL">
			<desc><![CDATA[eqr_inp_dat_red_lgt_alt 테이블의 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT   		
	MAX(RLA_TP_CD)   as ALT_ITM_DIV_CD									
    ,MAX(TYPE2)	     as CNTR_SZ_CD
    ,TYPE1_DESC	     as RCC_DESC													
    ,MAX(TYPE2)	     as CNTR_SZ_CD_DISPLAY											
    ,MAX(USNYC_COST) as ALT_COST_AMT_USNYC	 												
    ,MAX(USNYC_BSE)  as ALT_BSE_CD_USNYC	 												
    ,MAX(DEHAM_COST) as ALT_COST_AMT_DEHAM	 													
    ,MAX(DEHAM_BSE)  as ALT_BSE_CD_DEHAM	 												
    ,MAX(SGSIN_COST) as ALT_COST_AMT_SGSIN	 												 	
    ,MAX(SGSIN_BSE)  as ALT_BSE_CD_SGSIN	 												
    ,MAX(KRSEL_COST) as ALT_COST_AMT_KRSEL	  													
    ,MAX(KRSEL_BSE)  as ALT_BSE_CD_KRSEL	 												
    ,MAX(CNHKG_COST) as ALT_COST_AMT_CNHKG	  													
    ,MAX(CNHKG_BSE)  as ALT_BSE_CD_CNHKG	 													
    ,MAX(CNSHA_COST) as ALT_COST_AMT_CNSHA	 													
    ,MAX(CNSHA_BSE)  as ALT_BSE_CD_CNSHA	 												
    ,MAX(JPTYO_COST) as ALT_COST_AMT_JPTYO	 													
    ,MAX(JPTYO_BSE)	 as ALT_BSE_CD_JPTYO 												
    ,MAX(TWTPE_COST) as ALT_COST_AMT_TWTPE 													
    ,MAX(TWTPE_BSE)  as ALT_BSE_CD_TWTPE												
    ,MAX(TYPE1NUM)   																		
FROM 
    (																	
        SELECT 
            RCC    			 																							
            ,TYPE1_DESC																								
            ,TYPE1_CODE RLA_TP_CD									
            ,TYPE2 																								
            ,MAX(DECODE(RCC, 'USNYC', cost_amt))  USNYC_COST		
            ,MAX(DECODE(RCC, 'USNYC', bse_cd))  USNYC_BSE 		
            ,MAX(DECODE(RCC, 'DEHAM', cost_amt))  DEHAM_COST 		   
            ,MAX(DECODE(RCC, 'DEHAM', bse_cd))  DEHAM_BSE 			
            ,MAX(DECODE(RCC, 'SGSIN', cost_amt))  SGSIN_COST 	 	 
            ,MAX(DECODE(RCC, 'SGSIN', bse_cd))  SGSIN_BSE 		
            ,MAX(DECODE(RCC, 'KRSEL', cost_amt))  KRSEL_COST 	 	
            ,MAX(DECODE(RCC, 'KRSEL', bse_cd))  KRSEL_BSE 			
            ,MAX(DECODE(RCC, 'CNHKG', cost_amt))  CNHKG_COST 	 	 
            ,MAX(DECODE(RCC, 'CNHKG', bse_cd))  CNHKG_BSE 			
            ,MAX(DECODE(RCC, 'CNSHA', cost_amt))  CNSHA_COST 		 
            ,MAX(DECODE(RCC, 'CNSHA', bse_cd))  CNSHA_BSE 			
            ,MAX(DECODE(RCC, 'JPTYO', cost_amt))  JPTYO_COST 		 
            ,MAX(DECODE(RCC, 'JPTYO', bse_cd))  JPTYO_BSE 		
            ,MAX(DECODE(RCC, 'TWTPE', cost_amt))  TWTPE_COST 			   
            ,MAX(DECODE(RCC, 'TWTPE', bse_cd))  TWTPE_BSE 			
            ,RCCNUM    											                                                              
            ,TYPE1NUM  											 
            ,TYPE2NUM   											                                                            
        FROM    													                                                                      
            (  														
                SELECT 
                    A.RCC RCC    									                                                      
                    ,A.TYPE1_CODE TYPE1_CODE  						                                        
                    ,A.TYPE1_DESC TYPE1_DESC			 				                                           
                    ,A.TYPE2 TYPE2		 							
                    ,B.cntr_sz_cd cntr_cd	  							                                                   
                    ,B.alt_cost_amt cost_amt 							
                    ,B.alt_bse_cd bse_cd   							                                            
                    ,A.RCCNUM       									                                                    
                    ,A.TYPE1NUM    									                                                     
                    ,A.TYPE2NUM   									
                FROM               									                                                       
                    (  													
                        SELECT 
                            A.intg_cd_val_dp_desc RCC    				                                  
                            ,B.intg_cd_val_ctnt    TYPE1_CODE 			                              
                            ,B.intg_cd_val_dp_desc TYPE1_DESC  			                             
                            ,C.PARAM               TYPE2    				                                
                            ,A.intg_cd_val_dp_seq  RCCNUM   				                                
                            ,B.NUM                 TYPE1NUM  			                               
                            ,C.NUM                 TYPE2NUM   			                              
                        FROM 
                            com_intg_cd_dtl A,     						                                          
                            (                         						                                            
                                SELECT 
                                    intg_cd_val_ctnt  						                                        
                                    ,intg_cd_val_dp_desc  						                                      
                                    ,intg_cd_val_dp_seq NUM   					                                      
                                FROM 
                                    com_intg_cd_dtl         					                                      
                                WHERE 
                                    intg_cd_id = @[intg_cd_id_1] 							                                     
                                ORDER BY 
                                    intg_cd_val_dp_seq  						                                     
                            ) B, 												
                            (													
                                    SELECT '20' param, 1 num FROM dual				
                                UNION ALL										
                                    SELECT '40' param, 2 num FROM dual				
                                UNION ALL										
                                    SELECT '45' param, 3 num FROM dual				
                            ) C 												
                        WHERE 
                            A.intg_cd_id = @[intg_cd_id_2]		 							                                       
                        ORDER BY 
                            A.intg_cd_val_dp_seq  							                                      
                            ,B.num            								                                           
                            ,B.intg_cd_val_ctnt 							 
                            ,C.num        									                                              
                    ) A,													
                    eqr_inp_dat_red_lgt_alt B    							  
                WHERE 
                    A.RCC = B.RCC_CD  								                                        
        		   	AND   A.TYPE1_CODE = B.alt_itm_div_cd						
        		 	AND   A.TYPE2 = B.cntr_sz_cd 								
            )															
        GROUP BY 
            RCC 													
            ,TYPE1_CODE  											                                                        
            ,TYPE1_DESC  											                                                        
            ,TYPE2      											                                                         
            ,RCCNUM    											                                                          
            ,TYPE1NUM   											                                                         
            ,TYPE2NUM   											   
        ORDER BY 
            RCCNUM												
            ,TYPE1NUM												
            ,TYPE2NUM												
    )																
GROUP BY 
    RLA_TP_CD													
    ,TYPE1_DESC													
    ,TYPE2														
    ,TYPE1NUM   																					
ORDER BY 
    TYPE1NUM			]]></sql>
			<params>
				<param name="intg_cd_id_1" type="12" value="" out="N"/>
				<param name="intg_cd_id_2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
