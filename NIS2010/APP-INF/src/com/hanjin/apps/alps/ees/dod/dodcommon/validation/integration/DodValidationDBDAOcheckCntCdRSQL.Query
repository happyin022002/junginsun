<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodValidationDBDAOcheckCntCdRSQL">
			<desc><![CDATA[Country Code Validation]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM MDM_COUNTRY
 WHERE CNT_CD = @[s_value]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="s_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
