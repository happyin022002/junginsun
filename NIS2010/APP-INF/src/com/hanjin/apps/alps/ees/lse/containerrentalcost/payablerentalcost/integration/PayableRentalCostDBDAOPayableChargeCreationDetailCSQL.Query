<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOPayableChargeCreationDetailCSQL">
			<desc><![CDATA[Payable Charge Creation Detail Data Insert]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_PAY_RNTL_CHG_DTL (
         CHG_SEQ
       , CNTR_NO
       , LSE_PAY_CHG_TP_CD
       , DTL_SEQ
       , AGMT_CTY_CD                             --05
       , AGMT_SEQ
       , CNTR_TPSZ_CD
       , ONH_DT
       , OFFH_DT
       , ONH_LOC_CD                              --10
       , OFFH_LOC_CD
       , CHG_FREE_DYS
       , PD_RT_AMT
       , TTL_COST_AMT
       , CR_AMT                                  --15
       , CNTR_AUD_STS_CD
       , COST_DYS
       , BIL_DYS
       , DSCR_RT_AMT
       , DSCR_COST_AMT                           --20
       , CRE_USR_ID
       , CRE_DT
       , UPD_USR_ID
       , UPD_DT
       , ACCT_CD                                 --25
       , COST_CD
)
SELECT   @[chg_seq]
       , @[cntr_no]
       , T21.LSE_PAY_CHG_TP_CD
       , NVL(MAX(T23.DTL_SEQ), 0) + 1
       , T21.AGMT_CTY_CD                        --05
       , T21.AGMT_SEQ
       , @[cntr_tpsz_cd]
       , TO_DATE(@[onh_dt] ,'YYYYMMDD')
       , TO_DATE(@[offh_dt],'YYYYMMDD')
       , @[onh_loc_cd]                          --10
       , @[offh_loc_cd]
       , @[chg_free_dys]
       , @[pd_rt_amt]
       , @[ttl_cost_amt]
       , 0                                      --15
       , 'H'
       , @[cost_dys]
       , @[bil_dys]
       , 0
       , 0                                      --20
       , @[cre_usr_id]
       , SYSDATE
       , @[upd_usr_id]
       , SYSDATE
       , NVL(T21.ACCT_CD, T22.ACCT_CD)          --25
       , NVL(T21.COST_CD, T22.COST_CD)
FROM     (
           SELECT   T11.AGMT_CTY_CD
                  , T11.AGMT_SEQ
                  , T11.LSE_PAY_CHG_TP_CD
                  , T11.LSTM_CD
                  , T12.ACCT_CD
                  , T12.COST_CD
           FROM     (
                      SELECT   @[agmt_cty_cd]          AS AGMT_CTY_CD
                             , @[agmt_seq]             AS AGMT_SEQ
                             , @[lse_pay_chg_tp_cd]    AS LSE_PAY_CHG_TP_CD
                             , LSTM_CD
                      FROM     LSE_AGREEMENT
                      WHERE    1 = 1
                      AND      AGMT_CTY_CD          = @[agmt_cty_cd]
                      AND      AGMT_SEQ             = @[agmt_seq]
                    ) T11
                  , (
                      SELECT   LSE_RCV_CHG_TP_CD
                             , ACCT_CD
                             , LSTM_CD
                             , COST_CD
                      FROM     LSE_RNTL_COST_ACCT_ORD
                      WHERE    1 = 1
                      AND      LSTM_CD = 'XX'
                      UNION ALL
                      SELECT   LSE_RCV_CHG_TP_CD
                             , ACCT_CD
                             , 'BX'
                             , COST_CD
                      FROM     LSE_RNTL_COST_ACCT_ORD
                      WHERE    1 = 1
                      AND      LSTM_CD = 'XX'
                      UNION ALL
                      SELECT   LSE_RCV_CHG_TP_CD
                             , ACCT_CD
                             , LSTM_CD
                             , COST_CD
                      FROM     (
                                 SELECT   T1.LSE_RCV_CHG_TP_CD
                                        , T1.LSTM_CD    AS ZZZ
                                        , T1.ACCT_CD
                                        , T1.COST_CD
                                        , T2.LSTM_CD
                                 FROM     LSE_RNTL_COST_ACCT_ORD T1
                                        , MST_LSE_TERM           T2
                                 WHERE    1 = 1
                                 AND      T1.LSTM_CD IN('XX', T2.LSTM_CD)
                               )
                      WHERE    1 = 1
                      AND      ( LSE_RCV_CHG_TP_CD, ZZZ, LSTM_CD ) NOT IN
                               (
                                 SELECT   S1.LSE_RCV_CHG_TP_CD
                                        , 'XX'
                                        , S2.LSTM_CD
                                 FROM     LSE_RNTL_COST_ACCT_ORD S1
                                        , MST_LSE_TERM           S2
                                 WHERE    1 = 1
                                 AND      S1.LSTM_CD = S2.LSTM_CD
                               )
                    ) T12
           WHERE    1 = 1
           AND      T11.LSTM_CD              = T12.LSTM_CD          (+)
           AND      T11.LSE_PAY_CHG_TP_CD    = T12.LSE_RCV_CHG_TP_CD(+)
         )                       T21
       , LSE_RNTL_COST_ACCT_ORD  T22
       , LSE_PAY_RNTL_CHG_DTL    T23
WHERE    1 = 1
AND      NVL2(T21.ACCT_CD, T21.LSE_PAY_CHG_TP_CD, 'XXX') = T22.LSE_RCV_CHG_TP_CD (+)
AND      CASE WHEN T21.ACCT_CD IS NULL AND T21.LSTM_CD IN('ST', 'LT') THEN T21.LSTM_CD ELSE 'XX' END = T22.LSTM_CD (+)
AND      T23.CHG_SEQ             (+) = @[chg_seq]
AND      T23.CNTR_NO             (+) = @[cntr_no]
AND      T23.LSE_PAY_CHG_TP_CD   (+) = T21.LSE_PAY_CHG_TP_CD
GROUP BY T21.LSE_PAY_CHG_TP_CD
       , T21.AGMT_CTY_CD
       , T21.AGMT_SEQ
       , NVL(T21.ACCT_CD, T22.ACCT_CD)
       , NVL(T21.COST_CD, T22.COST_CD)			]]></sql>
			<params>
				<param name="chg_seq" type="2" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="onh_dt" type="12" value="" out="N"/>
				<param name="offh_dt" type="12" value="" out="N"/>
				<param name="onh_loc_cd" type="12" value="" out="N"/>
				<param name="offh_loc_cd" type="12" value="" out="N"/>
				<param name="chg_free_dys" type="2" value="" out="N"/>
				<param name="pd_rt_amt" type="6" value="" out="N"/>
				<param name="ttl_cost_amt" type="6" value="" out="N"/>
				<param name="cost_dys" type="2" value="" out="N"/>
				<param name="bil_dys" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="lse_pay_chg_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
