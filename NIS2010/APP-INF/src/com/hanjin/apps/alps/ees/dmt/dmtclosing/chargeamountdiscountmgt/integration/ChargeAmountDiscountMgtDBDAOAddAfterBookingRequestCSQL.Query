<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAOAddAfterBookingRequestCSQL">
			<desc><![CDATA[After Booking 정보를 입력하기 위한 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_AFT_BKG_ADJ_RQST (
	AFT_EXPT_DAR_NO
,	DMDT_EXPT_RQST_STS_CD
,	RQST_USR_ID
,	RQST_OFC_CD
,	RQST_DT
,	APRO_OFC_CD
,	CRE_USR_ID
,	CRE_DT
,	CRE_OFC_CD
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
,	DMDT_EXPT_RQST_RSN_CD
) VALUES (
	@[aft_expt_dar_no]
,	@[dmdt_expt_rqst_sts_cd]
,	@[cre_usr_id]
,	@[cre_ofc_cd]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[apro_ofc_cd]
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[cre_ofc_cd]
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[cre_ofc_cd]
,	@[dmdt_expt_rqst_rsn_cd]
)			]]></sql>
			<params>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
				<param name="dmdt_expt_rqst_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="dmdt_expt_rqst_rsn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
