<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchAfterBookingListByDateUserOfficeRCVRSQL">
			<desc><![CDATA[DEM/DET Adjustment Request & Approval Status 조회(AfterBooking, DATE-Office, Received)용 쿼리]]></desc>
			<sql><![CDATA[
#if((${to_cc} == '' && ${usr_ofc_only} == 'N') || (${to_cc} == 'cc' && ${usr_ofc_only} == 'N'))
(
	SELECT  DISTINCT 'CC' TO_CC
	,	AFT_RQST.AFT_EXPT_DAR_NO AS DAR_NO
    ,   AFT_RQST.AFT_BKG_APRO_NO AS APVL_NO
    ,   CD_DTL.INTG_CD_VAL_DP_DESC AS STATUS

	#if(${group_by} == 'CVRG')
    ,   AFT_RQST_DTL.DMDT_TRF_CD
    ,   CASE
            WHEN AFT_RQST_DTL.DMDT_TRF_CD IN ('DMIF', 'DMOF') THEN SUBSTR(CHG_CALC.FM_MVMT_YD_CD, 1, 5)
            WHEN AFT_RQST_DTL.DMDT_TRF_CD IN ('DTIC', 'CTIC') THEN CHG_CNTR.DEL_CD
            WHEN AFT_RQST_DTL.DMDT_TRF_CD IN ('DTOC', 'CTOC') THEN CHG_CNTR.POR_CD
        END AS COVERAGE
    ,   AFT_RQST_DTL.BKG_NO
    ,   AFT_RQST_DTL.BL_NO
	#end

	,	(
			SELECT	/*+ INDEX_DESC(DMT_AFT_BKG_ADJ_PROG XPKDMT_AFT_BKG_ADJ_PROG) */ PROG_OFC_CD
			FROM	DMT_AFT_BKG_ADJ_PROG
			WHERE	AFT_EXPT_DAR_NO = AFT_RQST.AFT_EXPT_DAR_NO
				AND DMDT_EXPT_RQST_STS_CD = 'R'
				AND ROWNUM = 1
		) AS REQ_OFC_CD
	,	(
			SELECT	/*+ INDEX_DESC(DMT_AFT_BKG_ADJ_PROG XPKDMT_AFT_BKG_ADJ_PROG) */ PROG_OFC_CD
			FROM	DMT_AFT_BKG_ADJ_PROG
			WHERE	AFT_EXPT_DAR_NO = AFT_RQST.AFT_EXPT_DAR_NO
				AND DMDT_EXPT_RQST_STS_CD IN ('A', 'C', 'J', 'O')
				AND AFT_RQST.DMDT_EXPT_RQST_STS_CD NOT IN ('C', 'R')
				AND ROWNUM = 1
		) AS APRO_OFC_CD
        ,	(
        	SELECT APRO_OFC_CD
			FROM DMT_AFT_BKG_APRO_PATH C
			WHERE 1=1
			AND C.AFT_EXPT_DAR_NO = AFT_RQST.AFT_EXPT_DAR_NO
            AND C.AFT_BKG_PATH_LVL = ( SELECT MAX(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH
                                       WHERE AFT_EXPT_DAR_NO = C.AFT_EXPT_DAR_NO
                                       AND AFT_BKG_PATH_CPLS_FLG = 'Y' )
            AND ROWNUM = 1
            ) AS FINAL_OFC_CD
    ,   TO_CHAR(AFT_RQST.UPD_DT, 'YYYY-MM-DD') AS UPD_DT
    ,   CHG_CNTR.SC_NO
    ,   CHG_CNTR.RFA_NO
	,	DECODE(NVL(CHG_CNTR.RFA_NO, ''), '', PRI_SP_CUST.CUST_CD, PRI_RP_CUST.CUST_CD) AS CUST_CD
	,	DECODE(NVL(CHG_CNTR.RFA_NO, ''), '', PRI_SP_CUST.CUST_NM, PRI_RP_CUST.CUST_NM) AS CUST_NM
		
	FROM	DMT_AFT_BKG_ADJ_RQST AFT_RQST
		,	DMT_AFT_BKG_ADJ_RQST_DTL AFT_RQST_DTL
    	,   DMT_CHG_BKG_CNTR CHG_CNTR
	    ,   DMT_CHG_CALC CHG_CALC
		,	(
				SELECT	HDR.RFA_NO
					, 	MN.CTRT_CUST_CNT_CD CUST_CNT_CD
					, 	MN.CTRT_CUST_SEQ CUST_SEQ
					,	MN.CTRT_CUST_CNT_CD || LPAD(MN.CTRT_CUST_SEQ, 6, '0') CUST_CD
					, 	CUST.CUST_LGL_ENG_NM CUST_NM
				FROM	PRI_RP_HDR HDR, PRI_RP_MN MN, MDM_CUSTOMER CUST
				WHERE	HDR.PROP_NO 	= MN.PROP_NO
					AND	MN.AMDT_SEQ 	= 
						(	
							SELECT 	/*+ INDEX_DESC(PRI_RP_MN XPKPRI_RP_MN) */AMDT_SEQ
							FROM	PRI_RP_MN
							WHERE	PROP_NO 	= MN.PROP_NO
								AND	ROWNUM 		= 1
						)
					AND MN.CTRT_CUST_CNT_CD 	= CUST.CUST_CNT_CD
					AND MN.CTRT_CUST_SEQ 		= CUST.CUST_SEQ
			) PRI_RP_CUST
		,	(
				SELECT	HDR.SC_NO
					, 	PTY.CUST_CNT_CD
					,	PTY.CUST_SEQ
					,	PTY.CUST_CNT_CD || LPAD(PTY.CUST_SEQ, 6, '0') CUST_CD
					, 	CUST.CUST_LGL_ENG_NM CUST_NM
				FROM	PRI_SP_HDR HDR, PRI_SP_CTRT_PTY PTY, MDM_CUSTOMER CUST
				WHERE	HDR.PROP_NO = PTY.PROP_NO
					AND	PTY.AMDT_SEQ = 
						( 
							SELECT	/*+ INDEX_DESC(PRI_SP_CTRT_PTY XPKPRI_SP_CTRT_PTY) */AMDT_SEQ
							FROM	PRI_SP_CTRT_PTY
							WHERE	PROP_NO 			= PTY.PROP_NO
								AND PRC_CTRT_PTY_TP_CD 	= 'C'
								AND	ROWNUM 				= 1
						)
					AND PTY.PRC_CTRT_PTY_TP_CD 	= 'C'
					AND PTY.CUST_CNT_CD 		= CUST.CUST_CNT_CD
					AND PTY.CUST_SEQ 			= CUST.CUST_SEQ
			) PRI_SP_CUST
		,	COM_INTG_CD_DTL CD_DTL
	WHERE	AFT_RQST.UPD_DT BETWEEN TO_DATE(@[fm_dt], 'YYYY-MM-DD') AND TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
		AND	AFT_RQST.AFT_EXPT_DAR_NO 		= AFT_RQST_DTL.AFT_EXPT_DAR_NO
		AND AFT_RQST_DTL.DMDT_TRF_CD IN (
			#foreach( $trf_cd in ${trf_cd_list} )
				#if($velocityCount < $trf_cd_list.size()) '$trf_cd', #else '$trf_cd' #end
			#end
		)
   		AND AFT_RQST_DTL.BKG_NO 			= CHG_CNTR.BKG_NO(+)
		AND	(
				CHG_CNTR.SYS_AREA_GRP_ID = 
				(
					SELECT  SYS_AREA_GRP_ID
					FROM    COM_SYS_AREA_GRP_ID
					WHERE   CNT_CD = 
							(
								CASE 
									WHEN SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 2, 1) = 'M' AND SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 3, 1) = 'I' 
										THEN SUBSTR(CHG_CNTR.POD_CD, 0, 2)
									WHEN SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 2, 1) = 'M' AND SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 3, 1) = 'O' 
										THEN SUBSTR(CHG_CNTR.POL_CD, 0, 2)
									WHEN SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 2, 1) = 'T' AND SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 3, 1) = 'I' 
										THEN SUBSTR(CHG_CNTR.DEL_CD, 0, 2)
									WHEN SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 2, 1) = 'T' AND SUBSTR(AFT_RQST_DTL.DMDT_TRF_CD, 3, 1) = 'O' 
										THEN SUBSTR(CHG_CNTR.POR_CD, 0, 2)
								END    
							)
						AND CO_IND_CD = 'H'
				)
			OR
				CHG_CNTR.SYS_AREA_GRP_ID IS NULL
			)
		AND CHG_CNTR.SYS_AREA_GRP_ID 		= CHG_CALC.SYS_AREA_GRP_ID(+)
		AND CHG_CNTR.CNTR_NO 				= CHG_CALC.CNTR_NO(+)
		AND CHG_CNTR.CNTR_CYC_NO 			= CHG_CALC.CNTR_CYC_NO(+)
		AND (
				CHG_CALC.DMDT_TRF_CD 		= AFT_RQST_DTL.DMDT_TRF_CD
			OR
				CHG_CALC.DMDT_TRF_CD IS NULL
			)
		AND (
				CHG_CALC.DMDT_CHG_LOC_DIV_CD 	<> 'TSP'
			OR
				CHG_CALC.DMDT_CHG_LOC_DIV_CD IS NULL
			)
		AND CHG_CNTR.SC_NO 					= PRI_SP_CUST.SC_NO(+)
		AND CHG_CNTR.RFA_NO 				= PRI_RP_CUST.RFA_NO(+)

		#if(${is_owned_role} == 'Y')
		AND CHG_CALC.OFC_CD IN
			(
				SELECT	OFC_CD 
				FROM	( 
							SELECT	OFC_CD, PRNT_OFC_CD 
					        FROM	MDM_ORGANIZATION O
			             	WHERE	NVL(O.DELT_FLG, 'N') <> 'Y' 
					    )
			    START WITH OFC_CD 				= @[usr_ofc_cd]
			    CONNECT BY NOCYCLE PRIOR OFC_CD = PRNT_OFC_CD
			)
		#else
		AND CHG_CALC.OFC_CD IN			
		( SELECT OFC_CD FROM MAS_OFC_LVL WHERE TO_CHAR(SYSDATE,'YYYYMM') BETWEEN OFC_APLY_FM_YRMON AND OFC_APLY_TO_YRMON
				  AND OFC_N5TH_LVL_CD IN
			(
				#foreach( $ofc_cd in ${ofc_cd_list} )
					#if($velocityCount < $ofc_cd_list.size()) '$ofc_cd', #else '$ofc_cd' #end
				#end
			)
		 )
		#end

		#if(${cust_cd} != '')
		AND (	PRI_SP_CUST.CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND PRI_SP_CUST.CUST_SEQ = SUBSTR(@[cust_cd], 3)
				OR
				PRI_RP_CUST.CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND PRI_RP_CUST.CUST_SEQ = SUBSTR(@[cust_cd], 3)
			)
		#end

		AND AFT_RQST.DMDT_EXPT_RQST_STS_CD 	= CD_DTL.INTG_CD_VAL_CTNT
		AND CD_DTL.INTG_CD_ID 				= 'CD02069'
)
#end

#if(${to_cc} == '' && ${usr_ofc_only} == 'N')
UNION ALL
#end

#if(${to_cc} == '' || ${to_cc} == 'to')
(
	SELECT  DISTINCT 'TO' TO_CC
	,	AFT_RQST.AFT_EXPT_DAR_NO AS DAR_NO
    ,   AFT_RQST.AFT_BKG_APRO_NO AS APVL_NO
    ,   CD_DTL.INTG_CD_VAL_DP_DESC AS STATUS

	#if(${group_by} == 'CVRG')
    ,   AFT_RQST_DTL.DMDT_TRF_CD
    ,   CASE
            WHEN AFT_RQST_DTL.DMDT_TRF_CD = 'DMIF' THEN SUBSTR(BOOKING.POD_NOD_CD, 1, 5)
			WHEN AFT_RQST_DTL.DMDT_TRF_CD = 'DMOF' THEN SUBSTR(BOOKING.POL_NOD_CD, 1, 5)
            WHEN AFT_RQST_DTL.DMDT_TRF_CD IN ('DTIC', 'CTIC') THEN BOOKING.DEL_CD
            WHEN AFT_RQST_DTL.DMDT_TRF_CD IN ('DTOC', 'CTOC') THEN BOOKING.POR_CD
        END AS COVERAGE
    ,   AFT_RQST_DTL.BKG_NO
    ,   AFT_RQST_DTL.BL_NO
	#end

	,   AFT_PROG.PROG_OFC_CD AS REQ_OFC_CD
	,	DECODE(AFT_RQST.DMDT_EXPT_RQST_STS_CD, 'C', '', AFT_PROG2.PROG_OFC_CD) AS APRO_OFC_CD
        ,	(
        	SELECT APRO_OFC_CD
			FROM DMT_AFT_BKG_APRO_PATH C
			WHERE 1=1
			AND C.AFT_EXPT_DAR_NO = AFT_RQST.AFT_EXPT_DAR_NO
            AND C.AFT_BKG_PATH_LVL = ( SELECT MAX(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH
                                       WHERE AFT_EXPT_DAR_NO = C.AFT_EXPT_DAR_NO
                                       AND AFT_BKG_PATH_CPLS_FLG = 'Y' )
            AND ROWNUM = 1
            ) AS FINAL_OFC_CD
    ,   TO_CHAR(AFT_RQST.UPD_DT, 'YYYY-MM-DD') AS UPD_DT
    ,   BOOKING.SC_NO
    ,   BOOKING.RFA_NO
	,	DECODE(NVL(BOOKING.RFA_NO, ''), '', PRI_SP_CUST.CUST_CD, PRI_RP_CUST.CUST_CD) AS CUST_CD
	,	DECODE(NVL(BOOKING.RFA_NO, ''), '', PRI_SP_CUST.CUST_NM, PRI_RP_CUST.CUST_NM) AS CUST_NM
		
	FROM	DMT_AFT_BKG_ADJ_RQST AFT_RQST
		,	DMT_AFT_BKG_ADJ_PROG AFT_PROG
		,	DMT_AFT_BKG_ADJ_PROG AFT_PROG2
		,	DMT_AFT_BKG_ADJ_RQST_DTL AFT_RQST_DTL
		,	BKG_BOOKING BOOKING
		,	(
				SELECT	HDR.RFA_NO
					, 	MN.CTRT_CUST_CNT_CD CUST_CNT_CD
					, 	MN.CTRT_CUST_SEQ CUST_SEQ
					,	MN.CTRT_CUST_CNT_CD || LPAD(MN.CTRT_CUST_SEQ, 6, '0') CUST_CD
					, 	CUST.CUST_LGL_ENG_NM CUST_NM
				FROM	PRI_RP_HDR HDR, PRI_RP_MN MN, MDM_CUSTOMER CUST
				WHERE	HDR.PROP_NO 	= MN.PROP_NO
					AND	MN.AMDT_SEQ 	= 
						(	
							SELECT 	/*+ INDEX_DESC(PRI_RP_MN XPKPRI_RP_MN) */AMDT_SEQ
							FROM	PRI_RP_MN
							WHERE	PROP_NO 	= MN.PROP_NO
								AND	ROWNUM 		= 1
						)
					AND MN.CTRT_CUST_CNT_CD 	= CUST.CUST_CNT_CD
					AND MN.CTRT_CUST_SEQ 		= CUST.CUST_SEQ
			) PRI_RP_CUST
		,	(
				SELECT	HDR.SC_NO
					, 	PTY.CUST_CNT_CD
					,	PTY.CUST_SEQ
					,	PTY.CUST_CNT_CD || LPAD(PTY.CUST_SEQ, 6, '0') CUST_CD
					, 	CUST.CUST_LGL_ENG_NM CUST_NM
				FROM	PRI_SP_HDR HDR, PRI_SP_CTRT_PTY PTY, MDM_CUSTOMER CUST
				WHERE	HDR.PROP_NO 	= PTY.PROP_NO
					AND	PTY.AMDT_SEQ 	= 
						( 
							SELECT	/*+ INDEX_DESC(PRI_SP_CTRT_PTY XPKPRI_SP_CTRT_PTY) */AMDT_SEQ
		 					FROM	PRI_SP_CTRT_PTY
							WHERE	PROP_NO 			= PTY.PROP_NO
								AND PRC_CTRT_PTY_TP_CD 	= 'C'
								AND	ROWNUM 				= 1
						)
					AND PTY.PRC_CTRT_PTY_TP_CD 	= 'C'
					AND PTY.CUST_CNT_CD 		= CUST.CUST_CNT_CD
					AND PTY.CUST_SEQ 			= CUST.CUST_SEQ
			) PRI_SP_CUST
		,	COM_INTG_CD_DTL CD_DTL
	WHERE	AFT_RQST.AFT_EXPT_DAR_NO 		= AFT_PROG.AFT_EXPT_DAR_NO
		AND	AFT_RQST.AFT_EXPT_DAR_NO 		= AFT_PROG2.AFT_EXPT_DAR_NO
	    AND AFT_RQST.DMDT_EXPT_RQST_STS_CD IN ('A', 'J', 'O')
		AND AFT_RQST.UPD_DT BETWEEN TO_DATE(@[fm_dt], 'YYYY-MM-DD') AND TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
		AND	AFT_PROG.PROG_SEQ 				=
			(
				SELECT	/*+ INDEX_DESC(DMT_AFT_BKG_ADJ_PROG XPKDMT_AFT_BKG_ADJ_PROG) */ PROG_SEQ
				FROM	DMT_AFT_BKG_ADJ_PROG
				WHERE	AFT_EXPT_DAR_NO 		= AFT_PROG.AFT_EXPT_DAR_NO
					AND DMDT_EXPT_RQST_STS_CD 	= 'R'
					AND ROWNUM 					= 1
			)

		#if(${usr_ofc_only} == 'Y')
		AND AFT_PROG.PROG_OFC_CD 			= @[usr_ofc_cd]
		#else
		AND AFT_PROG.PROG_OFC_CD IN 
			(
				#foreach( $ofc_cd in ${ofc_cd_list} )
					#if($velocityCount < $ofc_cd_list.size()) '$ofc_cd', #else '$ofc_cd' #end
				#end
			)
		#end

		AND	AFT_PROG2.PROG_SEQ 				=
			(
				SELECT	/*+ INDEX_DESC(DMT_AFT_BKG_ADJ_PROG XPKDMT_AFT_BKG_ADJ_PROG) */ PROG_SEQ
				FROM	DMT_AFT_BKG_ADJ_PROG
				WHERE	AFT_EXPT_DAR_NO 	= AFT_PROG.AFT_EXPT_DAR_NO
					AND DMDT_EXPT_RQST_STS_CD IN ('A', 'C', 'J', 'O')
					AND	ROWNUM 				= 1	
			)
		AND	AFT_RQST.AFT_EXPT_DAR_NO = AFT_RQST_DTL.AFT_EXPT_DAR_NO
		AND AFT_RQST_DTL.DMDT_TRF_CD IN 
			(
				#foreach( $trf_cd in ${trf_cd_list} )
					#if($velocityCount < $trf_cd_list.size()) '$trf_cd', #else '$trf_cd' #end
				#end
			)
   		AND AFT_RQST_DTL.BKG_NO 			= BOOKING.BKG_NO(+)
		AND BOOKING.SC_NO 					= PRI_SP_CUST.SC_NO(+)
		AND BOOKING.RFA_NO 					= PRI_RP_CUST.RFA_NO(+)

		#if(${cust_cd} != '')
		AND (	PRI_SP_CUST.CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND PRI_SP_CUST.CUST_SEQ = SUBSTR(@[cust_cd], 3)
				OR
				PRI_RP_CUST.CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND PRI_RP_CUST.CUST_SEQ = SUBSTR(@[cust_cd], 3)
			)
		#end

		AND AFT_RQST.DMDT_EXPT_RQST_STS_CD 	= CD_DTL.INTG_CD_VAL_CTNT
		AND CD_DTL.INTG_CD_ID 				= 'CD02069'

	UNION

	SELECT  DISTINCT 'TO' TO_CC
	,	AFT_RQST.AFT_EXPT_DAR_NO AS DAR_NO
    ,   AFT_RQST.AFT_BKG_APRO_NO AS APVL_NO
    ,   CD_DTL.INTG_CD_VAL_DP_DESC AS STATUS

	#if(${group_by} == 'CVRG')
    ,   AFT_RQST_DTL.DMDT_TRF_CD
    ,   CASE
            WHEN AFT_RQST_DTL.DMDT_TRF_CD = 'DMIF' THEN SUBSTR(BOOKING.POD_NOD_CD, 1, 5)
			WHEN AFT_RQST_DTL.DMDT_TRF_CD = 'DMOF' THEN SUBSTR(BOOKING.POL_NOD_CD, 1, 5)
            WHEN AFT_RQST_DTL.DMDT_TRF_CD IN ('DTIC', 'CTIC') THEN BOOKING.DEL_CD
            WHEN AFT_RQST_DTL.DMDT_TRF_CD IN ('DTOC', 'CTOC') THEN BOOKING.POR_CD
        END AS COVERAGE
    ,   AFT_RQST_DTL.BKG_NO
    ,   AFT_RQST_DTL.BL_NO
	#end

	,   AFT_PROG.PROG_OFC_CD AS REQ_OFC_CD
	,	'' AS APRO_OFC_CD
        ,	(
        	SELECT APRO_OFC_CD
			FROM DMT_AFT_BKG_APRO_PATH C
			WHERE 1=1
			AND C.AFT_EXPT_DAR_NO = AFT_RQST.AFT_EXPT_DAR_NO
            AND C.AFT_BKG_PATH_LVL = ( SELECT MAX(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH
                                       WHERE AFT_EXPT_DAR_NO = C.AFT_EXPT_DAR_NO
                                       AND AFT_BKG_PATH_CPLS_FLG = 'Y' )
            AND ROWNUM = 1
            ) AS FINAL_OFC_CD
    ,   TO_CHAR(AFT_RQST.UPD_DT, 'YYYY-MM-DD') AS UPD_DT
    ,   BOOKING.SC_NO
    ,   BOOKING.RFA_NO
	,	DECODE(NVL(BOOKING.RFA_NO, ''), '', PRI_SP_CUST.CUST_CD, PRI_RP_CUST.CUST_CD) AS CUST_CD
	,	DECODE(NVL(BOOKING.RFA_NO, ''), '', PRI_SP_CUST.CUST_NM, PRI_RP_CUST.CUST_NM) AS CUST_NM
		
	FROM	DMT_AFT_BKG_ADJ_RQST AFT_RQST
		,	DMT_AFT_BKG_ADJ_PROG AFT_PROG
		,	DMT_AFT_BKG_ADJ_RQST_DTL AFT_RQST_DTL
		,	BKG_BOOKING BOOKING
		,	(
				SELECT	HDR.RFA_NO
					, 	MN.CTRT_CUST_CNT_CD CUST_CNT_CD
					, 	MN.CTRT_CUST_SEQ CUST_SEQ
					,	MN.CTRT_CUST_CNT_CD || LPAD(MN.CTRT_CUST_SEQ, 6, '0') CUST_CD
					, 	CUST.CUST_LGL_ENG_NM CUST_NM
				FROM	PRI_RP_HDR HDR, PRI_RP_MN MN, MDM_CUSTOMER CUST
				WHERE	HDR.PROP_NO 	= MN.PROP_NO
					AND	MN.AMDT_SEQ 	= 
						(	
							SELECT 	/*+ INDEX_DESC(PRI_RP_MN XPKPRI_RP_MN) */AMDT_SEQ
							FROM	PRI_RP_MN
							WHERE	PROP_NO = MN.PROP_NO
								AND	ROWNUM 	= 1
						)
					AND MN.CTRT_CUST_CNT_CD = CUST.CUST_CNT_CD
					AND MN.CTRT_CUST_SEQ 	= CUST.CUST_SEQ
			) PRI_RP_CUST
		,	(
				SELECT	HDR.SC_NO
					, 	PTY.CUST_CNT_CD
					,	PTY.CUST_SEQ
					,	PTY.CUST_CNT_CD || LPAD(PTY.CUST_SEQ, 6, '0') CUST_CD
					, 	CUST.CUST_LGL_ENG_NM CUST_NM
				FROM	PRI_SP_HDR HDR, PRI_SP_CTRT_PTY PTY, MDM_CUSTOMER CUST
				WHERE	HDR.PROP_NO 	= PTY.PROP_NO
					AND	PTY.AMDT_SEQ 	= 
						( 
							SELECT	/*+ INDEX_DESC(PRI_SP_CTRT_PTY XPKPRI_SP_CTRT_PTY) */AMDT_SEQ
							FROM	PRI_SP_CTRT_PTY
							WHERE	PROP_NO 			= PTY.PROP_NO
								AND PRC_CTRT_PTY_TP_CD 	= 'C'
								AND	ROWNUM 				= 1
						)
					AND PTY.PRC_CTRT_PTY_TP_CD 	= 'C'
					AND PTY.CUST_CNT_CD 		= CUST.CUST_CNT_CD
					AND PTY.CUST_SEQ 			= CUST.CUST_SEQ
			) PRI_SP_CUST
		,	COM_INTG_CD_DTL CD_DTL
	WHERE	AFT_RQST.AFT_EXPT_DAR_NO 		= AFT_PROG.AFT_EXPT_DAR_NO
	    AND AFT_RQST.DMDT_EXPT_RQST_STS_CD 	IN ('R', 'C', 'B', 'P', 'Q')
		AND AFT_RQST.UPD_DT BETWEEN TO_DATE(@[fm_dt], 'YYYY-MM-DD') AND TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999
		AND	AFT_PROG.PROG_SEQ =
			(
				SELECT	/*+ INDEX_DESC(DMT_AFT_BKG_ADJ_PROG XPKDMT_AFT_BKG_ADJ_PROG) */ PROG_SEQ
				FROM	DMT_AFT_BKG_ADJ_PROG
				WHERE	AFT_EXPT_DAR_NO 		= AFT_PROG.AFT_EXPT_DAR_NO
					AND DMDT_EXPT_RQST_STS_CD 	IN ('R', 'C', 'B', 'P', 'Q')
					AND	ROWNUM 					= 1	
			)

		-- 2014.02.26 APPROVAL OFFICE ( SELHO ) 지정 가능
        -- 실제 승인 업무는 SELHO 또는 SELCON에서 가능토록 HARD CODING.
		#if(${usr_ofc_only} == 'Y')
		AND ( ( 
        	SELECT APRO_OFC_CD
			FROM DMT_AFT_BKG_APRO_PATH C
			WHERE 1=1
			AND C.AFT_EXPT_DAR_NO = AFT_RQST.AFT_EXPT_DAR_NO
            AND C.AFT_BKG_PATH_LVL = ( SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH
                                       WHERE AFT_EXPT_DAR_NO = C.AFT_EXPT_DAR_NO
                                       AND DMDT_EXPT_RQST_STS_CD IS NULL
                                       AND AFT_BKG_PATH_CPLS_FLG = 'Y' )
            AND ROWNUM = 1 )
			IN ( DECODE( @[usr_ofc_cd], 'SELCON', 'SELHO', NULL), @[usr_ofc_cd] )
             OR AFT_RQST.RQST_OFC_CD = DECODE(@[login_ofc_cd],'SELCON',AFT_RQST.RQST_OFC_CD,@[login_ofc_cd]) )
		#else
		AND ( ( 
        	SELECT APRO_OFC_CD
			FROM DMT_AFT_BKG_APRO_PATH C
			WHERE 1=1
			AND C.AFT_EXPT_DAR_NO = AFT_RQST.AFT_EXPT_DAR_NO
            AND C.AFT_BKG_PATH_LVL = ( SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH
                                       WHERE AFT_EXPT_DAR_NO = C.AFT_EXPT_DAR_NO
                                       AND DMDT_EXPT_RQST_STS_CD IS NULL
                                       AND AFT_BKG_PATH_CPLS_FLG = 'Y' )
            AND ROWNUM = 1 ) IN
			(
				#foreach( $ofc_cd in ${ofc_cd_list} )
					#if($velocityCount < $ofc_cd_list.size()) '$ofc_cd', #else '$ofc_cd' #end
				#end
				
				#foreach( $ofc_cd in ${ofc_cd_list} )
					#if($ofc_cd == 'SELCON') , 'SELHO' #end
				#end
			) OR AFT_RQST.RQST_OFC_CD = DECODE(@[login_ofc_cd],'SELCON',AFT_RQST.RQST_OFC_CD,@[login_ofc_cd]) )
		#end

		AND	AFT_RQST.AFT_EXPT_DAR_NO 		= AFT_RQST_DTL.AFT_EXPT_DAR_NO
		AND AFT_RQST_DTL.DMDT_TRF_CD IN 
			(
				#foreach( $trf_cd in ${trf_cd_list} )
					#if($velocityCount < $trf_cd_list.size()) '$trf_cd', #else '$trf_cd' #end
				#end
			)
	   	AND AFT_RQST_DTL.BKG_NO 			= BOOKING.BKG_NO(+)
		AND BOOKING.SC_NO 					= PRI_SP_CUST.SC_NO(+)
		AND BOOKING.RFA_NO 					= PRI_RP_CUST.RFA_NO(+)

		#if(${cust_cd} != '')
		AND (	PRI_SP_CUST.CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND PRI_SP_CUST.CUST_SEQ = SUBSTR(@[cust_cd], 3)
				OR
				PRI_RP_CUST.CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND PRI_RP_CUST.CUST_SEQ = SUBSTR(@[cust_cd], 3)
			)
		#end

		AND AFT_RQST.DMDT_EXPT_RQST_STS_CD 	= CD_DTL.INTG_CD_VAL_CTNT
		AND CD_DTL.INTG_CD_ID 				= 'CD01971'
)
#end

ORDER BY TO_CC, UPD_DT, DAR_NO			]]></sql>
			<params>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="login_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
