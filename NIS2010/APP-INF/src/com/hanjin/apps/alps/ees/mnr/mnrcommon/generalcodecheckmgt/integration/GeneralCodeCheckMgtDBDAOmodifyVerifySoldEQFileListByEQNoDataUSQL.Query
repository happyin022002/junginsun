<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyVerifySoldEQFileListByEQNoDataUSQL">
			<desc><![CDATA[수정 - SoldEQFile Verify EQ No]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
   SET A.INP_MSG4 = 'UE'
 WHERE A.TMP_SEQ = @[tmp_seq]
   AND (NVL(A.INP_MSG1,' ') NOT IN (SELECT EQ_NO
                              FROM MNR_EQ_STS
                             WHERE MNR_DISP_FLG = 'N'
                                       AND MNR_DISP_SEL_FLG = 'Y'
                                       --AND EQ_NO NOT IN (SELECT EQ_NO
                                                           --FROM MNR_DISP_DTL)
                                    )
        OR
        A.INP_MSG1 IN (SELECT EQ_NO 
                        FROM MNR_DISP_DTL
                       WHERE EQ_NO = A.INP_MSG1)
        )			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
