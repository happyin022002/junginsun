<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOSearchInactiveReasonRSQL">
			<desc><![CDATA[ChargeCalculationDBDAOSearchInactiveReasonRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
       CASE WHEN @[mn_rsn_cd] IS NULL THEN A.ATTR_CTNT1
                          ELSE A.ATTR_CTNT3 END RSN_CD,  

       CASE WHEN @[mn_rsn_cd] IS NULL THEN A.ATTR_CTNT2
                          ELSE A.ATTR_CTNT4 END RSN_NM
FROM DMT_HRD_CDG_CTNT A
where 1=1
  AND A.HRD_CDG_ID = 'CHG_DELT_RSN_CD'
#if (${mn_rsn_cd} != '' && ${mn_rsn_cd} != 'All')
	 AND	A.ATTR_CTNT1   IN (
			#foreach( $mn_rsn_cd in ${mn_rsn_cd_list} )
				#if($velocityCount < $mn_rsn_cd_list.size()) '$mn_rsn_cd', #else '$mn_rsn_cd' #end
			#end
			)
#end
			]]></sql>
			<params>
				<param name="mn_rsn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
