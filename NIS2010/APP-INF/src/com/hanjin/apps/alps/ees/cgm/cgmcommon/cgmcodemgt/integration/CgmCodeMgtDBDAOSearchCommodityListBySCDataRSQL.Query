<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOSearchCommodityListBySCDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT 
        A.PRC_CMDT_DEF_CD CMDT_CD
    ,   B.CMDT_NM

FROM    (
			SELECT	PRC_CMDT_DEF_CD
			FROM	PRI_SP_SCP_RT_CMDT
			WHERE	1=1
			#if(${prop_no} == '')
				AND PROP_NO = (SELECT DISTINCT PROP_NO
						 				FROM PRI_SP_HDR
										WHERE SC_NO = @[sc_no])
			#else
				AND PROP_NO = @[prop_no]
			#end
			  AND PRC_CMDT_TP_CD = 'C'

			UNION ALL

			SELECT	PRC_CMDT_DEF_CD
			FROM	PRI_SP_SCP_GRP_CMDT_DTL
			WHERE	1=1
			#if(${prop_no} == '')
				AND PROP_NO = (SELECT DISTINCT PROP_NO
						 				FROM PRI_SP_HDR
										WHERE SC_NO = @[sc_no])
			#else
				AND PROP_NO = @[prop_no]
			#end
				AND PRC_CMDT_TP_CD = 'C'
		) A
	, 	MDM_COMMODITY B
            
WHERE A.PRC_CMDT_DEF_CD = B.CMDT_CD
ORDER BY CMDT_CD			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
