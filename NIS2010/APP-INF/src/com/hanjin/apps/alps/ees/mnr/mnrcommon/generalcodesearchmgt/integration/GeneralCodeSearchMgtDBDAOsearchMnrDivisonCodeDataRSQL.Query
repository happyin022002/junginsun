<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchMnrDivisonCodeDataRSQL">
			<desc><![CDATA[Division Code를 조회하는 Operation]]></desc>
			<sql><![CDATA[
SELECT   CD_ID
       , CD_DESC
FROM     (
           SELECT   A.TO_RLT_CD AS CD_ID
                  , A.MNR_RLT_CD_DESC AS CD_DESC
           FROM     MNR_CD_RLT A
           WHERE    1 = 1
           AND      A.EQ_CEDEX_RLT_TP_CD = 'CTV'	  
#if (${searchcon} == 'COMMON')
           AND      A.FM_RLT_CD = @[fm_rlt_cd]
#else
           AND      A.COST_GRP_CD LIKE @[cost_grp_cd] || '%' 
           AND      A.FM_RLT_CD = @[fm_rlt_cd]
#end  
           UNION ALL
           SELECT   ' ' AS CD_ID
                  , ' ' AS CD_DESC
           FROM     DUAL
         )
ORDER BY CD_ID			]]></sql>
			<params>
				<param name="fm_rlt_cd" type="12" value="" out="N"/>
				<param name="cost_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
