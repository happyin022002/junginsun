<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TotalLossMgtDBDAOsearchOfficeAreaUSRSQL">
			<desc><![CDATA[searchOfficeAreaUS]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM MDM_ORGANIZATION A, MDM_ORGANIZATION B
WHERE A.OFC_CD = B.AR_HD_QTR_OFC_CD
AND B.OFC_CD = @[rqst_ofc_cd]
AND A.LOC_CD LIKE 'US%'			]]></sql>
			<params>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
