<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOModifyAuthNoCntrMasterDataUSQL">
			<desc><![CDATA[ModifyAuthNoCntrMasterData]]></desc>
			<sql><![CDATA[
UPDATE  MST_CONTAINER A
SET     A.CNTR_AUTH_NO = @[auth_no],
        A.UPD_USR_ID   = @[upd_usr_id],
        A.UPD_DT       = SYSDATE
WHERE   A.CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="auth_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
