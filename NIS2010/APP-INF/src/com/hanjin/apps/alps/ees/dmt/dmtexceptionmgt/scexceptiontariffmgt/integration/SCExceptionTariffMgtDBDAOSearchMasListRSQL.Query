<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOSearchMasListRSQL">
			<desc><![CDATA[Proposal No. 에 해당되는 모든 Version 과 Status 정보를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT PROP_NO
     , SC_EXPT_VER_SEQ
     , SC_EXPT_MAS_RQST_SEQ
     , CUST_CNT_CD
     , CUST_SEQ
     , BKG_CTRT_TP_CD
     , AFT_BKG_CTRT_NO
     , LOD_QTY
     , CM_AMT
     , CMPB_AMT
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
  FROM DMT_SC_EXPT_MAS_RQST 
 WHERE PROP_NO = @[prop_no]
   AND SC_EXPT_VER_SEQ = @[sc_expt_ver_seq]			]]></sql>
			<params>
				<param name="prop_no" type="12" value="SEL150129" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="3" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
