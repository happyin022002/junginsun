<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchExistMstRfaExptTrfYnRSQL">
			<desc><![CDATA[RFAExceptionTariffMgtDBDAOSearchExistMstRfaExptTrfYnRSQL]]></desc>
			<sql><![CDATA[
SELECT  DECODE(COUNT(1), 0, 'N', 'Y') AS EXIST_YN
  FROM  DMT_RFA_EXPT_TRF
 WHERE  PROP_NO = @[prop_no]
   AND  DMDT_EXPT_RQST_STS_CD = 'A'			]]></sql>
			<params>
				<param name="prop_no" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
