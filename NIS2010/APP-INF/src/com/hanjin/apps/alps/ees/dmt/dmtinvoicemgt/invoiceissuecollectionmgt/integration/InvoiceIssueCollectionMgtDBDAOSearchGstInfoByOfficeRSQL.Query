<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchGstInfoByOfficeRSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOSearchGstInfoByOfficeRSQL]]></desc>
			<sql><![CDATA[
SELECT  A2.IDA_GST_RGST_NO
       ,A2.IDA_PAN_NO
	   ,A4.IDA_STE_CD
	   ,A4.STE_NM 				AS IDA_STE_NM
	   ,A5.ATTR_CTNT3			AS IDA_TAX_CIN
	   ,A5.ATTR_CTNT4			AS IDA_SVC_CATE_RMK

  FROM  MDM_ORGANIZATION  A1
       ,MDM_CUSTOMER      A2
	   ,MDM_LOCATION      A3
	   ,MDM_STATE         A4	
	   ,DMT_HRD_CDG_CTNT  A5
   
 WHERE  A1.OFC_CD          = @[ofc_cd]
   AND  A1.REP_CUST_CNT_CD = A2.CUST_CNT_CD
   AND  A1.REP_CUST_SEQ    = A2.CUST_SEQ
   AND  A1.LOC_CD          = A3.LOC_CD
   AND  A3.STE_CD          = A4.STE_CD
   AND  A1.DELT_FLG        = 'N'
   AND  A2.DELT_FLG        = 'N'
   AND  A3.DELT_FLG        = 'N'						   
   AND  A4.DELT_FLG        = 'N'
   AND  A5.HRD_CDG_ID      = 'IDA_TAX_INFO'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="BOMSC" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
