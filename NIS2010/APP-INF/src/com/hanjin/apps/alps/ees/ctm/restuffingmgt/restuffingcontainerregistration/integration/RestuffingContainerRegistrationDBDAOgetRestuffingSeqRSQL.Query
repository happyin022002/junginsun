<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RestuffingContainerRegistrationDBDAOgetRestuffingSeqRSQL">
			<desc><![CDATA[RestuffingContainerRegistration 생성될 Seq를 구한다]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(CTM_MVMT_XCH, XPKCTM_MVMT_XCH) */
       CNTR_XCH_REF_SEQ + 1 AS MAX_SEQ
  FROM CTM_MVMT_XCH
 WHERE ROWNUM = 1
   AND CNTR_NO = @[cntr_no]
   AND CNMV_ID_NO = @[cnmv_id_no]
   AND CNMV_YR = @[cnmv_yr]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
