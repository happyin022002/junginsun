<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchSheetOptionByOfficeRSQL">
			<desc><![CDATA[sheet option 데이터를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT 
	DC_AMT_FLG
,	OFC_CD
,	BIL_TO_LOC_DIV_CD
,	CUST_REF_PRN_FLG
,	PHN_FAX_PRN_FLG
,	CUST_VAT_PRN_FLG
,	DFLT_TAX_RTO
,	TAX_AMT_PRN_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
FROM DMT_OFC_SH_OPT
WHERE OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
