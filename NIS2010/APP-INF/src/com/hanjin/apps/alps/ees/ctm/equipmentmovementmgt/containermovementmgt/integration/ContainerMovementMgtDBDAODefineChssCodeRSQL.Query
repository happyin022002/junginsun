<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAODefineChssCodeRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT SUBSTR (@[chss_code], 1, 4) || LPAD (SUBSTR (@[chss_code], 5, 10), 6, '0') AS CHSS_CODE
  FROM DUAL			]]></sql>
			<params>
				<param name="chss_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
