<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyVerifyTariffFileListByCostGroupDataUSQL">
			<desc><![CDATA[수정 - Tariff File Verify CostGroup]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
	       SET A.INP_MSG4 = 'CG'
	 WHERE A.TMP_SEQ = @[tmp_seq]
	   AND NVL(A.INP_MSG17,'') NOT IN (SELECT MNR_CD_ID
	 									 FROM MNR_GEN_CD
	 						   			WHERE PRNT_CD_ID = 'CC'
	                                      AND EQ_KND_CD = @[eq_knd_cd])			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
