<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnsiteInspectionResultMgtDBDAOUpdateDetailUSQL">
			<desc><![CDATA[M&R Onsite Inspection Result의 Detail 정보를 업데이트]]></desc>
			<sql><![CDATA[
UPDATE MNR_ONSITE_INSP_RSLT_DTL
SET 
RSLT_GD_FLG = DECODE(@[rslt_gd_flg], 'O', 'Y', ''),
RSLT_NORM_FLG = DECODE(@[rslt_norm_flg], 'O', 'Y', ''),
RSLT_BAD_FLG = DECODE(@[rslt_bad_flg], 'O', 'Y', ''),
FLD_AUD_RMK = @[fld_aud_rmk],
UPD_USR_ID = @[usr_id],
UPD_DT = SYSDATE
WHERE
VNDR_SEQ = @[vndr_seq_for_save]
AND YD_CD = @[yd_cd_for_save]
AND TO_CHAR(FLD_INSP_DT, 'YYYY-MM-DD') = @[fld_insp_dt_for_save]
AND EV_ITM_ORD_NO = @[ev_itm_ord_no]			]]></sql>
			<params>
				<param name="rslt_gd_flg" type="12" value="" out="N"/>
				<param name="rslt_norm_flg" type="12" value="" out="N"/>
				<param name="rslt_bad_flg" type="12" value="" out="N"/>
				<param name="fld_aud_rmk" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="vndr_seq_for_save" type="12" value="" out="N"/>
				<param name="yd_cd_for_save" type="12" value="" out="N"/>
				<param name="fld_insp_dt_for_save" type="12" value="" out="N"/>
				<param name="ev_itm_ord_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
