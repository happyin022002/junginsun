<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAORemoveAfterBookingRsnRmkDSQL">
			<desc><![CDATA[ChargeAmountDiscountMgtDBDAORemoveAfterBookingRsnRmkDSQL]]></desc>
			<sql><![CDATA[
DELETE DMT_AFT_BKG_RSN_RMK_RQST
 WHERE AFT_EXPT_DAR_NO = @[aft_expt_dar_no]
   AND 0 < (
             SELECT COUNT(*)
               FROM DMT_AFT_BKG_RSN_RMK_RQST
              WHERE AFT_EXPT_DAR_NO = @[aft_expt_dar_no]
                AND AFT_BKG_FILE_DIV_CD != @[aft_bkg_file_div_cd] )
			]]></sql>
			<params>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
				<param name="aft_bkg_file_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
