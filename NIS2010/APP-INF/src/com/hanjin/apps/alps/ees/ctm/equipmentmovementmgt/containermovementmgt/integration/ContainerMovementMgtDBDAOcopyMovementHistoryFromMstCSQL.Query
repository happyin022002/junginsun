<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOcopyMovementHistoryFromMstCSQL">
			<desc><![CDATA[Make Movement History]]></desc>
			<sql><![CDATA[
INSERT INTO CTM_MVMT_CORR
            (BKG_CGO_TP_CD,
             BKG_KNT,
             BKG_NO,
             BKG_RCV_TERM_CD,
             BL_NO,
             CHSS_NO,
             CNMV_CO_CD,
             CNMV_STS_CD,
             CNMV_CYC_NO,
             CNMV_EVNT_DT,
             CNMV_ID_NO,
             CNMV_LVL_NO,
             CNMV_RMK,
             CNMV_SEQ,
             CNMV_SPLIT_NO,
             CNMV_YR,
             CNTR_ACT_CD,
             CNTR_DISP_FLG,
             CNTR_DMG_FLG,
             CNTR_HNGR_BAR_ATCH_KNT,
             CNTR_HNGR_RCK_FLG,
             CNTR_NO,
             CNTR_RFUB_FLG,
             CNTR_SEAL_NO,
             SYS_AREA_GRP_ID,
             CNTR_TPSZ_CD,
             CNTR_XCH_CD,
             CRE_DT,
             CRE_USR_ID,
             CTRT_OFC_CTY_CD,
             CTRT_SEQ,
             DELT_DT,
             DELT_USR_ID,
             DEST_YD_CD,
             FCNTR_FLG,
             FDR_SKD_DIR_CD,
             FDR_SKD_VOY_NO,
             FDR_VSL_CD,
             GMT_DT,
             IMDT_EXT_FLG,
             INLND_TRSP_LIC_NO,
             INP_YD_CD,
             LOC_CD,
             MGST_NO,
             MVMT_CRE_TP_CD,
             MVMT_EDI_MSG_AREA_CD,
             MVMT_EDI_MSG_SEQ,
             MVMT_EDI_MSG_TP_ID,
             MVMT_EDI_MSG_YRMONDY,
             MVMT_EDI_TP_CD,
             MVMT_TRSP_MOD_CD,
             OB_CNTR_FLG,
             OFC_CD,
             ORG_YD_CD,
             PKUP_NO,
             SPCL_CGO_FLG,
             SUBST_RULE_CD,
             TRNK_SKD_DIR_CD,
             TRNK_SKD_VOY_NO,
             TRNK_VSL_CD,
             UPD_DT,
             UPD_USR_ID,
             USR_NM,
             VNDR_SEQ,
             WBL_NO,
             CRE_LOCL_DT,
             UPD_LOCL_DT
            )
   (SELECT BKG_CGO_TP_CD,
           BKG_KNT,
           BKG_NO,
           BKG_RCV_TERM_CD,
           BL_NO,
           CHSS_NO,
           CNMV_CO_CD,
           MVMT_STS_CD,
           CNMV_CYC_NO,
           CNMV_EVNT_DT,
           CNMV_ID_NO,
           CNMV_LVL_NO,
           @[cnmv_rmk],
           CNMV_SEQ,
           CNMV_SPLIT_NO,
           CNMV_YR,
           CNTR_ACT_CD,
           CNTR_DISP_FLG,
           CNTR_DMG_FLG,
           CNTR_HNGR_BAR_ATCH_KNT,
           CNTR_HNGR_RCK_FLG,
           CNTR_NO,
           CNTR_RFUB_FLG,
           CNTR_SEAL_NO,
           SYS_AREA_GRP_ID,
           CNTR_TPSZ_CD,
           CNTR_XCH_CD,
           CRE_DT,
           CRE_USR_ID,
           CTRT_OFC_CTY_CD,
           CTRT_SEQ,
           SYSDATE,
           @[upd_usr_id],
           DEST_YD_CD,
           FCNTR_FLG,
           CRNT_SKD_DIR_CD,
           CRNT_SKD_VOY_NO,
           CRNT_VSL_CD,
           GMT_DT,
           IMDT_EXT_FLG,
           INLND_TRSP_LIC_NO,
           INP_YD_CD,
           LOC_CD,
           MGST_NO,
           MVMT_CRE_TP_CD,
           MVMT_EDI_MSG_AREA_CD,
           MVMT_EDI_MSG_SEQ,
           MVMT_EDI_MSG_TP_ID,
           MVMT_EDI_MSG_YRMONDY,
           MVMT_EDI_TP_CD,
           MVMT_TRSP_MOD_CD,
           OB_CNTR_FLG,
           OFC_CD,
           ORG_YD_CD,
           PKUP_NO,
           SPCL_CGO_FLG,
           SUBST_RULE_CD,
           TRNK_SKD_DIR_CD,
           TRNK_SKD_VOY_NO,
           TRNK_VSL_CD,
           UPD_DT,
           UPD_USR_ID,
           USR_NM,
           VNDR_SEQ,
           WBL_NO,
           CRE_LOCL_DT,
           UPD_LOCL_DT
      FROM CTM_MOVEMENT
     WHERE CNTR_NO = @[cntr_no]
       AND CNMV_YR = @[cnmv_yr]
       AND CNMV_ID_NO = @[cnmv_id_no])
			]]></sql>
			<params>
				<param name="cnmv_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
