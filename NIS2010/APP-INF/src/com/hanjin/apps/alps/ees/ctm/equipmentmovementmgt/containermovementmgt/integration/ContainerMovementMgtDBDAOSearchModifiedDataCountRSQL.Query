<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchModifiedDataCountRSQL">
			<desc><![CDATA[마지막 조회 후 수정된 데이터 count 조회.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT
FROM CTM_MVMT_MNL_HIS
WHERE 1 = 1
#if (${cntr_no} != '')
  AND CNTR_NO IN (
  #foreach ($cntr_cd in ${cntr_no_list})
    #if ($velocityCount < $cntr_no_list.size()) '$cntr_cd', #else '$cntr_cd' #end
  #end
                 )
#end
  AND UPD_DT > TO_DATE(@[last_retrieve_date], 'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="last_retrieve_date" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
