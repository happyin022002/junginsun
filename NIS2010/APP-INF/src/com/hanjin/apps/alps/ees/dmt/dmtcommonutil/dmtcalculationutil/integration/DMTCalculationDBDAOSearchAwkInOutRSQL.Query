<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchAwkInOutRSQL">
			<desc><![CDATA[searchAwkInOut]]></desc>
			<sql><![CDATA[
SELECT DECODE (SIGN (OVR_FWRD_LEN
                     + OVR_BKWD_LEN
                     + OVR_HGT
                     + OVR_LF_LEN
                     + OVR_RT_LEN)
              ,0, 'I'
              ,'O'
              ) AWK_IN_OUT
  FROM BKG_AWK_CGO
 WHERE BKG_NO = @[bkg_no]
   AND CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="KORZ1025302" out="N"/>
				<param name="cntr_no" type="12" value="HJCU3204314" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
