<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMovementHistoryDBDAOcheckCHSAtdtDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[

SELECT  to_char( AA.ATCH_DT ,'yyyymmddHH24MIss') AS atch_dt
      , AA.DTCH_YD_CD AS dtch_yd_cd
      , AA.EQ_NO      AS eq_no
 FROM
	(
	SELECT  
		  A.ATCH_DT 
		, A.DTCH_YD_CD 
        , A.EQ_NO
	FROM CGM_EQ_ATCH_DTCH_HIS A
	WHERE A.CNTR_NO = @[cntr_no]
     AND  A.EQ_KND_CD = 'Z'
	ORDER BY A.ATCH_DT DESC
	) AA
WHERE ROWNUM =1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
