<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOMergeEqrHolidayEffectCSQL">
			<desc><![CDATA[EQR_HOLIDAY 테이블에 정보 입력/수정]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_HOLIDAY  A 
    USING (         							
        SELECT  
            @[hol_yr] AS HOL_YR ,      			
            @[cnt_cd] AS CNT_CD ,    			
            @[rcc_div_flg] AS RCC_DIV_FLG ,     		
            @[st_dt] AS ST_DT     			
        FROM DUAL     						
    ) B     							
    ON(     									
        A.HOL_YR = B.HOL_YR 			
        AND A.CNT_CD = B.CNT_CD 			
        AND A.RCC_DIV_FLG = B.RCC_DIV_FLG 	
        AND A.ST_DT = B.ST_DT 				
    )     									
    WHEN NOT MATCHED THEN 					
        INSERT  (  
            HOL_YR,                    			
            CNT_CD,               					
            RCC_DIV_FLG,              				 
            ST_DT,                					
            END_DT,                				
            CNT_NM,                				
            HOL_NM,                				
            HOL_RMK,                										        
            DELT_FLG,                										        
            CRE_USR_ID,                 			
            CRE_DT,                     			
            UPD_USR_ID,                 			
            UPD_DT
        ) VALUES (
            @[hol_yr],
            @[cnt_cd],
            @[rcc_div_flg],
            @[st_dt],
            @[end_dt],
            @[cnt_nm],
            @[hol_nm],
            @[hol_rmk],
            @[delt_flg],
            @[user_id],
            SYSDATE,
            @[user_id],
            SYSDATE
        )			]]></sql>
			<params>
				<param name="hol_yr" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rcc_div_flg" type="12" value="" out="N"/>
				<param name="st_dt" type="12" value="" out="N"/>
				<param name="end_dt" type="12" value="" out="N"/>
				<param name="cnt_nm" type="12" value="" out="N"/>
				<param name="hol_nm" type="12" value="" out="N"/>
				<param name="hol_rmk" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
