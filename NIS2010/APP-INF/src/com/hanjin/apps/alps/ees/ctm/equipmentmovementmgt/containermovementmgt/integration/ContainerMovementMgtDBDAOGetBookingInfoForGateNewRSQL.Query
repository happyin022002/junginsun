<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetBookingInfoForGateNewRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL (BKG_STS_CD, '') AS BKG_STS,
       NVL (SPLIT_FLG, '') AS BKG_SPLIT_IND,
       NVL (TO_BKG_NO, '') AS BKG_COM_NO,
       LENGTH (NVL (@[bkg_number], '')) AS BKG_LENGTH,
       NVL (BKG_CRE_TP_CD, '') AS BKG_CRE_TP_CD
  FROM BKG_BOOKING
 WHERE BKG_NO = @[bkg_number]
			]]></sql>
			<params>
				<param name="bkg_number" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
