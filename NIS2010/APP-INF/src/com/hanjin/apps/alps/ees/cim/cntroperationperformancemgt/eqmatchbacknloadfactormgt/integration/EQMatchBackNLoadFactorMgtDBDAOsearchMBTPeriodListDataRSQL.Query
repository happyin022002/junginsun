<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EQMatchBackNLoadFactorMgtDBDAOsearchMBTPeriodListDataRSQL">
			<desc><![CDATA[searchMBTPeriodListData]]></desc>
			<sql><![CDATA[
#if (${period} == 'M') 
SELECT DAY_UNIT DAY_UNIT, ROWNUM DAY_SEQ
	FROM (SELECT DISTINCT PLN_YR || '-' || PLN_MON DAY_UNIT
			FROM EQR_WK_PRD
			WHERE PLN_YR || PLN_MON || '01' >= @[from_dt]
            	AND PLN_YR || PLN_MON || SUBSTR(@[to_dt], 7, 2) <= @[to_dt]
			ORDER BY PLN_YR || '-' || PLN_MON)
#else
SELECT PLN_YR || '-' ||PLN_WK DAY_UNIT, ROWNUM DAY_SEQ
  FROM EQR_WK_PRD
 WHERE PLN_YR || PLN_WK BETWEEN REPLACE(@[froms], '-', '') AND REPLACE(@[tos], '-', '')
 ORDER BY PLN_YR, PLN_WK
#end			]]></sql>
			<params>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="froms" type="12" value="" out="N"/>
				<param name="tos" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
