<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmEdiSendDBDAOAddCgmChssExptEdiHisCSQL">
			<desc><![CDATA[Chss EID 발송 정보 저장]]></desc>
			<sql><![CDATA[
INSERT INTO  CGM_CHSS_EXPT_EDI_HIS
(
    BKG_NO
   ,CNTR_NO
   ,EDI_HIS_SEQ
   ,EDI_ISS_DT
   ,EDI_ISS_USR_ID
   ,CHSS_EXPT_FLG
   ,CHSS_FT_DYS
   ,FULL_MTY_CD
   ,IO_BND_CD
   ,VSL_CD
   ,SKD_VOY_NO
   ,SKD_DIR_CD
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT
)
VALUES 
(
    @[bkgnbr]
   ,@[cntr_no]
   ,(SELECT NVL(MAX(EDI_HIS_SEQ),0)+1 FROM CGM_CHSS_EXPT_EDI_HIS WHERE BKG_NO = @[bkgnbr] AND CNTR_NO=@[cntr_no])
   ,SYSDATE 
   ,'SYSTEM'
   ,@[chz_except]
   ,@[chz_except_days]
   ,@[fmInd]
   ,@[ie_ind]
   ,@[vsl_cd]
   ,@[skd_voy]
   ,@[skd_dir]
   ,'SYSTEM'
   ,SYSDATE
   ,'SYSTEM'
   ,SYSDATE
)			]]></sql>
			<params>
				<param name="bkgnbr" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="chz_except" type="12" value="" out="N"/>
				<param name="chz_except_days" type="12" value="" out="N"/>
				<param name="fmInd" type="12" value="" out="N"/>
				<param name="ie_ind" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy" type="12" value="" out="N"/>
				<param name="skd_dir" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
