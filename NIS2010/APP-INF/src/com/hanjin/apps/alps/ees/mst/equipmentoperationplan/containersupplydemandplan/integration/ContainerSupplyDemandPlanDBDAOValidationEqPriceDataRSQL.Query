<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSupplyDemandPlanDBDAOValidationEqPriceDataRSQL">
			<desc><![CDATA[Validation EqPriceData]]></desc>
			<sql><![CDATA[
SELECT * 
FROM MST_EQ_PUR_PRC
WHERE BSE_YRMON = REPLACE(@[bse_yrmon],'-','')
AND   LOC_CD =@[loc_cd]
AND   EQ_TPSZ_CD =@[eq_tpsz_cd]
AND   VNDR_SEQ =@[vndr_seq]
AND   EQ_KND_CD=@[eq_knd_cd]			]]></sql>
			<params>
				<param name="bse_yrmon" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
