<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrPlanGuidelineEmailDBDAOSearchContentsVslLaneCdRSQL">
			<desc><![CDATA[가장 최근된 guideline lane 검색(복수개가 나올수도 있음)]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.VSL_LANE_CD
FROM EQR_CTRL_GLINE_HDR A
    ,(
         -- 가장 최근 CONFIRM 날짜
         SELECT MAX(UPD_DT) MAX_UPD_DT  
         FROM EQR_CTRL_GLINE_HDR
         WHERE CFM_FLG = 'Y'  -- CONFIRM
     ) B
WHERE A.UPD_DT = B.MAX_UPD_DT
AND   A.CFM_FLG = 'Y'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
