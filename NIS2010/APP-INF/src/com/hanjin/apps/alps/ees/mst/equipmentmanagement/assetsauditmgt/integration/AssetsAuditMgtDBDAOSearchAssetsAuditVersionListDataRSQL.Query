<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AssetsAuditMgtDBDAOSearchAssetsAuditVersionListDataRSQL">
			<desc><![CDATA[SearchAssetsAuditVersionListdata]]></desc>
			<sql><![CDATA[
SELECT A.VER_NO 
FROM MST_EQ_ASET_AUD A
WHERE A.JB_EXE_YRMON = REPLACE(NVL(@[yr_mon],'0'),'-','')
AND   A.EQ_KND_CD = @[eq_type]			]]></sql>
			<params>
				<param name="yr_mon" type="12" value="" out="N"/>
				<param name="eq_type" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
