<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LeaseInfoManageDBDAOCheckUpdateHireECCRSQL">
			<desc><![CDATA[EQR_ECC_MST 테이블 데이터 중복조회]]></desc>
			<sql><![CDATA[
SELECT 
	COUNT(*) coun 
FROM 
	EQR_ECC_MST
WHERE 
	ECC_CD = @[ecc_cd]
	AND DELT_FLG ='N'			]]></sql>
			<params>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
