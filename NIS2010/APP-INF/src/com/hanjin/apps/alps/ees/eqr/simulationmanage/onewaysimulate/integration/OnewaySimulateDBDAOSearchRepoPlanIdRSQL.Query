<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnewaySimulateDBDAOSearchRepoPlanIdRSQL">
			<desc><![CDATA[repo_pln_id의 존재 여부 확인]]></desc>
			<sql><![CDATA[
SELECT  
	REPO_PLN_ID 
   	,SCNR_ID  
FROM   
	EQR_EQ_REPO_PLN 
WHERE 
	REPO_PLN_ID = @[repo_pln_id]			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
