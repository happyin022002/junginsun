<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOLocationCdRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT LOC_CD,
    (
		SELECT AR_HD_QTR_OFC_CD
		FROM MDM_ORGANIZATION
		WHERE OFC_CD = (
		                SELECT DMDT_OFC_CD
		                FROM MDM_YARD
		                WHERE LOC_CD = @[loc_cd]
		                AND DMDT_OFC_CD IS NOT NULL
		                AND DELT_FLG = 'N' 
		                AND ROWNUM = 1
    					)
	) LOC_RHQ_CD
FROM MDM_LOCATION
WHERE LOC_CD = @[loc_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
