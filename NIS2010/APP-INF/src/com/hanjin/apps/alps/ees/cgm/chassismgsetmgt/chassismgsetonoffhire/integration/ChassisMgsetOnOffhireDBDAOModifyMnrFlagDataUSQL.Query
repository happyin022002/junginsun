<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOModifyMnrFlagDataUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE CGM_EQUIPMENT
SET DMG_FLG    = DECODE(@[flag_type],'DMG',@[sts_flag],DMG_FLG)  
  , DISP_FLG   = DECODE(@[flag_type],'DSP',@[sts_flag],DISP_FLG)  
  , UPD_DT     = SYSDATE
WHERE EQ_NO = @[eq_no]  			]]></sql>
			<params>
				<param name="flag_type" type="12" value="" out="N"/>
				<param name="sts_flag" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
