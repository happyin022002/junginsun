<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOCreateManageUncollectedCargoFileCSQL">
			<desc><![CDATA[UC Activity 저장 처리 ]]></desc>
			<sql><![CDATA[
INSERT INTO CIM_UC_CGO_FILE
	(UC_CS_NO
	, UC_CGO_FILE_ID
	, FILE_NO_SEQ
	, FILE_SAV_ID
	, FILE_DESC
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
	)
VALUES (@[uc_cs_no]
	,@[uc_cgo_file_id]
	,@[file_no_seq]
	,' '
	,@[file_desc]
	,@[cre_usr_id]
	,SYSDATE
	,@[upd_usr_id]
	,SYSDATE)			]]></sql>
			<params>
				<param name="uc_cs_no" type="12" value="" out="N"/>
				<param name="uc_cgo_file_id" type="12" value="" out="N"/>
				<param name="file_no_seq" type="12" value="" out="N"/>
				<param name="file_desc" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
