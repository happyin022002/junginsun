<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAttachDetachHistoryDBDAOaddMGSAtdtHistoryDataCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO CGM_EQ_ATCH_DTCH_HIS
(
 EQ_NO
,ATCH_DT
,ATCH_DTCH_SEQ
,EQ_KND_CD
,CHSS_NO
,CNTR_NO
,DTCH_DT
,ATCH_YD_CD
,ATCH_INP_TP_CD
,CRE_USR_ID
,CRE_DT 
,UPD_USR_ID
,UPD_DT
)
values
(
 @[eq_no]         
,TO_DATE(@[atch_dt] ,'YYYY-MM-DD HH24:MI:SS') 
,(SELECT DECODE(MAX(ATCH_DTCH_SEQ)+1,NULL,1,MAX(ATCH_DTCH_SEQ)+1) FROM CGM_EQ_ATCH_DTCH_HIS 
   WHERE EQ_NO= @[eq_no] AND ATCH_DT = TO_DATE(@[atch_dt] ,'YYYY-MM-DD HH24:MI:SS')  )  
,'G'
,@[chss_no]
,@[cntr_no]
,TO_DATE('8888-12-31' ,'YYYY-MM-DD HH24:MI:SS') 
,@[atch_yd_cd]
,'M'
,@[cre_usr_id]      
,sysdate          
,@[upd_usr_id]       
,sysdate     
)			]]></sql>
			<params>
				<param name="eq_no" type="12" value="" out="N"/>
				<param name="atch_dt" type="12" value="" out="N"/>
				<param name="chss_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="atch_yd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
