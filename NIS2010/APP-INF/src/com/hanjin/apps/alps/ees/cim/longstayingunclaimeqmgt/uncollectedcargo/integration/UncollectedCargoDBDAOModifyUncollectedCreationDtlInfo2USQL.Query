<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOModifyUncollectedCreationDtlInfo2USQL">
			<desc><![CDATA[Counter Office 의 Branch/Agent 의 경우 변경할 수 있는 내용이 제한 되어 있음. ]]></desc>
			<sql><![CDATA[
UPDATE	CIM_UC_CGO_DTL
SET	
		kntr_ofc_opin_desc 		=	@[kntr_ofc_opin_desc]
		, upd_usr_id 			=	@[upd_usr_id]
		, upd_dt 				=	SYSDATE
WHERE	1 = 1
		AND	uc_cs_no 			=	@[uc_cs_no]
		AND bl_no				= 	@[bl_no]			]]></sql>
			<params>
				<param name="kntr_ofc_opin_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="uc_cs_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
