<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioManageDBDAOUpdateScenarioIDUSQL">
			<desc><![CDATA[EQR_SCNR_MST]]></desc>
			<sql><![CDATA[
UPDATE	EQR_SCNR_MST SET 
	DELT_FLG = 'Y'
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = SYSDATE
WHERE	SCNR_ID = @[scnr_id]
			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
