                SUM(CASE WHEN A.RPT_DP_SEQ =29 THEN A.CAL_PART_AMT END) AS TPSZ_DP29,
                SUM(CASE WHEN A.RPT_DP_SEQ =30 THEN A.CAL_PART_AMT END) AS TPSZ_DP30
        FROM    TEMP_DROP A
