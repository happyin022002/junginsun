                SUM(CASE WHEN A.RPT_DP_SEQ =15 THEN A.CAL_PART_AMT END) AS TPSZ_DP15,
                SUM(CASE WHEN A.RPT_DP_SEQ =16 THEN A.CAL_PART_AMT END) AS TPSZ_DP16,
