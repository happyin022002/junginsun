                SUM(CASE WHEN A.RPT_DP_SEQ =22 THEN A.CAL_PART_AMT END) AS TPSZ_DP22,
                SUM(CASE WHEN A.RPT_DP_SEQ =23 THEN A.CAL_PART_AMT END) AS TPSZ_DP23,
                SUM(CASE WHEN A.RPT_DP_SEQ =24 THEN A.CAL_PART_AMT END) AS TPSZ_DP24,
                SUM(CASE WHEN A.RPT_DP_SEQ =25 THEN A.CAL_PART_AMT END) AS TPSZ_DP25,
                SUM(CASE WHEN A.RPT_DP_SEQ =26 THEN A.CAL_PART_AMT END) AS TPSZ_DP26,
                SUM(CASE WHEN A.RPT_DP_SEQ =27 THEN A.CAL_PART_AMT END) AS TPSZ_DP27,
