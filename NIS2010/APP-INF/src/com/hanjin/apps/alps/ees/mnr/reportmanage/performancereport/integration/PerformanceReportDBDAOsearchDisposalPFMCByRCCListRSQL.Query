<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOsearchDisposalPFMCByRCCListRSQL">
			<desc><![CDATA[RCC/LCC/SCC 지역별 매각실적 현황목록을 조회합니다.]]></desc>
			<sql><![CDATA[
WITH TEMP_DROP AS (
    SELECT  A.DISP_NO, A.APRO_DT, A.EQ_KND_CD, A.CURR_CD,
            A.DISP_DTL_SEQ, A.EQ_TPSZ_CD, A.DISP_QTY, A.DISP_YD_CD,
            A.PART_AMT, A.CAL_PART_AMT, B.RPT_DP_SEQ,
            C.LOC_CD, C.SCC_CD, C.LCC_CD, C.RCC_CD,
            TO_CHAR(A.APRO_DT, 'YYYYMM') AS APRO_MON,
            SUM(A.DISP_QTY) OVER() DISP_TOT
    FROM   (SELECT  A.DISP_NO, B.DISP_SOLD_DT AS APRO_DT, A.EQ_KND_CD, A.CURR_CD,
                    B.DISP_DTL_SEQ, B.EQ_TPSZ_CD, B.DISP_QTY, B.PART_AMT, B.DISP_YD_CD,
                    MNR_COMMON_PKG.MNR_CAL_CURR_RATE_FNC(TO_CHAR(B.DISP_SOLD_DT, 'YYYYMM'),
                            A.CURR_CD, 'USD', B.PART_AMT) AS CAL_PART_AMT
            FROM    MNR_DISP_HDR A,
                    MNR_DISP_DTL B
            WHERE   A.DISP_NO = B.DISP_NO
            AND     B.DISP_SOLD_DT IS NOT NULL
            AND     B.DISP_SOLD_DT BETWEEN TO_DATE(@[p_str_evnt_dt],'YYYYMMDD') AND TO_DATE(@[p_end_evnt_dt],'YYYYMMDD')
#if (${p_eq_knd_cd} != '' )
            AND     A.EQ_KND_CD = @[p_eq_knd_cd]
#end
#if (${p_disp_tp_cd} != '' )
            AND     A.DISP_TP_CD = @[p_disp_tp_cd]
#end
#if (${p_disp_rsn_cd} != '' )
            AND     B.DISP_RSN_CD = @[p_disp_rsn_cd]
#end
#if (${p_cust_cd} != '')
            AND     B.MNR_PRNR_CNT_CD = SUBSTR(@[p_cust_cd],0,2)
            AND     B.MNR_PRNR_SEQ = SUBSTR(@[p_cust_cd],3,8)
#end
            ) A,
           (SELECT  EQ_KND_CD, EQ_TPSZ_CD,
                    ROW_NUMBER() OVER(PARTITION BY EQ_KND_CD ORDER BY DP_SEQ) AS RPT_DP_SEQ
            FROM   (SELECT  EQ_KND_CD, EQ_TPSZ_CD, DP_SEQ
                    FROM    CGM_EQ_TP_SZ
                    UNION ALL
                    SELECT  'U' AS EQ_KND_CD, CNTR_TPSZ_CD, RPT_DP_SEQ
                    FROM    MDM_CNTR_TP_SZ
                    )
            ) B,
           (SELECT  A.LOC_CD, A.RGN_CD, A.SCC_CD, A.EQ_CTRL_OFC_CD,
                    C.LCC_CD, C.ECC_CD, C.RCC_CD
            FROM    MDM_LOCATION A,
                    MDM_EQ_ORZ_CHT C
            WHERE   A.SCC_CD = C.SCC_CD
            ) C
    WHERE   A.EQ_KND_CD  = B.EQ_KND_CD
    AND     A.EQ_TPSZ_CD = B.EQ_TPSZ_CD
    AND     SUBSTR(A.DISP_YD_CD,1,5) = C.LOC_CD
#if (${p_loc_cd} != '')
	#if (${p_loc_tp} == 'RCC')
    AND    	C.RCC_CD = @[p_loc_cd]
	#elseif (${p_loc_tp} == 'LCC')
    AND    	C.LCC_CD = @[p_loc_cd]
	#else
    AND    	C.SCC_CD = @[p_loc_cd]
	#end
#end
)
SELECT  CASE WHEN A.RCC_CD IS NULL THEN 0
             WHEN A.LCC_CD IS NULL THEN 0
             WHEN A.SCC_CD IS NULL THEN 1
             ELSE 2 END LEVEL_NO,
        CASE WHEN A.RCC_CD IS NULL THEN NVL(A.LOC_CD, 'G.TTL')
             WHEN A.LCC_CD IS NULL THEN A.RCC_CD
             WHEN A.SCC_CD IS NULL THEN A.LCC_CD
             ELSE A.SCC_CD END LOC_CD,
        A.RCC_CD, A.LCC_CD, A.SCC_CD,
        A.DISP_QTY_TOT,
        CASE WHEN A.LOC_CD = 'Ratio'
             THEN LTRIM(TO_CHAR(A.DISP_QTY_CNT / A.DISP_QTY_TOT * 100, '990.00'))||'%'
             ELSE LTRIM(TO_CHAR(A.DISP_QTY_CNT, '999,999,990')) END DISP_QTY_CNT,
        CASE WHEN A.LOC_CD = 'Ratio' THEN NULL
             ELSE LTRIM(TO_CHAR(A.DISP_QTY_CNT / A.DISP_QTY_TOT * 100, '990.00'))||'%'
             END DISP_QTY_RTO,
        LTRIM(TO_CHAR(A.CAL_PART_AMT_TOT, '999,999,990.00')) AS CAL_PART_AMT_TOT,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP01, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP01, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP01, 0), '999,999,990')) END TPSZ_DP01,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP02, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP02, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP02, 0), '999,999,990')) END TPSZ_DP02,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP03, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP03, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP03, 0), '999,999,990')) END TPSZ_DP03,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP04, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP04, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP04, 0), '999,999,990')) END TPSZ_DP04,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP05, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP05, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP05, 0), '999,999,990')) END TPSZ_DP05,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP06, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP06, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP06, 0), '999,999,990')) END TPSZ_DP06,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP07, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP07, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP07, 0), '999,999,990')) END TPSZ_DP07,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP08, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP08, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP08, 0), '999,999,990')) END TPSZ_DP08,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP09, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP09, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP09, 0), '999,999,990')) END TPSZ_DP09,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP10, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP10, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP10, 0), '999,999,990')) END TPSZ_DP10,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP11, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP11, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP11, 0), '999,999,990')) END TPSZ_DP11,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP12, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP12, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP12, 0), '999,999,990')) END TPSZ_DP12,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP13, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP13, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP13, 0), '999,999,990')) END TPSZ_DP13,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP14, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP14, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP14, 0), '999,999,990')) END TPSZ_DP14,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP15, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP15, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP15, 0), '999,999,990')) END TPSZ_DP15,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP16, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP16, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP16, 0), '999,999,990')) END TPSZ_DP16,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP17, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP17, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP17, 0), '999,999,990')) END TPSZ_DP17,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP18, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP18, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP18, 0), '999,999,990')) END TPSZ_DP18,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP19, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP19, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP19, 0), '999,999,990')) END TPSZ_DP19,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP20, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP20, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP20, 0), '999,999,990')) END TPSZ_DP20,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP21, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP21, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP21, 0), '999,999,990')) END TPSZ_DP21,
        CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP22, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP22, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP22, 0), '999,999,990')) END TPSZ_DP22,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP23, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP23, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP23, 0), '999,999,990')) END TPSZ_DP23,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP24, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP24, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP24, 0), '999,999,990')) END TPSZ_DP24,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP25, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP25, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP25, 0), '999,999,990')) END TPSZ_DP25,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP26, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP26, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP26, 0), '999,999,990')) END TPSZ_DP26,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP27, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP27, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP27, 0), '999,999,990')) END TPSZ_DP27,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP28, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP28, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP28, 0), '999,999,990')) END TPSZ_DP28,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP29, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP29, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP29, 0), '999,999,990')) END TPSZ_DP29,
		CASE WHEN A.LOC_CD = 'Ratio' THEN LTRIM(TO_CHAR(A.TPSZ_DP30, '990.00'))||'%'
             WHEN A.LOC_CD IN('G.AMT','G.AVG') THEN LTRIM(TO_CHAR(A.TPSZ_DP30, '999,999,990.00'))
             ELSE LTRIM(TO_CHAR(NVL(A.TPSZ_DP30, 0), '999,999,990')) END TPSZ_DP30
FROM   (SELECT  '' AS LOC_CD, A.RCC_CD, A.LCC_CD, A.SCC_CD,
                MAX(A.DISP_TOT) AS DISP_QTY_TOT,
                SUM(A.DISP_QTY) AS DISP_QTY_CNT,
                SUM(A.PART_AMT) AS PART_AMT_TOT,
                SUM(A.CAL_PART_AMT) AS CAL_PART_AMT_TOT,
                SUM(CASE WHEN A.RPT_DP_SEQ = 1 THEN A.DISP_QTY END) AS TPSZ_DP01,
                SUM(CASE WHEN A.RPT_DP_SEQ = 2 THEN A.DISP_QTY END) AS TPSZ_DP02,
                SUM(CASE WHEN A.RPT_DP_SEQ = 3 THEN A.DISP_QTY END) AS TPSZ_DP03,
                SUM(CASE WHEN A.RPT_DP_SEQ = 4 THEN A.DISP_QTY END) AS TPSZ_DP04,
                SUM(CASE WHEN A.RPT_DP_SEQ = 5 THEN A.DISP_QTY END) AS TPSZ_DP05,
                SUM(CASE WHEN A.RPT_DP_SEQ = 6 THEN A.DISP_QTY END) AS TPSZ_DP06,
                SUM(CASE WHEN A.RPT_DP_SEQ = 7 THEN A.DISP_QTY END) AS TPSZ_DP07,
                SUM(CASE WHEN A.RPT_DP_SEQ = 8 THEN A.DISP_QTY END) AS TPSZ_DP08,
                SUM(CASE WHEN A.RPT_DP_SEQ = 9 THEN A.DISP_QTY END) AS TPSZ_DP09,
                SUM(CASE WHEN A.RPT_DP_SEQ =10 THEN A.DISP_QTY END) AS TPSZ_DP10,
                SUM(CASE WHEN A.RPT_DP_SEQ =11 THEN A.DISP_QTY END) AS TPSZ_DP11,
                SUM(CASE WHEN A.RPT_DP_SEQ =12 THEN A.DISP_QTY END) AS TPSZ_DP12,
                SUM(CASE WHEN A.RPT_DP_SEQ =13 THEN A.DISP_QTY END) AS TPSZ_DP13,
                SUM(CASE WHEN A.RPT_DP_SEQ =14 THEN A.DISP_QTY END) AS TPSZ_DP14,
                SUM(CASE WHEN A.RPT_DP_SEQ =15 THEN A.DISP_QTY END) AS TPSZ_DP15,
                SUM(CASE WHEN A.RPT_DP_SEQ =16 THEN A.DISP_QTY END) AS TPSZ_DP16,
                SUM(CASE WHEN A.RPT_DP_SEQ =17 THEN A.DISP_QTY END) AS TPSZ_DP17,
                SUM(CASE WHEN A.RPT_DP_SEQ =18 THEN A.DISP_QTY END) AS TPSZ_DP18,
                SUM(CASE WHEN A.RPT_DP_SEQ =19 THEN A.DISP_QTY END) AS TPSZ_DP19,
                SUM(CASE WHEN A.RPT_DP_SEQ =20 THEN A.DISP_QTY END) AS TPSZ_DP20,
                SUM(CASE WHEN A.RPT_DP_SEQ =21 THEN A.DISP_QTY END) AS TPSZ_DP21,
                SUM(CASE WHEN A.RPT_DP_SEQ =22 THEN A.DISP_QTY END) AS TPSZ_DP22,
                SUM(CASE WHEN A.RPT_DP_SEQ =23 THEN A.DISP_QTY END) AS TPSZ_DP23,
                SUM(CASE WHEN A.RPT_DP_SEQ =24 THEN A.DISP_QTY END) AS TPSZ_DP24,
                SUM(CASE WHEN A.RPT_DP_SEQ =25 THEN A.DISP_QTY END) AS TPSZ_DP25,
                SUM(CASE WHEN A.RPT_DP_SEQ =26 THEN A.DISP_QTY END) AS TPSZ_DP26,
                SUM(CASE WHEN A.RPT_DP_SEQ =27 THEN A.DISP_QTY END) AS TPSZ_DP27,
                SUM(CASE WHEN A.RPT_DP_SEQ =28 THEN A.DISP_QTY END) AS TPSZ_DP28,
                SUM(CASE WHEN A.RPT_DP_SEQ =29 THEN A.DISP_QTY END) AS TPSZ_DP29,
                SUM(CASE WHEN A.RPT_DP_SEQ =30 THEN A.DISP_QTY END) AS TPSZ_DP30
        FROM    TEMP_DROP A
        GROUP BY ROLLUP(A.RCC_CD, A.LCC_CD, A.SCC_CD)
        UNION ALL
        SELECT  'G.AMT' AS LOC_CD, '' AS RCC_CD, '' AS LCC_CD, '' AS SCC_CD,
                MAX(A.DISP_TOT) AS DISP_QTY_TOT,
                SUM(A.DISP_QTY) AS DISP_QTY_CNT,
                SUM(A.CAL_PART_AMT) AS PART_AMT_TOT,
                SUM(A.CAL_PART_AMT) AS CAL_PART_AMT_TOT,
                SUM(CASE WHEN A.RPT_DP_SEQ = 1 THEN A.CAL_PART_AMT END) AS TPSZ_DP01,
                SUM(CASE WHEN A.RPT_DP_SEQ = 2 THEN A.CAL_PART_AMT END) AS TPSZ_DP02,
                SUM(CASE WHEN A.RPT_DP_SEQ = 3 THEN A.CAL_PART_AMT END) AS TPSZ_DP03,
                SUM(CASE WHEN A.RPT_DP_SEQ = 4 THEN A.CAL_PART_AMT END) AS TPSZ_DP04,
                SUM(CASE WHEN A.RPT_DP_SEQ = 5 THEN A.CAL_PART_AMT END) AS TPSZ_DP05,
                SUM(CASE WHEN A.RPT_DP_SEQ = 6 THEN A.CAL_PART_AMT END) AS TPSZ_DP06,
