<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OfficeGeneralInfoMgtDBDAOsearchOfficeGeneralInfoListDataRSQL">
			<desc><![CDATA[select searchOfficeGeneralInfolst]]></desc>
			<sql><![CDATA[
SELECT 
    C.AR_HD_QTR_OFC_CD  
,	A.OFC_CD AS OFC_CD
,	A.MNR_GRP_TP_CD
,	A.COST_CD
,	A.EQ_KND_CD
,	A.UPPR_OFC_CD
,   TO_CHAR(A.EFF_DT,'YYYY-MM-DD') AS EFF_DT
,	A.CURR_CD
,	A.BFR_AUTO_APRO_AMT
,	A.BFR_SELF_AUTH_AMT
,	A.AFT_AUTO_APRO_AMT
,	A.AFT_SELF_AUTH_AMT
,	A.CRE_USR_ID
,	TO_CHAR(A.CRE_DT,'YYYY-MM-DD') AS CRE_DT
,	A.UPD_USR_ID
,	A.OFC_CD AS ORG_OFC_CD
,   A.MNR_GRP_TP_CD AS ORG_MNR_GRP_TP_CD
,	A.COST_CD AS ORG_COST_CD
,	A.OFC_CD AS ORG_OFC_CD
,	A.MNR_GRP_TP_CD AS ORG_MNR_GRP_TP_CD
,	A.COST_CD AS ORG_COST_CD
FROM 
MDM_ORGANIZATION C , MNR_OFC_GEN_INFO A
WHERE A.MNR_GRP_TP_CD = @[mnr_grp_tp_cd]
AND C.AR_HD_QTR_OFC_CD LIKE '%'|| NVL(@[ar_hd_qtr_ofc_cd], '') ||'%'
AND A.OFC_CD = C.OFC_CD
AND A.OFC_CD LIKE  '%'|| NVL(@[ofc_cd], '') ||'%'
AND A.COST_CD LIKE '%'|| NVL(@[cost_cd], '') ||'%'			]]></sql>
			<params>
				<param name="mnr_grp_tp_cd" type="12" value="" out="N"/>
				<param name="ar_hd_qtr_ofc_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cost_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
