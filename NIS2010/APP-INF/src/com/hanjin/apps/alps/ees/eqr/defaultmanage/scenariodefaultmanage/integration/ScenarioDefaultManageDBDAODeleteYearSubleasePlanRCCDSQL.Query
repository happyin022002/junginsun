<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAODeleteYearSubleasePlanRCCDSQL">
			<desc><![CDATA[eqr_sublease  테이블 데이터 삭제 (RCC 단위 삭제)]]></desc>
			<sql><![CDATA[
DELETE FROM EQR_SUBLEASE 
WHERE SUBSTR(PLN_YRWK,1,4) = @[fm_pln_yr]
AND FM_ECC_CD IN (
    SELECT ECC_CD FROM EQR_ECC_MST WHERE RCC_CD = @[s1_rcc_cd]
)			]]></sql>
			<params>
				<param name="fm_pln_yr" type="12" value="" out="N"/>
				<param name="s1_rcc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
