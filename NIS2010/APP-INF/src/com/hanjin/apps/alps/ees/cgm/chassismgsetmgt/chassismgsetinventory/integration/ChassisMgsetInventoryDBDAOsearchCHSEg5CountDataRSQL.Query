<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOsearchCHSEg5CountDataRSQL">
			<desc><![CDATA[chungpa 20090811 1111 start]]></desc>
			<sql><![CDATA[
     
select 
b.yd_cd AS YD_CD, 
NVL((select t1.eg5_pre_knt_qty from cgm_chss_utl_eg5_knt t1 where b.yd_cd = t1.yd_cd),0) AS EG5_PRE_KNT_QTY 
from MDM_LOCATION  a, MDM_YARD b
where  a.scc_cd = @[loc_cd]
   AND b.loc_cd = a.loc_cd
   AND A.delt_flg = 'N'
   AND b.delt_flg = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
