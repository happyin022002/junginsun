<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOSearchFAXSendHistoryRSQL">
			<desc><![CDATA[COM_FAX_SND_INFO 테이블에서 FAX SEND HISTORY 조회]]></desc>
			<sql><![CDATA[
SELECT  
    DISTINCT TRIM(RCVR_INFO_CTNT) VALUE  
FROM 
    COM_FAX_SND_INFO	           	
WHERE 
    RD_SUB_SYS_CD = 'EQR'       	
    AND RD_APPL_CD	=  @[rd_appl_cd]            	
    AND CRE_USR_ID  =  @[user_id]			]]></sql>
			<params>
				<param name="rd_appl_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
