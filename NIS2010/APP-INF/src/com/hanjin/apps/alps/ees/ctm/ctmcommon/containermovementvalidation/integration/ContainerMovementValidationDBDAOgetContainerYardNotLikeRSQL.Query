<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOgetContainerYardNotLikeRSQL">
			<desc><![CDATA[ContainerMovementValidationDBDAOgetContainerYardRSQL.Query와 동일
추후 DBDAO, BCImpl메서드와 함께 삭제할 대상임]]></desc>
			<sql><![CDATA[
SELECT A.CNTR_NO,
       A.CNMV_STS_CD,
       A.CNTR_TPSZ_CD,
       A.CRNT_YD_CD,
       A.ACIAC_DIV_CD,
       A.HJS_CRE_FLG,
       A.IMDT_EXT_FLG,
       A.NEW_FLG,
       DECODE (B.CNTR_NO, NULL, 0, 1) CNTR_EX
  FROM (SELECT MM.CNTR_NO,
               MM.CNMV_STS_CD,
               MM.CNTR_TPSZ_CD,
               MM.CRNT_YD_CD,
               MM.ACIAC_DIV_CD,
               MM.HJS_CRE_FLG,
               MM.IMDT_EXT_FLG,
               MM.NEW_FLG
          FROM (SELECT CNTR_NO,
                       CNMV_STS_CD,
                       CNTR_TPSZ_CD,
                       CRNT_YD_CD,
                       ACIAC_DIV_CD,
                       CASE WHEN SYS_AREA_GRP_ID = RGN_CD THEN 'Y' ELSE 'N' END HJS_CRE_FLG,
                       IMDT_EXT_FLG,
                       HJS_CRE_FLG NEW_FLG
                  FROM MST_CONTAINER M,
                       (SELECT SYS_AREA_GRP_ID AS RGN_CD
                          FROM COM_SYS_AREA_GRP_ID
                         WHERE CNT_CD = SUBSTR (@[yard_cd], 1, 2)
                           AND CO_IND_CD = 'H')
                 WHERE CNTR_NO LIKE DECODE(@[cntr_no], NULL, '9999999999999999', 'SMCU', '9999999999999999', @[cntr_no])||'%'
              ORDER BY CNMV_DT DESC) MM
         WHERE ROWNUM = 1) A,
       (SELECT CNTR_NO
          FROM (SELECT CNTR_NO
                  FROM CTM_MOVEMENT
                 WHERE CNTR_NO LIKE DECODE(@[cntr_no], NULL, '9999999999999999', 'SMCU', '9999999999999999', @[cntr_no])||'%'
              ORDER BY CNMV_EVNT_DT DESC)
         WHERE ROWNUM = 1) B
 WHERE A.CNTR_NO = B.CNTR_NO(+)
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="yard_cd" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
