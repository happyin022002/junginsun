<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAttachDetachHistoryDBDAOmodifyMGSAtdtHistoryDataUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE CGM_EQ_ATCH_DTCH_HIS
SET DTCH_DT  =TO_DATE(@[dtch_dt]  ,'YYYY-MM-DD HH24:MI:SS')          
   ,DTCH_YD_CD    = @[dtch_yd_cd]
   ,UPD_USR_ID = @[upd_usr_id]
   ,UPD_DT = SYSDATE
WHERE   EQ_NO=@[eq_no]
AND DTCH_DT = TO_DATE('88881231','YYYYMMDD')			]]></sql>
			<params>
				<param name="dtch_dt" type="12" value="" out="N"/>
				<param name="dtch_yd_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
