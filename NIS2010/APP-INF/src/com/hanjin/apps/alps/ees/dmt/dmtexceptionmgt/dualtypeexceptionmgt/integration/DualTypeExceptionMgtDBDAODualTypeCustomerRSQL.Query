        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd7] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd7])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd8] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd8])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd9] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd9])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '10')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd4] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd4])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd5] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd5])
