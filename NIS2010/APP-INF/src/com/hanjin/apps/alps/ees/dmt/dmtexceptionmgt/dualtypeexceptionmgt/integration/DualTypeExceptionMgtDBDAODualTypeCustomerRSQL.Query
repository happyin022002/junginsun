        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd4] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd4])
        		OR
