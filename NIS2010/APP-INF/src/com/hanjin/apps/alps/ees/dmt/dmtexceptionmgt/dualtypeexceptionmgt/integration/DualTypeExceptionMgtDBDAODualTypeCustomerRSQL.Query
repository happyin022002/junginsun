<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DualTypeExceptionMgtDBDAODualTypeCustomerRSQL">
			<desc><![CDATA[지역 Calculation Type과 무관하게 Combined Tariff 적용이 필요한 등록된 화주정보 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT  DECODE(A.DUL_EXPT_DELT_FLG, 'N', 'Live', 'Deleted') AS DUL_EXPT_DELT_DESC
	   ,A.CUST_CNT_CD || LPAD(A.CUST_SEQ, 6, '0') 			AS CUST_CD
	   ,B.CUST_LGL_ENG_NM 									AS CUST_NM
       ,DMDT_CTRT_EXPT_TP_CD
       ,TO_CHAR(A.DUL_EXPT_EFF_DT, 'YYYY-MM-DD') 			AS DUL_EXPT_EFF_DT
       ,TO_CHAR(A.DUL_EXPT_EXP_DT, 'YYYY-MM-DD') 			AS DUL_EXPT_EXP_DT
       ,A.IO_BND_CD
       ,A.CVRG_CNT_CD
       ,A.CVRG_RGN_STE_CD
       ,A.CVRG_LOC_CD
	   ,CASE 
			WHEN DMDT_CTRT_EXPT_TP_CD = 'B' THEN
				CASE 
					WHEN DMDT_CNTR_TP_CD IS NOT NULL AND DMDT_CGO_TP_CD IS NOT NULL THEN
						DMDT_CNTR_TP_CD || ':' || DMDT_CGO_TP_CD
					ELSE 
						''
			    END						
			WHEN DMDT_CTRT_EXPT_TP_CD = 'S' THEN 
				DMDT_CNTR_CGO_TP_CD
		END DMDT_CNTR_CGO_TP_CD
       ,A.DUL_EXPT_RMK
	   ,CUST_EXPT_SEQ
	   ,C.USR_NM UPD_USR_NM
       ,TO_CHAR(A.UPD_DT, 'YYYY-MM-DD') UPD_DT
       ,A.UPD_OFC_CD 
       ,A.PROP_NO
	   
  FROM  DMT_DUL_TP_EXPT A
       ,MDM_CUSTOMER 	B
       ,COM_USER 		C

 WHERE  A.CUST_CNT_CD = B.CUST_CNT_CD
   AND  A.CUST_SEQ = B.CUST_SEQ
   
   #if(${cust_cnt_cd} != '')
   AND  A.CUST_CNT_CD = @[cust_cnt_cd]
   #end
   
   #if(${cust_seq} != '')
   AND  A.CUST_SEQ = TO_NUMBER(@[cust_seq])
   #end

   #if(${dul_expt_eff_dt} != '')
   AND (
		(A.DUL_EXPT_EFF_DT >= TO_DATE(@[dul_expt_eff_dt], 'YYYYMMDD') AND A.DUL_EXPT_EFF_DT <= TO_DATE(@[dul_expt_exp_dt], 'YYYYMMDD'))
		OR
		(A.DUL_EXPT_EFF_DT <= TO_DATE(@[dul_expt_eff_dt], 'YYYYMMDD') AND NVL(A.DUL_EXPT_EXP_DT, TO_DATE('99991231', 'YYYYMMDD')) >= TO_DATE(@[dul_expt_eff_dt], 'YYYYMMDD'))
   )
   #end

   #if(${dul_expt_delt_flg} != '')
   AND  A.DUL_EXPT_DELT_FLG = @[dul_expt_delt_flg]
   #end

   #if(${io_bnd_cd} != '' && ${io_bnd_cd} != 'A')
   AND  A.IO_BND_CD = @[io_bnd_cd]
   #end

   #if(${prop_no} != '')
   AND  A.PROP_NO = @[prop_no]
   #end
   
   #if(${dmdt_ctrt_expt_tp_cd} != '' && ${dmdt_ctrt_expt_tp_cd} != 'A')
   AND  A.DMDT_CTRT_EXPT_TP_CD = @[dmdt_ctrt_expt_tp_cd]
   #end
   
   #if(${cvrg_cnt_cd} != '')
   AND  A.CVRG_CNT_CD = @[cvrg_cnt_cd]
   #end
   
   #if(${cvrg_rgn_ste_cd} != '')
   AND  A.CVRG_RGN_STE_CD = @[cvrg_rgn_ste_cd]
   #end
   
   #if(${cvrg_loc_cd} != '')
   AND A.CVRG_LOC_CD = @[cvrg_loc_cd]
   #end
   
   AND A.UPD_USR_ID = C.USR_ID

   #if (${dmdt_sc_cntr_cgo_cd_in} == 'Y' && ${dmdt_cntr_cgo_cd_in} == 'N')
   AND  A.DMDT_CNTR_CGO_TP_CD IN (		
		#foreach( $cntr_cgo_tp in ${dmdt_sc_cntr_cgo_tp_list} )
			#if($velocityCount < $dmdt_sc_cntr_cgo_tp_list.size()) '$cntr_cgo_tp', #else '$cntr_cgo_tp' #end
		#end
		)
   #elseif (${dmdt_sc_cntr_cgo_cd_in} == 'N' && ${dmdt_cntr_cgo_cd_in} == 'Y')
   AND  (
			#if(${dmdt_cntr_cgo_cd_size} == '1')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '2')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '3')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '4')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd4] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd4])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '5')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd4] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd4])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd5] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd5])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '6')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd4] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd4])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd5] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd5])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd6] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd6])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '7')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd4] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd4])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd5] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd5])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd6] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd6])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd7] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd7])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '8')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd2] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd2])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd3] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd3])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd4] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd4])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd5] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd5])
        		OR
