        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd8] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd8])
        		OR
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd9] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd9])
