        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd10] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd10])
        	#end
		)
   #elseif (${dmdt_sc_cntr_cgo_cd_in} == 'Y' && ${dmdt_cntr_cgo_cd_in} == 'Y')
   AND  (
		(A.DMDT_CNTR_CGO_TP_CD IN (		
		#foreach( $cntr_cgo_tp in ${dmdt_sc_cntr_cgo_tp_list} )
			#if($velocityCount < $dmdt_sc_cntr_cgo_tp_list.size()) '$cntr_cgo_tp', #else '$cntr_cgo_tp' #end
		#end
		))
		OR (
			#if(${dmdt_cntr_cgo_cd_size} == '1')
        		(A.DMDT_CNTR_TP_CD = @[dmdt_cntr_tp_cd1] AND A.DMDT_CGO_TP_CD = @[dmdt_cgo_tp_cd1])
        	#elseif(${dmdt_cntr_cgo_cd_size} == '2')
