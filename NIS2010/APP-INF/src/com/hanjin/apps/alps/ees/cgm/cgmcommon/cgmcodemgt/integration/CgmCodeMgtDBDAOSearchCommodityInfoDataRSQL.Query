<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOSearchCommodityInfoDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT cmdt_cd||'|'||cmdt_nm||'|'||rep_cmdt_cd||'|'||rep_imdg_lvl_cd||'|'||to_char(upd_dt,'YYYY-MM-DD') AS commodity
FROM ( 
	SELECT ROW_NUMBER() OVER (ORDER BY cmdt_cd ASC) no,
		cmdt_cd,              
        cmdt_nm,
        rep_cmdt_cd,
		rep_imdg_lvl_cd,
		upd_dt
	FROM mdm_commodity
	WHERE 1 = 1
	AND cmdt_cd = @[cmdt_cd])			]]></sql>
			<params>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
