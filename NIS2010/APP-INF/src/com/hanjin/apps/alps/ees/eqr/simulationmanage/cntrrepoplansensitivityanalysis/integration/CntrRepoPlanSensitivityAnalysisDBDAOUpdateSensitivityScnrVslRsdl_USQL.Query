<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanSensitivityAnalysisDBDAOUpdateSensitivityScnrVslRsdl_USQL">
			<desc><![CDATA[EQR_SCNR_VSL_RSDL_CAPA 테이블에 대한  QTY 수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_SCNR_VSL_RSDL_CAPA 
      SET  TTL_RSDL_SPC = ${ttl_rsdl_spc}                             	
           ,UPD_USR_ID   = @[user_id]                             	 
           ,UPD_DT       = sysdate                        	
WHERE  FCAST_YRWK = @[fcast_yrwk]                             	             
AND ECC_CD                             = @[ecc_cd]  	
AND VSL_CD || SKD_VOY_NO || SKD_DIR_CD = @[vvd]  	             
AND SCNR_ID                            = @[scnr_id]  	
AND VLS_LANE_CD                        = @[vsl_lane_cd]			   
AND CO_CD ='H'			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="fcast_yrwk" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="vsl_lane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
