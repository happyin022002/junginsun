<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmEdiSendDBDAOSearchEdiMsgToUsCaCntInfoRSQL">
			<desc><![CDATA[container looping]]></desc>
			<sql><![CDATA[
SELECT
      -- CNTR INFO
       @[cntr_no] CNTRNBR
      ,(SELECT CNTR_TPSZ_CD FROM MST_CONTAINER WHERE CNTR_NO = @[cntr_no] AND ROWNUM=1) CNTRTYPE
      ,(
			SELECT                
                ROUND(TO_NUMBER(CNTR_GRS_WGT) * 2.2046,0) TARE_WGT_LBS -- LBS 환산
			FROM 
				MST_CNTR_SPEC B, 
				MST_CONTAINER C 
			WHERE 1 = 1
			AND   C.CNTR_NO = @[cntr_no]
			AND   C.CNTR_SPEC_NO = B.CNTR_SPEC_NO(+)
            AND   ROWNUM=1
       ) CNTR_TOTAL_WGT
      ,'LBS'  CNTR_TOTAL_WGT_UNIT  -- 하드코딩

FROM BKG_BOOKING  A
WHERE A.BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
