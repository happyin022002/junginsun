<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOConvertDayWeeklyRSQL">
			<desc><![CDATA[day정보를 week로 변경]]></desc>
			<sql><![CDATA[
SELECT 
    PLN_YR||PLN_WK                      
FROM 
    EQR_WK_PRD                            
WHERE 
    @[date] BETWEEN WK_ST_DT AND WK_END_DT 				]]></sql>
			<params>
				<param name="date" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
