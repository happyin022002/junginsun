<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOSearchAgreementByPoolDataRSQL">
			<desc><![CDATA[CgmCodeMgtDBSearchAgreemenByPoolData
-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
2014.12.31 Chang Young Kim
[CHM-201433507] EES_CGM_1124화면에서 AGMT_VER_NO가 Fix되어 있는 문제 수정
-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=]]></desc>
			<sql><![CDATA[
#if (${chss_pool_cd} == '') 
SELECT A.AGMT_OFC_CTY_CD      AS CODE1,   
       LPAD(A.AGMT_SEQ,6,'0') AS CODE2,
       A.AGMT_REF_NO          AS CODE3,
       A.VNDR_SEQ             AS CODE4,
       B.VNDR_LGL_ENG_NM      AS DESC4,
       A.CHSS_POOL_CD         AS CODE5,
       A.CURR_CD              AS CODE6,
       A.AGMT_VER_NO          AS DESC1
  FROM CGM_AGREEMENT A, MDM_VENDOR B
 WHERE A.VNDR_SEQ = B.VNDR_SEQ
   AND A.EQ_KND_CD = 'Z'
   AND A.LST_VER_FLG = 'Y'
   AND A.AGMT_LSTM_CD = @[agmt_lstm_cd]
   AND A.DELT_FLG = 'N'
#else
SELECT A.AGMT_OFC_CTY_CD AS CODE1,
       A.AGMT_SEQ        AS CODE2,
       A.AGMT_REF_NO     AS CODE3,
       A.VNDR_SEQ        AS CODE4,
       B.VNDR_LGL_ENG_NM AS DESC4,
       A.CHSS_POOL_CD    AS CODE5,
       A.CURR_CD         AS CODE6,
       A.AGMT_VER_NO     AS DESC1
  FROM CGM_AGREEMENT A, MDM_VENDOR B
 WHERE @[chss_pool_cd] = A.CHSS_POOL_CD
   AND A.VNDR_SEQ = B.VNDR_SEQ
   AND A.EQ_KND_CD = 'Z'
   AND A.LST_VER_FLG = 'Y'
   AND A.AGMT_LSTM_CD = @[agmt_lstm_cd]
   AND A.DELT_FLG = 'N'
#end			]]></sql>
			<params>
				<param name="agmt_lstm_cd" type="12" value="" out="N"/>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
