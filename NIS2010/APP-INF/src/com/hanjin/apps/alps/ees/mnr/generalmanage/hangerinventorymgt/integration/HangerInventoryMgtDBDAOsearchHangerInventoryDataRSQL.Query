<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="HangerInventoryMgtDBDAOsearchHangerInventoryDataRSQL">
			<desc><![CDATA[HangerInventoryMgtDBDAOsearchHangerInventoryDataRSQL]]></desc>
			<sql><![CDATA[
SELECT
A.OFC_CD
,A.MNR_HNGR_BAR_TP_CD
,A.HNGR_INVT_VER_NO
,A.HNGR_INVT_LST_VER_FLG
,NVL(A.INVT_QTY,0) INVT_QTY
,NVL(A.MNR_HNGR_DMG_QTY,0) MNR_HNGR_DMG_QTY
,NVL(A.PUR_QTY,0) PUR_QTY
,NVL(A.CSM_QTY,0) CSM_QTY
,NVL(A.RCVR_QTY,0) RCVR_QTY
,NVL(A.ACT_INVT_QTY,0) ACT_INVT_QTY
,NVL(A.MNR_LOST_HNGR_QTY,0) MNR_LOST_HNGR_QTY
,NVL(A.MNR_DISP_HNGR_QTY,0) MNR_DISP_HNGR_QTY
,A.INVT_RMK
,A.CRE_USR_ID
,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.CRE_DT, @[ofc_cd]), 'yyyy-mm-dd') CRE_DT
,A.UPD_USR_ID
,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC(MNR_COMMON_PKG.MNR_GET_HOOFC_FNC(), A.UPD_DT, @[ofc_cd]), 'yyyy-mm-dd') UPD_DT
FROM MNR_HNGR_INVT A
WHERE A.HNGR_INVT_LST_VER_FLG='Y'
AND   A.OFC_CD =@[ofc_cd]
AND A.MNR_HNGR_BAR_TP_CD=@[mnr_hngr_bar_tp_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="mnr_hngr_bar_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
