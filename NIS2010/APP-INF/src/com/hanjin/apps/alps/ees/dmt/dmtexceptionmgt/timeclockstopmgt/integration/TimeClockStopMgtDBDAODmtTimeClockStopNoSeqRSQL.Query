<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TimeClockStopMgtDBDAODmtTimeClockStopNoSeqRSQL">
			<desc><![CDATA[Make a sequence]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(A XPKDMT_TM_CLK_STOP_NO) */
       clk_stop_no_seq
       + 1 AS seq
  FROM dmt_tm_clk_stop_no 
 WHERE clk_stop_ofc_cd = @[clk_stop_ofc_cd]
   AND clk_stop_yrmon_no = TO_CHAR ( NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE), 'rrmm')			]]></sql>
			<params>
				<param name="clk_stop_ofc_cd" type="12" value="ABCDEF" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
