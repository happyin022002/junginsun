<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOMergeEqrHolidayEffectDetailCSQL">
			<desc><![CDATA[EQR_HOL_EFF_RTO 테이블에 정보 입력/수정]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_HOL_EFF_RTO A   		 		
    USING (         								
        SELECT  
            @[hol_yr] AS HOL_YR ,       				
            @[cnt_cd] AS CNT_CD ,    				
            @[rcc_div_flg] AS RCC_DIV_FLG ,     			
            @[st_dt] AS ST_DT ,    				
            @[hol_eff_yrwk] AS HOL_EFF_YRWK ,     			
            @[io_bnd_cd] AS IO_BND_CD ,    			
            @[wk_dy_cd] AS WK_DY_CD     				
        FROM DUAL     							
    ) B     								
    ON(     										
        A.HOL_YR = B.HOL_YR 				
        AND A.CNT_CD = B.CNT_CD 				
        AND A.RCC_DIV_FLG = B.RCC_DIV_FLG 		
        AND A.ST_DT = B.ST_DT 					
        AND A.HOL_EFF_YRWK = B.HOL_EFF_YRWK 	
        AND A.IO_BND_CD = B.IO_BND_CD 			
        AND A.WK_DY_CD = B.WK_DY_CD 			
    )     										
    WHEN  MATCHED THEN 					    	
        UPDATE  SET  
            HOL_EFF_RTO = @[hol_eff_rto],                    	
            UPD_USR_ID  = @[user_id],               		
            UPD_DT      = SYSDATE			
    WHEN NOT MATCHED THEN 						
        INSERT (  
            HOL_YR,                    			
            CNT_CD,               					
            RCC_DIV_FLG,              				 
            ST_DT,                					
            HOL_EFF_YRWK,                			
            IO_BND_CD,                				
            WK_DY_CD,                				
            HOL_EFF_RTO,                									        
            CNT_NM,                										        
            CRE_USR_ID,                 			
            CRE_DT,                     			
            UPD_USR_ID,                 			
            UPD_DT
        ) VALUES (
            @[hol_yr],
            @[cnt_cd],
            @[rcc_div_flg],
            @[st_dt],
            @[hol_eff_yrwk],
            @[io_bnd_cd],
            @[wk_dy_cd],
            @[hol_eff_rto],
            @[cnt_nm], 
            @[user_id],
            SYSDATE,
            @[user_id],
            SYSDATE
        )
			]]></sql>
			<params>
				<param name="hol_yr" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rcc_div_flg" type="12" value="" out="N"/>
				<param name="st_dt" type="12" value="" out="N"/>
				<param name="hol_eff_yrwk" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="wk_dy_cd" type="12" value="" out="N"/>
				<param name="hol_eff_rto" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cnt_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
