<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOModifyManageUncollectedCargoMemoUSQL">
			<desc><![CDATA[관리자 메모 저장]]></desc>
			<sql><![CDATA[
UPDATE  CIM_UC_CGO_DTL
#if     (${isAuthority} == '1')
SET     UC_CGO_RHQ_MM_DESC = @[managerMemo]
      , UC_CGO_RHQ_MM_DESC_USR_ID = @[usr_id]
      , UC_CGO_RHQ_MM_DESC_UPD_DT = SYSDATE
#else
SET     UC_CGO_HO_MM_DESC = @[managerMemo]
      , UC_CGO_HO_MM_DESC_USR_ID = @[usr_id]
      , UC_CGO_HO_MM_DESC_UPD_DT = SYSDATE
#end
WHERE UC_CS_NO = @[ucCsNo]
AND   BL_NO=@[blNo]			]]></sql>
			<params>
				<param name="managerMemo" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ucCsNo" type="12" value="" out="N"/>
				<param name="blNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
