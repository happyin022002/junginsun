<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOCheckDmtExceptionChargeCalculationRSQL">
			<desc><![CDATA[Exception (S/C, RFA, After Booking) DMT_EXPT_CHG_CALC 테이블에 데이터가 존재하는지 확인한다.]]></desc>
			<sql><![CDATA[
SELECT	COUNT(*) AS CNT
FROM    DMT_EXPT_CHG_CALC
WHERE   BKG_NO                  = @[bkg_no]
AND     CNTR_NO                 = @[cntr_no]
AND     CNTR_CYC_NO             = @[cntr_cyc_no]
AND     DMDT_TRF_CD             = @[dmdt_trf_cd]
AND     DMDT_CHG_LOC_DIV_CD     = @[dmdt_chg_loc_div_cd]
AND     CHG_SEQ                 = @[chg_seq]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
