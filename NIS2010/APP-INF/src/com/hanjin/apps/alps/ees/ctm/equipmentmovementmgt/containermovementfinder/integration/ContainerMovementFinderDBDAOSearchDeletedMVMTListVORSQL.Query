<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOSearchDeletedMVMTListVORSQL">
			<desc><![CDATA[EES_CTM_0415 : 삭제된 Movement List 조회]]></desc>
			<sql><![CDATA[
SELECT   A.CNMV_CYC_NO,
         A.CNMV_CO_CD,
         A.CNMV_STS_CD,
         A.MVMT_CRE_TP_CD,
         A.ORG_YD_CD,
         A.DEST_YD_CD,
         TO_CHAR (A.CNMV_EVNT_DT, 'YYYY-MM-DD HH24:MI') AS CNMV_EVNT_DT,
         A.FDR_VSL_CD||A.FDR_SKD_VOY_NO||A.FDR_SKD_DIR_CD AS FDR_CD,
         A.BKG_KNT,
         A.BKG_NO,
         A.BL_NO,
         DECODE (A.FCNTR_FLG, 'Y', 'F', 'N', 'M', A.FCNTR_FLG) AS FCNTR_FLG,
         DECODE (A.OB_CNTR_FLG, 'Y', 'O', 'N', 'I', A.OB_CNTR_FLG) AS OB_CNTR_FLG,
         A.MVMT_EDI_MSG_TP_ID,
         A.BKG_CGO_TP_CD,
         A.CNTR_DMG_FLG,
         A.CNTR_DISP_FLG,
         A.IMDT_EXT_FLG,
         A.CNTR_RFUB_FLG,
         A.SPCL_CGO_FLG,
         A.VNDR_SEQ,
         B.VNDR_ABBR_NM,
         A.MVMT_TRSP_MOD_CD,
         TRIM (A.CHSS_NO) AS CHSS_NO,
         TRIM (A.MGST_NO) AS MGST_NO,
         TRIM (A.CNTR_SEAL_NO) AS CNTR_SEAL_NO,
         TRIM (A.WBL_NO) AS WBL_NO,
         TRIM (A.PKUP_NO) AS PKUP_NO,
         TO_CHAR (A.UPD_LOCL_DT, 'YYYY-MM-DD HH24:MI') AS UPD_LOCL_DT,
         TO_CHAR (A.CRE_LOCL_DT, 'YYYY-MM-DD HH24:MI') AS CRE_LOCL_DT,
         TO_CHAR (A.UPD_DT, 'YYYY-MM-DD HH24:MI') AS UPD_DT,
         TO_CHAR (A.CRE_DT, 'YYYY-MM-DD HH24:MI') AS CRE_DT,
         TO_CHAR (A.DELT_DT, 'YYYY-MM-DD HH24:MI') AS DELT_DT,
         A.DELT_USR_ID,
         C.USR_NM,
         A.CNMV_RMK
    FROM CTM_MVMT_CORR A,
         MDM_VENDOR B,
         COM_USER C
   WHERE A.VNDR_SEQ = B.VNDR_SEQ(+)
     AND A.DELT_USR_ID = C.USR_ID(+)
#if (${p_cntrno} != '')
     AND A.CNTR_NO = @[p_cntrno]||@[check_digit]
#end
#if (${p_date1} != '' && ${p_date2} != '')
     AND A.DELT_DT BETWEEN TO_DATE (@[p_date1], 'YYYY-MM-DD') AND TO_DATE (@[p_date2], 'YYYY-MM-DD') + 0.99999
#end
ORDER BY A.DELT_DT			]]></sql>
			<params>
				<param name="p_cntrno" type="12" value="" out="N"/>
				<param name="check_digit" type="12" value="" out="N"/>
				<param name="p_date1" type="12" value="" out="N"/>
				<param name="p_date2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
