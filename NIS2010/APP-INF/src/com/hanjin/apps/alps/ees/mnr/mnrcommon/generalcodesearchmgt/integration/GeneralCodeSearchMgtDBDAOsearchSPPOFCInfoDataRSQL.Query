<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchSPPOFCInfoDataRSQL">
			<desc><![CDATA[searchSPPOFCInfoData]]></desc>
			<sql><![CDATA[
SELECT MCM.OFC_CD AS SPP_OFC_CD FROM MDM_CUSTOMER MCM,MNR_PARTNER MP
WHERE MCM.CUST_CNT_CD = MP.MNR_PRNR_CNT_CD
AND MCM.CUST_SEQ = MP.MNR_PRNR_SEQ
AND MP.SP_PTAL_ID = @[sp_ptal_id]
AND MP.MNR_GRP_TP_CD = 'DSP'
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="sp_ptal_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
