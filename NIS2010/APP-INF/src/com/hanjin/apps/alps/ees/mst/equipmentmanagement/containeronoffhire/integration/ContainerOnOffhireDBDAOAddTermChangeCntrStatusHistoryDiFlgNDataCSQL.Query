<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAOAddTermChangeCntrStatusHistoryDiFlgNDataCSQL">
			<desc><![CDATA[AddTermChangeCntrStatusHistoryDiFlgNData]]></desc>
			<sql><![CDATA[
INSERT INTO MST_CNTR_STS_HIS
(
     CNTR_NO
    ,CNTR_STS_SEQ
    ,CO_CD
    ,YD_CD
    ,LOC_CD
    ,SCC_CD
    ,LCC_CD
    ,ECC_CD
    ,RCC_CD
    ,AGMT_CTY_CD
    ,AGMT_SEQ
    ,CNTR_STS_CD
    ,OFC_CD
    ,CNTR_STS_EVNT_DT
    ,CNTR_PKUP_CHG_AMT
    ,CURR_CD
    ,RNTL_CHG_FREE_DYS
    ,CNTR_DIR_ITCHG_FEE_AMT
    ,DIR_ITCHG_VNDR_SEQ   
    ,CNTR_LSTM_CNG_FLG
    ,PRNR_YD_CD
    ,PRNR_STS_SEQ
    ,CNMV_STS_CD
    ,CNTR_FULL_FLG
    ,CNTR_MIN_ONH_DYS
    ,CRE_USR_ID
    ,CRE_DT
    ,UPD_USR_ID
    ,UPD_DT    
)
SELECT 
    @[cntr_no] AS CNTR_NO,
    MST_CNTR_STS_HIS_SEQ.NEXTVAL AS CNTR_STS_SEQ,
    'H' AS CO_CD,
    @[yd_cd] YD_CD,    
    @[loc_cd] LOC_CD,
    @[scc_cd] SCC_CD,
    @[lcc_cd] LCC_CD,
    @[ecc_cd] ECC_CD,
    @[rcc_cd] RCC_CD,
    DECODE(LEVEL,1,@[cur_agmt_cty_cd],2,@[aft_agmt_cty_cd]) AS AGMT_CTY_CD,
    DECODE(LEVEL,1,@[cur_agmt_seq],2,@[aft_agmt_seq]) AS AGMT_SEQ, 
    CASE 
         WHEN 'OW' = @[aft_lstm_cd] THEN
            DECODE(LEVEL,1,'LSO',2,'OWN')
         WHEN 'OW' != @[aft_lstm_cd] THEN 
            DECODE(LEVEL,1,'LSO',2,'LSI')
    END CNTR_STS_CD,  
    @[ofc_cd]     AS OFC_CD,
    TO_DATE(@[act_dt],'YYYYMMDD') AS CNTR_STS_EVNT_DT,        -- Activity Date--
    CASE 
         WHEN @[di_flag] = 'N' THEN
            DECODE(LEVEL,1,NULL,2,(CASE WHEN TO_NUMBER(@[cntr_pkup_psv_amt]) > 0 THEN
                                            @[cntr_pkup_psv_amt]
                                        WHEN TO_NUMBER(@[cntr_pkup_ngv_amt]) > 0 THEN
                                            TO_CHAR(TO_NUMBER(@[cntr_pkup_ngv_amt])*-1)
                                        END
                                  )
                  )
    END CNTR_PKUP_CHG_AMT,           
    'USD' AS CURR_CD,
    CASE 
         WHEN 'OW' = @[aft_lstm_cd] THEN
            DECODE(LEVEL,1,NULL,2,@[rntl_chg_free_dys])
         WHEN 'OW' != @[aft_lstm_cd] THEN 
            DECODE(LEVEL,1,NULL,2,@[rntl_chg_free_dys])
    END RNTL_CHG_FREE_DYS, 
    CASE 
         WHEN 'OW' = @[aft_lstm_cd] THEN
            DECODE(LEVEL,1,NULL,2,NULL)
         WHEN 'OW' != @[aft_lstm_cd] THEN 
            DECODE(LEVEL,1,NULL,2,NULL)  
    END CNTR_DIR_ITCHG_FEE_AMT, 
    CASE 
         WHEN 'OW' = @[aft_lstm_cd] THEN
            DECODE(LEVEL,1,NULL,2,NULL)
         WHEN 'OW' != @[aft_lstm_cd] THEN 
            DECODE(LEVEL,1,NULL,2,NULL)         
    END DIR_ITCHG_VNDR_SEQ, 
    'Y' AS CNTR_LSTM_CNG_FLG,       
    CASE 
            WHEN @[di_flag] = 'N' THEN 
				DECODE(LEVEL,1, NVL((SELECT  /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */ A.YD_CD
                                 FROM    MST_CNTR_STS_HIS A
                                 WHERE   A.CNTR_NO = @[cntr_no] 
                                 AND     A.CNTR_STS_CD = 'LSI'
                                 AND     ROWNUM =1),@[yd_cd])
                            ,2,NULL)
    END PRNR_YD_CD, 
    CASE 
      WHEN @[di_flag] = 'N' THEN 
				DECODE(LEVEL,1, NVL((SELECT  /*+ INDEX_DESC(A XAK1MST_CNTR_STS_HIS) */ A.CNTR_STS_SEQ
                                 FROM    MST_CNTR_STS_HIS A
                                 WHERE   A.CNTR_NO = @[cntr_no]
                                 AND     A.CNTR_STS_CD = 'LSI'
                                 AND     ROWNUM =1), @[cntr_sts_seq])
                            ,2,NULL)
    END PRNR_STS_SEQ,     
    @[cnmv_sts_cd] AS CNMV_STS_CD,
    @[cntr_full_flg] AS CNTR_FULL_FLG,           
    CASE 
         WHEN 'OW' = @[aft_lstm_cd] THEN
            DECODE(LEVEL,1,NULL,2,@[cntr_min_onh_dys])
         WHEN 'OW' != @[aft_lstm_cd] THEN 
            DECODE(LEVEL,1,NULL,2,@[cntr_min_onh_dys])         
    END CNTR_MIN_ONH_DYS,  
    @[cre_usr_id]     AS CRE_USR_ID,
    SYSDATE AS CRE_DT,
    @[upd_usr_id]      AS UPD_USR_ID,
    SYSDATE AS UPD_DT
FROM DUAL
CONNECT BY LEVEL <=2			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="cur_agmt_cty_cd" type="12" value="" out="N"/>
				<param name="aft_agmt_cty_cd" type="12" value="" out="N"/>
				<param name="cur_agmt_seq" type="12" value="" out="N"/>
				<param name="aft_agmt_seq" type="12" value="" out="N"/>
				<param name="aft_lstm_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="act_dt" type="12" value="" out="N"/>
				<param name="di_flag" type="12" value="" out="N"/>
				<param name="cntr_pkup_psv_amt" type="12" value="" out="N"/>
				<param name="cntr_pkup_ngv_amt" type="12" value="" out="N"/>
				<param name="rntl_chg_free_dys" type="12" value="" out="N"/>
				<param name="cntr_sts_seq" type="12" value="" out="N"/>
				<param name="cnmv_sts_cd" type="12" value="" out="N"/>
				<param name="cntr_full_flg" type="12" value="" out="N"/>
				<param name="cntr_min_onh_dys" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
