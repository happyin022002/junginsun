<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanOptiExecuteDBDAOInsertRepoPlanIdCSQL">
			<desc><![CDATA[eqr_eq_repo_pln 테이블에 신규 repo plan id 입력]]></desc>
			<sql><![CDATA[
INSERT INTO EQR_EQ_REPO_PLN
            (  REPO_PLN_ID 
              ,SCNR_ID
              ,INCL_ONH_FLG
              ,INCL_OFFH_FLG	
			  ,REPO_PLN_AUTO_GEN_FLG 
              ,SIM_PLN_ID
              ,REPO_PLN_RMK
              ,DELT_FLG
              ,CRE_USR_ID
              ,CRE_DT
              ,UPD_USR_ID
              ,UPD_DT
              ,INCL_CNTR_TPSZ_CTNT
              ,ENG_RUN_ST_YRWK
              ,ENG_RUN_END_YRWK
              ,SIM_PLN_SNS_CD
              ,SIM_PLN_SNS_OBJ_CD
            )
            VALUES
            (  @[repo_plan_id]
              ,@[scnr_id]
              ,@[incl_onh_flg]
              ,@[incl_offh_flg]
              ,@[repo_plan_auto_gen_flg]
              ,@[sim_plan_id]
              ,@[repo_plan_rmk]
              ,'N'
              ,@[cre_usr_id]
              ,SYSDATE
              ,@[upd_usr_id]
              ,SYSDATE
              ,@[incl_tpsz_ctnt]
              ,@[eng_run_st_yrwk]
              ,@[eng_run_end_yrwk]
              ,@[sim_pln_sns_cd]
              ,@[sim_pln_sns_obj_cd]
             )			]]></sql>
			<params>
				<param name="repo_plan_id" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="incl_onh_flg" type="12" value="" out="N"/>
				<param name="incl_offh_flg" type="12" value="" out="N"/>
				<param name="repo_plan_auto_gen_flg" type="12" value="" out="N"/>
				<param name="sim_plan_id" type="12" value="" out="N"/>
				<param name="repo_plan_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="incl_tpsz_ctnt" type="12" value="" out="N"/>
				<param name="eng_run_st_yrwk" type="12" value="" out="N"/>
				<param name="eng_run_end_yrwk" type="12" value="" out="N"/>
				<param name="sim_pln_sns_cd" type="12" value="" out="N"/>
				<param name="sim_pln_sns_obj_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
