<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAOSearchAfterBookingExptClrRqstRSQL">
			<desc><![CDATA[ChargeAmountDiscountMgtDBDAOSearchAfterBookingExptClrRqst]]></desc>
			<sql><![CDATA[
SELECT NVL(C.BKG_NO, B.BKG_NO) BKG_NO, 
       NVL(C.CNTR_NO, B.CNTR_NO) CNTR_NO,
       NVL(C.DMDT_CHG_STS_CD, A.DMDT_CHG_STS_CD) DMDT_CHG_STS_CD,
       TO_CHAR(C.CGOR_DT,'YYYYMMDD') CGOR_DT, 
       TO_CHAR(C.MCNTR_RTN_DT,'YYYYMMDD') MCNTR_RTN_DT
FROM DMT_AFT_BKG_EXPT_CLR_RQST C, DMT_CHG_CALC A, DMT_CHG_BKG_CNTR B
WHERE C.AFT_EXPT_DAR_NO(+) = @[aft_expt_dar_no]
AND C.BKG_NO(+) = B.BKG_NO
AND C.CNTR_NO(+) = B.CNTR_NO
AND A.SYS_AREA_GRP_ID = B.SYS_AREA_GRP_ID
AND A.CNTR_NO = B.CNTR_NO
AND A.CNTR_CYC_NO = B.CNTR_CYC_NO
AND B.BKG_NO = @[bkg_no]
AND A.DMDT_TRF_CD = @[dmdt_trf_cd]
AND A.DMDT_CHG_STS_CD = 'L'			]]></sql>
			<params>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
