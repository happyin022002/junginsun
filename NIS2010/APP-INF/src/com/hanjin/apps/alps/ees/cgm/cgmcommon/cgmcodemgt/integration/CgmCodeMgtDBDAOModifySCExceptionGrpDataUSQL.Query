<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOModifySCExceptionGrpDataUSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
UPDATE CGM_SC_EXPT_LIST SET 
	EFF_DT 				= TO_DATE(@[eff_dt],'YYYY-MM-DD')
,	EXP_DT 				= TO_DATE(@[exp_dt],'YYYY-MM-DD')
,	LOC_CD 				= @[loc_cd]
,	CHSS_CNTR_CGO_TP_CD = @[chss_cntr_cgo_tp_cd]
,	CMDT_CD				= @[cmdt_cd]
,	CUST_CNT_CD 		= @[cust_cnt_cd]
,	CUST_SEQ 			= @[cust_seq]
,	USA_SC_EXPT_RMK 	= @[usa_sc_expt_rmk]
,	UPD_USR_ID 			= @[upd_usr_id]
,	UPD_DT 				= NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), SYSDATE)
,   FT_DYS              = @[ft_dys]
WHERE	PROP_NO 		= @[prop_no]
	AND	SC_EXPT_VER_SEQ = @[sc_expt_ver_seq]
	AND	SC_EXPT_GRP_SEQ = @[sc_expt_grp_seq]			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="chss_cntr_cgo_tp_cd" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="usa_sc_expt_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="ft_dys" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
