<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOGetOldBkgNoForGateNewRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT BO.BKG_NO
  FROM BKG_CONTAINER BC, BKG_BOOKING BO
 WHERE BC.CNTR_NO = @[cntr_no]
   AND BC.CNMV_CYC_NO = 9999
   AND SYSDATE - BC.CRE_DT < 60
   AND BC.BKG_NO = BO.BKG_NO
   AND NVL (BO.BKG_STS_CD, '') <> 'S'
   AND NVL (BO.BKG_STS_CD, '') <> 'X'
   AND NVL (BO.BKG_STS_CD, '') <> DECODE (@[gate_io_cd], 'AE', 'A', 'UV', 'A', 'X')
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="gate_io_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
