<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanSensitivityAnalysisDBDAOSearchPlanSensivityLimit04RSQL">
			<desc><![CDATA[On-Hire/Off-Hire Limit 민감도 조회]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(PLN_YRWK,1,4) || '-' || SUBSTR(PLN_YRWK,5,2) WEEK    	
	   , @[sens_typ] SENSITY    	                                    
	   , @[obj_txt] OBJ    	                                        
	   , '' LANE    	                                        
	   , '' VVD
#if (${limitObj} == 'O') 
       , '' FM_LOC	                                    
       , B.ECC_CD TO_LOC	 
#elseif (${limitObj} == 'F') 
	   , B.ECC_CD FM_LOC	                              	
	   , '' TO_LOC	  
#end    	                                        
       , B.CNTR_TPSZ_CD ts_type
#if (${limitObj} == 'O') 
	   , SUM (AVAL_CNTR_QTY) CURR_LIMIT 
#elseif (${limitObj} == 'F') 
	   , SUM (AVAL_CNTR_QTY) CURR_LIMIT 
#end    	                                    
       , SUM(ONF_HIR_BND_QTY) SHADOW_PRC    	                
       , SUM(RNG_MIN_QTY) LIMIT_LANG    	                    
       , SUM(RNG_MAX_QTY) LIMIT_LANG1    	                    
FROM EQR_EQ_REPO_PLN A    	                                    
       , EQR_EQ_BND_SHDW B    	                                
       , (SELECT ECC_CD, LCC_CD, RCC_CD FROM EQR_ECC_MST) C     
	   , EQR_SHRT_TERM_ONH_COND D                                
WHERE A.REPO_PLN_ID = B.REPO_PLN_ID    	                    
AND B.ECC_CD = C.ECC_CD    	                            
AND A.REPO_PLN_ID = @[repo_pln_id]    	                            
AND B.ONF_HIR_DIV_CD = @[limitObj]		                       		
AND B.CNTR_TPSZ_CD IN (${arrtpszcd}) 
#if (${limitobj} == 'O') 
AND B.PLN_YRWK BETWEEN @[tostartprd] AND @[toendprd]  
#elseif (${limitobj} == 'F') 
AND B.PLN_YRWK BETWEEN @[fmstartprd] AND @[fmendprd] 
#end	       	    
AND D.ECC_CD  = B.ECC_CD                                  
AND D.CNTR_TPSZ_CD = B.CNTR_TPSZ_CD  

#if (${limitObj} == 'O') 
#if (${toecccd} != '') 
     #if (${totype} == 'R')
     AND C.RCC_CD IN (${arrtoecccd}) 
	 #end
     #if (${totype} == 'L')
     AND C.LCC_CD IN (${arrtoecccd})
	 #end
     #if (${totype} == 'E')
     AND C.ECC_CD IN (${arrtoecccd})
	 #end
#end   
#elseif (${limitObj} == 'F') 
#if (${fmecccd} != '') 
   	#if (${fmtype} == 'R')
    AND C.RCC_CD IN (${arrfmecccd}) 
	#end
    #if (${fmtype} == 'L')
    AND C.LCC_CD IN (${arrfmecccd})
	#end
    #if (${fmtype} == 'E')
    AND C.ECC_CD IN (${arrfmecccd})
	#end
#end
#end                    
GROUP BY B.PLN_YRWK, B.ECC_CD, B.CNTR_TPSZ_CD    	                
ORDER BY B.PLN_YRWK, B.ECC_CD, B.CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="sens_typ" type="12" value="" out="N"/>
				<param name="obj_txt" type="12" value="" out="N"/>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="limitObj" type="12" value="" out="N"/>
				<param name="tostartprd" type="12" value="" out="N"/>
				<param name="toendprd" type="12" value="" out="N"/>
				<param name="fmstartprd" type="12" value="" out="N"/>
				<param name="fmendprd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
