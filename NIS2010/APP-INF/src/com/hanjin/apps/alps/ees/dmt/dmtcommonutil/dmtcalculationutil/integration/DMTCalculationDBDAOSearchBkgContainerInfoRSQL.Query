<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchBkgContainerInfoRSQL">
			<desc><![CDATA[SearchBkgContainerInfo]]></desc>
			<sql><![CDATA[
SELECT BB.BL_NO 				BL_NO
      ,BB.POR_CD 				POR_CD
      ,NVL (RL.CONTI_CD, ' ') 	POR_CONTI_CD
      ,NVL (RL.CNT_CD, ' ') 	POR_CNT_CD
      ,NVL (RL.RGN_CD, ' ') 	POR_RGN_CD
      ,NVL (RL.STE_CD, ' ') 	POR_STE_CD
      ,NVL (HRD.ATTR_CTNT6,BB.POD_CD) 				POD_CD
      ,BB.POL_CD 				POL_CD
      ,NVL (LL.CONTI_CD, ' ') 	POL_CONTI_CD
      ,NVL (LL.CNT_CD, ' ') 	POL_CNT_CD
      ,NVL (LL.RGN_CD, ' ') 	POL_RGN_CD
      ,NVL (LL.STE_CD, ' ') 	POL_STE_CD
      ,NVL (HRD.ATTR_CTNT6,BB.DEL_CD) 				DEL_CD
      ,NVL (EL.CONTI_CD, ' ') 	DEL_CONTI_CD
      ,NVL (EL.CNT_CD, ' ') 	DEL_CNT_CD
      ,NVL (EL.RGN_CD, ' ') 	DEL_RGN_CD
      ,NVL (EL.STE_CD, ' ') 	DEL_STE_CD
      ,SUBSTR (@[fm_yd_cd] ,1 ,5) YRD_CD
      ,NVL (ML.CONTI_CD, ' ') 	YRD_CONTI_CD
      ,NVL (ML.CNT_CD, ' ') 	YRD_CNT_CD
      ,NVL (ML.RGN_CD, ' ') 	YRD_RGN_CD
      ,NVL (ML.STE_CD, ' ') 	YRD_STE_CD
      ,CC.DCGO_FLG 				DCGO_FLG
      ,CC.RC_FLG 				RC_FLG
      ,CC.AWK_CGO_FLG 			AWK_CGO_FLG
      ,CC.RD_CGO_FLG 			RD_CGO_FLG
      ,CC.BB_CGO_FLG 			BB_CGO_FLG
      ,CC.SOC_FLG 				SOC_FLG
      ,CC.CNTR_PRT_FLG 			CNTR_PRT_FLG
      ,CC.ADV_SHTG_CD 			ADV_SHTG_CD
      ,BB.OB_SLS_OFC_CD 		OB_SLS_OFC_CD
      ,SV.OFC_N3RD_LVL_CD 		SAL_RHQ
      ,TRIM (BB.SC_NO) 			SC_NO
      ,BB.RFA_NO 				RFA_NO
      ,BB.CMDT_CD 				CMDT_CD
      ,BB.REP_CMDT_CD 			REP_CMDT_CD
      ,BB.PST_RLY_PORT_CD 		PST_RLY_PORT_CD
      ,CC.DE_TERM_CD 			DE_TERM_CD
      ,CC.RCV_TERM_CD 			RCV_TERM_CD
      ,BB.PRE_RLY_PORT_CD 		PRE_RLY_PORT_CD
      ,BB.RCV_TERM_CD           BB_RCV_TERM_CD
      ,BB.DE_TERM_CD            BB_DE_TERM_CD
  FROM BKG_CONTAINER CC
      ,BKG_BOOKING BB
	  ,MDM_LOCATION RL
      ,MDM_LOCATION LL
      ,MDM_LOCATION EL
      ,MDM_LOCATION ML
      ,DMT_OFC_LVL_V SV
	  ,DMT_HRD_CDG_CTNT HRD
 WHERE CC.BKG_NO = @[bkg_no]
   AND CC.CNTR_NO = @[cntr_no]
   AND BB.BKG_NO = CC.BKG_NO
   AND BB.POR_CD = RL.LOC_CD
   AND BB.POL_CD = LL.LOC_CD
   AND NVL (HRD.ATTR_CTNT6, BB.DEL_CD) = EL.LOC_CD
   AND BB.BKG_STS_CD <> 'S'
#if (${bkg_sts_cd} != 'CANCEL') 
   AND BB.BKG_STS_CD <> 'X'   
#end
   AND BB.BKG_CGO_TP_CD = 'F'
   AND ML.LOC_CD = SUBSTR (@[fm_yd_cd],1,5)
   AND BB.OB_SLS_OFC_CD = SV.OFC_N8TH_LVL_CD(+)
   AND HRD.HRD_CDG_ID(+) = 'SHIP_BACK_CDG'
   AND HRD.ATTR_CTNT1(+) = @[svr_id]
   AND CC.BKG_NO = HRD.ATTR_CTNT2(+)
   AND CC.CNTR_NO = HRD.ATTR_CTNT3(+)
   AND HRD.ATTR_CTNT4(+) = @[cntr_cyc_no]
   AND HRD.ATTR_CTNT5(+) = @[dmdt_trf_cd]			]]></sql>
			<params>
				<param name="fm_yd_cd" type="12" value="FIULV" out="N"/>
				<param name="bkg_no" type="12" value="RTMZA090030" out="N"/>
				<param name="cntr_no" type="12" value="HJCU8280322" out="N"/>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
