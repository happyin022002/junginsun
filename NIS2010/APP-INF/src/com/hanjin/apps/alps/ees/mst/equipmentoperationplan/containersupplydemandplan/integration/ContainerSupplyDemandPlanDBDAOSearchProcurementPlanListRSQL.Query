<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSupplyDemandPlanDBDAOSearchProcurementPlanListRSQL">
			<desc><![CDATA[SearchProcurementPlanList]]></desc>
			<sql><![CDATA[
SELECT
    A.PLN_YR, 
	A.BSE_YRMON AS H_BSE_YRMON, 
	A.CNTR_PROCU_PLN_CD, 
	A.D2_QTY,
	A.D4_QTY,
	A.D5_QTY,
	A.D7_QTY,
	A.R2_QTY,
	A.R4_QTY,
	A.R5_QTY,
	A.O2_QTY,
	A.O4_QTY,
	A.F2_QTY,
	A.F4_QTY
FROM MST_CNTR_PROCU_PLN A
WHERE A.PLN_YR = @[input_pln_yr]
#if (${input_sw} == 'S')
AND A.CNTR_PROCU_PLN_CD = @[cntr_procu_pln_cd]
#end
ORDER BY A.CNTR_PROCU_PLN_CD			]]></sql>
			<params>
				<param name="input_pln_yr" type="12" value="" out="N"/>
				<param name="cntr_procu_pln_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
