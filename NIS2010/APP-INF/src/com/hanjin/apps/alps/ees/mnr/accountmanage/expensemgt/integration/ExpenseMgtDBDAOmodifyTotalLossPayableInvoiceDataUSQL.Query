<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExpenseMgtDBDAOmodifyTotalLossPayableInvoiceDataUSQL">
			<desc><![CDATA[Total Loss 에서 inv_rgst_no 를 저장한다.
-- 2014.11 10만불 결제관련]]></desc>
			<sql><![CDATA[
UPDATE MNR_PAY_INV_WRK
   SET INV_RGST_NO = @[inv_rgst_no]
     , UPD_USR_ID  = @[upd_usr_id]
     , UPD_DT      = SYSDATE
#if (${file_seq} != '')
	,FILE_SEQ = @[file_seq]
#end
 WHERE PAY_INV_SEQ = @[pay_inv_seq]			]]></sql>
			<params>
				<param name="inv_rgst_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="file_seq" type="12" value="" out="N"/>
				<param name="pay_inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
