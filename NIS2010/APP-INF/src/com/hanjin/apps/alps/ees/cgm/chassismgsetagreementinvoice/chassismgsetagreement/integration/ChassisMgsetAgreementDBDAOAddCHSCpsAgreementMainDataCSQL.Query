<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOAddCHSCpsAgreementMainDataCSQL">
			<desc><![CDATA[NP(ZP) Agreement 정보를 CGM_AGREEMENT 테이블에 create 한다.
-- 2014.11 10만불 결제관련]]></desc>
			<sql><![CDATA[
INSERT INTO CGM_AGREEMENT 
(
    AGMT_OFC_CTY_CD, 
    AGMT_SEQ, 
    AGMT_VER_NO, 
    EQ_KND_CD, 
    LST_VER_FLG, 
    AGMT_ISS_OFC_CD, 
    AGMT_REF_NO, 
    CURR_CD, 
    VNDR_SEQ, 
    PAY_TERM_DYS,
    AGMT_LSTM_CD,
    CHSS_POOL_CD,
    DIFF_RMK,
	AGMT_EFF_DT,
	AGMT_EXP_DT,
    EFF_DT,
    EXP_DT,
    CTRT_NO,
    GW_UQ_DOC_TIT_NM,
    GW_UQ_DOC_NO,
    CRE_USR_ID,
    CRE_DT,
	UPD_USR_ID,
	UPD_DT
) VALUES (
    @[agmt_ofc_cty_cd],
    @[agmt_seq],
    @[agmt_ver_no],
    'Z',
    @[lst_ver_flg],
    @[agmt_iss_ofc_cd],
    @[agmt_ref_no],
    @[curr_cd],
    @[vndr_seq],
    @[pay_term_dys],
    @[agmt_lstm_cd],
    @[chss_pool_cd],
    @[diff_rmk],
	TO_DATE(@[agmt_eff_dt],'YYYYMMDD'),
	TO_DATE(@[agmt_exp_dt],'YYYYMMDD'),
    TO_DATE(@[agmt_eff_dt],'YYYYMMDD'),
    TO_DATE(@[agmt_exp_dt],'YYYYMMDD'),
    @[ctrt_no],
    @[gw_uq_doc_tit_nm],
    @[gw_uq_doc_no],
    @[cre_usr_id],
    SYSDATE,
	@[upd_usr_id],
	SYSDATE
)			]]></sql>
			<params>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
				<param name="lst_ver_flg" type="12" value="" out="N"/>
				<param name="agmt_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="agmt_ref_no" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="pay_term_dys" type="12" value="" out="N"/>
				<param name="agmt_lstm_cd" type="12" value="" out="N"/>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="agmt_eff_dt" type="12" value="" out="N"/>
				<param name="agmt_exp_dt" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
				<param name="gw_uq_doc_tit_nm" type="12" value="" out="N"/>
				<param name="gw_uq_doc_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
