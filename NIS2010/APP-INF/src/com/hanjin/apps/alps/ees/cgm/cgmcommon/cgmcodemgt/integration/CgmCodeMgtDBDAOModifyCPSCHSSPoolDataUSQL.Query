<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOModifyCPSCHSSPoolDataUSQL">
			<desc><![CDATA[CPS Neutral Pool을 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE CGM_CPS_POOL_LIST
SET	
	CHSS_POOL_CD = @[chss_pool_cd], 
    CHSS_POOL_NM = @[chss_pool_nm], 
    VNDR_SEQ = @[vndr_seq], 
	UPD_USR_ID = @[upd_usr_id],
	UPD_DT = SYSDATE
WHERE CHSS_POOL_CD = @[chss_pool_cd]			]]></sql>
			<params>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
				<param name="chss_pool_nm" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
