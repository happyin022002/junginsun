<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOSearchBeforeBookingListByOfficeRSQL">
			<desc><![CDATA[SC & RFA Exception Inquiry (BeforeBooking, OFFICE) 조회쿼리]]></desc>
			<sql><![CDATA[
SELECT  /*+ ORDERED USE_NL(X TRF MN HDR CUST CUST2 COM_DTL COM_DTL2 COM_DTL3) */
        HDR.RFA_NO,
        COM_DTL.INTG_CD_VAL_DP_DESC   STATUS,
        X.RQST_SEQ,
        X.TARIFF,
        X.EFF_DT,
        X.EXP_DT,
        COM_DTL2.INTG_CD_VAL_DP_DESC || ' - ' || COM_DTL3.INTG_CD_VAL_DP_DESC   CNTRCGO,
        X.CVRG_SEQ,
        X.CNT_CD,
        X.RGN_CD,
        X.LOC_CD,
        X.FT_ADD_DYS,
        X.FT_TOT_DYS,
        X.XCLD_SAT_FLG,
        X.XCLD_SUN_FLG,
        X.XCLD_HOL_FLG,
        X.CURR_CD,
        X.ORG_DEST_MULTI,
        X.ORG_DEST_CONTI_CD,
        X.ORG_DEST_CNT_CD,
        X.ORG_DEST_RGN_CD,
        X.ORG_DEST_LOC_CD,
        X.FNL_DEST_CNT_CD,
        X.FNL_DEST_RGN_CD,
        X.FNL_DEST_LOC_CD,
        X.ACT_CUST_CNT_CD || LPAD(X.ACT_CUST_SEQ, 6, '0')   ACT_CUST_CD,
        CUST2.CUST_LGL_ENG_NM                               ACT_CUST_NM,
        X.RT_FLG,
        X.DAR_NO,
        X.VER_SEQ,
        TRF.RFA_EXPT_APRO_NO    APVL_NO,
        TRF.PROP_NO,
        MN.CTRT_CUST_CNT_CD || LPAD(MN.CTRT_CUST_SEQ, 6, '0')    CUST_CD,
        CUST.CUST_LGL_ENG_NM                                     CUST_NM
FROM   (
        SELECT  DISTINCT
                RQST_SEQ
        ,       TARIFF
        ,       EFF_DT
        ,       EXP_DT
        ,       CVRG_SEQ
        ,       CNT_CD
        ,       RGN_CD
        ,       LOC_CD
        ,       FT_ADD_DYS
        ,       FT_TOT_DYS
        ,       XCLD_SAT_FLG
        ,       XCLD_SUN_FLG
        ,       XCLD_HOL_FLG
        ,       CURR_CD
        ,       DECODE(CVRG_COUNT, 1, 'Single', 'Multi') ORG_DEST_MULTI
        ,       DECODE(CVRG_COUNT, 1, ORG_DEST_CONTI_CD, '') ORG_DEST_CONTI_CD
        ,       DECODE(CVRG_COUNT, 1, ORG_DEST_CNT_CD, '') ORG_DEST_CNT_CD
        ,       DECODE(CVRG_COUNT, 1, ORG_DEST_RGN_CD, '') ORG_DEST_RGN_CD
        ,       DECODE(CVRG_COUNT, 1, ORG_DEST_LOC_CD, '') ORG_DEST_LOC_CD
        ,       FNL_DEST_CNT_CD
        ,       FNL_DEST_RGN_CD
        ,       FNL_DEST_LOC_CD
        ,       ACT_CUST_CNT_CD
        ,       ACT_CUST_SEQ
        ,       RT_FLG
        ,       DAR_NO
        ,       VER_SEQ
        ,       DMDT_CNTR_TP_CD
        ,       DMDT_CGO_TP_CD
        ,       RFA_EXPT_MAPG_SEQ
        FROM   (
                SELECT  /*+ ORDERED USE_NL(TRF_DTL CVRG) */
                        TRF_DTL.RFA_RQST_DTL_SEQ RQST_SEQ
                ,       TRF_DTL.DMDT_TRF_CD TARIFF
                ,       TO_CHAR(TRF_DTL.EFF_DT, 'YYYY-MM-DD') EFF_DT
                ,       TO_CHAR(TRF_DTL.EXP_DT, 'YYYY-MM-DD') EXP_DT
                ,       CVRG.CVRG_CMB_SEQ CVRG_SEQ
                ,       CVRG.CVRG_CNT_CD CNT_CD
                ,       CASE WHEN CVRG.CVRG_CNT_CD IN ('CA', 'US') THEN CVRG.CVRG_STE_CD ELSE CVRG.CVRG_RGN_CD END RGN_CD
                ,       CVRG.CVRG_LOC_CD LOC_CD
                ,       TRF_DTL.ADD_DYS FT_ADD_DYS
                ,       TRF_DTL.TTL_DYS FT_TOT_DYS
                ,       TRF_DTL.XCLD_SAT_FLG
                ,       TRF_DTL.XCLD_SUN_FLG
                ,       TRF_DTL.XCLD_HOL_FLG
                ,       TRF_DTL.CURR_CD
                ,       CVRG.ORG_DEST_CONTI_CD
                ,       CVRG.ORG_DEST_CNT_CD
                ,       CASE WHEN CVRG.ORG_DEST_CNT_CD IN ('CA', 'US') THEN CVRG.ORG_DEST_STE_CD ELSE CVRG.ORG_DEST_RGN_CD END ORG_DEST_RGN_CD
                ,       CVRG.ORG_DEST_LOC_CD
                ,       TRF_DTL.FNL_DEST_CNT_CD
                ,       CASE WHEN TRF_DTL.FNL_DEST_CNT_CD IN ('CA', 'US') THEN TRF_DTL.FNL_DEST_STE_CD ELSE TRF_DTL.FNL_DEST_RGN_CD END FNL_DEST_RGN_CD
                ,       TRF_DTL.FNL_DEST_LOC_CD
                ,       TRF_DTL.ACT_CUST_CNT_CD
                ,       TRF_DTL.ACT_CUST_SEQ
                ,       TRF_DTL.RT_USE_FLG RT_FLG
                ,       TRF_DTL.RFA_EXPT_DAR_NO DAR_NO
                ,       LPAD(TRF_DTL.RFA_EXPT_VER_SEQ, 3, '0') VER_SEQ
                ,       TRF_DTL.DMDT_CNTR_TP_CD
                ,       TRF_DTL.DMDT_CGO_TP_CD
                ,       TRF_DTL.RFA_EXPT_MAPG_SEQ
                ,      (
                        SELECT  COUNT(CVRG_CMB_SEQ)
                        FROM    DMT_RFA_EXPT_CVRG_CMB
                        WHERE   RFA_EXPT_DAR_NO     = TRF_DTL.RFA_EXPT_DAR_NO
                        AND     RFA_EXPT_MAPG_SEQ   = TRF_DTL.RFA_EXPT_MAPG_SEQ
                        AND     RFA_EXPT_VER_SEQ    = TRF_DTL.RFA_EXPT_VER_SEQ
                        AND     RFA_RQST_DTL_SEQ    = TRF_DTL.RFA_RQST_DTL_SEQ
                       )  CVRG_COUNT
                FROM    DMT_RFA_EXPT_TRF_DTL TRF_DTL    -- 기준 집합 레벨
                    ,   DMT_RFA_EXPT_CVRG_CMB CVRG      -- 1집합
                    ,  (SELECT  LCT.CNT_CD, LCT.RGN_CD, LCT.STE_CD, LCT.LOC_CD, ROWNUM
                        FROM    MDM_YARD YARD
                            ,   MDM_LOCATION LCT
                        WHERE   YARD.DMDT_OFC_CD IN (
									#foreach( $ofc_cd in ${dmdt_ofc_list} )
										#if($velocityCount < $dmdt_ofc_list.size()) '$ofc_cd', #else '$ofc_cd' #end
									#end
								)
                        AND     YARD.LOC_CD = LCT.LOC_CD
                       )  X
                WHERE   TRF_DTL.DMDT_TRF_CD IN (
							#foreach( $trf_cd in ${trf_cd_list} )
								#if($velocityCount < $trf_cd_list.size()) '$trf_cd', #else '$trf_cd' #end
							#end
						)
                AND    (
                        (TRF_DTL.EFF_DT >= TO_DATE(@[fm_dt], 'YYYY-MM-DD') AND TRF_DTL.EFF_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999)
                         OR
                        (TRF_DTL.EXP_DT >= TO_DATE(@[fm_dt], 'YYYY-MM-DD') AND TRF_DTL.EXP_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999)
                         OR
                        (TRF_DTL.EFF_DT <= TO_DATE(@[fm_dt], 'YYYY-MM-DD') AND TRF_DTL.EXP_DT >= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999)
                         OR
                        (TRF_DTL.EFF_DT >= TO_DATE(@[fm_dt], 'YYYY-MM-DD') AND TRF_DTL.EXP_DT <= TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999)
                       )
                AND     CVRG.ROWID = (SELECT  /*+ INDEX_ASC(DMT_RFA_EXPT_CVRG_CMB XPKDMT_RFA_EXPT_CVRG_CMB) */
                                              ROWID
                                      FROM    DMT_RFA_EXPT_CVRG_CMB
                                      WHERE   RFA_EXPT_DAR_NO = TRF_DTL.RFA_EXPT_DAR_NO
                                      AND     RFA_EXPT_MAPG_SEQ = TRF_DTL.RFA_EXPT_MAPG_SEQ
                                      AND     RFA_EXPT_VER_SEQ = TRF_DTL.RFA_EXPT_VER_SEQ
                                      AND     RFA_RQST_DTL_SEQ = TRF_DTL.RFA_RQST_DTL_SEQ
                                      AND     ROWNUM = 1
                                     )
                AND     CVRG.CVRG_CNT_CD = X.CNT_CD
                AND     CVRG.CVRG_RGN_CD = DECODE(CVRG.CVRG_RGN_CD, ' ', ' ', DECODE(X.CNT_CD, 'CA', ' ', 'US', ' ', X.RGN_CD))
                AND     CVRG.CVRG_STE_CD = DECODE(CVRG.CVRG_STE_CD, ' ', ' ', DECODE(X.CNT_CD, 'CA', X.CNT_CD, 'US', X.STE_CD, ' '))
                AND     CVRG.CVRG_LOC_CD = DECODE(CVRG.CVRG_LOC_CD, ' ', ' ', X.LOC_CD)
               )
       )  X,
        DMT_RFA_EXPT_TRF TRF,           -- 1집합
        PRI_RP_MN MN,                   -- 1집합
        PRI_RP_HDR HDR,                 -- 1집합
        MDM_CUSTOMER CUST,              -- 1집합
        MDM_CUSTOMER CUST2,             -- 1집합
        COM_INTG_CD_DTL COM_DTL,        -- 1집합
        COM_INTG_CD_DTL COM_DTL2,       -- 1집합
        COM_INTG_CD_DTL COM_DTL3        -- 1집합
WHERE   HDR.PROP_NO = MN.PROP_NO
AND     MN.ROWID = (SELECT /*+ INDEX_DESC(PRI_RP_MN XPKPRI_RP_MN) */
                           ROWID
                    FROM   PRI_RP_MN
                    WHERE  PROP_NO = TRF.PROP_NO
                    AND    ROWNUM = 1
                   )

#if(${cust_cd} != '' && ${act_cust_cd} != '')
AND 	MN.CTRT_CUST_CNT_CD 	= CUST.CUST_CNT_CD(+)
AND 	MN.CTRT_CUST_SEQ 		= CUST.CUST_SEQ(+)
#else
AND 	MN.CTRT_CUST_CNT_CD 	= CUST.CUST_CNT_CD
AND 	MN.CTRT_CUST_SEQ 		= CUST.CUST_SEQ
#end

#if(${cust_cd} != '' && ${act_cust_cd} == '')
AND 	MN.CTRT_CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND MN.CTRT_CUST_SEQ = SUBSTR(@[cust_cd], 3)
#elseif(${cust_cd} == '' && ${act_cust_cd} != '')
AND 	X.ACT_CUST_CNT_CD = SUBSTR(@[act_cust_cd], 0, 2) AND X.ACT_CUST_SEQ = SUBSTR(@[act_cust_cd], 3)
#elseif(${cust_cd} != '' && ${act_cust_cd} != '')
AND 	(
			(MN.CTRT_CUST_CNT_CD = SUBSTR(@[cust_cd], 0, 2) AND MN.CTRT_CUST_SEQ = SUBSTR(@[cust_cd], 3))
			OR
			(X.ACT_CUST_CNT_CD = SUBSTR(@[act_cust_cd], 0, 2) AND X.ACT_CUST_SEQ = SUBSTR(@[act_cust_cd], 3))
		)
#end

AND     X.ACT_CUST_CNT_CD         = CUST2.CUST_CNT_CD(+)
AND     X.ACT_CUST_SEQ            = CUST2.CUST_SEQ(+)
AND     TRF.DMDT_EXPT_RQST_STS_CD = COM_DTL.INTG_CD_VAL_CTNT
AND     COM_DTL.INTG_CD_ID        = 'CD02069'
AND     X.DMDT_CNTR_TP_CD         = COM_DTL2.INTG_CD_VAL_CTNT
AND     COM_DTL2.INTG_CD_ID       = 'CD02053'
AND     X.DMDT_CGO_TP_CD          = COM_DTL3.INTG_CD_VAL_CTNT
AND     COM_DTL3.INTG_CD_ID       = 'CD01963'
AND     TRF.DMDT_EXPT_RQST_STS_CD IN (
			#foreach( $sts_cd in ${sts_cd_list} )
				#if($velocityCount < $sts_cd_list.size()) '$sts_cd', #else '$sts_cd' #end
			#end
		)
AND     TRF.RFA_EXPT_DAR_NO       = X.DAR_NO
AND     TRF.RFA_EXPT_MAPG_SEQ     = X.RFA_EXPT_MAPG_SEQ
AND     TRF.RFA_EXPT_VER_SEQ      = TO_NUMBER(X.VER_SEQ)
ORDER BY HDR.RFA_NO, TRF.PROP_NO, X.VER_SEQ, X.RQST_SEQ, X.CVRG_SEQ			]]></sql>
			<params>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="cust_cd" type="12" value="" out="N"/>
				<param name="act_cust_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
