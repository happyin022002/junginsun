<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAODomesticManualProccesRSQL">
			<desc><![CDATA[Domestic 에서 CO 이후에 EN/TN 판정이 나는경우를 방지하기 위한처리]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC(B XUK1CTM_MOVEMENT) */
                B.MVMT_STS_CD,B.ORG_YD_CD, SUBSTR(B.ORG_YD_CD,1,5),
                MST_LOC_FNC(SUBSTR(B.ORG_YD_CD,1,5),'SCC') SCC_CD ,MST_LOC_FNC(SUBSTR(B.ORG_YD_CD,1,5),'LCC') LCC_CD,
                MST_LOC_FNC(SUBSTR(B.ORG_YD_CD,1,5),'ECC') ECC_CD ,MST_LOC_FNC(SUBSTR(B.ORG_YD_CD,1,5),'RCC') RCC_CD,
                MST_LOC_FNC(SUBSTR(@[event_yard],1,5),'LCC') ORG_LCC_CD, MST_LOC_FNC(SUBSTR(@[event_yard],1,5),'SCC') ORG_SCC_CD,
                 MST_LOC_FNC(SUBSTR(@[dest_yard],1,5),'LCC') DEST_LCC_CD, MST_LOC_FNC(SUBSTR(@[dest_yard],1,5),'SCC') DEST_SCC_CD,
                DECODE(B.BKG_NO,null,'TCHIDUMMYBKG',B.BKG_NO) BKG_NO
        FROM    CTM_MOVEMENT B
        WHERE   B.CNTR_NO   = MST_COMMON_PKG.MST_CNTR_NO_FNC(@[cntr_no])
        AND     ROWNUM      = 1			]]></sql>
			<params>
				<param name="event_yard" type="12" value="" out="N"/>
				<param name="dest_yard" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
