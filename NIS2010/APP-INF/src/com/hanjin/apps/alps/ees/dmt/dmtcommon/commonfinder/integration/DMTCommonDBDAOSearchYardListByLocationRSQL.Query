<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchYardListByLocationRSQL">
			<desc><![CDATA[searchYardListByLocation]]></desc>
			<sql><![CDATA[
SELECT YD_CD, SUBSTR(YD_CD, 6,2) AS YD_CD2
FROM MDM_YARD
WHERE
 
##${yd_cd1}
#if ($yd_cd1.length() == 5)
	SUBSTR(YD_CD, 1,5) = @[yd_cd1]
#else
	YD_CD = @[yd_cd1]
#end

AND DELT_FLG = 'N'
AND LOC_CD IS NOT NULL
ORDER BY YD_CD





			]]></sql>
			<params>
				<param name="yd_cd1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
