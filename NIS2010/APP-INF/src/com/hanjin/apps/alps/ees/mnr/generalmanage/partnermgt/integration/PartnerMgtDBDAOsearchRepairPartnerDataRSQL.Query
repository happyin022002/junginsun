<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerMgtDBDAOsearchRepairPartnerDataRSQL">
			<desc><![CDATA[searchRepairPartnerData]]></desc>
			<sql><![CDATA[
SELECT
B.VNDR_SEQ
,C.MNR_PRNR_CRE_SEQ
,C.CTRL_OFC_CD
,C.MNR_GRP_TP_CD
,C.MNR_PRNR_TP_CD
,C.MNR_PRNR_KND_CD
,C.MNR_PRNR_KND_DTL_CD
,C.MNR_PRNR_STS_CD
,C.MNR_PRNR_CNT_CD
,C.MNR_PRNR_SEQ
,C.EDI_ID
,C.SP_PTAL_ID
,C.SP_PTAL_PWD
,C.MNR_PRNR_LGL_ENG_NM
,C.MNR_PRNR_LOCL_LANG_NM
,C.MNR_PRNR_ADDR
,C.MNR_BIL_TO_NM
,TO_CHAR(C.EFF_DT, 'yyyy-mm-dd') EFF_DT
,TO_CHAR(C.EXP_DT, 'yyyy-mm-dd') EXP_DT
,C.BANK_NM
,C.BANK_ACCT_NO
,C.PAY_MZD_CD
,C.PAY_TERM_DYS
,C.ZIP_CD
,C.OWNR_NM
,C.BZCT_NM
,C.BZTP_NM
,C.BIZ_RGST_NO
,C.MNR_SHOP_FLG
,C.MNR_PAYR_CNT_CD
,C.MNR_PAYR_SEQ
,C.MNR_PRNR_CAPI_AMT
,C.EMPE_KNT
,C.DPT_DESC
,C.MNR_PRNR_ABBR_NM
,C.INTL_PHN_NO
,C.PHN_NO
,C.INTL_FAX_NO
,C.FAX_NO
,C.MNR_PRNR_EML
,C.MNR_PRNR_RMK
,C.TRSM_MOD_CD
,C.FILE_SEQ
,B.VNDR_LGL_ENG_NM
,B.GEN_PAY_TERM_CD
,B.PAY_CURR_CD
,C.CRE_USR_ID
,TO_CHAR(C.CRE_DT, 'yyyy-mm-dd') CRE_DT
,C.UPD_USR_ID
,TO_CHAR(C.UPD_DT, 'yyyy-mm-dd') UPD_DT
FROM MDM_VENDOR B,
(
SELECT
A.MNR_PRNR_CRE_SEQ
,A.CTRL_OFC_CD
,A.MNR_GRP_TP_CD
,A.MNR_PRNR_TP_CD
,A.MNR_PRNR_KND_CD
,A.MNR_PRNR_KND_DTL_CD
,A.MNR_PRNR_STS_CD
,A.MNR_PRNR_CNT_CD
,A.MNR_PRNR_SEQ
,A.EDI_ID
,A.SP_PTAL_ID
,A.SP_PTAL_PWD
,A.MNR_PRNR_LGL_ENG_NM
,A.MNR_PRNR_LOCL_LANG_NM
,A.MNR_PRNR_ADDR
,A.MNR_BIL_TO_NM
,A.EFF_DT
,A.EXP_DT
,A.BANK_NM
,A.BANK_ACCT_NO
,A.PAY_MZD_CD
,A.PAY_TERM_DYS
,A.ZIP_CD
,A.OWNR_NM
,A.BZCT_NM
,A.BZTP_NM
,A.BIZ_RGST_NO
,A.MNR_SHOP_FLG
,A.MNR_PAYR_CNT_CD
,A.MNR_PAYR_SEQ
,A.MNR_PRNR_CAPI_AMT
,A.EMPE_KNT
,A.DPT_DESC
,A.MNR_PRNR_ABBR_NM
,A.INTL_PHN_NO
,A.PHN_NO
,A.INTL_FAX_NO
,A.FAX_NO
,A.MNR_PRNR_EML
,A.MNR_PRNR_RMK
,A.TRSM_MOD_CD
,A.FILE_SEQ
,A.CRE_USR_ID
,A.CRE_DT
,A.UPD_USR_ID
,A.UPD_DT
FROM MNR_PARTNER A
WHERE
#if (${mnr_prnr_seq} != '')
	A.MNR_PRNR_SEQ = TO_NUMBER(@[mnr_prnr_seq])
	AND   A.MNR_GRP_TP_CD = @[mnr_grp_tp_cd]
#else
	A.MNR_GRP_TP_CD = @[mnr_grp_tp_cd]
#end
) C
WHERE
NVL(B.DELT_FLG, 'N') <> 'Y'
AND B.VNDR_SEQ = TO_NUMBER(@[mnr_prnr_seq])
AND B.VNDR_SEQ = C.MNR_PRNR_SEQ(+)			]]></sql>
			<params>
				<param name="mnr_prnr_seq" type="12" value="" out="N"/>
				<param name="mnr_grp_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
