<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOsearchMonthListRSQL">
			<desc><![CDATA[GRID 타이틀 월정보 조회]]></desc>
			<sql><![CDATA[
SELECT BSE_DT
FROM
(
    SELECT BSE_DT
    FROM
    (
        SELECT B.SEQ SEQ
              ,TO_CHAR(ADD_MONTHS(A.BSE_DT,SEQ-1),'YYYYMM') BSE_DT
        FROM (
                 SELECT TO_DATE(@[from_bse_dt],'YYYYMM') BSE_DT   
                 FROM DUAL
             ) A
            ,(
                 SELECT LEVEL SEQ   
                 FROM DUAL 
                 CONNECT BY LEVEL <=12 -- 최대 12개 하드코딩
             ) B

    )
) A
WHERE A.BSE_DT BETWEEN @[from_bse_dt] AND @[to_bse_dt]			]]></sql>
			<params>
				<param name="from_bse_dt" type="12" value="" out="N"/>
				<param name="to_bse_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
