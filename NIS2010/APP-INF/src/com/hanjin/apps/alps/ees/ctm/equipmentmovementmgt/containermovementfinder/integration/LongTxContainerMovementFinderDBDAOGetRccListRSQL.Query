<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongTxContainerMovementFinderDBDAOGetRccListRSQL">
			<desc><![CDATA[RCC LIST를 받아온다]]></desc>
			<sql><![CDATA[
SELECT DISTINCT B.RCC_CD, DECODE (A.RCC_CD, NULL, '0', '1') CHECKED
           FROM (SELECT RCC_CD
                   FROM MDM_LOCATION LOC,
                        MDM_EQ_ORZ_CHT EQ,
                        MDM_ORGANIZATION OFC
                  WHERE LOC.SCC_CD = EQ.SCC_CD
                    AND OFC.LOC_CD = LOC.LOC_CD
                    AND OFC.OFC_CD = @[ofc_cd]) A,
                (SELECT RCC_CD
                   FROM MDM_LOCATION LOC,
                        MDM_EQ_ORZ_CHT EQ
                  WHERE LOC.SCC_CD = EQ.SCC_CD) B
          WHERE B.RCC_CD = A.RCC_CD(+)
			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
