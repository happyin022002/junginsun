<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LeasePlanDBDAONewVanCNTRDeliveryPlanRSQL">
			<desc><![CDATA[신조장비(OW/LP/OL) 계획내용의 존재여부를 확인한다.]]></desc>
			<sql><![CDATA[
SELECT	A.PLN_YRMON,
		A.NEW_VAN_YRMON,
		A.MFT_VNDR_SEQ,
		A.DEL_CD,
		A.CNTR_TPSZ_CD,
		A.AGMT_CTY_CD,
		A.AGMT_SEQ,
		A.LSTM_CD,
		A.DE_QTY,
		A.CRE_USR_ID,
		A.CRE_DT,
		A.UPD_USR_ID,
		A.UPD_DT,
		A.PLN_RMK
FROM	LSE_NEW_VAN_DE_PLN A
WHERE	1 = 1
#if (${pln_yrmon} != "")
AND 	A.PLN_YRMON = @[pln_yrmon]
#end
#if (${new_van_yrmon} != "")
AND 	A.NEW_VAN_YRMON = @[new_van_yrmon]
#end
#if (${mft_vndr_seq} != "")
AND 	A.MFT_VNDR_SEQ = @[mft_vndr_seq]
#end
#if (${del_cd} != "")
AND 	A.DEL_CD = @[del_cd]
#end
#if (${cntr_tpsz_cd} != "")
AND 	A.CNTR_TPSZ_CD = @[cntr_tpsz_cd]
#end
#if (${agmt_cty_cd} != "")
AND     A.AGMT_CTY_CD = @[agmt_cty_cd]
#end
#if (${agmt_seq} != "")
AND     A.AGMT_SEQ = @[agmt_seq]
#end
#if (${lstm_cd} != "")
AND     A.LSTM_CD = @[lstm_cd]
#end
#if (${de_qty} != "")
AND     A.DE_QTY = @[de_qty]
#end			]]></sql>
			<params>
				<param name="pln_yrmon" type="12" value="" out="N"/>
				<param name="new_van_yrmon" type="12" value="" out="N"/>
				<param name="mft_vndr_seq" type="2" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
				<param name="de_qty" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
