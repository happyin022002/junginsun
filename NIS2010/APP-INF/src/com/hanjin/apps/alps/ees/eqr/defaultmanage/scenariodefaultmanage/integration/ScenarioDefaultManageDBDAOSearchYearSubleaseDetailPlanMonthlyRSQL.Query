<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOSearchYearSubleaseDetailPlanMonthlyRSQL">
			<desc><![CDATA[eqr_sublease + eqr_slse_perf 테이블 데이터 조회(Monthly)]]></desc>
			<sql><![CDATA[
SELECT 
    E.FM_RCC_CD S2_FM_RCC_CD, 
    E.FM_ECC_CD S2_FM_ECC_CD, 
    E.TO_ECC_CD S2_TO_ECC_CD, 
    CNTR_TPSZ_CD TPSZ, 
    TO_CHAR(NVL(ROUND(MAX(SLSE_RTO),2),0), '990.99')||'%' S2_DMST_RTO,
    SUM (DECODE (PLN_MON, 01, CNTR_VOL_QTY)) S2_1_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 02, CNTR_VOL_QTY)) S2_2_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 03, CNTR_VOL_QTY)) S2_3_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 04, CNTR_VOL_QTY)) S2_4_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 05, CNTR_VOL_QTY)) S2_5_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 06, CNTR_VOL_QTY)) S2_6_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 07, CNTR_VOL_QTY)) S2_7_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 08, CNTR_VOL_QTY)) S2_8_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 09, CNTR_VOL_QTY)) S2_9_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 10, CNTR_VOL_QTY)) S2_10_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 11, CNTR_VOL_QTY)) S2_11_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 12, CNTR_VOL_QTY)) S2_12_CNTR_VOL_QTY
FROM
(
    SELECT 
        (
            SELECT
#if (${fm_to_at} == '1') 
	#if (${fm_type_by} == 'R') 
		RCC_CD
	#elseif (${fm_type_by} == 'C') 
		CNT_CD
	#elseif (${fm_type_by} == 'L') 
		LCC_CD
	#else
		ECC_CD
	#end
#else 
	#if (${at_type_by} == 'R') 
		RCC_CD
	#elseif (${at_type_by} == 'C') 
		CNT_CD
	#elseif (${at_type_by} == 'L') 
		LCC_CD
	#else
		ECC_CD
	#end
#end
            FROM EQR_ECC_MST WHERE A.FM_ECC_CD = ECC_CD
        ) FM_ECC_CD,
        (
            SELECT
#if (${fm_to_at} == '1') 
	#if (${to_type_by} == 'R') 
		RCC_CD
	#elseif (${to_type_by} == 'C') 
		CNT_CD
	#elseif (${to_type_by} == 'L') 
		LCC_CD
	#else
		ECC_CD
	#end
#else 
	#if (${at_type_by} == 'R') 
		RCC_CD
	#elseif (${at_type_by} == 'C') 
		CNT_CD
	#elseif (${at_type_by} == 'L') 
		LCC_CD
	#else
		ECC_CD
	#end
#end
            FROM EQR_ECC_MST WHERE A.TO_ECC_CD = ECC_CD
        ) TO_ECC_CD,
        (
            SELECT RCC_CD FROM EQR_ECC_MST WHERE A.FM_ECC_CD = ECC_CD
        ) FM_RCC_CD,
        A.CNTR_TPSZ_CD, 
        MIN(PLN_MON)PLN_MON, 
        PLN_YRWK, 
        MAX(CNTR_VOL_QTY)CNTR_VOL_QTY, 
        SLSE_RTO
    FROM EQR_SUBLEASE A, EQR_WK_PRD B,
#if (${fm_to_at} == '1') 
	(SELECT ECC_CD FROM EQR_ECC_MST WHERE 1 = 1
	#if ($arrFmEccCd.size() > 0) 
		#if (${fm_type} == 'R') 
			AND RCC_CD
		#elseif (${fm_type} == 'L') 
			AND LCC_CD
		#elseif (${fm_type} == 'E') 
			AND ECC_CD
		#end
		IN(
  		#foreach( $key in ${arrFmEccCd}) 
    		#if($velocityCount < $arrFmEccCd.size())
    			'$key',
    		#else 
    			'$key'
    		#end
  		#end
		)) C,
	#else 
		) C,
	#end
	(SELECT ECC_CD FROM EQR_ECC_MST WHERE 1 = 1
	#if ($arrToEccCd.size() > 0) 
		#if (${to_type} == 'R') 
			AND RCC_CD
		#elseif (${to_type} == 'L') 
			AND LCC_CD
		#elseif (${to_type} == 'E') 
			AND ECC_CD
		#end
		IN(
  		#foreach( $key in ${arrToEccCd}) 
    		#if($velocityCount < $arrToEccCd.size())
    			'$key',
    		#else 
    			'$key'
    		#end
  		#end
		)) D,
	#else
		) D,
	#end
#else 
	#if (${at_type} != '' && $arrAtEccCd.size() > 0) 
		(SELECT ECC_CD FROM EQR_ECC_MST WHERE 1 = 1
		#if (${at_type} == 'R') 
			AND RCC_CD
		#elseif (${at_type} == 'L') 
			AND LCC_CD
		#elseif (${at_type} == 'E') 
			AND ECC_CD
		#end
		IN(
  		#foreach( $key in ${arrAtEccCd}) 
    		#if($velocityCount < $arrAtEccCd.size())
    			'$key',
    		#else 
    			'$key'
    		#end
  		#end
		)) C,
	#end
#end
    EQR_SLSE_PERF E
    WHERE A.PLN_YRWK = B.PLN_YR || B.PLN_WK
    AND A.FM_ECC_CD = E.FM_ECC_CD(+)
    AND A.TO_ECC_CD = E.TO_ECC_CD(+)
    AND A.CNTR_TPSZ_CD = E.CNTR_TPSZ_CD(+)
#if (${fm_to_at} == '1') 
	AND A.FM_ECC_CD = C.ECC_CD AND A.TO_ECC_CD = D.ECC_CD
#else
	#if (${at_type} != '' && $arrAtEccCd.size() > 0) 
		AND (A.FM_ECC_CD = C.ECC_CD OR A.TO_ECC_CD = C.ECC_CD)
	#end
#end
    AND SUBSTR (A.PLN_YRWK, 1, 4) = @[plnyr]
#if ($arrTpszcd.size() > 0) 
  AND A.CNTR_TPSZ_CD IN(
  #foreach( $key in ${arrTpszcd})
    #if($velocityCount < $arrTpszcd.size())
    '$key',
    #else 
    '$key'
    #end
  #end
  )
#end
    GROUP BY A.FM_ECC_CD, A.TO_ECC_CD, A.CNTR_TPSZ_CD, PLN_YRWK, SLSE_RTO
) E
GROUP BY E.FM_RCC_CD, E.FM_ECC_CD, E.TO_ECC_CD, CNTR_TPSZ_CD
ORDER BY E.FM_RCC_CD, E.FM_ECC_CD, E.TO_ECC_CD, CNTR_TPSZ_CD			]]></sql>
			<params>
				<param name="plnyr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
