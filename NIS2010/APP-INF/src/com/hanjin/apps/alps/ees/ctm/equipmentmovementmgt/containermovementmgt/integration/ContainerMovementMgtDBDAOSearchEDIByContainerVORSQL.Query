<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchEDIByContainerVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX CTM_MVMT_EDI_MSG XAK2CTM_MVMT_EDI_MSG */
       BKG_KNT,
       BKG_NO,
       BKG_POD_CD,
       BKG_POL_CD,
       EDI_BL_NO AS BL_NO,
       NVL(CALL_SGN_NO, LLOYD_NO) AS CALL_SGN_NO,
       CHSS_NO,
       CNMV_CO_CD,
       CNTR_DMG_FLG,
       CNTR_FULL_STS_CD,
       CNTR_NO,
       CNTR_SEAL_NO,
       CRNT_VSL_CD||CRNT_SKD_VOY_NO||CRNT_SKD_DIR_CD AS VVD_CD,
       DEST_YD_CD,
       EDI_GATE_IO_CD,
       EDI_MVMT_STS_CD,
       TO_CHAR(EVNT_DT, 'YYYY-MM-DD HH24:MI') AS EVNT_DT,
       EVNT_YD_CD,
       MGST_NO,
       MVMT_EDI_MSG_AREA_CD||MVMT_EDI_MSG_YRMONDY||MVMT_EDI_MSG_SEQ AS MUID,
       MVMT_EDI_MSG_TP_ID,
       MVMT_EDI_TP_CD,
       MVMT_EDI_RSLT_CD,
       MVMT_TRSP_MOD_CD,
       PKUP_NO,
       RTY_KNT AS EDI_RTY_KNT,
       TML_NM,
       VNDR_SEQ,
       WBL_NO
  FROM CTM_MVMT_EDI_MSG
 WHERE MVMT_EDI_MSG_AREA_CD = @[mvmt_edi_msg_area_cd]
   AND MVMT_EDI_MSG_SEQ = @[mvmt_edi_msg_seq]
   AND MVMT_EDI_MSG_TP_ID = @[mvmt_edi_msg_tp_id]
   AND MVMT_EDI_MSG_YRMONDY = @[mvmt_edi_msg_yrmondy]
   AND MVMT_EDI_TP_CD = @[mvmt_edi_tp_cd]			]]></sql>
			<params>
				<param name="mvmt_edi_msg_area_cd" type="12" value="" out="N"/>
				<param name="mvmt_edi_msg_seq" type="12" value="" out="N"/>
				<param name="mvmt_edi_msg_tp_id" type="12" value="" out="N"/>
				<param name="mvmt_edi_msg_yrmondy" type="12" value="" out="N"/>
				<param name="mvmt_edi_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
