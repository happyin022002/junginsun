<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOCheckRHQRSQL">
			<desc><![CDATA[Agent 에 해당하는 RHQ 조회 ]]></desc>
			<sql><![CDATA[
SELECT	o.ar_hd_qtr_ofc_cd rhq
FROM 	MDM_ORGANIZATION o 
WHERE 	1 = 1
   		AND	o.ofc_cd = @[rhq]
		AND o.delt_flg = 'N'			]]></sql>
			<params>
				<param name="rhq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
