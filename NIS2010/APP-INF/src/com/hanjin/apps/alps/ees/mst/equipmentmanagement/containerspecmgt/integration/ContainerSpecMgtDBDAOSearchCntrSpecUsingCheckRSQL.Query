<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSpecMgtDBDAOSearchCntrSpecUsingCheckRSQL">
			<desc><![CDATA[삭제하기전에 삭제할수 있는지 체크한다.]]></desc>
			<sql><![CDATA[
#if (${own_cntr_flg} == 'N') 
SELECT A.AGMT_CTY_CD || LPAD(A.AGMT_SEQ,6,'0') AS AGMT
FROM LSE_AGMT_RT A
WHERE A.CNTR_SPEC_NO = @[cntr_spec_no]
AND   ROWNUM = 1
#end
#if (${own_cntr_flg} == 'Y') 
SELECT A.LOT_PLN_YR||'-'|| A.LOT_LOC_CD||'-'|| A.CNTR_TPSZ_CD||'-'|| LTRIM(TO_CHAR(A.LOT_SEQ,'000'))  AS AGMT
FROM MST_CNTR_LOT A
WHERE A.CNTR_SPEC_NO = @[cntr_spec_no]
AND   ROWNUM = 1
#end			]]></sql>
			<params>
				<param name="cntr_spec_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
