<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepoThresholdManageDBDAOCheckCntrFcstRLSThresholdRSQL">
			<desc><![CDATA[EQR_OB_FCAST_RED_LGT_ALT 테이블의 중복데이터 검색]]></desc>
			<sql><![CDATA[
SELECT 
	COUNT(*) COUN
FROM 
	EQR_OB_FCAST_RED_LGT_ALT
WHERE 
	RCC_CD         = @[rcc_cd]
	AND   RLA_TP_CD      = @[rla_tp_cd]
	AND   CNTR_TPSZ_CD   = @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="rla_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
