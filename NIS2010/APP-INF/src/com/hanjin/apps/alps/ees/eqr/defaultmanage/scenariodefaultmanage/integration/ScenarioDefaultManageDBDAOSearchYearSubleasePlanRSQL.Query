<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOSearchYearSubleasePlanRSQL">
			<desc><![CDATA[eqr_sublease 테이블 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT 
    D.INTG_CD_VAL_CTNT S1_RCC_CD,
    SUM (DECODE (PLN_MON, 01, CNTR_VOL_QTY)) S1_1_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 02, CNTR_VOL_QTY)) S1_2_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 03, CNTR_VOL_QTY)) S1_3_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 04, CNTR_VOL_QTY)) S1_4_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 05, CNTR_VOL_QTY)) S1_5_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 06, CNTR_VOL_QTY)) S1_6_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 07, CNTR_VOL_QTY)) S1_7_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 08, CNTR_VOL_QTY)) S1_8_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 09, CNTR_VOL_QTY)) S1_9_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 10, CNTR_VOL_QTY)) S1_10_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 11, CNTR_VOL_QTY)) S1_11_CNTR_VOL_QTY,
    SUM (DECODE (PLN_MON, 12, CNTR_VOL_QTY)) S1_12_CNTR_VOL_QTY,
    @[plnyr] S1_PLNYR
FROM (
  SELECT 
      B.PLN_MON,
      (SELECT RCC_CD FROM EQR_ECC_MST WHERE A.FM_ECC_CD = ECC_CD) RCC_CD,
      SUM (A.CNTR_VOL_QTY) CNTR_VOL_QTY
  FROM EQR_SUBLEASE A, EQR_WK_PRD B
  WHERE A.PLN_YRWK = B.PLN_YR || B.PLN_WK
  AND SUBSTR(a.PLN_YRWK,1,4) = @[plnyr]
  GROUP BY A.FM_ECC_CD, B.PLN_MON
) C, COM_INTG_CD_DTL D
WHERE C.RCC_CD(+)  = D.INTG_CD_VAL_CTNT
AND D.INTG_CD_ID = 'CD00255'
GROUP BY D.INTG_CD_VAL_CTNT, INTG_CD_VAL_DP_SEQ
ORDER BY INTG_CD_VAL_DP_SEQ			]]></sql>
			<params>
				<param name="plnyr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
