<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongstayingUnclaimEQFlaggingDBDAOMailingServiceSettingListFromMdmRSQL">
			<desc><![CDATA[[EES_CIM_0062] Mailing Service Setting List From MDM_COUNTRY]]></desc>
			<sql><![CDATA[
SELECT SCONTI_CD as SCONTI_CD_H
	,'Y' SCONTI_CHK
	, SCONTI_CD
	, CNT_CD as CNT_CD_H
	, 'Y' CNT_CHK
	, CNT_CD
	, CNT_NM
  FROM MDM_COUNTRY
 WHERE DELT_FLG = 'N'
ORDER BY SCONTI_CD, CNT_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
