<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOsearchVLVDYardCodeRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT YD_CD
	FROM VSK_VSL_PORT_SKD
	WHERE VSL_CD = SUBSTR (@[p_vvdcd],0,4)
		AND SKD_VOY_NO = SUBSTR(@[p_vvdcd],5,4)
		AND SKD_DIR_CD = SUBSTR(@[p_vvdcd],9,1)
		AND VPS_PORT_CD = @[p_yard1]			]]></sql>
			<params>
				<param name="p_vvdcd" type="12" value="" out="N"/>
				<param name="p_yard1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
