<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncomeMgtDBDAOcheckReceivableInvSumDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT SUM(CHG_AMT)
  FROM INV_AR_IF_CHG
 WHERE SRC_IF_SEQ IN ( SELECT SRC_IF_SEQ 
						 FROM INV_AR_IF_MN
						WHERE INV_SRC_NO = @[input_inv_no] )			]]></sql>
			<params>
				<param name="input_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
