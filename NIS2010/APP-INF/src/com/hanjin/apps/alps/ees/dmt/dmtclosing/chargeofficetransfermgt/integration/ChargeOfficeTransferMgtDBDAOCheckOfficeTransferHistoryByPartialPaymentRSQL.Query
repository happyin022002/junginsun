<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeOfficeTransferMgtDBDAOCheckOfficeTransferHistoryByPartialPaymentRSQL">
			<desc><![CDATA[ChargeOfficeTransferMgtDBDAOCheckOfficeTransferHistoryByPartialPaymentRSQL]]></desc>
			<sql><![CDATA[
SELECT
	NVL(
		(SELECT	'Y'
		FROM	DMT_OFC_TRNS_HIS
		WHERE	CNTR_NO					= @[cntr_no]
		AND		CNTR_CYC_NO				= @[cntr_cyc_no]
		AND		DMDT_TRF_CD				= @[dmdt_trf_cd]
		AND		DMDT_CHG_LOC_DIV_CD		= @[dmdt_chg_loc_div_cd]
		AND		CHG_SEQ					= @[chg_seq]
		AND		ROWNUM = 1
		), 'N'
	) AS EXIST_YN
FROM DUAL			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
