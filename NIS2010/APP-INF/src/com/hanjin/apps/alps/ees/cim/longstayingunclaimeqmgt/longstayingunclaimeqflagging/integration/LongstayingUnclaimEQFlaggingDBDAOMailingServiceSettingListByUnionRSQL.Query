<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongstayingUnclaimEQFlaggingDBDAOMailingServiceSettingListByUnionRSQL">
			<desc><![CDATA[[EES_CIM_0062] Mailing Service Setting List By Union]]></desc>
			<sql><![CDATA[
SELECT *
  FROM 
(   
	SELECT E.SCONTI_CD as SCONTI_CD_H
	    , 'N' SCONTI_CHK
	    , E.SCONTI_CD
	    , E.CNT_CD as CNT_CD_H
	    , 'Y' CNT_CHK
	    , E.CNT_CD
	    , E.CNT_NM
	  FROM  CIM_EQ_SPLS_DFCT_EML D, MDM_COUNTRY E
	 WHERE E.CNT_CD  = D.CNT_CD
	   AND D.USR_ID    = @[usr_id]
	
	UNION
	
	SELECT SCONTI_CD as SCONTI_CD_H
	    , 'N' SCONTI_CHK
	    , SCONTI_CD
	    , CNT_CD as CNT_CD_H
	    , 'N' CNT_CHK
	    , CNT_CD
	    , CNT_NM
	  FROM MDM_COUNTRY
	 WHERE DELT_FLG = 'N'
	   AND CNT_CD NOT IN 
	( 
		SELECT D.CNT_CD
	      FROM  CIM_EQ_SPLS_DFCT_EML D
	     WHERE D.USR_ID    = @[usr_id]
	)
)
ORDER BY SCONTI_CD, CNT_CD			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
