<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffMgtDBDAOmodifyDisposalTariffRegionInfoUSQL">
			<desc><![CDATA[분기별 매각기준 가격정보를 개별 수정합니다.]]></desc>
			<sql><![CDATA[
UPDATE  MNR_DISP_TRF
SET     EQ_KND_CD   = @[eq_knd_cd],
        EQ_TPSZ_CD  = @[eq_tpsz_cd],
        LOC_CD      = @[loc_cd],
        CURR_CD     = @[curr_cd],
        MNR_DISP_TRF_AMT = @[mnr_disp_trf_amt],
        MNR_TRF_RMK = @[mnr_trf_rmk],
        UPD_USR_ID  = @[upd_usr_id],
        UPD_DT      = SYSDATE 
WHERE	TRF_EFF_YR  	 = @[trf_eff_yr]
AND	    TRF_EFF_QTR_NO 	 = @[trf_eff_qtr_no]
AND	    MNR_DISP_TRF_SEQ = @[mnr_disp_trf_seq]			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="mnr_disp_trf_amt" type="12" value="" out="N"/>
				<param name="mnr_trf_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="trf_eff_yr" type="12" value="" out="N"/>
				<param name="trf_eff_qtr_no" type="12" value="" out="N"/>
				<param name="mnr_disp_trf_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
