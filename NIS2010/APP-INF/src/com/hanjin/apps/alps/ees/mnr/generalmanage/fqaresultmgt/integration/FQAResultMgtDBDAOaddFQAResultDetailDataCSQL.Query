<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FQAResultMgtDBDAOaddFQAResultDetailDataCSQL">
			<desc><![CDATA[FQAResultMgtDBDAOaddFQAResultDetailDataCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_FLD_QLTY_AUD_RSLT
	(
    	VNDR_SEQ
		,   YD_CD
		,	OFC_CD
		,	FLD_AUD_DT
		,   FLD_AUD_DTL_SEQ
		,	EV_DESC
		,	MAX_PNT_NO
		,	PNT_NO
		,	PNT_CALC_FLG
		,	FLD_AUD_RMK
		,	FILE_SEQ
		,	CRE_USR_ID
		,	CRE_DT
		,	UPD_USR_ID
	) VALUES( 
		TO_NUMBER(@[vndr_seq])
		,   @[yd_cd]
		,	@[ofc_cd]
           ,   TO_DATE( (@[fld_aud_dt] || to_char( sysdate, 'HH24:MI:SS') ) , 'yyyy-mm-dd hh24:mi:ss') 
		,   @[fld_aud_dtl_seq]
		,	@[ev_desc]
		,	@[max_pnt_no]
		,	@[pnt_no]
		,	'N'
		,	@[fld_aud_rmk]
		,	@[file_seq]
		,	@[cre_usr_id]
		,	sysdate
		,	@[upd_usr_id]
	)			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="fld_aud_dt" type="12" value="" out="N"/>
				<param name="fld_aud_dtl_seq" type="12" value="" out="N"/>
				<param name="ev_desc" type="12" value="" out="N"/>
				<param name="max_pnt_no" type="12" value="" out="N"/>
				<param name="pnt_no" type="12" value="" out="N"/>
				<param name="fld_aud_rmk" type="12" value="" out="N"/>
				<param name="file_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
