<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WriteOffMgtDBDAOsearchWriteOffRemarkListDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 
	MWRH.TTL_LSS_DTL_RSN_RMK,
	MWRH.DPC_CLT_FALD_RSN_RMK,
	MWRH.RCVR_ACT_HIS_RMK,
	MWRH.FILE_SEQ AS SUB_FILE_SEQ,
	MWRH.WRTF_STS_CD,
	MWRH.WRTF_NO,
	(SELECT DISTINCT FILE_SEQ
	   FROM MNR_TTL_LSS_RQST_HDR
	  WHERE TTL_LSS_NO = MWRH.TTL_LSS_NO) AS FILE_SEQ
FROM MNR_WRTF_RQST_HDR MWRH
WHERE 1=1
#if (${ttl_lss_no} != '')
   AND MWRH.TTL_LSS_NO = @[ttl_lss_no]
#end
#if (${wrtf_no} != '')
   AND MWRH.WRTF_NO = @[wrtf_no]
#end			]]></sql>
			<params>
				<param name="ttl_lss_no" type="12" value="" out="N"/>
				<param name="wrtf_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
