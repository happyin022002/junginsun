		<query name="CEDEXCodeMgtDBDAOmodifyComponentCodeDataUSQL">
			<desc><![CDATA[수정]]></desc>
			<sql><![CDATA[
UPDATE MNR_EQ_CMPO_CD SET 
	EQ_PRNT_CMPO_GRP_TP_CD = @[eq_prnt_cmpo_grp_tp_cd],
	EQ_KND_CD = @[eq_knd_cd],
	UPD_USR_ID = @[upd_usr_id],
	UPD_DT = SYSDATE,
	EQ_CMPO_NUM_ISO_CD = @[eq_cmpo_num_iso_cd],
	EQ_CMPO_CD = @[eq_cmpo_cd],
	EQ_CMPO_GRP_TP_CD = @[eq_cmpo_grp_tp_cd],
	EQ_CMPO_NM = @[eq_cmpo_nm],
	EQ_CMPO_DESC = @[eq_cmpo_desc],
	EQ_PRNT_CMPO_CD = @[eq_prnt_cmpo_cd],
	CNTR_CMPO_FLG = DECODE(@[cntr_cmpo_flg],'0','N','Y'),
	CHSS_CMPO_FLG = DECODE(@[chss_cmpo_flg],'0','N','Y'), 
	MGST_CMPO_FLG = DECODE(@[mgst_cmpo_flg],'0','N','Y')
WHERE	EQ_CMPO_NUM_ISO_CD = @[eq_cmpo_num_iso_cd]
AND	EQ_CMPO_CD = @[eq_cmpo_cd]
AND	EQ_CMPO_GRP_TP_CD = @[eq_cmpo_grp_tp_cd]			]]></sql>
			<params>
				<param name="eq_prnt_cmpo_grp_tp_cd" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
