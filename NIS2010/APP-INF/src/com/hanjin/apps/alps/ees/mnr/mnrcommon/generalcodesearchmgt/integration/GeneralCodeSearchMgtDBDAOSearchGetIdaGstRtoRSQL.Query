<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOSearchGetIdaGstRtoRSQL">
			<desc><![CDATA[SearchGetIdaGstRto]]></desc>
			<sql><![CDATA[
SELECT   @[ida_sac_cd] AS IDA_SAC_CD
       , IDA_CGST_RTO
       , IDA_SGST_RTO
       , IDA_IGST_RTO
       , IDA_UGST_RTO
FROM     TABLE ( INV_GET_IDA_GST_RTO_FNC ( INV_GET_GST_DIV_CD_FNC ( @[ofc_cd], 'V', NULL, NULL, @[vndr_seq], NULL ), @[ida_sac_cd] ) ) I
WHERE    1 = 1			]]></sql>
			<params>
				<param name="ida_sac_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
