<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOSearchDefaultYearDomesticPlanRSQL">
			<desc><![CDATA[Domestic 조회]]></desc>
			<sql><![CDATA[
SELECT MAX (DECODE (PLN_MON, 01, CNTR_VOL_QTY)) S1_1_CNTR_VOL_QTY, 
       MAX (DECODE (PLN_MON, 02, CNTR_VOL_QTY)) S1_2_CNTR_VOL_QTY, 
       MAX (DECODE (PLN_MON, 03, CNTR_VOL_QTY)) S1_3_CNTR_VOL_QTY,
       '' QTY1, 
       MAX (DECODE (PLN_MON, 04, CNTR_VOL_QTY)) S1_4_CNTR_VOL_QTY, 
       MAX (DECODE (PLN_MON, 05, CNTR_VOL_QTY)) S1_5_CNTR_VOL_QTY,
       MAX (DECODE (PLN_MON, 06, CNTR_VOL_QTY)) S1_6_CNTR_VOL_QTY, 
       '' QTY2,
       MAX (DECODE (PLN_MON, 07, CNTR_VOL_QTY)) S1_7_CNTR_VOL_QTY,
       MAX (DECODE (PLN_MON, 08, CNTR_VOL_QTY)) S1_8_CNTR_VOL_QTY,
       MAX (DECODE (PLN_MON, 09, CNTR_VOL_QTY)) S1_9_CNTR_VOL_QTY,
       '' QTY3, 
       MAX (DECODE (PLN_MON, 10, CNTR_VOL_QTY)) S1_10_CNTR_VOL_QTY,
       MAX (DECODE (PLN_MON, 11, CNTR_VOL_QTY)) S1_11_CNTR_VOL_QTY,
	   MAX (DECODE (PLN_MON, 12, CNTR_VOL_QTY)) S1_12_CNTR_VOL_QTY,
       '1' FLAG
FROM (SELECT B.PLN_MON,                                             
           	SUM (A.CNTR_VOL_QTY) CNTR_VOL_QTY                       
      		FROM EQR_DMST_PLN A, EQR_WK_PRD B 
                 WHERE A.PLN_YRWK = B.PLN_YR || B.PLN_WK                           
         		 AND SUBSTR(A.PLN_YRWK,1,4) = @[pln_yr]                              
			     GROUP BY B.PLN_MON
      ) C			]]></sql>
			<params>
				<param name="pln_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
