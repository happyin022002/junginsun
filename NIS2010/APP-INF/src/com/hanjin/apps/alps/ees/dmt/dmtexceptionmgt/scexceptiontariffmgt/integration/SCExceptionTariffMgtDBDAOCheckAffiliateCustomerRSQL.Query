<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOCheckAffiliateCustomerRSQL">
			<desc><![CDATA[S/C Tariff Exception 에 등록된 Actual Customer 가 Affiliate 인지를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	COUNT(PROP_NO) AS CNT
FROM	DMT_SC_EXPT_ACT_CUST
WHERE	PROP_NO = @[prop_no]
	AND	ACT_CUST_FLG = 'N'			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
