<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOGetPlusDayRSQL">
			<desc><![CDATA[GetPlusDay]]></desc>
			<sql><![CDATA[
SELECT	TO_CHAR( TO_DATE(@[ftime_cmnc],'YYYYMMDDHH24MI') + 1, 'DY', 'NLS_DATE_LANGUAGE = American') week_of_day ,
		TO_CHAR( TO_DATE(@[ftime_cmnc],'YYYYMMDDHH24MI') + 1, 'YYYYMMDDHH24MI') rtn_ftime_cmnc
FROM	DUAL			]]></sql>
			<params>
				<param name="ftime_cmnc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
