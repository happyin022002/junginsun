<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchMaxRepoPlanIdRSQL">
			<desc><![CDATA[REPO PLAN ID 중에 배포된 최근 PLAN ID 를 검색
- 리턴형식 (WEEK, SEQ)]]></desc>
			<sql><![CDATA[
SELECT 
	SUBSTR(REPO_PLN_ID, 5, 4)||SUBSTR(REPO_PLN_ID, 9, 2) WEEK 	
    ,SUBSTR(REPO_PLN_ID, 12, 3) SEQ		  								
FROM 												  				
(  																
    SELECT MAX(REPO_PLN_ID)	 REPO_PLN_ID				  			
    FROM EQR_EQ_REPO_PLN				      						
    WHERE DELT_FLG = 'N'   		                                
    AND REPO_PLN_DTRB_FLG = 'Y'   									
)			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
