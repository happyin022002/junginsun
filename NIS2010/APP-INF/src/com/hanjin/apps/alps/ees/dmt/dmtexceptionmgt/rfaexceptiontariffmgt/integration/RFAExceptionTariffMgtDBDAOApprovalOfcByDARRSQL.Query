<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOApprovalOfcByDARRSQL">
			<desc><![CDATA[DAR No. 에 해당되는 Approval Office Code 를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	APRO_OFC_CD
FROM	(
			SELECT	ROWNUM IDX, APRO_OFC_CD
			FROM	DMT_RFA_EXPT_TRF
			WHERE	RFA_EXPT_DAR_NO = @[rfa_expt_dar_no]
		)
WHERE IDX = 1			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
