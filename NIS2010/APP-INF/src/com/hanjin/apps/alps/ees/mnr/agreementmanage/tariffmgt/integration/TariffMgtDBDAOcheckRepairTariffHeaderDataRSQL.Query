<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TariffMgtDBDAOcheckRepairTariffHeaderDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
SELECT COUNT(A.TRF_NO) CNT 
  FROM MNR_RPR_TRF_HDR A
 WHERE A.EQ_KND_CD = @[eq_knd_cd]
   AND TO_CHAR(A.EFF_DT,'YYYY-MM-DD') = @[eff_dt]
   AND A.MNR_TRF_STS_CD NOT IN ('HD', 'SD')

   #if (${mnr_trf_knd_cd} == 'LCL') 
   AND A.VNDR_SEQ = @[vndr_seq]
   #end			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="U" out="N"/>
				<param name="eff_dt" type="12" value="2009-06-19" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
