<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOSearchScExptVerSeqRSQL">
			<desc><![CDATA[SCExceptionTariffMgtDBDAOSearchScExptVerSeqRSQL]]></desc>
			<sql><![CDATA[
SELECT	NVL(TO_CHAR(MAX(SC_EXPT_VER_SEQ)),'1') AS SC_EXPT_VER_SEQ
FROM	DMT_SC_EXPT_VER
WHERE	PROP_NO 				= @[prop_no]
AND     DMDT_EXPT_VER_STS_CD 	= 'A'
AND     SC_EXPT_VER_SEQ = ( SELECT MAX(SC_EXPT_VER_SEQ) FROM DMT_SC_EXPT_VER WHERE	PROP_NO = @[prop_no] )			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
