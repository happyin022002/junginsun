<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchBasicRfaYnRSQL">
			<desc><![CDATA[RFAExceptionTariffMgtDBDAOSearchBasicRfaYnRSQL]]></desc>
			<sql><![CDATA[
SELECT  DECODE(T2.RFA_CTRT_TP_CD, 'B', 'Y', 'N') AS BZC_RFA_YN
  FROM  PRI_RP_HDR  T1
       ,PRI_RP_MN   T2
 WHERE  T1.PROP_NO = @[prop_no]
   AND  T1.PROP_NO = T2.PROP_NO
   AND  T2.AMDT_SEQ = 0			]]></sql>
			<params>
				<param name="prop_no" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
