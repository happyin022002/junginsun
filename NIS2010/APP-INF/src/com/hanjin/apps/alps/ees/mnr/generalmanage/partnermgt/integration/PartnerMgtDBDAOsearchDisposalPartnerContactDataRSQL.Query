<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerMgtDBDAOsearchDisposalPartnerContactDataRSQL">
			<desc><![CDATA[searchDisposalPartnerContactData]]></desc>
			<sql><![CDATA[
SELECT
         A.MNR_PRNR_CRE_SEQ
        ,A.MNR_PRNR_CRE_DTL_SEQ
        ,A.CNT_CD
        ,A.OFC_CD
        ,A.MNR_CNTC_PRNR_NM
        ,A.INTL_PHN_NO
        ,A.PHN_NO
        ,A.INTL_FAX_NO
        ,A.FAX_NO
        ,A.MNR_PRNR_EML
        ,A.PRMRY_CHK_FLG
        ,A.CNTC_DIV_CD
        ,A.MNR_PRNR_ADDR
        ,A.MNR_PRNR_RMK
        ,DECODE(A.APLY_FLG,'Y','1','0') AS APLY_FLG
        ,A.CRE_USR_ID
        ,TO_CHAR(A.CRE_DT, 'yyyy-mm-dd') CRE_DT
        ,A.UPD_USR_ID
        ,TO_CHAR(A.UPD_DT, 'yyyy-mm-dd') UPD_DT
FROM MNR_PRNR_CNTC_PNT A
WHERE A.MNR_PRNR_CRE_SEQ = @[mnr_prnr_cre_seq]			]]></sql>
			<params>
				<param name="mnr_prnr_cre_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
