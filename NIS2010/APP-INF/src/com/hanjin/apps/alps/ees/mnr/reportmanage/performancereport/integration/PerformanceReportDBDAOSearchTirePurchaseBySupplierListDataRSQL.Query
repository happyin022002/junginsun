<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PerformanceReportDBDAOSearchTirePurchaseBySupplierListDataRSQL">
			<desc><![CDATA[PerformanceReportDBDAOSearchTirePurchaseBySupplierListDataRSQL]]></desc>
			<sql><![CDATA[
WITH PARAM 
AS (
    SELECT P_CD,
           MAX(DECODE(RNK, 1, CD)) CD01,
           MAX(DECODE(RNK, 2, CD)) CD02,
           MAX(DECODE(RNK, 3, CD)) CD03,
           MAX(DECODE(RNK, 4, CD)) CD04,
           MAX(DECODE(RNK, 5, CD)) CD05,
           MAX(DECODE(RNK, 6, CD)) CD06,
           MAX(DECODE(RNK, 7, CD)) CD07,
           MAX(DECODE(RNK, 8, CD)) CD08,
           MAX(DECODE(RNK, 9, CD)) CD09,
           MAX(DECODE(RNK, 10, CD)) CD10,
           MAX(DECODE(RNK, 11, CD)) CD11,
           MAX(DECODE(RNK, 12, CD)) CD12,
           MAX(DECODE(RNK, 13, CD)) CD13,
           MAX(DECODE(RNK, 14, CD)) CD14,
           MAX(DECODE(RNK, 15, CD)) CD15,
           MAX(DECODE(RNK, 16, CD)) CD16,
           MAX(DECODE(RNK, 17, CD)) CD17,
           MAX(DECODE(RNK, 18, CD)) CD18,
           MAX(DECODE(RNK, 19, CD)) CD19,
           MAX(DECODE(RNK, 20, CD)) CD20,
           MAX(DECODE(RNK, 1, CD_DESC)) CD_DESC01,
           MAX(DECODE(RNK, 2, CD_DESC)) CD_DESC02,
           MAX(DECODE(RNK, 3, CD_DESC)) CD_DESC03,
           MAX(DECODE(RNK, 4, CD_DESC)) CD_DESC04,
           MAX(DECODE(RNK, 5, CD_DESC)) CD_DESC05,
           MAX(DECODE(RNK, 6, CD_DESC)) CD_DESC06,
           MAX(DECODE(RNK, 7, CD_DESC)) CD_DESC07,
           MAX(DECODE(RNK, 8, CD_DESC)) CD_DESC08,
           MAX(DECODE(RNK, 9, CD_DESC)) CD_DESC09,
           MAX(DECODE(RNK, 10, CD_DESC)) CD_DESC10,
           MAX(DECODE(RNK, 11, CD_DESC)) CD_DESC11,
           MAX(DECODE(RNK, 12, CD_DESC)) CD_DESC12,
           MAX(DECODE(RNK, 13, CD_DESC)) CD_DESC13,
           MAX(DECODE(RNK, 14, CD_DESC)) CD_DESC14,
           MAX(DECODE(RNK, 15, CD_DESC)) CD_DESC15,
           MAX(DECODE(RNK, 16, CD_DESC)) CD_DESC16,
           MAX(DECODE(RNK, 17, CD_DESC)) CD_DESC17,
           MAX(DECODE(RNK, 18, CD_DESC)) CD_DESC18,
           MAX(DECODE(RNK, 19, CD_DESC)) CD_DESC19,
           MAX(DECODE(RNK, 20, CD_DESC)) CD_DESC20
    FROM (
             SELECT  P_CD, CD, CD_DESC,  ROW_NUMBER() OVER(ORDER BY DP_SEQ) RNK
             FROM
                   (
                    SELECT MR.PRNT_CD_ID P_CD, MR.MNR_CD_ID CD, MR.MNR_CD_DP_DESC CD_DESC, MNR_CD_DP_SEQ DP_SEQ
                    FROM MNR_GEN_CD MR
                    WHERE PRNT_CD_ID = 'MRZSTP'                    
                    )                                           
         )       
    GROUP BY P_CD     
    ), DUMMY_COL AS (
       SELECT 'Q' DCOL FROM DUAL  --QTY
       UNION ALL
       SELECT 'S' DCOL FROM DUAL  --AMOUNT
       UNION ALL 
       SELECT 'Z' DCOL FROM DUAL  --AVG
    ), RPC_DATA AS (
        SELECT OH.COST_OFC_CD OFC_CD, 
               OH.VNDR_SEQ V_SEQ, 
               MAX(MV.VNDR_LGL_ENG_NM) VNDR_NM,
               MAX(OD.MNR_EXPN_DTL_NM)  BRAND_NM,
               MAX(OH.CURR_CD) CURR,
               C.DCOL TYPE,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD01, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD01, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD01, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD01, OD.RPR_QTY)), 2) END CD01,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD02, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD02, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD02, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD02, OD.RPR_QTY)), 2) END CD02,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD03, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD03, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD03, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD03, OD.RPR_QTY)), 2) END CD03,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD04, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD04, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD04, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD04, OD.RPR_QTY)), 2) END CD04,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD05, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD05, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD05, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD05, OD.RPR_QTY)), 2) END CD05,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD06, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD06, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD06, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD06, OD.RPR_QTY)), 2) END CD06,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD07, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD07, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD07, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD07, OD.RPR_QTY)), 2) END CD07,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD08, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD08, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD08, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD08, OD.RPR_QTY)), 2) END CD08,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD09, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD09, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD09, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD09, OD.RPR_QTY)), 2) END CD09,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD10, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD10, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD10, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD10, OD.RPR_QTY)), 2) END CD10,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD01, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD11, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD11, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD11, OD.RPR_QTY)), 2) END CD11,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD02, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD12, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD12, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD12, OD.RPR_QTY)), 2) END CD12,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD03, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD13, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD13, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD13, OD.RPR_QTY)), 2) END CD13,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD04, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD14, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD14, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD14, OD.RPR_QTY)), 2) END CD14,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD05, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD15, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD15, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD15, OD.RPR_QTY)), 2) END CD15,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD06, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD16, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD16, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD16, OD.RPR_QTY)), 2) END CD16,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD07, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD17, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD17, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD17, OD.RPR_QTY)), 2) END CD17,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD08, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD18, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD18, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD18, OD.RPR_QTY)), 2) END CD18,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD09, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD19, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD19, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD19, OD.RPR_QTY)), 2) END CD19,
               CASE WHEN C.DCOL = 'Q' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD10, OD.RPR_QTY)) WHEN C.DCOL = 'S' THEN SUM(DECODE(OD.COST_DTL_CD, P.CD20, OD.COST_AMT)) WHEN C.DCOL = 'Z' THEN ROUND(SUM(DECODE(OD.COST_DTL_CD, P.CD20, OD.COST_AMT))/ SUM(DECODE(OD.COST_DTL_CD, P.CD20, OD.RPR_QTY)), 2) END CD20                    
        FROM  PARAM P, DUMMY_COL C, MNR_ORD_HDR OH, MNR_ORD_DTL OD, MDM_VENDOR MV
        WHERE OH.MNR_ORD_OFC_CTY_CD = OD.MNR_ORD_OFC_CTY_CD
        AND   OH.MNR_ORD_SEQ = OD.MNR_ORD_SEQ
        AND   OH.VNDR_SEQ = MV.VNDR_SEQ
		#if (${ofc_cd} != '') 
        AND   RTRIM(OH.COST_OFC_CD) = @[ofc_cd]
		#end
        AND   OH.CRE_DT BETWEEN  GLOBALDATE_PKG.TIME_CONV_OFC_FNC(@[user_ofc_cd],TO_DATE(@[from_dt], 'YYYY-MM-DD'), MNR_COMMON_PKG.MNR_GET_HOOFC_FNC()) AND GLOBALDATE_PKG.TIME_CONV_OFC_FNC(@[user_ofc_cd],TO_DATE(@[to_dt], 'YYYY-MM-DD') + 0.99999, MNR_COMMON_PKG.MNR_GET_HOOFC_FNC())  
		AND   RTRIM(OH.EQ_KND_CD) = 'Z'
		AND   RTRIM(OH.MNR_WO_TP_CD) = 'EXT'
		AND   RTRIM(OD.COST_CD) = 'MRZSTP'
        GROUP BY OH.COST_OFC_CD, OH.VNDR_SEQ, C.DCOL
      )
SELECT  RHQ, OFC, SUP_CD, SUP_NM, BRAND_NM, CUR, DECODE(TYPE, 'Q', 'QTY', 'S', 'AMT', 'Z', 'AVG') TYPE,
                                       DECODE(TYPE, 'Q', '1', 'S', '2', 'Z', '3') TYPE2,
        DECODE(RHQ, 'RHQ', CD01||'|'||CD02||'|'||CD03||'|'||CD04||'|'||CD05||'|'||CD06||'|'||CD07||'|'||CD08||'|'||CD09||'|'||CD10||'|'||CD11||'|'||CD12||'|'||CD13||'|'||CD14||'|'||CD15||'|'||CD16||'|'||CD17||'|'||CD18||'|'||CD19||'|'||CD20, '') AS TITLE,
        CD01, CD02, CD03, CD04, CD05, CD06, CD07, CD08, CD09, CD10, CD11, CD12, CD13, CD14, CD15, CD16, CD17, CD18, CD19, CD20
FROM        
    (    
        SELECT 'A' PRI, 'RHQ' RHQ,  'Office' OFC, 'Supplier Code' SUP_CD, 'Supplier' SUP_NM, 'Brand Name' BRAND_NM, 'Curr' CUR, 'Type' TYPE,
                NVL(CD_DESC01, 'N') CD01,
                NVL(CD_DESC02, 'N') CD02,
                NVL(CD_DESC03, 'N') CD03,
                NVL(CD_DESC04, 'N') CD04,
                NVL(CD_DESC05, 'N') CD05,
                NVL(CD_DESC06, 'N') CD06,
                NVL(CD_DESC07, 'N') CD07,
                NVL(CD_DESC08, 'N') CD08,
                NVL(CD_DESC09, 'N') CD09,
                NVL(CD_DESC10, 'N') CD10,
                NVL(CD_DESC11, 'N') CD11,
                NVL(CD_DESC12, 'N') CD12,
                NVL(CD_DESC13, 'N') CD13,
                NVL(CD_DESC14, 'N') CD14,
                NVL(CD_DESC15, 'N') CD15,
                NVL(CD_DESC16, 'N') CD16,
                NVL(CD_DESC17, 'N') CD17,
                NVL(CD_DESC18, 'N') CD18,
                NVL(CD_DESC19, 'N') CD19,
                NVL(CD_DESC20, 'N') CD20                   
        FROM PARAM P
        UNION ALL        
        SELECT 'B' PRI, MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(RD.OFC_CD) RHQ, OFC_CD OFC, TO_CHAR(V_SEQ) SUP_SEQ, VNDR_NM SUP_NM, 
               BRAND_NM, CURR CUR, TYPE TYPE,
               TO_CHAR(CD01) CD01,
               TO_CHAR(CD02) CD02,
               TO_CHAR(CD03) CD03,
               TO_CHAR(CD04) CD04,
               TO_CHAR(CD05) CD05,
               TO_CHAR(CD06) CD06,
               TO_CHAR(CD07) CD07,
               TO_CHAR(CD08) CD08,
               TO_CHAR(CD09) CD09,
               TO_CHAR(CD10) CD10,
               TO_CHAR(CD11) CD11,
               TO_CHAR(CD12) CD12,
               TO_CHAR(CD13) CD13,
               TO_CHAR(CD14) CD14,
               TO_CHAR(CD15) CD15,
               TO_CHAR(CD16) CD16,
               TO_CHAR(CD17) CD17,
               TO_CHAR(CD18) CD18,
               TO_CHAR(CD19) CD19,
               TO_CHAR(CD20) CD20
        FROM  RPC_DATA RD
    ) 
ORDER BY PRI, RHQ, OFC, SUP_CD, SUP_NM, CUR, TYPE2			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="user_ofc_cd" type="12" value="" out="N"/>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
