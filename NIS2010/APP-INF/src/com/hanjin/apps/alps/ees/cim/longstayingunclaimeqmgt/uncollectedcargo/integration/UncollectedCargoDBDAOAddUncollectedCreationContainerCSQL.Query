<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOAddUncollectedCreationContainerCSQL">
			<desc><![CDATA[Uncollected Creation 의 UC, B/L 별 Container 정보 생성 ]]></desc>
			<sql><![CDATA[
INSERT INTO CIM_UC_CGO_CNTR
	(	uc_cs_no, bl_no, cntr_no, cre_usr_id, cre_dt, upd_usr_id, upd_dt

	)
VALUES
	(
		@[uc_cs_no], @[bl_no], @[cntr_no], @[bkg_no], SYSDATE, @[bkg_no], SYSDATE
	)			]]></sql>
			<params>
				<param name="uc_cs_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
