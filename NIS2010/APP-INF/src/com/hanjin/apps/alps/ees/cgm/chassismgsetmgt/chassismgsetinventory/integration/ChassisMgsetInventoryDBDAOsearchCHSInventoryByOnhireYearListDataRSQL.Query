<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOsearchCHSInventoryByOnhireYearListDataRSQL">
			<desc><![CDATA[chungpa 20090730 1100 popup start]]></desc>
			<sql><![CDATA[
SELECT
    A.EQ_NO,
    A.EQ_TPSZ_CD,
    A.AGMT_LSTM_CD,
    A.AGMT_OFC_CTY_CD || LPAD(A.AGMT_SEQ, 6, '0') AS AGMT_NO,
    B.VNDR_ABBR_NM,
    A.CHSS_MVMT_STS_CD,
    D.LCC_CD,
    C.SCC_CD,
    A.CRNT_YD_CD,
    A.ONH_DT,
    A.CHSS_MVMT_DT,
    TRUNC(SYSDATE - A.CHSS_MVMT_DT,0) AS LSDAYS
FROM    
	CGM_EQUIPMENT A,
    CGM_EQ_STS_HIS t2,
    MDM_VENDOR B,
    MDM_LOCATION C,
    MDM_EQ_ORZ_CHT D
WHERE A.VNDR_SEQ = B.VNDR_SEQ (+)
      AND A.CRNT_LOC_CD = C.LOC_CD
	  AND A.EQ_NO = t2.EQ_NO
      AND A.eq_sts_seq= t2.eq_sts_seq
      AND C.SCC_CD = D.SCC_CD
      AND C.DELT_FLG = 'N'
      AND D.DELT_FLG = 'N'
	  AND A.EQ_KND_CD = @[eq_knd_cd]
#if (${location} == 'RCC')
	AND D.RCC_CD = @[crnt_loc_cd]
#elseif (${location} == 'LCC')
	AND D.LCC_CD = @[crnt_loc_cd]
#elseif (${location} == 'SCC')
	AND D.SCC_CD = @[crnt_loc_cd]
#end
#if (${crnt_yd_cd} != '')
	AND A.CRNT_YD_CD IN ($crnt_yd_cd)
#end
#if (${aciac_div_cd} != '')
	AND A.ACIAC_DIV_CD = @[aciac_div_cd]
#end

#if (${include_np} != 'Y')
	AND A.AGMT_LSTM_CD <>'NP'
#end

#if (${inq_to_dys} != '')
	#if(${inq_fm_dys} != '')
        AND TO_CHAR(A.ONH_DT,'YYYY') >= @[inq_fm_dys] AND TO_CHAR(A.ONH_DT,'YYYY')<= @[inq_to_dys]
	#else
		AND TO_CHAR(A.ONH_DT,'YYYY')<= @[inq_to_dys]
	#end
#end
#if (${eq_tpsz_cd} != '')
	AND A.EQ_TPSZ_CD IN ($agmt_lstm_cd)
#end
#if (${agmt_lstm_cd} != '')
	AND A.AGMT_LSTM_CD IN ($agmt_lstm_cd)
#end
#if (${vndr_seq} != '')
	AND A.VNDR_SEQ IN ($vndr_seq)
#end
#if (${chss_mvmt_sts_cd} != '')
	AND A.CHSS_MVMT_STS_CD IN ($chss_mvmt_sts_cd)
#end

#if ( ${s3_gtotal} == 'GTOTAL' )

#else
#if (${group1} == '1') 
	AND D.LCC_CD = @[group_value1]
#elseif (${group1} == '2') 
	AND C.SCC_CD = @[group_value1]
#elseif (${group1} == '3') 
	AND A.CRNT_YD_CD = @[group_value1]
#elseif (${group1} == '4') 
	AND A.AGMT_LSTM_CD = @[group_value1]
#elseif (${group1} == '5') 
	AND A.VNDR_SEQ = SUBSTR(@[group_value1], INSTR(@[group_value1],'(')+1, 6)
#elseif (${group1} == '6') 
	AND A.CHSS_MVMT_STS_CD = @[group_value1]
#end

#if (${s_group1_val} != '')
	AND A.EQ_TPSZ_CD = @[s_group1_val]
#end
#end			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="crnt_loc_cd" type="12" value="" out="N"/>
				<param name="aciac_div_cd" type="12" value="" out="N"/>
				<param name="inq_fm_dys" type="12" value="" out="N"/>
				<param name="inq_to_dys" type="12" value="" out="N"/>
				<param name="group_value1" type="12" value="" out="N"/>
				<param name="s_group1_val" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
