<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CEDEXCodeMgtDBDAOremoveDivisionCodeDataDSQL">
			<desc><![CDATA[removeDivisionCodeData]]></desc>
			<sql><![CDATA[
DELETE FROM MNR_CD_RLT A
WHERE A.EQ_CEDEX_RLT_TP_CD = @[eq_cedex_rlt_tp_cd]
AND   A.FM_RLT_CD = @[fm_rlt_cd]
AND   A.TO_RLT_CD = @[to_rlt_cd]
AND   A.COST_GRP_CD = @[cost_grp_cd]			]]></sql>
			<params>
				<param name="eq_cedex_rlt_tp_cd" type="12" value="" out="N"/>
				<param name="fm_rlt_cd" type="12" value="" out="N"/>
				<param name="to_rlt_cd" type="12" value="" out="N"/>
				<param name="cost_grp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
