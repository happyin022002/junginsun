<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrPlanGuidelineManageDBDAOSearchAddGlineEtaDtRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_ASC(A XAK3VSK_VSL_PORT_SKD) */ 
       TO_CHAR(VPS_ETA_DT, 'MON-DD')    	ETA_DT     -- 화면표현
      ,TO_CHAR(A.VPS_ETA_DT, 'YYYYMMDD')    EFF_ST_DT  -- HIDDEN
  FROM VSK_VSL_PORT_SKD A
 --WHERE A.SLAN_CD       = SUBSTR([lane],6)	
 WHERE A.SLAN_CD       = @[lane]			        -- 입력값
   AND VSL_CD          = SUBSTR(@[s_vvd_cd], 0, 4)  -- VVD 변수
   AND SKD_VOY_NO      = SUBSTR(@[s_vvd_cd], 5, 4)  -- VVD 변수
   AND SKD_DIR_CD      = SUBSTR(@[s_vvd_cd], 9, 1)  -- VVD 변수
   AND VPS_PORT_CD     = @[s_pol_cd]  -- POL_CD 변수
#if (${h_eta_dt} != '') 
   AND A.VPS_ETA_DT > TO_DATE(@[h_eta_dt],'YYYYMMDD HH24MISS')+0.99999 --GUIDELINE AMEND
#else 
--   AND A.VPS_ETA_DT > SYSDATE  --GUIDELINE ADD
#end 
   AND CALL_YD_IND_SEQ = 1                          -- 하드코딩
--   AND VPS_PORT_CD IN (
--                        SELECT LOC_CD 
--                          FROM MDM_LOCATION
--                         WHERE CONTI_CD      <> 'A'  -- 하드코딩, ASIA 제외
--                           AND CALL_PORT_FLG = 'Y'   -- 하드코딩, PORT 만 검색
--                           AND DELT_FLG      = 'N'   -- 하드코딩, DELT 제거
--                     )
  ORDER BY CLPT_SEQ			]]></sql>
			<params>
				<param name="lane" type="12" value="" out="N"/>
				<param name="s_vvd_cd" type="12" value="" out="N"/>
				<param name="s_pol_cd" type="12" value="" out="N"/>
				<param name="h_eta_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
