<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOsearchSCExptAcptStatusDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 
	A.SC_EXPT_VER_SEQ,
	A.PROP_NO,
	TO_CHAR(A.EFF_DT, 'YYYY-MM-DD') EFF_DT,
	TO_CHAR(A.EXP_DT, 'YYYY-MM-DD') EXP_DT,
    A.FT_FLG
FROM CGM_SC_EXPT_VER A
WHERE 1=1
  AND A.PROP_NO = @[prop_no]
  AND A.SC_EXPT_VER_SEQ = 
		(
			SELECT	/*+ INDEX_DESC(CGM_SC_EXPT_VER XPKCGM_SC_EXPT_VER) */ SC_EXPT_VER_SEQ
			FROM	CGM_SC_EXPT_VER
			WHERE	PROP_NO = A.PROP_NO
			AND	ROWNUM = 1
		)
  AND A.CHSS_EXPT_VER_STS_CD IN ( 'A', 'L' )			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
