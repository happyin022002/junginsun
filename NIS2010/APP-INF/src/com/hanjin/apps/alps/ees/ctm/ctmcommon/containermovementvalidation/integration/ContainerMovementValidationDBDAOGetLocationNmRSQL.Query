<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementValidationDBDAOGetLocationNmRSQL">
			<desc><![CDATA[Loc Cd에 해당하는 Location Name을 조회한다]]></desc>
			<sql><![CDATA[
SELECT LOC_NM FROM MDM_LOCATION WHERE  LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
