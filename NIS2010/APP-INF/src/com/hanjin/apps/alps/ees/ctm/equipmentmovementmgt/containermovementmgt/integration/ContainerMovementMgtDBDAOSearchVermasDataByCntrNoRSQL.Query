<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOSearchVermasDataByCntrNoRSQL">
			<desc><![CDATA[Booking I/F를 위한 VERMAS data 조회.]]></desc>
			<sql><![CDATA[
SELECT VGM_WGT_QTY,
  VRFD_WGT_CD,
  VGM_MZD_TP_CD,
  TO_CHAR(VGM_VRFY_DT, 'YYYYMMDDHH24MI') AS VGM_VRFY_DT,
  PTY_PSON_NM,
  REF_NO
FROM (
   SELECT MSG.*,
     ROW_NUMBER() OVER(ORDER BY DECODE(PTY_FUNC_CD, 'SPC', 1, 'WPA', 2, 'AM', 3, 'WC', 4, 'OB', 5) ASC, VGM_SEQ DESC) AS RN
   FROM CTM_VRFD_GRS_MASS_EDI_MSG MSG
   WHERE 1 = 1
     AND CNTR_NO = @[cntr_no]
     AND BKG_NO = @[bkg_no]
  )
WHERE RN = 1
			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
