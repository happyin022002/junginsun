<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanSensitivityAnalysisDBDAOUpdateSensitivitySTOn_USQL">
			<desc><![CDATA[EQR_SHRT_TERM_ONH_COND테이블에 대한  QTY 수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_SHRT_TERM_ONH_COND 
       SET   AVAL_CNTR_QTY    = ${curr_limit}          
            ,UPD_USR_ID       = @[user_id]         	
            ,UPD_DT           = sysdate   			
WHERE ECC_CD = @[ecc_cd]                   
AND CNTR_TPSZ_CD   = @[cntr_tpsz_cd]        	
				]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
