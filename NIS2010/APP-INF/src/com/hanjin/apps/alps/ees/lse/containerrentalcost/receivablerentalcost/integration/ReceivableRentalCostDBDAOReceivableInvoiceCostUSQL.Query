<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceivableRentalCostDBDAOReceivableInvoiceCostUSQL">
			<desc><![CDATA[Receivable Rental Invoice Cost 내역을 갱신합니다.]]></desc>
			<sql><![CDATA[
UPDATE  LSE_RCV_RNTL_CHG_DTL
SET     ONH_DT       = TO_DATE(@[onh_dt], 'YYYYMMDD'),
        ONH_LOC_CD   = @[onh_loc_cd], 
        OFFH_DT      = TO_DATE(@[offh_dt],'YYYYMMDD'),
        OFFH_LOC_CD  = @[offh_loc_cd],
        BIL_DYS      = @[bil_dys],
#if (${lse_rcv_chg_tp_cd} != "PDM")
        CHG_RT_AMT   = @[cost_amt],
#end                
        COST_AMT     = @[cost_amt],
		CR_AMT       = @[cr_amt],
        LSE_RCV_CHG_CRE_CD = @[lse_rcv_chg_cre_cd],
        FREE_DYS     = @[free_dys],
        UPD_USR_ID	 = @[upd_usr_id],
        UPD_DT		 = SYSDATE
WHERE   COST_YRMON   = @[cost_yrmon]
AND     AGMT_CTY_CD  = @[agmt_cty_cd]
AND     AGMT_SEQ     = @[agmt_seq]
AND     RCV_RNTL_SEQ = @[rcv_rntl_seq]
AND     RCV_RNTL_DTL_SEQ = @[rcv_rntl_dtl_seq]			]]></sql>
			<params>
				<param name="onh_dt" type="12" value="" out="N"/>
				<param name="onh_loc_cd" type="12" value="" out="N"/>
				<param name="offh_dt" type="12" value="" out="N"/>
				<param name="offh_loc_cd" type="12" value="" out="N"/>
				<param name="bil_dys" type="2" value="" out="N"/>
				<param name="cost_amt" type="6" value="" out="N"/>
				<param name="cr_amt" type="6" value="" out="N"/>
				<param name="lse_rcv_chg_cre_cd" type="12" value="" out="N"/>
				<param name="free_dys" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cost_yrmon" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="rcv_rntl_seq" type="2" value="" out="N"/>
				<param name="rcv_rntl_dtl_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
