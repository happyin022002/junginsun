<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchCHSEqSpecDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT A.EQ_SPEC_NO,
       A.EQ_KND_CD,
       A.EQ_TPSZ_CD,
       A.VNDR_SEQ,
       A.CHSS_TARE_WGT,
       A.CHSS_PAYLD_WGT,
       A.CHSS_TTL_DIM_LEN,
       A.CHSS_TTL_DIM_WDT,
       A.CHSS_TTL_DIM_HGT,
       A.MGST_VLTG_CAPA,
       A.MGST_FUEL_CAPA,
       A.CRE_USR_ID,
       A.CRE_DT,
       A.UPD_USR_ID,
       A.UPD_DT
  FROM CGM_EQ_SPEC A
 WHERE A.EQ_SPEC_NO = @[eq_spec_no]
   AND A.EQ_KND_CD = @[eq_knd_cd]			]]></sql>
			<params>
				<param name="eq_spec_no" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
