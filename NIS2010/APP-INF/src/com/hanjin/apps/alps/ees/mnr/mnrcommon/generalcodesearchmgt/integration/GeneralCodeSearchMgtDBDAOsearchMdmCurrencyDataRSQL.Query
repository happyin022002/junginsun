<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchMdmCurrencyDataRSQL">
			<desc><![CDATA[MDM Currency 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
         A.CURR_CD AS CD_ID
        ,A.CURR_DESC AS CD_DESC
FROM MDM_CURRENCY A
WHERE A.DELT_FLG = 'N'
ORDER BY A.CURR_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
