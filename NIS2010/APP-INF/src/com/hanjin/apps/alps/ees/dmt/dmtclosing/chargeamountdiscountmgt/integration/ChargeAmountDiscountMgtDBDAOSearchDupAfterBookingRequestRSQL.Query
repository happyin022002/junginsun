<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAOSearchDupAfterBookingRequestRSQL">
			<desc><![CDATA[중복된 After Booking Request 인지를 확인하는 조회쿼리]]></desc>
			<sql><![CDATA[
SELECT	COUNT(AFT_EXPT_DAR_NO)
FROM	DMT_AFT_BKG_ADJ_RQST
WHERE	AFT_EXPT_DAR_NO = @[aft_expt_dar_no]			]]></sql>
			<params>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
