<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOCheckBkgVolumeTargetRSQL">
			<desc><![CDATA[fixed plan 수정이 필요한 경우 기본정보를 수집]]></desc>
			<sql><![CDATA[
SELECT	 PLN.REPO_PLN_ID
		,PLN.PLN_YRWK
		,PLN.PLN_SEQ
		,PLN.REF_ID
		,PLN.TRSP_MOD_CD
		,PLN.FM_YD_CD
		,PLN.TO_YD_CD
		,PLN.VSL_CD
		,PLN.SKD_VOY_NO
		,PLN.SKD_DIR_CD
		,QTY.CNTR_TPSZ_CD
		,QTY.CNTR_QTY
FROM	${qty_table_name} QTY
		,(SELECT	 REPO_PLN_ID
					,PLN_YRWK
					,PLN_SEQ
					,REF_ID
					,TRSP_MOD_CD
					,FM_YD_CD
					,TO_YD_CD
					,VSL_CD
					,SKD_VOY_NO
					,SKD_DIR_CD
			FROM	${table_name}
			WHERE	MTY_BKG_NO	= @[mty_bkg_no]) PLN
WHERE	PLN.REPO_PLN_ID	= QTY.REPO_PLN_ID
	AND	PLN.PLN_YRWK	= QTY.PLN_YRWK
	AND	PLN.PLN_SEQ		= QTY.PLN_SEQ
	AND	PLN.REF_ID		= QTY.REF_ID
	AND	QTY.CNTR_TPSZ_CD= @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="mty_bkg_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
