<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOmodifyEQFlagFileListByYDDataUSQL">
			<desc><![CDATA[modifyEQFlagFileListByYDData]]></desc>
			<sql><![CDATA[
UPDATE MNR_DAT_VRFY A
SET INP_MSG4 = 'UY'
WHERE TMP_SEQ = @[tmp_seq]
AND   INP_MSG4 = 'SS'
AND NOT EXISTS 
( 
    SELECT B.YD_CD
    FROM MDM_YARD B
    WHERE A.INP_MSG2 = B.YD_CD
 )			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
