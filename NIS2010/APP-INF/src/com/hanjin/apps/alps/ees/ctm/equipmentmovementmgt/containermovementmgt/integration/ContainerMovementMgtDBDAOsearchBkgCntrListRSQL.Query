<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOsearchBkgCntrListRSQL">
			<desc><![CDATA[ContainerMovementMgtDBDAO
SEARCH BOOKING CONTAINER LIST BY CONTAINER]]></desc>
			<sql><![CDATA[
SELECT SUBSTR (CNT.CNTR_NO, 0, 10) CNTR_NO, SUBSTR (CNT.CNTR_NO, 11, 11) CHECK_DIGIT,
       CNT.CNTR_TPSZ_CD, CNT.CNMV_STS_CD PREV_STS_CD, CNT.BKG_NO, CNT.RCV_TERM_CD,
       CNT.CNMV_EVNT_DT
  FROM BKG_CONTAINER CNT, BKG_BOOKING BKG
 WHERE CNT.CNMV_CYC_NO = '9999'
   AND CNT.BKG_NO = BKG.BKG_NO
   AND BKG.BKG_CGO_TP_CD != 'P'
   AND NVL(BKG.BKG_STS_CD,' ') <> 'S'
   AND NVL(BKG.BKG_STS_CD,' ') <> 'X'
   AND CNT.CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
