<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchYardCdRSQL">
			<desc><![CDATA[mdm_yard 에 존재하는 Code 인지 확인]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX('Y'),'N') AS CHK_YD_CD
FROM MDM_YARD
WHERE YD_CD = @[yd_cd]
AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
