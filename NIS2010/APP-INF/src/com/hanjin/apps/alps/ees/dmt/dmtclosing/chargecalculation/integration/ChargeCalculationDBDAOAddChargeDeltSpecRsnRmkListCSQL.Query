<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOAddChargeDeltSpecRsnRmkListCSQL">
			<desc><![CDATA[ChargeCalculationDBDAOAddChargeDeltSpecRsnRmkListCSQL]]></desc>
			<sql><![CDATA[
MERGE INTO DMT_CHG_DELT_SPEC_RSN_RMK
  USING DUAL
  ON (      DELT_SPEC_RSN_RMK_SEQ   =   to_number(@[delt_spec_rsn_rmk_seq])
     AND    DELT_RMK_LVL            =   to_number(@[delt_rmk_lvl])
     )
  WHEN MATCHED THEN
    UPDATE
      SET DELT_RMK      =   @[delt_rmk]
        , UPD_USR_ID    =   @[upd_usr_id]
        , UPD_DT        =   nvl(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), sysdate)
  WHEN NOT MATCHED THEN
    INSERT (DELT_SPEC_RSN_RMK_SEQ 
           ,DELT_RMK_LVL
           ,DELT_RMK
           ,CRE_USR_ID
           ,CRE_DT
           ,UPD_USR_ID
           ,UPD_DT )
    VALUES (
    		to_number(@[delt_spec_rsn_rmk_seq])
    	   ,to_number(@[delt_rmk_lvl])
    	   ,@[delt_rmk]
    	   ,@[cre_usr_id]
    	   ,nvl(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), sysdate)
    	   ,@[upd_usr_id]
    	   ,nvl(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), sysdate)
   )			]]></sql>
			<params>
				<param name="delt_spec_rsn_rmk_seq" type="12" value="" out="N"/>
				<param name="delt_rmk_lvl" type="12" value="" out="N"/>
				<param name="delt_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
