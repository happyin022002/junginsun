<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MisUseApprovalDBDAOMisUseApprovalNumberRSQL">
			<desc><![CDATA[Miss Use 최대 Approval No.를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  RQST_FIX||TRIM(TO_CHAR(RQST_CNT,'000000')) APRO_NO
FROM   (SELECT  @[ofc_cd]||TO_CHAR(SYSDATE, 'YYMMDD')||'H' AS RQST_FIX,
               (SELECT  COUNT(APRO_NO) +1 
				FROM 	LSE_MSS_USD_APRO) AS RQST_CNT
        FROM    DUAL)			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
