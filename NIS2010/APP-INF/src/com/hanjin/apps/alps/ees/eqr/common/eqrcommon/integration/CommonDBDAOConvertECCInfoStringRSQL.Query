<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOConvertECCInfoStringRSQL">
			<desc><![CDATA[ecc code를 검색해서 String 으로 리턴 EesEqr0105 참조]]></desc>
			<sql><![CDATA[
SELECT 
	DISTINCT(ECC_CD) ECC_CD			 
FROM MDM_EQ_ORZ_CHT						                    
#if(${loctype} == 'R') 
	WHERE RCC_CD IN (
		#if($locationArr.size() == 0)
			''
		#else
			#foreach( ${key} in ${locationArr}) 
				#if($velocityCount < $locationArr.size()) 
					'$key', 
				#else 
					'$key' 
				#end 
			#end
		#end
	)
	  		
#elseif(${loctype} == 'L')
	WHERE LCC_CD IN (
		#if($locationArr.size() == 0)
			''
		#else
			#foreach( ${key} in ${locationArr}) 
				#if($velocityCount < $locationArr.size()) 
					'$key', 
				#else 
					'$key' 
				#end 
			#end
		#end
	)
	  		
#elseif(${loctype} == 'E')
	WHERE ECC_CD IN (
		#if($locationArr.size() == 0)
			''
		#else
			#foreach( ${key} in ${locationArr}) 
				#if($velocityCount < $locationArr.size()) 
					'$key', 
				#else 
					'$key' 
				#end 
			#end
		#end
	)
			
#elseif(${loctype} == 'S')
	WHERE SCC_CD IN (
		#if($locationArr.size() == 0)
			''
		#else
			#foreach( ${key} in ${locationArr}) 
				#if($velocityCount < $locationArr.size()) 
					'$key', 
				#else 
					'$key' 
				#end 
			#end
		#end
	)
			
#elseif(${loctype} == 'Y')
	WHERE SCC_CD IN ( 
		SELECT 
			LOC_CD 
		FROM MDM_YARD  
	  	WHERE  
			YD_CD IN (            
				#if($locationArr.size() == 0)
					''
				#else
					#foreach( ${key} in ${locationArr}) 
						#if($velocityCount < $locationArr.size()) 
							'$key', 
						#else 
							'$key' 
						#end 
					#end
				#end
			)
	)			
#end
ORDER BY 1			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
