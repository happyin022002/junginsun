<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnsiteInspectionResultMgtDBDAOInsertHeaderCSQL">
			<desc><![CDATA[사용자가 지정한 Yard Code, Vendor Sequence, Inspection Date가 기존에 저장되지 않은 정보일 경우,
Header를 생성하여 초기화를 하여 준다.]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_ONSITE_INSP_RSLT 
( 
VNDR_SEQ, 
YD_CD, 
FLD_INSP_DT, 
INSP_OFC_CD, 
FILE_SEQ, 
CRE_USR_ID, 
CRE_DT, 
UPD_USR_ID, 
UPD_DT,
BRNC_INSP_FLG,
HDBRN_INSP_FLG
)
VALUES
(
@[vndr_seq],
@[yd_cd],
TO_DATE(@[fld_insp_dt], 'YYYY-MM-DD HH:MI:SS'), 
@[insp_ofc_cd],
'0',
@[usr_id],
sysdate,
@[usr_id],
sysdate,
'',
''
)			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="fld_insp_dt" type="12" value="" out="N"/>
				<param name="insp_ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
