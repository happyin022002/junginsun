<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOdeleteBareMGSetDataDSQL">
			<desc><![CDATA[deleteBareMGSetData]]></desc>
			<sql><![CDATA[
UPDATE CGM_BARE_MGST_REPO
SET DELT_FLG = 'Y'
   ,UPD_USR_ID        = @[upd_usr_id]
   ,UPD_DT            = SYSDATE
WHERE MGST_NO           = @[mgst_no]
AND   MGST_BARE_STS_SEQ = @[mgst_bare_sts_seq] 			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mgst_no" type="12" value="" out="N"/>
				<param name="mgst_bare_sts_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
