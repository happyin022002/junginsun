<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOModifyBeforeExceptionCoverageUSQL">
			<desc><![CDATA[RFA Exception Terms Entry 에 대한  Multi Origin or Destination 수정용 쿼리]]></desc>
			<sql><![CDATA[
UPDATE DMT_RFA_EXPT_CVRG_CMB SET 
	CVRG_CONTI_CD = NVL(@[cvrg_conti_cd], ' ')
,	CVRG_CNT_CD = NVL(@[cvrg_cnt_cd], ' ')
,	CVRG_RGN_CD = NVL(@[cvrg_rgn_cd], ' ')
,	CVRG_STE_CD = NVL(@[cvrg_ste_cd], ' ')
,	CVRG_LOC_CD = NVL(@[cvrg_loc_cd], ' ')
,	ORG_DEST_CONTI_CD = NVL(@[org_dest_conti_cd], ' ')
,	ORG_DEST_CNT_CD = NVL(@[org_dest_cnt_cd], ' ')
,	ORG_DEST_RGN_CD = NVL(@[org_dest_rgn_cd], ' ')
,	ORG_DEST_STE_CD = NVL(@[org_dest_ste_cd], ' ')
,	ORG_DEST_LOC_CD = NVL(@[org_dest_loc_cd], ' ')
,	N1ST_CMNC_VER_SEQ = @[n1st_cmnc_ver_seq]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), SYSDATE)
,	UPD_OFC_CD = @[upd_ofc_cd]
WHERE	RFA_EXPT_DAR_NO = @[rfa_expt_dar_no]
AND	RFA_EXPT_MAPG_SEQ = @[rfa_expt_mapg_seq]
AND	RFA_EXPT_VER_SEQ = @[rfa_expt_ver_seq]
AND	RFA_RQST_DTL_SEQ = @[rfa_rqst_dtl_seq]
AND	CVRG_CMB_SEQ = @[cvrg_cmb_seq]			]]></sql>
			<params>
				<param name="cvrg_conti_cd" type="12" value="" out="N"/>
				<param name="cvrg_cnt_cd" type="12" value="" out="N"/>
				<param name="cvrg_rgn_cd" type="12" value="" out="N"/>
				<param name="cvrg_ste_cd" type="12" value="" out="N"/>
				<param name="cvrg_loc_cd" type="12" value="" out="N"/>
				<param name="org_dest_conti_cd" type="12" value="" out="N"/>
				<param name="org_dest_cnt_cd" type="12" value="" out="N"/>
				<param name="org_dest_rgn_cd" type="12" value="" out="N"/>
				<param name="org_dest_ste_cd" type="12" value="" out="N"/>
				<param name="org_dest_loc_cd" type="12" value="" out="N"/>
				<param name="n1st_cmnc_ver_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
				<param name="rfa_rqst_dtl_seq" type="12" value="" out="N"/>
				<param name="cvrg_cmb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
