<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrSafetyStockManageDBDAODeleteCntrSafetyStockDSQL">
			<desc><![CDATA[EES_EQR_0026 : Safty Stock
	- EQR_SCNR_ECC_SFT_STK]]></desc>
			<sql><![CDATA[
DELETE FROM EQR_SCNR_ECC_SFT_STK
WHERE	SCNR_ID = @[scnr_id]
	AND	ECC_CD	= @[ecc_cd]
	AND	CNTR_TPSZ_CD	= @[cntr_tpsz_cd]
	#if (${lvl_cd} != '') 
	AND	SFSTK_LVL_CD = @[sfstk_lvl_cd]
	#end
			]]></sql>
			<params>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="sfstk_lvl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
