<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchUserOfcCdRSQL">
			<desc><![CDATA[User ID 소속 Office 조회.(Session 정보가 아닌 실제 소속)]]></desc>
			<sql><![CDATA[
SELECT OFC_CD FROM COM_USER WHERE USR_ID  = @[usr_id]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
