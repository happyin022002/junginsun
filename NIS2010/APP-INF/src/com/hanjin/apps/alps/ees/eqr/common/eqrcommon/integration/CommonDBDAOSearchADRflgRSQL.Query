<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchADRflgRSQL">
			<desc><![CDATA[EQR_SCNR_MST ADFflag 조회]]></desc>
			<sql><![CDATA[
SELECT 
  SCNR_AUTO_GEN_FLG, 
  REPO_PLN_CRE_FLG, 
  REPO_PLN_DTRB_FLG , 		
  CASE WHEN (SCNR_AUTO_GEN_FLG='Y' OR REPO_PLN_CRE_FLG ='Y' OR REPO_PLN_DTRB_FLG = 'Y') THEN 
    'FALSE' 
  ELSE 
    'TRUE'
  END AS ADRflg  		
FROM EQR_SCNR_MST						
WHERE SCNR_ID = @[scnr_id]                             								]]></sql>
			<params>
				<param name="scnr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
