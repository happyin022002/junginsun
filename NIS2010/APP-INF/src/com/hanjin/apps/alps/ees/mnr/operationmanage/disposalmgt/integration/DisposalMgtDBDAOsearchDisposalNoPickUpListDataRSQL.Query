<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DisposalMgtDBDAOsearchDisposalNoPickUpListDataRSQL">
			<desc><![CDATA[searchDisposalNoPickUpListData]]></desc>
			<sql><![CDATA[
SELECT
    C.DISP_NO
    , A.INV_NO
    , (SELECT MNR_PRNR_LGL_ENG_NM 
     FROM MNR_PARTNER P
     WHERE A.MNR_PRNR_CNT_CD = P.MNR_PRNR_CNT_CD
     AND A.MNR_PRNR_SEQ = P.MNR_PRNR_SEQ
     AND ROWNUM = 1 ) AS BUYER_NAME
    , A.EQ_NO
    , A.EQ_TPSZ_CD
    , B.LSTM_CD
    , B.MVMT_CD
    , A.DISP_YD_CD
    , TO_CHAR(C.RQST_DT, 'YYYY-MM-DD') RQST_DT
    , TO_CHAR(C.DISP_BULTN_DT, 'YYYY-MM-DD') DISP_BULTN_DT
    , TO_CHAR(C.DISP_ST_DT, 'YYYY-MM-DD HH24:MI:SS') DISP_ST_DT
    , TO_CHAR(C.DISP_END_DT, 'YYYY-MM-DD HH24:MI:SS') DISP_END_DT
    , TO_CHAR(C.DISP_PKUP_ST_DT, 'YYYY-MM-DD') DISP_PKUP_ST_DT
    , TO_CHAR(C.DISP_PKUP_END_DT, 'YYYY-MM-DD') DISP_PKUP_END_DT
    , A.PART_AMT
FROM
    MNR_DISP_DTL A
    , MNR_EQ_STS_V B
    , MNR_DISP_HDR C
WHERE 

#if (${disp_no_list} != '')
	A.DISP_NO IN (
		#foreach ($user_disp_no IN ${dispNos})
			#if($velocityCount < $dispNos.size())
				'$user_disp_no',
			#else
				'$user_disp_no'
			#end
		#end                      
	)
#end

    AND A.EQ_NO = B.EQ_NO(+)
    AND A.DISP_NO = C.DISP_NO
	AND A.DISP_SOLD_DT IS NULL			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
