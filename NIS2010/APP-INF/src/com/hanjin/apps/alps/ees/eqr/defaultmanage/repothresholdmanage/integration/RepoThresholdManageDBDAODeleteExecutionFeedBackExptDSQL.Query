<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepoThresholdManageDBDAODeleteExecutionFeedBackExptDSQL">
			<desc><![CDATA[EQR_REPO_EXE_PLN_FB_EXPT 테이블의 데이터 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM 
	EQR_REPO_EXE_PLN_FB_EXPT
WHERE 
	FB_ITM_CD      = @[fb_itm_cd]
	#if (${ismain} == 'N') 
		AND   FB_ITM_BSE_CD  = @[fb_itm_bse_cd]
	#else
		AND   FB_ITM_BSE_CD  = @[old_bse_cd]
	#end
	#if (${ismain} == 'N') 
		AND   LOC_GRP_CD	 = @[loc_grp_cd]
		AND   LOC_CD         = @[loc_cd]
		AND   CNTR_TPSZ_CD   = @[cntr_tpsz_cd]
	#end			]]></sql>
			<params>
				<param name="fb_itm_cd" type="12" value="" out="N"/>
				<param name="fb_itm_bse_cd" type="12" value="" out="N"/>
				<param name="old_bse_cd" type="12" value="" out="N"/>
				<param name="loc_grp_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
