<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UncollectedCargoDBDAOSearchUncollectedVolDtlListRSQL">
			<desc><![CDATA[UC-VOL_DTL 조회 ]]></desc>
			<sql><![CDATA[
SELECT  DECODE(C.CNTR_NO,U.CNTR_NO,'1','0') AS SEL
        , U.UC_CS_NO
        , B.BKG_NO
        , B.BL_NO
        , C.CNTR_NO
        , C.CNTR_TPSZ_CD
		, '' AS VOL_DTL_GB
FROM BKG_BOOKING B
    , BKG_CONTAINER C
    , CIM_UC_CGO_CNTR U
WHERE B.BKG_NO = C.BKG_NO
AND B.BL_NO  = U.BL_NO
	AND C.CNTR_NO  = U.CNTR_NO
	AND U.UC_CS_NO = @[uc_cs_no]
ORDER BY U.CNTR_NO			]]></sql>
			<params>
				<param name="uc_cs_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
