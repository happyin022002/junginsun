<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOSearchVDMovementByPodEtaRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT	TO_CHAR (M.CNMV_EVNT_DT, 'YYYYMMDD') VD_MVMT_DT
FROM	CTM_MOVEMENT M
WHERE	M.CNTR_NO		= @[cntr_no]	
AND		M.BKG_NO 		= @[bkg_no]
AND     M.ORG_YD_CD     = @[fm_mvmt_yd_cd]
AND		M.MVMT_STS_CD	= 'VD'
AND		ROWNUM			= 1			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="fm_mvmt_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
