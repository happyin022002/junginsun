<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCollectionOfficeMgtDBDAODeleteYardExceptionCostDSQL">
			<desc><![CDATA[Yard Exception code 삭제]]></desc>
			<sql><![CDATA[
DELETE DMT_YD_EXPT_COST
WHERE YD_CD = @[yd_cd]           
AND   YD_EXPT_COST_SEQ = @[yd_expt_cost_seq]
AND   CFM_FLG <> 'Y'			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="yd_expt_cost_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
