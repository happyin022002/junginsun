<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOSearchCHSNuPoolChargeMainDataRSQL">
			<desc><![CDATA[ChassisMgsetInvoiceDB.SearchCHSNuPoolChargeMainData]]></desc>
			<sql><![CDATA[
SELECT A.PAY_INV_SEQ,
	   A.INV_NO,
	   A.VNDR_SEQ,
		(SELECT VNDR_LGL_ENG_NM FROM MDM_VENDOR 
         WHERE VNDR_SEQ = A.VNDR_SEQ) AS VNDR_LGL_ENG_NM,
	   A.COST_YRMON, 
	   A.COST_OFC_CD, 
	   A.AGMT_OFC_CTY_CD, 
	   A.AGMT_SEQ, 
	   A.DIFF_RMK,
	   A.CHSS_MGST_INV_KND_CD,
	   A.CHSS_MGST_INV_STS_CD,
	   A.POOL_MAX_RT_AMT,
       A.POOL_MIN_RT_AMT,
	   TO_CHAR(A.INV_DT,'YYYY-MM-DD') AS INV_DT
FROM CGM_PAY_INV A
WHERE A.INV_NO = @[inv_no]
	  AND A.EQ_KND_CD = 'Z'
	  AND A.CHSS_MGST_INV_KND_CD = 'NP'			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
