<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchEqSpecChkDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[



SELECT A.EQ_LOT_NO,
       A.EQ_KND_CD,
       A.EQ_PFX_CD||A.FM_SER_NO AS SERIAL_NO,
       A.EQ_PFX_CD,
       A.FM_SER_NO,
       A.TO_SER_NO,
       EQ_SPEC_NO
  FROM CGM_EQ_LOT A
 WHERE @[eq_lot_no_etc] BETWEEN A.EQ_PFX_CD||A.FM_SER_NO AND A.EQ_PFX_CD ||A.TO_SER_NO			]]></sql>
			<params>
				<param name="eq_lot_no_etc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
