<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReeferSparePartMgtDBDAOsearchVesselInventoryListDataRSQL">
			<desc><![CDATA[조회]]></desc>
			<sql><![CDATA[
WITH PC AS (
SELECT SPR_PRT_VER_SEQ, SPR_PRT_VNDR_SEQ, SPR_UT_MDL_NM, VNDR_NM, SPR_PRT_DP_SEQ
FROM (
    SELECT SPR_PRT_VER_SEQ, SPR_PRT_VNDR_SEQ, SPR_UT_MDL_NM        
        , CASE WHEN SPR_UT_MDL_NM = 'GE' THEN 'General'
            ELSE ( SELECT NVL(VNDR_ABBR_NM,VNDR_LGL_ENG_NM) AS VNDR_NM
                    FROM MDM_VENDOR V
                    WHERE V.VNDR_SEQ = C.SPR_PRT_VNDR_SEQ
                    AND V.DELT_FLG = 'N'
                   ) 
          END AS VNDR_NM
		, MAX(C.SPR_PRT_DP_SEQ) AS SPR_PRT_DP_SEQ
    FROM MNR_VSL_SPR_PRT_CD C
    WHERE C.SPR_PRT_DELT_FLG = 'N'
    AND C.SPR_PRT_VER_SEQ = (
#if (${spr_prt_invt_no} == '') 
                                SELECT MAX(SPR_PRT_VER_SEQ) AS SPR_PRT_VER_SEQ
                                FROM MNR_VSL_SPR_PRT_CD
                                WHERE SPR_PRT_DELT_FLG = 'N'
#end

#if (${spr_prt_invt_no} != '')
								SELECT MAX( SPR_PRT_VER_SEQ ) AS SPR_PRT_VER_SEQ
                                FROM MNR_VSL_SPR_PRT_INVT IT
                                WHERE SPR_PRT_INVT_NO = @[spr_prt_invt_no]
                                AND DELT_FLG = 'N'
#end
                            )
    GROUP BY C.SPR_PRT_VER_SEQ, C.SPR_PRT_VNDR_SEQ, C.SPR_UT_MDL_NM

    ) SAM
    
    ORDER BY SPR_PRT_VER_SEQ, SPR_PRT_DP_SEQ, VNDR_NM
)
, MV AS (
    SELECT SPR_PRT_INVT_NO, CRE_USR_ID, CRE_DT
    -- SELECT *
    FROM MNR_VSL_SPR_PRT_INVT IT
    WHERE SPR_PRT_INVT_NO = @[spr_prt_invt_no]
    AND DELT_FLG = 'N'
    AND SPR_PRT_INVT_VER_SEQ = (
                                    SELECT MIN(SPR_PRT_INVT_VER_SEQ) AS SPR_PRT_INVT_VER_SEQ
                                    FROM MNR_VSL_SPR_PRT_INVT IV
                                    WHERE IV.SPR_PRT_INVT_NO = @[spr_prt_invt_no]

                                    GROUP BY IV.SPR_PRT_INVT_NO
                               )
	GROUP BY SPR_PRT_INVT_NO, CRE_USR_ID, CRE_DT
)
, VSPI AS (
    SELECT SPR_PRT_INVT_NO
		, MAX(SPR_PRT_INVT_VER_SEQ) AS SPR_PRT_INVT_VER_SEQ
		, LANE_CD, VSL_CD
        , SPR_PRT_VNDR_SEQ
        , SPR_PRT_TP_CD
        , MAX(SPR_PRT_VER_SEQ) AS SPR_PRT_VER_SEQ
        -- select *
    FROM MNR_VSL_SPR_PRT_INVT IT
    WHERE SPR_PRT_INVT_NO = @[spr_prt_invt_no]
	AND DELT_FLG = 'N'
    AND SPR_PRT_INVT_VER_SEQ = (
                                    SELECT MAX(SPR_PRT_INVT_VER_SEQ) AS SPR_PRT_INVT_VER_SEQ
                                    FROM MNR_VSL_SPR_PRT_INVT IV
                                    WHERE IV.SPR_PRT_INVT_NO = @[spr_prt_invt_no]

                                    GROUP BY IV.SPR_PRT_INVT_NO
                                )
    GROUP BY SPR_PRT_INVT_NO, SPR_PRT_INVT_VER_SEQ, LANE_CD, VSL_CD, SPR_PRT_VNDR_SEQ, SPR_PRT_TP_CD
)

SELECT SPI.SPR_PRT_INVT_NO, rownum AS SPR_PRT_INVT_SEQ, SPI.SPR_PRT_INVT_VER_SEQ
    , SPI.LANE_CD, SPI.VSL_CD
    , ( SELECT VSL_ENG_NM
        FROM MDM_VSL_CNTR
        WHERE VSL_CD = SPI.VSL_CD
        AND DELT_FLG = 'N'
       )AS VSL_NM
    , ( SELECT CRR_CD
        FROM MDM_VSL_CNTR
        WHERE VSL_CD = SPI.VSL_CD
        AND DELT_FLG = 'N'
       )  AS CRR_CD
    , SPI.SPR_PRT_TP_CD
    , SPI.SPR_PRT_VNDR_SEQ
    , SPI.SPR_PRT_VER_SEQ
    , SPI.VNDR_NM
    , SPI.SPR_UT_MDL_NM
    , SPI.SPR_UT_SHR_CD
    , SPI.SPR_PRT_CRNT_AMT
	, 'N' AS DELT_FLG
	, MV.CRE_USR_ID AS CRE_USR_ID
	, TO_CHAR(MV.CRE_DT, 'YYYY-MM-DD') AS CRE_DT
	, '' AS UPD_USR_ID
	, '' AS UPD_DT
	, '' AS OFC_CD
	, '' AS FROM_DT
	, '' AS TO_DT
FROM (

        SELECT VSPI.SPR_PRT_INVT_NO
            , VSPI.SPR_PRT_INVT_VER_SEQ, VSPI.LANE_CD
            , VSPI.VSL_CD
            , VSPI.SPR_PRT_TP_CD
            , VSPI.SPR_PRT_VNDR_SEQ
            , VSPI.SPR_PRT_VER_SEQ
            , PC.VNDR_NM
            , PC.SPR_UT_MDL_NM
            , NVL(
                    (SELECT SPR_UT_SHR_CD
                       FROM MNR_VSL_SPR_PRT_INVT
                      WHERE SPR_PRT_INVT_NO = VSPI.SPR_PRT_INVT_NO
                        AND SPR_PRT_INVT_VER_SEQ = VSPI.SPR_PRT_INVT_VER_SEQ
						AND SPR_PRT_VNDR_SEQ = VSPI.SPR_PRT_VNDR_SEQ						
                        AND LANE_CD = VSPI.LANE_CD
                        AND VSL_CD = VSPI.VSL_CD
                        AND SPR_PRT_TP_CD = VSPI.SPR_PRT_TP_CD
                        AND SPR_UT_MDL_NM = PC.SPR_UT_MDL_NM
                        AND SPR_PRT_VER_SEQ = VSPI.SPR_PRT_VER_SEQ
                    ), '') AS SPR_UT_SHR_CD
            , NVL(
                    (SELECT SPR_PRT_CRNT_AMT 
                       FROM MNR_VSL_SPR_PRT_CD
                      WHERE SPR_PRT_DELT_FLG = 'N'
                        AND SPR_PRT_VNDR_SEQ = VSPI.SPR_PRT_VNDR_SEQ
                        AND SPR_PRT_TP_CD = VSPI.SPR_PRT_TP_CD
						AND SPR_UT_MDL_NM = PC.SPR_UT_MDL_NM
                        AND SPR_PRT_VER_SEQ = (
                                                   SELECT MAX(SPR_PRT_VER_SEQ)
                                                   FROM MNR_VSL_SPR_PRT_CD
                                                   WHERE SPR_PRT_DELT_FLG = 'N'
                                                    AND SPR_PRT_VNDR_SEQ = VSPI.SPR_PRT_VNDR_SEQ
                                                    AND SPR_PRT_TP_CD = VSPI.SPR_PRT_TP_CD
                                                    AND SPR_PRT_VER_SEQ = VSPI.SPR_PRT_VER_SEQ
                                                )
                    ),0) AS SPR_PRT_CRNT_AMT
            , PC.SPR_PRT_DP_SEQ
            , TO_NUMBER( VSPI.SPR_PRT_VNDR_SEQ ) AS VNDR_SEQ_NUM   

        FROM PC
            , VSPI
        WHERE PC.SPR_PRT_VNDR_SEQ = VSPI.SPR_PRT_VNDR_SEQ
    ) SPI
    , MV
WHERE SPI. SPR_PRT_INVT_NO = MV.SPR_PRT_INVT_NO   
ORDER BY SPI.SPR_PRT_INVT_NO, SPI.LANE_CD, SPI.VSL_CD, SPI.SPR_PRT_TP_CD, SPI.SPR_PRT_VER_SEQ, SPI.SPR_PRT_DP_SEQ, SPI.VNDR_SEQ_NUM			]]></sql>
			<params>
				<param name="spr_prt_invt_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
