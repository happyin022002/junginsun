<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LongstayingUnclaimEQFlaggingDBDAONowWeekRSQL">
			<desc><![CDATA[현재주차]]></desc>
			<sql><![CDATA[
SELECT W.PLN_YR||W.PLN_WK as NOW_WEEK
  FROM EQR_WK_PRD W
 WHERE 1=1
   AND TO_CHAR(SYSDATE,'YYYYMMDD') BETWEEN W.WK_ST_DT AND W.WK_END_DT			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
