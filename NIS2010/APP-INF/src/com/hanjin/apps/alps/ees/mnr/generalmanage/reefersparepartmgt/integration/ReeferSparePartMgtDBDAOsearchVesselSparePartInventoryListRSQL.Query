<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReeferSparePartMgtDBDAOsearchVesselSparePartInventoryListRSQL">
			<desc><![CDATA[ReeferSparePartMgtDBDAOsearchVesselSparePartInventoryListRSQL]]></desc>
			<sql><![CDATA[
SELECT 
    SPR_PRT_INVT_NO, 
    MAX(SPR_PRT_INVT_VER_SEQ) AS SPR_PRT_INVT_VER_SEQ,
	TO_CHAR(MAX(CRE_DT),'YYYY-MM-DD') AS CRE_DT
FROM MNR_VSL_SPR_PRT_INVT
WHERE DELT_FLG = 'N'

#if (${from_dt} != '' && ${to_dt} != '') 
AND (TO_CHAR(CRE_DT,'YYYYMMDD') BETWEEN REPLACE(@[from_dt],'-','') AND REPLACE(@[to_dt],'-',''))
#end
#if (${lane_cd} != '') 
AND LANE_CD = @[lane_cd]
#end
#if (${vsl_cd} != '') 
AND VSL_CD = @[vsl_cd]
#end
#if (${spr_prt_tp_cd} != '') 
AND SPR_PRT_TP_CD = @[spr_prt_tp_cd]
#end
#if (${spr_prt_vndr_seq} != '') 
AND SPR_PRT_VNDR_SEQ = @[spr_prt_vndr_seq]
#end
GROUP BY SPR_PRT_INVT_NO
ORDER BY SPR_PRT_INVT_NO			]]></sql>
			<params>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="spr_prt_tp_cd" type="12" value="" out="N"/>
				<param name="spr_prt_vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
