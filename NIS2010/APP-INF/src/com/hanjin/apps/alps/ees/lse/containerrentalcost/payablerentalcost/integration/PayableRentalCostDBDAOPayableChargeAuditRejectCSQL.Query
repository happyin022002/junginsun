<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOPayableChargeAuditRejectCSQL">
			<desc><![CDATA[Payable Charge Audit Data backup. Reject Table Insert.]]></desc>
			<sql><![CDATA[
INSERT INTO LSE_PAY_RNTL_CHG_DTL_RJCT (
       CHG_SEQ
     , CNTR_NO
     , LSE_PAY_CHG_TP_CD
     , DTL_SEQ
     , AGMT_CTY_CD
     , AGMT_SEQ
     , RJCT_HIS_SEQ
     , CNTR_TPSZ_CD
     , ONH_DT
     , OFFH_DT
     , ONH_LOC_CD
     , OFFH_LOC_CD
     , CHG_FREE_DYS
     , PD_RT_AMT
     , TTL_COST_AMT
     , CR_AMT
     , CR_NO
     , CNTR_AUD_STS_CD
     , COST_DYS
     , BIL_DYS
     , DSCR_RT_AMT
     , DSCR_COST_AMT
     , DSCR_ONH_DT
     , DSCR_OFFH_DT
     , DSCR_ONH_LOC_CD
     , DSCR_OFFH_LOC_CD
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
)
SELECT CHG_SEQ
     , CNTR_NO
     , LSE_PAY_CHG_TP_CD
     , DTL_SEQ
     , AGMT_CTY_CD
     , AGMT_SEQ
     , ( SELECT NVL(MAX(RJCT_HIS_SEQ), 0) + 1
         FROM   LSE_PAY_RNTL_CHG_DTL_RJCT
         WHERE  AGMT_CTY_CD = @[agmt_cty_cd]
         AND    AGMT_SEQ    = @[agmt_seq]
         AND    CHG_SEQ     = @[chg_seq] ) AS RJCT_HIS_SEQ
     , CNTR_TPSZ_CD
     , ONH_DT
     , OFFH_DT
     , ONH_LOC_CD
     , OFFH_LOC_CD
     , CHG_FREE_DYS
     , PD_RT_AMT
     , TTL_COST_AMT
     , CR_AMT
     , CR_NO
     , CNTR_AUD_STS_CD
     , COST_DYS
     , BIL_DYS
     , DSCR_RT_AMT
     , DSCR_COST_AMT
     , DSCR_ONH_DT
     , DSCR_OFFH_DT
     , DSCR_ONH_LOC_CD
     , DSCR_OFFH_LOC_CD
     , @[usr_id]
     , SYSDATE
     , @[usr_id]
     , SYSDATE
FROM   LSE_PAY_RNTL_CHG_DTL
WHERE  AGMT_CTY_CD = @[agmt_cty_cd]
AND    AGMT_SEQ = @[agmt_seq]
AND    CHG_SEQ = @[chg_seq]			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="chg_seq" type="2" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
