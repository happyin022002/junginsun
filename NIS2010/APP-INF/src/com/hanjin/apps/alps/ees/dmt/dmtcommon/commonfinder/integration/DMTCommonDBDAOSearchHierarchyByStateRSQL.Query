<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchHierarchyByStateRSQL">
			<desc><![CDATA[SearchHierarchyByState]]></desc>
			<sql><![CDATA[
SELECT 
	D.CONTI_CD
,	D.CONTI_NM
,	B.CNT_CD
,	B.CNT_NM
,	A.STE_CD
,	A.STE_NM
  FROM MDM_STATE A
      ,MDM_COUNTRY B
      ,MDM_SUBCONTINENT C
      ,MDM_CONTINENT D
 WHERE A.STE_CD = @[ste_cd]
	AND A.CNT_CD IN ('US','CA')
	AND A.DELT_FLG = 'N'
	AND A.CNT_CD = B.CNT_CD
	AND B.SCONTI_CD = C.SCONTI_CD
	AND C.CONTI_CD = D.CONTI_CD			]]></sql>
			<params>
				<param name="ste_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
