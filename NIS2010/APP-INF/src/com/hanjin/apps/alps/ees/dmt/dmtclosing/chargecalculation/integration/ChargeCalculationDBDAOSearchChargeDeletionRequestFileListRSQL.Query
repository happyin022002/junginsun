<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOSearchChargeDeletionRequestFileListRSQL">
			<desc><![CDATA[ChargeCalculationDBDAOSearchChargeDeletionRequestFileListRSQL]]></desc>
			<sql><![CDATA[
select  T1.FILE_SAV_ID
       ,T2.FILE_UPLD_NM     				as FILE_NM
       ,T2.FILE_SZ_CAPA     				as FILE_SIZE
       ,T2.FILE_PATH_URL    				as FILE_PATH
       ,to_char(T2.UPD_DT, 'YYYY-MM-DD')   	as UPD_DT
  from  DMT_CHG_DELT_RQST_APRO  T0
       ,DMT_CHG_DELT_RQST_FILE  T1 
       ,COM_UPLD_FILE           T2
 where  T0.SYS_AREA_GRP_ID  	= @[sys_area_grp_id]
   and  T0.CNTR_NO 				= @[cntr_no]
   and  T0.CNTR_CYC_NO 			= to_number(@[cntr_cyc_no])
   and  T0.DMDT_TRF_CD 			= @[dmdt_trf_cd]
   and  T0.DMDT_CHG_LOC_DIV_CD 	= @[dmdt_chg_loc_div_cd]
   and  T0.CHG_SEQ 				= to_number(@[chg_seq])
   and  T0.CHG_OFC_CD 			= NVL(@[chg_ofc_cd],T0.CHG_OFC_CD)
   and  T0.DELT_SEQ 			= NVL(to_number(@[delt_seq]),T0.DELT_SEQ)
   AND  T0.DMDT_DELT_RQST_STS_CD = NVL(@[chg_delt_sts_cd],T0.DMDT_DELT_RQST_STS_CD)
   AND  T0.SYS_AREA_GRP_ID  	= T1.SYS_AREA_GRP_ID 
   and  T0.CNTR_NO 				= T1.CNTR_NO
   and  T0.CNTR_CYC_NO 			= T1.CNTR_CYC_NO 
   and  T0.DMDT_TRF_CD 			= T1.DMDT_TRF_CD
   and  T0.DMDT_CHG_LOC_DIV_CD 	= T1.DMDT_CHG_LOC_DIV_CD
   and  T0.CHG_SEQ 				= T1.CHG_SEQ
   and  T0.CHG_OFC_CD 			= T1.CHG_OFC_CD 
   and  T0.DELT_SEQ 			= T1.DELT_SEQ 
   and  T1.FILE_SAV_ID          = T2.FILE_SAV_ID
   and  T2.DELT_FLG             = 'N'			]]></sql>
			<params>
				<param name="sys_area_grp_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="dmdt_chg_loc_div_cd" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
				<param name="chg_ofc_cd" type="12" value="" out="N"/>
				<param name="delt_seq" type="12" value="" out="N"/>
				<param name="chg_delt_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
