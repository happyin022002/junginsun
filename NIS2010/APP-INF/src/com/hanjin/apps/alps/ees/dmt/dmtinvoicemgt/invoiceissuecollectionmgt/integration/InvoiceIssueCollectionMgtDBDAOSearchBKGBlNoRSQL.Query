<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchBKGBlNoRSQL">
			<desc><![CDATA[BKG_BOOKING 테이블에서 BKG NO에 해당하는 BL NO를 조회해 옴.]]></desc>
			<sql><![CDATA[
SELECT BL_NO 
FROM   BKG_BOOKING
WHERE  BKG_NO = @[bkg_no]			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
