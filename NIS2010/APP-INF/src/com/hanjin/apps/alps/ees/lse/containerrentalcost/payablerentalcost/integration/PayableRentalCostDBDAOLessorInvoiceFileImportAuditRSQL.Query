<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOLessorInvoiceFileImportAuditRSQL">
			<desc><![CDATA[Audit 된 Charge Creation 조회.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT
FROM   LSE_PAY_RNTL_CHG
WHERE  LSE_PAY_RNTL_STS_CD IN ('A','I')
AND    CHG_COST_YRMON = @[chg_cost_yrmon]
AND    AGMT_SEQ       = @[agmt_seq]
AND    AGMT_CTY_CD    = @[agmt_cty_cd]			]]></sql>
			<params>
				<param name="chg_cost_yrmon" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
