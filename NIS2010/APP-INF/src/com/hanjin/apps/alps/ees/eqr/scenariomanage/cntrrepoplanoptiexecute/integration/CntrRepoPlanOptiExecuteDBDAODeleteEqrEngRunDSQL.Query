<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanOptiExecuteDBDAODeleteEqrEngRunDSQL">
			<desc><![CDATA[eqr_eng_inp_opmz_run 테이블에 해당 엔진 run정보 삭제]]></desc>
			<sql><![CDATA[
DELETE EQR_ENG_INP_OPMZ_RUN	
      WHERE RUN_ID_NO = @[run_id_no]
            AND RUN_STS_CD IN ('CREATED','ABORTED')			]]></sql>
			<params>
				<param name="run_id_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
