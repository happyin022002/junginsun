<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchSysAreaGrpIdRSQL">
			<desc><![CDATA[OFC에 해당하는 SYS_AREA_GRP_ID(SVR_ID) 를 구한다.]]></desc>
			<sql><![CDATA[
SELECT SYS_AREA_GRP_ID SVR_ID
FROM   COM_SYS_AREA_GRP_ID
WHERE  CNT_CD = (SELECT CNT_CD
        FROM   MDM_LOCATION
        WHERE  LOC_CD = (SELECT LOC_CD
                FROM   MDM_ORGANIZATION
                WHERE  OFC_CD = SUBSTR(@[ofc_cd], 0, 5)))
AND    CO_IND_CD = 'H'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
