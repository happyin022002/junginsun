<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAODeleteYearNewVanPlanDSQL">
			<desc><![CDATA[EQR_NEW_VAN_LONG_TERM 테이블에서 특정 ECC삭제]]></desc>
			<sql><![CDATA[
DELETE 
    FROM EQR_NEW_VAN_LONG_TERM    					
WHERE 
    CO_CD		     = @[co_cd]                                
    AND   ECC_CD         = @[ecc_cd]                                
    AND   CNTR_LSTM_CD   = @[cntr_lstm_cd]                                
    AND   CNTR_TPSZ_CD   = @[cntr_tpsz_cd]                                
    AND   VNDR_CNT_CD    = @[vndr_cnt_cd]                                
    AND   VNDR_SEQ       = @[vndr_seq]                                
    AND   CNTR_DE_STS_CD = @[cntr_de_sts_cd]                                			]]></sql>
			<params>
				<param name="co_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_lstm_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="cntr_de_sts_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
