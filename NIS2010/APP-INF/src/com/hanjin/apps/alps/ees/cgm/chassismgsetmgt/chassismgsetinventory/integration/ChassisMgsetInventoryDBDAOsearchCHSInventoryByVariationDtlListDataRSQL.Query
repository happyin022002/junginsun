<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOsearchCHSInventoryByVariationDtlListDataRSQL">
			<desc><![CDATA[chungpa 20090810 1103 start]]></desc>
			<sql><![CDATA[
SELECT T1.EQ_NO ,
  T2.EQ_TPSZ_CD ,
  T2.ONH_OFC_CD ,
  SUBSTR(T7.PK, 18, 3) AS PRE_EQ_ASET_STS_CD ,
  SUBSTR(T7.PK, 1, 10) AS PRE_STS_EVENT_DT ,
  SUBSTR(T7.PK, 21, 5) AS PRE_STS_EVENT_LOC_CD ,
  SUBSTR(T7.PK, 21, 7) AS PRE_STS_EVNT_YD_CD ,
  T1.EQ_ASET_STS_CD AS EQ_ASET_STS_CD ,
  TO_CHAR( T1.STS_EVNT_DT, 'YYYY-MM-DD' ) AS STS_EVENT_DT ,
  T1.STS_EVNT_LOC_CD AS STS_EVENT_LOC_CD ,
  T1.STS_EVNT_YD_CD AS STS_EVNT_YD_CD
FROM CGM_EQ_STS_HIS T1 ,
  CGM_EQUIPMENT T2 ,
  MDM_LOCATION T3 ,
  MDM_EQ_ORZ_CHT T4 ,
  CGM_EQ_TP_SZ T5 ,
  (
    SELECT EQ_NO,
      MAX(TO_CHAR(STS_EVNT_DT, 'YYYY-MM-DD-HH24MISS')||EQ_ASET_STS_CD||STS_EVNT_YD_CD||TO_CHAR(CRE_DT, 'YYYY-MM-DD-HH24MISS') ) PK
    FROM CGM_EQ_STS_HIS A
    WHERE A.STS_EVNT_DT < TO_DATE( @[inq_fm_dys], 'YYYYMMDD' )
    GROUP BY EQ_NO ) T7
WHERE T2.EQ_TPSZ_CD = T5.EQ_TPSZ_CD
  AND T1.STS_EVNT_LOC_CD = T3.LOC_CD
  AND T3.SCC_CD = T4.SCC_CD
  AND T3.DELT_FLG = 'N'
  AND T4.DELT_FLG = 'N'
  AND T1.EQ_KND_CD = @[eq_knd_cd]
  AND T2.EQ_NO = T7.EQ_NO(+)
  AND T1.STS_EVNT_LOC_CD IN (
    SELECT AA.LOC_CD
    FROM MDM_LOCATION AA,
      MDM_EQ_ORZ_CHT BB
    WHERE AA.SCC_CD = BB.SCC_CD
      AND AA.DELT_FLG = 'N'
      AND BB.DELT_FLG = 'N' #if ( ${location} == 'RCC' )
      AND BB.RCC_CD = (@[crnt_loc_cd]) #elseif ( ${location} == 'LCC' )
      AND BB.LCC_CD = (@[crnt_loc_cd]) #elseif ( ${location} == 'SCC' )
      AND BB.SCC_CD = (@[crnt_loc_cd]) #end )
  AND T1.EQ_NO = T2.EQ_NO
  AND T1.ROWID = (
    SELECT /*+ INDEX_DESC(A XPKCGM_EQ_STS_HIS) */
      A.ROWID
    FROM CGM_EQ_STS_HIS A
    WHERE A.STS_EVNT_DT <= TO_DATE( @[inq_to_dys], 'YYYYMMDD' )
      AND A.STS_EVNT_DT >= TO_DATE( @[inq_fm_dys], 'YYYYMMDD' )
      AND A.EQ_NO = T1.EQ_NO
      AND ROWNUM = 1 )
  AND T1.EQ_ASET_STS_CD <> NVL( SUBSTR( T7.PK, 18, 3), 'XX')
  AND (T1.EQ_ASET_STS_CD,
      NVL( SUBSTR( T7.PK, 18, 3), 'XX')) NOT IN ( ('LST',
          'TLL') ,
      ('LSI',
          'FND') ,
      ('LST',
          'LSO'),
      ('TLL',
          'LSO'),
      ('FND',
          'LSI') )
  AND T1.TERM_CNG_SEQ IS NULL #if (${include_np} != 'Y')
  AND T2.AGMT_LSTM_CD <>'NP' #end #if ( ${eq_tpsz_cd_size} != '' )
  AND T5.EQ_TPSZ_REP_CD = @[eq_tpsz_cd_size] #end #if (${include_en} == 'Y')
UNION ALL
SELECT
  CASE
    WHEN LCC1 IS NULL THEN TRIM(SUBSTR(LCC2, 6, 14))
    ELSE TRIM(SUBSTR(LCC1, 6, 14))
  END AS EQ_NO ,
  CASE
    WHEN LCC1 IS NULL THEN TRIM(SUBSTR(LCC2, 20, 4))
    ELSE TRIM(SUBSTR(LCC1, 20, 4))
  END AS EQ_TPSZ_CD ,
  'EN' AS ONH_OFC_CD ,
  '' AS PRE_EQ_ASET_STS_CD ,
  CASE
    WHEN LCC2 IS NULL THEN ''
    ELSE TRIM(SUBSTR(LCC2, 31, 14))
  END AS PRE_STS_EVENT_DT ,
  CASE
    WHEN LCC2 IS NULL THEN ''
    ELSE TRIM(SUBSTR(LCC2, 1, 5))
  END AS PRE_STS_EVNET_LOC_CD ,
  CASE
    WHEN LCC2 IS NULL THEN ''
    ELSE TRIM(SUBSTR(LCC2, 24, 7))
  END AS PRE_STS_EVNT_YD_CD ,
  '' AS EQ_ASET_STS_CD ,
  CASE
    WHEN LCC1 IS NULL THEN ''
    ELSE TRIM(SUBSTR(LCC1, 31, 14))
  END AS STS_EVENT_DT ,
  CASE
    WHEN LCC1 IS NULL THEN ''
    ELSE TRIM(SUBSTR(LCC1, 1, 5))
  END AS STS_EVNET_LOC_CD ,
  CASE
    WHEN LCC1 IS NULL THEN ''
    ELSE TRIM(SUBSTR(LCC1, 24, 7))
  END AS STS_EVNT_YD_CD
FROM (
    SELECT A.LCC1 AS LCC1 ,
      A.LCC2 AS LCC2
    FROM (
        SELECT /*+ FULL(EQ) */
          (
            SELECT /*+ INDEX_DESC(A XAK2CGM_CHSS_MVMT_HIS)   */
              RPAD(NVL(A.LCC_CD, ' '), 5, ' ')||RPAD(EQ.EQ_NO, 14, ' ')||RPAD(EQ.EQ_TPSZ_CD, 4, ' ')||RPAD(A.YD_CD, 7, ' ')||RPAD(TO_CHAR(A.MVMT_DT, 'YYYY-MM-DD'), 14, ' ')
            FROM CGM_CHSS_MVMT_HIS A
            WHERE A.MVMT_DT BETWEEN TO_DATE( @[inq_fm_dys], 'YYYYMMDD' ) AND TO_DATE( @[inq_to_dys] , 'YYYYMMDD' )
              AND A.CHSS_NO = EQ.EQ_NO
              AND ROWNUM = 1) LCC1,
          (
            SELECT /*+ INDEX_DESC(A XAK2CGM_CHSS_MVMT_HIS)   */
              RPAD(NVL(A.LCC_CD, ' '), 5, ' ')||RPAD(EQ.EQ_NO, 14, ' ')||RPAD(EQ.EQ_TPSZ_CD, 4, ' ')||RPAD(A.YD_CD, 7, ' ')||RPAD(TO_CHAR(A.MVMT_DT, 'YYYY-MM-DD'), 14, ' ')
            FROM CGM_CHSS_MVMT_HIS A
            WHERE A.MVMT_DT < TO_DATE( @[inq_fm_dys], 'YYYYMMDD' )
              AND A.CHSS_NO = EQ.EQ_NO
              AND ROWNUM = 1) LCC2,
          EQ.EQ_NO,
          EQ.EQ_TPSZ_CD
        FROM CGM_EQUIPMENT EQ
        WHERE EQ.EQ_KND_cD = @[eq_knd_cd] #if (${include_np} != 'Y')
          AND EQ.AGMT_LSTM_CD <>'NP' #end ) A
    WHERE LCC1 IS NOT NULL
      AND SUBSTR(LCC1, 1, 5) <> NVL( SUBSTR(LCC2, 1, 5) , 'NIL')
    GROUP BY (A.LCC1 ,
          A.LCC2) ) A
WHERE (SUBSTR(LCC1, 1, 5) IN (
        SELECT LCC_CD
        FROM MDM_EQ_ORZ_CHT M
        WHERE #if ( ${location} == 'RCC' ) M.RCC_CD = (@[crnt_loc_cd]) #elseif ( ${location} == 'LCC' ) M.LCC_CD = (@[crnt_loc_cd]) #elseif ( ${location} == 'SCC' ) M.SCC_CD = (@[crnt_loc_cd]) #end )
      OR SUBSTR(LCC2, 1, 5) IN (
        SELECT LCC_CD
        FROM MDM_EQ_ORZ_CHT M
        WHERE #if ( ${location} == 'RCC' ) M.RCC_CD = (@[crnt_loc_cd]) #elseif ( ${location} == 'LCC' ) M.LCC_CD = (@[crnt_loc_cd]) #elseif ( ${location} == 'SCC' ) M.SCC_CD = (@[crnt_loc_cd]) #end ) ) #end
ORDER BY ONH_OFC_CD, EQ_NO, EQ_TPSZ_CD			]]></sql>
			<params>
				<param name="inq_fm_dys" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="crnt_loc_cd" type="12" value="" out="N"/>
				<param name="inq_to_dys" type="12" value="" out="N"/>
				<param name="eq_tpsz_cd_size" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
