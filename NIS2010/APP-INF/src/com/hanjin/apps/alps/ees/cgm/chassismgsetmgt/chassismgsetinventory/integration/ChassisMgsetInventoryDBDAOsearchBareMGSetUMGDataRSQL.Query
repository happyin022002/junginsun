<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOsearchBareMGSetUMGDataRSQL">
			<desc><![CDATA[MVMT  LSDAYS 조회]]></desc>
			<sql><![CDATA[
SELECT CHSS_MVMT_STS_CD MVMT
      ,TRUNC(SYSDATE - CHSS_MVMT_DT,0) LSDAYS  
FROM CGM_EQUIPMENT
WHERE EQ_NO = @[chss_no] -- CHASS NO 입력값 			]]></sql>
			<params>
				<param name="chss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
