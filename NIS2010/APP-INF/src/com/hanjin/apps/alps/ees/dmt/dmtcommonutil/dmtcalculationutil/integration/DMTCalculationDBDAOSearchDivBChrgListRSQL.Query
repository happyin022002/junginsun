<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOSearchDivBChrgListRSQL">
			<desc><![CDATA[SearchDivBChrgList]]></desc>
			<sql><![CDATA[
SELECT      FT_OVR_DYS 	ft_over,
            FT_UND_DYS 	ft_under,
            DECODE( SUBSTR(@[cntrts],2,1), '2', CNTR_20FT_RT_AMT,
                                           '4', CNTR_40FT_RT_AMT,
                                           '5', CNTR_HC_RT_AMT,
                                           '7', CNTR_45FT_RT_AMT,
                                           '8', CNTR_R9_RT_AMT,
                                           '9', CNTR_R9_RT_AMT   ) rate
    FROM DMT_RFA_EXPT_RT 
   WHERE RFA_EXPT_DAR_NO 	= @[dar_no]
	 AND RFA_EXPT_MAPG_SEQ 	= @[mapg_seq]
     AND RFA_EXPT_VER_SEQ 	= @[ver_seq]
     AND RFA_RQST_DTL_SEQ 	= @[dtl_seq] 
 	 AND CVRG_CMB_SEQ       = 1
     AND FT_OVR_DYS 		<= (@[over_day] + @[div_over_day])
ORDER BY RFA_EXPT_RT_SEQ			]]></sql>
			<params>
				<param name="cntrts" type="12" value="" out="N"/>
				<param name="dar_no" type="12" value="" out="N"/>
				<param name="mapg_seq" type="12" value="" out="N"/>
				<param name="ver_seq" type="12" value="" out="N"/>
				<param name="dtl_seq" type="12" value="" out="N"/>
				<param name="over_day" type="12" value="" out="N"/>
				<param name="div_over_day" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
