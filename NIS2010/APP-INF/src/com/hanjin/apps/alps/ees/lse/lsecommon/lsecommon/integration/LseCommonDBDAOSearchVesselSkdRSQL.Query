<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LseCommonDBDAOSearchVesselSkdRSQL">
			<desc><![CDATA[VVD 코드조회]]></desc>
			<sql><![CDATA[
SELECT  A.SLAN_CD, A.VPS_PORT_CD, 
		A.VSL_CD, A.SKD_VOY_NO, A.SKD_DIR_CD,
        A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD AS VVD
FROM    VSK_VSL_PORT_SKD A
WHERE   1 = 1
AND     A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD = @[vvd_cd]			]]></sql>
			<params>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
