<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ExpensePlanDBDAOExpenseVerSeqRSQL">
			<desc><![CDATA[년도별 최대 버전번호를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  NVL(MAX(VER_SEQ),0) +1 AS VER_SEQ
FROM    LSE_EQ_EXPN_PLN
WHERE   PLN_YR = @[pln_yr]			]]></sql>
			<params>
				<param name="pln_yr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
