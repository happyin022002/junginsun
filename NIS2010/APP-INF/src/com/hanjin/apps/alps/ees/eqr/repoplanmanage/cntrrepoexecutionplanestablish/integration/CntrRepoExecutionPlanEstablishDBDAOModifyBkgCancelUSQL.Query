<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOModifyBkgCancelUSQL">
			<desc><![CDATA[- BKG/DOC 에서 BKG CANCEL 한 데이터에 대해 수정
- BKG_NO, BKG_NO_SPLIT, BKG_fLAG, BKG_SPLIT_FLAG 모두 CLEAR
-수정자 정보는 BKG/DOC에서 추출, 수정일은 SYSDATE]]></desc>
			<sql><![CDATA[
UPDATE	${table_name} SET 
		REPO_MTY_BKG_FLG	= ''
	,	MTY_BKG_NO			= ''	
#if (${gubun} == 'V') 
	,	SPLIT_REPO_PLN_FLG	= 'N'
	,	OLD_BKG_GRP_NO		= ''
#end
	,	UPD_USR_ID			= @[usr_id]
	,	UPD_DT				= SYSDATE
WHERE	MTY_BKG_NO = @[mty_bkg_no]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="mty_bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
