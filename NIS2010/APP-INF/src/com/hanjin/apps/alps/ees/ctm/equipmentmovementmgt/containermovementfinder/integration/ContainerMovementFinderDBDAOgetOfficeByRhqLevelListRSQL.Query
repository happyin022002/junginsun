<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOgetOfficeByRhqLevelListRSQL">
			<desc><![CDATA[RHQ 레벨에 따른 office 코드를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT OFC_N8TH_LVL_CD VALUE0
FROM DMT_OFC_LVL_V
WHERE OFC_KND_CD BETWEEN '2' AND '6'
#if(${value0} != '')
 AND OFC_N3RD_LVL_CD = @[value0]
#end
GROUP BY OFC_N8TH_LVL_CD			]]></sql>
			<params>
				<param name="value0" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
