<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLocYardInfoRSQL">
			<desc><![CDATA[LOC Yard combo box 정보를 검색]]></desc>
			<sql><![CDATA[
SELECT 
  YD_CD                                               
  ,REPLACE(YD_NM, '''', ' ') YD_NM                                               
FROM 
  MDM_YARD   								            
WHERE 
  DELT_FLG <> 'Y'                  					

-- modified by shin yongchan 20061121
--AND   LOC_CD LIKE '${locyard_searchword}%'    			
AND LOC_CD IN ( 											
				SELECT SCC_CD 								
				FROM MDM_EQ_ORZ_CHT 						
				WHERE ECC_CD LIKE '${locyard_searchword}%' 
			   ) 											 
ORDER BY YD_CD		            										]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
