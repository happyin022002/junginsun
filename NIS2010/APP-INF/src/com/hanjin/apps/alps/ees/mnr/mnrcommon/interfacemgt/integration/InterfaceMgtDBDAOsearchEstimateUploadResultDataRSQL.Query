<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InterfaceMgtDBDAOsearchEstimateUploadResultDataRSQL">
			<desc><![CDATA[Estimate Upload 처리된 결과를 조회합니다.
--------------------------------------------------------
History
2011.10.28 김상수 [CHM-201114133-01] ALPS MNR->REPAIR Estimate 에서 EDI Upload 시 Man-Hour 계산로직 수정]]></desc>
			<sql><![CDATA[
SELECT A.RQST_EQ_NO||A.RPR_RQST_TMP_SEQ ||A.RPR_RQST_TMP_VER_NO AS COMPLEX_PK,
       A.RQST_EQ_NO,
       A.RQST_REF_NO,
       TO_CHAR(A.EQ_DMG_DT, 'YYYYMMDD') AS EQ_DMG_DT,
       A.CURR_CD,
       A.EDI_ID,
       A.RPR_OFFH_FLG,
       A.EDI_ERR_CD,
       B.EQ_LOC_CD,
       B.EQ_CMPO_CD,
       B.EQ_DMG_CD,
       B.EQ_RPR_CD,
       B.TRF_DIV_CD,
       B.VOL_TP_CD,
       B.RPR_QTY,
       B.RPR_SZ_NO,
       B.RPR_LBR_HRS,
       B.RPR_LBR_RT,
       B.LBR_COST_AMT,
       B.MTRL_COST_AMT,
       B.MNR_WRK_AMT,
       C.MNR_CD_DP_DESC AS EDI_ERR_NM
  FROM MNR_RPR_RQST_TMP_HDR A,
       MNR_RPR_RQST_TMP_DTL B,
       MNR_GEN_CD C
 WHERE A.RQST_EQ_NO = B.RQST_EQ_NO
   AND A.RPR_RQST_TMP_SEQ = B.RPR_RQST_TMP_SEQ
   AND A.RPR_RQST_TMP_VER_NO = B.RPR_RQST_TMP_VER_NO
   AND A.RPR_RQST_LST_VER_FLG = 'Y'
   AND A.EDI_ERR_CD = C.MNR_CD_ID
   AND C.PRNT_CD_ID = 'CD00004'
#if (${complex_pk} != '')
   AND A.RQST_EQ_NO||A.RPR_RQST_TMP_SEQ||A.RPR_RQST_TMP_VER_NO IN (
     #foreach ($key IN ${complex_pk})
          #if($velocityCount < $complex_pk.size())
               '$key',
          #else
               '$key'
          #end
     #end
       )
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
