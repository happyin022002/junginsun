<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchManualInvoiceDetailListRSQL">
			<desc><![CDATA[Invoice No. 로 Invoice  Detail 정보를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	DMDT_INV_NO
	,	CRE_OFC_CD
	,	INV_DTL_SEQ
	,	SYS_AREA_GRP_ID
	,	CNTR_NO
	,	CNTR_CYC_NO
	,	DMDT_TRF_CD
	,	DMDT_CHG_LOC_DIV_CD
	,	CHG_SEQ
	,	CNTR_TPSZ_CD
	,	TO_CHAR(FM_MVMT_DT, 'YYYY-MM-DD') FM_MVMT_DT
	,	TO_CHAR(TO_MVMT_DT, 'YYYY-MM-DD') TO_MVMT_DT
	,	FT_DYS
	,	TO_CHAR(FT_CMNC_DT, 'YYYY-MM-DD') FT_CMNC_DT
	,	TO_CHAR(FT_END_DT, 'YYYY-MM-DD') FT_END_DT
	,	FX_FT_OVR_DYS
	,	XCLD_SAT_FLG
	,	XCLD_SUN_FLG
	,	XCLD_HOL_FLG
	,	ORG_CHG_AMT
	,	SC_RFA_EXPT_AMT
	,	AFT_EXPT_DC_AMT
	,	BIL_AMT
	,	CNTR_INV_AMT
	,	TAX_RTO
	,	TAX_AMT
	,	INV_PRT_FLG
	,	CRE_USR_ID
	,	CRE_DT
	,	UPD_USR_ID
	,	UPD_DT
	,	UPD_OFC_CD
    , 0 AS COL_CHARGE
    , 0 AS COL_TAX
	, 0 AS UNCOL_AMT
FROM	DMT_INV_DTL
WHERE	DMDT_INV_NO = @[dmdt_inv_no]
ORDER BY INV_DTL_SEQ			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
