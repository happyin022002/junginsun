<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodFileUploadDBDAOFileUploadListVOByIDRSQL">
			<desc><![CDATA[DOD Charge에 각 해당하는 첨부파일 목록을 가져온다.]]></desc>
			<sql><![CDATA[
SELECT /*+INDEX_DESC(F XPKDOD_ATCH_FILE) */
       F.ATCH_FILE_LNK_ID
      ,F.ATCH_FILE_LNK_SEQ
      ,F.FILE_SAV_ID
      ,C.FILE_PATH_URL 
      ,C.FILE_UPLD_NM
      ,TO_CHAR (F.UPD_DT, 'YYYY-MM-DD') UPD_DT  
      ,F.UPD_USR_ID
	  ,'0' AS FILE_DOWNLOAD
  FROM DOD_ATCH_FILE F, COM_UPLD_FILE C
 WHERE 1=1
   AND F.ATCH_FILE_LNK_ID = @[atch_file_lnk_id]
   AND F.FILE_SAV_ID  = C.FILE_SAV_ID			]]></sql>
			<params>
				<param name="atch_file_lnk_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
