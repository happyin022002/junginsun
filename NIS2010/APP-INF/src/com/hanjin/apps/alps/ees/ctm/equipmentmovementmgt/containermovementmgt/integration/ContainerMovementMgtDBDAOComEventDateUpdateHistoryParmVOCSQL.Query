<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOComEventDateUpdateHistoryParmVOCSQL">
			<desc><![CDATA[ComEventDateUpdateHistoryParmVO]]></desc>
			<sql><![CDATA[
INSERT INTO CTM_MVMT_EVNT_DT_HIS
(
CNTR_NO
,CNMV_YR
,CNMV_ID_NO
,CNMV_UPD_HIS_SEQ
,CNTR_TPSZ_CD
,MVMT_STS_CD
,PRE_CNMV_EVNT_DT
,CRNT_CNMV_EVNT_DT
,CRNT_VSL_CD
,CRNT_SKD_VOY_NO
,CRNT_SKD_DIR_CD
,FCNTR_FLG
,BKG_NO
,BL_NO
,MVMT_EDI_MSG_TP_ID
,OFC_CD
,CRE_LOCL_DT
,CRE_USR_ID
,UPD_LOCL_DT
,UPD_USR_ID
,ORG_YD_CD
,CNMV_RMK
)
SELECT
 @[cntr_no]
,@[cnmv_yr]
,CNMV_ID_NO
,(SELECT NVL(MAX(CNMV_UPD_HIS_SEQ),0)+1
    FROM CTM_MVMT_EVNT_DT_HIS 
   WHERE CNTR_NO = @[cntr_no]
     AND CNMV_YR = @[cnmv_yr]
     AND CNMV_ID_NO  = (SELECT CNMV_ID_NO
                          FROM (SELECT /*+ INDEX_DESC (CTM_MOVEMENT XUK1CTM_MOVEMENT) */
                                    ROWNUM RM,
                                    CNMV_ID_NO
                                 FROM CTM_MOVEMENT
                                WHERE CNTR_NO = @[cntr_no])
                         WHERE RM = 1)
)
,CNTR_TPSZ_CD
,@[mvmt_sts_cd]
,CNMV_EVNT_DT
,TO_DATE (@[cnmv_evnt_dt], 'YYYY-MM-DD HH24:MI')
,CRNT_VSL_CD
,CRNT_SKD_VOY_NO
,CRNT_SKD_DIR_CD
,DECODE (@[fcntr_flg], 'F', 'Y', 'M', 'N' )
,@[bkg_no]
,@[bl_no]
,MVMT_EDI_MSG_TP_ID
,OFC_CD
,CRE_LOCL_DT
,@[cre_usr_id]
,UPD_LOCL_DT
,@[cre_usr_id]
,@[org_yd_cd]
,CNMV_RMK
 FROM CTM_MOVEMENT
WHERE CNTR_NO = @[cntr_no]
  AND CNMV_YR = @[cnmv_yr]
  AND CNMV_ID_NO  = (SELECT CNMV_ID_NO
                       FROM (SELECT /*+ INDEX_DESC (CTM_MOVEMENT XUK1CTM_MOVEMENT) */
                                    ROWNUM RM,
                                    CNMV_ID_NO
                               FROM CTM_MOVEMENT
                              WHERE CNTR_NO = @[cntr_no])
                      WHERE RM = 1)
  AND TO_CHAR(CNMV_EVNT_DT, 'YYYYMMDD') <> substr(@[cnmv_evnt_dt], 1, 8)			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="mvmt_sts_cd" type="12" value="" out="N"/>
				<param name="cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="fcntr_flg" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="org_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
