<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOSearchInvoiceChargeWithoutCreOfcCdRSQL">
			<desc><![CDATA[InvoiceIssueCollectionMgtDBDAOSearchInvoiceChargeWithoutCreOfcCdRSQL]]></desc>
			<sql><![CDATA[
SELECT SYS_AREA_GRP_ID AS SVR_ID
	,CNTR_NO
	,CNTR_CYC_NO
	,DMDT_TRF_CD
	,DMDT_CHG_LOC_DIV_CD
	,CHG_SEQ
	,DMDT_INV_NO
	,CRE_OFC_CD
	,NVL(FX_FT_OVR_DYS, 0) AS FX_FT_OVR_DYS
FROM DMT_INV_DTL
WHERE DMDT_INV_NO 	= @[dmdt_inv_no]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
