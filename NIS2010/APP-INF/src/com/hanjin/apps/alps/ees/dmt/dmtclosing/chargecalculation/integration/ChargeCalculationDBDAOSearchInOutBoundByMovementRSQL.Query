<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOSearchInOutBoundByMovementRSQL">
			<desc><![CDATA[CNTR Movement 정보에 대해서 해당 CNTR의 In/Out Bound 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT	NVL(ATTR_CTNT8,OB_CNTR_FLG)
FROM	CTM_MOVEMENT CTM, DMT_HRD_CDG_CTNT HRD
WHERE	CTM.CNTR_NO		= @[cntr_no]
AND		CTM.CNMV_CYC_NO	= @[cntr_cyc_no]
AND		CTM.MVMT_STS_CD	= 'VD'
AND		CTM.ORG_YD_CD	= @[fm_mvmt_yd_cd]
AND 	HRD.HRD_CDG_ID(+) = 'SHIP_BACK_CDG'
AND     HRD.ATTR_CTNT3(+) = CTM.CNTR_NO
AND     HRD.ATTR_CTNT4(+) = CTM.CNMV_CYC_NO
AND     HRD.ATTR_CTNT7(+) = CTM.ORG_YD_CD			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_cyc_no" type="12" value="" out="N"/>
				<param name="fm_mvmt_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
