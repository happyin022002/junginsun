			<desc><![CDATA[[EES_CIM_0060] EQ Balance Report Input => update]]></desc>
			<sql><![CDATA[
UPDATE CIM_EQ_SPLS_DFCT_STS
set SPLS_DFCT_STS_CTNT = @[spls_dfct_sts_ctnt]
,CFM_FLG = @[cfm_flg]
,STS_RMK = @[sts_rmk]
,UPD_USR_ID = @[upd_usr_id]
,UPD_DT = SYSDATE
 WHERE TGT_YRWK = @[tgt_yrwk]
   AND ECC_CD = @[ecc_cd]
   AND CNTR_TPSZ_CD = @[cntr_tpsz_cd]
   AND FULL_MTY_CD = @[full_mty_cd]
   AND SOC_FLG = @[soc_flg]
   AND RD_FLG = @[rd_flg]
   AND ENR_FLG = @[enr_flg]
   AND MVMT_CO_CD = @[mvmt_co_cd]
   AND TN_ROUT_FLG = @[tn_rout_flg]			]]></sql>
			<params>
				<param name="spls_dfct_sts_ctnt" type="12" value="" out="N"/>
				<param name="cfm_flg" type="12" value="" out="N"/>
				<param name="sts_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="tgt_yrwk" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
