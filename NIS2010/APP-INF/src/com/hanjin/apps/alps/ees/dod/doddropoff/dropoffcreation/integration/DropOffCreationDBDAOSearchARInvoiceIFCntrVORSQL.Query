<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DropOffCreationDBDAOSearchARInvoiceIFCntrVORSQL">
			<desc><![CDATA[AR 전송을 위한 Invoice Container Data를 읽어온다.]]></desc>
			<sql><![CDATA[
SELECT '' AS SRC_IF_DT,
       '' AS SRC_IF_SEQ,
       '' AS CNTR_SEQ,
       CNTR_NO,
       CNTR_TPSZ_CD,
       CRE_USR_ID,
       TO_CHAR(CRE_DT,'yyyymmdd') AS CRE_DT,
       UPD_USR_ID,
       TO_CHAR(UPD_DT,'yyyymmdd') AS UPD_DT
  FROM DOD_DRP_OFF_CHG
 WHERE 1 = 1
#if (${bkg_no} != '') 
   AND BKG_NO = @[bkg_no]
#end
#if (${inv_src_no} != '') 
   AND INV_SRC_NO = @[inv_src_no]
#end
#if (${drp_off_chg_seq} != '') 
   AND DRP_OFF_CHG_SEQ = @[drp_off_chg_seq]
#end			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="inv_src_no" type="12" value="" out="N"/>
				<param name="drp_off_chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
