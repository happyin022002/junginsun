<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOCheckCPSCHSSPoolCodeDupDataRSQL">
			<desc><![CDATA[입력된 Pool Code의 중복을 체크한다.]]></desc>
			<sql><![CDATA[
SELECT CHSS_POOL_CD,
       CHSS_POOL_NM
FROM CGM_CPS_POOL_LIST
WHERE CHSS_POOL_CD = @[chss_pool_cd]			]]></sql>
			<params>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
