		<query name="EQMatchBackNLoadFactorMgtDBDAOmodifyLocationPOPDataUSQL">
			<desc><![CDATA[modifyLocationPOPData]]></desc>
			<sql><![CDATA[
UPDATE CIM_COA_MTCH_BAK_LOC
   SET USE_FLG = DECODE(@[use_flg],'0','N','Y'),
       UPD_USR_ID = @[usr_id],
       UPD_DT = SYSDATE
 WHERE USR_ID = @[usr_id]
   AND RCC_CD = @[rcc_cd]
   AND ECC_CD = @[ecc_cd]
   AND ECC_LVL = @[ecc_lvl]			]]></sql>
			<params>
				<param name="use_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="ecc_lvl" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
