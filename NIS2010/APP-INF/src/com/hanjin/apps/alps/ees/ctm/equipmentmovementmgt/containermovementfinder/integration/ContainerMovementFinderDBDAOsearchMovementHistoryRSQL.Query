<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementFinderDBDAOsearchMovementHistoryRSQL">
			<desc><![CDATA[ContainerMovementFinderDBDAO
컨테이너의 이동정보 상세 내역을 얻어온다]]></desc>
			<sql><![CDATA[
SELECT CNMV_CYC_NO,
  CNMV_CO_CD,
  MVMT_STS_CD,
  MVMT_CRE_TP_CD,
  ORG_YD_CD,
  DEST_YD_CD,
  TO_CHAR (CNMV_EVNT_DT, 'YYYY-MM-DD HH24:MI') AS CNMV_EVNT_DT,
  FCNTR_FLG,
  OB_CNTR_FLG,
  MVMT_EDI_MSG_TP_ID,
  BKG_CGO_TP_CD,
  CNTR_DMG_FLG,
  CNTR_HNGR_RCK_FLG,
  CNTR_HNGR_BAR_ATCH_KNT,
  CNTR_DISP_FLG,
  IMDT_EXT_FLG,
  CNTR_RFUB_FLG CNTR_XCH_CD,
  SPCL_CGO_FLG,
  VNDR_SEQ,
  MVMT_TRSP_MOD_CD,
  CHSS_NO,
  MGST_NO,
  CNTR_SEAL_NO,
  WBL_NO,
  PKUP_NO,
  TO_CHAR(UPD_LOCL_DT, 'YYYY-MM-DD HH24:MI') AS UPD_LOCL_DT,
  TO_CHAR(CRE_LOCL_DT, 'YYYY-MM-DD HH24:MI') AS CRE_LOCL_DT,
  OFC_CD,
  USR_NM,
  CNMV_RMK
FROM CTM_MOVEMENT
WHERE CNTR_NO = @[p_cntrno] || @[check_digit]
  AND CNMV_EVNT_DT BETWEEN TO_DATE(@[p_date1], 'YYYY-MM-DD') AND TO_DATE(@[p_date2], 'YYYY-MM-DD') + 0.99999
			]]></sql>
			<params>
				<param name="p_cntrno" type="12" value="" out="N"/>
				<param name="check_digit" type="12" value="" out="N"/>
				<param name="p_date1" type="12" value="" out="N"/>
				<param name="p_date2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
