<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOsearchVerifyEstimateCalcDataRSQL">
			<desc><![CDATA[--------------------------------------------------------------------------------------------------------------------------------------
searchVerifyEstimateCalcData
2014-04-30 Jonghee HAN Repair Estimates Creation Calculation Bug Fix - ORDER BY Phrase Added
--------------------------------------------------------------------------------------------------------------------------------------]]></desc>
			<sql><![CDATA[
SELECT   A.TMP_SEQ
       , A.TMP_DTL_SEQ
       , A.INP_MSG1
       , A.INP_MSG2
       , A.INP_MSG3
       , A.INP_MSG4
       , A.INP_MSG5
       , A.INP_MSG6
       , A.INP_MSG7
       , A.INP_MSG8
       , A.INP_MSG9
       , A.INP_MSG10
       , DECODE(A.INP_MSG11, NULL, A.INP_MSG16, 0, A.INP_MSG16, A.INP_MSG11) AS INP_MSG11
       , DECODE(A.INP_MSG12, NULL, A.INP_MSG18, 0, A.INP_MSG18, A.INP_MSG12) AS INP_MSG12
       , DECODE(A.INP_MSG13, NULL, A.INP_MSG28, 0, A.INP_MSG28, A.INP_MSG13) AS INP_MSG13
       , DECODE(A.INP_MSG14, NULL, A.INP_MSG17, 0, A.INP_MSG17, A.INP_MSG14) AS INP_MSG14
       , DECODE(A.INP_MSG15, NULL, A.INP_MSG29, 0, A.INP_MSG29, A.INP_MSG15) AS INP_MSG15
       , A.INP_MSG16
       , A.INP_MSG17
       , A.INP_MSG18
       , A.INP_MSG19
       , A.INP_MSG20
       , A.INP_MSG21
       , A.INP_MSG22
       , A.INP_MSG23
       , A.INP_MSG24
       , A.INP_MSG25
       , A.INP_MSG26
       , A.INP_MSG27
       , A.INP_MSG28
       , A.INP_MSG29
       , A.INP_MSG30
FROM     MNR_DAT_VRFY A
WHERE    1 = 1
AND      A.TMP_SEQ = @[tmp_seq]
ORDER BY A.TMP_SEQ, A.TMP_DTL_SEQ			]]></sql>
			<params>
				<param name="tmp_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
