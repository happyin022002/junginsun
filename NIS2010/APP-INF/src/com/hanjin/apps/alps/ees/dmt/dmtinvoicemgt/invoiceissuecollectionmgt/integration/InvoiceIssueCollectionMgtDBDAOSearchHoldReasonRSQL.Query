        REPLACE(DMDT_INV_NO||'||'||INV_HLD_RSN_CD||'||'||INV_HLD_RMK||'||'||TO_CHAR(UPD_DT,'YYYY-MM-DD')||'||'||UPD_OFC_CD||'||'||UPD_USR_ID,'@*','') XXX
