<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchSubTradeComboListRSQL">
			<desc><![CDATA[SUB TRADE]]></desc>
			<sql><![CDATA[
/* Search Sub Trade List, CommonDBDAOSearchTradeComboListRSQL  */
SELECT DISTINCT V.TRD_CD
      ,V.SUB_TRD_CD
      ,V.SUB_TRD_NM
      ,V.TRD_CD||V.SUB_TRD_CD VAL_CD
  FROM EQR_CTRL_TRADE_LANE_V V
 WHERE 1=1
#if (${trdCd} != '')
   AND V.TRD_CD IN ($trdCd)
#end
#if (${subTrdCd} != '')
   AND V.SUB_TRD_CD   IN (${subtrdcd}) 
#end
 ORDER BY V.TRD_CD
         ,V.SUB_TRD_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
