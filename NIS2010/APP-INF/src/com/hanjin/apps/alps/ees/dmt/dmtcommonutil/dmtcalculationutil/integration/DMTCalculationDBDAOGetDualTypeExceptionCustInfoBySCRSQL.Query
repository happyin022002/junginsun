<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOGetDualTypeExceptionCustInfoBySCRSQL">
			<desc><![CDATA[getDualTypeExceptionCustInfoBySC]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC( PSM XPKPRI_RP_SCP_MN ) */
       CUST_CNT_CD
      ,CUST_SEQ
  FROM PRI_SP_MN PSM
      ,PRI_SP_CTRT_PTY SCP
      ,PRI_SP_HDR PSH
 WHERE PSM.PROP_NO = PSH.PROP_NO
   AND PSH.SC_NO = @[sc_no]
   AND PSM.PROP_NO = SCP.PROP_NO
   AND PSM.AMDT_SEQ = SCP.AMDT_SEQ
   AND SCP.PRC_CTRT_PTY_TP_CD = 'C'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
