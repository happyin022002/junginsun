<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOModifyMtyBkgNoUSQL">
			<desc><![CDATA[Mty Booking Creation 후 테이블에 Mty Booking No. 업데이트 해주기..]]></desc>
			<sql><![CDATA[
UPDATE ${table_name}
SET 
	 MTY_BKG_NO			= @[mty_bkg_no]
#if( ${gubun} == 'V')
	,OLD_BKG_GRP_NO		= @[mty_bkg_no]
#end
	,REPO_MTY_BKG_FLG	= 'Y'
	,UPD_USR_ID			= @[upd_usr_id]
	,UPD_DT				= SYSDATE
WHERE	REPO_PLN_ID = @[repo_pln_id]
	AND	PLN_YRWK	= @[pln_yrwk]
	AND	PLN_SEQ		= @[pln_seq]
	AND	REF_ID		= @[ref_id]			]]></sql>
			<params>
				<param name="mty_bkg_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="repo_pln_id" type="12" value="" out="N"/>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="pln_seq" type="12" value="" out="N"/>
				<param name="ref_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
