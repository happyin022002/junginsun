<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchPrntVndrSeqDataRSQL">
			<desc><![CDATA[GeneralCodeSearchMgtDBDAOsearchPrntVndrSeqDataRSQL]]></desc>
			<sql><![CDATA[
SELECT MNR_COMMON_PKG.MNR_CONV_PARTNER_CD_FNC(A.PRNT_VNDR_SEQ) AS CD_ID,
      (SELECT B.VNDR_LGL_ENG_NM 
       FROM MDM_VENDOR  B
       WHERE B.VNDR_SEQ = A.PRNT_VNDR_SEQ) AS CD_DESC
FROM MDM_VENDOR A
WHERE A.VNDR_SEQ = @[searchkey]			]]></sql>
			<params>
				<param name="searchkey" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
