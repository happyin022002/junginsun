<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOmodifyBareMGSetDataUSQL">
			<desc><![CDATA[modifyBareMGSetData]]></desc>
			<sql><![CDATA[
UPDATE CGM_BARE_MGST_REPO
SET VNDR_SEQ          = @[vndr_seq]
   ,EQ_TRSP_MOD_CD    = @[eq_trsp_mod_cd]
   ,RTN_YD_CD         = @[rtn_yd_cd]
   ,MGST_BARE_EVNT_DT = TO_DATE(@[mgst_bare_evnt_dt], 'YYYYMMDD')
   ,BARE_MGST_RMK     = @[bare_mgst_rmk]
   ,UPD_USR_ID        = @[upd_usr_id]
   ,UPD_DT            = SYSDATE
WHERE MGST_NO           = @[mgst_no]
AND   MGST_BARE_STS_SEQ = @[mgst_bare_sts_seq] 			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="eq_trsp_mod_cd" type="12" value="" out="N"/>
				<param name="rtn_yd_cd" type="12" value="" out="N"/>
				<param name="mgst_bare_evnt_dt" type="12" value="" out="N"/>
				<param name="bare_mgst_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="mgst_no" type="12" value="" out="N"/>
				<param name="mgst_bare_sts_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
