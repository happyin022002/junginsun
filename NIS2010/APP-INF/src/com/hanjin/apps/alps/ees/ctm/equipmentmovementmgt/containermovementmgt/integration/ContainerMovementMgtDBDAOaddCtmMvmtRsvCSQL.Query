<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOaddCtmMvmtRsvCSQL">
			<desc><![CDATA[컨테이너의 예약 설정]]></desc>
			<sql><![CDATA[
INSERT INTO CTM_MVMT_RSV
            (CNTR_NO,
             CNMV_STS_CD,
             CNMV_EVNT_DT,
             ORG_YD_CD,
             INP_YD_CD,
             CRNT_VSL_CD,
             CRNT_SKD_VOY_NO,
             CRNT_SKD_DIR_CD,
             BKG_POL_CD,
             BKG_POD_CD,
             GMT_DT,
             CRE_LOCL_DT,
             UPD_LOCL_DT,
             CRE_USR_ID,
             UPD_USR_ID,
             USR_NM,
             OFC_CD,
             CNMV_RMK,
             CRE_DT,
             UPD_DT
            )
     VALUES (@[cntr_no] || @[check_digit],
             @[mvmt_sts_cd],
             TO_DATE (@[cnmv_evnt_dt], 'YYYY-MM-DD HH24:MI'),
             @[org_yd_cd],
             @[dest_yd_cd],
             @[crnt_vsl_cd],
             @[crnt_skd_voy_no],
             @[crnt_skd_dir_cd],
             @[pol_cd],
             @[pod_cd],
             GLOBALDATE_PKG.TIME_CONV_FNC (SUBSTR (@[org_yd_cd], 0, 5), TO_DATE (@[cnmv_evnt_dt], 'YYYY-MM-DD HH24:MI'), 'GMT'),
             GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', SYSDATE, SUBSTR (@[org_yd_cd], 0, 5)),
             GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', SYSDATE, SUBSTR (@[org_yd_cd], 0, 5)),
             @[cre_usr_id],
             @[upd_usr_id],
             @[usr_nm],
             @[ofc_cd],
             @[cnmv_rmk],
             SYSDATE,
             SYSDATE
            )			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="check_digit" type="12" value="" out="N"/>
				<param name="mvmt_sts_cd" type="12" value="" out="N"/>
				<param name="cnmv_evnt_dt" type="12" value="" out="N"/>
				<param name="org_yd_cd" type="12" value="" out="N"/>
				<param name="dest_yd_cd" type="12" value="" out="N"/>
				<param name="crnt_vsl_cd" type="12" value="" out="N"/>
				<param name="crnt_skd_voy_no" type="12" value="" out="N"/>
				<param name="crnt_skd_dir_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="usr_nm" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cnmv_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
