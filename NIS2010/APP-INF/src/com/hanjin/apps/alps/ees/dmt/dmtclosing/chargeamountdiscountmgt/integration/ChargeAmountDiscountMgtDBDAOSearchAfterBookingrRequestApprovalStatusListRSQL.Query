<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAOSearchAfterBookingrRequestApprovalStatusListRSQL">
			<desc><![CDATA[AfterBkgRqstAproStsVO]]></desc>
			<sql><![CDATA[
WITH MAIN AS 
(
	SELECT  R.AFT_EXPT_DAR_NO
           ,R.DMDT_EXPT_RQST_STS_CD 			AS CURR_STS
           ,B.BKG_NO
           ,B.BL_NO
           ,B.SC_NO
           ,B.RFA_NO
           ,B.TAA_NO
           ,C.DMDT_TRF_CD
		   ,C.OFC_CD 							AS DMDT_OFC_CD
           ,RQST_OFC_CD
           ,RQST_USR_ID
           ,(
				SELECT  USR_NM
                  FROM  COM_USER
                 WHERE  USR_ID = RQST_USR_ID
			) 									AS RQST_USR_NM
           ,RQST_DT
           ,AFT_BKG_APRO_NO
           ,R.UPD_DT
           ,G.PROG_SEQ
           ,P.AFT_BKG_PATH_CPLS_FLG
           ,P.DMDT_EXPT_RQST_STS_CD 			AS PATH_STS
           ,G.DMDT_EXPT_RQST_STS_CD
           ,G.PROG_DT
           ,G.PROG_USR_ID
           ,(
				SELECT  USR_NM
                  FROM  COM_USER
                 WHERE  USR_ID = G.PROG_USR_ID
			) 									AS PROG_USR_NM
           ,G.PROG_OFC_CD
           ,P.AFT_BKG_PATH_CD
           ,AFT_BKG_PATH_LVL
           ,P.APRO_OFC_CD
		   
	  FROM  DMT_AFT_BKG_ADJ_RQST 		R
	       ,DMT_AFT_BKG_ADJ_RQST_DTL 	D
	       ,DMT_AFT_BKG_ADJ_PROG 		G
	       ,DMT_AFT_BKG_APRO_PATH 		P
	       ,BKG_BOOKING 				B
	       ,DMT_AFT_BKG_CNTR 			ABC
	       ,DMT_CHG_CALC 				C

     WHERE  R.AFT_EXPT_DAR_NO        = D.AFT_EXPT_DAR_NO
       AND  D.AFT_EXPT_DAR_NO        = G.AFT_EXPT_DAR_NO
       AND  G.AFT_EXPT_DAR_NO        = P.AFT_EXPT_DAR_NO(+)
       AND  G.AFT_BKG_PATH_CD        = P.AFT_BKG_PATH_CD(+)
       AND  D.BKG_NO                 = B.BKG_NO
       AND  ABC.AFT_EXPT_DAR_NO      = D.AFT_EXPT_DAR_NO
       AND  ABC.AFT_EXPT_ADJ_SEQ     = D.AFT_EXPT_ADJ_SEQ
       AND  ABC.SYS_AREA_GRP_ID      = C.SYS_AREA_GRP_ID
       AND  ABC.CNTR_NO              = C.CNTR_NO
       AND  ABC.CNTR_CYC_NO          = C.CNTR_CYC_NO
       AND  ABC.DMDT_TRF_CD          = C.DMDT_TRF_CD
       AND  ABC.DMDT_CHG_LOC_DIV_CD  = C.DMDT_CHG_LOC_DIV_CD
       AND  ABC.CHG_SEQ              = C.CHG_SEQ
       AND  R.DMDT_EXPT_RQST_STS_CD != 'T'
       AND  G.PROG_SEQ               >=
            (
                SELECT  /*+ INDEX_DESC (DMT_AFT_BKG_ADJ_PROG XPKDMT_AFT_BKG_ADJ_PROG) */ PROG_SEQ
                  FROM  DMT_AFT_BKG_ADJ_PROG
                 WHERE  AFT_EXPT_DAR_NO       = G.AFT_EXPT_DAR_NO
                   AND  DMDT_EXPT_RQST_STS_CD = 'R'
                   AND  ROWNUM                = 1
            )
       AND  (P.AFT_BKG_PATH_CPLS_FLG = 'Y' OR P.AFT_BKG_PATH_CPLS_FLG IS NULL)
	   
#if (${fm_dt} != '' && ${to_dt} != '')
	#if (${dt_tp} != '' && ${dt_tp} == 'R')
       AND  TO_CHAR(R.RQST_DT, 'YYYY-MM-DD') BETWEEN @[fm_dt] AND @[to_dt]
	#else
       AND  TO_CHAR(R.UPD_DT, 'YYYY-MM-DD') BETWEEN @[fm_dt] AND @[to_dt]
	#end
#end
#if (${rqst_sts_cd} != '')
       AND  R.DMDT_EXPT_RQST_STS_CD IN (
	#foreach($sts_cd in ${sts_cd_list})
		#if($velocityCount < $sts_cd_list.size()) '$sts_cd', #else '$sts_cd' #end
	#end
			)
#end

#if (${ofc_tp} == 'RQ' && ${ofc_cd} != '')
       AND  RQST_OFC_CD IN (
	#foreach($ofc_cd in ${ofc_cd_list})
		#if($velocityCount < $ofc_cd_list.size()) '$ofc_cd', #else '$ofc_cd' #end
	#end
			)
#elseif (${ofc_tp} == 'RV' && ${ofc_cd} != '')
       AND  C.OFC_CD IN (
	#foreach($ofc_cd in ${ofc_cd_list})
		#if($velocityCount < $ofc_cd_list.size()) '$ofc_cd', #else '$ofc_cd' #end
	#end
			)
#end

#if (${trf_cd} != '')
       AND  C.DMDT_TRF_CD IN (
	#foreach($trf_cd in ${trf_cd_list})
		#if($velocityCount < $trf_cd_list.size()) '$trf_cd', #else '$trf_cd' #end
	#end
			)
#end

#if (${dar_no} != '')
       AND  R.AFT_EXPT_DAR_NO IN (
	#foreach($dar_no in ${dar_no_list})
		#if($velocityCount < $dar_no_list.size()) '$dar_no', #else '$dar_no' #end
	#end
			)
#end
#if (${apvl_no} != '')
       AND  R.AFT_BKG_APRO_NO IN (
	#foreach($apvl_no in ${apvl_no_list})
		#if($velocityCount < $apvl_no_list.size()) '$apvl_no', #else '$apvl_no' #end
	#end
			)
#end

#if (${bkg_no} != '')
       AND  B.BKG_NO IN (
	#foreach($bkg_no in ${bkg_no_list})
		#if($velocityCount < $bkg_no_list.size()) '$bkg_no', #else '$bkg_no' #end
	#end
			)
#end

#if (${bl_no} != '')
       AND  B.BL_NO IN (
	#foreach($bl_no in ${bl_no_list})
		#if($velocityCount < $bl_no_list.size()) '$bl_no', #else '$bl_no' #end
	#end
			)
#end
#if (${sc_no} != '')
       AND  B.SC_NO IN (
	#foreach($sc_no in ${sc_no_list})
		#if($velocityCount < $sc_no_list.size()) '$sc_no', #else '$sc_no' #end
	#end
			)
#end
#if (${rfa_no} != '')
       AND  B.RFA_NO IN (
	#foreach($rfa_no in ${rfa_no_list})
		#if($velocityCount < $rfa_no_list.size()) '$rfa_no', #else '$rfa_no' #end
	#end
			)
#end
#if (${taa_no} != '')
       AND  B.TAA_NO IN (
	#foreach($taa_no in ${taa_no_list})
		#if($velocityCount < $taa_no_list.size()) '$taa_no', #else '$taa_no' #end
	#end
			)
#end
    ORDER BY R.AFT_EXPT_DAR_NO
	        ,PROG_SEQ
			,BKG_NO
			,BL_NO
			,AFT_BKG_PATH_LVL
)
--// END MAIN

, TEMP AS 
(
	SELECT  AFT_EXPT_DAR_NO
           ,CURR_STS 			AS DMDT_EXPT_RQST_STS_CD
           ,DECODE(CURR_STS
				   ,'O'
				   ,(
						SELECT  (
									SELECT  ATTR_CTNT2
									  FROM  DMT_HRD_CDG_CTNT
									 WHERE  HRD_CDG_ID = 'AFT_BKG_APRO_PATH'
									   AND  ATTR_CTNT1 = B.AFT_BKG_PATH_CD
								) || ' Counter-Offer'
						  FROM  DMT_AFT_BKG_ADJ_PROG  	A
						       ,DMT_AFT_BKG_APRO_PATH 	B
						 WHERE  A.AFT_EXPT_DAR_NO = B.AFT_EXPT_DAR_NO
						   AND  A.AFT_BKG_PATH_CD = B.AFT_BKG_PATH_CD
						   AND  A.AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO
						   AND  PROG_SEQ          = 
								(
									SELECT  MAX(PROG_SEQ)
									  FROM  DMT_AFT_BKG_ADJ_PROG
									 WHERE  AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO
								)
					)
				   ,(
						SELECT  INTG_CD_VAL_DP_DESC						                 
						  FROM  COM_INTG_CD_DTL
				         WHERE  INTG_CD_ID       = 'CD01971'
				           AND  INTG_CD_VAL_CTNT = CURR_STS
					)
			) AS DMDT_EXPT_RQST_STS_CD_DESC
           ,SC_NO                                   
           ,RFA_NO
           ,TAA_NO
           ,CASE WHEN TAA_NO IS NOT NULL THEN (SELECT DISTINCT DECODE(CTRT_CUST_CNT_CD, '00', '', CTRT_CUST_CNT_CD) || LPAD(CTRT_CUST_SEQ, 6, 0) FROM PRI_TAA_MN A, PRI_TAA_HDR B WHERE  A.TAA_PROP_NO = B.TAA_PROP_NO AND TAA_NO = T.TAA_NO AND ROWNUM = 1)
                 WHEN SC_NO  IS NOT NULL THEN (SELECT DISTINCT DECODE(REAL_CUST_CNT_CD, '00', '', REAL_CUST_CNT_CD) || LPAD(REAL_CUST_SEQ, 6, 0) FROM PRI_SP_MN A,  PRI_SP_HDR  B WHERE  A.PROP_NO     = B.PROP_NO     AND SC_NO  = T.SC_NO  AND ROWNUM = 1)
                 WHEN RFA_NO IS NOT NULL THEN (SELECT DISTINCT DECODE(CTRT_CUST_CNT_CD, '00', '', CTRT_CUST_CNT_CD) || LPAD(CTRT_CUST_SEQ, 6, 0) FROM PRI_RP_MN A,  PRI_RP_HDR  B WHERE  A.PROP_NO     = B.PROP_NO     AND RFA_NO = T.RFA_NO AND ROWNUM = 1)
	        END AS CTRT_CUST_CD
           ,CASE WHEN TAA_NO IS NOT NULL THEN (SELECT DISTINCT RESPB_SREP_CD FROM PRI_TAA_MN A, PRI_TAA_HDR B WHERE A.TAA_PROP_NO = B.TAA_PROP_NO AND TAA_NO = T.TAA_NO AND ROWNUM = 1)
                 WHEN SC_NO  IS NOT NULL THEN (SELECT DISTINCT PROP_OFC_CD   FROM PRI_SP_MN A,  PRI_SP_HDR  B WHERE A.PROP_NO     = B.PROP_NO     AND SC_NO  = T.SC_NO  AND ROWNUM = 1)
                 WHEN RFA_NO IS NOT NULL THEN (SELECT DISTINCT PROP_OFC_CD   FROM PRI_RP_MN A,  PRI_RP_HDR  B WHERE A.PROP_NO     = B.PROP_NO     AND RFA_NO = T.RFA_NO AND ROWNUM = 1)
	        END AS CTRT_OFC_CD
           ,DMDT_TRF_CD
		   
		   #if (${group_by} == 'BKG')
           ,BKG_NO
           ,BL_NO
		   #end
           
		   ,DMDT_OFC_CD
           ,RQST_OFC_CD
           ,RQST_USR_ID
           ,RQST_USR_NM
           ,RQST_DT
		   
		   #if (${tab_cd} != 'A')
           ,CASE 
				WHEN CURR_STS = 'R' AND RQST_USR_ID = @[usr_id] AND RQST_OFC_CD = @[usr_ofc] THEN 
					'Y' 
				ELSE 'N' 
			END 				AS RQST_AUTH
		   #end
		   
           ,AFT_BKG_APRO_NO
           ,UPD_DT
           ,NVL(
				(
					SELECT  APRO_OFC_CD
					  FROM  DMT_AFT_BKG_APRO_PATH
					 WHERE  (AFT_EXPT_DAR_NO, AFT_BKG_PATH_LVL) = 
							(
								SELECT  AFT_EXPT_DAR_NO
								       ,MAX(AFT_BKG_PATH_LVL)
								  FROM  DMT_AFT_BKG_APRO_PATH
								 WHERE  AFT_EXPT_DAR_NO       = T.AFT_EXPT_DAR_NO
								   AND  AFT_BKG_PATH_CPLS_FLG = 'Y'
								GROUP BY AFT_EXPT_DAR_NO
							)
				)
			   ,RQST_OFC_CD
			) 					AS FINAL_APRO_OFC_CD
			
			--//====================================================================================================
			--// APRO LEVEL 1 ( Contract SCG )
			--//====================================================================================================
           ,MAX(LV1_OFC) 		AS LV1_OFC
           ,MAX(LV1_STS) 		AS LV1_STS
           ,MAX(LV1_USR_ID) 	AS LV1_USR_ID
           ,MAX(
				(
					SELECT  USR_NM
                      FROM  COM_USER
					 WHERE  USR_ID = LV1_USR_ID
				)
			) 					AS LV1_USR_NM
           ,MAX(LV1_DT) 		AS LV1_DT
			#if (${tab_cd} != 'A')
           ,CASE 
				WHEN EXISTS 
					(
						SELECT  A.USR_ID 
						  FROM  DMT_USR_ROLE_MTCH 	A
						       ,COM_USER 			B 
						 WHERE  A.USR_ID             = B.USR_ID 
						   AND  USR_ROLE_CD          = 
								(
									SELECT  ATTR_CTNT3
									  FROM  DMT_HRD_CDG_CTNT
									 WHERE  HRD_CDG_ID = 'AFT_BKG_APRO_PATH'
									   AND  ATTR_CTNT1 = 'SSZMGR'
								)
						   AND  SUBSTR(OFC_CD, 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
						   AND  A.USR_ID             = @[usr_id] 
						   AND  USE_FLG              = 'Y'
					)
					#if(${tab_cd} == 'P')
					AND (CURR_STS = 'R' AND (SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH WHERE AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO AND AFT_BKG_PATH_CPLS_FLG = 'Y') = 1)
					#else
					AND CURR_STS IN ('F', 'O', 'J')
					#end
					AND SUBSTR(MAX(LV1_OFC), 0, 5) = SUBSTR(@[usr_ofc], 0, 5) THEN 
					'Y' 					
				ELSE 
					'N' 
			END 		AS LV1_AUTH
			#end
		   
		   --//====================================================================================================
			--// APRO LEVEL 2 ( SCO/BAO PIC Confirm - 지점/대리점 담당자 )
			--//====================================================================================================
           ,MAX(LV2_OFC) 		AS LV2_OFC
           ,MAX(LV2_STS) 		AS LV2_STS
           ,MAX(LV2_USR_ID) 	AS LV2_USR_ID
           ,MAX(
				(
					SELECT  USR_NM
                      FROM  COM_USER
					 WHERE  USR_ID = LV2_USR_ID
				)
			) 					AS LV2_USR_NM
           ,MAX(LV2_DT) 		AS LV2_DT
			#if (${tab_cd} != 'A')
           ,CASE 
				WHEN EXISTS 
					(
						SELECT  A.USR_ID 
						  FROM  DMT_USR_ROLE_MTCH 	A
						       ,COM_USER 			B 
						 WHERE  A.USR_ID             = B.USR_ID 
						   AND  USR_ROLE_CD          = 
								(
									SELECT  ATTR_CTNT3
									  FROM  DMT_HRD_CDG_CTNT
									 WHERE  HRD_CDG_ID = 'AFT_BKG_APRO_PATH'
									   AND  ATTR_CTNT1 = 'BBOPIC'
								)						   
						   AND  SUBSTR(OFC_CD, 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
						   AND  A.USR_ID             = @[usr_id] 
						   AND  USE_FLG              = 'Y'
					)
					#if(${tab_cd} == 'P')
					AND (CURR_STS IN ('F', 'R') AND (SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH WHERE AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO AND AFT_BKG_PATH_CPLS_FLG = 'Y') = 1)
					#else
					AND CURR_STS IN ('V', 'O', 'J')
					#end
					AND SUBSTR(MAX(LV2_OFC), 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
				THEN 'Y' 
			ELSE 'N' END 		AS LV2_AUTH
			#end

			--//====================================================================================================
			--// APRO LEVEL 3 ( OFC O.Manager Approval )
			--//====================================================================================================
           ,MAX(LV3_OFC) 		AS LV3_OFC
           ,MAX(LV3_STS) 		AS LV3_STS
           ,MAX(LV3_USR_ID) 	AS LV3_USR_ID
           ,MAX(
				(
					SELECT  USR_NM
					  FROM  COM_USER
					 WHERE  USR_ID = LV3_USR_ID
				)
			) 					AS LV3_USR_NM
           ,MAX(LV3_DT) 		AS LV3_DT
			#if (${tab_cd} != 'A')
           ,CASE 
				WHEN EXISTS 
					(
						SELECT  A.USR_ID 
						  FROM  DMT_USR_ROLE_MTCH 	A
							   ,COM_USER 			B 
						 WHERE  A.USR_ID             = B.USR_ID 
						   AND  USR_ROLE_CD          = 
								(
									SELECT  ATTR_CTNT3
									  FROM  DMT_HRD_CDG_CTNT
									 WHERE  HRD_CDG_ID = 'AFT_BKG_APRO_PATH'
									   AND  ATTR_CTNT1 = 'OOMMGR'
								)	
						   AND  SUBSTR(OFC_CD, 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
						   AND  A.USR_ID             = @[usr_id] 
						   AND USE_FLG               = 'Y'
					)
					#if(${tab_cd} == 'P')
					AND (CURR_STS IN ('V', 'R') AND (SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH WHERE AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO AND AFT_BKG_PATH_CPLS_FLG = 'Y') = 2)
					#else
					AND CURR_STS IN ('G', 'O', 'J')
					#end
					AND SUBSTR(MAX(LV3_OFC), 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
				THEN 'Y' 
			ELSE 'N' END 		AS LV3_AUTH
			#end			
			
			--//====================================================================================================
			--// APRO LEVEL 4 ( SSZ/BAG Approval - 지점/대리점 장 )
			--//====================================================================================================
           ,MAX(LV4_OFC) 		AS LV4_OFC
           ,MAX(LV4_STS) 		AS LV4_STS
           ,MAX(LV4_USR_ID) 	AS LV4_USR_ID
           ,MAX(
				(
					SELECT  USR_NM
					  FROM  COM_USER
					 WHERE  USR_ID = LV4_USR_ID
				)
			) 					AS LV4_USR_NM
           ,MAX(LV4_DT) 		AS LV4_DT
			#if (${tab_cd} != 'A')
           ,CASE 
				WHEN EXISTS 
					(
						SELECT  A.USR_ID 
						  FROM  DMT_USR_ROLE_MTCH 	A
							   ,COM_USER 			B 
						 WHERE  A.USR_ID             = B.USR_ID 
						   AND  USR_ROLE_CD          =
								(
									SELECT  ATTR_CTNT3
									  FROM  DMT_HRD_CDG_CTNT
									 WHERE  HRD_CDG_ID = 'AFT_BKG_APRO_PATH'
									   AND  ATTR_CTNT1 = 'BBGMGR'
								)						   
						   AND  SUBSTR(OFC_CD, 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
						   AND  A.USR_ID             = @[usr_id] 
						   AND USE_FLG               = 'Y'
					)
					#if(${tab_cd} == 'P')
					AND (CURR_STS IN ('G', 'R') AND (SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH WHERE AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO AND AFT_BKG_PATH_CPLS_FLG = 'Y') = 2)
					#else
					AND CURR_STS IN ('H', 'O', 'J')
					#end
					AND SUBSTR(MAX(LV4_OFC), 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
				THEN 'Y' 
			ELSE 'N' END 		AS LV4_AUTH
			#end

			--//====================================================================================================
			--// APRO LEVEL 5 ( RHQ PIC Comfirm )
			--//====================================================================================================			
           ,MAX(LV5_OFC) 		AS LV5_OFC
           ,MAX(LV5_STS) 		AS LV5_STS
           ,MAX(LV5_USR_ID) 	AS LV5_USR_ID
           ,MAX(
				(
					SELECT  USR_NM
					  FROM  COM_USER
					 WHERE  USR_ID = LV5_USR_ID
				)
			) 					AS LV5_USR_NM
           ,MAX(LV5_DT) 		AS LV5_DT
			#if (${tab_cd} != 'A')
           ,CASE 
				WHEN EXISTS 
					(
						SELECT  A.USR_ID 
						  FROM  DMT_USR_ROLE_MTCH 	A
							   ,COM_USER 			B 
						 WHERE  A.USR_ID             = B.USR_ID 
						   AND  USR_ROLE_CD          = 
								(
									SELECT  ATTR_CTNT3
									  FROM  DMT_HRD_CDG_CTNT
									 WHERE  HRD_CDG_ID = 'AFT_BKG_APRO_PATH'
									   AND  ATTR_CTNT1 = 'RHQPIC'
								)						   
						   AND  SUBSTR(OFC_CD, 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
						   AND  A.USR_ID             = @[usr_id] 
						   AND USE_FLG               = 'Y'
					)
					#if(${tab_cd} == 'P')
					AND (CURR_STS IN ('H', 'R') AND (SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH WHERE AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO AND AFT_BKG_PATH_CPLS_FLG = 'Y') = 2)
					#else
					AND CURR_STS IN ('M', 'O', 'J')	--// I : HO PIC Confirm
					#end
					AND SUBSTR(MAX(LV5_OFC), 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
				THEN 'Y' 
			ELSE 'N' END 		AS LV5_AUTH
			#end
			
			--//====================================================================================================			
			--// APRO LEVEL 6 ( RHQ O.MGR Approval )
			--//====================================================================================================
           ,MAX(LV6_OFC) 		AS LV6_OFC
           ,MAX(LV6_STS) 		AS LV6_STS
           ,MAX(LV6_USR_ID) 	AS LV6_USR_ID
           ,MAX(
				(
					SELECT  USR_NM
					  FROM  COM_USER
					 WHERE  USR_ID = LV6_USR_ID
				)
			) 					AS LV6_USR_NM
           ,MAX(LV6_DT) 		AS LV6_DT
			#if (${tab_cd} != 'A')
           ,CASE 
				WHEN EXISTS 
					(
						SELECT  A.USR_ID 
						  FROM  DMT_USR_ROLE_MTCH 	A
							   ,COM_USER 			B 
						 WHERE  A.USR_ID             = B.USR_ID 
						   AND  USR_ROLE_CD          = 
								(
									SELECT  ATTR_CTNT3
									  FROM  DMT_HRD_CDG_CTNT
									 WHERE  HRD_CDG_ID = 'AFT_BKG_APRO_PATH'
									   AND  ATTR_CTNT1 = 'RHQMGR'
								)							   
						   AND  SUBSTR(OFC_CD, 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
						   AND  A.USR_ID             = @[usr_id] 
						   AND USE_FLG               = 'Y'
					)
					#if(${tab_cd} == 'P')
					AND (CURR_STS IN ('M', 'R') AND (SELECT MIN(AFT_BKG_PATH_LVL) FROM DMT_AFT_BKG_APRO_PATH WHERE AFT_EXPT_DAR_NO = T.AFT_EXPT_DAR_NO AND AFT_BKG_PATH_CPLS_FLG = 'Y') = 2)
					#else
					AND CURR_STS IN ('I', 'O', 'J')	--// I : HO PIC Confirm
					#end
					AND SUBSTR(MAX(LV6_OFC), 0, 5) = SUBSTR(@[usr_ofc], 0, 5) 
