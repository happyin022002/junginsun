                                   ,TO_CHAR(UPD_DT, 'YYYY-MM-DD') 	AS UPD_DT
                                   ,MAX(PROG_SEQ) 					AS PROG_SEQ
                                   ,AFT_BKG_PATH_LVL
                                   ,AFT_BKG_PATH_CD
								   
							  FROM  MAIN
							 WHERE  1 = 1
							 
							GROUP BY AFT_EXPT_DAR_NO, CURR_STS, SC_NO, RFA_NO, TAA_NO, DMDT_TRF_CD, DMDT_OFC_CD
								#if (${group_by} == 'BKG')
								,BKG_NO
								,BL_NO
								#end
								,RQST_OFC_CD
								,RQST_DT
								,RQST_USR_ID
								,RQST_USR_NM
								,AFT_BKG_APRO_NO
								,UPD_DT
								,AFT_BKG_PATH_LVL
								,AFT_BKG_PATH_CD
								
							ORDER BY AFT_EXPT_DAR_NO, AFT_BKG_PATH_LVL 
								#if (${group_by} == 'BKG')
								,BKG_NO
								,BL_NO
								#end
						) T 
			) T
	GROUP BY AFT_EXPT_DAR_NO
	        ,CURR_STS
			,SC_NO
			,RFA_NO
			,TAA_NO
			,DMDT_TRF_CD
			,DMDT_OFC_CD

		    #if (${group_by} == 'BKG')
