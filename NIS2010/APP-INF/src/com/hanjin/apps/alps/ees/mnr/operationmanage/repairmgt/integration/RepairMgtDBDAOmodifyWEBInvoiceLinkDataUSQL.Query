<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOmodifyWEBInvoiceLinkDataUSQL">
			<desc><![CDATA[RepairMgtDBDAOmodifyWEBInvoiceLinkDataUSQL
-- 2014.11 10만불 결제관련]]></desc>
			<sql><![CDATA[
UPDATE   MNR_ORD_DTL
SET      INV_NO = @[inv_no]
       , INV_AMT = REPLACE (@[inv_amt], ',', '')
       , PAY_INV_SEQ = @[pay_inv_seq]
       , RPR_RSLT_DT = TO_DATE(@[rpr_rslt_dt], 'YYYY-MM-DD')
       , IDA_SAC_CD = @[ida_sac_cd]
       , IDA_CGST_RTO = @[ida_cgst_rto]
       , IDA_SGST_RTO = @[ida_sgst_rto]
       , IDA_IGST_RTO = @[ida_igst_rto]
       , IDA_UGST_RTO = @[ida_ugst_rto]
       , IDA_CGST_AMT = @[ida_cgst_amt]
       , IDA_SGST_AMT = @[ida_sgst_amt]
       , IDA_IGST_AMT = @[ida_igst_amt]
       , IDA_UGST_AMT = @[ida_ugst_amt]
       , FILE_SEQ = @[file_seq]
       , UPD_USR_ID = @[cre_usr_id]
       , UPD_DT = sysdate
WHERE    1 = 1
AND      MNR_ORD_OFC_CTY_CD = @[mnr_ord_ofc_cty_cd]
AND      MNR_ORD_SEQ = @[mnr_ord_seq]
AND      ORD_DTL_SEQ = @[ord_dtl_seq]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_amt" type="12" value="" out="N"/>
				<param name="pay_inv_seq" type="12" value="" out="N"/>
				<param name="rpr_rslt_dt" type="12" value="" out="N"/>
				<param name="ida_sac_cd" type="12" value="" out="N"/>
				<param name="ida_cgst_rto" type="12" value="" out="N"/>
				<param name="ida_sgst_rto" type="12" value="" out="N"/>
				<param name="ida_igst_rto" type="12" value="" out="N"/>
				<param name="ida_ugst_rto" type="12" value="" out="N"/>
				<param name="ida_cgst_amt" type="12" value="" out="N"/>
				<param name="ida_sgst_amt" type="12" value="" out="N"/>
				<param name="ida_igst_amt" type="12" value="" out="N"/>
				<param name="ida_ugst_amt" type="12" value="" out="N"/>
				<param name="file_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="mnr_ord_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="mnr_ord_seq" type="12" value="" out="N"/>
				<param name="ord_dtl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
