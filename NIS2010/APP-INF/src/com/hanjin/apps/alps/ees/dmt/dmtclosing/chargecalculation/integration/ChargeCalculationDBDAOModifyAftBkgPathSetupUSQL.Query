<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeCalculationDBDAOModifyAftBkgPathSetupUSQL">
			<desc><![CDATA[ChargeCalculationDBDAOModifyAftBkgPathSetupUSQL]]></desc>
			<sql><![CDATA[
UPDATE  DMT_AFT_BKG_PATH_OFC_STUP 
   SET  EFF_DT                   = TO_DATE(@[eff_dt], 'YYYYMMDD') + .0
	   ,FM_DC_AMT                = TO_NUMBER(@[fm_dc_amt])
	   ,EXP_DT                   = TO_DATE(@[exp_dt], 'YYYYMMDD') + 0.99999
	   ,TO_DC_AMT                = TO_NUMBER(@[to_dc_amt])
	   ,BRNC_OFC_PIC_CHK_FLG     = DECODE(@[brnc_ofc_pic_chk_flg],     '0', 'N', '1', 'Y')
	   ,BRNC_OFC_OP_MGR_APRO_FLG = DECODE(@[brnc_ofc_op_mgr_apro_flg], '0', 'N', '1', 'Y')
	   ,BRNC_OFC_MGR_APRO_FLG    = DECODE(@[brnc_ofc_mgr_apro_flg],    '0', 'N', '1', 'Y')
	   ,RHQ_PIC_CHK_FLG          = DECODE(@[rhq_pic_chk_flg],          '0', 'N', '1', 'Y')
	   ,RHQ_MGR_APRO_FLG         = DECODE(@[rhq_mgr_apro_flg],         '0', 'N', '1', 'Y')
	   ,HO_PIC_CHK_FLG           = DECODE(@[ho_pic_chk_flg],           '0', 'N', '1', 'Y')
	   ,HO_MGR_APRO_FLG          = DECODE(@[ho_mgr_apro_flg],          '0', 'N', '1', 'Y')
	   ,UPD_USR_ID               = @[upd_usr_id]
	   ,UPD_DT                   = SYSDATE
 WHERE  AFT_BKG_PATH_STUP_SEQ = TO_NUMBER(@[aft_bkg_path_stup_seq])
   AND  RHQ_CD = @[rhq_cd]			]]></sql>
			<params>
				<param name="eff_dt" type="12" value="20140101" out="N"/>
				<param name="fm_dc_amt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
				<param name="to_dc_amt" type="12" value="" out="N"/>
				<param name="brnc_ofc_pic_chk_flg" type="12" value="" out="N"/>
				<param name="brnc_ofc_op_mgr_apro_flg" type="12" value="" out="N"/>
				<param name="brnc_ofc_mgr_apro_flg" type="12" value="" out="N"/>
				<param name="rhq_pic_chk_flg" type="12" value="" out="N"/>
				<param name="rhq_mgr_apro_flg" type="12" value="" out="N"/>
				<param name="ho_pic_chk_flg" type="12" value="" out="N"/>
				<param name="ho_mgr_apro_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="9350302" out="N"/>
				<param name="aft_bkg_path_stup_seq" type="12" value="6" out="N"/>
				<param name="rhq_cd" type="12" value="SINRS" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
