<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOCheckCPSScExptCustNameDataRSQL">
			<desc><![CDATA[입력된 Customer Code로부터 Customer Name을 체크한다.]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD,
       CUST_SEQ,
       CUST_LGL_ENG_NM
FROM MDM_CUSTOMER
WHERE CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = TRANSLATE(@[cust_seq], '0123456789' || @[cust_seq], '0123456789')			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
