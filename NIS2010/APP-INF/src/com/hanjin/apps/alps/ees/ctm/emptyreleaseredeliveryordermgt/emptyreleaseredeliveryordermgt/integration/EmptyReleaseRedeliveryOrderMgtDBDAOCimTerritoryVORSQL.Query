<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="EmptyReleaseRedeliveryOrderMgtDBDAOCimTerritoryVORSQL">
			<desc><![CDATA[ EES_CTM_0428 : TerritoryList에 해당되는 값을 조회]]></desc>
			<sql><![CDATA[
SELECT A.CNTR_STK_TERR_CD,
       B.CNT_NM,
       B.CNT_NM AS CNT_NM0,
       A.CNT_CD,
       A.OFC_CD,
       A.UPD_USR_ID AS USR_ID,
       TO_CHAR (A.CRE_DT, 'YYYY-MM-DD HH24:MI') AS CRE_DT,
       TO_CHAR (A.UPD_DT, 'YYYY-MM-DD HH24:MI') AS UPD_DT,
       A.CNTR_STK_TERR_CD CNTR_STK_TERR_CD0,
       A.CNT_CD AS CNT_CD0,
       A.OFC_CD AS OFC_CD0
  FROM CIM_TERRITORY A,
       MDM_COUNTRY B
 WHERE A.CO_CD IN ('A', 'H')
   AND A.CNT_CD = B.CNT_CD
   AND B.DELT_FLG = 'N'
 ORDER BY A.CNTR_STK_TERR_CD,
       A.CNT_CD,
       A.OFC_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
