<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOsearchEstimateSeqNewEqDataRSQL">
			<desc><![CDATA[searchEstimateSeqNewEqData]]></desc>
			<sql><![CDATA[
SELECT @[rqst_eq_no] AS RQST_EQ_NO,
         NVL(MAX(B.RPR_RQST_SEQ) + 1, 1) AS RPR_RQST_SEQ,
         '1' AS RPR_RQST_VER_NO
    FROM MNR_RPR_RQST_HDR B
   WHERE B.RQST_EQ_NO = @[rqst_eq_no]			]]></sql>
			<params>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
