<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOCheckCHSCpsAgreementYardDataRSQL">
			<desc><![CDATA[Tab2에서 입력된 Yard Code로부터 Yard Name과 Tab1의 SCC를 체크한다.]]></desc>
			<sql><![CDATA[
SELECT YD_CD,
       YD_NM,
       LOC_CD
FROM MDM_YARD
WHERE YD_CD = @[chk_yd_cd]			]]></sql>
			<params>
				<param name="chk_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
