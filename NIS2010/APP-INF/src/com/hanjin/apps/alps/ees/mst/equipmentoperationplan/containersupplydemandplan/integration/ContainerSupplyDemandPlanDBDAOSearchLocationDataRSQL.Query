<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSupplyDemandPlanDBDAOSearchLocationDataRSQL">
			<desc><![CDATA[SearchLocationData]]></desc>
			<sql><![CDATA[
SELECT A.LOC_CD CODE, A.LOC_CD CODE_NM
FROM  MDM_LOCATION A
WHERE 1 = 1 
AND A.LOC_CD = @[loc_cd]
AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
