<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DropOffTariffDBDAOSearchDodTariffContiRSQL">
			<desc><![CDATA[SearchDodTariffConti]]></desc>
			<sql><![CDATA[
SELECT A.CONTI_CD POL_CONTI_CD
            FROM   MDM_CONTINENT A, MDM_SUBCONTINENT B, MDM_COUNTRY C
            WHERE  1=1
            AND    A.DELT_FLG = 'N'
            AND    A.CONTI_CD = B.CONTI_CD
            AND    B.DELT_FLG = 'N'
            AND    B.SCONTI_CD = C.SCONTI_CD
            AND    C.DELT_FLG = 'N'
            and    c.cnt_cd = @[s_cnt_cd]
			and    a.conti_cd IN (SELECT A.CONTI_CD 
            FROM   MDM_CONTINENT A, MDM_SUBCONTINENT B, MDM_COUNTRY C
            WHERE  1=1
            AND    A.DELT_FLG = 'N'
            AND    A.CONTI_CD = B.CONTI_CD
            AND    B.DELT_FLG = 'N'
            AND    B.SCONTI_CD = C.SCONTI_CD
            AND    C.DELT_FLG = 'N'
            and    c.cnt_cd = substr(@[s_rtn_loc_cd], 0, 2))			]]></sql>
			<params>
				<param name="s_cnt_cd" type="12" value="" out="N"/>
				<param name="s_rtn_loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
