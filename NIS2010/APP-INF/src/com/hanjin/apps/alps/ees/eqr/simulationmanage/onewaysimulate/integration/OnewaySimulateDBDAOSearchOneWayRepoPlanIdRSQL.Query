<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnewaySimulateDBDAOSearchOneWayRepoPlanIdRSQL">
			<desc><![CDATA[OneWay Off 테이블에 해당 repo_pln_id가 존재하는지를 조회]]></desc>
			<sql><![CDATA[
SELECT 
	REPO_PLN_ID 
FROM 
	EQR_ONE_WY_OFFR 
WHERE 
	REPO_PLN_ID = @[repo_pln_id]			]]></sql>
			<params>
				<param name="repo_pln_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
