<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOAddCreditInvoiceDetailCSQL">
			<desc><![CDATA[Invoice Cancel Reason Entry]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_INV_DTL (
	DMDT_INV_NO
,	CRE_OFC_CD
,	INV_DTL_SEQ
,	SYS_AREA_GRP_ID
,	CNTR_NO
,	CNTR_CYC_NO
,	DMDT_TRF_CD
,	DMDT_CHG_LOC_DIV_CD
,	CHG_SEQ
,	CNTR_TPSZ_CD
,	FM_MVMT_DT
,	TO_MVMT_DT
,	FT_DYS
,	FT_CMNC_DT
,	FT_END_DT
,	FX_FT_OVR_DYS
,	XCLD_SAT_FLG
,	XCLD_SUN_FLG
,	XCLD_HOL_FLG
,	ORG_CHG_AMT
,	SC_RFA_EXPT_AMT
,	AFT_EXPT_DC_AMT
,	BIL_AMT
,	CNTR_INV_AMT
,	TAX_RTO
,	TAX_AMT
,	INV_PRT_FLG
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
)
SELECT
	@[dmdt_inv_no]
,	@[cre_ofc_cd]
,	INV_DTL_SEQ
,	SYS_AREA_GRP_ID
,	CNTR_NO
,	CNTR_CYC_NO
,	DMDT_TRF_CD
,	DMDT_CHG_LOC_DIV_CD
,	CHG_SEQ
,	CNTR_TPSZ_CD
,	FM_MVMT_DT
,	TO_MVMT_DT
,	FT_DYS
,	FT_CMNC_DT
,	FT_END_DT
,	FX_FT_OVR_DYS
,	XCLD_SAT_FLG
,	XCLD_SUN_FLG
,	XCLD_HOL_FLG
,	0 - ORG_CHG_AMT
,	0 - SC_RFA_EXPT_AMT
,	0 - AFT_EXPT_DC_AMT
,	0 - BIL_AMT
,	0 - CNTR_INV_AMT
,	TAX_RTO
,	0 - TAX_AMT
,	INV_PRT_FLG
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]),SYSDATE)
,	@[upd_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]),SYSDATE)
,	@[upd_ofc_cd]
FROM DMT_INV_DTL
WHERE DMDT_INV_NO = @[old_dmt_inv_no]
AND	CRE_OFC_CD  = @[cre_ofc_cd]			]]></sql>
			<params>
				<param name="dmdt_inv_no" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="old_dmt_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
