<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOAddSCExceptionCommoditiesCSQL">
			<desc><![CDATA[S/C Exception Terms Entry 의 Group 에 소속된 Commodity 정보를 입력하는 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_SC_EXPT_CMDT (
	PROP_NO
,	SC_EXPT_VER_SEQ
,	SC_EXPT_GRP_SEQ
,	CMDT_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[prop_no]
,	@[sc_expt_ver_seq]
,	@[sc_expt_grp_seq]
,	@[cmdt_cd]
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
)			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
