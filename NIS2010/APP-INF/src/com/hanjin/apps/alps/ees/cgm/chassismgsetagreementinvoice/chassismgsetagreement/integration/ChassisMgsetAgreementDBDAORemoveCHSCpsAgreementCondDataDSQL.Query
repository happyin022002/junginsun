<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAORemoveCHSCpsAgreementCondDataDSQL">
			<desc><![CDATA[NP(ZP) Agreement 의 Condition 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM CGM_AGMT_CPS_COND
WHERE AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
	  AND AGMT_SEQ = @[agmt_seq]
	  AND AGMT_VER_NO = @[agmt_ver_no]			]]></sql>
			<params>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
