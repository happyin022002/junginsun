<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerSupplyDemandPlanDBDAOSearchEqPriceDataSubSeqRSQL">
			<desc><![CDATA[Select SubSEQ]]></desc>
			<sql><![CDATA[
SELECT ROWNUM CNT, AAA.LOC_CD 
FROM
(
SELECT AA.LOC_CD LOC_CD
FROM
(
SELECT A.LOC_CD
FROM MST_EQ_PUR_PRC A,
     MDM_LOCATION B, 
     MDM_EQ_ORZ_CHT C
WHERE A.BSE_YRMON BETWEEN @[bse_yrmon0] AND @[bse_yrmon1]
AND   A.LOC_CD   =   B.LOC_CD
AND   B.SCC_CD   =   C.SCC_CD
#if(${mloc_cd} == 'R')
AND   C.RCC_CD   =   @[loc_cd]
#end
#if(${mloc_cd} == 'L')
AND   C.LCC_CD   =   @[loc_cd]
#end
#if(${mloc_cd} == 'E')
AND   C.ECC_CD   =   @[loc_cd]
#end
#if(${mloc_cd} == 'S')
AND   C.SCC_CD   =   @[loc_cd]
#end
#if (${cntr_tpsz_cd} != '' && ${cntr_tpsz_cd} != 'ALL') 
AND   A.EQ_TPSZ_CD  IN ( 
		#foreach($cntrtpszcd in ${vel_tpsz_cd})  
			'$cntrtpszcd',  
			#end  
			'') 
#end
#if(${eq_knd_cd} != '')
AND   A.EQ_KND_CD   =  @[eq_knd_cd]
#end
GROUP BY A.LOC_CD
) AA
ORDER BY AA.LOC_CD ASC
) AAA			]]></sql>
			<params>
				<param name="bse_yrmon0" type="12" value="" out="N"/>
				<param name="bse_yrmon1" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
