<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOSearchCHSOnHireReportbyMonthSummaryDataRSQL">
			<desc><![CDATA[월별 장비 임차내역  Summary 조회
2015 조직코드개편 Chang-Young Kim]]></desc>
			<sql><![CDATA[
WITH PARAM 
AS (SELECT @[period_stdt] PERIOD_STDT ,
               @[period_eddt] PERIOD_EDDT ,
               @[loc_cd] LOC_CD ,
               @[loc_tp] LOC_TP ,
               @[term_change] TERM_CHANGE, 
               @[eq_knd_cd] EQ_KND_CD
        FROM   DUAL ) ,
      XXX 
AS (SELECT #foreach($key IN ${arrTpsz}) SUM(DECODE(DD, '$key' , EE, 0)) $key ,
           #end CC
        FROM   (SELECT TO_CHAR(C.STS_EVNT_DT , 'YYYY-MM') CC,
                       A.EQ_TPSZ_CD DD,
                       COUNT(A.EQ_NO) EE
                FROM   CGM_EQUIPMENT A,
                       CGM_EQ_TP_SZ B,
                       CGM_EQ_STS_HIS C,
                       CGM_AGREEMENT D ,
                       MDM_LOCATION E,
                       MDM_EQ_ORZ_CHT F,
                       PARAM P
                WHERE  A.EQ_TPSZ_CD = B.EQ_TPSZ_CD
                AND    A.EQ_NO = C.EQ_NO
                AND    C.STS_EVNT_DT BETWEEN TO_DATE(P.PERIOD_STDT, 'YYYYMMDD') AND TO_DATE(P.PERIOD_EDDT, 'YYYYMMDD')+0.99999
                AND    C.AGMT_OFC_CTY_CD = D.AGMT_OFC_CTY_CD
                AND    C.AGMT_SEQ = D.AGMT_SEQ
                AND    C.AGMT_VER_NO = D.AGMT_VER_NO
                AND    E.SCC_CD = F.SCC_CD
				AND    B.EQ_KND_CD = P.EQ_KND_CD
                AND    A.CRNT_LOC_CD = E.LOC_CD
                AND    SUBSTR(NVL(C.DIFF_RMK, ' '), 1, 6) <> ( SELECT MNR_CD_ID FROM MNR_GEN_CD WHERE PRNT_CD_ID = 'HOOFC' AND ROWNUM = 1 )
                AND    C.AGMT_SEQ <> 999998
                AND    P.TERM_CHANGE IS NULL 
          #if (${dii} != '' ) 
             #if (${dii} == 'N' )
                AND    C.EQ_ASET_STS_CD IN ('LSI', 'OWN') 
             #elseif (${dii} == 'Y' )
                AND    C.EQ_ASET_STS_CD = 'DII' 
             #end 
          #else
                AND    C.EQ_ASET_STS_CD IN ('LSI', 'DII', 'OWN') 
          #end 
          #if (${loc_cd} != '' )
                AND    DECODE(P.loc_tp, 'R', F.RCC_CD, 'L', F.LCC_CD, 'S', F.SCC_CD ) = P.LOC_CD #end #if (${lstm_cd_str} != '' )
                AND    D.AGMT_LSTM_CD IN (#foreach($key IN ${lstm_cd}) 
                                             #if($velocityCount < $lstm_cd.size()) '$key',
                                             #else '$key'#end 
                                         #end ) 
          #end
		  #if (${np} == 'Y')
                AND D.AGMT_LSTM_CD ='NP'
          #elseif (${np} == 'N')
			    AND D.AGMT_LSTM_CD NOT IN ('NP')
		  #end
                GROUP BY TO_CHAR(C.STS_EVNT_DT ,'YYYY-MM'), A.EQ_TPSZ_CD
                UNION ALL /* Term Change Excluding */
                SELECT TO_CHAR(C.STS_EVNT_DT , 'YYYY-MM') CC,
                       A.EQ_TPSZ_CD DD,
                       COUNT(A.EQ_NO) EE
                FROM   CGM_EQUIPMENT A,
                       CGM_EQ_TP_SZ B,
                       (SELECT A.*
                        FROM   (SELECT A.*,
                                       (SELECT /*+ INDEX_ASC(T02 XPKCGM_EQ_STS_HIS) */
                                          EQ_STS_SEQ
                                        FROM   CGM_EQ_STS_HIS T02
                                        WHERE  A.EQ_NO = T02.EQ_NO
                                        AND    A.EQ_STS_SEQ < T02.EQ_STS_SEQ
                                        AND    T02.EQ_ASET_STS_CD IN ('LSO', 'TLL', 'SLD', 'DON', 'SCR')
                                        AND    T02.TERM_CNG_SEQ IS NULL
                                        AND    ROWNUM = 1 ) AS LSO_STS_SEQ
                                FROM   CGM_EQ_STS_HIS A,
                                       PARAM P
                                WHERE  A.STS_EVNT_DT BETWEEN TO_DATE(P.PERIOD_STDT, 'YYYYMMDD') AND TO_DATE(P.PERIOD_EDDT, 'YYYYMMDD')+0.99999
                                AND    A.AGMT_SEQ <> 999998
                                AND    A.EQ_ASET_STS_CD IN ('LSI', 'DII',  'OWN')
                                AND    A.TERM_CNG_SEQ IS NULL
                                AND    SUBSTR(NVL(A.DIFF_RMK, ' '), 1, 6) <> ( SELECT MNR_CD_ID FROM MNR_GEN_CD WHERE PRNT_CD_ID = 'HOOFC' AND ROWNUM = 1 ) )A
                        WHERE  LSO_STS_SEQ IS NULL )C,
                       CGM_AGREEMENT D ,
                       MDM_LOCATION E,
                       MDM_EQ_ORZ_CHT F,
                       PARAM P
                WHERE  A.EQ_TPSZ_CD = B.EQ_TPSZ_CD
                AND    A.EQ_NO = C.EQ_NO
		        AND    B.EQ_KND_CD = P.EQ_KND_CD
                AND    C.AGMT_OFC_CTY_CD = D.AGMT_OFC_CTY_CD
                AND    C.AGMT_SEQ = D.AGMT_SEQ
                AND    C.AGMT_VER_NO = D.AGMT_VER_NO
                AND    E.SCC_CD = F.SCC_CD
                AND    A.CRNT_LOC_CD = E.LOC_CD
                AND    P.TERM_CHANGE = 'N' 
          #if (${dii} != '' ) 
            #if (${dii} == 'N' )
                AND    C.EQ_ASET_STS_CD IN ('LSI','OWN') 
            #elseif (${dii} == 'Y' )
                AND    C.EQ_ASET_STS_CD = 'DII' 
            #end 
          #else
                AND    C.EQ_ASET_STS_CD IN ('LSI', 'DII', 'OWN') 
          #end 
          #if (${loc_cd} != '' )
                AND    DECODE(P.loc_tp, 'R', F.RCC_CD, 'L', F.LCC_CD, 'S', F.SCC_CD ) = P.LOC_CD 
          #end 
          #if (${lstm_cd_str} != '' )
                AND    D.AGMT_LSTM_CD IN ( #foreach($key IN ${lstm_cd}) 
                                             #if($velocityCount < $lstm_cd.size()) '$key',
                                             #else '$key' #end 
                                           #end ) 
          #end
		  #if (${np} == 'Y')
                AND D.AGMT_LSTM_CD ='NP'
          #elseif (${np} == 'N')
			    AND D.AGMT_LSTM_CD NOT IN ('NP')
		  #end
                GROUP BY TO_CHAR(C.STS_EVNT_DT ,'YYYY-MM'), A.EQ_TPSZ_CD
                UNION ALL /* Term Change Only */
                SELECT TO_CHAR(C.STS_EVNT_DT , 'YYYY-MM') CC,
                       A.EQ_TPSZ_CD DD,
                       COUNT(A.EQ_NO) EE
                FROM   CGM_EQUIPMENT A,
                       CGM_EQ_TP_SZ B,
                       (SELECT A.*
                        FROM   (SELECT A.*,
                                       (SELECT /*+ INDEX_ASC(T02 XPKCGM_EQ_STS_HIS) */
                                          TERM_CNG_SEQ
                                        FROM   CGM_EQ_STS_HIS T02
                                        WHERE  A.EQ_NO = T02.EQ_NO
                                        AND    A.EQ_STS_SEQ > T02.EQ_STS_SEQ
                                        AND    T02.EQ_ASET_STS_CD IN ('LSO', 'TLL', 'SLD', 'DON', 'SCR')
                                        AND    T02.TERM_CNG_SEQ IS NOT NULL
                                        AND    ROWNUM = 1 ) AS LSO_STS_SEQ
                                FROM   CGM_EQ_STS_HIS A,
                                       PARAM P
                                WHERE  A.STS_EVNT_DT BETWEEN TO_DATE(P.PERIOD_STDT, 'YYYYMMDD') AND TO_DATE(P.PERIOD_EDDT, 'YYYYMMDD')+0.99999
                                AND    A.AGMT_SEQ <> 999998
                                AND    A.EQ_ASET_STS_CD IN ('LSI', 'DII', 'OWN')
                                AND    A.TERM_CNG_SEQ IS NOT NULL
                                AND    SUBSTR(NVL(A.DIFF_RMK, ' '), 1, 6) <> ( SELECT MNR_CD_ID FROM MNR_GEN_CD WHERE PRNT_CD_ID = 'HOOFC' AND ROWNUM = 1 ) )A
                        WHERE  LSO_STS_SEQ IS NOT NULL)C,
                       CGM_AGREEMENT D ,
                       MDM_LOCATION E,
                       MDM_EQ_ORZ_CHT F,
                       PARAM P
                WHERE  A.EQ_TPSZ_CD = B.EQ_TPSZ_CD
                AND    A.EQ_NO = C.EQ_NO
				AND    B.EQ_KND_CD = P.EQ_KND_CD
                AND    C.AGMT_OFC_CTY_CD = D.AGMT_OFC_CTY_CD
                AND    C.AGMT_SEQ = D.AGMT_SEQ
                AND    C.AGMT_VER_NO = D.AGMT_VER_NO
                AND    E.SCC_CD = F.SCC_CD
                AND    A.CRNT_LOC_CD = E.LOC_CD
                AND    'Y' = NVL(P.TERM_CHANGE, 'N') 
          #if (${dii} != '' ) 
            #if (${dii} == 'N' )
                AND    C.EQ_ASET_STS_CD IN ('LSI', 'OWN') 
            #elseif (${dii} == 'Y' )
                AND    C.EQ_ASET_STS_CD = 'DII' 
            #end 
          #else
                AND    C.EQ_ASET_STS_CD IN ('LSI', 'DII', 'OWN') 
          #end 
          #if (${loc_cd} != '' )
                AND    DECODE(P.loc_tp, 'R', F.RCC_CD, 'L', F.LCC_CD, 'S', F.SCC_CD ) = P.LOC_CD 
          #end 
          #if (${lstm_cd_str} != '' )
                AND    D.AGMT_LSTM_CD IN ( #foreach($key IN ${lstm_cd}) 
                                             #if($velocityCount < $lstm_cd.size()) '$key',
                                             #else '$key' #end 
                                           #end ) 
          #end
		  #if (${np} == 'Y')
                AND D.AGMT_LSTM_CD ='NP'
          #elseif (${np} == 'N')
			    AND D.AGMT_LSTM_CD NOT IN ('NP')
		  #end
                GROUP BY TO_CHAR(C.STS_EVNT_DT ,'YYYY-MM'), A.EQ_TPSZ_CD )
        GROUP BY CC
        ORDER BY CC )
        
SELECT #foreach($key IN ${arrTpsz}) 
       DECODE( AA.YM , 'Ratio' , TO_CHAR( AA.$key , '990.00') , TO_CHAR( AA.$key , '999,999,999,990' )) $key ,
       #end 
       TO_CHAR( AA.DIV_TOTAL , '999,999,999,990' ) DIV_TOTAL ,
       TO_CHAR((AA.DIV_TOTAL / BB.DIV_TOTAL ) * 100 , '990.00' ) || '%' RATIO ,
       AA.YM
FROM   (SELECT #foreach($key IN ${arrTpsz}) $key ,
               #end ( #foreach($key IN ${arrTpsz}) #if($velocityCount < $arrTpsz.size()) $key + #else $key #end #end ) DIV_TOTAL ,
               CC YM
        FROM   XXX
        UNION ALL
        SELECT #foreach($key IN ${arrTpsz}) SUM( $key ) $key,
               #end ( #foreach($key IN ${arrTpsz}) #if($velocityCount < $arrTpsz.size()) SUM($key) + #else SUM($key) #end #end ) DIV_TOTAL ,
               'G.TTL' YM
        FROM   XXX
        UNION ALL
        SELECT #foreach($key IN ${arrTpsz}) SUM( $key ) / ( #foreach($key IN ${arrTpsz}) 
               #if($velocityCount < $arrTpsz.size()) SUM($key) + #else SUM($key) #end #end ) * 100 $key ,
               #end 100 DIV_TOTAL ,
               'Ratio' YM
        FROM   XXX ) AA ,
       (SELECT (#foreach($key IN ${arrTpsz}) 
                  #if($velocityCount < $arrTpsz.size()) SUM($key) + 
                  #else SUM($key) 
                  #end 
                #end ) DIV_TOTAL
        FROM   XXX ) BB			]]></sql>
			<params>
				<param name="period_stdt" type="12" value="20110101" out="N"/>
				<param name="period_eddt" type="12" value="20110131" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="loc_tp" type="12" value="" out="N"/>
				<param name="term_change" type="12" value="N" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
