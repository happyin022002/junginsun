<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DemDetTariffMgtDBDAOAddDmtTrfFreeTmCSQL">
			<desc><![CDATA[Basic Tariff Creation - Group]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_TRF_FREE_TM
(
	SYS_AREA_GRP_ID
,	DMDT_TRF_CD
,	TRF_SEQ
,   DMDT_DE_TERM_CD
,	TRF_GRP_SEQ
,	TRF_FT_SEQ
,	FT_FM_QTY
,	FT_TO_QTY
,	FT_DYS
,	CRE_USR_ID
,	CRE_DT
,	CRE_OFC_CD
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
)
VALUES
( 
	@[svr_id]
,	@[dmdt_trf_cd]
,	@[trf_seq]
,   @[dmdt_de_term_cd]
,	@[trf_grp_seq]
,	(
    SELECT  NVL(MAX(TRF_FT_SEQ), 0) + 1
    FROM    DMT_TRF_FREE_TM 
    WHERE   SYS_AREA_GRP_ID = @[svr_id] 
    AND     DMDT_TRF_CD     = @[dmdt_trf_cd]
    AND     TRF_SEQ         = @[trf_seq]
    AND     DMDT_DE_TERM_CD = @[dmdt_de_term_cd]
    AND     TRF_GRP_SEQ     = @[trf_grp_seq]
    )
,	@[ft_fm_qty]
,	@[ft_to_qty]
,	@[ft_dys]
,	@[usr_id]
,	sysdate
,	@[ofc_cd]
,	@[usr_id]
,	sysdate
,	@[ofc_cd]
)			]]></sql>
			<params>
				<param name="svr_id" type="12" value="" out="N"/>
				<param name="dmdt_trf_cd" type="12" value="" out="N"/>
				<param name="trf_seq" type="2" value="" out="N"/>
				<param name="dmdt_de_term_cd" type="12" value="" out="N"/>
				<param name="trf_grp_seq" type="2" value="" out="N"/>
				<param name="ft_fm_qty" type="2" value="" out="N"/>
				<param name="ft_to_qty" type="2" value="" out="N"/>
				<param name="ft_dys" type="2" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
