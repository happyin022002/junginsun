<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOCheckContainerNoRSQL">
			<desc><![CDATA[Container No. 가 존재하는지 여부와 있다면 Container 의 Type/Size 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT	CNTR_TPSZ_CD
FROM	MST_CONTAINER
WHERE	CNTR_NO = @[cntr_no]			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
