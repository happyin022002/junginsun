<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOcheckBareMGSetContNoDataRSQL">
			<desc><![CDATA[(Container No) 사용여부 확인]]></desc>
			<sql><![CDATA[
SELECT CASE WHEN COUNT(1) >= 1 THEN 'T'  -- T: 사용가능
                               ELSE 'F'  -- F: 입력불가 
       END CHK_EQNO 
FROM MST_CONTAINER
WHERE CNTR_NO = @[cntr_no]
AND   ACIAC_DIV_CD = 'A' 			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
