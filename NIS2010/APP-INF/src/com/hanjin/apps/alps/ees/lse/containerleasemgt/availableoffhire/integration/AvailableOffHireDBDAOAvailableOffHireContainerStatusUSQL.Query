<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AvailableOffHireDBDAOAvailableOffHireContainerStatusUSQL">
			<desc><![CDATA[선정된 대상장비의 반납상태 정보를 갱신합니다.]]></desc>
			<sql><![CDATA[
UPDATE  LSE_AVAL_OFFH
SET     OFFH_STS_CD  = @[offh_sts_cd], 
        UPD_USR_ID   = @[upd_usr_id],
        UPD_DT       = SYSDATE 
WHERE   OFFH_DUE_DT >= TO_CHAR(SYSDATE,'YYYYMMDD')
AND     CNTR_NO      = @[cntr_no]
AND     OFFH_STS_CD  = DECODE(@[offh_sts_cd],'L','C','C','L')
			]]></sql>
			<params>
				<param name="offh_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
