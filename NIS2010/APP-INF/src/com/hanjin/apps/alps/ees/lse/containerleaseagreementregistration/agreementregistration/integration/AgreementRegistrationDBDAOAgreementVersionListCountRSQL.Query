<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AgreementRegistrationDBDAOAgreementVersionListCountRSQL">
			<desc><![CDATA[Lease Agreement List Total Count Search]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM   LSE_AGREEMENT A
     , LSE_AGMT_VER B
     , MDM_VENDOR C
WHERE  A.VNDR_SEQ = C.VNDR_SEQ(+)
AND    B.AGMT_CTY_CD = A.AGMT_CTY_CD
AND    B.AGMT_SEQ = A.AGMT_SEQ
#if (${agmt_cty_cd} != "") 
   			AND A.AGMT_CTY_CD = @[agmt_cty_cd]
#end
#if (${agmt_seq} != "") 
   			AND A.AGMT_SEQ = @[agmt_seq]
#end
#if (${vndr_seq} != "") 
   			AND A.VNDR_SEQ = @[vndr_seq]
#end
#if (${lstm_cd} != "") 
   			AND A.LSTM_CD = @[lstm_cd]
#end			]]></sql>
			<params>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="vndr_seq" type="2" value="" out="N"/>
				<param name="lstm_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
