<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOgetOfficeByYardRSQL">
			<desc><![CDATA[야드 코드로 해당 컨테이너의 OFC CD를 얻어온다]]></desc>
			<sql><![CDATA[
SELECT OFC_CD FROM MDM_YARD WHERE YD_CD = @[yard_cd]			]]></sql>
			<params>
				<param name="yard_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
