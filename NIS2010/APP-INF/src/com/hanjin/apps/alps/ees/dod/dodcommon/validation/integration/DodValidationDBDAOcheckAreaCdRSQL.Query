<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodValidationDBDAOcheckAreaCdRSQL">
			<desc><![CDATA[Area Code 유효성 검증]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM MDM_EQ_ORZ_CHT
 WHERE ROWNUM = 1
 #if(${s_type} == 'RCC')
   AND RCC_CD = @[s_value]
 #elseif(${s_type} == 'LCC')
   AND LCC_CD = @[s_value]
 #elseif(${s_type} == 'ECC')
   AND ECC_CD = @[s_value]
 #elseif(${s_type} == 'SCC')
   AND SCC_CD = @[s_value]
 #else
   AND 1 = 2 -- DATA 검색 안되도록 조치
 #end

			]]></sql>
			<params>
				<param name="s_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
