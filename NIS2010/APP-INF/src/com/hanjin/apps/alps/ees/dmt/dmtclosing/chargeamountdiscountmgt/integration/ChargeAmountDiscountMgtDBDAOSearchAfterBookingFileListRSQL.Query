<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChargeAmountDiscountMgtDBDAOSearchAfterBookingFileListRSQL">
			<desc><![CDATA[ChargeAmountDiscountMgtDBDAOSearchAfterBookingFileList]]></desc>
			<sql><![CDATA[
SELECT  ( SELECT ATTR_CTNT3
            FROM DMT_HRD_CDG_CTNT 
           WHERE HRD_CDG_ID = 'AFT_BKG_SPEC_RSN_CD'
             AND ATTR_CTNT1 = T1.AFT_BKG_FILE_DIV_CD
             AND ATTR_CTNT2 = T1.AFT_BKG_RMK_LVL
             AND ATTR_CTNT4 = 'file'
		) AS AFT_BKG_RMK_LVL
       ,T1.FILE_SAV_ID
       ,NVL(T2.FILE_UPLD_NM,'')				AS FILE_NM
       ,T2.FILE_SZ_CAPA     				AS FILE_SIZE
       ,T2.FILE_PATH_URL    				AS FILE_PATH
       ,TO_CHAR(T2.UPD_DT, 'YYYY-MM-DD')   	AS UPD_DT
	   ,T1.AFT_BKG_FILE_DIV_CD
  FROM  DMT_AFT_BKG_FILE_RQST  T1 
       ,COM_UPLD_FILE           T2
 WHERE  1=1
   AND  T1.AFT_EXPT_DAR_NO = @[aft_expt_dar_no]
#if(${aft_bkg_file_div_cd} == 'All' )
   AND  T1.AFT_BKG_FILE_DIV_CD IN ( SELECT ATTR_CTNT3 FROM DMT_HRD_CDG_CTNT WHERE HRD_CDG_ID = 'AFT_BKG_RSN_CD' )
#else
   AND  T1.AFT_BKG_FILE_DIV_CD = @[aft_bkg_file_div_cd]
#end
   AND  T1.FILE_SAV_ID          = T2.FILE_SAV_ID(+)
   AND  T2.DELT_FLG(+)             = 'N'			]]></sql>
			<params>
				<param name="aft_expt_dar_no" type="12" value="" out="N"/>
				<param name="aft_bkg_file_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
