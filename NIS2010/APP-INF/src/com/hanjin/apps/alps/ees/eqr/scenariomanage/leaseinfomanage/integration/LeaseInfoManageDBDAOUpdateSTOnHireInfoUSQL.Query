	AVAL_CNTR_QTY	= @[aval_cntr_qty]
,	LFT_CHG_AMT		= @[lft_chg_amt]
,	DRYG_AMT		= @[dryg_amt]
,	PKUP_CHG_CR_AMT = @[pkup_chg_cr_amt]
,	PD_COST_AMT		= @[pd_cost_amt]
,	DFLT_USD_DYS	= @[dflt_usd_dys]
,	UPD_USR_ID		= @[upd_usr_id]
,	UPD_DT			= SYSDATE
WHERE	SCNR_ID		= @[scnr_id]
	AND	ECC_CD		= @[ecc_cd]
	AND	CNTR_TPSZ_CD= @[cntr_tpsz_cd]			]]></sql>
