<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodValidationDBDAOcheckIssueUserRSQL">
			<desc><![CDATA[Login한 사용자가 AR Invoice Issue화면에 접근권한이 있는지 확인한다]]></desc>
			<sql><![CDATA[
SELECT /*+ USE_HASH(BA BB BC BD) ORDERED */
--        DISTINCT BA.PGM_NO,
        COUNT(1) CNT
--       BA.PGM_NM,
--       BA.PGM_URL,
--       BA.PGM_LVL_DIV_CD,
--       BA.POPUP_FLG
  FROM COM_PROGRAM BA,
       COM_PGM_ROLE BB,
       COM_USR_ROLE BC,
       COM_USR_ROLE_MTCH BD
 WHERE BA.PGM_TP_CD = '00'
   AND BA.USE_FLG = 'Y'
   AND BA.POPUP_FLG = 'N'
   AND BA.PGM_MNU_DIV_CD = '02'
   AND BA.PGM_NO = BB.PGM_NO
   AND BB.USR_ROLE_CD = BC.USR_ROLE_CD
   AND BC.USR_ROLE_CD = BD.USR_ROLE_CD
   AND BD.USR_ID = @[s_value] -- Login UserId
   AND BA.PGM_NO ='FNS_INV_0034-01' -- AR Invoice Issue Program			]]></sql>
			<params>
				<param name="s_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
