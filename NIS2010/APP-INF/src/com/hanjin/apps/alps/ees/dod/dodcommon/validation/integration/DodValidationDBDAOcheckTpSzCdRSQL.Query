<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodValidationDBDAOcheckTpSzCdRSQL">
			<desc><![CDATA[Container Type Size 유효성 검증]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM MDM_CNTR_TP_SZ
 WHERE CNTR_TPSZ_CD = @[s_value]
   AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="s_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
