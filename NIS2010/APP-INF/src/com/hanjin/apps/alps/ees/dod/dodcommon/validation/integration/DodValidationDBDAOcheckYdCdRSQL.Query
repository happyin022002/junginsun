<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodValidationDBDAOcheckYdCdRSQL">
			<desc><![CDATA[Yard Code 유효성 체크]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM MDM_YARD
 WHERE YD_CD = @[s_value]
   AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="s_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
