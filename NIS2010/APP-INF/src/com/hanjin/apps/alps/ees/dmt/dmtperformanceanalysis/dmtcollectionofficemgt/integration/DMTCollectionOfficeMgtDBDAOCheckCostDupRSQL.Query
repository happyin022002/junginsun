<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCollectionOfficeMgtDBDAOCheckCostDupRSQL">
			<desc><![CDATA[YardExceptionCost 데이터 중복 check.]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX('Y'),'N') AS expt_cost
FROM DMT_YD_EXPT_COST 
WHERE YD_CD    = @[yd_cd]           
AND (( TO_CHAR(EFF_DT,'YYYYMMDD')  BETWEEN  REPLACE(@[eff_dt],'-','') 
                                 AND REPLACE(NVL(@[exp_dt],'99991231'),'-',''))
    OR ( NVL(TO_CHAR(EXP_DT,'YYYYMMDD'),'99991231') BETWEEN  REPLACE(@[eff_dt],'-','') 
                                 AND REPLACE(NVL(@[exp_dt],'99991231'),'-','')))			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="eff_dt" type="12" value="" out="N"/>
				<param name="exp_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
