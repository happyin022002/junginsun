<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SCExceptionTariffMgtDBDAOModifySCExceptionCoveragesUSQL">
			<desc><![CDATA[S/C Exception Terms Entry 의 등록된 Group 정보의 하위 Multi Coverage 정보를 수정하는 쿼리]]></desc>
			<sql><![CDATA[
UPDATE DMT_SC_EXPT_CVRG SET 
	CNT_CD = NVL(@[cnt_cd], ' ')
,	RGN_CD = NVL(@[rgn_cd], ' ')
,	STE_CD = NVL(@[ste_cd], ' ')
,	LOC_CD = NVL(@[loc_cd], ' ')
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[upd_ofc_cd]), SYSDATE)
WHERE	PROP_NO = @[prop_no]
AND	SC_EXPT_VER_SEQ = @[sc_expt_ver_seq]
AND	SC_EXPT_GRP_SEQ = @[sc_expt_grp_seq]
AND	CVRG_SEQ = @[cvrg_seq]			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_ofc_cd" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="sc_expt_ver_seq" type="12" value="" out="N"/>
				<param name="sc_expt_grp_seq" type="12" value="" out="N"/>
				<param name="cvrg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
