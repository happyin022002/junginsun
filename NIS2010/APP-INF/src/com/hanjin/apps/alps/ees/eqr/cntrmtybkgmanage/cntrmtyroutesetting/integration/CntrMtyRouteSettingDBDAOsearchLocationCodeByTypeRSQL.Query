<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrMtyRouteSettingDBDAOsearchLocationCodeByTypeRSQL">
			<desc><![CDATA[CntrMtyRouteSettingDBDAOsearchLocationCodeByType]]></desc>
			<sql><![CDATA[
WITH RCC_INFO AS 
(SELECT A.RCC_CD
   FROM MDM_EQ_ORZ_CHT A, MDM_LOCATION B
  WHERE A.SCC_CD = B.SCC_CD
    AND B.LOC_CD = @[loccd]
)
SELECT  COUNT(1) CNT 
 FROM RCC_INFO
#if(${rcccd} != 'ALL') 
 WHERE RCC_CD = @[rcccd]
#end			]]></sql>
			<params>
				<param name="loccd" type="12" value="" out="N"/>
				<param name="rcccd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
