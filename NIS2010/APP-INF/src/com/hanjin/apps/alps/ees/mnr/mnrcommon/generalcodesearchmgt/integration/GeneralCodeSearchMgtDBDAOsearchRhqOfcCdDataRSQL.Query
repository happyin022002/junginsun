<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeSearchMgtDBDAOsearchRhqOfcCdDataRSQL">
			<desc><![CDATA[searchRhqOfcCdData]]></desc>
			<sql><![CDATA[
SELECT B.OFC_CD AS CD_ID, B.OFC_ENG_NM AS CD_DESC
  FROM (
        SELECT DISTINCT AR_HD_QTR_OFC_CD AS HQ_OFC
          FROM MDM_ORGANIZATION
		#if(${searchcon} == 'TRUESELHOFALSE')
		  WHERE AR_HD_QTR_OFC_CD <> 'SELHO'
		 UNION ALL
        SELECT MNR_CD_ID AS HQ_OFC
          FROM MNR_GEN_CD
         WHERE PRNT_CD_ID = 'HOOFC'
           AND ROWNUM = 1   
	    #elseif (${searchcon} == 'ALLFALSE') 
		  WHERE AR_HD_QTR_OFC_CD <> 'SELHO'
        #elseif (${searchcon} == 'TRUE')
        UNION ALL 
        SELECT MNR_CD_ID AS HQ_OFC
          FROM MNR_GEN_CD
         WHERE PRNT_CD_ID = 'HOOFC'
           AND ROWNUM = 1
        #elseif (${searchcon} == 'ASIA')
          WHERE AR_HD_QTR_OFC_CD IN ('SHARC','SINRS','SELIB','TYOIB')
        #end      
       ) A , MDM_ORGANIZATION B
 WHERE B.OFC_CD = A.HQ_OFC
   AND B.DELT_FLG = 'N'
#if(${rhqplus} == 'onsiteinspection')
   AND B.OFC_CD NOT IN ('SELIB','SELHO', 'VVOIA')
UNION ALL
SELECT B.OFC_CD||'(SELSC)' AS CD_ID,
       B.OFC_ENG_NM||'(KOREA BRANCH OFFICE)' AS CD_DESC
  FROM MDM_ORGANIZATION B
 WHERE B.OFC_CD = 'SELIB'
   AND B.DELT_FLG != 'Y'
 ORDER BY CD_ID 
#else
 ORDER BY B.OFC_CD
#end			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
