<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralCodeCheckMgtDBDAOcheckYardCodeDataRSQL">
			<desc><![CDATA[야드정보체크를 위해 데이타를 가져옴]]></desc>
			<sql><![CDATA[
SELECT A.YD_CD as CD_ID,A.YD_NM as CD_DESC 
FROM MDM_YARD A
WHERE A.DELT_FLG = 'N'
AND A.YD_CD = @[check_value]
AND ROWNUM = 1

			]]></sql>
			<params>
				<param name="check_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
