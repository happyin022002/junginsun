VALUES (
  @[edi_id],
  @[msg_id],
  @[muid_area],
  @[muid_dt],
  @[muid_seq],
  @[term_id],
  @[cntr_number],
  @[event_yard],
  TO_DATE (@[event_date], 'YYYYMMDDHH24MISS'),
  @[gate_io],
  @[cont_stat],
  @[chss_code],
  @[vessel],
  @[voyage],
  @[dir],
  @[bl_no],
  @[bkg_count],
  @[bkg_number0],
  @[pol],
  @[pod],
  @[dest_loc],
  @[seal_no],
  @[vndr_seq],
  @[trans_mode],
  @[flat_car_nbr],
  @[mvmt_status],
  @[hanger_tag],
  @[way_bill_no],
  @[pickup_no],
  @[result_indicator],
  REPLACE (@[result_message], '^#^', CHR(39)), /* 2010 @F_DB_CHECK Return Value */
  0,
  @[sight_cd],
  @[dmg_flag],
  @[call_sign_no],
  @[lloyd_no],
  @[mvmt_edi_sts_tp_flg],
  SUBSTR (@[event_yard], 1, 5),
  'SML',
  @[ffile_rref_no],
  @[user_id],
  @[user_id],
  SYSDATE,
  GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', SYSDATE, DECODE (@[event_yard], '', DECODE (@[muid_area], 'USA', 'USNYC', 'KOR', 'KRSEL', 'CHN', 'CHSHA', 'SWA', 'SGSIN', 'EUR', 'DEHAM', ''), SUBSTR(@[event_yard], 1, 5))),
  SYSDATE,
  GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', SYSDATE, DECODE (@[event_yard], '', DECODE (@[muid_area], 'USA', 'USNYC', 'KOR', 'KRSEL', 'CHN', 'CHSHA', 'SWA', 'SGSIN', 'EUR', 'DEHAM', ''), SUBSTR(@[event_yard], 1, 5))),
  SUBSTR (TO_CHAR (GLOBALDATE_PKG.TIME_CONV_FNC ('KRPUS', SYSDATE, DECODE (@[event_yard], '', DECODE (@[muid_area], 'USA', 'USNYC', 'KOR', 'KRSEL', 'CHN', 'CHSHA', 'SWA', 'SGSIN', 'EUR', 'DEHAM', ''), SUBSTR(@[event_yard], 1, 5))), 'YYYYMMDDHH24MISS'), 1, 8),
  SUBSTR (@[event_date], 1, 8),
  @[cntr_tpsz_cd],
  @[edi_bkg_no],
  @[stowage],
  @[vgm_mzd_tp_cd],
  @[vgm_wgt_ut_cd],
  TO_NUMBER(@[vgm_wgt_qty]),
  TO_DATE(@[vgm_vrfy_dt], 'YYYYMMDDHH24MI'),
  @[vgm_sig_ctnt],
  @[vgm_ref_no],
  @[vgm_wgt_pty_ctnt],
  @[vgm_vrfy_ord_ctnt]
)
			]]></sql>
