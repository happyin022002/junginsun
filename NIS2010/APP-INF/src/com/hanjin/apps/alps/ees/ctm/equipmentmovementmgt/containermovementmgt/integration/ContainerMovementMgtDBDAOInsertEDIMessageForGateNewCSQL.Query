  EDI_GATE_IO_CD,
  CNTR_FULL_STS_CD,
  CHSS_NO,
  CRNT_VSL_CD,
  CRNT_SKD_VOY_NO,
  CRNT_SKD_DIR_CD,
  EDI_BL_NO,
  BKG_KNT,
  BKG_NO,
  BKG_POL_CD,
  BKG_POD_CD,
  DEST_YD_CD,
  CNTR_SEAL_NO,
  VNDR_SEQ,
  MVMT_TRSP_MOD_CD,
