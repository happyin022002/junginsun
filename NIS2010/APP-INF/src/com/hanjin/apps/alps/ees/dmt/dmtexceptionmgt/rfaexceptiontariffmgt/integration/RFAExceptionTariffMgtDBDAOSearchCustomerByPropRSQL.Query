<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOSearchCustomerByPropRSQL">
			<desc><![CDATA[Proposal No. 에 해당되는 Customer 정보를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	
	A.CTRT_CUST_CNT_CD || A.CTRT_CUST_SEQ CUST_CD
, 	B.CUST_LGL_ENG_NM CUST_NM

FROM	PRI_RP_NM A, MDM_CUSTOMER B
WHERE	A.PROP_NO = @[prop_no]
	AND A.AMDT_SEQ = @[amdt_seq]
	AND A.CTRT_CUST_CNT_CD = B.CUST_CNT_CD
	AND A.CTRT_CUST_SEQ = B.CUST_SEQ			]]></sql>
			<params>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="amdt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
