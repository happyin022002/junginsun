<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAODualTypeCountryRSQL">
			<desc><![CDATA[Dual 인 Country 정보 조회를 위한 쿼리]]></desc>
			<sql><![CDATA[
SELECT	DISTINCT a.cnt_cd, b.cnt_nm
FROM	dmt_calc_tp a, mdm_country b 
WHERE	a.dmdt_calc_tp_cd = 'D'
	AND a.cnt_cd = b.cnt_cd			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
