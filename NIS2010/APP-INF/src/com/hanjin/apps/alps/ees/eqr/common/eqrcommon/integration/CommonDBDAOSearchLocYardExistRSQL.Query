<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLocYardExistRSQL">
			<desc><![CDATA[LOC Yard 가 사용해도 되는것인지 확인]]></desc>
			<sql><![CDATA[
SELECT 
	COUNT(1) YD_CD							
FROM
	MDM_YARD A, 								
    MDM_LOCATION B   			                
WHERE 
	A.LOC_CD = B.SCC_CD 						
	AND   A.DELT_FLG <> 'Y'                        
	AND   A.YD_CD = @[locyard]                              

#if(${ecc} != '')
	AND   A.LOC_CD IN ( 							
					      SELECT SCC_CD 			
					      FROM MDM_EQ_ORZ_CHT 		
					      WHERE ECC_CD = @[ecc] 			
				       ) 							 
#end

ORDER BY A.YD_CD		            						]]></sql>
			<params>
				<param name="locyard" type="12" value="" out="N"/>
				<param name="ecc" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
