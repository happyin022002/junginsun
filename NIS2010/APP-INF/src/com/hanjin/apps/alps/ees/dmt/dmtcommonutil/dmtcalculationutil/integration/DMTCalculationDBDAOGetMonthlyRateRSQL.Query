<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCalculationDBDAOGetMonthlyRateRSQL">
			<desc><![CDATA[GetMonthlyRate]]></desc>
			<sql><![CDATA[
SELECT	USD_LOCL_XCH_RT	acct_rate					
  FROM	GL_MON_XCH_RT
 WHERE	ACCT_XCH_RT_YRMON		=	TO_CHAR(SYSDATE,'YYYYMM')
   AND	ACCT_XCH_RT_LVL			=	'1'
   AND	CURR_CD					=	@[cur_cd]



			]]></sql>
			<params>
				<param name="cur_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
