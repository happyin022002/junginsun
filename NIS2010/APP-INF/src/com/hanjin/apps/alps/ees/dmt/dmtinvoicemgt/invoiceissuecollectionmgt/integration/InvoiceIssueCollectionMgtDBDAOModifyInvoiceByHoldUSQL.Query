<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueCollectionMgtDBDAOModifyInvoiceByHoldUSQL">
			<desc><![CDATA[INVOICE 생성 및 징수관리
EES_DMT_5101
Hold Reason Entry]]></desc>
			<sql><![CDATA[
UPDATE  DMT_INV_MN
SET     DMDT_AR_IF_CD  = 'H',
        AR_IF_DT       = GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usof]),
        AR_IF_USR_ID   = @[usid],
        AR_IF_OFC_CD   = @[usof],
        INV_HLD_RSN_CD = @[holdReasn],
        INV_HLD_RMK    = @[holdRemrk],
		INV_HLD_USR_ID = @[inv_hld_usr_id],
 		INV_HLD_DT     = SYSDATE,
		INV_HLD_OFC_CD = @[inv_hld_ofc_cd]
WHERE   DMDT_INV_NO    = @[invoiceNo]			]]></sql>
			<params>
				<param name="usof" type="12" value="" out="N"/>
				<param name="usid" type="12" value="" out="N"/>
				<param name="holdReasn" type="12" value="" out="N"/>
				<param name="holdRemrk" type="12" value="" out="N"/>
				<param name="inv_hld_usr_id" type="12" value="" out="N"/>
				<param name="inv_hld_ofc_cd" type="12" value="" out="N"/>
				<param name="invoiceNo" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
