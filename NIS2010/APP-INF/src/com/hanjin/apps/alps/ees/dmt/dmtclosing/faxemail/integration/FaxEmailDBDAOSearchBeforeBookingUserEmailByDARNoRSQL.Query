<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="FaxEmailDBDAOSearchBeforeBookingUserEmailByDARNoRSQL">
			<desc><![CDATA[Before Booking Request 한 사용자의 메일정보를 조회하는 쿼리]]></desc>
			<sql><![CDATA[
SELECT	COM_USER.USR_EML
FROM	DMT_RFA_EXPT_TRF_PROG RFA_PROG
	,	COM_USER
WHERE	RFA_PROG.RFA_EXPT_DAR_NO = @[rfa_expt_dar_no]
	AND	RFA_PROG.RFA_EXPT_MAPG_SEQ = @[rfa_expt_mapg_seq]
	AND RFA_PROG.RFA_EXPT_VER_SEQ = @[rfa_expt_ver_seq]
	AND RFA_PROG.PROG_SEQ =
		(
			SELECT	/*+ INDEX_DESC(DMT_RFA_EXPT_TRF_PROG, XPKDMT_RFA_EXPT_TRF_PROG)  */ PROG_SEQ
			FROM	DMT_RFA_EXPT_TRF_PROG
			WHERE	RFA_EXPT_DAR_NO = RFA_PROG.RFA_EXPT_DAR_NO
				AND	RFA_EXPT_MAPG_SEQ = RFA_PROG.RFA_EXPT_MAPG_SEQ
				AND	RFA_EXPT_VER_SEQ = RFA_PROG.RFA_EXPT_VER_SEQ
				AND DMDT_EXPT_RQST_STS_CD = 'R'
				AND ROWNUM = 1
		)
	AND RFA_PROG.PROG_USR_ID = COM_USER.USR_ID			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
