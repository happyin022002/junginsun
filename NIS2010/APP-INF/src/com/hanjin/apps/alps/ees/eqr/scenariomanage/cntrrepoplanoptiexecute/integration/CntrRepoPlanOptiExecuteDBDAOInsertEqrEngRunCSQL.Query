				<param name="run_id_no" type="12" value="" out="N"/>
				<param name="st_dt" type="12" value="" out="N"/>
				<param name="dur_wks" type="12" value="" out="N"/>
				<param name="onh_flg" type="12" value="" out="N"/>
				<param name="offh_flg" type="12" value="" out="N"/>
				<param name="sold_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
