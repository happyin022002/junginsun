            (  @[run_id_no]
              ,@[st_dt]
              ,@[dur_wks]
              ,@[onh_flg]
              ,@[offh_flg]
              ,@[sold_flg]
              ,@[cre_usr_id]
              ,SYSDATE
              ,@[upd_usr_id]
              ,SYSDATE
              ,'TEST'
              ,'N'
              ,'N'
              , 0
              ,'CREATED'
            )			]]></sql>
