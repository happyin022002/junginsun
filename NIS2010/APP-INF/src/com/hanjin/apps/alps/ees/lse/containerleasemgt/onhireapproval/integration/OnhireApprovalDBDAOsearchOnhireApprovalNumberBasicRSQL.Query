<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OnhireApprovalDBDAOsearchOnhireApprovalNumberBasicRSQL">
			<desc><![CDATA[OnhireApprovalNumberBasic search List]]></desc>
			<sql><![CDATA[
SELECT
   DISTINCT A.CNTR_ONH_AUTH_NO CNTR_ONH_AUTH_NO
  ,A.ONH_LOC_CD
  ,A.PKUP_DUE_DT
  ,A.CRE_DT
FROM LSE_ONH_APRO A
WHERE A.ONH_LOC_CD = @[loc_cd]
AND   A.LSTM_CD    = @[ls_tm_cd]
AND   A.DELT_FLG   = 'N'
#if (${period_eddt} != "" )
AND   SUBSTR(A.CNTR_ONH_AUTH_NO,6,6) BETWEEN  @[period_stdt] AND  @[period_eddt]
#end
ORDER BY  A.CNTR_ONH_AUTH_NO DESC			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="ls_tm_cd" type="12" value="" out="N"/>
				<param name="period_stdt" type="12" value="" out="N"/>
				<param name="period_eddt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
