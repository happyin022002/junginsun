<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOMergeYearDomesticPlanCSQL">
			<desc><![CDATA[Mergeë¬¸]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_DMST_PLN A                            
USING                                                
    (
        SELECT 	  
            @[pln_yrwk] 		PLN_YRWK                          
			, @[fm_ecc_cd] 		FM_ECC_CD                           
		    , @[to_ecc_cd]  	TO_ECC_CD                           
            , @[cntr_tpsz_cd]  	CNTR_TPSZ_CD                          
 	    FROM  
 	        DUAL          
    ) B             
ON                                                 
    (  
        A.PLN_YRWK     = B.PLN_YRWK            
      	AND A.FM_ECC_CD    = B.FM_ECC_CD           
       	AND A.TO_ECC_CD    = B.TO_ECC_CD             
       	AND A.CNTR_TPSZ_CD = B.CNTR_TPSZ_CD          
    )                                                
WHEN MATCHED THEN                                     	
    UPDATE SET 	  
        A.CNTR_VOL_QTY = @[cntr_vol_qty]                 	
		, A.UPD_USR_ID   = @[upd_usr_id]                  	
		, A.UPD_DT       = SYSDATE            			    		            
WHEN NOT MATCHED THEN                                	
    INSERT(		
        PLN_YRWK
        , FM_ECC_CD
        , TO_ECC_CD
        , CNTR_TPSZ_CD
        , CNTR_VOL_QTY
        , CRE_USR_ID
        , CRE_DT
        , UPD_USR_ID
        , UPD_DT
    ) VALUES ( 
        @[pln_yrwk]
        ,@[fm_ecc_cd]
        ,@[to_ecc_cd]
        ,@[cntr_tpsz_cd]
        ,@[cntr_vol_qty]
        ,@[cre_usr_id] 
        ,SYSDATE
        ,@[upd_usr_id]
        , SYSDATE
    )			]]></sql>
			<params>
				<param name="pln_yrwk" type="12" value="" out="N"/>
				<param name="fm_ecc_cd" type="12" value="" out="N"/>
				<param name="to_ecc_cd" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_vol_qty" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
