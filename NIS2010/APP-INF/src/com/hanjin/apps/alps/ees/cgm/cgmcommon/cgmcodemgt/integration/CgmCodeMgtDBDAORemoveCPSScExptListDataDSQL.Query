<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAORemoveCPSScExptListDataDSQL">
			<desc><![CDATA[S/C Exception List를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE 
FROM CGM_CPS_SC_EXPT_LIST
WHERE EFF_YRMON = @[eff_yrmon]
	  AND SC_NO = @[sc_no]
	  AND LOC_CD = @[loc_cd]			]]></sql>
			<params>
				<param name="eff_yrmon" type="12" value="" out="N"/>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
