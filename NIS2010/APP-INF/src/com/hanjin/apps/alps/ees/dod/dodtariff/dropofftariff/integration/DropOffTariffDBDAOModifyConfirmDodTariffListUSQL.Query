<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DropOffTariffDBDAOModifyConfirmDodTariffListUSQL">
			<desc><![CDATA[DOD Tariff 를 confirm 한다.]]></desc>
			<sql><![CDATA[
UPDATE DOD_DRP_OFF_CHG_TRF
SET DRP_OFF_CHG_TRF_CFM_USR_ID = @[drp_off_chg_trf_cfm_usr_id]
	,DRP_OFF_CHG_TRF_CFM_FLG = 'Y'
	,DRP_OFF_CHG_TRF_CFM_DT = sysdate
	,DRP_OFF_CHG_TRF_RMK = @[drp_off_chg_trf_rmk]
WHERE DRP_OFF_CHG_TRF_SEQ = @[drp_off_chg_trf_seq]			]]></sql>
			<params>
				<param name="drp_off_chg_trf_cfm_usr_id" type="12" value="" out="N"/>
				<param name="drp_off_chg_trf_rmk" type="12" value="" out="N"/>
				<param name="drp_off_chg_trf_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
