<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAOSearchAutoRunParameterRSQL">
			<desc><![CDATA[Demand forcate]]></desc>
			<sql><![CDATA[
SELECT A.EFF_ST_YRWK AS EFF_ST_YRWK
       ,RCC_CD
       , CASE WHEN A.CO_CD = 'H' THEN 'SML'  ELSE 'SEN' END AS CO_NM
       , CO_CD
       , REPO_FCAST_TP_CD
       , CASE WHEN A.REPO_FCAST_TP_CD = 'SV'  THEN '1' END AS SALES
       , CASE WHEN A.REPO_FCAST_TP_CD = 'SD'  THEN '1' END AS STATISTICAL
       , CASE WHEN A.REPO_FCAST_TP_CD = 'AV'  THEN '1' END AS AVERAGE
       , A.UPD_USR_ID
       , TO_CHAR(A.UPD_DT,'YYYYMMDD') UPD_DT							
 FROM  EQR_AUTO_RUN_FCAST_PARA A, COM_INTG_CD_DTL C							
 WHERE  A.EFF_ST_YRWK = ( 													
 			SELECT MAX(EFF_ST_YRWK) 											
 			FROM EQR_AUTO_RUN_FCAST_PARA										
 			WHERE  EFF_END_YRWK IS NULL )										
 AND  A.EFF_END_YRWK IS NULL 								             		
 AND  C.INTG_CD_ID = 'CD00255'			              									
 AND  A.RCC_CD = C.INTG_CD_VAL_CTNT              								
 ORDER  BY INTG_CD_VAL_DP_SEQ 
           , CO_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
