<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DodValidationDBDAOcheckBkgNoRSQL">
			<desc><![CDATA[Bkg NO 유효성 체크]]></desc>
			<sql><![CDATA[
SELECT COUNT(1) CNT
  FROM BKG_BOOKING
 WHERE BKG_NO = @[s_value]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="s_value" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
