<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOPayableChargeAuditRejectDetailUSQL">
			<desc><![CDATA[Charge Creation Detail Data 를 Audit 이전 상태로 Update.]]></desc>
			<sql><![CDATA[
UPDATE LSE_PAY_RNTL_CHG_DTL
SET    CNTR_AUD_STS_CD = 'H'
     , DSCR_RT_AMT      = 0.0
     , DSCR_COST_AMT    = 0.0
     , DSCR_ONH_DT      = NULL
     , DSCR_OFFH_DT     = NULL
     , DSCR_ONH_LOC_CD  = NULL
     , DSCR_OFFH_LOC_CD = NULL
     , UPD_USR_ID       = @[usr_id]
     , UPD_DT           = SYSDATE
WHERE  AGMT_CTY_CD = @[agmt_cty_cd]
AND    AGMT_SEQ    = @[agmt_seq]
AND    CHG_SEQ     = @[chg_seq]			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
				<param name="chg_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
