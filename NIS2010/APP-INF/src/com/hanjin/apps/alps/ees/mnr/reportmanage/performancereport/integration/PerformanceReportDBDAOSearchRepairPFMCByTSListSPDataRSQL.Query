AND   OH.VNDR_SEQ   = TO_NUMBER(@[vndr_seq])
#end
#if ((${manu_yr_fr} != '' && ${manu_yr_to} != '') || ${lstm_cd} != 'A' || ${mkr_nm} != 'A') 
