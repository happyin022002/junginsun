	)
#end
#if (${rhq} != 'A') 
AND   MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(OH.COST_OFC_CD)  = @[rhq]
#end
#if (${ofc_cd} != 'A') 
AND   OH.COST_OFC_CD = @[ofc_cd]
#end
