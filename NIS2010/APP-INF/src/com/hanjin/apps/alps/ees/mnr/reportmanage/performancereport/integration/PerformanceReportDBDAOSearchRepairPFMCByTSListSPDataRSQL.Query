GROUP BY OH.COST_OFC_CD, OH.VNDR_SEQ, C.DCOL
ORDER BY MAX(MNR_COMMON_PKG.MNR_GET_RHQ_OFC_FNC(OH.COST_OFC_CD)), OH.COST_OFC_CD, OH.VNDR_SEQ, C.DCOL
)
SELECT
    RSV.RHQ, RSV.OFC_CD, MNR_COMMON_PKG.MNR_CONV_PARTNER_CD_FNC(RSV.SP_CD) AS SP_CD, RSV.SP_NM, RSV.CURR_CD, RSV.DCOL,
	DECODE(RSV.RHQ,'AAA', NVL(P.TS01,'N') || '|' || NVL(P.TS02,'N') || '|' ||
	NVL(P.TS03,'N') || '|' || NVL(P.TS04,'N') || '|' ||
	NVL(P.TS05,'N') || '|' || NVL(P.TS06,'N') || '|' ||
	NVL(P.TS07,'N') || '|' || NVL(P.TS08,'N') || '|' ||
