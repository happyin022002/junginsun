<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrPlanGuidelineManageDBDAOCheckMaxEqGlineSeqRSQL">
			<desc><![CDATA[Guideline Amend시 마치막 차수인지 확인]]></desc>
			<sql><![CDATA[
SELECT MAX(EQ_GLINE_SEQ) MAX_GLINE_SEQ
  FROM EQR_CTRL_GLINE_HDR
 WHERE TRD_CD 		= @[trade]
   AND SUB_TRD_CD 	= @[subtrade]
   AND VSL_LANE_CD 	= @[lane]
   AND CFM_FLG      = 'Y'			]]></sql>
			<params>
				<param name="trade" type="12" value="" out="N"/>
				<param name="subtrade" type="12" value="" out="N"/>
				<param name="lane" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
