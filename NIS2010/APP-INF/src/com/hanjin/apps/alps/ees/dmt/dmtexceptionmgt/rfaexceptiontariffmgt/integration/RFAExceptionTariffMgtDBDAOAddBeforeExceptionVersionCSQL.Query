<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RFAExceptionTariffMgtDBDAOAddBeforeExceptionVersionCSQL">
			<desc><![CDATA[RFA Exception Tariff Version 정보를 입력하기 위한 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO DMT_RFA_EXPT_TRF (
	RFA_EXPT_DAR_NO
,	RFA_EXPT_MAPG_SEQ
,	RFA_EXPT_VER_SEQ
,	PROP_NO
,	DMDT_EXPT_RQST_STS_CD
,	BKG_CUST_TP_CD
,	CUST_CNT_CD
,	CUST_SEQ
,	RQST_USR_ID
,	RQST_OFC_CD
,	RQST_DT
,	RFA_EXPT_APRO_NO
,	APRO_OFC_CD
,	MSG_DT
,	CRE_USR_ID
,	CRE_DT
,	CRE_OFC_CD
,	UPD_USR_ID
,	UPD_DT
,	UPD_OFC_CD
) VALUES( 
	@[rfa_expt_dar_no]
,	@[rfa_expt_mapg_seq]
,	@[rfa_expt_ver_seq]
,	@[prop_no]
,	@[dmdt_expt_rqst_sts_cd]
,	@[bkg_cust_tp_cd]
,	@[cust_cnt_cd]
,	@[cust_seq]
,	@[rqst_usr_id]
,	@[rqst_ofc_cd]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[rfa_expt_apro_no]
,	@[apro_ofc_cd]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[cre_ofc_cd]
,	@[cre_usr_id]
,	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[cre_ofc_cd]), SYSDATE)
,	@[cre_ofc_cd]
)			]]></sql>
			<params>
				<param name="rfa_expt_dar_no" type="12" value="" out="N"/>
				<param name="rfa_expt_mapg_seq" type="12" value="" out="N"/>
				<param name="rfa_expt_ver_seq" type="12" value="" out="N"/>
				<param name="prop_no" type="12" value="" out="N"/>
				<param name="dmdt_expt_rqst_sts_cd" type="12" value="" out="N"/>
				<param name="bkg_cust_tp_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_ofc_cd" type="12" value="" out="N"/>
				<param name="rfa_expt_apro_no" type="12" value="" out="N"/>
				<param name="apro_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
