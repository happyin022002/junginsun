<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOSearchCHSInventoryGeneralDataRSQL">
			<desc><![CDATA[ChassisMgsetInventoryDB.SearchCHSInventoryGeneralData]]></desc>
			<sql><![CDATA[
SELECT
#if (${group1} == '1') 
	C.LCC_CD AS GROUP1,
#elseif (${group1} == '2') 
	A.ONH_OFC_CD AS GROUP1,
#elseif (${group1} == '3') 
	B.SCC_CD AS GROUP1,
#elseif (${group1} == '4') 
	A.CRNT_YD_CD AS GROUP1,
#elseif (${group1} == '5') 
	A.AGMT_LSTM_CD AS GROUP1,
	D.INTG_CD_VAL_DP_SEQ,
#elseif (${group1} == '6') 
	F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')' AS GROUP1,
#elseif (${group1} == '7') 
	A.CHSS_MVMT_STS_CD AS GROUP1,
	E.INTG_CD_VAL_DP_SEQ,
#end
#if (${group2} == '1') 
	C.LCC_CD AS GROUP2,
#elseif (${group2} == '2') 
	A.ONH_OFC_CD AS GROUP2,
#elseif (${group2} == '3') 
	B.SCC_CD AS GROUP2,
#elseif (${group2} == '4') 
	A.CRNT_YD_CD AS GROUP2,
#elseif (${group2} == '5') 
	A.AGMT_LSTM_CD AS GROUP2,
	D.INTG_CD_VAL_DP_SEQ,
#elseif (${group2} == '6') 
	F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')' AS GROUP2,
#elseif (${group2} == '7') 
	A.CHSS_MVMT_STS_CD AS GROUP2,
	E.INTG_CD_VAL_DP_SEQ,
#end
#if (${group3} == '1') 
	C.LCC_CD AS GROUP3,
#elseif (${group3} == '2') 
	A.ONH_OFC_CD AS GROUP3,
#elseif (${group3} == '3') 
	B.SCC_CD AS GROUP3,
#elseif (${group3} == '4') 
	A.CRNT_YD_CD AS GROUP3,
#elseif (${group3} == '5') 
	A.AGMT_LSTM_CD AS GROUP3,
	D.INTG_CD_VAL_DP_SEQ,
#elseif (${group3} == '6') 
	F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')' AS GROUP3,
#elseif (${group3} == '7') 
	A.CHSS_MVMT_STS_CD AS GROUP3,
	E.INTG_CD_VAL_DP_SEQ,
#end
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'SF2' THEN 1 END),0) AS EQ_TPSZ_CD_SF2,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'SL2' THEN 1 END),0) AS EQ_TPSZ_CD_SL2,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'TA2' THEN 1 END),0) AS EQ_TPSZ_CD_TA2,  
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'SF4' THEN 1 END),0) AS EQ_TPSZ_CD_SF4,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'GN4' THEN 1 END),0) AS EQ_TPSZ_CD_GN4,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'CB4' THEN 1 END),0) AS EQ_TPSZ_CD_CB4,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'EG5' THEN 1 END),0) AS EQ_TPSZ_CD_EG5,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'GN5' THEN 1 END),0) AS EQ_TPSZ_CD_GN5,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'EG8' THEN 1 END),0) AS EQ_TPSZ_CD_EG8,
    NVL(SUM(CASE WHEN A.EQ_TPSZ_CD = 'ZT4' THEN 1 END),0) AS EQ_TPSZ_CD_ZT4      
FROM 
    CGM_EQUIPMENT A,
    MDM_LOCATION B,
    MDM_EQ_ORZ_CHT C,
    COM_INTG_CD_DTL D,
    COM_INTG_CD_DTL E,
	MDM_VENDOR F
WHERE 
	A.VNDR_SEQ = F.VNDR_SEQ(+)
    AND A.CRNT_LOC_CD = B.LOC_CD
    AND B.SCC_CD = C.SCC_CD
    AND B.DELT_FLG = 'N'
    AND C.DELT_FLG = 'N'
	AND A.EQ_KND_CD = @[eq_knd_cd]
#if (${location} == 'RCC')
	AND C.RCC_CD = @[crnt_loc_cd]
#elseif (${location} == 'LCC')
	AND C.LCC_CD = @[crnt_loc_cd]
#elseif (${location} == 'SCC')
	AND C.SCC_CD = @[crnt_loc_cd]
#end
    AND A.AGMT_LSTM_CD = D.INTG_CD_VAL_CTNT (+)
    AND 'CD01948' = D.INTG_CD_ID (+)
    
    AND A.CHSS_MVMT_STS_CD = E.INTG_CD_VAL_CTNT (+)
    AND 'CD02386' = E.INTG_CD_ID (+)
    
#if (${crnt_yd_cd} != '')
	AND A.CRNT_YD_CD IN ($crnt_yd_cd)
#end
#if (${aciac_div_cd} != '')
	AND A.ACIAC_DIV_CD = @[aciac_div_cd]
#end
#if (${chss_pool_cd} != 'I')
	#if (${chss_pool_cd} == 'E')
		AND A.CHSS_POOL_CD IS NULL
	#elseif (${chss_pool_cd} == 'O')
		AND A.CHSS_POOL_CD IS NOT NULL
	#else
		AND A.CHSS_POOL_CD = @[chss_pool_cd]
	#end
#end
#if (${include_np} != 'Y')
	AND A.AGMT_LSTM_CD <>'NP'
#end
#if (${staying_days} != '')
	AND A.CHSS_MVMT_DT < SYSDATE - TO_NUMBER(${staying_days}) 
#end
#if (${eq_tpsz_cd} != '')
	AND A.EQ_TPSZ_CD IN ($eq_tpsz_cd)
#end
#if (${agmt_lstm_cd} != '')
	AND A.AGMT_LSTM_CD IN ($agmt_lstm_cd)
#end
#if (${vndr_seq} != '')
	AND A.VNDR_SEQ IN ($vndr_seq)
#end
#if (${chss_mvmt_sts_cd} != '')
	AND A.CHSS_MVMT_STS_CD IN ($chss_mvmt_sts_cd)
#end
#if (${group1} != '') 
GROUP BY
#end
#if (${group1} == '1') 
	C.LCC_CD
#elseif (${group1} == '2') 
	A.ONH_OFC_CD
#elseif (${group1} == '3') 
	B.SCC_CD
#elseif (${group1} == '4') 
	A.CRNT_YD_CD
#elseif (${group1} == '5') 
	A.AGMT_LSTM_CD
	,D.INTG_CD_VAL_DP_SEQ
#elseif (${group1} == '6') 
	F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')'
#elseif (${group1} == '7') 
	A.CHSS_MVMT_STS_CD
	,E.INTG_CD_VAL_DP_SEQ
#end
#if (${group2} == '1') 
	, C.LCC_CD
#elseif (${group2} == '2') 
	, A.ONH_OFC_CD
#elseif (${group2} == '3') 
	, B.SCC_CD
#elseif (${group2} == '4') 
	, A.CRNT_YD_CD
#elseif (${group2} == '5') 
	, A.AGMT_LSTM_CD
	, D.INTG_CD_VAL_DP_SEQ
#elseif (${group2} == '6') 
	, F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')'
#elseif (${group2} == '7') 
	, A.CHSS_MVMT_STS_CD
	, E.INTG_CD_VAL_DP_SEQ
#end
#if (${group3} == '1') 
	, C.LCC_CD
#elseif (${group3} == '2') 
	, A.ONH_OFC_CD
#elseif (${group3} == '3') 
	, B.SCC_CD
#elseif (${group3} == '4') 
	, A.CRNT_YD_CD
#elseif (${group3} == '5') 
	, A.AGMT_LSTM_CD 
	, D.INTG_CD_VAL_DP_SEQ
#elseif (${group3} == '6') 
	, F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')'
#elseif (${group3} == '7') 
	, A.CHSS_MVMT_STS_CD
	, E.INTG_CD_VAL_DP_SEQ
#end

#if (${group1} != '') 
ORDER BY
#end
#if (${group1} == '1') 
	C.LCC_CD
#elseif (${group1} == '2') 
	A.ONH_OFC_CD
#elseif (${group1} == '3') 
	B.SCC_CD
#elseif (${group1} == '4') 
	A.CRNT_YD_CD
#elseif (${group1} == '5') 
	--A.AGMT_LSTM_CD
	D.INTG_CD_VAL_DP_SEQ
#elseif (${group1} == '6') 
	F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')'
#elseif (${group1} == '7') 
	--A.CHSS_MVMT_STS_CD
	E.INTG_CD_VAL_DP_SEQ
#end
#if (${group2} == '1') 
	, C.LCC_CD
#elseif (${group2} == '2') 
	, A.ONH_OFC_CD
#elseif (${group2} == '3') 
	, B.SCC_CD
#elseif (${group2} == '4') 
	, A.CRNT_YD_CD
#elseif (${group2} == '5') 
	--, A.AGMT_LSTM_CD
	, D.INTG_CD_VAL_DP_SEQ
#elseif (${group2} == '6') 
	, F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')'
#elseif (${group2} == '7') 
	--, A.CHSS_MVMT_STS_CD
	,E.INTG_CD_VAL_DP_SEQ
#end
#if (${group3} == '1') 
	, C.LCC_CD
#elseif (${group3} == '2') 
	, A.ONH_OFC_CD
#elseif (${group3} == '3') 
	, B.SCC_CD
#elseif (${group3} == '4') 
	, A.CRNT_YD_CD
#elseif (${group3} == '5') 
	--, A.AGMT_LSTM_CD
	, D.INTG_CD_VAL_DP_SEQ
#elseif (${group3} == '6') 
	, F.VNDR_ABBR_NM||' ('||A.VNDR_SEQ||')'
#elseif (${group3} == '7') 
	--, A.CHSS_MVMT_STS_CD
	,E.INTG_CD_VAL_DP_SEQ
#end			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="crnt_loc_cd" type="12" value="" out="N"/>
				<param name="aciac_div_cd" type="12" value="" out="N"/>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
