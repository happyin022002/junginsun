<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepairMgtDBDAOmanageEstimateAuditItLaterDataUSQL">
			<desc><![CDATA[manageEstimateAuditItLaterData]]></desc>
			<sql><![CDATA[
UPDATE   MNR_RPR_RQST_HDR A
SET      A.RPR_OFFH_FLG = @[rpr_offh_flg]
       , A.N3PTY_FLG = @[n3pty_flg]
       , A.RPR_YD_CD = @[rpr_yd_cd]
       , A.RPR_WRK_TP_CD = @[rpr_wrk_tp_cd]
       , A.EQ_DMG_TP_CD = 'Y'
       , A.MNR_RPR_RMK = @[mnr_rpr_rmk]
       , A.UPD_USR_ID = @[upd_usr_id]
       , A.UPD_DT = SYSDATE
WHERE    1 = 1
AND      A.RQST_EQ_NO = @[rqst_eq_no]
AND      A.RPR_RQST_SEQ = @[rpr_rqst_seq]
AND      A.RPR_RQST_VER_NO = @[rpr_rqst_ver_no]			]]></sql>
			<params>
				<param name="rpr_offh_flg" type="12" value="" out="N"/>
				<param name="n3pty_flg" type="12" value="" out="N"/>
				<param name="rpr_yd_cd" type="12" value="" out="N"/>
				<param name="rpr_wrk_tp_cd" type="12" value="" out="N"/>
				<param name="mnr_rpr_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rqst_eq_no" type="12" value="" out="N"/>
				<param name="rpr_rqst_seq" type="12" value="" out="N"/>
				<param name="rpr_rqst_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
