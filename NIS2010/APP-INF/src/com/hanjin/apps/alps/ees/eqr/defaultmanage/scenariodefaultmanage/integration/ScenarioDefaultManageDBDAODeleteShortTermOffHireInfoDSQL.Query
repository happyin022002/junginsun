<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ScenarioDefaultManageDBDAODeleteShortTermOffHireInfoDSQL">
			<desc><![CDATA[EQR_SHRT_TERM_OFFH_COND 테이블에서 특정 ECC삭제]]></desc>
			<sql><![CDATA[
DELETE FROM EQR_SHRT_TERM_OFFH_COND
WHERE	ECC_CD			= @[ecc_cd]
	AND	CTRT_OFC_CTY_CD	= @[ctrt_ofc_cty_cd]
	AND CTRT_SEQ		= @[ctrt_seq]
	AND CNTR_TPSZ_CD	= @[cntr_tpsz_cd]			]]></sql>
			<params>
				<param name="ecc_cd" type="12" value="" out="N"/>
				<param name="ctrt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="ctrt_seq" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
