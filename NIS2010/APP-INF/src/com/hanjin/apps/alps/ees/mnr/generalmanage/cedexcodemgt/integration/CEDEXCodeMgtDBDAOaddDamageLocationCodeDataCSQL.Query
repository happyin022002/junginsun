<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CEDEXCodeMgtDBDAOaddDamageLocationCodeDataCSQL">
			<desc><![CDATA[addDamageLocationCodeData]]></desc>
			<sql><![CDATA[
INSERT INTO MNR_EQ_LOC_CD
(
	EQ_LOC_CD_LVL, 
	EQ_LOC_CD, EQ_LOC_NM, 
	EQ_LESS_20FT_FLG, 
	EQ_KND_CD, 
	EQ_LOC_PRNT_CD, 
	CRE_USR_ID, 
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
)
VALUES
(@[eq_loc_cd_lvl], @[eq_loc_cd], @[eq_loc_nm], DECODE(NVL(@[eq_less_20ft_flg],'0'),'0','N','Y'), @[eq_knd_cd], @[eq_loc_prnt_cd], @[cre_usr_id], sysdate, @[cre_usr_id], sysdate)			]]></sql>
			<params>
				<param name="eq_loc_cd_lvl" type="12" value="" out="N"/>
				<param name="eq_loc_cd" type="12" value="" out="N"/>
				<param name="eq_loc_nm" type="12" value="" out="N"/>
				<param name="eq_less_20ft_flg" type="12" value="" out="N"/>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="eq_loc_prnt_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
