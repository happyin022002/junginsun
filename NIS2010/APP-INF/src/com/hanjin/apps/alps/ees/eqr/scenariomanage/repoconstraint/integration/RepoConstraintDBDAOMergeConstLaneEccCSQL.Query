<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepoConstraintDBDAOMergeConstLaneEccCSQL">
			<desc><![CDATA[<EES_EQR_0138 Constraint by Lane/ECC>
EQR_SCNR_PORT_DCHG_CNST  테이블 데이터 수정

<Change History>
1	2009.08.12	Lee ByoungHun	최초작성]]></desc>
			<sql><![CDATA[
MERGE INTO EQR_SCNR_PORT_DCHG_CNST A
USING (
    SELECT
    @[scnr_id]  AS SCNR_ID
    ,@[vsl_lane_cd]  AS LANE_CD
    ,@[vsl_loc_cd]  AS LOC_CD
    ,@[cnst_cntr_tpsz_cd]  AS CNTR_TPSZ_CD
    FROM DUAL
) B
ON    (
    A.VSL_LANE_CD 			= B.LANE_CD
    AND  A.VSL_LOC_CD  			= B.LOC_CD
    AND  A.CNST_CNTR_TPSZ_CD   	= B.CNTR_TPSZ_CD
    AND  A.SCNR_ID   	        = B.SCNR_ID
)
WHEN MATCHED THEN
    UPDATE
    SET    	 A.CNTR_VOL_QTY  = @[cntr_vol_qty] ,
    A.UPD_USR_ID    = @[upd_usr_id] ,
    A.UPD_DT        = SYSDATE
WHEN NOT MATCHED THEN
    INSERT
    (    
        A.SCNR_ID      ,
        A.VSL_LANE_CD  ,
        A.VSL_LOC_CD   ,
        A.CNST_CNTR_TPSZ_CD ,
        A.CNTR_VOL_QTY ,
        A.CRE_USR_ID   ,
        A.CRE_DT       ,
        A.UPD_USR_ID   ,
        A.UPD_DT
    ) VALUES
    (
        @[scnr_id],
        @[vsl_lane_cd],
        @[vsl_loc_cd],
        @[cnst_cntr_tpsz_cd],
        @[cntr_vol_qty],
        @[upd_usr_id],
        SYSDATE,
        @[upd_usr_id],
        SYSDATE
    )			]]></sql>
			<params>
				<param name="scnr_id" type="12" value="" out="N"/>
				<param name="vsl_lane_cd" type="12" value="" out="N"/>
				<param name="vsl_loc_cd" type="12" value="" out="N"/>
				<param name="cnst_cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cntr_vol_qty" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
