<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReeferSparePartMgtDBDAOmodifyVesselSparePartInventoryDataUSQL">
			<desc><![CDATA[수정]]></desc>
			<sql><![CDATA[
UPDATE MNR_VSL_SPR_PRT_INVT SET
		SPR_UT_SHR_CD = @[spr_ut_shr_cd]
        ,DELT_FLG   = @[spr_prt_del_flg]
        ,UPD_USR_ID        = @[upd_usr_id]
        ,UPD_DT = sysdate 
WHERE	SPR_PRT_INVT_NO = @[spr_prt_invt_no]
AND SPR_PRT_INVT_VER_SEQ = @[spr_prt_ver_seq]
AND VSL_CD = @[vsl_cd]
AND LANE_CD = @[lane_cd]
AND SPR_PRT_VNDR_SEQ = @[spr_prt_vndr_seq]
AND SPR_PRT_TP_CD = @[spr_prt_tp_cd]
AND SPR_UT_MDL_NM = @[spr_ut_mdl_nm]			]]></sql>
			<params>
				<param name="spr_ut_shr_cd" type="12" value="" out="N"/>
				<param name="spr_prt_del_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="spr_prt_invt_no" type="12" value="" out="N"/>
				<param name="spr_prt_ver_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="lane_cd" type="12" value="" out="N"/>
				<param name="spr_prt_vndr_seq" type="12" value="" out="N"/>
				<param name="spr_prt_tp_cd" type="12" value="" out="N"/>
				<param name="spr_ut_mdl_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
