<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetAgreementDBDAOModifyCHSCpsAgreementMainDataUSQL">
			<desc><![CDATA[CGM_AGREEMENT 데이터를 수정한다.
-- 2014.11 10만불 결제관련]]></desc>
			<sql><![CDATA[
UPDATE CGM_AGREEMENT
SET 
    LST_VER_FLG			= @[lst_ver_flg], 
    AGMT_ISS_OFC_CD		= @[agmt_iss_ofc_cd], 
    AGMT_REF_NO			= @[agmt_ref_no], 
    CURR_CD				= @[curr_cd], 
    VNDR_SEQ			= @[vndr_seq], 
    PAY_TERM_DYS		= @[pay_term_dys],
    AGMT_LSTM_CD		= @[agmt_lstm_cd],
    CHSS_POOL_CD		= @[chss_pool_cd],
    DIFF_RMK			= @[diff_rmk],
	AGMT_EFF_DT			= TO_DATE(@[agmt_eff_dt],'YYYYMMDD'),
	AGMT_EXP_DT			= TO_DATE(@[agmt_exp_dt],'YYYYMMDD'),
    EFF_DT			    = TO_DATE(@[agmt_eff_dt],'YYYYMMDD'),
	EXP_DT			    = TO_DATE(@[agmt_exp_dt],'YYYYMMDD'),
    CTRT_NO             = @[ctrt_no],
    GW_UQ_DOC_TIT_NM	= @[gw_uq_doc_tit_nm],
    GW_UQ_DOC_NO		= @[gw_uq_doc_no],
    UPD_USR_ID			= @[upd_usr_id],
    UPD_DT				= SYSDATE
WHERE
	AGMT_OFC_CTY_CD = @[agmt_ofc_cty_cd]
    AND AGMT_SEQ = @[agmt_seq]
    AND AGMT_VER_NO = @[agmt_ver_no]			]]></sql>
			<params>
				<param name="lst_ver_flg" type="12" value="" out="N"/>
				<param name="agmt_iss_ofc_cd" type="12" value="" out="N"/>
				<param name="agmt_ref_no" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="pay_term_dys" type="12" value="" out="N"/>
				<param name="agmt_lstm_cd" type="12" value="" out="N"/>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
				<param name="diff_rmk" type="12" value="" out="N"/>
				<param name="agmt_eff_dt" type="12" value="" out="N"/>
				<param name="agmt_exp_dt" type="12" value="" out="N"/>
				<param name="ctrt_no" type="12" value="" out="N"/>
				<param name="gw_uq_doc_tit_nm" type="12" value="" out="N"/>
				<param name="gw_uq_doc_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agmt_ofc_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
				<param name="agmt_ver_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
