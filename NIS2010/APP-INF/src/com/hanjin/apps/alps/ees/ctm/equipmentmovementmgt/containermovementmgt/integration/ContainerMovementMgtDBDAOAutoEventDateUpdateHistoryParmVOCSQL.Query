<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOAutoEventDateUpdateHistoryParmVOCSQL">
			<desc><![CDATA[AutoEventDateUpdateHistoryParmVO]]></desc>
			<sql><![CDATA[
INSERT INTO CTM_MVMT_EVNT_DT_HIS
(
CNTR_NO
,CNMV_YR
,CNMV_ID_NO
,CNMV_UPD_HIS_SEQ
,CNTR_TPSZ_CD
,MVMT_STS_CD
,PRE_CNMV_EVNT_DT
,CRNT_CNMV_EVNT_DT
,CRNT_VSL_CD
,CRNT_SKD_VOY_NO
,CRNT_SKD_DIR_CD
,FCNTR_FLG
,BKG_NO
,BL_NO
,MVMT_EDI_MSG_TP_ID
,OFC_CD
,CRE_LOCL_DT
,CRE_USR_ID
,UPD_LOCL_DT
,UPD_USR_ID
,ORG_YD_CD
,CNMV_RMK
)
SELECT
 CNTR_NO
,CNMV_YR
,CNMV_ID_NO
,(SELECT NVL(MAX(CNMV_UPD_HIS_SEQ),0)+1
    FROM CTM_MVMT_EVNT_DT_HIS 
   WHERE CNTR_NO = @[cntr_no]
     AND CNMV_YR = @[cnmv_yr]
     AND CNMV_ID_NO  = @[cnmv_id])
,CNTR_TPSZ_CD
,MVMT_STS_CD
,CNMV_EVNT_DT
,TO_DATE (@[tar_date], 'YYYY-MM-DD HH24:MI')
,CRNT_VSL_CD
,CRNT_SKD_VOY_NO
,CRNT_SKD_DIR_CD
,FCNTR_FLG
,BKG_NO
,BL_NO
,MVMT_EDI_MSG_TP_ID
,OFC_CD
,CRE_LOCL_DT
,@[upd_usr_id]
,UPD_LOCL_DT
,@[upd_usr_id]
,ORG_YD_CD
,@[cnmv_rmk]
 FROM CTM_MOVEMENT
WHERE CNTR_NO = @[cntr_no]
  AND CNMV_YR = @[cnmv_yr]
  AND CNMV_ID_NO  = @[cnmv_id]
  AND TO_CHAR(CNMV_EVNT_DT, 'YYYYMMDD') <> substr(@[tar_date], 1, 8)			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cnmv_yr" type="12" value="" out="N"/>
				<param name="cnmv_id" type="12" value="" out="N"/>
				<param name="tar_date" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cnmv_rmk" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
