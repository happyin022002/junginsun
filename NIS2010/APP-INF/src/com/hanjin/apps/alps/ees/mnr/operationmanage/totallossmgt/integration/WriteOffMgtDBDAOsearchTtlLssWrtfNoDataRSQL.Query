<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="WriteOffMgtDBDAOsearchTtlLssWrtfNoDataRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT COUNT(TTL_LSS_NO) TTL_LSS_NO_CNT
  FROM MNR_TTL_LSS_RQST_DTL
 WHERE TTL_LSS_NO = @[ttl_lss_no]
   AND (MTLRD.WRTF_NO NOT IN (SELECT DISTINCT WRTF_NO 
								FROM MNR_WRTF_RQST_HDR 
							   WHERE WRTF_STS_CD = 'RQ'
								 AND TTL_LSS_NO = MTLRD.TTL_LSS_NO) 
		OR MTLRD.WRTF_NO IS NULL)
   AND MTLRD.MNR_INV_TP_CD = 'DV'			]]></sql>
			<params>
				<param name="ttl_lss_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
