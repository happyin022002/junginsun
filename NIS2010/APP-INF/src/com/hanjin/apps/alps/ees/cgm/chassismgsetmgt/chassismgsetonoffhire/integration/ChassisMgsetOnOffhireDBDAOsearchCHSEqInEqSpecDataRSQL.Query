<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetOnOffhireDBDAOsearchCHSEqInEqSpecDataRSQL">
			<desc><![CDATA[// chungpa 20091006 check equipment in eq_spec]]></desc>
			<sql><![CDATA[
SELECT 
*
FROM CGM_EQUIPMENT t1

WHERE
t1.EQ_SPEC_NO = @[eq_spec_no]
AND ROWNUM = 1
			]]></sql>
			<params>
				<param name="eq_spec_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
