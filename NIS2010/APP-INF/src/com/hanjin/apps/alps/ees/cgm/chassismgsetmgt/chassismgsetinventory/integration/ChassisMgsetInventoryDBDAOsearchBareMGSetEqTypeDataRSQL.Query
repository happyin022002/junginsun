<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOsearchBareMGSetEqTypeDataRSQL">
			<desc><![CDATA[chassis no  조회]]></desc>
			<sql><![CDATA[
SELECT EQ_TPSZ_CD
      ,CRNT_YD_CD AS ORG_YD_CD
FROM CGM_EQUIPMENT
WHERE EQ_NO        = @[mgst_no]  -- 검색조건(mgset)
AND   EQ_KND_CD    = 'G' -- M.G.Set (Hard Coding)
AND   ACIAC_DIV_CD = 'A' -- Active(Hard Coding) 			]]></sql>
			<params>
				<param name="mgst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
