<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanOptiExecuteDBDAOSearchMonitorRSQL">
			<desc><![CDATA[실시간으로 엔진 running 상태를 체크를 한다.]]></desc>
			<sql><![CDATA[
SELECT RUN_ID_NO
      , RUN_STS_CD
      , RUN_PROG_KNT
      , RUN_STS_DESC
FROM EQR_ENG_INP_OPMZ_RUN
WHERE RUN_ID_NO = @[run_id_no]			]]></sql>
			<params>
				<param name="run_id_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
