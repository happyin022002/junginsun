<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInvoiceDBDAOSearchSCNoInfoDataRSQL">
			<desc><![CDATA[Sc No. 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT SC_NO
  FROM PRI_SP_HDR
 WHERE 1=1
   AND SC_NO = @[sc_no]
			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
