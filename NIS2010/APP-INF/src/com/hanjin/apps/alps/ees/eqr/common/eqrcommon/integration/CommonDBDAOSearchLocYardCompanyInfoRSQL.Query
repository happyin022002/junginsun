<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommonDBDAOSearchLocYardCompanyInfoRSQL">
			<desc><![CDATA[LOC Yard (INLAND+VESSEL) combo box 정보를 검색]]></desc>
			<sql><![CDATA[
SELECT 
  LSE_CO_YD_CD YD_CD ,                                  
  REPLACE(LSE_CO_YD_NM, '''', ' ') YD_NM                                  
FROM 
  MDM_LSE_CO_YD								            		
WHERE 
  SUBSTR(LSE_CO_YD_CD, 0, 5) IN ( 						
				SELECT 
				  SCC_CD 								
				FROM 
				  MDM_EQ_ORZ_CHT 						
--			WHERE ECC_CD LIKE '"+locyard_searchword+"%' 
				WHERE ECC_CD LIKE @[locyard_searchword]                         
	) 											 

-- 20070723 added by shin yongchan
--AND  DELT_FLG <> 'Y'                           	    		
  AND  DELT_FLG <> @[delt_flg]                               
ORDER BY LSE_CO_YD_CD	            									]]></sql>
			<params>
				<param name="locyard_searchword" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
