<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PayableRentalCostDBDAOLessorInvoiceFileImportUSQL">
			<desc><![CDATA[Lessor Invoice File Data Update.]]></desc>
			<sql><![CDATA[
UPDATE LSE_PAY_RNTL_CHG_CO
SET    LSE_PAY_CHG_TP_NM = @[lse_pay_chg_tp_nm]
     , UPD_USR_ID        = @[upd_usr_id]
     , UPD_DT            = SYSDATE
WHERE  CO_COST_YRMON     = @[co_cost_yrmon]
AND    CNTR_NO           = @[cntr_no]
AND    LSE_PAY_CHG_TP_CD = @[lse_pay_chg_tp_cd]
AND    AGMT_CTY_CD       = @[agmt_cty_cd]
AND    AGMT_SEQ          = @[agmt_seq]			]]></sql>
			<params>
				<param name="lse_pay_chg_tp_nm" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="co_cost_yrmon" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="lse_pay_chg_tp_cd" type="12" value="" out="N"/>
				<param name="agmt_cty_cd" type="12" value="" out="N"/>
				<param name="agmt_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
