<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOsearchInventoryByStaydaysDataRSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT   

#if (${group1} == '1') 
	t1.EQ_TPSZ_CD AS GROUP1,
	t7.DP_SEQ,
#elseif (${group1} == '2') 
	t4.LCC_CD AS GROUP1,
#elseif (${group1} == '3') 
	t1.ONH_OFC_CD AS GROUP1,
#elseif (${group1} == '4') 
	t3.SCC_CD AS GROUP1,
#elseif (${group1} == '5') 
	t1.CRNT_YD_CD AS GROUP1,
#elseif (${group1} == '6') 
	t1.AGMT_LSTM_CD AS GROUP1,
	t6.INTG_CD_VAL_DP_SEQ,
#elseif (${group1} == '7') 
	t9.VNDR_ABBR_NM||' ('||t1.VNDR_SEQ||')' AS GROUP1,
#elseif (${group1} == '8')
	t1.CHSS_MVMT_STS_CD AS GROUP1,
	t5.INTG_CD_VAL_DP_SEQ,
#end

      count(*) as chss_mvmt_dt_tot -- Total
#if (${staying_days} != '')
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt >= TO_NUMBER(${staying_days}) THEN 1 END ),0) as chss_mvmt_dt_0_or_over  
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt >= TO_NUMBER(${staying_days}) THEN 1 END ) /  count(*) * 100 ),0), 'FM999,990.00') || '%' as chss_mvmt_dt_0_or_over_rate
#else
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt >= 0 THEN 1 END ),0) as chss_mvmt_dt_0_or_over  
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt >= 0 THEN 1 END ) /  count(*) * 100 ),0), 'FM999,990.00') || '%' as chss_mvmt_dt_0_or_over_rate
#end
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n1st_inq_fm_dys}) and TO_NUMBER(${n1st_inq_to_dys})+1 THEN 1 END ),0) as chss_mvmt_dt_today_15 
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n1st_inq_fm_dys}) and TO_NUMBER(${n1st_inq_to_dys})+1 THEN 1 END ) / count(*) * 100),0), 'FM999,990.00') || '%' as chss_mvmt_dt_today_15_rate 
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n2nd_inq_fm_dys}) and TO_NUMBER(${n2nd_inq_to_dys})+1 THEN 1 END ),0) as chss_mvmt_dt_16_30 
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n2nd_inq_fm_dys}) and TO_NUMBER(${n2nd_inq_to_dys})+1 THEN 1 END ) / count(*) * 100),0), 'FM999,990.00') || '%' as chss_mvmt_dt_16_30_rate 
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n3rd_inq_fm_dys}) and TO_NUMBER(${n3rd_inq_to_dys})+1 THEN 1 END ),0) as chss_mvmt_dt_31_50 
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n3rd_inq_fm_dys}) and TO_NUMBER(${n3rd_inq_to_dys})+1 THEN 1 END ) / count(*) * 100),0), 'FM999,990.00') || '%' as chss_mvmt_dt_31_50_rate 
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n4th_inq_fm_dys}) and TO_NUMBER(${n4th_inq_to_dys})+1 THEN 1 END ),0) as chss_mvmt_dt_51_100 
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n4th_inq_fm_dys}) and TO_NUMBER(${n4th_inq_to_dys})+1 THEN 1 END ) / count(*) * 100),0), 'FM999,990.00') || '%' as chss_mvmt_dt_51_100_rate
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n5th_inq_fm_dys}) and TO_NUMBER(${n5th_inq_to_dys})+1 THEN 1 END ),0) as chss_mvmt_dt_101_180 
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt between TO_NUMBER(${n5th_inq_fm_dys}) and TO_NUMBER(${n5th_inq_to_dys})+1 THEN 1 END ) / count(*) * 100),0), 'FM999,990.00') || '%' as chss_mvmt_dt_101_180_rate
      ,NVL(sum( case when  sysdate - t1.chss_mvmt_dt > TO_NUMBER(${n5th_inq_to_dys})+1 THEN 1 END ),0) as chss_mvmt_dt_181_over  
      ,TO_CHAR(NVL((sum( case when  sysdate - t1.chss_mvmt_dt > TO_NUMBER(${n5th_inq_to_dys}) THEN 1 END ) / count(*) * 100),0), 'FM999,990.00') || '%' as chss_mvmt_dt_181_over_rate                               
FROM 
    CGM_EQUIPMENT t1
#if (${atch_bare} != '')
	, ( SELECT A.EQ_NO FROM  CGM_EQ_ATCH_DTCH_HIS A
    	WHERE A.DTCH_DT = TO_DATE('88881231','YYYYMMDD')
	    GROUP BY A.EQ_NO
	   ) T8
#end
    , CGM_EQ_STS_HIS t2
    , MDM_LOCATION t3
    , MDM_EQ_ORZ_CHT t4     
    , COM_INTG_CD_DTL t5
    , COM_INTG_CD_DTL t6
	, CGM_EQ_TP_SZ t7
	, MDM_VENDOR t9

WHERE 
	  t1.VNDR_SEQ = t9.VNDR_SEQ(+)
      AND t1.EQ_NO = t2.EQ_NO
#if (${atch_bare} != '')
	  AND t1.EQ_NO= T8.EQ_NO(+)
#end
      AND t1.eq_sts_seq= t2.eq_sts_seq
      AND t1.CRNT_LOC_CD = t3.LOC_CD
      AND t3.SCC_CD = t4.SCC_CD
      AND t3.DELT_FLG = 'N'
      AND t4.DELT_FLG = 'N'
      AND t1.CHSS_MVMT_STS_CD = t5.INTG_CD_VAL_CTNT (+)
      AND 'CD02386' = t5.INTG_CD_ID (+)

      AND t1.AGMT_LSTM_CD = t6.INTG_CD_VAL_CTNT (+)
      AND 'CD01948' = t6.INTG_CD_ID (+)
              
	  AND t1.EQ_TPSZ_CD = t7.EQ_TPSZ_CD (+)



	  AND t1.EQ_KND_CD = @[eq_knd_cd]
				#if ( ${location} == 'RCC' )
			   	AND  t4.RCC_CD = (@[crnt_loc_cd])
				#elseif ( ${location} == 'LCC' )
				AND  t4.LCC_CD = (@[crnt_loc_cd])
				#elseif ( ${location} == 'SCC' )
				AND   t4.SCC_CD = (@[crnt_loc_cd])
				#end

       
#if (${crnt_yd_cd} != '')
	  AND t1.CRNT_YD_CD IN ($crnt_yd_cd)
#end      
#if (${aciac_div_cd} != '')
	  AND t1.ACIAC_DIV_CD = @[aciac_div_cd] 
#end      
#if (${chss_pool_cd} != 'I')
	#if (${chss_pool_cd} == 'E')
		AND t1.CHSS_POOL_CD IS NULL
	#elseif (${chss_pool_cd} == 'O')
		AND t1.CHSS_POOL_CD IS NOT NULL
	#else
		AND t1.CHSS_POOL_CD = @[chss_pool_cd]
	#end
#end
#if (${include_np} != 'Y')
	AND t1.AGMT_LSTM_CD <>'NP' 
#end      

#if (${eq_tpsz_cd} != '')
	AND t1.EQ_TPSZ_CD IN ($eq_tpsz_cd) 
#end
#if (${agmt_lstm_cd} != '')
	AND t1.AGMT_LSTM_CD IN ($agmt_lstm_cd) 
#end
#if (${vndr_seq} != '')
	AND t1.VNDR_SEQ IN ($vndr_seq) 
#end
#if (${chss_mvmt_sts_cd} != '')
	AND t1.CHSS_MVMT_STS_CD IN ($chss_mvmt_sts_cd) 
#end
#if (${include_status_lst} == 'Y')
	AND t2.EQ_ASET_STS_CD = 'LST' 
#end   
#if (${include_out_gated} == 'Y')
	AND t1.GATE_IO_CD = 'O' 
#end   

#if (${atch_bare} == 'ATTACHED')
	AND	T8.EQ_NO IS NOT NULL
#elseif (${atch_bare} == 'BARE')
	AND T8.EQ_NO IS NULL  
#end

#if (${dmg_snd} == 'DAMAGE')
AND t1.DMG_FLG = 'Y'
#elseif (${dmg_snd} == 'SOUND')
AND ((t1.DMG_FLG <> 'Y') or (t1.DMG_FLG IS NULL))
#end


#if (${group1} != '') 
GROUP BY
#end
#if (${group1} == '1') 
	t1.EQ_TPSZ_CD
	,t7.DP_SEQ
#elseif (${group1} == '2') 
	t4.LCC_CD
#elseif (${group1} == '3') 
	t1.ONH_OFC_CD
#elseif (${group1} == '4') 
	t3.SCC_CD
#elseif (${group1} == '5') 
	t1.CRNT_YD_CD
#elseif (${group1} == '6') 
	t1.AGMT_LSTM_CD
	,t6.INTG_CD_VAL_DP_SEQ
#elseif (${group1} == '7') 
	t9.VNDR_ABBR_NM||' ('||t1.VNDR_SEQ||')'
#elseif (${group1} == '8') 
	t1.CHSS_MVMT_STS_CD
	,t5.INTG_CD_VAL_DP_SEQ
#end

#if (${group1} != '') 
ORDER BY
#end
#if (${group1} == '1') 
	--t1.EQ_TPSZ_CD
	t7.DP_SEQ
#elseif (${group1} == '2') 
	t4.LCC_CD
#elseif (${group1} == '3') 
	t1.ONH_OFC_CD
#elseif (${group1} == '4') 
	t3.SCC_CD
#elseif (${group1} == '5') 
	t1.CRNT_YD_CD
#elseif (${group1} == '6') 
	--t1.AGMT_LSTM_CD
	t6.INTG_CD_VAL_DP_SEQ
#elseif (${group1} == '7') 
	t9.VNDR_ABBR_NM||' ('||t1.VNDR_SEQ||')'
#elseif (${group1} == '8') 
	--t1.CHSS_MVMT_STS_CD
	t5.INTG_CD_VAL_DP_SEQ
#end			]]></sql>
			<params>
				<param name="eq_knd_cd" type="12" value="" out="N"/>
				<param name="crnt_loc_cd" type="12" value="" out="N"/>
				<param name="aciac_div_cd" type="12" value="" out="N"/>
				<param name="chss_pool_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
