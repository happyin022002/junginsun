<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ChassisMgsetInventoryDBDAOmanageCHSLongstayEnvDataUSQL">
			<desc><![CDATA[chungpa 20090722 1094 update query]]></desc>
			<sql><![CDATA[

MERGE INTO CGM_LONG_STAY_DYS_ENV t1
      USING DUAL
      ON (t1.STAY_DYS_INP_USR_ID = @[stay_dys_inp_usr_id] )
      WHEN MATCHED THEN
           		UPDATE SET 
                    N1ST_INQ_FM_DYS=@[n1st_inq_fm_dys]
                    ,N1ST_INQ_TO_DYS=@[n1st_inq_to_dys]
                    , N2ND_INQ_FM_DYS=@[n2nd_inq_fm_dys]
                    , N2ND_INQ_TO_DYS=@[n2nd_inq_to_dys]
                    , N3RD_INQ_FM_DYS=@[n3rd_inq_fm_dys]
                    , N3RD_INQ_TO_DYS=@[n3rd_inq_to_dys]
                    , N4TH_INQ_FM_DYS=@[n4th_inq_fm_dys]
                    , N4TH_INQ_TO_DYS=@[n4th_inq_to_dys]
                    , N5TH_INQ_FM_DYS=@[n5th_inq_fm_dys]
                    , N5TH_INQ_TO_DYS=@[n5th_inq_to_dys]
                    , UPD_USR_ID=@[stay_dys_inp_usr_id]
                    , UPD_DT=sysdate                        
                       WHERE STAY_DYS_INP_USR_ID = @[stay_dys_inp_usr_id]
       WHEN NOT MATCHED THEN
           		INSERT (
                    STAY_DYS_INP_USR_ID
                    , N1ST_INQ_FM_DYS
                    , N1ST_INQ_TO_DYS
                    , N2ND_INQ_FM_DYS
                    , N2ND_INQ_TO_DYS
                    , N3RD_INQ_FM_DYS
                    , N3RD_INQ_TO_DYS
                    , N4TH_INQ_FM_DYS
                    , N4TH_INQ_TO_DYS
                    , N5TH_INQ_FM_DYS
                    , N5TH_INQ_TO_DYS
                    , CRE_USR_ID
                    , CRE_DT
                    , UPD_USR_ID
                    , UPD_DT
                   ) 
                   VALUES (
                   		@[stay_dys_inp_usr_id]
                   		,@[n1st_inq_fm_dys]
                   		,@[n1st_inq_to_dys]
                   		,@[n2nd_inq_fm_dys]
                   		,@[n2nd_inq_to_dys]
                   		,@[n3rd_inq_fm_dys]
                   		,@[n3rd_inq_to_dys]
                   		,@[n4th_inq_fm_dys]
                   		,@[n4th_inq_to_dys]
                   		,@[n5th_inq_fm_dys]
                   		,@[n5th_inq_to_dys]
                   		,@[stay_dys_inp_usr_id]
                   		,sysdate
                   		,@[stay_dys_inp_usr_id]
                   		,sysdate)			]]></sql>
			<params>
				<param name="stay_dys_inp_usr_id" type="12" value="" out="N"/>
				<param name="n1st_inq_fm_dys" type="12" value="" out="N"/>
				<param name="n1st_inq_to_dys" type="12" value="" out="N"/>
				<param name="n2nd_inq_fm_dys" type="12" value="" out="N"/>
				<param name="n2nd_inq_to_dys" type="12" value="" out="N"/>
				<param name="n3rd_inq_fm_dys" type="12" value="" out="N"/>
				<param name="n3rd_inq_to_dys" type="12" value="" out="N"/>
				<param name="n4th_inq_fm_dys" type="12" value="" out="N"/>
				<param name="n4th_inq_to_dys" type="12" value="" out="N"/>
				<param name="n5th_inq_fm_dys" type="12" value="" out="N"/>
				<param name="n5th_inq_to_dys" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
