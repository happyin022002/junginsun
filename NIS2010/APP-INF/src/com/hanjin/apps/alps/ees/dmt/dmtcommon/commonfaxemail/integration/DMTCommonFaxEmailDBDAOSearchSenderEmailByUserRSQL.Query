<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonFaxEmailDBDAOSearchSenderEmailByUserRSQL">
			<desc><![CDATA[UserId 의 Sender Email 주소 조회]]></desc>
			<sql><![CDATA[
SELECT DECODE(@[usr_ofc],'GOASC','claims.hjs@hanjinitaly.com', USR_EML ) AS DFLT_EML
FROM COM_USER
WHERE USR_ID = @[usr_id]			]]></sql>
			<params>
				<param name="usr_ofc" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
