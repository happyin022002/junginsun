<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrMtyBkgCreateDBDAOaddCntrMtyBkgSplitListQtyCSQL">
			<desc><![CDATA[EQR_CTRL_MTY_BKG_EXE_QTY 실행테이블에 QTY=0으로 입력]]></desc>
			<sql><![CDATA[
INSERT INTO EQR_CTRL_MTY_BKG_EXE_QTY
(
    TRSP_MOD_CD
   ,VSL_CD
   ,SKD_VOY_NO
   ,SKD_DIR_CD
   ,BKG_EXE_SEQ
   ,CNTR_TPSZ_CD
   ,CNTR_QTY
   ,CRE_USR_ID
   ,CRE_DT
   ,UPD_USR_ID
   ,UPD_DT  
)

SELECT @[trsp_mod_cd] TRSP_MOD_CD
      ,@[vsl_cd] VSL_CD
      ,@[skd_voy_no]  SKD_VOY_NO
      ,@[skd_dir_cd] SKD_DIR_CD
      ,@[bkg_exe_seq] BKG_EXE_SEQ
      ,B.CNTR_TPSZ_CD
      ,0 CNTR_QTY
      ,@[cre_usr_id] CRE_USR_ID
      ,SYSDATE
      ,@[cre_usr_id] UPD_USR_ID
      ,SYSDATE 
FROM DUAL A
    ,(
        SELECT INTG_CD_VAL_CTNT CNTR_TPSZ_CD
        FROM COM_INTG_CD_DTL
        WHERE INTG_CD_ID = 'CD00830'  -- EQR TYPE SIZE ALL 
     ) B			]]></sql>
			<params>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bkg_exe_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
