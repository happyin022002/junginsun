<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CgmCodeMgtDBDAOCheckCPSScExptGroupCustNameDataRSQL">
			<desc><![CDATA[입력된 Group Customer Code로부터 Group Customer Name을 체크한다.]]></desc>
			<sql><![CDATA[
SELECT  CUST_GRP_ID,
        CUST_GRP_NM
FROM MDM_CUST_PERF_GRP
WHERE CUST_GRP_ID = @[cust_grp_id]			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
