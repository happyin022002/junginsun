<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerOnOffhireDBDAORemoveMstContainerDataDSQL">
			<desc><![CDATA[CNTR STS 삭제 시 MST_CONTAINER의 정보도 삭제한다]]></desc>
			<sql><![CDATA[
DELETE FROM MST_CONTAINER
WHERE CNTR_NO = @[cntr_no]
AND 0 = (SELECT COUNT(*) FROM CTM_MOVEMENT WHERE CNTR_NO = @[cntr_no])
AND 0 = (SELECT COUNT(*) FROM MST_CNTR_STS_HIS WHERE CNTR_NO = @[cntr_no])			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
