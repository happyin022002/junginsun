<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RepoConstraintDBDAODeleteEmptyRepoConstDSQL">
			<desc><![CDATA[eqr_scnr_repo_cnst 테이블 데이터 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM 
    EQR_SCNR_REPO_CNST    							
WHERE 
    REPO_CNST_SEQ	= @[repo_cnst_seq]                               
    AND SCNR_ID	    = @[scnr_id]                               		
			]]></sql>
			<params>
				<param name="repo_cnst_seq" type="12" value="" out="N"/>
				<param name="scnr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
