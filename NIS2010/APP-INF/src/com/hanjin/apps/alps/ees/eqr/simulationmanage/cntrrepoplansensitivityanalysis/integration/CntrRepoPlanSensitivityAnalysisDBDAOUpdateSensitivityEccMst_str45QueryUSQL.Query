<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoPlanSensitivityAnalysisDBDAOUpdateSensitivityEccMst_str45QueryUSQL">
			<desc><![CDATA[EQR_ECC_MST  테이블에 대한 STO 45feet amount  수정]]></desc>
			<sql><![CDATA[
UPDATE EQR_ECC_MST 
       SET  STO_45FT_COST_AMT  = ${curr_cost}          	
            ,UPD_USR_ID        = @[user_id]         	
            ,UPD_DT             = sysdate   		
WHERE ECC_CD   = @[ecc_cd]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
