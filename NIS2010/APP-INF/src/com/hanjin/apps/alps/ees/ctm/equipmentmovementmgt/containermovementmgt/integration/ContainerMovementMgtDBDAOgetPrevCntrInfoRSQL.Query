<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ContainerMovementMgtDBDAOgetPrevCntrInfoRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT /*+ INDEX_DESC (CTM XUK1CTM_MOVEMENT) */
       CTM.CNTR_NO,
       CTM.CNMV_YR,
       CTM.CNMV_ID_NO,
       CTM.CNMV_SEQ,
       CTM.CNMV_SPLIT_NO,
       CTM.CNTR_TPSZ_CD,
       CTM.MVMT_STS_CD,
       CTM.BKG_CGO_TP_CD,
       CTM.CNMV_CYC_NO,
       CTM.CNMV_LVL_NO,
       TO_CHAR(CTM.CNMV_EVNT_DT, 'YYYYMMDDHH24MISS') CNMV_EVNT_DT,
       CTM.DEST_YD_CD,
       CTM.INP_YD_CD,
       CTM.ORG_YD_CD,
       CTM.CRNT_VSL_CD,
       CTM.CRNT_SKD_VOY_NO,
       CTM.CRNT_SKD_DIR_CD,
       CTM.TRNK_VSL_CD,
       CTM.TRNK_SKD_VOY_NO,
       CTM.TRNK_SKD_DIR_CD,
       CTM.CHSS_NO,
       CTM.MGST_NO,
       CTM.CNTR_SEAL_NO,
       CTM.CNTR_DMG_FLG,
       CTM.FCNTR_FLG,
       CTM.OB_CNTR_FLG,
       CTM.BKG_RCV_TERM_CD,
       CTM.VNDR_SEQ,
       CTM.MVMT_TRSP_MOD_CD,
       CTM.LOC_CD,
       CTM.CNMV_RMK,
       CTM.USR_NM,
       CTM.MVMT_CRE_TP_CD,
       CTM.SUBST_RULE_CD,
       CTM.SPCL_CGO_FLG,
       CTM.BKG_NO,
       CTM.BKG_KNT,
       CTM.BL_NO,
       CTM.CNTR_HNGR_RCK_FLG,
       CTM.CNTR_HNGR_BAR_ATCH_KNT,
       CTM.CNTR_ACT_CD,
       CTM.CNTR_RFUB_FLG,
       CTM.CNTR_DISP_FLG,
       CTM.IMDT_EXT_FLG,
       CTM.CNTR_XCH_CD,
       CTM.INLND_TRSP_LIC_NO,
       CTM.CTRT_OFC_CTY_CD,
       CTM.CTRT_SEQ,
       CTM.MVMT_EDI_TP_CD,
       CTM.MVMT_EDI_MSG_TP_ID,
       CTM.MVMT_EDI_MSG_AREA_CD,
       CTM.MVMT_EDI_MSG_YRMONDY,
       CTM.MVMT_EDI_MSG_SEQ,
       CTM.WBL_NO,
       CTM.PKUP_NO,
       CTM.CNTR_STS_SEQ,
       CTM.CALL_SGN_NO,
       CTM.LLOYD_NO,
       CTM.MTY_REPO_VL_RMK,
       CTM.MVMT_INP_TP_CD,
       CTM.CNMV_CO_CD,
       CTM.SYS_AREA_GRP_ID AS CNTR_SVR_ID,
       CTM.OFC_CD,
       CTM.PRE_STS_FLG,
       CTM.GMT_DT,
       CTM.CRE_LOCL_DT,
       CTM.UPD_LOCL_DT,
       CTM.CRE_USR_ID,
       CTM.CRE_DT,
       CTM.UPD_USR_ID,
       CTM.UPD_DT,
       MST.LSTM_CD
  FROM CTM_MOVEMENT CTM, MST_CONTAINER MST
 WHERE CTM.CNTR_NO = @[cntr_no]
  AND  CTM.CNTR_NO = MST.CNTR_NO
   AND ROWNUM <= ${ord}			]]></sql>
			<params>
				<param name="cntr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
