<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CntrRepoExecutionPlanEstablishDBDAOInsertBkgCancelHistoryCSQL">
			<desc><![CDATA[EQR_BKG_CXL_HIS 테이블에 bkg cancel history 입력]]></desc>
			<sql><![CDATA[
INSERT INTO	EQR_BKG_CXL_HIS
(
	REPO_PLN_ID
	,PLN_YRWK
	,PLN_SEQ
	,REF_ID
	,MTY_BKG_NO
	,TRSP_MOD_CD
	,CRE_USR_ID
	,CRE_DT
	,UPD_USR_ID
	,UPD_DT
)

SELECT	 DISTINCT REPO_PLN_ID
		,PLN_YRWK
		,PLN_SEQ
		,REF_ID
		,@[mty_bkg_no] MTY_BKG_NO
		,TRSP_MOD_CD
		,@[usr_id] CRE_USR_ID
		,SYSDATE CRE_DT
		,@[usr_id] UPD_USR_ID
		,SYSDATE UPD_DT
FROM	${table_name}
WHERE	MTY_BKG_NO	= @[mty_bkg_no]			]]></sql>
			<params>
				<param name="mty_bkg_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
