<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DMTCommonDBDAOSearchUserCntCodeRSQL">
			<desc><![CDATA[국가코드 조회]]></desc>
			<sql><![CDATA[
SELECT S.CNT_CD
  FROM COM_SYS_AREA_GRP_ID S,
       MDM_ORGANIZATION O
 WHERE S.CO_IND_CD = 'H'
   AND S.CNT_CD = SUBSTR (O.LOC_CD, 1, 2)
   AND O.OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
