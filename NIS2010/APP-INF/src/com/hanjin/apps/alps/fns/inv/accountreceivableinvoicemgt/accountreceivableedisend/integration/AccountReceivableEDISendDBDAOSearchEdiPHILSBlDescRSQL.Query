<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchEdiPHILSBlDescRSQL">
			<desc><![CDATA[Get Booking Description.]]></desc>
			<sql><![CDATA[
SELECT NVL(REPLACE(BD.CMDT_DESC,chr(13)||chr(10),' '), REPLACE(MC.CMDT_NM,chr(13)||chr(10),' ')) BL_DESC
FROM   BKG_BOOKING BK,
       BKG_BL_MK_DESC BD,
       MDM_COMMODITY MC
WHERE  BK.BKG_NO = BD.BKG_NO
AND    BK.CMDT_CD = MC.CMDT_CD
AND    BK.BKG_NO = @[bkg_no] 			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
