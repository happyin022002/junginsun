<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchRevenueVVDLaneRdRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchRevenueVVDLaneRdRSQL]]></desc>
			<sql><![CDATA[
SELECT RLANE_CD
     , VSL_CD||SKD_VOY_NO||SKD_DIR_CD||RLANE_DIR_CD REV_VVD
  FROM AR_MST_REV_VVD
 WHERE VSL_CD = DECODE(SUBSTR(@[vvd],1,4), 'COMC', SUBSTR(@[vvd],1,4), 'USAC', SUBSTR(@[vvd],1,4),  DECODE(SUBSTR(@[vvd],1,4), 'CNTC', 'CNTC', 'CFDR'))
   AND SKD_VOY_NO = TO_CHAR(SYSDATE, 'YYMM')			]]></sql>
			<params>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
