			<sql><![CDATA[
DELETE
FROM INV_ISS_CUST_ATCH
WHERE CUST_CNT_CD = @[cust_cnt_cd]
  AND CUST_SEQ = @[cust_seq]
  AND AR_OFC_CD = @[ar_ofc_cd]			]]></sql>
