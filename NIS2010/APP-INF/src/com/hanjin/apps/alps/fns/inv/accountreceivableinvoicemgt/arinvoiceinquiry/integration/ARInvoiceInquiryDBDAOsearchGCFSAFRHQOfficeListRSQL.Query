<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOsearchGCFSAFRHQOfficeListRSQL">
			<desc><![CDATA[searchGCFSAFRHQOfficeList]]></desc>
			<sql><![CDATA[
SELECT DISTINCT(A.AR_HD_QTR_OFC_CD) AR_OFC_CD
FROM MDM_ORGANIZATION A
WHERE A.AR_HD_QTR_OFC_CD IN (
    SELECT AR_HD_QTR_OFC_CD
    FROM MDM_ORGANIZATION
    WHERE DELT_FLG = 'N' )
    AND A.DELT_FLG = 'N'
	AND A.AR_HD_QTR_OFC_CD <>'NYCNA'
  ORDER BY AR_HD_QTR_OFC_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
