<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchTvaFlgByChargeRSQL">
			<desc><![CDATA[office 별 charge code tva flag 여부]]></desc>
			<sql><![CDATA[
SELECT MAX(TVA_FLG) AS TVA_FLG  FROM (
SELECT DECODE(@[ofc_cd],'BOMSC', 
                        DECODE(@[io_bnd_cd],'O', DECODE(@[chg_cd],  'BCC','Y','BLR','Y','CMC','Y','CMS','Y','CSR','Y',
                                                                    'CTS','Y','DAR','Y','DHF','Y','DDF','Y','DIV','Y',
                                                                    'DMR','Y','DRC','Y','DTC','Y','DTS','Y','DVC','Y',
                                                                    'EIS','Y','EMS','Y','ENS','Y','EPS','Y','ETC','Y',
                                                                    'ICS','Y','IHC','Y','MCF','Y','MIS','Y','NFC','Y','NMS','Y','OAR','Y','OBS','Y','OTR','Y','OTS','Y',
                                                                    'PCS','Y','SMC','Y','SOC','Y','TSC','Y','WHF','Y','OTH','Y','OIH','Y','CFR','Y','ITR','Y','CYR','Y',
                                                                    'SLF','Y','INF','Y','DTH','Y','DIH','Y','CFD','Y','ITR','Y','CLN','Y','CLC','Y','PIF','Y','INF','Y',
                                                                    'CMR','Y','OCH','Y','DCH','Y','CCL','Y','CMF','Y','NMC','Y','CHC','Y','CAM','Y','MDA','Y','LBP','Y',
                                                                    'IDS','Y','TAD','Y','CIF','Y','MCF','Y','AFR','Y','OCR','Y','NST','Y','DHC','Y','APS','Y','GOH','Y',
                                                                    'IFR','Y','N'),
                                                 DECODE(@[chg_cd],  'BCC','Y','BLR','Y','CMC','Y','CMS','Y','CSR','Y','CTS','Y','DAR','Y','DHF','Y','DDF','Y','DIV','Y',	
                                                                    'DMR','Y','DRC','Y','DTC','Y','DTS','Y','DVC','Y','EIS','Y','EMS','Y','ENS','Y','EPS','Y','ETC','Y',	
                                                                    'ICS','Y','IHC','Y','MCF','Y','MIS','Y','NFC','Y','NMS','Y','OAR','Y','OBS','Y','OTR','Y','OTS','Y',	
                                                                    'PCS','Y','SMC','Y','SOC','Y','TSC','Y','WHF','Y','OTH','Y','OIH','Y','CFR','Y','ITR','Y','CYR','Y',
                                                                    'SLF','Y','INF','Y','DTH','Y','DIH','Y','CFD','Y','ITR','Y','CLN','Y','CLC','Y','PIF','Y','INF','Y',
                                                                    'CMR','Y','OCH','Y','DCH','Y','CCL','Y','CMF','Y','NMC','Y','CHC','Y','CAM','Y','MDA','Y','LBP','Y',
                                                                    'IDS','Y','TAD','Y','CIF','Y','MCF','Y','AFR','Y','OCR','Y','NST','Y','DHC','Y','OFT','Y','BUC','Y',
                                                                    'LSF','Y','CAF','Y','BAF','Y','WSC','Y','ACT','Y','FRC','Y','FRS','Y','PSC','Y','EFS','Y','FAF','Y',
                                                                    'GRI','Y','TXS','Y','DIS','Y','CTO','Y','CDC','Y','N'))
                      ,'N') AS TVA_FLG                
FROM DUAL
UNION ALL 
SELECT DECODE(@[ofc_cd],'BOMSC', DECODE(@[chg_cd],'MVC',DECODE(@[io_bnd_cd],'O','Y','N'),'N'),'N') AS TVA_FLG FROM DUAL WHERE TO_CHAR(SYSDATE,'YYYYMMDD') >='20160701'
UNION ALL
SELECT DECODE(@[ofc_cd],'BOMSC', DECODE(@[chg_cd],'MUC','Y','N'),'N') AS TVA_FLG FROM DUAL WHERE TO_CHAR(SYSDATE,'YYYYMMDD') >='20160711'
)			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
