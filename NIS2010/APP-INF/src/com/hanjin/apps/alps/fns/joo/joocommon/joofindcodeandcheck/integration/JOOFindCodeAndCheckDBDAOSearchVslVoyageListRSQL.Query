<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchVslVoyageListRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT A.VSL_CD CODE
FROM   VSK_VSL_PORT_SKD A
WHERE  A.VSL_CD     = SUBSTR(@[code],1,4)
AND    A.SKD_VOY_NO = SUBSTR(@[code],5)
AND    ROWNUM=1			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
