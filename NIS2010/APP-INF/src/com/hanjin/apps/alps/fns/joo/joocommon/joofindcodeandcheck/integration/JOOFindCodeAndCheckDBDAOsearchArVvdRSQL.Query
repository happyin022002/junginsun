<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOsearchArVvdRSQL">
			<desc><![CDATA[AR_MST_REV_VVD 에서 Voayge 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 	VSL_CD, SKD_VOY_NO, SKD_DIR_CD
FROM   	VSK_VSL_PORT_SKD
WHERE  	1 = 1
	   	AND	   VSL_CD       = @[super_cd1]
		AND    SKD_VOY_NO   = @[super_cd2]
		AND    SKD_DIR_CD   = @[super_cd3]			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="" out="N"/>
				<param name="super_cd2" type="12" value="" out="N"/>
				<param name="super_cd3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
