<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOSlotHireTgtDSQL">
			<desc><![CDATA[Slot Hire Settlement정보를 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM JOO_SLT_TGT J
WHERE 1=1
AND J.REV_YRMON = @[rev_yrmon]
AND J.REV_YRMON_SEQ = @[rev_yrmon_seq]			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
