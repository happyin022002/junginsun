<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAOUnusedTotVesselVOCSQL">
			<desc><![CDATA[Unused Vessel Over 30 Days insert]]></desc>
			<sql><![CDATA[
INSERT INTO TOT_VESSEL (
	STL_YR
,	VSL_CD
,	VSL_SEQ
,	TONG_FLET_TP_CD
,	CTRT_ST_DT
,	CTRT_END_DT
,	LDB_CAPA_QTY
,	DELT_FLG
,	VSL_RMK
,	CRE_DT
,	CRE_USR_ID
,	UPD_DT
,	UPD_USR_ID
) VALUES( 
	@[stl_yr]
,	@[vsl_cd]
,	(SELECT NVL(MAX(VSL_SEQ)+1,1) FROM TOT_VESSEL WHERE VSL_CD = @[vsl_cd] AND STL_YR = @[stl_yr])
,	'E'
,	TO_DATE(@[ctrt_st_dt],'YYYYMMDD')
,	TO_DATE(@[ctrt_end_dt],'YYYYMMDD')
,	@[ldb_capa_qty]
,	@[delt_flg]
,	@[vsl_rmk]
,	sysdate
,	@[cre_usr_id]
,	sysdate
,	@[cre_usr_id]
)			]]></sql>
			<params>
				<param name="stl_yr" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="ctrt_st_dt" type="12" value="" out="N"/>
				<param name="ctrt_end_dt" type="12" value="" out="N"/>
				<param name="ldb_capa_qty" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="vsl_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
