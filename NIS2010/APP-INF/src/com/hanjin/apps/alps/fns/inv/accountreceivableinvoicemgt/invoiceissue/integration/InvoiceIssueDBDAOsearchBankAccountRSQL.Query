<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOsearchBankAccountRSQL">
			<desc><![CDATA[searchBankAccount]]></desc>
			<sql><![CDATA[
SELECT	A.BANK_ACCT_NO
FROM	INV_AR_BANK A
WHERE	A.OFC_CD = 'SHARC'
AND		A.BANK_ACCT_CURR_CD = @[curr_cd]
ORDER BY A.BANK_ACCT_NO			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
