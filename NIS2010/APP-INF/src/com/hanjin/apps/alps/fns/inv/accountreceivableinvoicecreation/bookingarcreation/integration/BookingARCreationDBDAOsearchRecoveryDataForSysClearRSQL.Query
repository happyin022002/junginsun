<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchRecoveryDataForSysClearRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT  AR_IF_NO
      , INV_NO
      , DUE_DT
      , (
        SELECT ERP_IF_GL_DT
        FROM   INV_AR_AMT S
        WHERE  S.AR_IF_NO = T.AR_IF_NO
        AND    ROWNUM     = 1
        ) AS ERP_IF_GL_DT
      , ISS_DT
      , INV_REF_NO
      , BKG_TEU_QTY
      , BKG_FEU_QTY
      , UPD_USR_ID
      , TO_CHAR(UPD_DT,'YYYYMMDDHH24MISS') UPD_DT
FROM  INV_AR_MN T
WHERE AR_IF_NO IN (
                  SELECT AR_IF_NO
                  FROM   INV_AR_ISS_FTR
                  WHERE  INV_ISS_WRK_NO = @[wrk_no]
                  GROUP BY AR_IF_NO
                  )			]]></sql>
			<params>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
