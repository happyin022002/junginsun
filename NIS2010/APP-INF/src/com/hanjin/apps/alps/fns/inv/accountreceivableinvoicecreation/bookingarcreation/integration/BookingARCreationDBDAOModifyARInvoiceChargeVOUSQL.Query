<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyARInvoiceChargeVOUSQL">
			<desc><![CDATA[INV_AR_CHG]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_CHG SET 
	MF_DIV_CD = NVL(@[mf_div_cd], MF_DIV_CD)
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
,	CHG_CD = @[chg_cd]
,	CURR_CD = @[curr_cd]
,	PER_TP_CD = @[per_tp_cd]
,	TRF_RT_AMT = @[trf_rt_amt]
,	RAT_AS_CNTR_QTY = @[rat_as_cntr_qty]
,	CHG_AMT = @[chg_amt]
,	INV_XCH_RT = NVL(@[inv_xch_rt], 0)
,   REV_COA_CO_CD = NVL(@[rev_coa_co_cd],REV_COA_CO_CD)
,   REV_COA_RGN_CD = NVL(@[rev_coa_rgn_cd],REV_COA_RGN_CD)
,   REV_COA_CTR_CD = NVL(@[rev_coa_ctr_cd],REV_COA_CTR_CD)
,   REV_COA_ACCT_CD = NVL(@[rev_coa_acct_cd],REV_COA_ACCT_CD)
,   REV_COA_INTER_CO_CD = NVL(@[rev_coa_inter_co_cd],REV_COA_INTER_CO_CD)
,   REV_COA_VSL_CD = NVL(DECODE(@[rev_coa_acct_cd], null, '', DECODE(SUBSTR(@[rev_coa_acct_cd],1,1), '4', @[rev_coa_vsl_cd], '7', @[rev_coa_vsl_cd], DECODE(SUBSTR(@[rev_coa_acct_cd],1,2), '51', @[rev_coa_vsl_cd], '0000'))),REV_COA_VSL_CD)
,   REV_COA_VOY_NO = NVL(DECODE(@[rev_coa_acct_cd], null, '', DECODE(SUBSTR(@[rev_coa_acct_cd],1,1), '4', @[rev_coa_voy_no], '7', @[rev_coa_voy_no], DECODE(SUBSTR(@[rev_coa_acct_cd],1,2), '51', @[rev_coa_voy_no], '0000'))),REV_COA_VOY_NO)
,   REV_COA_SKD_DIR_CD = NVL(DECODE(@[rev_coa_acct_cd], null, '', DECODE(SUBSTR(@[rev_coa_acct_cd],1,1), '4', @[rev_coa_skd_dir_cd], '7', @[rev_coa_skd_dir_cd], DECODE(SUBSTR(@[rev_coa_acct_cd],1,2), '51', @[rev_coa_skd_dir_cd], '0'))),REV_COA_SKD_DIR_CD)
,   REV_COA_DIR_CD = NVL(DECODE(@[rev_coa_acct_cd], null, '', DECODE(SUBSTR(@[rev_coa_acct_cd],1,1), '4', @[rev_coa_dir_cd], '7', @[rev_coa_dir_cd], DECODE(SUBSTR(@[rev_coa_acct_cd],1,2), '51', @[rev_coa_dir_cd], '0'))),REV_COA_DIR_CD)
,   ACCT_CD = NVL(@[acct_cd],ACCT_CD)   
,	TVA_FLG = DECODE(@[tva_flg],'1','Y','0','N','',TVA_FLG,@[tva_flg])
,	INV_REV_TP_SRC_CD = NVL(@[inv_rev_tp_src_cd],INV_REV_TP_SRC_CD)   
WHERE	AR_IF_NO = @[ar_if_no]
AND AR_IF_SER_NO = @[ar_if_ser_no]
AND CHG_SEQ = @[chg_seq]			]]></sql>
			<params>
				<param name="mf_div_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="per_tp_cd" type="12" value="" out="N"/>
				<param name="trf_rt_amt" type="6" value="" out="N"/>
				<param name="rat_as_cntr_qty" type="6" value="" out="N"/>
				<param name="chg_amt" type="6" value="" out="N"/>
				<param name="inv_xch_rt" type="6" value="" out="N"/>
				<param name="rev_coa_co_cd" type="12" value="" out="N"/>
				<param name="rev_coa_rgn_cd" type="12" value="" out="N"/>
				<param name="rev_coa_ctr_cd" type="12" value="" out="N"/>
				<param name="rev_coa_acct_cd" type="12" value="" out="N"/>
				<param name="rev_coa_inter_co_cd" type="12" value="" out="N"/>
				<param name="rev_coa_vsl_cd" type="12" value="" out="N"/>
				<param name="rev_coa_voy_no" type="12" value="" out="N"/>
				<param name="rev_coa_skd_dir_cd" type="12" value="" out="N"/>
				<param name="rev_coa_dir_cd" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="tva_flg" type="12" value="" out="N"/>
				<param name="inv_rev_tp_src_cd" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="ar_if_ser_no" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
