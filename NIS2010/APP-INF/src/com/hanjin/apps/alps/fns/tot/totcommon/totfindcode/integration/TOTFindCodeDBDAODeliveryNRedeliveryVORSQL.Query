<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TOTFindCodeDBDAODeliveryNRedeliveryVORSQL">
			<desc><![CDATA[선박명, 계약시작일자, 종료일자를 가져옴]]></desc>
			<sql><![CDATA[
SELECT A.VSL_ENG_NM
      ,TO_CHAR(B.EFF_DT,'YYYYMMDD') EFF_DT
      ,TO_CHAR(B.EXP_DT,'YYYYMMDD') EXP_DT
FROM MDM_VSL_CNTR A, 
     (SELECT FLET_CTRT_NO, VSL_CD, EFF_DT, EXP_DT
      FROM  FMS_CONTRACT
      WHERE FLET_CTRT_NO = (SELECT MAX(FLET_CTRT_NO) FROM FMS_CONTRACT WHERE VSL_CD = @[vsl_cd])
      ) B
WHERE A.VSL_CD = @[vsl_cd]
  AND A.DELT_FLG = 'N'
  AND A.VSL_CD = B.VSL_CD(+)			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="HJBJ" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
