<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOcreateInvoiceMappingVOCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO INV_AR_ISS_DTL (
       INV_NO
     , AR_IF_NO
     , CHG_SEQ
     , CRE_USR_ID
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
) 
SELECT @[inv_no]
     , B.AR_IF_NO
     , B.CHG_SEQ
     , @[user_id]
     , SYSDATE
     , @[user_id]
     , SYSDATE
FROM    INV_AR_MN A,INV_AR_CHG B
WHERE   A.AR_IF_NO = B.AR_IF_NO
AND     A.BL_SRC_NO = @[bl_no]
AND     A.AR_OFC_CD = @[ofc_cd] 
AND     A.AR_IF_NO = @[ar_if_no]
#if (${curr_cd} != '') 
AND		B.CURR_CD = @[curr_cd]
#end			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
