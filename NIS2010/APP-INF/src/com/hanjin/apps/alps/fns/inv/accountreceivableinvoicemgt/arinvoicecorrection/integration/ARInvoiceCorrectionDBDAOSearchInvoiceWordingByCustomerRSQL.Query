<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchInvoiceWordingByCustomerRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.CUST_CNT_CD,
  LPAD(A.CUST_SEQ, 6, '0') CUST_SEQ,
  CUST_LGL_ENG_NM CUST_NM,
  A.AR_OFC_CD,
  (
    SELECT INV_ISS_CTNT
    FROM INV_ISS_CUST_ATCH S1
    WHERE S1.CUST_CNT_CD = @[cust_cnt_cd]
      AND S1.CUST_SEQ = @[cust_seq]
      AND S1.AR_OFC_CD = @[ar_ofc_cd]
      AND S1.TXT_NO = 1) SUBJECT,
  (
    SELECT INV_ISS_CTNT
    FROM INV_ISS_CUST_ATCH S2
    WHERE S2.CUST_CNT_CD = @[cust_cnt_cd]
      AND S2.CUST_SEQ = @[cust_seq]
      AND S2.AR_OFC_CD = @[ar_ofc_cd]
      AND S2.TXT_NO = 2) TEXT1,
  (
    SELECT FONT_BOLD_FLG
    FROM INV_ISS_CUST_ATCH S2
    WHERE S2.CUST_CNT_CD = @[cust_cnt_cd]
      AND S2.CUST_SEQ = @[cust_seq]
      AND S2.AR_OFC_CD = @[ar_ofc_cd]
      AND S2.TXT_NO = 2) HIGH_LIGHT1,
  (
    SELECT INV_ISS_CTNT
    FROM INV_ISS_CUST_ATCH S3
    WHERE S3.CUST_CNT_CD = @[cust_cnt_cd]
      AND S3.CUST_SEQ = @[cust_seq]
      AND S3.AR_OFC_CD = @[ar_ofc_cd]
      AND S3.TXT_NO = 3) TEXT2,
  (
    SELECT FONT_BOLD_FLG
    FROM INV_ISS_CUST_ATCH S3
    WHERE S3.CUST_CNT_CD = @[cust_cnt_cd]
      AND S3.CUST_SEQ = @[cust_seq]
      AND S3.AR_OFC_CD = @[ar_ofc_cd]
      AND S3.TXT_NO = 3) HIGH_LIGHT2,
  (
    SELECT INV_ISS_CTNT
    FROM INV_ISS_CUST_ATCH S4
    WHERE S4.CUST_CNT_CD = @[cust_cnt_cd]
      AND S4.CUST_SEQ = @[cust_seq]
      AND S4.AR_OFC_CD = @[ar_ofc_cd]
      AND S4.TXT_NO = 4) TEXT3,
  (
    SELECT FONT_BOLD_FLG
    FROM INV_ISS_CUST_ATCH S4
    WHERE S4.CUST_CNT_CD = @[cust_cnt_cd]
      AND S4.CUST_SEQ = @[cust_seq]
      AND S4.AR_OFC_CD = @[ar_ofc_cd]
      AND S4.TXT_NO = 4) HIGH_LIGHT3,
  (
    SELECT INV_ISS_CTNT
    FROM INV_ISS_CUST_ATCH S5
    WHERE S5.CUST_CNT_CD = @[cust_cnt_cd]
      AND S5.CUST_SEQ = @[cust_seq]
      AND S5.AR_OFC_CD = @[ar_ofc_cd]
      AND S5.TXT_NO = 5) TEXT4,
  (
    SELECT FONT_BOLD_FLG
    FROM INV_ISS_CUST_ATCH S5
    WHERE S5.CUST_CNT_CD = @[cust_cnt_cd]
      AND S5.CUST_SEQ = @[cust_seq]
      AND S5.AR_OFC_CD = @[ar_ofc_cd]
      AND S5.TXT_NO = 5) HIGH_LIGHT4
FROM INV_ISS_CUST_ATCH A,
  MDM_CUSTOMER B
WHERE A.CUST_CNT_CD = B.CUST_CNT_CD
  AND A.CUST_SEQ = B.CUST_SEQ
  AND A.CUST_CNT_CD = @[cust_cnt_cd]
  AND A.CUST_SEQ = @[cust_seq]
  AND A.AR_OFC_CD = @[ar_ofc_cd]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
