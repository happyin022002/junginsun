<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAORecalculateNrtForModiFlgVORSQL">
			<desc><![CDATA[bsa 테이블의 modify flag 체크]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
	   SUBSTR(@[stl_yrmon],1,4) AS STL_YRMON 
      ,V.VSL_CD
      ,V.NET_RGST_TONG_WGT AS NRT_WGT
      ,@[upd_usr_id] AS UPD_USR_ID
  FROM TOT_VVD_STL_AMT T, MDM_VSL_CNTR V
 WHERE T.STL_YRMON LIKE SUBSTR(@[stl_yrmon],1,4)||'%'
   AND T.VSL_CD  =  V.VSL_CD
   AND T.NRT_WGT <> V.NET_RGST_TONG_WGT			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
