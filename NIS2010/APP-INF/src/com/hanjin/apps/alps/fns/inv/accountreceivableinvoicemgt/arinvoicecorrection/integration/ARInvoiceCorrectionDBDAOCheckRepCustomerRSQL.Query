<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOCheckRepCustomerRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM MDM_ORGANIZATION 
WHERE REP_CUST_CNT_CD = @[act_cust_cnt_cd]
AND  REP_CUST_SEQ = @[act_cust_seq]			]]></sql>
			<params>
				<param name="act_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="act_cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
