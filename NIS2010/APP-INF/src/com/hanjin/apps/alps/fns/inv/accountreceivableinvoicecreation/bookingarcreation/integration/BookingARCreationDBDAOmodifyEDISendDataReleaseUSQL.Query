<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOmodifyEDISendDataReleaseUSQL">
			<desc><![CDATA[VLCSC의 EDI 다운로드 했던 데이터에 대해서 INV_AR_MN의 EDI_SND_DT 값을 Null 를 update 한다.]]></desc>
			<sql><![CDATA[
UPDATE  INV_AR_MN
SET     EDI_SND_DT = NULL
WHERE   EDI_SND_DT IS NOT NULL
AND     AR_IF_NO    IN (SELECT  AR_IF_NO
                        FROM    INV_AR_ISS_DTL
                        WHERE   INV_NO  BETWEEN @[fm_inv_no] AND @[to_inv_no]
                        GROUP BY AR_IF_NO
                       )			]]></sql>
			<params>
				<param name="fm_inv_no" type="12" value="" out="N"/>
				<param name="to_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
