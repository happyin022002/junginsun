<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOInvArEmlCustRgstUSQL">
			<desc><![CDATA[Update InvArEmlCustRgst]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_EML_CUST_RGST
SET INV_EML_SPLIT_FLG = @[inv_eml_split_flg],
    CUST_RMK = @[cust_rmk],
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT = SYSDATE
WHERE
    AR_OFC_CD = @[ar_ofc_cd]
    AND CUST_CNT_CD = @[cust_cnt_cd]
    AND CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="inv_eml_split_flg" type="12" value="" out="N"/>
				<param name="cust_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
