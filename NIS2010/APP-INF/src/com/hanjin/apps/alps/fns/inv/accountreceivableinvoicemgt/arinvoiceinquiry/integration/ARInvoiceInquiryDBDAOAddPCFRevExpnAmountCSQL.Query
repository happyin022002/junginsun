<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOAddPCFRevExpnAmountCSQL">
			<desc><![CDATA[Add PCF Rev Expn Amount]]></desc>
			<sql><![CDATA[
INSERT INTO INV_PCF_REV_EXPN_AMT
(
      PORT_CD
    , RE_DIVR_CD
    , RE_PORT_SEQ
    , PCF_UT_AMT1
    , PCF_UT_AMT2
    , PCF_UT_AMT3
    , PCF_UT_AMT4
    , PCF_UT_AMT5
    , PCF_UT_AMT6
    , PCF_UT_AMT7
    , PCF_UT_AMT8
    , FM_EFF_DT
    , TO_EFF_DT
    , CRE_USR_ID
    , CRE_DT
    , UPD_USR_ID
    , UPD_DT

)
VALUES
(
      @[port_cd]
    , @[re_divr_cd]
    , (SELECT NVL(MAX(RE_PORT_SEQ), 0) + 1
       FROM INV_PCF_REV_EXPN_AMT
       WHERE PORT_CD = @[port_cd]
       AND RE_DIVR_CD = @[re_divr_cd])
    , @[pcf_ut_amt1]
    , @[pcf_ut_amt2]
    , @[pcf_ut_amt3]
    , @[pcf_ut_amt4]
    , @[pcf_ut_amt5]
    , @[pcf_ut_amt6]
    , @[pcf_ut_amt7]
    , @[pcf_ut_amt8]
    , @[fm_eff_dt]
    , @[to_eff_dt]
    , @[cre_usr_id]
    , SYSDATE
    , @[upd_usr_id]
    , SYSDATE
)			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="pcf_ut_amt1" type="12" value="" out="N"/>
				<param name="pcf_ut_amt2" type="12" value="" out="N"/>
				<param name="pcf_ut_amt3" type="12" value="" out="N"/>
				<param name="pcf_ut_amt4" type="12" value="" out="N"/>
				<param name="pcf_ut_amt5" type="12" value="" out="N"/>
				<param name="pcf_ut_amt6" type="12" value="" out="N"/>
				<param name="pcf_ut_amt7" type="12" value="" out="N"/>
				<param name="pcf_ut_amt8" type="12" value="" out="N"/>
				<param name="fm_eff_dt" type="12" value="" out="N"/>
				<param name="to_eff_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
