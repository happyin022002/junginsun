<sqls>
	<querys>
		<query name="JointOperationAccrualCreationDBDAOSearchEstmPerformanceChangeStatusCSQL">
			<desc><![CDATA[FNS_JOO_0088 Estimate Performance Change Status Inquiry]]></desc>
			<sql><![CDATA[
INSERT INTO JOO_ESTM_ACT_RSLT_ANAL (
	ADJ_RMK
	, EXE_YRMON
	, JB_EXE_YRMON
	, REV_YRMON
	, JO_ESTM_ANAL_ID
	, JO_CRR_CD
	, VSL_CD
	, SKD_VOY_NO
	, SKD_DIR_CD
	, REV_DIR_CD
	, RLANE_CD
	, JO_STL_JB_CD
	, ACCT_CD
	, CRE_DT
	, CRE_USR_ID
	, UPD_DT
	, UPD_USR_ID
) VALUES (
	@[adj_rmk]
	, @[exe_yrmon]
	, @[jb_exe_yrmon]
	, @[rev_yrmon]
	, @[estm_option]
	, @[jo_crr_cd]
	, SUBSTR(@[vvd], 0, 4)
	, SUBSTR(@[vvd], 5, 4)
	, SUBSTR(@[vvd], 9, 1)
	, SUBSTR(@[vvd], 10, 1)
	, @[rlane_cd]
	, DECODE(@[jo_stl_jb_cd], 'JOINT OPERATION', '101', 'LEASE', '102', 'ADDITIONAL', '103')
	, @[acct_cd]
	, SYSDATE
	, @[cre_usr_id]
	, SYSDATE
	, @[cre_usr_id]
)			]]></sql>
			<params>
				<param name="adj_rmk" type="12" value="" out="N"/>
				<param name="exe_yrmon" type="12" value="" out="N"/>
				<param name="jb_exe_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="estm_option" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="jo_stl_jb_cd" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
