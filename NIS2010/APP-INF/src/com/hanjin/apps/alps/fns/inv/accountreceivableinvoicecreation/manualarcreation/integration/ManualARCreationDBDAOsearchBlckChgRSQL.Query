<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOsearchBlckChgRSQL">
			<desc><![CDATA[ManualARCreationDBDAOsearchBlckChgRSQL]]></desc>
			<sql><![CDATA[
SELECT CHG_CD
  FROM INV_AR_MISC_BLCK_CHG A
 WHERE A.AR_OFC_CD = @[ofc_cd]
   AND A.CHG_CD = @[chg_cd]
 UNION  
SELECT B.CHG_CD
  FROM INV_AR_LOCL_CHG B
 WHERE B.AR_OFC_CD = @[ofc_cd]
   AND B.CHG_CD = @[chg_cd]
   AND EXISTS (SELECT 1 FROM INV_AR_STUP_OFC WHERE AR_OFC_CD = B.AR_OFC_CD AND MRI_LOCL_CHG_APLY_FLG ='Y')			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
