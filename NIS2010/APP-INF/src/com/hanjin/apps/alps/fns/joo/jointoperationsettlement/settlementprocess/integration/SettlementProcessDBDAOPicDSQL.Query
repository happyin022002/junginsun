<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOPicDSQL">
			<desc><![CDATA[PIC 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM JOO_STL_PIC J
WHERE 1=1
AND J.TRD_CD = @[trd_cd]
AND J.CRR_CD = @[crr_cd]
AND J.RLANE_CD = @[rlane_cd]
AND J.RE_DIVR_CD = @[re_divr_cd]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
