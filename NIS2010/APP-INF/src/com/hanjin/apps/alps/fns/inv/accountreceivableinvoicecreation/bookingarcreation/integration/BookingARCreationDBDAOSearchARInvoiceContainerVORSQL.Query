<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchARInvoiceContainerVORSQL">
			<desc><![CDATA[[] BookingARCreationDBDAO::searchARInvoiceContainer ( ifNo )  INV_AR_CNTR에서 AR_IF_NO로 조회함.  InvArCntrVO[]]]></desc>
			<sql><![CDATA[
SELECT 
	AR_IF_NO
,	CNTR_SEQ
,	CNTR_NO
,	CNTR_TPSZ_CD
FROM INV_AR_CNTR
WHERE	AR_IF_NO = @[if_no]			]]></sql>
			<params>
				<param name="if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
