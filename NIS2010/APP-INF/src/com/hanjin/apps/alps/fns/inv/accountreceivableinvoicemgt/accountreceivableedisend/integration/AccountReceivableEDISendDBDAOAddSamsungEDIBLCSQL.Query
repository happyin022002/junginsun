<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOAddSamsungEDIBLCSQL">
			<desc><![CDATA[GERP BL 테이블에 insert.]]></desc>
			<sql><![CDATA[
INSERT
INTO INV_AR_EDI_GERP_BL (MSG_ID, MSG_NO, BL_LINE_NO, BL_SRC_NO, FCTRY_CTNT, FCTRY_NM, DIV_CTNT, DIV_NM, SR_INV_NO, GERP_TRSP_CD, GERP_TRSP_NM, OBRD_DT, GERP_CRR_CTNT, GERP_CRR_NM, POR_CD, POL_CD, POD_CD, DEL_CD, ISS_DT, INV_EDI_D2_QTY, INV_EDI_D4_QTY, INV_EDI_D5_QTY, INV_EDI_D7_QTY, INV_EDI_ETC_QTY, INV_EDI_R2_QTY, INV_EDI_R4_QTY, INV_EDI_F4_QTY, GRS_CNTR_WGT, GRS_CBM_CAPA, SND_FLG, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT, BL_CNTR_GRP_CTNT)
VALUES (@[msg_id],
      @[msg_no],
      @[bl_line_no],
      @[bl_src_no],
      ( SELECT NVL(MAX(B.FCTRY_CTNT), ' ')
        FROM   BKG_XTER_RQST_MISC B
        WHERE  B.XTER_RQST_NO =  @[sr_inv_no_org] ),
      (
        SELECT NVL(MAX(B.FCTRY_NM), ' ')
        FROM   BKG_XTER_RQST_MISC B
        WHERE  B.XTER_RQST_NO =  @[sr_inv_no_org] ) ,
      (
        SELECT NVL(MAX(B.DIV_CTNT), ' ')
        FROM   BKG_XTER_RQST_MISC B
        WHERE  B.XTER_RQST_NO =  @[sr_inv_no_org] ),
      (
        SELECT NVL(MAX(B.DIV_NM), ' ')
        FROM   BKG_XTER_RQST_MISC B
        WHERE  B.XTER_RQST_NO =  @[sr_inv_no_org] ),
      @[sr_inv_no],
      (
        SELECT NVL(MAX(B.TS_TP_CTNT ), '04')
        FROM INV_AR_MN A,
          BKG_XTER_RQST_MISC B
        WHERE A.BKG_REF_NO = B.XTER_RQST_NO
          AND A.BL_SRC_NO = @[bl_src_no]
          AND A.BKG_REF_NO = @[sr_inv_no_org]
          AND ROWNUM = 1),
      (
        SELECT NVL(MAX(B.TS_NM ), 'SEA')
        FROM INV_AR_MN A,
          BKG_XTER_RQST_MISC B
        WHERE A.BKG_REF_NO = B.XTER_RQST_NO
          AND A.BL_SRC_NO = @[bl_src_no]
          AND A.BKG_REF_NO = @[sr_inv_no_org]
          AND ROWNUM = 1),
      @[obrd_dt],
      @[gerp_crr_cd],
      @[gerp_crr_nm],
      @[por_cd],
      @[pol_cd],
      @[pod_cd],
      @[del_cd],
      (SELECT BIL_DT 
         FROM INV_AR_EDI_GERP_HDR 
        WHERE MSG_ID = @[msg_id]
          AND MSG_NO = @[msg_no]), 
      @[inv_edi_d2_qty],
      @[inv_edi_d4_qty],
      @[inv_edi_d5_qty],
      @[inv_edi_d7_qty],
      @[inv_edi_etc_qty],
      0,
      0,
      0,
      @[grs_cntr_wgt],
      @[grs_cbm_capa],
      'N',
      @[cre_usr_id],
      SYSDATE,
      @[upd_usr_id],
      SYSDATE,
	  @[bl_cntr_grp_ctnt])			]]></sql>
			<params>
				<param name="msg_id" type="12" value="" out="N"/>
				<param name="msg_no" type="12" value="" out="N"/>
				<param name="bl_line_no" type="12" value="" out="N"/>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="sr_inv_no_org" type="12" value="" out="N"/>
				<param name="sr_inv_no" type="12" value="" out="N"/>
				<param name="obrd_dt" type="12" value="" out="N"/>
				<param name="gerp_crr_cd" type="12" value="" out="N"/>
				<param name="gerp_crr_nm" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="inv_edi_d2_qty" type="12" value="" out="N"/>
				<param name="inv_edi_d4_qty" type="12" value="" out="N"/>
				<param name="inv_edi_d5_qty" type="12" value="" out="N"/>
				<param name="inv_edi_d7_qty" type="12" value="" out="N"/>
				<param name="inv_edi_etc_qty" type="12" value="" out="N"/>
				<param name="grs_cntr_wgt" type="12" value="" out="N"/>
				<param name="grs_cbm_capa" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="bl_cntr_grp_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
