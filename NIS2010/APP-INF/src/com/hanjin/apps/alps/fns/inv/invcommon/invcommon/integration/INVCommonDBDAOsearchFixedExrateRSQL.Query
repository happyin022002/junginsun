<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOsearchFixedExrateRSQL">
			<desc><![CDATA[searchFixedExrate]]></desc>
			<sql><![CDATA[
SELECT NVL(FX_CURR_RT, '0') EX_RATE
  FROM MDM_ORGANIZATION
 WHERE OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
