<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOCombinedRlaneRSQL">
			<desc><![CDATA[조회조건에 맞는 RLANE 과 CMB_CFM_FLG를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
       A.RLANE_CD
      ,NVL(A.CMB_CFM_FLG,'N') AS CMB_CFM_FLG
      ,C.JO_CRR_AUTH_CD AS STL_DUP_FLG -- TABLE VO이므로 임의로 쓰자 ㅡ,.ㅡ
FROM   JOO_SETTLEMENT  A,
       JOO_STL_CMB_DTL B,
       JOO_CRR_AUTH    C
WHERE  A.ACCT_YRMON  = B.ACCT_YRMON (+)
AND    A.STL_VVD_SEQ = B.STL_VVD_SEQ(+)
AND    A.STL_SEQ     = B.STL_SEQ    (+)
AND    A.JO_CRR_CD   = C.JO_CRR_CD
AND    A.RLANE_CD    = C.RLANE_CD
AND    C.AUTH_OFC_CD = @[ofc_cd]
AND    C.DELT_FLG    = 'N'
--2010.01.28 WRITE권한만 조회되게 한다 BY 권상준 수석
AND    C.JO_CRR_AUTH_CD = 'W'
AND    A.ACCT_YRMON = REPLACE(@[acct_yrmon],'-','')
AND    A.JO_CRR_CD  = @[jo_crr_cd]
#if (${trd_cd} != '')
AND    A.TRD_CD     = @[trd_cd]
#end
#if (${re_divr_cd} != '')
AND    A.RE_DIVR_CD = @[re_divr_cd]
#end
#if (${stl_cmb_seq} != '')
AND    B.STL_CMB_SEQ = @[stl_cmb_seq]
#else
AND    NVL(A.CMB_CFM_FLG,'N') = 'N'
#end
GROUP  BY
       A.RLANE_CD
      ,NVL(A.CMB_CFM_FLG,'N')
      ,C.JO_CRR_AUTH_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="SELFAR" out="N"/>
				<param name="acct_yrmon" type="12" value="2009-05" out="N"/>
				<param name="jo_crr_cd" type="12" value="COS" out="N"/>
				<param name="trd_cd" type="12" value="AES" out="N"/>
				<param name="re_divr_cd" type="12" value="R" out="N"/>
				<param name="stl_cmb_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
