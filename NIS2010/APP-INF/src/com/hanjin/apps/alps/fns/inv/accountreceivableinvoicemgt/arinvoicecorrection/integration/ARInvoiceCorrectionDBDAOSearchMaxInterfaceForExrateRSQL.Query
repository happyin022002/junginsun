<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchMaxInterfaceForExrateRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT MAX(AR_IF_NO) AR_IF_NO
  FROM INV_AR_MN
 WHERE AR_OFC_CD = @[ar_ofc_cd]
   AND BL_SRC_NO = @[bl_no]
   AND INV_NO = @[inv_no]
   AND REV_TP_CD<> 'M'
   AND NVL(INV_DELT_DIV_CD,'N') <> 'Y'
   AND NVL(INV_SPLIT_CD,'N') NOT IN ('M','X')
   AND NOT EXISTS (SELECT 1 FROM INV_AR_MN
                    WHERE AR_IF_NO > ( SELECT MAX(AR_IF_NO) AR_IF_NO
  										 FROM INV_AR_MN
 										WHERE AR_OFC_CD = @[ar_ofc_cd]
   										  AND BL_SRC_NO = @[bl_no]
   										  AND INV_NO = @[inv_no]
   										  AND REV_TP_CD<> 'M'
   										  AND NVL(INV_DELT_DIV_CD,'N') <> 'Y'
   										  AND NVL(INV_SPLIT_CD,'N') NOT IN ('M','X')) 
                      AND AR_OFC_CD = @[ar_ofc_cd]
   					  AND BL_SRC_NO = @[bl_no]
   					  AND REV_TP_CD<> 'M'
   					  AND NVL(INV_DELT_DIV_CD,'N') <> 'Y'
   					  AND NVL(INV_SPLIT_CD,'N') NOT IN ('M','X')
					  AND INV_ISS_FLG = 'N')			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_no" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
