<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOJooAddSlotMgrVODSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE joo_fx_agmt
SET delt_flg = 'Y'
,  UPD_DT    = sysdate                   
,  UPD_USR_ID  = @[upd_usr_id]  
WHERE	rlane_cd = @[rlane_cd]
AND jo_crr_cd =  @[acctg_crr_cd]
AND	trd_cd = @[trd_cd]
AND	re_divr_cd = @[rev_divr]
AND fx_agmt_seq = @[agmt_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="acctg_crr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rev_divr" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
