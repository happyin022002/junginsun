<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceExRateMgtDBDAOSearchCustomerMonExRateVORSQL">
			<desc><![CDATA[customer per month duplicate check]]></desc>
			<sql><![CDATA[
SELECT 
COUNT(*) CNT
FROM INV_CUST_AND_DLY_XCH_RT
WHERE	CUST_CNT_CD = @[cust_cnt_cd]
AND	CUST_SEQ = @[cust_seq]
AND
(
	SUBSTR(FM_DT,0,6) = @[mon]
	OR	
	SUBSTR(TO_DT,0,6) = @[mon]
)			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="CN" out="N"/>
				<param name="cust_seq" type="2" value="00382" out="N"/>
				<param name="mon" type="12" value="200805" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
