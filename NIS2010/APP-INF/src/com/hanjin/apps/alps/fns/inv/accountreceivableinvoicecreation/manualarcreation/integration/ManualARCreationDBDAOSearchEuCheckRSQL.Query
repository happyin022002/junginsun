<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOSearchEuCheckRSQL">
			<desc><![CDATA[SearchEuCheck]]></desc>
			<sql><![CDATA[
SELECT DECODE (SUM(CONTI_CD), 0 , 'N', 1,'N','Y')  EU_CHECK 
FROM(
    SELECT COUNT(CONTI_CD) CONTI_CD
    FROM MDM_COUNTRY MCNT
       , MDM_SUBCONTINENT SCNT
    WHERE MCNT.CNT_CD = SUBSTR(@[pol_cd], 1,2)
    AND MCNT.SCONTI_CD = SCNT.SCONTI_CD
    AND SCNT.CONTI_CD = 'E' 
    UNION ALL
    SELECT COUNT(CONTI_CD) CONTI_CD
    FROM MDM_COUNTRY MCNT
       , MDM_SUBCONTINENT SCNT
    WHERE MCNT.CNT_CD = SUBSTR(@[pod_cd], 1,2)
     AND MCNT.SCONTI_CD = SCNT.SCONTI_CD
     AND SCNT.CONTI_CD = 'E' 
)			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
