<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchChinaCustomerCodeByVesselRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT CUST_CNT_CD ACT_CUST_CNT_CD, 
       CUST_SEQ ACT_CUST_SEQ
  FROM INV_VSL_AGN_CUST_CD
 WHERE VSL_CD = @[vsl_cd]
   AND NVL(AR_OFC_CD, 'NONE') = @[ar_ofc_cd]
   AND NVL(DELT_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
