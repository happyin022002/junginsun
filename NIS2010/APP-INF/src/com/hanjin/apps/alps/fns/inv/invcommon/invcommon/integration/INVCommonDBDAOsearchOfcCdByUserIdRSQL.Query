<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOsearchOfcCdByUserIdRSQL">
			<desc><![CDATA[COM_USER에서 ID에 대한 OFC_CD를 조회한다.]]></desc>
			<sql><![CDATA[
select OFC_CD 
from com_user 
where USR_ID = @[user_id]			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
