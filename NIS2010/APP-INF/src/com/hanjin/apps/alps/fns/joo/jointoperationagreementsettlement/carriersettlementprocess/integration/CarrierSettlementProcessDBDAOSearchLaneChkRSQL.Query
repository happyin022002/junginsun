<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOSearchLaneChkRSQL">
			<desc><![CDATA[Lane이  적합한지 조회]]></desc>
			<sql><![CDATA[
SELECT
DECODE(WM_CONCAT(LANE),NULL,'Y',WM_CONCAT(LANE)) AS LANE
FROM
(
    SELECT
    AA.LANE
    FROM
    (
        SELECT
         A.LANE
        ,DECODE(A.LANE, M.VSL_SLAN_CD,1,0) AS LANE_YN_CNT
        FROM
        (
			#foreach( ${key} in ${arrLane})
				#if($velocityCount == $arrLane.size())
					SELECT '${key}' AS LANE FROM DUAL
				#else
				 	SELECT '${key}' AS LANE FROM DUAL UNION ALL
				#end
			#end
        ) A, MDM_VSL_SVC_LANE M
        WHERE 1=1
        AND A.LANE = M.VSL_SLAN_CD(+)
    ) AA
    WHERE 1=1
    AND AA.LANE_YN_CNT != 1
) AAA			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
