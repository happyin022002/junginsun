<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOSearchPCFRevExpnAmountRSQL">
			<desc><![CDATA[Search PCF Rev Expn Amount]]></desc>
			<sql><![CDATA[
SELECT PORT_CD
     , RE_DIVR_CD
     , RE_PORT_SEQ
     , 'CNY' CURR_CD
     , PCF_UT_AMT1
     , PCF_UT_AMT2
     , PCF_UT_AMT3
     , PCF_UT_AMT4
     , PCF_UT_AMT5
     , PCF_UT_AMT6
     , PCF_UT_AMT7
     , PCF_UT_AMT8
     , FM_EFF_DT
     , TO_EFF_DT
     , TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
     , UPD_USR_ID
FROM INV_PCF_REV_EXPN_AMT
WHERE PORT_CD = @[port_cd]
AND RE_DIVR_CD = @[re_divr_cd]
ORDER BY RE_PORT_SEQ DESC			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
