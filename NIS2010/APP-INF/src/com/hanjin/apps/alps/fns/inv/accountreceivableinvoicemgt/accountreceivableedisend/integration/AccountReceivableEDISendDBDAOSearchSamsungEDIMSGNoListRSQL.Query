<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchSamsungEDIMSGNoListRSQL">
			<desc><![CDATA[VVD와 Customer로 기 생성된 EDI 문서번호(message no) list를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT A.MSG_ID,
  A.MSG_NO
FROM INV_AR_EDI_GERP_HDR A
WHERE A.MSG_ID = 'FREINV'
  AND A.RCVR_ID = @[rcvr_id]
  AND A.CUST_CNT_CD = @[act_cust_cnt_cd]
  AND A.CUST_SEQ = @[act_cust_seq]
  AND A.VSL_CD = @[vsl_cd]
  AND A.SKD_VOY_NO = @[skd_voy_no]
  AND A.SKD_DIR_CD = @[skd_dir_cd]
  AND A.DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="rcvr_id" type="12" value="" out="N"/>
				<param name="act_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="act_cust_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
