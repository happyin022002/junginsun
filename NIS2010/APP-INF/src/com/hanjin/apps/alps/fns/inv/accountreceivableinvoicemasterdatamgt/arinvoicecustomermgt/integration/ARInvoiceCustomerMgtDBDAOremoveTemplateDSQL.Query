<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOremoveTemplateDSQL">
			<desc><![CDATA[removeTemplate]]></desc>
			<sql><![CDATA[
DELETE 	FROM INV_CPRT_TMPLT
WHERE	RPT_TMPLT_NM = @[rpt_tmplt_nm]
AND		AR_OFC_CD = @[ar_ofc_cd]			]]></sql>
			<params>
				<param name="rpt_tmplt_nm" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
