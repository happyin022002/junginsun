<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOsearchPrintSplitInvoiceRSQL">
			<desc><![CDATA[InvoiceIssueDBDAOsearchPrintSplitInvoiceRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.INV_NO
     , 'N' INV_ISS_CMB_FLG
     , C.IO_BND_CD IO_BND_CD
	 , 'N' VN_INV_PAY_MZD_CD
     , A.AR_IF_NO IF_NO
  FROM INV_AR_SPLIT_ISS A
     , INV_AR_SPLIT_ISS_CHG B
     , INV_AR_MN C  
#if (${flag} == 'S') 
 WHERE A.INV_NO IN (${invs})
#elseif (${flag} == 'M') 
 WHERE A.INV_NO BETWEEN @[f_inv] AND @[t_inv]
#end 
   AND A.INV_SEQ = 1
   AND A.INV_NO = B.INV_NO
   AND A.AR_IF_NO = C.AR_IF_NO
   AND C.AR_OFC_CD = @[iss_ofc_cd]
 ORDER BY A.INV_NO DESC			]]></sql>
			<params>
				<param name="f_inv" type="12" value="" out="N"/>
				<param name="t_inv" type="12" value="" out="N"/>
				<param name="iss_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
