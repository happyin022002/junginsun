<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAOTaxableAmountSummaryDetailDownRSQL">
			<desc><![CDATA[TaxableAmountSummaryDetailDown]]></desc>
			<sql><![CDATA[
SELECT A.VSL_CD
, A.NRT_WGT
, A.LDB_CAPA_QTY BSA_CAPA
, A.ACT_BSA_CAPA ACT_BSA_CAPA
, A.USG_RT * 100 USG_RT
, TO_CHAR(A.POL_DEP_DT, 'YYYY-MM-DD') FM_VVD_STL_DT
, TO_CHAR(A.POD_DEP_DT , 'YYYY-MM-DD') TO_VVD_STL_DT
, A.NRT_TONG_TAX_AMT
, A.TONG_TAX_AMT
, A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD VVD_CD
FROM TOT_FDR_STL_AMT A
WHERE STL_YRMON = @[stl_yrmon]
AND TONG_STL_BAT_JB_SEQ = (SELECT MAX(TONG_STL_BAT_JB_SEQ) FROM TOT_FDR_STL_AMT WHERE STL_YRMON = '201208')
--and rownum < 300
ORDER BY VSL_CD			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
