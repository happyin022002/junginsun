<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOSubAgentUSQL">
			<desc><![CDATA[INV_SUB_AGN_CUST_CD 테이블에  Office 별 Agent정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE INV_SUB_AGN_CUST_CD SET
	   AGN_NM = @[agn_nm],
	   REP_CUST_CNT_CD = @[rep_cust_cnt_cd],
	   REP_CUST_SEQ = @[rep_cust_seq],
	   UPD_USR_ID = @[upd_usr_id],
	   DELT_FLG = @[delt_flg],
	   UPD_DT = SYSDATE
 WHERE AR_OFC_CD = @[ar_ofc_cd]
   AND IO_BND_CD = @[io_bnd_cd]
   AND POR_CD = DECODE(@[por_cd], null, 'XXXXX', @[por_cd])
   AND POL_CD = DECODE(@[pol_cd], null, 'XXXXX', @[pol_cd])
   AND POD_CD = DECODE(@[pod_cd], null, 'XXXXX', @[pod_cd])
   AND DEL_CD = DECODE(@[del_cd], null, 'XXXXX', @[del_cd])			]]></sql>
			<params>
				<param name="agn_nm" type="12" value="" out="N"/>
				<param name="rep_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="rep_cust_seq" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
