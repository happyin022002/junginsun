<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOJooStlCmbVODSQL">
			<desc><![CDATA[JOO_STL_CMB 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM JOO_STL_CMB
WHERE	ACCT_YRMON = REPLACE(@[acct_yrmon],'-','')
AND	JO_CRR_CD = @[jo_crr_cd]
AND	STL_CMB_SEQ = @[stl_cmb_seq]			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="stl_cmb_seq" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
