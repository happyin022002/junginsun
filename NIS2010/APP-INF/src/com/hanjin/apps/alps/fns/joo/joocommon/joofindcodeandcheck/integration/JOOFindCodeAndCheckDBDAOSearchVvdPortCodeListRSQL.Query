<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchVvdPortCodeListRSQL">
			<desc><![CDATA[SearchVvdPortCodeList]]></desc>
			<sql><![CDATA[
SELECT vsl_cd||skd_voy_no||skd_dir_cd AS CODE
FROM VSK_VSL_PORT_SKD
WHERE	vsl_cd = substr(@[super_cd1],1,4)
AND	skd_voy_no = substr(@[super_cd1],5,4)
AND	skd_dir_cd = substr(@[super_cd1],9,1)
#if (${super_cd2} != '') 
AND VPS_PORT_CD = @[super_cd2]
#end			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="" out="N"/>
				<param name="super_cd2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
