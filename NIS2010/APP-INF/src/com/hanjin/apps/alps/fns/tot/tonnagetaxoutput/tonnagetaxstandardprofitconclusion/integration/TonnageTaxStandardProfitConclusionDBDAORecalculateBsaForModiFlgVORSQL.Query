<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAORecalculateBsaForModiFlgVORSQL">
			<desc><![CDATA[bsa 테이블의 modify flag 체크]]></desc>
			<sql><![CDATA[
SELECT STL_YRMON
     , TRD_CD
     , SLAN_CD
     , VSL_CD
     , SKD_VOY_NO
     , SKD_DIR_CD
     , TONG_STL_BAT_JB_SEQ
     , LDB_CAPA_QTY
FROM TOT_BSA
WHERE STL_YRMON = @[stl_yrmon]
   AND TONG_STL_BAT_JB_SEQ = (SELECT MAX(TONG_STL_BAT_JB_SEQ) FROM TOT_BSA WHERE STL_YRMON = @[stl_yrmon])
   AND MODI_FLG = 'Y'			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
