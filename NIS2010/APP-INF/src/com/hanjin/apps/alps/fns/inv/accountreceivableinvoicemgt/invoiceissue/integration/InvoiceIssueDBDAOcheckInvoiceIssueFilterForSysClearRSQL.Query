<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOcheckInvoiceIssueFilterForSysClearRSQL">
			<desc><![CDATA[Issue 동작 중에 Sys Clear 동작이 발생한 Detail 정보를 확인한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) AS CNT
FROM   INV_AR_MN
WHERE  1=1
AND    INV_CLR_FLG  = 'Y'
AND    AR_IF_NO IN (
                  SELECT AR_IF_NO
                  FROM   INV_AR_ISS_FTR
                  WHERE  INV_ISS_WRK_NO = @[wrk_no]
                  GROUP BY AR_IF_NO
                  )
AND     ROWNUM  = 1
			]]></sql>
			<params>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
