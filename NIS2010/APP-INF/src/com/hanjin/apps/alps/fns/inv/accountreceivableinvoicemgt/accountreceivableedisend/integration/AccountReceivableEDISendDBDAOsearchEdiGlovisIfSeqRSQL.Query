<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOsearchEdiGlovisIfSeqRSQL">
			<desc><![CDATA[Interface No를 가져온다]]></desc>
			<sql><![CDATA[
SELECT  /*+INDEX_DESC(T1 XAK1INV_EDI_GLOVIS_HDR) */
        AR_IF_NO,  IF_SEQ, CXL_FLG
FROM    INV_EDI_GLOVIS_HDR T1
WHERE   1=1 
	AND 	BL_SRC_NO	= @[bl_src_no]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
