<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOsearchChargeNameRSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOsearchChargeNameRSQL]]></desc>
			<sql><![CDATA[
SELECT CHG_NM  
FROM (
       SELECT NUM, CHG_NM 
       FROM (
              SELECT 1 NUM, DECODE(@[chg_cd],'JOP','TERMINAL STEVEDORAGE','CRC', 'MIS. REVENUE(CNTR) - EQ RENTAL', CHG_NM) CHG_NM
              FROM   MDM_CHARGE
              WHERE  CHG_CD = @[chg_cd]
              UNION ALL
              SELECT (CHG_SEQ+1) NUM, CHG_DESC_CONV_CTNT CHG_NM
              FROM   INV_CHG_DESC_CONV
              WHERE  AR_OFC_CD = @[ar_ofc_cd]
              AND    CHG_CD = @[chg_cd]
              AND    NVL(DELT_FLG,'N') <> 'Y'
    )
    ORDER BY NUM DESC )
WHERE ROWNUM = 1			]]></sql>
			<params>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
