<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOSearchToCustListRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT CUST_LGL_ENG_NM 
  FROM MDM_CUSTOMER  A
 WHERE (A.CUST_CNT_CD, A.CUST_SEQ) IN 
      (SELECT B.CUST_CNT_CD, B.CUST_SEQ
         FROM JOO_CARRIER B 
        WHERE B.JO_CRR_CD = @[jo_crr_cd] 
          AND B.DELT_FLG  = 'N')			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
