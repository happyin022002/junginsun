<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOVIEInvoiceNumberRSQL">
			<desc><![CDATA[VIE Invoice Number 테이블을 Select]]></desc>
			<sql><![CDATA[
SELECT (@[vn_inv_pfx_cd] || SUBSTR(VN_INV_ISS_DT,4,5) || LPAD(VN_INV_MAX_SEQ+1,3,0)) INV_NO
  FROM INV_AR_VN_ISS_NO
 WHERE VN_INV_PFX_CD = @[vn_inv_pfx_cd]
   AND VN_INV_ISS_DT = TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]),'YYYYMMDD')
FOR UPDATE			]]></sql>
			<params>
				<param name="vn_inv_pfx_cd" type="12" value="s" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
