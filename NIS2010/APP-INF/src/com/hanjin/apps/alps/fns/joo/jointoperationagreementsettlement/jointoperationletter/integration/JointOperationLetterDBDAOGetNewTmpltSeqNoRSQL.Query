<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOGetNewTmpltSeqNoRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT   (SELECT   /*+ index_desc(A, XPKJOO_LTR_TMPLT)*/
                   SUBSTR(  @[ofc_cd], -3)||LPAD(NVL(MAX( SUBSTR(JO_TMPLT_NO, 4) ) , 0) +1, 3,'0')
            FROM   JOO_LTR_TMPLT A
           WHERE   OFC_CD =  @[ofc_cd]
             AND   JO_LTR_TP_CD = @[jo_ltr_tp_cd] AND CRE_USR_ID= @[cre_usr_id])jo_tmplt_no,
         (SELECT  /*+ index_desc(A, XPKJOO_LTR_TMPLT) */
                    NVL(MAX(A.JO_LTR_TMPLT_SEQ), 0) +1 
            FROM  JOO_LTR_TMPLT A)jo_ltr_tmplt_seq
  FROM   DUAL			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="jo_ltr_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
