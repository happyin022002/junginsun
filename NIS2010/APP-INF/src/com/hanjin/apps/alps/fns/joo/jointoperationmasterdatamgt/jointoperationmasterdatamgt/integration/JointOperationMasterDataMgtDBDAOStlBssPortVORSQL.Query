<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOStlBssPortVORSQL">
			<desc><![CDATA[Basic Port Choose By Settlement Item]]></desc>
			<sql><![CDATA[
SELECT
       A.JO_CRR_CD,
       A.TRD_CD,
       A.RLANE_CD,
       A.JO_STL_ITM_CD,
       A.SKD_DIR_CD,
       A.STL_TGT_VVD_BSS_CD,
       A.JO_STL_TGT_TP_CD,
       A.N1ST_STL_BZC_PORT_CD,
       A.N2ND_STL_BZC_PORT_CD,
       A.N3RD_STL_BZC_PORT_CD,
       A.N1ST_STL_PAIR_PORT_CD,
       A.N2ND_STL_PAIR_PORT_CD,
       A.N3RD_STL_PAIR_PORT_CD,
       A.UC_BSS_PORT_CD,
       A.AGMT_MON_COND_CD,
       A.AGMT_PORT_COND_CD,
       A.AGMT_PORT_TP_COND_CD,
       A.AGMT_OP_TP_COND_CD,
       B.JO_STL_ITM_NM
FROM   JOO_STL_BSS_PORT A,
       JOO_STL_ITM      B
WHERE  A.JO_STL_ITM_CD = B.JO_STL_ITM_CD
AND    A.JO_CRR_CD     = @[jo_crr_cd]
AND    A.RLANE_CD      = @[rlane_cd]
#if(${jo_stl_itm_cd}!= '')
AND    A.JO_STL_ITM_CD = @[jo_stl_itm_cd]
#end
#if(${skd_dir_cd}!= '')
AND    A.SKD_DIR_CD = @[skd_dir_cd]
#end
ORDER  BY A.SKD_DIR_CD, A.JO_STL_ITM_CD			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="jo_stl_itm_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
