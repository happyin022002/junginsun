<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAORevDirListRSQL">
			<desc><![CDATA[Settlement 에서 VVD 9자리 입력시 Fin. Dir List를 대상항차에서 조회한다.]]></desc>
			<sql><![CDATA[
SELECT
	C.RLANE_DIR_CD   AS REV_DIR_CD          
FROM AR_MST_REV_VVD C 
WHERE 1=1
AND C.VSL_CD     = @[vsl_cd]
AND C.SKD_VOY_NO = @[skd_voy_no]
AND C.SKD_DIR_CD = @[skd_dir_cd]
AND C.RLANE_CD   = @[rlane_cd]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
