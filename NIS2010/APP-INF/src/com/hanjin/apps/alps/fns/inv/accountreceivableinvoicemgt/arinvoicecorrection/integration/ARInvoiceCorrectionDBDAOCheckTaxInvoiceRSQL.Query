<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOCheckTaxInvoiceRSQL">
			<desc><![CDATA[Check Tax Invoice]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) CNT
FROM INV_AR_MN
WHERE BL_SRC_NO = @[bl_src_no]
AND AR_OFC_CD = @[ar_ofc_cd]
AND NVL(IDA_ISS_TP_CD, 'P') = 'T'			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
