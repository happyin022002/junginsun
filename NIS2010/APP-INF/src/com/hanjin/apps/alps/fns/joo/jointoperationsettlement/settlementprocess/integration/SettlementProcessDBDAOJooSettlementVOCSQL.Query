  ,@[sconti_cd]
  ,@[fnl_hjs_bsa_qty]
  ,@[fnl_bsa_wgt]
  ,@[usd_slt_bsa_qty]
  ,@[usd_slt_wgt]
  ,@[bsa_per_wgt]
  ,@[fm_port_cd]
  ,@[to_port_cd]
  ,@[rf_scg_stl_tp_cd]
  ,@[rf_scg_prc]
  ,@[stl_rmk]
  ,NVL(@[cmb_cfm_flg],'N')
  ,@[stl_tj_no]
  ,NVL(@[stl_adj_flg],'N')
  ,@[pre_acct_yrmon]
  ,@[pre_stl_vvd_seq]
  ,@[pre_stl_seq]
  ,NVL(@[stl_lst_flg],'Y')
  ,SYSDATE
  ,@[cre_usr_id]
  ,SYSDATE
  ,@[cre_usr_id]
  ,@[uc_bss_port_cd]
  ,TO_DATE(@[uc_bss_port_etd_dt],'YYYYMMDDHH24MISS')
  ,NVL(@[stl_dup_flg],'N')
)			]]></sql>
