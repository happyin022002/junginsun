  ,@[jo_mnu_nm]
  ,@[vsl_cd]
  ,@[skd_voy_no]
  ,@[skd_dir_cd]
  ,@[rev_dir_cd]
  ,@[stl_bzc_port_cd]
  ,TO_DATE(@[eta_dt],'YYYYMMDDHH24MISS')
  ,@[jo_stl_jb_cd]
  ,@[bsa_qty]
  ,@[bsa_slt_prc]
  ,@[locl_curr_cd]
  ,@[adj_bsa_qty_locl_amt]
  ,@[adj_bsa_slt_prc_locl_amt]
  ,@[stl_locl_amt]
  ,@[stl_usd_amt]
