<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOStlTgtClzUSQL">
			<desc><![CDATA[Settlement Target Close 수정]]></desc>
			<sql><![CDATA[
UPDATE JOO_STL_TGT J
SET 
 J.STL_TGT_FLG = @[stl_tgt_flg]
,J.STL_CLZ_FLG = @[stl_clz_flg]
#if (${fnl_bsa_qty} != '')
,J.FNL_BSA_QTY = @[fnl_bsa_qty]
#end
#if (${fnl_bsa_slt_prc} != '')
,J.FNL_BSA_SLT_PRC = @[fnl_bsa_slt_prc]
#end
#if (${rev_crr_cd} != '')
,J.REV_CRR_CD = @[rev_crr_cd]
#end
#if (${rev_bsa_qty} != '')
,J.REV_BSA_QTY = @[rev_bsa_qty]
#end
#if (${rev_bsa_slt_prc} != '')
,J.REV_BSA_SLT_PRC = @[rev_bsa_slt_prc]
#end
#if (${rev_enbl_flg} != '')
,J.REV_ENBL_FLG = @[rev_enbl_flg]
#end
#if (${n2nd_rev_crr_cd} != '')
,J.N2ND_REV_CRR_CD = @[n2nd_rev_crr_cd]
#end
#if (${n2nd_rev_bsa_qty} != '')
,J.N2ND_REV_BSA_QTY = @[n2nd_rev_bsa_qty]
#end
#if (${n2nd_rev_bsa_slt_prc} != '')
,J.N2ND_REV_BSA_SLT_PRC = @[n2nd_rev_bsa_slt_prc]
#end
#if (${n2nd_rev_enbl_flg} != '')
,J.N2ND_REV_ENBL_FLG = @[n2nd_rev_enbl_flg]
#end
#if (${n3rd_rev_crr_cd} != '')
,J.N3RD_REV_CRR_CD = @[n3rd_rev_crr_cd]
#end
#if (${n3rd_rev_bsa_qty} != '')
,J.N3RD_REV_BSA_QTY = @[n3rd_rev_bsa_qty]
#end
#if (${n3rd_rev_bsa_slt_prc} != '')
,J.N3RD_REV_BSA_SLT_PRC = @[n3rd_rev_bsa_slt_prc]
#end
#if (${n3rd_rev_enbl_flg} != '')
,J.N3RD_REV_ENBL_FLG = @[n3rd_rev_enbl_flg]
#end
#if (${stl_rmk} != '')
,J.STL_RMK = @[stl_rmk]
#end
#if (${stl_locl_amt} != '')
,J.STL_LOCL_AMT = @[stl_locl_amt]
#end
WHERE 1=1
AND J.REV_YRMON = @[rev_yrmon]
AND J.REV_YRMON_SEQ =  @[rev_yrmon_seq]
AND J.REV_SEQ =  @[rev_seq]			]]></sql>
			<params>
				<param name="stl_tgt_flg" type="12" value="" out="N"/>
				<param name="stl_clz_flg" type="12" value="" out="N"/>
				<param name="fnl_bsa_qty" type="12" value="" out="N"/>
				<param name="fnl_bsa_slt_prc" type="12" value="" out="N"/>
				<param name="rev_crr_cd" type="12" value="" out="N"/>
				<param name="rev_bsa_qty" type="12" value="" out="N"/>
				<param name="rev_bsa_slt_prc" type="12" value="" out="N"/>
				<param name="rev_enbl_flg" type="12" value="" out="N"/>
				<param name="n2nd_rev_crr_cd" type="12" value="" out="N"/>
				<param name="n2nd_rev_bsa_qty" type="12" value="" out="N"/>
				<param name="n2nd_rev_bsa_slt_prc" type="12" value="" out="N"/>
				<param name="n2nd_rev_enbl_flg" type="12" value="" out="N"/>
				<param name="n3rd_rev_crr_cd" type="12" value="" out="N"/>
				<param name="n3rd_rev_bsa_qty" type="12" value="" out="N"/>
				<param name="n3rd_rev_bsa_slt_prc" type="12" value="" out="N"/>
				<param name="n3rd_rev_enbl_flg" type="12" value="" out="N"/>
				<param name="stl_rmk" type="12" value="" out="N"/>
				<param name="stl_locl_amt" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon_seq" type="12" value="" out="N"/>
				<param name="rev_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
