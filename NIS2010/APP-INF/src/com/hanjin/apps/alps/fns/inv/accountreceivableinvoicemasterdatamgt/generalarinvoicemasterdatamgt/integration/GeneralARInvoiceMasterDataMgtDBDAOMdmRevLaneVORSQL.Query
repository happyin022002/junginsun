<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceMasterDataMgtDBDAOMdmRevLaneVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT 
SLAN_CD,
SCONTI_CD,
SLAN_DIR_CD,
RLANE_DIR_CD,
DIR_CNG_CD,
DELT_FLG,
RLANE_CD,
POD_CONTI_CD,
(POD_SLANE_DIR_CD ||POD_RLANE_DIR_CD ) AS POD_RLANE_DIR_CD,
POD_RLANE_CD 
--POD_SLANE_DIR_CD 
FROM AR_FINC_DIR_CONV
#if ((${slan_cd} != '')||(${dir_cng_cd} != '')||(${delt_flg} != '')) 
WHERE
#end
#if (${slan_cd} != '') 
	SLAN_CD LIKE '%' ||@[slan_cd]|| '%'
#end
#if ((${slan_cd} != '')&&(${dir_cng_cd} != '')) 
AND	DIR_CNG_CD = @[dir_cng_cd]
#elseif((${slan_cd} == '')&&(${dir_cng_cd} != '')) 
DIR_CNG_CD = @[dir_cng_cd]
#end
#if (((${slan_cd} != '')||(${dir_cng_cd} != ''))&&(${delt_flg} != '')) 
AND	DELT_FLG = @[delt_flg]
#elseif ((${slan_cd} == '')&&(${dir_cng_cd} == '')&&(${delt_flg} != '')) 
DELT_FLG = @[delt_flg]
#end
ORDER BY SLAN_CD,SCONTI_CD,SLAN_DIR_CD,RLANE_DIR_CD,DIR_CNG_CD,DELT_FLG			]]></sql>
			<params>
				<param name="slan_cd" type="12" value="PDS" out="N"/>
				<param name="dir_cng_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
