<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOSearchSpndVatRgstNoRSQL">
			<desc><![CDATA[INV_AR_SPND_VAT_RGST_NO테이블에서 해당 CUSTOMER CODE에 해당하는 SPND_VAT_RGST_NO를 취득한다.]]></desc>
			<sql><![CDATA[
SELECT SPND_VAT_RGST_NO 
FROM INV_AR_SPND_VAT_RGST_NO
WHERE CUST_CNT_CD = @[cust_cnt_cd] 
AND CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
