<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOAddKORIssueMainCSQL">
			<desc><![CDATA[KOR Invoice Main 테이블에 Insert]]></desc>
			<sql><![CDATA[
INSERT
INTO INV_AR_KR_ISS(INV_NO, INV_SEQ, AR_OFC_CD, ACT_CUST_CNT_CD, ACT_CUST_SEQ, INV_RMK, ISS_DT, RISS_DT, CUST_EML, CUST_FAX_NO, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
VALUES(@[inv_no],
      @[inv_seq],
      @[ar_ofc_cd],
      @[act_cust_cnt_cd],
      @[act_cust_seq],
      @[inv_rmk],
      REPLACE(@[iss_dt],'-',''),
      (SELECT DECODE(NVL(MAX(INV_SEQ) + 1, 1), 1, '', TO_CHAR(SYSDATE,'YYYYMMDD')) FROM INV_AR_KR_ISS WHERE INV_NO=@[inv_no]),
      @[cust_eml],
      @[cust_fax_no],
      @[cre_usr_id],
      SYSDATE,
      @[upd_usr_id],
      SYSDATE)			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="act_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="act_cust_seq" type="12" value="" out="N"/>
				<param name="inv_rmk" type="12" value="" out="N"/>
				<param name="iss_dt" type="12" value="" out="N"/>
				<param name="cust_eml" type="12" value="" out="N"/>
				<param name="cust_fax_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
