<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOVIEInvoiceNumberCSQL">
			<desc><![CDATA[채번정보를 Insert]]></desc>
			<sql><![CDATA[
INSERT INTO INV_AR_VN_ISS_NO (
	   VN_INV_PFX_CD,
	   VN_INV_ISS_DT,
	   VN_INV_MAX_SEQ,
	   CRE_USR_ID,
	   CRE_DT,
	   UPD_USR_ID,
	   UPD_DT
) VALUES (
       @[vn_inv_pfx_cd],
  	   TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]),'YYYYMMDD'),
	   0,
	   @[cre_usr_id],
	   SYSDATE,
	   @[upd_usr_id],
	   SYSDATE
)			]]></sql>
			<params>
				<param name="vn_inv_pfx_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
