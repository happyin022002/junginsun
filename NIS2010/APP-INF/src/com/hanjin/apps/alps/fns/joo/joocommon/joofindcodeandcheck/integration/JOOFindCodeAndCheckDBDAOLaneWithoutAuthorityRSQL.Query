<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOLaneWithoutAuthorityRSQL">
			<desc><![CDATA[권한없이 rlane코드 조회]]></desc>
			<sql><![CDATA[
SELECT
     DISTINCT
	 A.RLANE_CD AS CODE,
	 A.RLANE_CD AS NAME
FROM   JOO_CARRIER A
WHERE  A.DELT_FLG = 'N'
#if (${super_cd1} != '')
AND	  A.JO_CRR_CD = @[super_cd1]
#end
#if (${super_cd2} != '')
AND	  A.TRD_CD = @[super_cd2]
#end
#if (${code} != '')
AND	  A.RLANE_CD = @[code]
#end
ORDER BY 1			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="" out="N"/>
				<param name="super_cd2" type="12" value="" out="N"/>
				<param name="code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
