<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyWHFDeclNoUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
   SET WHF_DECL_NO  = '',
       WHF_DECL_CFM_DT = '',
       WHF_DECL_VSL_CD = '',
       WHF_DECL_VOY_NO = '',
       WHF_DECL_DIR_CD = '',       
       WHF_DECL_OFC_CD = '',
       WHF_MRN_NO = '',
       WHF_NTC_NO = '',
       CSR_NO = '',
       WHF_FLG = 'N',
       INV_RMK = @[whf_decl_no]||'-CANCEL',
	   UPD_DT = SYSDATE,
	   UPD_USR_ID = @[usr_id]
 WHERE AR_OFC_CD = @[ar_ofc_cd]
   AND BL_SRC_NO = @[bl_src_no]
   AND WHF_DECL_NO = @[whf_decl_no]			]]></sql>
			<params>
				<param name="whf_decl_no" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="bl_src_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
