<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAOAddTotLaneTradeCSQL">
			<desc><![CDATA[tot_lane_trade 에 해당 lane, trade 데이터를 넣는다]]></desc>
			<sql><![CDATA[
INSERT INTO TOT_LANE_TRD (
	STL_YRMON
,	VSL_SLAN_CD
,	LANE_SEQ
,	TRD_CD
,	CRE_DT
,	CRE_USR_ID
,	UPD_DT
,	UPD_USR_ID
) 
VALUES( 
	@[stl_yrmon]
,	@[vsl_slan_cd]
,	(	SELECT COUNT(VSL_SLAN_CD)+1 
		FROM TOT_LANE_TRD 
		WHERE STL_YRMON = @[stl_yrmon]
		AND VSL_SLAN_CD=@[vsl_slan_cd])
,	@[trd_cd]
, 	sysdate
,	@[cre_usr_id]
, 	sysdate
,	@[cre_usr_id]
)			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
