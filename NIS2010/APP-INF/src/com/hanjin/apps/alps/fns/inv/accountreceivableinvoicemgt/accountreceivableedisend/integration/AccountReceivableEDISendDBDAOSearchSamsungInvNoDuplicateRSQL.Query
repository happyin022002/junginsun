<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchSamsungInvNoDuplicateRSQL">
			<desc><![CDATA[1. 중복 되는 SR_INV_NO 이 있는지 검색.]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) ROW_COUNT
FROM INV_AR_EDI_GERP_BL
WHERE BL_SRC_NO = @[bl_src_no]
  AND SR_INV_NO = @[sr_inv_no]			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="sr_inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
