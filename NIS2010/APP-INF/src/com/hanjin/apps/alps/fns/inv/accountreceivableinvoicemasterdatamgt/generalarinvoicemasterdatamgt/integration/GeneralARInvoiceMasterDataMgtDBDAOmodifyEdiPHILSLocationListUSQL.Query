<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceMasterDataMgtDBDAOmodifyEdiPHILSLocationListUSQL">
			<desc><![CDATA[modifyEdiPHILSLocationList]]></desc>
			<sql><![CDATA[
UPDATE INV_EDI_PHILS_LOC
SET PHILS_LOC_CD_CTNT = SUBSTR(@[phils_loc_cd_ctnt],0,50),
    PHILS_LOC_CD_RMK = SUBSTR(@[phils_loc_cd_rmk],0,500),
    UPD_USR_ID = @[upd_usr_id],
    UPD_DT = SYSDATE
WHERE
	1=1 
    AND CUST_CNT_CD = @[cust_cnt_cd]
    AND CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="phils_loc_cd_ctnt" type="12" value="TEST-PHIL" out="N"/>
				<param name="phils_loc_cd_rmk" type="12" value="TEST________________________________________" out="N"/>
				<param name="upd_usr_id" type="12" value="TES_HAMBB" out="N"/>
				<param name="cust_cnt_cd" type="12" value="DE" out="N"/>
				<param name="cust_seq" type="12" value="1" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
