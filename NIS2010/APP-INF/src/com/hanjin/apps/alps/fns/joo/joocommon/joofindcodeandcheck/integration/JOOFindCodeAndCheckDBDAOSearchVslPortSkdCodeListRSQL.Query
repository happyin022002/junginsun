<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchVslPortSkdCodeListRSQL">
			<desc><![CDATA[SearchVslPortSkdCodeList]]></desc>
			<sql><![CDATA[
        SELECT A.VSL_CD AS CODE
        FROM VSK_VSL_PORT_SKD A
        WHERE  A.VSL_CD     = @[super_cd1]
        AND    A.SKD_VOY_NO = @[super_cd2]
        AND    A.SKD_DIR_CD = @[super_cd3]			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="" out="N"/>
				<param name="super_cd2" type="12" value="" out="N"/>
				<param name="super_cd3" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
