<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOsearchBLOnDateRSQL">
			<desc><![CDATA[searchBLOnDate]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(B.BL_OBRD_DT,'YYYYMMDD') EX_RATE_DATE
FROM   BKG_BOOKING A, BKG_BL_DOC B
WHERE  A.BKG_NO = @[bkg_no]
AND    A.BKG_NO = B.BKG_NO			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
