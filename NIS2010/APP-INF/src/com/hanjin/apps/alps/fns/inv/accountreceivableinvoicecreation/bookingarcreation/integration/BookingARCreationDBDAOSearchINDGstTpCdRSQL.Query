<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchINDGstTpCdRSQL">
			<desc><![CDATA[Search INDIA Gst Tp Cd]]></desc>
			<sql><![CDATA[
SELECT B.AR_IF_NO
     , C.DP_PRCS_KNT
     , INV_GET_GST_DIV_CD_FNC(@[login_ofc_cd], 'C', B.ACT_CUST_CNT_CD, B.ACT_CUST_SEQ, '', DECODE(B.IO_BND_CD, 'O', B.POR_CD, B.DEL_CD)) IND_GST_TP_CD
FROM (SELECT AR_IF_NO
      FROM INV_AR_ISS_FTR 
      WHERE INV_ISS_WRK_NO = @[wrk_no] 		
      GROUP BY AR_IF_NO) A,
     INV_AR_MN B,
     MDM_CURRENCY C
WHERE A.AR_IF_NO = B.AR_IF_NO
AND B.LOCL_CURR_CD = C.CURR_CD			]]></sql>
			<params>
				<param name="login_ofc_cd" type="12" value="" out="N"/>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
