<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchBKGInterfaceNoRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchBKGInterfaceNoRSQL]]></desc>
			<sql><![CDATA[
SELECT LPAD((BKG_MAX_SEQ + 1),8,0) maxSeq
FROM INV_AR_BKG_IF_NO 
WHERE IF_PFX_CD =DECODE(SUBSTR(@[ofc_cd], 1,3),'FXT','LON',SUBSTR(@[ofc_cd], 1,3))
for update			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="AARAA" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
