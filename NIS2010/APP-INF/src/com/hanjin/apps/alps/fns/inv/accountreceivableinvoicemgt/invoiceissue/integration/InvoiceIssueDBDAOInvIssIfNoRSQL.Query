<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOInvIssIfNoRSQL">
			<desc><![CDATA[InvIssIfNo]]></desc>
			<sql><![CDATA[
SELECT DISTINCT(AR_IF_NO) AR_IF_NO
  FROM INV_AR_ISS_FTR
 WHERE INV_ISS_WRK_NO = @[wrk_no] 			]]></sql>
			<params>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
