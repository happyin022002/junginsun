<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchArTaxIndRSQL">
			<desc><![CDATA[BookingARCreationDBDAOSearchArTaxIndRSQL]]></desc>
			<sql><![CDATA[
SELECT '10' AR_TAX_IND
  FROM INV_BKG_IF_CHG
 WHERE BKG_NO = @[bkg_no]
   AND BKG_SEQ = @[bkg_seq]
   AND OFC_CD = @[ofc_cd]
   AND CHG_CD = 'TVA'
 GROUP BY BKG_NO, BKG_SEQ
 HAVING SUM(CHG_AMT) <> 0			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
