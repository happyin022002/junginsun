<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOsearchMRIRevenueVVDLaneByVvdPortRSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOsearchMRIRevenueVVDLaneByVvdPortRSQL]]></desc>
			<sql><![CDATA[
SELECT NVL(SUBSTR(AA,4,5),'') REV_LANE, NVL(SUBSTR(AA,9),'') REV_VVD   
    FROM (
    SELECT MIN(SUBSTR(TO_CHAR( N.RNK_SEQ ,'999'),2)||N.RLANE_CD||Q.VSL||Q.VOY_NO||Q.DEP) AA
    FROM AR_ROUT_RNK N,
    (  SELECT  V.RLANE_CD REV_LANE,  T.VSL, T.VOY_NO, T.DEP, T.LANE, T.SCONTI_CD, T.ZONE_IOC
      FROM AR_MST_REV_VVD V,
      (
        SELECT B.VSL, B.VOY_NO, DECODE(C.POD_CONTI_CD, B.POD_CONTI, POD_SLANE_DIR_CD||POD_RLANE_DIR_CD, NVL(SLAN_DIR_CD||RLANE_DIR_CD,B.DEP||B.DEP)) DEP,
          DECODE(B.VSL, 'COMC','COM','CNTC','CNT','WLXC','WLX', DECODE(B.LANE,'SYS','RBC',B.LANE) ) LANE, B.SCONTI_CD, B.ZONE_IOC
        FROM AR_FINC_DIR_CONV C ,
           (
          SELECT LANE, VSL, VOY_NO, DEP, POL, POD, SCONTI_CD, POD_CONTI, ZONE_IOC
          FROM (
            SELECT LANE, VSL, VOY_NO, DEP, POL, POD, MIN(SCONTI_CD)SCONTI_CD, MIN(POD_CONTI) POD_CONTI,
              DECODE(MIN(POL_CONTI)||MIN(POD_CONTI), 'AA','IA', 'EE','IE','MM','IM','EF','IE','FE','IE','FF','IE','OO') ZONE_IOC
            FROM        (
              SELECT @[lane] LANE, @[vsl] VSL, @[voy] VOY_NO, @[dep] DEP,
                      @[pol] POL, @[pod] POD, conti_cd POL_CONTI, NULL POD_CONTI, SCONTI_CD
                  FROM MDM_LOCATION L
                  WHERE L.LOC_CD = @[pol]
                 UNION ALL
                 SELECT @[lane] LANE, @[vsl] VSL, @[voy] VOY_NO, @[dep] DEP,
                      @[pol] POL, @[pod] POD, NULL POL_CONTI, CONTI_CD POD_CONTI, NULL SCONTI_CD
                  FROM MDM_LOCATION L
                  WHERE L.LOC_CD = @[pod]

				UNION ALL
                  SELECT V.SLAN_CD LANE, V.VSL_CD VSL, V.SKD_VOY_NO VOY_NO, V.SKD_DIR_CD DEP,
                                V.POL_CD POL, V.POD_CD POD, L.CONTI_CD POL_CONTI, NULL POD_CONTI, L.SCONTI_CD SCONTI_CD  
                  FROM BKG_VVD V, MDM_LOCATION L
                  WHERE V.BKG_NO =  @[bkg_no]
                  AND V.POL_CD = L.LOC_CD
                  UNION ALL
                  SELECT V.SLAN_CD LANE, V.VSL_CD VSL, V.SKD_VOY_NO VOY_NO, V.SKD_DIR_CD DEP,
                                V.POL_CD POL, V.POD_CD POD, NULL POL_CONTI, L.CONTI_CD POD_CONTI, NULL SCONTI_CD
                  FROM BKG_VVD V, MDM_LOCATION L
                  WHERE V.BKG_NO =  @[bkg_no]
                  AND V.POD_CD = L.LOC_CD
	
               )
            GROUP BY LANE, VSL, VOY_NO, DEP, POL, POD )
          WHERE SCONTI_CD IS NOT NULL 
           ) B
          WHERE  C.SLAN_CD(+) = B.LANE
          AND  C.SLAN_DIR_CD(+) = B.DEP
          --AND  C.    C.DCM_AP_MK(+) <> 'Y'
          AND  C.SCONTI_CD(+) = B.SCONTI_CD )T
      WHERE V.VSL_CD = T.VSL
      AND   V.SKD_VOY_NO = T.VOY_NO
      AND   V.SKD_DIR_CD||V.RLANE_DIR_CD  = T.DEP 
      AND   V.DELT_FLG <> 'Y' ) Q
    WHERE N.RLANE_CD = Q.REV_LANE
    AND   N.SLAN_CD = Q.LANE
    AND   SUBSTR(N.ZN_IOC_CD,1,2) = Q.ZONE_IOC )			]]></sql>
			<params>
				<param name="lane" type="12" value="" out="N"/>
				<param name="vsl" type="12" value="" out="N"/>
				<param name="voy" type="12" value="" out="N"/>
				<param name="dep" type="12" value="" out="N"/>
				<param name="pol" type="12" value="" out="N"/>
				<param name="pod" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
