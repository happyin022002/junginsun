<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOSearchAddBsaCarrieHistoryChkRSQL">
			<desc><![CDATA[SearchAddBsaCarrieHistoryChk]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(JO_BSA_ADD_REF_SEQ), 0) AS JO_BSA_ADD_REF_SEQ
          FROM JOO_BSA_ADD_CRR
          WHERE 1=1
            AND JO_BSA_REF_NO = @[jo_bsa_ref_no]
            --AND JO_VER_FLG = 'Y'			]]></sql>
			<params>
				<param name="jo_bsa_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
