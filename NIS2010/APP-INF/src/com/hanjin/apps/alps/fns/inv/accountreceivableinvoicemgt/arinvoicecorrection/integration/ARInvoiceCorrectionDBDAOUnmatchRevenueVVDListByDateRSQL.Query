<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOUnmatchRevenueVVDListByDateRSQL">
			<desc><![CDATA[INV_AR_MN, INV_AR_CHG, INV_REV_VVD_UMCH 테이블에서 select]]></desc>
			<sql><![CDATA[
SELECT 
	DECODE(NVL(REV_VVD_MTCH_FLG,'N'),'Y',1,0) REV_VVD_MTCH_FLG
	,AR_OFC_CD
	,AR_IF_NO
	,BL_SRC_NO
	,(REV_VSL_CD||REV_SKD_VOY_NO||REV_SKD_DIR_CD||REV_DIR_CD) REV_VVD
	,(NEW_REV_VSL_CD||NEW_REV_SKD_VOY_NO||NEW_REV_SKD_DIR_CD||NEW_REV_DIR_CD) NEW_VVD
	,INV_TTL_LOCL_AMT
	,BL_INV_IF_DT 
	,BL_INV_CFM_DT 
	,INV_DELT_DIV_CD
	,REV_SRC_CD
	,BKG_NO
	,ZN_IOC_CD
	,NEW_RLANE_CD
	,(OLD_REV_VSL_CD||OLD_REV_SKD_VOY_NO||OLD_REV_SKD_DIR_CD||OLD_REV_DIR_CD) OLD_VVD
	,(TRNK_VSL_CD||TRNK_SKD_VOY_NO||TRNK_SKD_DIR_CD) TRUNK_VVD
	,TO_CHAR(CRE_DT,'YYYY-MM-DD HH24:MI:SS') LOG_RGST_DT
  FROM INV_REV_VVD_UMCH
#if (${bkg_if_flg} !='')
 WHERE NVL(REV_VVD_MTCH_FLG,'N') LIKE @[bkg_if_flg]
#end
 ORDER BY BKG_NO, BL_SRC_NO, AR_OFC_CD, AR_IF_NO			]]></sql>
			<params>
				<param name="bkg_if_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
