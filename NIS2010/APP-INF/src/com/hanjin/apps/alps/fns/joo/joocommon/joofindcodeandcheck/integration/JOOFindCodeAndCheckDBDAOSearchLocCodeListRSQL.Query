<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchLocCodeListRSQL">
			<desc><![CDATA[SearchLocCodeList]]></desc>
			<sql><![CDATA[
    SELECT LOC_CD AS CODE
    FROM MDM_LOCATION
    WHERE DELT_FLG = 'N'
    AND LOC_CD = @[super_cd1]			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
