<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOSearchMcsTextNoRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT JO_TMPLT_NO,JO_LTR_TMPLT_SEQ 
  FROM JOO_LTR_TMPLT
 WHERE OFC_CD       = @[ofc_cd]
   AND JO_LTR_TP_CD = @[jo_ltr_tp_cd]
   AND  CRE_USR_ID   = @[cre_usr_id]
ORDER BY JO_TMPLT_NO			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="jo_ltr_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
