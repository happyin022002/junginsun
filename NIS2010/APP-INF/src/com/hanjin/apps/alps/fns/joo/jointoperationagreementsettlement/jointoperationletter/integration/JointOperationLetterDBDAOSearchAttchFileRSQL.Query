<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOSearchAttchFileRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT    A.EML_SND_NO,
          B.JO_LTR_SEQ,
          B.FILE_SAV_ID,
         (SELECT  S2.EML_FILE_PATH_URL_CTNT FROM COM_EML_SND_INFO S2 WHERE S2.EML_SND_NO = A.EML_SND_NO)
          EML_FILE_PATH_URL_CTNT,
          B.CRE_DT,
          B.CRE_USR_ID,--EML_FILE_PATH_URL_CTNT
          (SELECT S1.USR_NM FROM COM_USER S1 WHERE S1.USR_ID = B.CRE_USR_ID)USR_NM 
  FROM    JOO_LETTER A, 
          JOO_LTR_ATCH_FILE B
 WHERE    A.JO_LTR_SEQ  = B.JO_LTR_SEQ
   AND    A.JO_LTR_SEQ = @[jo_ltr_seq]			]]></sql>
			<params>
				<param name="jo_ltr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
