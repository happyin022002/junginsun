<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOAgentByVesselVORSQL">
			<desc><![CDATA[FNS_INV_0081 retrive]]></desc>
			<sql><![CDATA[
SELECT
A.VSL_CD VSL_CD
,	A.AGN_CD AGN_CD
,	A.AR_OFC_CD AR_OFC_CD
,	A.CUST_CNT_CD CUST_CNT_CD
,	lpad(A.CUST_SEQ,6,0) CUST_SEQ
,	A.VNDR_CNT_CD VNDR_CNT_CD
,	lpad(A.VNDR_SEQ,6,0) VNDR_SEQ
,   B.CUST_LGL_ENG_NM CUST_LGL_ENG_NM
FROM INV_VSL_AGN_CUST_CD A, MDM_CUSTOMER B
WHERE A.CUST_CNT_CD = B.CUST_CNT_CD
AND A.CUST_SEQ = B.CUST_SEQ
AND VSL_CD = @[vsl_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="WHYH" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
