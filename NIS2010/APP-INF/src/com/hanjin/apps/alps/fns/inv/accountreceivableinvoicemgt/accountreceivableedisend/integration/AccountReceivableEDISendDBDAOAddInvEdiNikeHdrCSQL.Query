<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOAddInvEdiNikeHdrCSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
INSERT INTO INV_EDI_NIKE_HDR
SELECT @[inv_no]
	  ,@[inv_seq]
	  ,@[iss_dt]
	  ,@[bl_src_no]
	  ,@[ttl_trf_rt_amt]
	  ,@[curr_cd]
	  ,@[nike_frt_mod_id]
	  ,@[nike_crr_id]
	  ,@[cntr_no]
	  ,@[edi_snd_flg]
      ,@[cre_usr_id]
      ,SYSDATE
      ,@[upd_usr_id]
      ,SYSDATE 
  FROM DUAL 
 WHERE NOT EXISTS (SELECT 1 FROM INV_EDI_NIKE_HDR WHERE INV_NO = @[inv_no] AND INV_SEQ = @[inv_seq])			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
				<param name="iss_dt" type="12" value="" out="N"/>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="ttl_trf_rt_amt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="nike_frt_mod_id" type="12" value="" out="N"/>
				<param name="nike_crr_id" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="edi_snd_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
