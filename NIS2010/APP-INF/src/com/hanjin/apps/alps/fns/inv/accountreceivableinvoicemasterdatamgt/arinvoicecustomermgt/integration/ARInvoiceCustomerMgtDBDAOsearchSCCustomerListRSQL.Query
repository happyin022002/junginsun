<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOsearchSCCustomerListRSQL">
			<desc><![CDATA[inv-91]]></desc>
			<sql><![CDATA[
SELECT 'Customer' CUSTOMER_TYPE,A.CUST_CNT_CD CUST_CD,LPAD(A.CUST_SEQ,6,0) CUST_SEQ,C.CUST_LGL_ENG_NM CUST_NM 
FROM PRI_SP_CTRT_PTY A,PRI_SP_HDR B,MDM_CUSTOMER C
WHERE A.PROP_NO = B.PROP_NO
AND A.CUST_CNT_CD = C.CUST_CNT_CD
AND A.CUST_SEQ = C.CUST_SEQ
AND B.SC_NO = @[sc_no]
UNION
SELECT 'R.Customer' CUSTOMER_TYPE,A.REAL_CUST_CNT_CD CUST_CD,LPAD(A.REAL_CUST_SEQ,6,0) CUST_SEQ,C.CUST_LGL_ENG_NM CUST_NM 
FROM PRI_SP_MN A,PRI_SP_HDR B,MDM_CUSTOMER C
WHERE A.PROP_NO = B.PROP_NO
AND A.REAL_CUST_CNT_CD = C.CUST_CNT_CD
AND A.REAL_CUST_SEQ = C.CUST_SEQ
AND B.SC_NO = @[sc_no]
UNION
SELECT 'Affiliate' CUSTOMER_TYPE,A.CUST_CNT_CD CUST_CD,LPAD(A.CUST_SEQ,6,0) CUST_SEQ,C.CUST_LGL_ENG_NM CUST_NM 
FROM PRI_SP_AFIL A,PRI_SP_HDR B,MDM_CUSTOMER C
WHERE A.PROP_NO = B.PROP_NO
AND A.CUST_CNT_CD = C.CUST_CNT_CD
AND A.CUST_SEQ = C.CUST_SEQ
AND B.SC_NO = @[sc_no]
			]]></sql>
			<params>
				<param name="sc_no" type="12" value="TES090001" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
