<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOJoSettlementPicTotalRSQL">
			<desc><![CDATA[정산 PIC 담당자 조회]]></desc>
			<sql><![CDATA[
SELECT
CEIL(COUNT(1)/TO_NUMBER(@[pagerows])) AS TOT_PAGE_CNT
FROM
(
        SELECT
          A.TRD_CD
        FROM
        (
            SELECT DISTINCT
                 A.TRD_CD
                ,B.JO_CRR_CD
                ,B.RLANE_CD
                ,B.RE_DIVR_CD
                ,B.UPD_USR_ID
            FROM   JOO_CARRIER      A,
                   JOO_FINC_MTX     B
            WHERE  1=1
 		    #if (${trd_cd} != '')
			AND A.TRD_CD = @[trd_cd]
			#end			
 		    #if (${jo_crr_cd} != '')
			AND B.JO_CRR_CD = @[jo_crr_cd]
			#end			
 		    #if (${rlane_cd} != '')
			AND B.RLANE_CD = @[rlane_cd]
			#end			
 		    #if (${re_divr_cd} != '')
			AND B.RE_DIVR_CD = @[re_divr_cd]
			#end			
			AND    A.JO_CRR_CD     = B.JO_CRR_CD
            AND    A.RLANE_CD      = B.RLANE_CD
        ) A, JOO_STL_PIC J
        WHERE 1=1
        AND A.TRD_CD = J.TRD_CD(+)
        AND A.JO_CRR_CD = J.CRR_CD(+)
        AND A.RLANE_CD = J.RLANE_CD(+)
        AND A.RE_DIVR_CD = J.RE_DIVR_CD(+)
)			]]></sql>
			<params>
				<param name="pagerows" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
