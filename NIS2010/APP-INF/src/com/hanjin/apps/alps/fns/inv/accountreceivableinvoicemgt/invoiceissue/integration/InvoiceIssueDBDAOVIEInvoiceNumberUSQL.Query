<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOVIEInvoiceNumberUSQL">
			<desc><![CDATA[채번정보를 Update]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_VN_ISS_NO SET
	   VN_INV_MAX_SEQ = TO_NUMBER(@[vn_inv_max_seq]),
	   UPD_USR_ID = @[upd_usr_id],
	   UPD_DT = SYSDATE
 WHERE VN_INV_PFX_CD = @[vn_inv_pfx_cd]
   AND VN_INV_ISS_DT = TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]),'YYYYMMDD')			]]></sql>
			<params>
				<param name="vn_inv_max_seq" type="2" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="vn_inv_pfx_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
