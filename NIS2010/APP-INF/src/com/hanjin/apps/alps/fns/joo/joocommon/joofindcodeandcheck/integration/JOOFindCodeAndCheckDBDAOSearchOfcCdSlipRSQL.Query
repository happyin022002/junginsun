<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchOfcCdSlipRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT     A.SLP_OFC_CD code, B.OFC_ENG_NM NAME
FROM      JOO_SLIP A, MDM_ORGANIZATION B
WHERE     A.SLP_OFC_CD  = B.OFC_CD
GROUP BY A.SLP_OFC_CD,B.OFC_ENG_NM
ORDER BY  A.SLP_OFC_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
