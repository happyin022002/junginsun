<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAOsearchBKGPortCdRSQL">
			<desc><![CDATA[searchBKGPortCd]]></desc>
			<sql><![CDATA[
SELECT NVL(DECODE(@[bnd], 'O', BV.POL_CD, BV.POD_CD), '') PORT
FROM   BKG_VVD BV, BKG_BOOKING BKG
WHERE  BKG.BKG_NO = @[bkg_no]
AND    BKG.BKG_NO = BV.BKG_NO
AND    DECODE(@[bnd], 'O', BKG.POL_CD, BKG.POD_CD) = DECODE(@[bnd], 'O', BV.POL_CD, BV.POD_CD)			]]></sql>
			<params>
				<param name="bnd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
