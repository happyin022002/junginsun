<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAOTaxCalculationJobSkdVORSQL">
			<desc><![CDATA[feeder , nofeeder batch job 스케쥴 정보 조회
2011.01.03 이병훈 [CHM-201008072-01] Summary Creation - Batch 신규화면 개발으로 인하여 Taxable Amount Creation - Batch 조회 기능 보완]]></desc>
			<sql><![CDATA[
SELECT 
	TO_CHAR(TO_DATE(A.JB_FM_YRMON,'YYYYMM'),'YYYY-MM')   JB_FM_YRMON
,   TO_CHAR(TO_DATE(A.JB_TO_YRMON,'YYYYMM'),'YYYY-MM')   JB_TO_YRMON
,	TO_CHAR(A.EFF_DT,'YYYY-MM-DD HH24:MI') EFF_DT
,	A.JB_ID
,	A.CRE_DT
,	A.CRE_USR_ID
,	TO_CHAR(A.UPD_DT,'YYYY-MM') UPD_DT
,	A.UPD_USR_ID
,   A.BAT_ITM_NM 
,  DECODE(A.BAT_ITM_NM,'ALL','FNS_TOT_B001','MAIN TRADE','FNS_TOT_B002','FEEDER','FNS_TOT_B003') BAT_ID

FROM TOT_JB_SKD A
WHERE 1=1
	
#if (${his_type} == "status")   
    AND ((A.JB_FM_YRMON  BETWEEN @[jb_fm_yrmon] AND @[jb_to_yrmon]) OR (A.JB_TO_YRMON  BETWEEN @[jb_fm_yrmon] AND @[jb_to_yrmon]))
  #if (${bat_itm_nm} == "ALL")   
    AND BAT_ITM_NM IN ('ALL','MAIN TRADE','FEEDER')
  #elseif (${bat_itm_nm} == "MAIN TRADE")  
    AND BAT_ITM_NM IN ('ALL','MAIN TRADE')
  #elseif (${bat_itm_nm} == "FEEDER")  
    AND BAT_ITM_NM IN ('ALL','FEEDER') 
  #end 
#elseif (${his_type} == "search")
    AND TO_CHAR(A.EFF_DT,'YYYY-MM-DD') BETWEEN @[from_dt] AND @[to_dt]
    AND BAT_ITM_NM IN ('ALL','MAIN TRADE','FEEDER')
#end 
ORDER BY A.EFF_DT DESC			]]></sql>
			<params>
				<param name="jb_fm_yrmon" type="12" value="" out="N"/>
				<param name="jb_to_yrmon" type="12" value="" out="N"/>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
