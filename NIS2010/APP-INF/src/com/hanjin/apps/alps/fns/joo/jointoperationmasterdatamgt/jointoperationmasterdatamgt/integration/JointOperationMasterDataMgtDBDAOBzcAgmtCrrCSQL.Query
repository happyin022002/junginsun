<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOBzcAgmtCrrCSQL">
			<desc><![CDATA[BzcAgmtCrr
2012.06.01 : 김상근[CHM-201218057-01]
Discription : Table의 Key변경 RLANE_CD, JO_REP_CRR_CD, JO_CRR_CD -> JO_REF_NO, JO_CRR_CD]]></desc>
			<sql><![CDATA[
MERGE INTO JOO_BZC_AGMT_CRR A 
USING
(
       SELECT  @[jo_ref_no] AS JO_REF_NO, @[jo_crr_cd] AS JO_CRR_CD FROM DUAL
) B
ON 
(
           A.JO_REF_NO = B.JO_REF_NO
       AND A.JO_CRR_CD = B.JO_CRR_CD
)
WHEN MATCHED THEN
UPDATE SET  A.UPD_USR_ID = @[cre_usr_id]
          , A.UPD_DT     = SYSDATE
WHEN NOT MATCHED THEN
INSERT
(  JO_REF_NO
,  JO_CRR_CD
,  CRE_DT
,  CRE_USR_ID
,  UPD_DT
,  UPD_USR_ID
)
VALUES
(B.JO_REF_NO
,B.JO_CRR_CD
,SYSDATE
,@[cre_usr_id]
,SYSDATE
,@[cre_usr_id]
)			]]></sql>
			<params>
				<param name="jo_ref_no" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
