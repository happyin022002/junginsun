<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAOAddTotLaneCSQL">
			<desc><![CDATA[tot_lane 테이블에 해당월의 lane 정보를 저장한다]]></desc>
			<sql><![CDATA[
INSERT INTO TOT_LANE(
	STL_YRMON
,	VSL_SLAN_CD
,	DELT_FLG
,	CRE_DT
,	CRE_USR_ID
,	UPD_DT
,	UPD_USR_ID
) 

VALUES( 
	@[stl_yrmon]
,	@[vsl_slan_cd]
,	'N'
, 	sysdate
,	@[cre_usr_id]
, 	sysdate
,	@[cre_usr_id]
)			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
