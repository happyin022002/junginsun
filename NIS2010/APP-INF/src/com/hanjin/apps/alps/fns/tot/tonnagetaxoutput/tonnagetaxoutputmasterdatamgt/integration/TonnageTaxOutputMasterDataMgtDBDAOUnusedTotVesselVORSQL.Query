<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAOUnusedTotVesselVORSQL">
			<desc><![CDATA[Unused Vessel Over 30 Days search list]]></desc>
			<sql><![CDATA[
SELECT
      A.STL_YR
      ,	A.VSL_CD
      ,	A.VSL_SEQ
      ,	A.TONG_FLET_TP_CD
      ,	TO_CHAR(A.CTRT_ST_DT,'YYYYMMDD')  CTRT_ST_DT
      ,	TO_CHAR(A.CTRT_END_DT,'YYYYMMDD') CTRT_END_DT
      ,	A.LDB_CAPA_QTY
      ,	A.DELT_FLG
      ,	A.VSL_RMK
      ,	TO_CHAR(A.CRE_DT,'YYYYMMDD')  CRE_DT
      ,	A.CRE_USR_ID
      ,	TO_CHAR(A.UPD_DT,'YYYYMMDD')  UPD_DT
      ,	A.UPD_USR_ID
      , NVL((SELECT DISTINCT VSL_ENG_NM FROM MDM_VSL_CNTR B WHERE B.VSL_CD = A.VSL_CD),'') VSL_ENG_NM

      FROM TOT_VESSEL A
      WHERE	A.STL_YR = @[stl_yr]
      AND A.TONG_FLET_TP_CD = 'E'			]]></sql>
			<params>
				<param name="stl_yr" type="12" value="2008" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
