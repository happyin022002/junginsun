<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAOLaneGroupVODSQL">
			<desc><![CDATA[tot_lane 테이블의 delt_flg = 'Y' 로 업데이트하여 삭제관리]]></desc>
			<sql><![CDATA[
DELETE FROM TOT_LANE
WHERE	STL_YRMON = @[stl_yrmon]
#if (${sts_flg} != "D")
AND	VSL_SLAN_CD = @[vsl_slan_cd]
#end			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
