<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOJooCngOfcDupCheckRSQL">
			<desc><![CDATA[Office Mapping 정보 조회]]></desc>
			<sql><![CDATA[
SELECT 
ofc_cd
FROM 
joo_cng_ofc a
WHERE
ofc_cd = @[ofc_cd]
#if (${ibflag} == 'U')
and cng_ofc_cd != @[cng_ofc_cd]
AND delt_flg = 'N'
#elseif (${ibflag} == 'I')
and  a.delt_flg = case when	nvl((select 1 from joo_cng_ofc x where x.ofc_cd = @[ofc_cd] and x.cng_ofc_cd = @[cng_ofc_cd]),0) = 1
				then a.delt_flg	else 'N' end
#end
			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="SELFAR" out="N"/>
				<param name="cng_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
