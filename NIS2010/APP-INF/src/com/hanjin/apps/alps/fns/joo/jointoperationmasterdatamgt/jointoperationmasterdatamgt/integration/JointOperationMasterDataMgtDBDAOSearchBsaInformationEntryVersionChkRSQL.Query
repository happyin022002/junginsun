<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOSearchBsaInformationEntryVersionChkRSQL">
			<desc><![CDATA[SearchBsaInformationEntryVersionChk]]></desc>
			<sql><![CDATA[
SELECT 
NVL(MAX(JO_VER_NO), 0) + 1 AS JO_VER_NO
FROM  JOO_BSA_INFO_ENTR
WHERE 1 = 1
       AND   VSL_CD            =  @[vsl_cd]       --19
       AND   SKD_DIR_CD        =  @[skd_dir_cd]   --20
       AND   PORT_CD           =  @[port_cd]      --21
       AND   PORT_SEQ          =  @[port_seq]     --22
       AND   JO_CRR_CD         =  @[jo_crr_cd]    --23
       AND   RLANE_CD          =  @[rlane_cd]			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="port_seq" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
