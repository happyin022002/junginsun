<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOsearchPaymentDateByCustomerRSQL">
			<desc><![CDATA[InvoiceIssueDBDAOsearchPaymentDateByCustomerRSQL]]></desc>
			<sql><![CDATA[
SELECT ACT_CUST_CNT_CD			
     , ACT_CUST_SEQ			
     , PAY_DT_DY1
     , PAY_DT_DY2
     , PAY_DT_DY3
     , PAY_DT_DY4	
     , CUST_CR_DUE_DT_DIV_CD
     , CR_FLG		
  FROM MDM_CR_CUST			
 WHERE CUST_CNT_CD = @[cust_cnt_cd]	
   AND CUST_SEQ = @[cust_seq]
   AND NVL(SUB_SYS_NM,'MDM') <> 'ERP'			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
