<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOModifySamsungBLUSQL">
			<desc><![CDATA[해당 문서번호 및 전송 BL의 내용에 send Flag='Y' 및 OBRD_DT를  update한다.
GERP BL 테이블에 update.]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_EDI_GERP_BL
SET SND_FLG = 'Y',
  OBRD_DT = TO_DATE(@[obrd_dt], 'YYYYMMDD'),
  UPD_USR_ID = @[upd_usr_id],
  UPD_DT = SYSDATE
WHERE MSG_ID = @[msg_id]
  AND MSG_NO = @[msg_no]
  AND BL_SRC_NO = @[bl_src_no]			]]></sql>
			<params>
				<param name="obrd_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="msg_id" type="12" value="" out="N"/>
				<param name="msg_no" type="12" value="" out="N"/>
				<param name="bl_src_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
