<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationAgreementSettlementDAOSearchUserNmRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT Z.*
FROM(
    SELECT USR_ID,USR_NM  FROM COM_USER A WHERE A.USR_ID=@[usr_id] UNION ALL
    SELECT
    b.usr_id,
    B.USR_NM
    FROM    JOO_LTR_TMPLT A, COM_USER B
    WHERE
    A.CRE_USR_ID = B.USR_ID
    #if (${ofc_cd} != '') 
      AND   a.ofc_cd = @[ofc_cd]
    #end
    #if (${jo_ltr_tp_cd} != '') 
      AND   a.jo_ltr_tp_cd = @[jo_ltr_tp_cd]
    #end
    )Z
GROUP BY     usr_id, usr_nm    
ORDER BY     usr_id , usr_nm			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="jo_ltr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
