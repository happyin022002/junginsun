<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOBsaInformationYyyyWwDtRSQL">
			<desc><![CDATA[BsaInformationYyyyWwDt]]></desc>
			<sql><![CDATA[
SELECT SUBSTR(CMV.COST_YRMON,1,4)||COST_WK YRWK
	  , BVM.REV_PORT_CD JO_RDR_PORT_CD
      ,TO_CHAR(BVM.REV_PORT_ETD_DT,'YYYYMMDD') REV_PORT_ETD_DT
  FROM MAS_MON_VVD CMV
      ,BSA_VVD_MST BVM
WHERE CMV.TRD_CD = BVM.TRD_CD
  AND CMV.RLANE_CD = BVM.RLANE_CD
  AND CMV.VSL_CD = BVM.VSL_CD
  AND CMV.SKD_VOY_NO = BVM.SKD_VOY_NO
  AND CMV.DIR_CD = BVM.SKD_DIR_CD
  AND CMV.TRD_CD = @[trd_cd]
  AND CMV.RLANE_CD = @[rlane_cd]
  AND CMV.VSL_CD = @[vsl_cd]
  AND CMV.SKD_VOY_NO = @[skd_voy_no]
  AND CMV.DIR_CD = @[skd_dir_cd]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
