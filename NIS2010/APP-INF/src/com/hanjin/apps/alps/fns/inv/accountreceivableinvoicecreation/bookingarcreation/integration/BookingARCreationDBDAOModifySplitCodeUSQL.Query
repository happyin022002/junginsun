<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifySplitCodeUSQL">
			<desc><![CDATA[INV_AR_MN 테이블에 Update

Master가 되는 대상  데이터에 Split ind를 'M'으로 update  함.]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
   SET INV_SPLIT_CD = @[inv_split_cd],
       UPD_USR_ID	= @[upd_usr_id],
	   UPD_DT       = SYSDATE
 WHERE AR_IF_NO = @[ar_if_no]
   AND NVL(INV_SPLIT_CD,'M') NOT IN ('S','C','E','X')			]]></sql>
			<params>
				<param name="inv_split_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
