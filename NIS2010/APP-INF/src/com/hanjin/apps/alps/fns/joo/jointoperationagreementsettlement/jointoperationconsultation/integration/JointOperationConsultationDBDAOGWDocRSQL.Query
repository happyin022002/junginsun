<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOGWDocRSQL">
			<desc><![CDATA[GW Contract Link 목록 조회]]></desc>
			<sql><![CDATA[
SELECT 
 A.CSR_NO 
,A.CSR_AGMT_DOC_SEQ 
,A.AGMT_DOC_NO 
,A.AGMT_DOC_DESC
,A.UPD_USR_ID 
,TO_CHAR(A.UPD_DT,'YYYY-MM-DD HH24:MI:SS') AS UPD_DT
FROM JOO_CSR_AGMT_DOC A
WHERE 1=1
AND A.CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
