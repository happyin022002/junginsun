<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAORcUpBsaVORSQL">
			<desc><![CDATA[BSA Creation by Vessel Voyage  최근 업데이트 일자 조회]]></desc>
			<sql><![CDATA[
SELECT MAX(UPD_DT) upd_dt
FROM TOT_BSA
WHERE	STL_YRMON = @[stl_yrmon]	
#if (${trd_cd} != 'ALL')
    AND TRD_CD = @[trd_cd]
#end
			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
