<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchUsrInfoRSQL">
			<desc><![CDATA[20160603 HongSeongPil user 정보를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT USR_ID CODE
     , USR_NM NAME
     , OFC_CD
  FROM COM_USER 
 WHERE USR_ID = @[super_cd1]
   AND USE_FLG = 'Y'			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
