<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchSamSungNextMsgNoRSQL">
			<desc><![CDATA[새로운 문서번호를 구함.]]></desc>
			<sql><![CDATA[
SELECT 'SMLINE'||TO_CHAR(SYSDATE, 'yyyymmdd')||LTRIM(TO_CHAR(NVL(MAX(SUBSTR(MSG_NO, 15, 3)), '000') + 1, '000')) MSG_NO
FROM INV_AR_EDI_GERP_HDR
WHERE SUBSTR(MSG_NO, 7, 8) = TO_CHAR(SYSDATE, 'yyyymmdd')			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
