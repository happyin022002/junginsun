<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOSearchKORInvoiceNumberRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT NVL(MAX(INV_SEQ) + 1, 1) INV_SEQ
FROM INV_AR_KR_ISS
WHERE INV_NO = @[inv_no]			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
