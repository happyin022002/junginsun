<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchChangedDataForSysClearRSQL">
			<desc><![CDATA[이슈 대상 중에 이미 Sys Clear가 실행된 AR IF NO를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT  AR_IF_NO
FROM    INV_AR_MN
WHERE    INV_CLR_FLG = 'Y'
AND       AR_IF_NO IN (
                  SELECT AR_IF_NO
                  FROM   INV_AR_ISS_FTR
                  WHERE  INV_ISS_WRK_NO = @[wrk_no]
                  GROUP BY AR_IF_NO
                  )			]]></sql>
			<params>
				<param name="wrk_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
