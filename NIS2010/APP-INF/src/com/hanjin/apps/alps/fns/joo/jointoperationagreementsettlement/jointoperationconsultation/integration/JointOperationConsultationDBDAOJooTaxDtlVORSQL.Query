<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOJooTaxDtlVORSQL">
			<desc><![CDATA[joo_tax_dtl 조회]]></desc>
			<sql><![CDATA[
SELECT
 TAX_INV_YRMON
,OFC_CD
,TAX_SER_NO
,TAX_SEQ
,ITM_NM
,SPL_AMT
,TAX_AMT
,TTL_AMT
,TO_CHAR(CRE_DT,'YYYYMMDDHH24MISS') AS CRE_DT
,CRE_USR_ID
,TO_CHAR(UPD_DT,'YYYYMMDDHH24MISS') AS UPD_DT
,UPD_USR_ID
FROM JOO_TAX_DTL
WHERE TAX_INV_YRMON = @[tax_inv_yrmon]
AND   OFC_CD        = @[ofc_cd]
AND   TAX_SER_NO    = @[tax_ser_no]
ORDER BY TAX_SEQ			]]></sql>
			<params>
				<param name="tax_inv_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="tax_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
