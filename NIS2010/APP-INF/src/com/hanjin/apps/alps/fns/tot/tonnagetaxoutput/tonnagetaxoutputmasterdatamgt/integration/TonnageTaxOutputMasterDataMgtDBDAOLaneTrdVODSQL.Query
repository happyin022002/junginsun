<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAOLaneTrdVODSQL">
			<desc><![CDATA[lane에 해당하는 trd_code를 모두 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM TOT_LANE_TRD
WHERE	STL_YRMON = @[stl_yrmon]
#if (${sts_flg} != "D")
AND	VSL_SLAN_CD = @[vsl_slan_cd]
#end			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
