<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOAddBsaAddTeuQtyCopyCSQL">
			<desc><![CDATA[BSA Entry화면에서 Row copy시 ADD BSA도 자동 생성되도록 한다.]]></desc>
			<sql><![CDATA[
INSERT INTO JOO_BSA_ADD_CRR
SELECT @[jo_bsa_ref_no]
     , JO_CRR_CD
     , JO_BSA_ADD_REF_SEQ
     , JO_BSA_TEU_QTY
     ,'Y'
     , SYSDATE
     ,@[upd_usr_id]
     , SYSDATE
     ,@[upd_usr_id]
FROM JOO_BSA_ADD_CRR
WHERE JO_BSA_REF_NO = @[jo_bsa_ref_no_copy]
  AND JO_VER_FLG = 'Y'			]]></sql>
			<params>
				<param name="jo_bsa_ref_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="jo_bsa_ref_no_copy" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
