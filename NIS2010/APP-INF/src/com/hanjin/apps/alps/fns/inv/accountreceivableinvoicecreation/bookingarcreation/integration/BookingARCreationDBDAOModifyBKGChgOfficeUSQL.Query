<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyBKGChgOfficeUSQL">
			<desc><![CDATA[INV_BKG_IF_CHG에 CutOff된 ArOfcCd를 Update]]></desc>
			<sql><![CDATA[
UPDATE INV_BKG_IF_CHG
   SET AR_OFC_CD = @[ar_ofc_cd]
 WHERE  BKG_NO = @[bkg_no]  
AND     BKG_SEQ = @[bkg_seq]
AND     CHG_SEQ = @[chg_seq]			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
				<param name="chg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
