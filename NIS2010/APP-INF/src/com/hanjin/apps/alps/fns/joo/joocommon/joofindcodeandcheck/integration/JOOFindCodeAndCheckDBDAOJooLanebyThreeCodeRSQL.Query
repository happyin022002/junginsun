<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOJooLanebyThreeCodeRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT
     distinct
	 substr(a.rlane_cd,0,3) as code,
	 a.rlane_cd as name
FROM joo_carrier  a
WHERE a.delt_flg = 'N'
#if (${super_cd1} != '')
AND	  a.jo_crr_cd = @[super_cd1]
#end
#if (${super_cd2} != '')
AND	  a.trd_cd = @[super_cd2]
#end
order by 1			]]></sql>
			<params>
				<param name="super_cd1" type="12" value="" out="N"/>
				<param name="super_cd2" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
