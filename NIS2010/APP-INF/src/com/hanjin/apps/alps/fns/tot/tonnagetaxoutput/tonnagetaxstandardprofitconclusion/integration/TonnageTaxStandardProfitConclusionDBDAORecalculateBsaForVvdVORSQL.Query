<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAORecalculateBsaForVvdVORSQL">
			<desc><![CDATA[재계산된 TOT_PORT_STL_AMT 의 데이터 추출]]></desc>
			<sql><![CDATA[
SELECT STL_YRMON
      ,VSL_CD
      ,MAX(LDB_CAPA_QTY)            LDB_CAPA_QTY
      ,MAX(USG_RT)                  USG_RT
      ,SUM(TRUNC(TONG_TAX_AMT,0))            TONG_TAX_AMT
FROM TOT_PORT_STL_AMT
WHERE STL_YRMON = @[stl_yrmon]
  AND VSL_CD = @[vsl_cd]
  AND TONG_STL_BAT_JB_SEQ = (SELECT MAX(TONG_STL_BAT_JB_SEQ) FROM TOT_PORT_STL_AMT WHERE STL_YRMON = @[stl_yrmon] 
                                                                                     AND VSL_CD = @[vsl_cd] 
                                                                                    )
GROUP BY STL_YRMON, VSL_CD			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
