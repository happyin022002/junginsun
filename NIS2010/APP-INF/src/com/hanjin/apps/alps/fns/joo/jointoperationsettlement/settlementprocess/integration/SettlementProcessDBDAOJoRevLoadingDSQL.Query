<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOJoRevLoadingDSQL">
			<desc><![CDATA[Jo Rev Loading 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM JOO_LODG_TGT J
WHERE 1=1
AND J.REV_YRMON		= @[rev_yrmon]		      
--AND J.REV_YRMON_SEQ 
AND J.TRD_CD        = @[trd_cd]
AND J.CRR_CD        = @[crr_cd]
AND J.RLANE_CD      = @[rlane_cd]
AND J.RE_DIVR_CD    = 'R'
AND J.VSL_CD        = @[vsl_cd]  
AND J.SKD_VOY_NO    = @[skd_voy_no]	 
AND J.SKD_DIR_CD    = @[skd_dir_cd]  
AND J.VPS_PORT_CD   = @[vps_port_cd] 
#if (${yd_cd} != '')  
AND J.YD_CD         = @[yd_cd]
#end
#if (${clpt_ind_seq} != '') 
AND J.CLPT_IND_SEQ	= @[clpt_ind_seq]
#end			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="vps_port_cd" type="12" value="" out="N"/>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="clpt_ind_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
