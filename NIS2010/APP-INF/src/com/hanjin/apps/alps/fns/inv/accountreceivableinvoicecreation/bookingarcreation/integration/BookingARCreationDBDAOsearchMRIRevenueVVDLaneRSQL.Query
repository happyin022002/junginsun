<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchMRIRevenueVVDLaneRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchMRIRevenueVVDLaneRSQL]]></desc>
			<sql><![CDATA[
#if (${vsl} == 'USAC') 

SELECT V.RLANE_CD REV_LANE , T.VSL_CD||T.SKD_VOY_NO||T.SKD_DIR_CD REV_VVD, T.LANE SLANE_CD 
FROM AR_MST_REV_VVD V,
        (
          SELECT VSL_CD, SKD_VOY_NO, SKD_DIR_CD||NVL(C.RLANE_DIR_CD, SKD_DIR_CD) SKD_DIR_CD,
               DECODE(VSL_CD, 'COMC','COM','CNTC','CNT', 'USAC','USA','WLXC','WLX', DECODE(LANE,'SYS','RBC',LANE) ) LANE
        FROM AR_FINC_DIR_CONV C,
                (
                SELECT  NVL(@[lane_cd],'RBC') LANE,  SUBSTR(@[vvd], 1, 4) VSL_CD, SUBSTR(@[vvd], 5, 4) SKD_VOY_NO
                      , SUBSTR(@[vvd], 9, 1) SKD_DIR_CD, @[pol] POL, SCONTI_CD
                FROM MDM_LOCATION L
                WHERE L.LOC_CD = @[pol]) B
        WHERE  C.SLAN_CD(+) = B.LANE
        AND  C.SLAN_DIR_CD(+) = B.SKD_DIR_CD
        AND  NVL(C.AP_MK_FLG(+),'N') <> 'Y'
        AND  C.SCONTI_CD(+) = B.SCONTI_CD ) T
    WHERE V.VSL_CD = T.VSL_CD
AND   V.SKD_VOY_NO = T.SKD_VOY_NO
--AND   V.SKD_DIR_CD = T.SKD_DIR_CD
AND   V.SKD_DIR_CD = SUBSTR(T.SKD_DIR_CD,1,1)
AND   V.RLANE_DIR_CD = SUBSTR(T.SKD_DIR_CD,2,1)
AND   NVL(V.DELT_FLG,'N') <> 'Y' 

#else
 
SELECT SUBSTR(AA,4,5) REV_LANE, SUBSTR(AA,9) REV_VVD, '' SLANE_CD
    FROM ( SELECT MIN(SUBSTR(TO_CHAR(N.RNK_SEQ,'999'),2)||N.RLANE_CD||Q.VSL_CD||Q.SKD_VOY_NO||Q.SKD_DIR_CD) AA
            FROM AR_ROUT_RNK N,
                    (
                    SELECT V.RLANE_CD , T.VSL_CD, T.SKD_VOY_NO, T.SKD_DIR_CD, T.LANE
                    FROM AR_MST_REV_VVD V,
                            (
                              SELECT VSL_CD, SKD_VOY_NO, SKD_DIR_CD||NVL(C.RLANE_DIR_CD, SKD_DIR_CD) SKD_DIR_CD,
                                   DECODE(VSL_CD, 'COMC','COM','CNTC','CNT', 'USAC','USA','WLXC','WLX', DECODE(LANE,'SYS','RBC',LANE) ) LANE
                            FROM AR_FINC_DIR_CONV C,
                                    (
                                    SELECT  NVL(@[lane_cd],'RBC') LANE,  SUBSTR(@[vvd], 1, 4) VSL_CD, SUBSTR(@[vvd], 5, 4) SKD_VOY_NO
                                          , SUBSTR(@[vvd], 9, 1) SKD_DIR_CD, @[pol] POL, SCONTI_CD
                                    FROM MDM_LOCATION L
                                    WHERE L.LOC_CD = @[pol]) B
                            WHERE  C.SLAN_CD(+) = B.LANE
                            AND  C.SLAN_DIR_CD(+) = B.SKD_DIR_CD
                            AND  NVL(C.AP_MK_FLG(+),'N') <> 'Y'
                            AND  C.SCONTI_CD(+) = B.SCONTI_CD ) T
                        WHERE V.VSL_CD = T.VSL_CD
                    AND   V.SKD_VOY_NO = T.SKD_VOY_NO
                    --AND   V.SKD_DIR_CD = T.SKD_DIR_CD
                    AND   V.SKD_DIR_CD = SUBSTR(T.SKD_DIR_CD,1,1)
                    AND   V.RLANE_DIR_CD = SUBSTR(T.SKD_DIR_CD,2,1)
                    AND   NVL(V.DELT_FLG,'N') <> 'Y' ) Q
            WHERE N.RLANE_CD = Q.RLANE_CD
            AND   N.SLAN_CD = Q.LANE
            AND   SUBSTR(N.ZN_IOC_CD,1,2) = @[zone_ioc] )
            
#end            			]]></sql>
			<params>
				<param name="lane_cd" type="12" value="IMU" out="N"/>
				<param name="vvd" type="12" value="CCMO1509W" out="N"/>
				<param name="pol" type="12" value="INNAH" out="N"/>
				<param name="zone_ioc" type="12" value="OO" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
