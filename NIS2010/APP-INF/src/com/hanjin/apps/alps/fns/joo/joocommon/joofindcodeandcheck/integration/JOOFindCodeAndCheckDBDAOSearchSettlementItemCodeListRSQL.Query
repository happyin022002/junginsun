<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JOOFindCodeAndCheckDBDAOSearchSettlementItemCodeListRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT A.JO_STL_ITM_CD CODE,
       A.JO_STL_ITM_NM NAME 
  FROM JOO_STL_ITM A 
 WHERE 1 = 1
#if (${code} != '') 
   AND A.JO_STL_ITM_CD = @[code]
#end
#if (${sortkey} != '') 
   ORDER BY @[sortkey]
#else
ORDER BY CASE WHEN A.JO_STL_ITM_CD = 'S/H'  THEN 1 
              WHEN A.JO_STL_ITM_CD = 'OUS'  THEN 2 
              WHEN A.JO_STL_ITM_CD = 'R/F'  THEN 3 
              WHEN A.JO_STL_ITM_CD = 'OTH'  THEN 4 
              WHEN A.JO_STL_ITM_CD = 'W/R'  THEN 5 
              WHEN A.JO_STL_ITM_CD = 'P/B'  THEN 6 END
#end
 			]]></sql>
			<params>
				<param name="code" type="12" value="" out="N"/>
				<param name="sortkey" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
