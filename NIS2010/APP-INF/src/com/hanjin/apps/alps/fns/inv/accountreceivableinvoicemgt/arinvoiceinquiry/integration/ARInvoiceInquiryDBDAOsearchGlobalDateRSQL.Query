<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAOsearchGlobalDateRSQL">
			<desc><![CDATA[searchGlobalDate]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('SELHO', TO_DATE(@[from_dt],'YYYY-MM-DD'),@[ofc_cd]),'YYYYMMDD') AS FROM_DT
      ,TO_CHAR(GLOBALDATE_PKG.TIME_CONV_OFC_FNC('SELHO', TO_DATE(@[to_dt],'YYYY-MM-DD'),@[ofc_cd]),'YYYYMMDD') AS TO_DT 
FROM DUAL 			]]></sql>
			<params>
				<param name="from_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
