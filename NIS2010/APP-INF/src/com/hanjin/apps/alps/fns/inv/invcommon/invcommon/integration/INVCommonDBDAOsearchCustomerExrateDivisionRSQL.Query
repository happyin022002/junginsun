<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOsearchCustomerExrateDivisionRSQL">
			<desc><![CDATA[searchCustomerExrateDivision]]></desc>
			<sql><![CDATA[
SELECT NVL(SUBSTR(XCH_RT_DIV_CD, 1, 1), 'N') div_cd1,
            NVL(SUBSTR(XCH_RT_DIV_CD, 2, 1), 'N') div_cd2,        
            NVL(DY_XCH_APLY_ST_DT, '99991231') aply_st_dt, 
            NVL(CNG_INDIV_CD, 'N') cng_indiv_cd
    FROM   MDM_CR_CUST
    WHERE  CUST_CNT_CD = @[inv_cntry_cd]
    AND    CUST_SEQ  = TO_NUMBER(@[inv_cust_cd])
    AND    DELT_FLG <> 'Y'
	AND    NVL(SUB_SYS_NM,'MDM') <> 'ERP'			]]></sql>
			<params>
				<param name="inv_cntry_cd" type="12" value="ES" out="N"/>
				<param name="inv_cust_cd" type="12" value="556" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
