<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAORemoveBsaInformationEntryUSQL">
			<desc><![CDATA[RemoveBsaInformationEntry

-- 2016.03.16 Carrier정보 상관없이 모두 지우던것을 Carrier적용하여 삭제처리하도록 변경]]></desc>
			<sql><![CDATA[
UPDATE JOO_BSA_AGMT 
SET DELT_FLG = 'Y'
 WHERE TRD_CD       = @[trd_cd]
   AND RLANE_CD     = @[rlane_cd]
   AND VSL_CD       = @[vsl_cd]
   AND SKD_VOY_NO   = @[skd_voy_no]
   AND SKD_DIR_CD   = @[skd_dir_cd]
   AND PORT_CD	    = @[port_cd]
   AND PORT_SEQ	    = @[port_seq]
   AND JO_CRR_CD    = @[jo_crr_cd]			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="port_seq" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
