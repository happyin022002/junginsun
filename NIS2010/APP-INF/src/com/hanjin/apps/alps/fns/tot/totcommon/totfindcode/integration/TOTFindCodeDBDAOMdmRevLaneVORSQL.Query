<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TOTFindCodeDBDAOMdmRevLaneVORSQL">
			<desc><![CDATA[mdm_rev_lane  테이블의 lane정보를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT 
	DISTINCT VSL_SLAN_CD
FROM MDM_REV_LANE
WHERE VSL_SLAN_CD = @[vsl_slan_cd]			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="AEC" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
