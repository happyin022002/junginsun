<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchMaxInterfaceListRSQL">
			<desc><![CDATA[BookingARCreationDBDAOSearchMaxInterfaceListRSQL]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
	   MN.AR_OFC_CD,
       MN.AR_IF_NO,
       MN.VSL_CD||MN.SKD_VOY_NO||MN.SKD_DIR_CD VVD_CD,
       MN.SVC_SCP_CD,
       MN.SLAN_CD,
       MN.POL_CD,
       MN.POD_CD,
       MN.REV_VSL_CD||MN.REV_SKD_VOY_NO||MN.REV_SKD_DIR_CD||MN.REV_DIR_CD REV_VVD_CD,
       MN.RLANE_CD,
       MN.INV_DELT_DIV_CD,
       MN.BL_INV_CFM_DT,
       MN.BKG_NO,
       SUBSTR(ORG.LOC_CD, 1, 2) LOC_CD,
	   MN.IO_BND_CD,
       '' CREATE_FLAG,
       MN.BKG_TEU_QTY,
       MN.BKG_FEU_QTY,
       MN.BL_INV_IF_DT,
	   MN.OLD_AR_IF_NO,
       MN.RVS_CHG_FLG,
       MN.ACT_CUST_CNT_CD,
       MN.ACT_CUST_SEQ
  FROM INV_AR_MN MN,
       MDM_ORGANIZATION ORG
 WHERE MN.AR_IF_NO  IN (SELECT MAX(AR_IF_NO) 
                      FROM INV_AR_MN
                     WHERE BL_SRC_NO = @[bl_no]
                       AND NVL(INV_DELT_DIV_CD,'N')<> 'Y'
                       AND REV_TP_CD<> 'M'
                       AND NVL(INV_SPLIT_CD,'N') NOT IN ('S','X','C','E') --2009.12.21 이상희과장 ,2009.12.24 C 다시뺌, 2010.03.05 C다시 넣음, 2010.04.07 C 다시뺌 PUSE52299004
                     GROUP BY AR_OFC_CD,BL_SRC_NO)
   AND MN.AR_OFC_CD = ORG.OFC_CD
   AND NVL(MN.INV_DELT_DIV_CD,'N')<> 'Y'
   AND MN.REV_TP_CD<> 'M'
   AND NVL(MN.INV_SPLIT_CD,'N') NOT IN ('S','X','C','E')			]]></sql>
			<params>
				<param name="bl_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
