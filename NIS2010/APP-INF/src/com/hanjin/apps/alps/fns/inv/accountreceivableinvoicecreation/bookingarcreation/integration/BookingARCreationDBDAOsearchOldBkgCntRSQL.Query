<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchOldBkgCntRSQL">
			<desc><![CDATA[searchOldBkgCnt]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) OLD_BKG_CNT
FROM BKG_BOOKING 
WHERE BKG_NO = @[bkg_no]
AND TO_CHAR(BKG_CRE_DT, 'YYYYMMDD') < '20170213'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
