<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOmodifyTeuFeuInvRefNumberUSQL">
			<desc><![CDATA[BookingARCreationDBDAOmodifyTeuFeuInvRefNumberUSQL]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
    SET BKG_TEU_QTY = NVL(@[bkg_teu_qty], 0)
      , BKG_FEU_QTY = NVL(@[bkg_feu_qty], 0)
      , INV_REF_NO = @[inv_ref_no]
      , UPD_USR_ID = @[user_id] 
      , UPD_DT = SYSDATE
  WHERE AR_IF_NO = @[if_no]			]]></sql>
			<params>
				<param name="bkg_teu_qty" type="12" value="" out="N"/>
				<param name="bkg_feu_qty" type="12" value="" out="N"/>
				<param name="inv_ref_no" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
