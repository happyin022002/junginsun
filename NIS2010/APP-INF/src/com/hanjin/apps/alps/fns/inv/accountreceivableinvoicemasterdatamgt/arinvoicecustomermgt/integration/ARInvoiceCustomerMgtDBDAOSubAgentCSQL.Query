<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOSubAgentCSQL">
			<desc><![CDATA[INV_SUB_AGN_CUST_CD 테이블에  Office 별 Agent정보를 등록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO INV_SUB_AGN_CUST_CD(
	AR_OFC_CD,
    IO_BND_CD, 
    POR_CD,
    POL_CD,
    POD_CD,
    DEL_CD,
	AGN_NM,
	REP_CUST_CNT_CD,
	REP_CUST_SEQ,
	DELT_FLG,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT
) VALUES (
	@[ar_ofc_cd],
    @[io_bnd_cd],
    DECODE(@[por_cd], null, 'XXXXX', @[por_cd]),
    DECODE(@[pol_cd], null, 'XXXXX', @[pol_cd]), 
    DECODE(@[pod_cd], null, 'XXXXX', @[pod_cd]),
    DECODE(@[del_cd], null, 'XXXXX', @[del_cd]),
	@[agn_nm],
	@[rep_cust_cnt_cd],
	@[rep_cust_seq],
	'N',
	@[cre_usr_id],
	SYSDATE,
	@[upd_usr_id],
	SYSDATE
)			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
				<param name="agn_nm" type="12" value="" out="N"/>
				<param name="rep_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="rep_cust_seq" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
