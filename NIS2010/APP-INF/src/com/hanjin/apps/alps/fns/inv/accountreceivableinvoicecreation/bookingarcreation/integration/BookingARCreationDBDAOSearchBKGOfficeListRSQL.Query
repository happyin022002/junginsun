<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchBKGOfficeListRSQL">
			<desc><![CDATA[BookingARCreationDBDAOSearchBKGOfficeListRSQL]]></desc>
			<sql><![CDATA[
SELECT BKG.IO_BND_CD IO_BND_CD,
BKG.CUST_CNT_CD CUST_CNT_CD,
BKG.CUST_SEQ CUST_SEQ,
BKG.N3RD_FLG N3RD_FLG,
BKG.OFC_CD OFC_CD,
BKG.CHG_SEQ CHG_SEQ,
ORG.AR_OFC_CD M_AR_OFC_CD
FROM
    (SELECT IO_BND_CD,
    CUST_CNT_CD,
    CUST_SEQ,
    N3RD_FLG,
    OFC_CD,
	CHG_SEQ
    FROM INV_BKG_IF_CHG
    WHERE BKG_NO = @[bkg_no]
    AND BKG_SEQ = @[bkg_seq])BKG, MDM_ORGANIZATION ORG
WHERE BKG.OFC_CD = ORG.OFC_CD			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="bkg_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
