<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyReverseChargeFlagUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
   SET RVS_CHG_FLG = @[rvs_chg_flg]
      ,UPD_USR_ID  = @[usr_id]
      ,UPD_DT      = SYSDATE
 WHERE AR_IF_NO = @[ar_if_no]
   AND NVL(RVS_CHG_FLG,'N') = DECODE(NVL(@[rvs_chg_flg],'N'),'Y','N','Y')			]]></sql>
			<params>
				<param name="rvs_chg_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
