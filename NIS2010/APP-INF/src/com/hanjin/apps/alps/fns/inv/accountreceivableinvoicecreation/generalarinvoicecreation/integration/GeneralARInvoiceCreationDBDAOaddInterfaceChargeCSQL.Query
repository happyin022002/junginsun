<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOaddInterfaceChargeCSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOaddInterfaceChargeCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO INV_AR_IF_CHG (
       SRC_IF_DT
     , SRC_IF_SEQ
     , CHG_SEQ
     , CHG_CD
     , CHG_AMT
     , CURR_CD -----
     , PER_TP_CD        
     , TRF_RT_AMT       
     , RAT_AS_CNTR_QTY  
     , INV_XCH_RT       
     , CHG_FULL_NM      
     , TRF_NO           
     , TVA_FLG          
     , REP_CHG_CD       
     , CHG_RMK   
     , CRE_USR_ID -----
     , CRE_DT
     , UPD_USR_ID
     , UPD_DT
) VALUES (
       @[src_if_dt]
     , @[src_if_seq]
     , (SELECT NVL(MAX(CHG_SEQ) + 1, 1)
          FROM INV_AR_IF_CHG
         WHERE SRC_IF_DT = @[src_if_dt]
           AND SRC_IF_SEQ = @[src_if_seq])
     , @[chg_cd]
     , NVL(@[chg_amt], 0)
     , @[curr_cd] -----
     , @[per_tp_cd]  
     , ABS(NVL(@[trf_rt_amt], 0))  
     , NVL(@[rat_as_cntr_qty], 0)  
     , NVL(@[inv_xch_rt], 0)  
     , @[chg_full_nm]  
     , @[trf_no]  
     , NVL(@[tva_flg], 'N')  
     , @[rep_chg_cd]  
     , @[chg_rmk]  
     , @[cre_usr_id] -----
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
)			]]></sql>
			<params>
				<param name="src_if_dt" type="12" value="" out="N"/>
				<param name="src_if_seq" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="chg_amt" type="12" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="per_tp_cd" type="12" value="" out="N"/>
				<param name="trf_rt_amt" type="12" value="" out="N"/>
				<param name="rat_as_cntr_qty" type="12" value="" out="N"/>
				<param name="inv_xch_rt" type="12" value="" out="N"/>
				<param name="chg_full_nm" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="tva_flg" type="12" value="" out="N"/>
				<param name="rep_chg_cd" type="12" value="" out="N"/>
				<param name="chg_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
