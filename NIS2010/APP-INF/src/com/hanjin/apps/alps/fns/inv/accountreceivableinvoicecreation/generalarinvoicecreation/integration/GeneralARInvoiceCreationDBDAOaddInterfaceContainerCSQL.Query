<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceCreationDBDAOaddInterfaceContainerCSQL">
			<desc><![CDATA[GeneralARInvoiceCreationDBDAOaddInterfaceContainerCSQL]]></desc>
			<sql><![CDATA[
INSERT INTO INV_AR_IF_CNTR(          
       SRC_IF_DT	
     , SRC_IF_SEQ	
     , CNTR_SEQ	
     , CNTR_NO	
     , CNTR_TPSZ_CD	
     , CRE_USR_ID	
     , CRE_DT	
     , UPD_USR_ID	
     , UPD_DT
) VALUES (
       @[src_if_dt]
     , @[src_if_seq]
     , (SELECT NVL(MAX(CNTR_SEQ) + 1, 1)
          FROM INV_AR_IF_CNTR
         WHERE SRC_IF_DT = @[src_if_dt]
           AND SRC_IF_SEQ = @[src_if_seq])
     , @[cntr_no]
     , @[cntr_tpsz_cd]	
     , @[cre_usr_id]
     , SYSDATE
     , @[upd_usr_id]
     , SYSDATE
)			]]></sql>
			<params>
				<param name="src_if_dt" type="12" value="" out="N"/>
				<param name="src_if_seq" type="12" value="" out="N"/>
				<param name="cntr_no" type="12" value="" out="N"/>
				<param name="cntr_tpsz_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
