<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOsearchAgentListVORSQL">
			<desc><![CDATA[FNS_INV_0081]]></desc>
			<sql><![CDATA[
SELECT DISTINCT 
	   A.AR_OFC_CD AGN_CD,
       A.AR_OFC_CD,
	   B.CHN_AGN_CD,
       B.CUST_CNT_CD, 
       LPAD(B.CUST_SEQ,6,0) CUST_SEQ, 
       B.VNDR_CNT_CD VNDR_CNT_CD, 
       LPAD(B.VNDR_SEQ,6,0) VNDR_SEQ
  FROM MDM_ORGANIZATION A, BKG_CHN_AGN B , MDM_ORGANIZATION D
 WHERE A.AR_HD_QTR_OFC_CD = (SELECT AR_HD_QTR_OFC_CD 
  FROM MDM_ORGANIZATION WHERE OFC_CD= @[ofc_cd])
   AND A.AR_OFC_CD NOT LIKE '%BB'
   AND B.CHN_AGN_CD = SUBSTR(A.AR_OFC_CD,4,2)
   AND D.OFC_CD = @[ofc_cd]
   AND SUBSTR(A.LOC_CD, 1, 2) = SUBSTR(D.LOC_CD, 1, 2) 
 ORDER BY A.AR_OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="SIABS" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
