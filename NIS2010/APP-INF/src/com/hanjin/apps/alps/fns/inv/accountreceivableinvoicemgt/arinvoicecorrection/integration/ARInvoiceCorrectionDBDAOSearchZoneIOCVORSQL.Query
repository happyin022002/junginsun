<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCorrectionDBDAOSearchZoneIOCVORSQL">
			<desc><![CDATA[ARInvoiceCorrectionDBDAO::searchZoneIOC ( pol , pod )]]></desc>
			<sql><![CDATA[
SELECT  DECODE(MIN(pol_conti)||MIN(pod_conti), 'AA','IA', 'EE','IE','MM','IM',
        'EF','IE','FE','IE','FF','IE','OO') zone_ioc
FROM   ( SELECT conti_cd pol_conti, NULL pod_conti, sconti_cd
    FROM  MDM_LOCATION 
    WHERE  loc_cd =@[pol_cd]
   UNION ALL
   SELECT NULL pol_conti, conti_cd pod_conti, NULL sconti_cd
    FROM  MDM_LOCATION
    WHERE  loc_cd =@[pod_cd] )			]]></sql>
			<params>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
