<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOBlankVoyageStatusUSQL">
			<desc><![CDATA[Blank Voyage Status 정보 Update]]></desc>
			<sql><![CDATA[
UPDATE 	joo_cmpn_agmt 
SET		ofc_cd				= @[ofc_cd],         
		agmt_ofc_cd			= @[agmt_ofc_cd],    
		jo_cmpn_knd_cd		= @[jo_cmpn_knd_cd], 
		cmpn_agmt_yrmon		= @[cmpn_agmt_yrmon],
		cmpn_agmt_yrwk		= @[cmpn_agmt_yrwk], 
		vsl_cd				= @[vsl_cd],         
		skd_voy_no			= @[skd_voy_no],     
		skd_dir_cd			= @[skd_dir_cd],     
		bsa_qty				= @[bsa_qty],        
		bsa_slt_prc			= @[bsa_slt_prc],    
		agmt_ttl_amt		= @[agmt_ttl_amt],   
		atch_file_id		= @[atch_file_id],   
		cmpn_agmt_rmk		= @[cmpn_agmt_rmk],  
		stl_flg				= @[stl_flg],        
		stl_vsl_cd			= @[stl_vsl_cd],     
		stl_voy_no			= @[stl_voy_no],     
		stl_dir_cd			= @[stl_dir_cd],     
		stl_dt				= @[stl_dt],         
		delt_flg			= @[delt_flg],           
		upd_dt				= sysdate,         
		upd_usr_id 			= @[upd_usr_id]  
WHERE	1 = 1
		AND jo_crr_cd 		= @[jo_crr_cd]
		AND trd_cd 			= @[trd_cd]
		AND rlane_cd		= @[rlane_cd]       
		AND re_divr_cd 		= @[re_divr_cd] 
		AND cmpn_agmt_seq	= @[cmpn_agmt_seq]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="agmt_ofc_cd" type="12" value="" out="N"/>
				<param name="jo_cmpn_knd_cd" type="12" value="" out="N"/>
				<param name="cmpn_agmt_yrmon" type="12" value="" out="N"/>
				<param name="cmpn_agmt_yrwk" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="bsa_qty" type="12" value="" out="N"/>
				<param name="bsa_slt_prc" type="12" value="" out="N"/>
				<param name="agmt_ttl_amt" type="12" value="" out="N"/>
				<param name="atch_file_id" type="12" value="" out="N"/>
				<param name="cmpn_agmt_rmk" type="12" value="" out="N"/>
				<param name="stl_flg" type="12" value="" out="N"/>
				<param name="stl_vsl_cd" type="12" value="" out="N"/>
				<param name="stl_voy_no" type="12" value="" out="N"/>
				<param name="stl_dir_cd" type="12" value="" out="N"/>
				<param name="stl_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="cmpn_agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
