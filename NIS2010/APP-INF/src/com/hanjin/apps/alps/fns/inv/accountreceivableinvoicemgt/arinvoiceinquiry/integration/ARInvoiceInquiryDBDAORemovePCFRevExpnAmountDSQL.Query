<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceInquiryDBDAORemovePCFRevExpnAmountDSQL">
			<desc><![CDATA[Remove PCF Rev Expn Amount]]></desc>
			<sql><![CDATA[
DELETE FROM INV_PCF_REV_EXPN_AMT
WHERE PORT_CD = @[port_cd]
AND RE_DIVR_CD = @[re_divr_cd]
AND RE_PORT_SEQ = @[re_port_seq]			]]></sql>
			<params>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="re_port_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
