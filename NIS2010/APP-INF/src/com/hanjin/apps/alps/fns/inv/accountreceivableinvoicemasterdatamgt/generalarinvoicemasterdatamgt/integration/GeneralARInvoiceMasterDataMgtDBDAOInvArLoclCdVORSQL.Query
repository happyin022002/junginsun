<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceMasterDataMgtDBDAOInvArLoclCdVORSQL">
			<desc><![CDATA[InvArLoclCdVO]]></desc>
			<sql><![CDATA[
SELECT  DISTINCT(LOC_CD) LOC_CD
FROM    INV_AR_LOCL_CHG
WHERE   1=1
#if(${ar_ofc_cd} != 'ALL')
AND AR_OFC_CD = @[ar_ofc_cd]
#end			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
