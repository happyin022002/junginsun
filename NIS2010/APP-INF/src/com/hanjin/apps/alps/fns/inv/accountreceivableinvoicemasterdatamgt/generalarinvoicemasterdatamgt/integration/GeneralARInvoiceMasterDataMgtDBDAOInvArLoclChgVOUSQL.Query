<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GeneralARInvoiceMasterDataMgtDBDAOInvArLoclChgVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_LOCL_CHG SET 
	TRF_RT_AMT = @[trf_rt_amt]
,	INV_CHG_UT_DIV_CD = @[inv_chg_ut_div_cd]
,	LOCL_CHG_IF_AUTO_CD = @[locl_chg_if_auto_cd]
,	CHG_RMK = @[chg_rmk]
,	UPD_DT = SYSDATE
,	UPD_USR_ID = @[upd_usr_id]
,	ACCT_CD = @[acct_cd]
WHERE	AR_OFC_CD = @[ar_ofc_cd]
AND	LOC_CD = @[loc_cd]
AND	SVC_SCP_CD = @[svc_scp_cd]
AND	CHG_CD = @[chg_cd]
AND	IO_BND_CD = @[io_bnd_cd]
AND	CUST_CNT_CD = @[cust_cnt_cd]
AND	CUST_SEQ = @[cust_seq]
AND	CURR_CD = @[curr_cd]			]]></sql>
			<params>
				<param name="trf_rt_amt" type="6" value="" out="N"/>
				<param name="inv_chg_ut_div_cd" type="12" value="" out="N"/>
				<param name="locl_chg_if_auto_cd" type="12" value="" out="N"/>
				<param name="chg_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="acct_cd" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
				<param name="chg_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="2" value="" out="N"/>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
