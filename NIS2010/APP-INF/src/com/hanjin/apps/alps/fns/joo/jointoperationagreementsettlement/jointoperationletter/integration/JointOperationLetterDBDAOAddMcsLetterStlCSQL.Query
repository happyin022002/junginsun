<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOAddMcsLetterStlCSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
INSERT INTO JOO_LTR_STL 
    (JO_LTR_SEQ,
    JO_LTR_STL_SEQ,
    VSL_SLAN_CD,
    TTL_AMT,
    JO_HJS_AMT,
    JO_PRNR_AMT,
    JO_BAL_AMT,
    STL_RMK,
    ACCT_YRMON,
    JO_CRR_CD, 
    STL_CMB_SEQ,
    CRE_DT,
    CRE_USR_ID,
    UPD_DT,
    UPD_USR_ID)
VALUES(@[jo_ltr_seq],
    @[jo_ltr_stl_seq],
    @[rlane_cd],
    @[ttl_amt],
    @[jo_hjs_amt],
    @[jo_prnr_amt],
    REPLACE(REPLACE(REPLACE(@[jo_bal_amt],',',''),')',''),'(','')   ,
    @[stl_rmk],
    REPLACE(@[acct_yrmon],'-',''),
    @[jo_crr_cd], 
    @[stl_cmb_seq],
    sysdate,
    @[cre_usr_id],
    sysdate,
    @[upd_usr_id] )				]]></sql>
			<params>
				<param name="jo_ltr_seq" type="12" value="" out="N"/>
				<param name="jo_ltr_stl_seq" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="ttl_amt" type="12" value="" out="N"/>
				<param name="jo_hjs_amt" type="12" value="" out="N"/>
				<param name="jo_prnr_amt" type="12" value="" out="N"/>
				<param name="jo_bal_amt" type="12" value="" out="N"/>
				<param name="stl_rmk" type="12" value="" out="N"/>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="stl_cmb_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
