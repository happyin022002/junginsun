<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchInvSubAgnCustomerRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT REP_CUST_CNT_CD ACT_CUST_CNT_CD
      ,REP_CUST_SEQ    ACT_CUST_SEQ
  FROM INV_SUB_AGN_CUST_CD A,
       MDM_CUSTOMER        B
 WHERE A.REP_CUST_CNT_CD = B.CUST_CNT_CD
   AND A.REP_CUST_SEQ = B.CUST_SEQ
   AND A.AR_OFC_CD = @[ar_ofc_cd]
   AND A.IO_BND_CD = @[io_bnd_cd]
   AND DECODE(A.POR_CD,'XXXXX', @[por_cd], A.POR_CD) = @[por_cd]
   AND DECODE(A.POL_CD,'XXXXX', @[pol_cd], A.POL_CD) = @[pol_cd]
   AND DECODE(A.POD_CD,'XXXXX', @[pod_cd], A.POD_CD) = @[pod_cd]
   AND DECODE(A.DEL_CD,'XXXXX', @[del_cd], A.DEL_CD) = @[del_cd]
   AND NVL(A.DELT_FLG,'N') <> 'Y'
   AND NVL(B.DELT_FLG,'N') <> 'Y'			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="por_cd" type="12" value="" out="N"/>
				<param name="pol_cd" type="12" value="" out="N"/>
				<param name="pod_cd" type="12" value="" out="N"/>
				<param name="del_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
