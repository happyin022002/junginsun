<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchContracSrepCdRSQL">
			<desc><![CDATA[BookingARCreationDBDAOSearchContracSrepCdRSQL]]></desc>
			<sql><![CDATA[
SELECT MAX(CTRT_SREP_CD) CTRT_SREP_CD
FROM   (
		SELECT DECODE(SUBSTR(CTRT_SREP_CD,1,2), 'US', CTRT_SREP_CD, 'CA', CTRT_SREP_CD, '') CTRT_SREP_CD
		FROM   BKG_BOOKING
		WHERE  BKG_NO = @[bl_src_no]
		AND    @[ar_ofc_cd] IN  ( SELECT ATTR_CTNT1
		                          FROM   BKG_HRD_CDG_CTNT
		                          WHERE  HRD_CDG_ID = 'BKG_INV_IF_SREP_OFC' 
		                        ) 
		UNION ALL
		SELECT '' CTRT_SREP_CD FROM DUAL
		)			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
