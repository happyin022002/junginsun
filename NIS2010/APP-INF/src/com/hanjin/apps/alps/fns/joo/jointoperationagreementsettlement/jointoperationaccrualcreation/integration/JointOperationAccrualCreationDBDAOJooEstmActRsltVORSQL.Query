<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationAccrualCreationDBDAOJooEstmActRsltVORSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
 SELECT  ITEM, REV_YRMON, RLANE_CD, RVVD, SUM(ESTM_AMT) ESTM_AMT, SUM(ACT_AMT) ACT_AMT, SUM(ACCL_AMT) ACCL_AMT
  FROM (
       SELECT DECODE(SUBSTR(ACCT_CD,1,1),'4','REV','5','EXP') ITEM
            , REV_YRMON, RLANE_CD, VSL_CD||SKD_VOY_NO||SKD_DIR_CD||REV_DIR_CD RVVD
            , ESTM_AMT, ACT_AMT, ACCL_AMT
         FROM JOO_ESTM_ACT_RSLT
        WHERE EXE_YRMON = @[exe_yrmon]
       )
GROUP BY ITEM, REV_YRMON, RLANE_CD, RVVD
 ORDER BY ITEM, RLANE_CD, RVVD			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
