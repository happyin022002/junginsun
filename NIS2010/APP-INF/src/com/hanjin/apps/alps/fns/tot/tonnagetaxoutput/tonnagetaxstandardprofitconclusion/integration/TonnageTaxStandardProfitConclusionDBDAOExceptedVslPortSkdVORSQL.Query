<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAOExceptedVslPortSkdVORSQL">
			<desc><![CDATA[운항스케쥴대비 배치시 누락 port 데이터 조회]]></desc>
			<sql><![CDATA[
SELECT T1.SLAN_CD                                                                                                                                                                                                                                                                                                          
      ,T2.TRD_CD                                                                                                                                                                                                                                                                                                           
      ,T1.VVD                                                                                                                                                                                                                                                                      
      ,T1.VPS_PORT_CD  PORT_CD                                                                                                                                                                                                                                                                                                   
      ,T1.TURN_PORT_IND_CD                                                                                                                                                                                                                                                                                                 
      ,T1.ACT_BSA_CAPA
      ,T1.VOY_DYS 
      ,T1.STL_YRMON      
      ,T1.VSL_CD                                                                                                                                                                                                                                                                                                           
      ,T1.SKD_VOY_NO                                                                                                                                                                                                                                                                                                       
      ,T1.SKD_DIR_CD    

FROM (SELECT C.SLAN_CD                                                                                                                                                                                                                                                                                                          
            ,C.TRD_CD                                                                                                                                                                                                                                                                                                           
            ,C.VVD                                                                                                                                                                                                                                                                      
            ,C.VPS_PORT_CD                                                                                                                                                                                                                                                                                                      
            ,C.TURN_PORT_IND_CD                                                                                                                                                                                                                                                                                                 
            ,DECODE(C.ACT_BSA_CAPA,NULL,'-',0,'0',C.ACT_BSA_CAPA)   ACT_BSA_CAPA
            ,DECODE(C.VOY_DYS,NULL,'-',0,'0',C.VOY_DYS) VOY_DYS 
            ,C.STL_YRMON      
            ,C.VSL_CD                                                                                                                                                                                                                                                                                                           
            ,C.SKD_VOY_NO                                                                                                                                                                                                                                                                                                       
            ,C.SKD_DIR_CD                                                                                                                                                                                                                                                                                                       
       FROM (SELECT A.VPS_ETD_DT                                                                                                                                                                                                                                                                                                 
                  ,A.SLAN_CD                                                                                                                                                                                                                                                                                                          
                  ,B.TRD_CD                                                                                                                                                                                                                                                                                                           
                  ,A.VSL_CD                                                                                                                                                                                                                                                                                                           
                  ,A.SKD_VOY_NO                                                                                                                                                                                                                                                                                                       
                  ,A.SKD_DIR_CD                                                                                                                                                                                                                                                                                                       
                  ,A.VSL_CD ||A.SKD_VOY_NO || A.SKD_DIR_CD   VVD                                                                                                                                                                                                                                                                      
                  ,A.VPS_PORT_CD                                                                                                                                                                                                                                                                                                      
                  ,A.TURN_PORT_IND_CD                                                                                                                                                                                                                                                                                                 
                  ,A.TURN_SKD_VOY_NO                                                                                                                                                                                                                                                                                                  
                  ,A.SKD_CNG_STS_CD                                                                                                                                                                                                                                                                                                   
                  ,B.TONG_STL_BAT_JB_SEQ                                                                                                                                                                                                                                                                                              
                  ,TO_CHAR(A.VPS_ETD_DT,'YYYYMM')  STL_YRMON
                  ,B.ACT_BSA_CAPA
                  ,B.VOY_DYS                                                                                                                                                                                                                                                                                                  
              FROM VSK_VSL_PORT_SKD A, (SELECT * FROM TOT_PORT_STL_AMT  X                                                                                                                                                                                                                                                                
                                        WHERE X.STL_YRMON BETWEEN @[stl_yrmon] AND @[e_stl_yrmon]                                                                                                                                                                                                                                                                      
                                         AND  X.TONG_STL_BAT_JB_SEQ = (SELECT MAX(TONG_STL_BAT_JB_SEQ) FROM TOT_PORT_STL_AMT WHERE STL_YRMON = X.STL_YRMON)) B                                                                                                                                                                               
              WHERE TO_CHAR(A.VPS_ETD_DT,'YYYYMM') BETWEEN @[stl_yrmon] AND @[e_stl_yrmon]                                                                                                                                                                                                                                                                        
                AND A.VSL_CD = B.VSL_CD(+)                                                                                                                                                                                                                                                                                               
                AND A.SKD_VOY_NO = B.SKD_VOY_NO(+)                                                                                                                                                                                                                                                                                       
                AND A.SKD_DIR_CD = B.SKD_DIR_CD(+)                                                                                                                                                                                                                                                                                       
                AND A.VPS_PORT_CD = B.PORT_CD(+)                                                                                                                                                                                                                                                                                         
                AND TO_CHAR(A.VPS_ETD_DT,'YYYYMM') = B.STL_YRMON(+) 
              ) C
       WHERE C.SLAN_CD IN ( SELECT VSL_SLAN_CD FROM MDM_VSL_SVC_LANE WHERE VSL_SVC_TP_CD  IN ('J','S','I') AND DELT_FLG = 'N') 
         AND C.TONG_STL_BAT_JB_SEQ IS NULL OR (C.ACT_BSA_CAPA = 0 AND C.VOY_DYS = 0)
      )T1,
      ( SELECT G.VSL_CD
             , G.SKD_VOY_NO
             , G.SKD_DIR_CD
             , G.SLAN_CD
             , G.TRD_CD
        FROM ( SELECT F.VSL_CD
                   , F.SKD_VOY_NO
                   , F.SKD_DIR_CD
                   , E.SLAN_CD
                   , F.TRD_CD
                   , F.FNL_HJS_BSA_CAPA
                   , ROW_NUMBER() OVER(PARTITION BY F.VSL_CD, F.SKD_VOY_NO, F.SKD_DIR_CD, E.SLAN_CD ORDER BY F.VSL_CD, F.SKD_VOY_NO, F.SKD_DIR_CD, E.SLAN_CD ASC, F.FNL_HJS_BSA_CAPA DESC, F.TRD_CD ASC) NUM_BY_LANE   
                FROM MAS_MON_VVD E, BSA_VVD_MST F
                WHERE E.DELT_FLG = 'N'
                  AND E.VSL_CD = F.VSL_CD
                  AND E.SKD_VOY_NO = F.SKD_VOY_NO
                  AND E.DIR_CD = F.SKD_DIR_CD 
                  AND E.TRD_CD = F.TRD_CD
                  AND E.RLANE_CD = F.RLANE_CD
              )G
         WHERE  G.NUM_BY_LANE = 1  
       ) T2      
   WHERE 1=1
 #if (${slan_cd} != 'ALL') 
     AND T1.SLAN_CD = @[slan_cd] 
 #end
     AND T1.VSL_CD =   T2.VSL_CD (+)
     AND T1.SKD_VOY_NO = T2.SKD_VOY_NO (+)
     AND T1.SKD_DIR_CD = T2.SKD_DIR_CD (+)
     AND T1.SLAN_CD = T2.SLAN_CD (+)
 #if (${s_exp_port} == 'A') 
     AND T1.VPS_PORT_CD NOT IN ('EGSUZ','PAPAC')  
 #elseif (${s_exp_port} == 'E')
     AND T1.VPS_PORT_CD <> 'EGSUZ'
 #elseif (${s_exp_port} == 'P')
     AND T1.VPS_PORT_CD <> 'PAPAC'   
 #end 

 #if (${s_exp_turn} == 'A') 
     AND T1.TURN_PORT_IND_CD NOT IN ('V','D','F')  
 #elseif (${s_exp_turn} == 'V')
     AND T1.TURN_PORT_IND_CD <> 'V'
 #elseif (${s_exp_turn} == 'D')
     AND T1.TURN_PORT_IND_CD<> 'D'  
 #elseif (${s_exp_turn} == 'F')
     AND T1.TURN_PORT_IND_CD <> 'F'  
 #end 
 #if (${trd_cd} != 'ALL') 
      AND T2.TRD_CD = @[trd_cd]
 #end
 ORDER BY T1.VVD			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="e_stl_yrmon" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
