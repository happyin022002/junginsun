<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchRepCustCdByArOfcCdRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
       REP_CUST_CNT_CD
     , REP_CUST_SEQ
  FROM MDM_ORGANIZATION 
 WHERE AR_OFC_CD = @[ar_ofc_cd] 
   AND DELT_FLG <> 'Y'			]]></sql>
			<params>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
