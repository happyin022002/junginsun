<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOModifyHPEDIHeaderUSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
UPDATE INV_HP_EDI_HDR
SET EDI_SND_FLG = 'Y',
  EDI_SND_DT = SYSDATE,	
  UPD_USR_ID = @[upd_usr_id],
  UPD_DT = SYSDATE
WHERE HP_INV_TP_CD = @[hp_inv_tp_cd]
  AND HP_INV_NO = @[hp_inv_no]
  AND INV_SEQ = @[inv_seq]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="hp_inv_tp_cd" type="12" value="" out="N"/>
				<param name="hp_inv_no" type="12" value="" out="N"/>
				<param name="inv_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
