<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDAOsearchAROfficeCodeByOfcCdRSQL">
			<desc><![CDATA[searchAROfficeCodeByOfcCd]]></desc>
			<sql><![CDATA[
SELECT AR_OFC_CD
  FROM MDM_ORGANIZATION
 WHERE 1=1
 AND   OFC_CD = @[ofc_cd]
 AND  NVL(DELT_FLG, 'N') <> 'Y'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
