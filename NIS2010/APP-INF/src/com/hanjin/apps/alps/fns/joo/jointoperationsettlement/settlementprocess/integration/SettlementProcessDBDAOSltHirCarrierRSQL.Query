<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOSltHirCarrierRSQL">
			<desc><![CDATA[Carrier 코드 조회]]></desc>
			<sql><![CDATA[
SELECT
       A.CRR_CD AS JO_CRR_CD
FROM   JOO_SLT_LIST		 A,
       JOO_CARRIER       B
WHERE  A.CRR_CD  = B.JO_CRR_CD
AND    A.RLANE_CD   = B.RLANE_CD
#if (${rev_yrmon_fr} != '')
AND    A.REV_YRMON >= REPLACE(@[rev_yrmon_fr],'-','')
#end
#if (${rev_yrmon_to} != '')
AND    A.REV_YRMON <= REPLACE(@[rev_yrmon_to],'-','')
#end
#if(${re_divr_cd} != '')
AND    A.ACCT_CD   LIKE DECODE(@[re_divr_cd],'R','4','5')||'%'
#end
#if(${trd_cd} != '')
AND    B.TRD_CD   = @[trd_cd]
#end
#if(${rlane_cd} != '')
AND    A.RLANE_CD   = @[rlane_cd]
#end
GROUP  BY A.CRR_CD
ORDER  BY A.CRR_CD			]]></sql>
			<params>
				<param name="rev_yrmon_fr" type="12" value="" out="N"/>
				<param name="rev_yrmon_to" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
