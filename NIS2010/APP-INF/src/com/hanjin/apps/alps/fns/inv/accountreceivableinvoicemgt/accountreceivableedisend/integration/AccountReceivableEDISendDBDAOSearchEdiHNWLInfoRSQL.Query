<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchEdiHNWLInfoRSQL">
			<desc><![CDATA[Get Honey Well information.]]></desc>
			<sql><![CDATA[
SELECT BL_SRC_NO,
  SND_SEQ,
  FLT_FILE_REF_NO,
  EDI_SND_DT,
  FRT_USD_RT_AMT,
  OTR_USD_CONV_AMT,
  TTL_USD_AMT,
  VSL_CD,
  SKD_VOY_NO,
  SKD_DIR_CD,
  SAIL_ARR_DT,
  CUST_REF_NO_CTNT,
  POR_CD,
  POL_CD,
  POD_CD,
  DEL_CD
FROM INV_EDI_HNWL
WHERE (BL_SRC_NO, SND_SEQ) IN (SELECT BL_SRC_NO, MAX(SND_SEQ)
                               FROM INV_EDI_HNWL
                               WHERE BL_SRC_NO = @[bl_src_no]
                               GROUP BY BL_SRC_NO)			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
