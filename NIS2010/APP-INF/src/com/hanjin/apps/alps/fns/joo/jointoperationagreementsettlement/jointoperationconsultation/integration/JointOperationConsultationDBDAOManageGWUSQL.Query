<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOManageGWUSQL">
			<desc><![CDATA[GW문서를 저장한다.(수정)]]></desc>
			<sql><![CDATA[
UPDATE JOO_CSR A
SET     UPD_USR_ID  	= @[upd_usr_id]
	   ,UPD_DT 			= SYSDATE
	   ,AGMT_DOC_NO 	= @[agmt_doc_no]
       ,AGMT_DOC_DESC	= @[agmt_doc_desc]
WHERE A.SLP_TP_CD||A.SLP_FUNC_CD||A.SLP_OFC_CD||TO_CHAR(TO_DATE(A.SLP_ISS_DT,'YYYYMMDD'),'RRMMDD')||A.SLP_SER_NO = @[csr_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="agmt_doc_no" type="12" value="" out="N"/>
				<param name="agmt_doc_desc" type="12" value="" out="N"/>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
