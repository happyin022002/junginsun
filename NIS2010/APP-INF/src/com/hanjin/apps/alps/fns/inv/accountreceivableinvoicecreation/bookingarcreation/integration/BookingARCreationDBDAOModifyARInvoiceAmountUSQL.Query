<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyARInvoiceAmountUSQL">
			<desc><![CDATA[INV_AR_AMT]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_AMT SET 
	ERP_IF_GL_DT = NVL(REPLACE(@[erp_if_gl_dt],'-',''), ERP_IF_GL_DT)
,   ERP_IF_OFC_CD = NVL(@[erp_if_ofc_cd], ERP_IF_OFC_CD)
,	AR_INV_SRC_CD = NVL(@[ar_inv_src_cd], AR_INV_SRC_CD)
,	INV_COA_CO_CD = NVL(@[inv_coa_co_cd], INV_COA_CO_CD)
,	INV_COA_RGN_CD = NVL(@[inv_coa_rgn_cd], INV_COA_RGN_CD)
,	INV_COA_CTR_CD = NVL(@[inv_coa_ctr_cd], INV_COA_CTR_CD)
,	INV_COA_ACCT_CD = NVL(@[inv_coa_acct_cd], INV_COA_ACCT_CD)
,	INV_COA_INTER_CO_CD = NVL(@[inv_coa_inter_co_cd], INV_COA_INTER_CO_CD)
,	INV_COA_VSL_CD = NVL(@[inv_coa_vsl_cd], INV_COA_VSL_CD)
,	INV_COA_VOY_NO = NVL(@[inv_coa_voy_no], INV_COA_VOY_NO)
,	INV_COA_SKD_DIR_CD = NVL(@[inv_coa_skd_dir_cd], INV_COA_SKD_DIR_CD)
,	INV_COA_REV_DIR_CD = NVL(@[inv_coa_rev_dir_cd], INV_COA_REV_DIR_CD)
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	AR_IF_NO = @[ar_if_no]
AND AR_IF_SER_NO = @[ar_if_ser_no]			]]></sql>
			<params>
				<param name="erp_if_gl_dt" type="12" value="" out="N"/>
				<param name="erp_if_ofc_cd" type="12" value="" out="N"/>
				<param name="ar_inv_src_cd" type="12" value="" out="N"/>
				<param name="inv_coa_co_cd" type="12" value="" out="N"/>
				<param name="inv_coa_rgn_cd" type="12" value="" out="N"/>
				<param name="inv_coa_ctr_cd" type="12" value="" out="N"/>
				<param name="inv_coa_acct_cd" type="12" value="" out="N"/>
				<param name="inv_coa_inter_co_cd" type="12" value="" out="N"/>
				<param name="inv_coa_vsl_cd" type="12" value="" out="N"/>
				<param name="inv_coa_voy_no" type="12" value="" out="N"/>
				<param name="inv_coa_skd_dir_cd" type="12" value="" out="N"/>
				<param name="inv_coa_rev_dir_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="ar_if_ser_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
