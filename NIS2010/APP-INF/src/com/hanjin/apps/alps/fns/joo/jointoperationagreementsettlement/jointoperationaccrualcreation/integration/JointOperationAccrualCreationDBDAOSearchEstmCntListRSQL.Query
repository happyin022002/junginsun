<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationAccrualCreationDBDAOSearchEstmCntListRSQL">
			<desc><![CDATA[I/F Send 대상건을 조회]]></desc>
			<sql><![CDATA[
SELECT 
 EXE_YRMON
,REV_YRMON
,ACCT_CD
,COUNT(EXE_YRMON) AS ESTM_CNT
FROM JOO_ESTM_ACT_RSLT AA 
WHERE 1=1
AND AA.EXE_YRMON = REPLACE(@[exe_yrmon],'-','')
GROUP BY EXE_YRMON, REV_YRMON, ACCT_CD			]]></sql>
			<params>
				<param name="exe_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
