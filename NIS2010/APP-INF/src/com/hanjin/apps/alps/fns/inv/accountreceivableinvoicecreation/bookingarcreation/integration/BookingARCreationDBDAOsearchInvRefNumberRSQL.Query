<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOsearchInvRefNumberRSQL">
			<desc><![CDATA[BookingARCreationDBDAOsearchInvRefNumberRSQL]]></desc>
			<sql><![CDATA[
SELECT NVL(SUBSTR(CUST_REF_NO_CTNT,1,50), '') CUST_REF_NO_CTNT
  FROM BKG_REFERENCE
 WHERE BKG_NO = @[bkg_no]
   AND BKG_REF_TP_CD = 'FINV'			]]></sql>
			<params>
				<param name="bkg_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
