<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOInvCprtCdConVODSQL">
			<desc><![CDATA[d]]></desc>
			<sql><![CDATA[
DELETE  FROM INV_CPRT_CD_CONV
WHERE	SC_NO = @[sc_no]
AND		RFA_NO = @[rfa_no]
AND		CPRT_CONV_TP_CD = @[cprt_conv_tp_cd]
AND		HJS_CD_CTNT	= @[hjs_cd_ctnt]			]]></sql>
			<params>
				<param name="sc_no" type="12" value="" out="N"/>
				<param name="rfa_no" type="12" value="" out="N"/>
				<param name="cprt_conv_tp_cd" type="12" value="" out="N"/>
				<param name="hjs_cd_ctnt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
