<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAORcUpVesselVORSQL">
			<desc><![CDATA[최근 업데이트 날짜 가져오기]]></desc>
			<sql><![CDATA[
SELECT	
    TO_CHAR(MAX(A.UPD_DT),'YYYY-MM-DD HH24:MI') UPD_DT

      FROM TOT_VESSEL A
      WHERE	A.STL_YR = @[stl_yr]
      AND	A.TONG_FLET_TP_CD =  @[tong_flet_tp_cd]			]]></sql>
			<params>
				<param name="stl_yr" type="12" value="2008" out="N"/>
				<param name="tong_flet_tp_cd" type="12" value="O" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
