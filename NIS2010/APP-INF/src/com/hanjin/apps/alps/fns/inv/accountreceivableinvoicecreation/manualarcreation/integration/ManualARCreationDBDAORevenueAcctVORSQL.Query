<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ManualARCreationDBDAORevenueAcctVORSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT ACCT_CD,
  ACCT_ENG_NM,
  nvl(REV_TP_SRC_CD, 'XXX') as REV_CD,
  nvl(CHG_CD, 'N') as CHG_CD,
  REP_CHG_CD,
  ACCT_KRN_NM
FROM INV_REV_ACCT_CD
#if (${rhq_cd} == 'HAMRU' || ${rhq_cd} == 'NYCRA')
WHERE ACCT_CD in ('411531',
      '411541',
      '411591',
      '411911',
      '411915',
      '212111',
	  '411917')
#else
WHERE (ACCT_CD in ('411531',
       '411541',
       '411591',
       '411911',
       '411915',
       '212111')
      OR (ACCT_CD ='411991' AND REV_TP_SRC_CD='OTH' )
	  #if (${rhq_cd} == 'SINRS')
        OR ACCT_CD = '411917'
      #end
	  )
#end 
  AND INV_SRC_CD = 'OTH'
UNION ALL
SELECT ACCT_CD,
  ACCT_ENG_NM,
  'XXX' as REV_CD,
  DECODE(ACCT_CD, '212111', 'TVA', 'XXX') as CHG_CD,
  DECODE(ACCT_CD, '212111', 'CST', 'XXX') as REP_CHG_CD,
  ACCT_KRN_NM
FROM mdm_account
WHERE ACCT_CD in ('212111',
      '957112',
      '954111',
      '422011',
      '710111')   
ORDER BY ACCT_CD			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
