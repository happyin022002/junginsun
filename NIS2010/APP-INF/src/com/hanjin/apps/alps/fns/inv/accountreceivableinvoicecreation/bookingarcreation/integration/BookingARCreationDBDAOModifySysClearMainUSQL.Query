<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifySysClearMainUSQL">
			<desc><![CDATA[INV_AR_MN 테이블에 Update
inv_iss_flg,inv_clr_flg 'Y'로 업데이트]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
   SET INV_ISS_FLG = 'Y',
       INV_CLR_FLG = 'Y',
	   ISS_DT = TO_CHAR( SYSDATE, 'YYYYMMDD' ),
       UPD_USR_ID	= @[upd_usr_id],
	   UPD_DT       = SYSDATE
 WHERE AR_IF_NO = @[ar_if_no]
   AND INV_ISS_FLG = 'N'
   AND INV_CLR_FLG = 'N'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
