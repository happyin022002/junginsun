<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAONrtBsaChgBsaModfiyVOUSQL">
			<desc><![CDATA[2012.01.02 이준범 NRT&BSA Change BSA 수정]]></desc>
			<sql><![CDATA[
UPDATE TOT_BSA
   SET FNL_HJS_BSA_CAPA = @[bsa_aft]
      ,MODI_FLG         = 'Y'
      ,UPD_USR_ID       = @[upd_usr_id]
      ,UPD_DT           = SYSDATE
 WHERE STL_YRMON  = @[stl_yrmon]
   AND SLAN_CD    = @[slan_cd]
   AND VSL_CD     = @[vsl_cd]
   AND SKD_VOY_NO = SUBSTR(@[vvd], 5, 4)
   AND SKD_DIR_CD = SUBSTR(@[vvd], 9, 1)			]]></sql>
			<params>
				<param name="bsa_aft" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="slan_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vvd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
