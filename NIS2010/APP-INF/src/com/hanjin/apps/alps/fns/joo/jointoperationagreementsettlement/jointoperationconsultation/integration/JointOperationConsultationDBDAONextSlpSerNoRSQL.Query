<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAONextSlpSerNoRSQL">
			<desc><![CDATA[Slip serial 번호를 채번한다.]]></desc>
			<sql><![CDATA[
SELECT
       LPAD(TO_NUMBER(A.SLP_SER_NO)+1,5,'0') AS SLP_SER_NO
FROM   JOO_SLP_SEQ A
WHERE  A.SLP_TP_CD   = @[slp_tp_cd]
AND    A.SLP_FUNC_CD = @[slp_func_cd]
AND    A.SLP_OFC_CD  = @[slp_ofc_cd]
AND    A.SLP_ISS_DT  = REPLACE(@[slp_iss_dt],'-','')			]]></sql>
			<params>
				<param name="slp_tp_cd" type="12" value="06" out="N"/>
				<param name="slp_func_cd" type="12" value="S" out="N"/>
				<param name="slp_ofc_cd" type="12" value="SELFAR" out="N"/>
				<param name="slp_iss_dt" type="12" value="20090901" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
