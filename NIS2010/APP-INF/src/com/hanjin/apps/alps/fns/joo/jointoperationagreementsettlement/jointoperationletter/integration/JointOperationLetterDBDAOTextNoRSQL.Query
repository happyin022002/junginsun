<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOTextNoRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT 
		A.JO_LTR_SEQ                         ,
		A.JO_LTR_TP_CD                       ,
		A.TRD_CD                             ,
		A.OFC_CD                             ,
		A.LTR_ISS_DT                         ,
		A.LTR_RCVR_CO_NM                     ,
		A.LTR_RCVR_CNTC_PSON_NM              ,
		A.LTR_CC_RCVR_CNTC_PSON_NM           ,
		A.LTR_SNDR_CO_NM                     ,
		A.SNDR_NM                            ,
		A.LTR_TIT_CTNT                       ,
		A.TTL_STL_AMT                        ,
		A.JO_SND_MZD_CD                      ,
		A.JO_LTR_NO                          ,
		A.OFC_ADDR                           ,
		A.N1ST_STMT_CTNT                     ,
		A.N2ND_STMT_CTNT                     ,
		A.N3RD_STMT_CTNT                     ,
		A.SIG_STMT_CTNT                      ,
(SELECT S1.ACCT_YRMON    FROM JOO_LTR_STL S1 WHERE S1.JO_LTR_SEQ= A.JO_LTR_SEQ AND ROWNUM=1)ACCT_YRMON,
(SELECT S1.JO_CRR_CD     FROM JOO_LTR_STL S1 WHERE S1.JO_LTR_SEQ= A.JO_LTR_SEQ AND ROWNUM=1)JO_CRR_CD, 
		''STL_CMB_SEQ                        ,
		A.SNDR_EML                           ,
		A.RCVR_EML                           ,
		A.CC_RCVR_EML                        ,
		A.EML_PROC_STS_CD                    ,
		A.EML_SND_NO                         ,
		A.EML_DT                             ,
		A.JO_CNTC_FAX_NO                     ,
		A.RCVR_INFO_CTNT                     ,
		A.FAX_PROC_STS_CD                    ,
		A.FAX_SND_NO                         ,
		A.FAX_DT                             ,
		A.DELT_FLG                           ,
		A.CRE_DT                             ,
		A.CRE_USR_ID                         ,
		A.UPD_DT                             ,
		A.UPD_USR_ID                         ,
		A.JOINT_OPERATION_LETTER_TEMPLAT     
  FROM  JOO_LETTER A
WHERE   A.JO_LTR_SEQ = @[jo_ltr_seq]			]]></sql>
			<params>
				<param name="jo_ltr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
