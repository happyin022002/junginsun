<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAOTaxableAmountCfmCheckVORSQL">
			<desc><![CDATA[tot_stl_cfm 테이블에 confirm 여부를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT DECODE(SUM(DECODE(A.TONG_ITM_CD,'NRT', COUNT(A.TONG_ITM_CD))),NULL,'N','Y') NRT_YN
      ,DECODE(SUM(DECODE(A.TONG_ITM_CD,'USE', COUNT(A.TONG_ITM_CD))),NULL,'N','Y') USE_YN
      ,DECODE(SUM(DECODE(A.TONG_ITM_CD,'DYS', COUNT(A.TONG_ITM_CD))),NULL,'N','Y') DAY_YN
FROM TOT_STL_CFM A
WHERE A.STL_YRMON = @[stl_yrmon]
  AND A.TRD_CD = @[trd_cd]
  AND A.TONG_STL_BAT_JB_SEQ = (SELECT MAX(TONG_STL_BAT_JB_SEQ) FROM TOT_PORT_STL_AMT WHERE STL_YRMON = @[stl_yrmon] AND TRD_CD = @[trd_cd]) 
GROUP BY A.TONG_ITM_CD			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
