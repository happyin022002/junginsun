<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOSlotHireTgtUSQL">
			<desc><![CDATA[Slot Hire Target 수정]]></desc>
			<sql><![CDATA[
UPDATE JOO_SLT_TGT J
SET   			
 J.JO_FSH_FLG 					 = @[jo_fsh_flg]
,J.FNL_BSA_QTY 					 = @[fnl_bsa_qty] 			
,J.FNL_BSA_SLT_PRC 				 = @[fnl_bsa_slt_prc] 		
,J.ESTM_STL_AMT 				 = @[estm_stl_amt] 			
,J.ACT_STL_AMT 					 = @[act_stl_amt] 			
,J.FNL_STL_AMT 					 = @[fnl_stl_amt] 			
,J.ADJ_RSLT_CD 					 = @[adj_rslt_cd] 			
,J.ADJ_RMK 						 = @[adj_rmk] 				
,J.JO_STL_STS_CD 				 = @[jo_stl_sts_cd]			
,J.UPD_DT              = SYSDATE 	
,J.UPD_USR_ID          = @[upd_usr_id]
WHERE 1=1
AND J.REV_YRMON = @[rev_yrmon]
AND J.REV_YRMON_SEQ = @[rev_yrmon_seq]			]]></sql>
			<params>
				<param name="jo_fsh_flg" type="12" value="" out="N"/>
				<param name="fnl_bsa_qty" type="12" value="" out="N"/>
				<param name="fnl_bsa_slt_prc" type="12" value="" out="N"/>
				<param name="estm_stl_amt" type="12" value="" out="N"/>
				<param name="act_stl_amt" type="12" value="" out="N"/>
				<param name="fnl_stl_amt" type="12" value="" out="N"/>
				<param name="adj_rslt_cd" type="12" value="" out="N"/>
				<param name="adj_rmk" type="12" value="" out="N"/>
				<param name="jo_stl_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
