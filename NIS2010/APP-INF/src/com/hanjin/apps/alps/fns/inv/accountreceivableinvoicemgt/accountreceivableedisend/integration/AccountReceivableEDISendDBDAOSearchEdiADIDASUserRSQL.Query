<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchEdiADIDASUserRSQL">
			<desc><![CDATA[SearchEdiADIDASUser]]></desc>
			<sql><![CDATA[
SELECT U.USR_NM PT_CTA_CONTACT,
  U.USR_EML PT_COM_NBR
FROM INV_AR_ISS I,
  COM_USER U
WHERE I.CRE_USR_ID = U.USR_ID
  AND I.INV_NO = @[inv_no]
  AND I.INV_SEQ = 1			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
