<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationBackEndDBDAOSearchACMBkgNoRSQL">
			<desc><![CDATA[Search ACM Bkg No]]></desc>
			<sql><![CDATA[
#if (${xch_rt_tp_cd} == 'V')
    SELECT DISTINCT BKG_NO
    FROM ACM_AGN_COMM
    WHERE SAIL_ARR_DT >= TO_CHAR(ADD_MONTHS(SYSDATE, -3), 'YYYYMMDD')  
    AND AC_VSL_CD = @[vsl_cd]
    AND AC_SKD_VOY_NO = @[skd_voy_no]
    AND AC_SKD_DIR_CD = @[skd_dir_cd]
    AND AC_OCCR_INFO_CD = @[port_cd]
    AND IO_BND_CD = @[io_bnd_cd]  
    AND CURR_CD = @[locl_curr_cd]
    AND PAY_XCH_RT = 0
    AND XCH_RT_APLY_LVL = '1'
#elseif (${xch_rt_tp_cd} == 'D')
    SELECT DISTINCT BKG_NO
    FROM ACM_AGN_COMM
    WHERE SAIL_ARR_DT BETWEEN REPLACE(@[fm_dt], '-', '') AND REPLACE(@[to_dt], '-', '')
    AND IO_BND_CD = DECODE(@[io_bnd_cd], '', IO_BND_CD, @[io_bnd_cd])  
    AND CURR_CD = @[locl_curr_cd]
    AND PAY_XCH_RT = 0
    AND XCH_RT_APLY_LVL = '3'
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="fm_dt" type="12" value="" out="N"/>
				<param name="to_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
