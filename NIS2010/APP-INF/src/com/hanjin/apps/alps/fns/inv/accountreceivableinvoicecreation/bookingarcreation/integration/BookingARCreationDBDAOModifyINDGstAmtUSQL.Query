<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyINDGstAmtUSQL">
			<desc><![CDATA[Modify INDIA Gst Amt]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_CHG   
SET IDA_CGST_AMT = ROUND(ROUND(CHG_AMT * INV_XCH_RT, @[dp_prcs_knt]) * IDA_CGST_RTO / 100, @[dp_prcs_knt])
  , IDA_SGST_AMT = ROUND(ROUND(CHG_AMT * INV_XCH_RT, @[dp_prcs_knt]) * IDA_SGST_RTO / 100, @[dp_prcs_knt])
  , IDA_IGST_AMT = ROUND(ROUND(CHG_AMT * INV_XCH_RT, @[dp_prcs_knt]) * IDA_IGST_RTO / 100, @[dp_prcs_knt])
  , IDA_UGST_AMT = ROUND(ROUND(CHG_AMT * INV_XCH_RT, @[dp_prcs_knt]) * IDA_UGST_RTO / 100, @[dp_prcs_knt])
  , UPD_USR_ID = @[user_id]
  , UPD_DT = SYSDATE
WHERE AR_IF_NO = @[ar_if_no]			]]></sql>
			<params>
				<param name="dp_prcs_knt" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
