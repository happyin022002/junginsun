<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOTmptAddressRSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT  (SELECT MA.BZET_ADDR  FROM MDM_CUST_ADDR MA WHERE MA.CUST_CNT_CD  = M.REP_CUST_CNT_CD 
                                          AND MA.CUST_SEQ     = M.REP_CUST_SEQ  AND MA.PRMRY_CHK_FLG = 'Y' AND ROWNUM=1)    OFC_ADDR
,       (SELECT   S.BANK_STMT_CTNT
/**/          FROM   JOO_LTR_TMPLT S
/**/     WHERE   S.JO_LTR_TMPLT_SEQ  =
/**/                                 (SELECT  /*+ index_desc( A  XPKJOO_LTR_TMPLT) */
/**/                                          JO_LTR_TMPLT_SEQ
/**/                                    FROM  JOO_LTR_TMPLT A
/**/                                  WHERE  OFC_CD = @[ofc_cd]
/**/                                     AND  JO_LTR_TP_CD = 'B'
/**/                                    AND  ROWNUM=1))BANK_STMT_CTNT,
/**/   (SELECT   S.SIG_STMT_CTNT
/**/      FROM   JOO_LTR_TMPLT S
/**/     WHERE   S.JO_LTR_TMPLT_SEQ  =
/**/                                (SELECT  /*+ index_desc( A  XPKJOO_LTR_TMPLT) */
/**/                                         JO_LTR_TMPLT_SEQ
/**/                                   FROM  JOO_LTR_TMPLT A
/**/                                  WHERE  OFC_CD = @[ofc_cd]
/**/                                    AND  JO_LTR_TP_CD = 'B'
/**/                                    AND  ROWNUM=1))SIG_STMT_CTNT
FROM MDM_ORGANIZATION M
WHERE OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
