<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOsearchRhqByOfcCdRSQL">
			<desc><![CDATA[RHQ 조회용 쿼리]]></desc>
			<sql><![CDATA[
SELECT AR_HD_QTR_OFC_CD 
  FROM MDM_ORGANIZATION 
 WHERE 1=1
#if (${ofc_cd} != '')
 AND   OFC_CD = @[ofc_cd]
#end
 AND   DELT_FLG <> 'Y' 
 GROUP BY AR_HD_QTR_OFC_CD 
 ORDER BY AR_HD_QTR_OFC_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
