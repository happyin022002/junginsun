<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOStlSeqRSQL">
			<desc><![CDATA[Settlement Seq 조회]]></desc>
			<sql><![CDATA[
SELECT
	NVL((SELECT /*+INDEX_DESC(A XPKJOO_SETTLEMENT)*/ A.STL_SEQ + 1 
		 FROM JOO_SETTLEMENT A 
		 WHERE  A.ACCT_YRMON = REPLACE(@[acct_yrmon],'-','') 
		 AND A.STL_VVD_SEQ = @[stl_vvd_seq] 
		 AND ROWNUM = 1),1) AS STL_SEQ
FROM DUAL			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
