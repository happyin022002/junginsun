<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOmodifyInvEdiGlovisHdrUSQL">
			<desc><![CDATA[Invoice Glovis Header table edi 전송 현황 Update]]></desc>
			<sql><![CDATA[
update inv_edi_glovis_hdr
   set EDI_SND_FLG = 'Y',
       EDI_SND_DT = sysdate,
       EDI_SND_RMK =@[inv_rmk]
where  AR_IF_NO = @[ar_if_no]
  and  IF_SEQ =@[if_seq] 			]]></sql>
			<params>
				<param name="inv_rmk" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
				<param name="if_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
