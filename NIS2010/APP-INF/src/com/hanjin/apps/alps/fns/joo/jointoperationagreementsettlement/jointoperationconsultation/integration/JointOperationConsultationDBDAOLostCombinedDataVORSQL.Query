<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOLostCombinedDataVORSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
SELECT DISTINCT
       A.ACCT_YRMON,
       NVL(D.LST_LGIN_OFC_CD,D.OFC_CD) AS OFC_CD,
       A.TRD_CD,
       A.RLANE_CD,
       A.JO_CRR_CD,
       NVL(A.CMB_CFM_FLG,'N') AS CMB_CFM_FLG,
       NULL AS STL_CMB_SEQ,
       A.STL_ADJ_FLG
FROM   JOO_SETTLEMENT   A,
       COM_USER         D
WHERE  A.CRE_USR_ID  = D.USR_ID(+)
AND    A.ACCT_YRMON >= REPLACE(@[fr_dt],'-','')
AND    A.ACCT_YRMON <= REPLACE(@[to_dt],'-','')
#if (${slp_ofc_cd} != '')
AND    NVL(D.LST_LGIN_OFC_CD,D.OFC_CD) = @[slp_ofc_cd]
#end
AND    NVL(A.CMB_CFM_FLG,'N') = 'N'
UNION  ALL
SELECT DISTINCT
       A.ACCT_YRMON,
       NVL(D.LST_LGIN_OFC_CD,D.OFC_CD) AS OFC_CD,
       A.TRD_CD,
       A.RLANE_CD,
       A.JO_CRR_CD,
       A.CMB_CFM_FLG,
       B.STL_CMB_SEQ,
       A.STL_ADJ_FLG
FROM   JOO_SETTLEMENT   A,
       JOO_STL_CMB_DTL  B,
       JOO_STL_CMB      C,
       COM_USER         D
WHERE  A.ACCT_YRMON  = B.ACCT_YRMON
AND    A.STL_VVD_SEQ = B.STL_VVD_SEQ
AND    A.STL_SEQ     = B.STL_SEQ
AND    B.ACCT_YRMON  = C.ACCT_YRMON
AND    B.JO_CRR_CD   = C.JO_CRR_CD
AND    B.STL_CMB_SEQ = C.STL_CMB_SEQ
AND    B.RE_DIVR_CD  = C.RE_DIVR_CD
AND    A.CRE_USR_ID  = D.USR_ID(+)
AND    A.ACCT_YRMON >= REPLACE(@[fr_dt],'-','')
AND    A.ACCT_YRMON <= REPLACE(@[to_dt],'-','')
#if (${slp_ofc_cd} != '')
AND    NVL(D.LST_LGIN_OFC_CD,D.OFC_CD) = @[slp_ofc_cd]
#end
AND    C.SLP_SER_NO IS NULL
ORDER  BY 1,2,3,4,5			]]></sql>
			<params>
				<param name="fr_dt" type="12" value="201001" out="N"/>
				<param name="to_dt" type="12" value="201001" out="N"/>
				<param name="slp_ofc_cd" type="12" value="SELFAR" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
