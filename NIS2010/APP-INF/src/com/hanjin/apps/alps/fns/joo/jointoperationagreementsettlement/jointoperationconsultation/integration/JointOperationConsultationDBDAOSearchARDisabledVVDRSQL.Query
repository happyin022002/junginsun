<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOSearchARDisabledVVDRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT  AR.RLANE_CD
,       AR.VSL_CD
,       AR.SKD_VOY_NO
,       AR.SKD_DIR_CD
,       AR.RLANE_DIR_CD
,       NVL2(MAX(SL.VSL_CD), 'Y','N') SLIP_YN 
,       AR.EAI_EVNT_DT
FROM    AR_MST_REV_VVD AR, JOO_SLIP SL
WHERE   AR.EAI_EVNT_DT BETWEEN TO_DATE( REPLACE(@[eai_evnt_dt], '-','')||'01', 'YYYYMMDD')  
                           AND LAST_DAY(TO_DATE( REPLACE(@[eai_evnt_dt], '-','')||'01', 'YYYYMMDD') ) 
AND    (INSTR( AR.REV_YRMON, '9999' ) >0
OR      LOWER( AR.REV_YRMON ) = 'null'
OR      AR.REV_YRMON  IS NULL)
AND     AR.SKD_DIR_CD  IN ('E','W','S','N')--벌크선을 제외하기 위함.

AND SL.VSL_CD(+)=AR.VSL_CD
AND SL.SKD_VOY_NO(+) = AR.SKD_VOY_NO
AND SL.SKD_DIR_CD(+)= AR.SKD_DIR_CD
AND SL.REV_DIR_CD(+) = AR.RLANE_DIR_CD
GROUP BY AR.RLANE_CD
,        AR.VSL_CD
,        AR.SKD_VOY_NO
,        AR.SKD_DIR_CD
,        AR.RLANE_DIR_CD 
,        AR.EAI_EVNT_DT
ORDER BY AR.RLANE_CD
,        AR.VSL_CD
,        AR.SKD_VOY_NO
,        AR.SKD_DIR_CD
,        AR.RLANE_DIR_CD			]]></sql>
			<params>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
