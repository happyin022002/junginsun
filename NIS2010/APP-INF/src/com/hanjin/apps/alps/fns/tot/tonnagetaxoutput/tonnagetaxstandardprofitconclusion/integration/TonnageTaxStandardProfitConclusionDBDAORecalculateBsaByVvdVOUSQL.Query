<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxStandardProfitConclusionDBDAORecalculateBsaByVvdVOUSQL">
			<desc><![CDATA[재계산된 데이터 업데이트(TOT_VVD_STL_AMT)]]></desc>
			<sql><![CDATA[
UPDATE TOT_VVD_STL_AMT SET 
	LDB_CAPA_QTY = @[ldb_capa_qty]  
,	USG_RT = @[usg_rt]
,	TONG_TAX_AMT = @[tong_tax_amt]
,	UPD_DT = SYSDATE
,	UPD_USR_ID = @[upd_usr_id]
WHERE STL_YRMON = @[stl_yrmon] 
  AND VSL_CD = @[vsl_cd] 
  AND TONG_STL_BAT_JB_SEQ = (SELECT MAX(TONG_STL_BAT_JB_SEQ) FROM TOT_PORT_STL_AMT WHERE STL_YRMON = @[stl_yrmon] AND VSL_CD = @[vsl_cd] )			]]></sql>
			<params>
				<param name="ldb_capa_qty" type="12" value="" out="N"/>
				<param name="usg_rt" type="12" value="" out="N"/>
				<param name="tong_tax_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="stl_yrmon" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
