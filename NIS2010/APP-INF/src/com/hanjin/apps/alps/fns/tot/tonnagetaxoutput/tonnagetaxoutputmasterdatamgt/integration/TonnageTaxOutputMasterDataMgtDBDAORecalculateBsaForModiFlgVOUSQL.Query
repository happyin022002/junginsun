<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="TonnageTaxOutputMasterDataMgtDBDAORecalculateBsaForModiFlgVOUSQL">
			<desc><![CDATA[tot_bsa 데이터 modi_flg를 'N'으로 변경]]></desc>
			<sql><![CDATA[
UPDATE TOT_BSA SET  
 MODI_FLG = 'N'
WHERE STL_YRMON = @[stl_yrmon]
 AND MODI_FLG= 'Y'
 AND TONG_STL_BAT_JB_SEQ = (SELECT MAX(TONG_STL_BAT_JB_SEQ) FROM TOT_BSA WHERE STL_YRMON = @[stl_yrmon])

			]]></sql>
			<params>
				<param name="stl_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
