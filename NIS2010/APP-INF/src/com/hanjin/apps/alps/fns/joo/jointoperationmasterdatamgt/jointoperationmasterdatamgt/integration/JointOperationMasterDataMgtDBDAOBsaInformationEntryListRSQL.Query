<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOBsaInformationEntryListRSQL">
			<desc><![CDATA[BsaInformationEntryList
1. 2013.01.28 이수진 [CHM-201322523] JO - BSA Entry 기능 수정관련 개발
    - 조건 중 Version 항목에 Latest를 선택한 경우 Ref No 중 Version Flag ="Y"이면서 
      Version 번호가 가장 큰 값만 조회되도록 쿼리 수정

2. 2013.02.06 이수진 [CHM-201322641] BSA Enmtry 기능 추가
   - Price Finish 항목 추가
   - Add Carrier  Checkbox 구분에 따라 Main 화면을 조회할지 Add Carrier 화면을 조회할지 구분
     : Uncheck시에는 Mian 화면 조회, Check시에는 Add Carrier 화면 조회]]></desc>
			<sql><![CDATA[
#if (${add_carrier} == 'Y')
SELECT C.VSL_CD||C.SKD_VOY_NO||C.SKD_DIR_CD||C.PORT_CD||C.PORT_SEQ||C.JO_CRR_CD VVD_PORT
     , C.JO_CRR_CD
     , C.JO_BSA_TEU_QTY
     , TO_CHAR(C.CRE_DT, 'YYYY-MM-DD')  CRE_DT               
     , C.UPD_USR_ID           
     , U.USR_NM 
     , C.JO_ADD_CRR_CD      
  FROM JOO_BSA_AGMT A, JOO_ADD_BSA_CRR C, COM_USER U
 WHERE 1 = 1

#if (${trd_cd}!= '')
       AND   A.TRD_CD        =  @[trd_cd]  
#end

#if (${rlane_cd}!= '')
       AND   A.RLANE_CD      =  @[rlane_cd]  
#end
#if (${jo_crr_cd}!= '')
       AND   A.JO_CRR_CD     =  @[jo_crr_cd]  
#end

#if (${vsl_cd}!= '')
       AND   A.VSL_CD        LIKE @[vsl_cd]||'%'  
#end

#if (${skd_dir_cd}!= '')
       AND   A.SKD_DIR_CD    =  @[skd_dir_cd]  
#end

#if (${fm_yr_wk}!= '')
       AND   A.YRWK BETWEEN @[fm_yr_wk] AND @[to_yr_wk]
#end

#if (${port_cd}!= '')
       AND   A.PORT_CD       =  @[port_cd]  
#end

#if (${jo_fsh_flg}!= '')
       AND   A.JO_FSH_FLG    =  @[jo_fsh_flg]  
#end

#if (${jo_prc_flg}!= '')
       AND   A.JO_PRC_FSH_FLG    =  @[jo_prc_flg] 
#end

#if (${delt_flg}!= '')
       AND   A.DELT_FLG      =  @[delt_flg] 
#end

#if (${re_divr_cd}!= '')
       AND   A.RE_DIVR_CD    =  @[re_divr_cd]  
#end

#if (${fm_etd_dt}!= '')
	   AND   A.REV_PORT_ETD_DT  BETWEEN  TO_DATE (REPLACE (@[fm_etd_dt], '-', ''), 'YYYYMMDD') AND TO_DATE (REPLACE (@[to_etd_dt], '-', ''), 'YYYYMMDD')  + 0.99999  
#end

#if (${ofc_cd}!= '')
       AND   A.OFC_CD        =  @[ofc_cd]  -- 11번
#end

#if (${cre_dt_fr}!= '')
	   AND   A.CRE_DT  BETWEEN  TO_DATE (REPLACE (@[cre_dt_fr], '-', ''), 'YYYYMMDD') AND TO_DATE (REPLACE (@[cre_dt_to], '-', ''), 'YYYYMMDD')  + 0.99999  -- 12번
#end

#if (${vvd_cd}!= '')
	   AND   A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD like @[vvd_cd]||'%'
#end

  AND A.VSL_CD = C.VSL_CD
  AND A.SKD_VOY_NO = C.SKD_VOY_NO
  AND A.SKD_DIR_CD = C.SKD_DIR_CD
  AND A.PORT_CD = C.PORT_CD
  AND A.PORT_SEQ = C.PORT_SEQ
  AND C.UPD_USR_ID    =  U.USR_ID
ORDER BY C.VSL_CD, C.SKD_VOY_NO, C.SKD_DIR_CD, C.PORT_CD, C.PORT_SEQ




#else



SELECT A.YRWK
     , A.OFC_CD
     , DECODE(A.RE_DIVR_CD,'R','Rev','Exp') RE_DIVR_CD
     , A.TRD_CD
     , A.RLANE_CD
     , A.VSL_CD
     , A.SKD_VOY_NO
     , A.SKD_DIR_CD
     , A.PORT_CD
     , A.PORT_SEQ
     , A.JO_CRR_CD
     , A.JO_BSA_TEU_QTY
     , A.JO_BSA_ADD_TEU_QTY
     , A.JO_ADD_BSA_CRR_FLG
     , A.JO_OVR_BSA_TEU_QTY
     , A.JO_TON_TEU_QTY
     , A.JO_OVR_TON_WGT
     , A.JO_20FT_SUB_TEU_QTY
     , A.JO_20FT_OVR_RTO
     , A.JO_40FT_SUB_TEU_QTY
     , A.JO_40FT_OVR_RTO
     , A.JO_45FT_SUB_TEU_QTY
     , A.JO_45FT_OVR_RTO
     , A.JO_45FT_UND_RTO
     , A.JO_RND_KND_FLG
     , A.JO_RND_RULE_LVL
     , A.JO_RF_OCN_TEU_QTY
     , A.JO_RF_INTER_TEU_QTY
     , A.JO_INTER_OVR_FLG
     , A.JO_RDR_PORT_CD
     , A.JO_FSH_FLG
     , A.JO_BSA_PRC
     , A.JO_OVR_OCN_PRC
     , A.JO_OVR_INTER_PRC
     , A.JO_OVR_MT_OCN_PRC
     , A.JO_OVR_MT_INTER_PRC
     , A.JO_SCTR_PRC_FLG
     , A.JO_RF_OCN_PRC
     , A.JO_RF_INTER_PRC
     , A.JO_PRC_FSH_FLG
     , A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD||A.PORT_CD||A.PORT_SEQ||A.JO_CRR_CD VVD_PORT
     , A.JO_BSA_ENTR_RDR_RMK
     , A.JO_BSA_ENTR_RMK
     , TO_CHAR(A.UPD_DT, 'YYYY-MM-DD')  UPD_DT
     , A.UPD_USR_ID 
     , U.USR_NM 
     , A.DELT_FLG  
     , TO_CHAR(TO_DATE(A.REV_PORT_ETD_DT,'YYYY-MM-DD'),'YYYY-MM-DD') REV_PORT_ETD_DT
  FROM JOO_BSA_AGMT A, COM_USER U
 WHERE 1=1
#if (${trd_cd}!= '')
       AND   A.TRD_CD        =  @[trd_cd]  
#end

#if (${rlane_cd}!= '')
       AND   A.RLANE_CD      =  @[rlane_cd]  
#end
#if (${jo_crr_cd}!= '')
       AND   A.JO_CRR_CD     =  @[jo_crr_cd] 
#end

#if (${vsl_cd}!= '')
       AND   A.VSL_CD        LIKE @[vsl_cd]||'%'  
#end

#if (${skd_dir_cd}!= '')
       AND   A.SKD_DIR_CD    =  @[skd_dir_cd]
#end

#if (${fm_yr_wk}!= '')
       AND   A.YRWK BETWEEN @[fm_yr_wk] AND @[to_yr_wk]
#end

#if (${port_cd}!= '')
       AND   A.PORT_CD       =  @[port_cd]  
#end

#if (${jo_fsh_flg}!= '')
       AND   A.JO_FSH_FLG    =  @[jo_fsh_flg]  
#end

#if (${jo_prc_flg}!= '')
       AND   A.JO_PRC_FSH_FLG    =  @[jo_prc_flg]  
#end

#if (${delt_flg}!= '')
       AND   A.DELT_FLG      =  @[delt_flg]  
#end

#if (${re_divr_cd}!= '')
       AND   A.RE_DIVR_CD    =  @[re_divr_cd] 
#end

#if (${fm_etd_dt}!= '')
	   AND   A.REV_PORT_ETD_DT  BETWEEN  REPLACE (@[fm_etd_dt], '-', '') AND REPLACE (@[to_etd_dt], '-', '')  
#end

#if (${ofc_cd}!= '')
       AND   A.OFC_CD        =  @[ofc_cd] 
#end

#if (${cre_dt_fr}!= '')
	   AND   A.CRE_DT  BETWEEN  TO_DATE (REPLACE (@[cre_dt_fr], '-', ''), 'YYYYMMDD') AND TO_DATE (REPLACE (@[cre_dt_to], '-', ''), 'YYYYMMDD')  + 0.99999  
#end

#if (${vvd_cd}!= '')
	   AND   A.VSL_CD||A.SKD_VOY_NO||A.SKD_DIR_CD like @[vvd_cd]||'%'
#end

AND   A.UPD_USR_ID    =  U.USR_ID

ORDER BY YRWK, OFC_CD, RE_DIVR_CD, TRD_CD, RLANE_CD, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, PORT_SEQ
#end			]]></sql>
			<params>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="fm_yr_wk" type="12" value="" out="N"/>
				<param name="to_yr_wk" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="jo_fsh_flg" type="12" value="" out="N"/>
				<param name="jo_prc_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="re_divr_cd" type="12" value="" out="N"/>
				<param name="fm_etd_dt" type="12" value="" out="N"/>
				<param name="to_etd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cre_dt_fr" type="12" value="" out="N"/>
				<param name="cre_dt_to" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
