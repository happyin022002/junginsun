<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOSearchKORIssuedChargeListRSQL">
			<desc><![CDATA[KOR Invoice main, KOR Invoice Charge 테이블에서 Select]]></desc>
			<sql><![CDATA[
SELECT A.INV_NO,
  A.INV_SEQ,
  B.BL_SRC_NO,
  B.CHG_SEQ,
  B.CHG_CD,
  B.CURR_CD,
  B.INV_XCH_RT,
  B.CHG_AMT,
  B.CHG_AMT ISS_AMT,
  B.RAT_AS_CNTR_QTY,
  B.TRF_RT_AMT,
  B.PER_TP_CD,
  C.AR_CURR_CD,
  D.DP_PRCS_KNT,
  B.CRE_USR_ID,
  B.CRE_DT,
  B.UPD_USR_ID,
  B.UPD_DT,
  A.AR_OFC_CD
FROM INV_AR_KR_ISS A,
  INV_AR_KR_ISS_CHG B,
  MDM_ORGANIZATION C,
  MDM_CURRENCY D
WHERE A.INV_NO = B.INV_NO
  AND A.INV_SEQ = B.INV_SEQ
  AND A.AR_OFC_CD = C.OFC_CD
  AND B.CURR_CD = D.CURR_CD
  AND A.INV_NO = @[inv_no]
  AND A.INV_SEQ = (
    SELECT MAX(INV_SEQ)
    FROM INV_AR_KR_ISS
    WHERE INV_NO = @[inv_no]
      AND AR_OFC_CD IN (
        SELECT DISTINCT AR_OFC_CD
        FROM MDM_ORGANIZATION
        --WHERE AR_HD_QTR_OFC_CD = 'SHAAS'
          WHERE (LOC_CD LIKE 'KR%' OR LOC_CD LIKE 'HQ%')
          AND DELT_FLG = 'N'
          AND AR_OFC_CD IS NOT NULL))			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
