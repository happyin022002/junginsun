<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOVendorInfoRSQL">
			<desc><![CDATA[Vendor 정보조회]]></desc>
			<sql><![CDATA[
select 
	   vndr_seq as code,
	   vndr_lgl_eng_nm as name,
       rgst_no as spl_rgst_no,
       vndr_seq,
       vndr_locl_lang_nm as co_nm,
       ceo_nm as ownr_nm,
       bzct_nm,
       bztp_nm,
       locl_lang_addr
from   mdm_vendor
where  delt_flg = 'N'
#if (${vndr_seq} != '')
and   vndr_seq = to_number(@[vndr_seq])
#end
order by 1			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
