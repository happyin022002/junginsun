<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOJoLoadingDSQL">
			<desc><![CDATA[Jo Loading 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM JOO_LODG_TGT J
WHERE 1=1
AND J.REV_YRMON					 = @[rev_yrmon]		      
AND J.REV_YRMON_SEQ           	 = @[rev_yrmon_seq]
AND J.RF_SCG_STL_TP_CD			 = 'I'			]]></sql>
			<params>
				<param name="rev_yrmon" type="12" value="" out="N"/>
				<param name="rev_yrmon_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
