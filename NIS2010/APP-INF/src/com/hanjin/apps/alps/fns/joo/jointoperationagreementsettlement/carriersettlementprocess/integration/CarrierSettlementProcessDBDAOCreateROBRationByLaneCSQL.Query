<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOCreateROBRationByLaneCSQL">
			<desc><![CDATA[ROB Container List Inquiry의 Sub-Allocation and Ratio을 저장]]></desc>
			<sql><![CDATA[
INSERT INTO JOO_ROB_RTO A 
(   SLAN_CD
  , SKD_DIR_CD
  , JO_20FT_N1ST_RTO
  , JO_20FT_SUB_TEU_QTY
  , JO_40FT_N1ST_RTO
  , JO_40FT_SUB_TEU_QTY
  , JO_45FT_N1ST_RTO
  , JO_45FT_N2ND_RTO
  , JO_45FT_SUB_TEU_QTY
  , JO_BSA_TEU_QTY
  , CGO_TON_WGT
  , JO_TON_TEU_QTY
  , JO_RND_RULE_LVL
  , CRE_DT
  , CRE_USR_ID
  , UPD_DT
  , UPD_USR_ID
)
VALUES
(   @[rlane_cd]
  , @[skd_dir_cd]
  , NVL(@[jo_20ft_n1st_rto], 0)
  , NVL(@[jo_20ft_sub_teu_qty], 0)
  , NVL(@[jo_40ft_n1st_rto], 0)
  , NVL(@[jo_40ft_sub_teu_qty], 0)
  , NVL(@[jo_45ft_n1st_rto], 0)
  , NVL(@[jo_45ft_n2nd_rto], 0)
  , NVL(@[jo_45ft_sub_teu_qty], 0)
  , NVL(@[jo_bsa_teu_qty], 0)
  , NVL(@[cgo_ton_wgt], 0)
  , NVL(@[jo_ton_teu_qty], 0)
  , @[jo_rnd_rule_lvl]
  , SYSDATE
  , @[cre_usr_id]
  , SYSDATE
  , @[usr_id]
)			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="jo_20ft_n1st_rto" type="12" value="" out="N"/>
				<param name="jo_20ft_sub_teu_qty" type="12" value="" out="N"/>
				<param name="jo_40ft_n1st_rto" type="12" value="" out="N"/>
				<param name="jo_40ft_sub_teu_qty" type="12" value="" out="N"/>
				<param name="jo_45ft_n1st_rto" type="12" value="" out="N"/>
				<param name="jo_45ft_n2nd_rto" type="12" value="" out="N"/>
				<param name="jo_45ft_sub_teu_qty" type="12" value="" out="N"/>
				<param name="jo_bsa_teu_qty" type="12" value="" out="N"/>
				<param name="cgo_ton_wgt" type="12" value="" out="N"/>
				<param name="jo_ton_teu_qty" type="12" value="" out="N"/>
				<param name="jo_rnd_rule_lvl" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
