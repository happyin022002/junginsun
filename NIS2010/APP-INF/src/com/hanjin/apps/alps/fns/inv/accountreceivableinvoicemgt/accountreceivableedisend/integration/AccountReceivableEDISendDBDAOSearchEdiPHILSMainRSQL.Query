<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountReceivableEDISendDBDAOSearchEdiPHILSMainRSQL">
			<desc><![CDATA[Search Edi Philips Main
2013.02.04 Actual Customer 조건을 INV customer 로 변경함.]]></desc>
			<sql><![CDATA[
SELECT A.BL_SRC_NO,
       A.BKG_NO,
       A.IO_BND_CD,
       CASE WHEN (SELECT COUNT(*) FROM INV_EDI_PHILS WHERE BL_SRC_NO = @[bl_src_no]) > 0 THEN 'Y'
       ELSE 'N'
       END IS_EXIST
FROM INV_AR_MN A,
     (SELECT BL_SRC_NO, MAX(AR_IF_NO) AR_IF_NO
      FROM   INV_AR_MN
      WHERE  AR_OFC_CD = @[ofc_cd]
      AND    ACT_CUST_CNT_CD = @[act_cust_cnt_cd]
      AND    ACT_CUST_SEQ = @[act_cust_seq]
      AND    INV_DELT_DIV_CD <> 'Y'
      AND    REV_TP_CD <> 'M'
      AND    BL_SRC_NO = @[bl_src_no]
      GROUP BY BL_SRC_NO
     ) B
WHERE A.AR_IF_NO = B.AR_IF_NO			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="act_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="act_cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
