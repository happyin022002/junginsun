<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ARInvoiceCustomerMgtDBDAOTemplateItemVORSQL">
			<desc><![CDATA[TemplateItemVO]]></desc>
			<sql><![CDATA[
SELECT  A.RPT_ITM_NM
		,B.RPT_TMPLT_NM
		,B.RPT_ITM_ID
        ,B.USR_DEF_NM        
        ,B.ITM_SEQ
		,B.RPT_AUTH_ID
        ,B.AR_OFC_CD
FROM    INV_CPRT_ITM A
        ,INV_CPRT_TMPLT B
WHERE   A.RPT_ITM_ID = B.RPT_ITM_ID
AND     B.RPT_TMPLT_NM = @[rpt_tmplt_nm]
AND     B.AR_OFC_CD    = @[ar_ofc_cd]
ORDER BY B.ITM_SEQ, B.RPT_ITM_ID			]]></sql>
			<params>
				<param name="rpt_tmplt_nm" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
