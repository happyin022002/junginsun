<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOSearchServerIDRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DECODE(CONTI_CD, 'F', DECODE(AR_HD_QTR_OFC_CD, 'SINRS', 'SWA', SYS_AREA_GRP_ID), 
                             DECODE(SYS_AREA_GRP_ID, 'ENT', 'KOR', 'KOR', DECODE(SUBSTR(LOC_CD, 1, 2), 'JP', 'JPN', 'KOR'), SYS_AREA_GRP_ID)
                            ) SVR_ID
FROM (  
       SELECT ML.CONTI_CD, MO.LOC_CD, MO.AR_OFC_CD, MO.AR_HD_QTR_OFC_CD, CS.SYS_AREA_GRP_ID
       FROM   MDM_ORGANIZATION MO,
              MDM_LOCATION ML,
              COM_SYS_AREA_GRP_ID CS
       WHERE  MO.LOC_CD = ML.LOC_CD
       AND    SUBSTR(MO.LOC_CD,1,2) = CS.CNT_CD
       AND    CS.CO_IND_CD = 'H'
       AND    MO.OFC_CD = @[ofc_cd]
   )			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
