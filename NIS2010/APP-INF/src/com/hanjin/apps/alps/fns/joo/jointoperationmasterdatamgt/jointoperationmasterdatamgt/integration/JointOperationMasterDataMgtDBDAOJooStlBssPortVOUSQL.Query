,N2ND_STL_PAIR_PORT_CD = @[n2nd_stl_pair_port_cd]
,N3RD_STL_PAIR_PORT_CD = @[n3rd_stl_pair_port_cd]
,STL_TGT_VVD_BSS_CD    = @[stl_tgt_vvd_bss_cd]
,TRD_CD                = @[trd_cd]
,UC_BSS_PORT_CD        = @[uc_bss_port_cd]
,AGMT_MON_COND_CD      = @[agmt_mon_cond_cd]    
,AGMT_PORT_COND_CD     = @[agmt_port_cond_cd]   
,AGMT_PORT_TP_COND_CD  = @[agmt_port_tp_cond_cd]
,AGMT_OP_TP_COND_CD    = @[agmt_op_tp_cond_cd]  
,UPD_DT                = SYSDATE
,UPD_USR_ID            = @[upd_usr_id]
WHERE	JO_CRR_CD        = @[jo_crr_cd]
AND	  RLANE_CD         = @[rlane_cd]
AND	  SKD_DIR_CD       = @[skd_dir_cd]
AND	  JO_STL_ITM_CD    = @[jo_stl_itm_cd]			]]></sql>
