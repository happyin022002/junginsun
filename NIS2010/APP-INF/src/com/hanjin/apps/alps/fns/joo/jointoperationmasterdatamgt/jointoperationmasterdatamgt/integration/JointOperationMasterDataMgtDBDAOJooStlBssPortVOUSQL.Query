,N1ST_STL_BZC_PORT_CD  = @[n1st_stl_bzc_port_cd]
,N2ND_STL_BZC_PORT_CD  = @[n2nd_stl_bzc_port_cd]
,N3RD_STL_BZC_PORT_CD  = @[n3rd_stl_bzc_port_cd]
