<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationLetterDBDAOCarrierSeqVORSQL">
			<desc><![CDATA[.]]></desc>
			<sql><![CDATA[
SELECT CRR_CNTC_SEQ CODE
	   ,'' NAME
	   ,'' jo_crr_cd
	   ,'' customer_code
  FROM JOO_CNTC_MBR
 WHERE JO_CRR_CD = @[jo_crr_cd]	
   AND CUST_CNT_CD || CUST_SEQ  = @[customer_code]			]]></sql>
			<params>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
				<param name="customer_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
