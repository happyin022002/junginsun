<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOsearchMaxSplitInvWorkNumberRSQL">
			<desc><![CDATA[searchMaxSplitInvWorkNumber]]></desc>
			<sql><![CDATA[
SELECT DECODE(MAX(INV_SPLIT_ISS_WRK_NO), NULL, 1, (MAX(INV_SPLIT_ISS_WRK_NO)+1)) INV_SPLIT_ISS_WRK_NO
   FROM   INV_AR_SPLIT_ISS
   WHERE  1=1
   #if (${bl_src_no} != '') 
   AND    BL_SRC_NO = @[bl_src_no] 
   #end
   #if (${bkg_no} != '') 
   AND    BKG_NO = @[bkg_no]
   #end
   AND    AR_OFC_CD = @[ar_ofc_cd]			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="bkg_no" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
