<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="SettlementProcessDBDAOStlVvdSeqRSQL">
			<desc><![CDATA[Settlement VVD Seq 조회]]></desc>
			<sql><![CDATA[
SELECT NVL((SELECT /*+INDEX_DESC(X XPKJOO_STL_VVD)*/ X.STL_VVD_SEQ+1 
			FROM JOO_STL_VVD X 
			WHERE X.ACCT_YRMON = REPLACE(@[acct_yrmon],'-','') AND ROWNUM = 1),1) AS STL_VVD_SEQ
FROM DUAL			]]></sql>
			<params>
				<param name="acct_yrmon" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
