<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOModifySummaryOfMcsListByCarrierUSQL">
			<desc><![CDATA[0036 조건에 해당하는 remark를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE JOO_SMRY_CRR_RMK SET
    JO_SMRY_RMK = @[jo_smry_rmk]
    , UPD_DT = SYSDATE
    , UPD_USR_ID = @[cre_usr_id]
WHERE 1=1
AND ACCT_YRMON = @[acct_yrmon]
AND TRD_CD = @[trd_cd]
AND RLANE_CD = @[rlane_cd]
AND JO_CRR_CD =@[jo_crr_cd]			]]></sql>
			<params>
				<param name="jo_smry_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
