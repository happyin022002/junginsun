<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JooFileUploadDBDAOFileUploadInfoRSQL">
			<desc><![CDATA[COM_UPLD_FILE 에서 Upload 된 file 정보를 조회]]></desc>
			<sql><![CDATA[
SELECT 	file_sav_id 	file_save_id,
		file_upld_nm 	file_nm,
		file_path_url	file_download,
		upd_usr_id,
		upd_dt
FROM	COM_UPLD_FILE
WHERE	1 = 1
		AND file_sav_id = @[file_save_id]
		#if (${col_nm1}!='')
		AND	@[col_nm1]   = @[col_val1]
		#end
		#if (${col_nm2}!='')
		AND	@[col_nm2]   = @[col_val2]
		#end 
		#if (${col_nm3}!='')
		AND	@[col_nm3]   = @[col_val3]
		#end
		#if (${col_nm4}!='')
		AND	@[col_nm4]   = @[col_val4]
		#end
		#if (${col_nm5}!='')
		AND	@[col_nm5]   = @[col_val5]
		#end
		#if (${col_nm6}!='')
		AND	@[col_nm6]   = @[col_val6]
		#end
		#if (${col_nm7}!='')
		AND	@[col_nm7]   = @[col_val7]
		#end			]]></sql>
			<params>
				<param name="file_save_id" type="12" value="" out="N"/>
				<param name="col_nm1" type="12" value="" out="N"/>
				<param name="col_val1" type="12" value="" out="N"/>
				<param name="col_nm2" type="12" value="" out="N"/>
				<param name="col_val2" type="12" value="" out="N"/>
				<param name="col_nm3" type="12" value="" out="N"/>
				<param name="col_val3" type="12" value="" out="N"/>
				<param name="col_nm4" type="12" value="" out="N"/>
				<param name="col_val4" type="12" value="" out="N"/>
				<param name="col_nm5" type="12" value="" out="N"/>
				<param name="col_val5" type="12" value="" out="N"/>
				<param name="col_nm6" type="12" value="" out="N"/>
				<param name="col_val6" type="12" value="" out="N"/>
				<param name="col_nm7" type="12" value="" out="N"/>
				<param name="col_val7" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
