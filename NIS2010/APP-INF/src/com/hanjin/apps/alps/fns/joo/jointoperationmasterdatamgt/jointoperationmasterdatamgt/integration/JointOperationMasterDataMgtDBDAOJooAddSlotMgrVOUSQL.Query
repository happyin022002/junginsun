<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOJooAddSlotMgrVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE joo_fx_agmt SET 
  OFC_CD          =     	@[ofc_cd]                 
,  AGMT_OFC_CD     =     	@[rel_ofc_cd]             
,  AGMT_TERM_CD    =     	@[term]                   
,  FX_AGMT_DT      =     	REPLACE(@[date_dt],'-','')
,  VSL_CD          =     	SUBSTR(@[vvd_cd],1,4)     
,  SKD_VOY_NO      =     	SUBSTR(@[vvd_cd],5,4)     
,  SKD_DIR_CD      =     	SUBSTR(@[vvd_cd],9,1)     
,  FM_PORT_CD      =     	@[leg_fport]              
,  TO_PORT_CD      =     	@[leg_tport]              
,  BSA_QTY         =     	@[jo_bsa]                 
,  BSA_PER_WGT     =     	@[jo_weight]              
,  BSA_SLT_PRC     =     	@[jo_price]               
,  ATCH_FILE_ID    =     	@[atch_file_id]             
,  FX_AGMT_RMK     =     	@[jo_remark]              
,  STL_DT          =     	@[settle_dt]              
,  ACT_OVR_USD_QTY =     	@[ous_qty]                
,  DELT_FLG        =     	@[delt_flg]   
,  PIC_USR_ID      =        @[pic_usr_id]            
,  UPD_DT          =     	sysdate                   
,  UPD_USR_ID      =     	@[upd_usr_id]  
WHERE  JO_CRR_CD     =     	@[acctg_crr_cd] 
AND  TRD_CD          =     	@[trd_cd]                 
AND  RLANE_CD        =     	@[rlane_cd]               
AND  RE_DIVR_CD      =     	@[rev_divr]               
AND  FX_AGMT_SEQ     =      @[agmt_seq]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="rel_ofc_cd" type="12" value="" out="N"/>
				<param name="term" type="12" value="" out="N"/>
				<param name="date_dt" type="12" value="" out="N"/>
				<param name="vvd_cd" type="12" value="" out="N"/>
				<param name="leg_fport" type="12" value="" out="N"/>
				<param name="leg_tport" type="12" value="" out="N"/>
				<param name="jo_bsa" type="12" value="" out="N"/>
				<param name="jo_weight" type="12" value="" out="N"/>
				<param name="jo_price" type="12" value="" out="N"/>
				<param name="atch_file_id" type="12" value="" out="N"/>
				<param name="jo_remark" type="12" value="" out="N"/>
				<param name="settle_dt" type="12" value="" out="N"/>
				<param name="ous_qty" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="pic_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="acctg_crr_cd" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="rev_divr" type="12" value="" out="N"/>
				<param name="agmt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
