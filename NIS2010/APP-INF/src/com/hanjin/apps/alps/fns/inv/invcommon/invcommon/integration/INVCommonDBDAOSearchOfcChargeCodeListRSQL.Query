<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="INVCommonDBDAOSearchOfcChargeCodeListRSQL">
			<desc><![CDATA[SearchChargeCodeList]]></desc>
			<sql><![CDATA[
SELECT ' ' CHG_CD, ' ' CHG_NM
  FROM DUAL
UNION ALL
SELECT CHG_CD, DECODE(CHG_CD,'JOP','TERMINAL STEVEDORAGE','CRC','MIS. REVENUE(CNTR) - EQ RENTAL',CHG_NM) CHG_NM
  FROM MDM_CHARGE 
 WHERE CHG_CD NOT IN (SELECT CHG_CD FROM INV_AR_LOCL_CHG WHERE AR_OFC_CD != @[ofc_cd] )
ORDER BY CHG_CD			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
