<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOModifyExcelBsaCarrieListUSQL">
			<desc><![CDATA[Excel Uplaod를 이용하여 Add Carrier BSA 정보를 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE JOO_BSA_ADD_CRR 
set JO_VER_FLG              = 'N'
    ,UPD_DT                 = sysdate
    ,UPD_USR_ID             = @[upd_usr_id]
WHERE	JO_BSA_REF_NO       = @[jo_bsa_ref_no]
  and JO_VER_FLG              = 'Y'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="jo_bsa_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
