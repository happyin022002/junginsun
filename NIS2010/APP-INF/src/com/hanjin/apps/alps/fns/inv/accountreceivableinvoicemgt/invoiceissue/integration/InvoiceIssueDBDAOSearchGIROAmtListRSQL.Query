<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOSearchGIROAmtListRSQL">
			<desc><![CDATA[INV_AR_GIRO 테이블에서 select]]></desc>
			<sql><![CDATA[
SELECT GIRO_NO,
  BL_SRC_NO,
  AR_OFC_CD,
  ACT_CUST_CNT_CD,
  ACT_CUST_SEQ,
  VSL_CD,
  SKD_VOY_NO,
  SKD_DIR_CD,
  SAIL_ARR_DT,
  SPL_GIRO_AMT,
  TVA_GIRO_AMT,
  ISS_DT,
  DUE_DT,
  GIRO_RMK
FROM INV_AR_GIRO
WHERE BL_SRC_NO = @[bl_src_no]
  AND AR_OFC_CD = @[ar_ofc_cd]
  AND DELT_FLG = 'N'
ORDER BY GIRO_NO			]]></sql>
			<params>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
