<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="BookingARCreationDBDAOModifyNoINDInvNoUSQL">
			<desc><![CDATA[Modify No IND Inv No]]></desc>
			<sql><![CDATA[
UPDATE INV_AR_MN
SET IDA_INV_NO = @[inv_no]
  , UPD_USR_ID = @[user_id]
  , UPD_DT = SYSDATE
WHERE AR_IF_NO = @[ar_if_no]
			]]></sql>
			<params>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="ar_if_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
