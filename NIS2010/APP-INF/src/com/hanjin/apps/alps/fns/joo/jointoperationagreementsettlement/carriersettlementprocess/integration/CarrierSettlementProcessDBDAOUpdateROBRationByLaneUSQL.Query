<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOUpdateROBRationByLaneUSQL">
			<desc><![CDATA[ROB Container List Inquiry의 Sub-Allocation and Ratio을 수정]]></desc>
			<sql><![CDATA[
UPDATE JOO_ROB_RTO A
SET  A.SLAN_CD 			   = @[rlane_cd]
   , A.SKD_DIR_CD		   = @[skd_dir_cd] 	
   , A.JO_20FT_N1ST_RTO    = NVL(@[jo_20ft_n1st_rto], 0)
   , A.JO_20FT_SUB_TEU_QTY = NVL(@[jo_20ft_sub_teu_qty], 0)
   , A.JO_40FT_N1ST_RTO    = NVL(@[jo_40ft_n1st_rto], 0)
   , A.JO_40FT_SUB_TEU_QTY = NVL(@[jo_40ft_sub_teu_qty], 0)
   , A.JO_45FT_N1ST_RTO    = NVL(@[jo_45ft_n1st_rto], 0)
   , A.JO_45FT_N2ND_RTO    = NVL(@[jo_45ft_n2nd_rto], 0)
   , A.JO_45FT_SUB_TEU_QTY = NVL(@[jo_45ft_sub_teu_qty], 0)
   , A.JO_BSA_TEU_QTY      = NVL(@[jo_bsa_teu_qty], 0)
   , A.CGO_TON_WGT		   = NVL(@[cgo_ton_wgt], 0)
   , A.JO_TON_TEU_QTY	   = NVL(@[jo_ton_teu_qty], 0)
   , A.JO_RND_RULE_LVL     = @[jo_rnd_rule_lvl]
   , A.UPD_USR_ID          = @[usr_id]
   , A.UPD_DT              = SYSDATE
WHERE 1=1
AND A.SLAN_CD = @[org_rlane_cd]
AND A.SKD_DIR_CD = @[org_skd_dir_cd]			]]></sql>
			<params>
				<param name="rlane_cd" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="jo_20ft_n1st_rto" type="12" value="" out="N"/>
				<param name="jo_20ft_sub_teu_qty" type="12" value="" out="N"/>
				<param name="jo_40ft_n1st_rto" type="12" value="" out="N"/>
				<param name="jo_40ft_sub_teu_qty" type="12" value="" out="N"/>
				<param name="jo_45ft_n1st_rto" type="12" value="" out="N"/>
				<param name="jo_45ft_n2nd_rto" type="12" value="" out="N"/>
				<param name="jo_45ft_sub_teu_qty" type="12" value="" out="N"/>
				<param name="jo_bsa_teu_qty" type="12" value="" out="N"/>
				<param name="cgo_ton_wgt" type="12" value="" out="N"/>
				<param name="jo_ton_teu_qty" type="12" value="" out="N"/>
				<param name="jo_rnd_rule_lvl" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="org_rlane_cd" type="12" value="" out="N"/>
				<param name="org_skd_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
