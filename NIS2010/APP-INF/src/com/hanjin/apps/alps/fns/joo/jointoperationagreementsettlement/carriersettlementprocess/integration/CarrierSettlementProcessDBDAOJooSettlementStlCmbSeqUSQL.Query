<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CarrierSettlementProcessDBDAOJooSettlementStlCmbSeqUSQL">
			<desc><![CDATA[JOO_SETTLEMENT 에 STL_CMB_SEQ를 UPDATE한다.]]></desc>
			<sql><![CDATA[
UPDATE JOO_SETTLEMENT
SET    CMB_CFM_FLG = @[cmb_cfm_flg]
      ,UPD_DT      = SYSDATE
      ,UPD_USR_ID  = @[upd_usr_id]
WHERE  ACCT_YRMON  = REPLACE(@[acct_yrmon],'-','')
AND    STL_VVD_SEQ = TO_NUMBER(@[stl_vvd_seq])
AND    STL_SEQ     = TO_NUMBER(@[stl_seq])			]]></sql>
			<params>
				<param name="cmb_cfm_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="acct_yrmon" type="12" value="" out="N"/>
				<param name="stl_vvd_seq" type="12" value="" out="N"/>
				<param name="stl_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
