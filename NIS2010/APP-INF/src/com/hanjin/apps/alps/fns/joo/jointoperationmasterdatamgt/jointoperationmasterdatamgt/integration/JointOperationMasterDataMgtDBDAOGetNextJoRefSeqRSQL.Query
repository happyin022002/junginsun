<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOGetNextJoRefSeqRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT /*+INDEX_DESC(A XPKJOO_BZC_AGMT)*/
       A.JO_REF_SEQ + 1 AS JO_REF_SEQ
  FROM JOO_BZC_AGMT A
 WHERE A.JO_REF_NO = @[jo_ref_no]
   AND ROWNUM    = 1			]]></sql>
			<params>
				<param name="jo_ref_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
