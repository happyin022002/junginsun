<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOCreateKORGIROCSQL">
			<desc><![CDATA[INV_AR_GIRO 테이블에 insert]]></desc>
			<sql><![CDATA[
INSERT
INTO INV_AR_GIRO (GIRO_NO, BL_SRC_NO, AR_OFC_CD, ACT_CUST_CNT_CD, ACT_CUST_SEQ, VSL_CD, SKD_VOY_NO, SKD_DIR_CD, SAIL_ARR_DT, SPL_GIRO_AMT, TVA_GIRO_AMT, ISS_DT, DUE_DT, GIRO_RMK, DELT_FLG, CRE_USR_ID, CRE_DT, UPD_USR_ID, UPD_DT)
VALUES(@[giro_no],
      @[bl_src_no],
      @[ar_ofc_cd],
      @[act_cust_cnt_cd],
      @[act_cust_seq],
      @[vsl_cd],
      @[skd_voy_no],
      @[skd_dir_cd],
      REPLACE(@[sail_arr_dt],'-',''),
      @[spl_giro_amt],
      @[tva_giro_amt],
      REPLACE(@[iss_dt],'-',''),
      REPLACE(@[due_dt],'-',''),
      @[giro_rmk],
      'N',
      @[cre_usr_id],
      SYSDATE,
      @[upd_usr_id],
      SYSDATE)			]]></sql>
			<params>
				<param name="giro_no" type="12" value="" out="N"/>
				<param name="bl_src_no" type="12" value="" out="N"/>
				<param name="ar_ofc_cd" type="12" value="" out="N"/>
				<param name="act_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="act_cust_seq" type="12" value="" out="N"/>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="sail_arr_dt" type="12" value="" out="N"/>
				<param name="spl_giro_amt" type="12" value="" out="N"/>
				<param name="tva_giro_amt" type="12" value="" out="N"/>
				<param name="iss_dt" type="12" value="" out="N"/>
				<param name="due_dt" type="12" value="" out="N"/>
				<param name="giro_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
