<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InvoiceIssueDBDAOsearchIssuedGeneralInvoiceListRSQL">
			<desc><![CDATA[InvoiceIssueDBDAOsearchIssuedGeneralInvoiceListRSQL]]></desc>
			<sql><![CDATA[
#if (${issue_gubn} != 'S' && ${issue_gubn} != 'SS') 
SELECT DISTINCT V1.INV_NO INV_NO
     , V1.INV_SEQ INV_SEQ 
     , T1.VSL_CD||T1.SKD_VOY_NO||T1.SKD_DIR_CD VVD
     , T1.ACT_CUST_CNT_CD||LPAD(T1.ACT_CUST_SEQ, 6, '0') CUST_CD
     , DECODE(SIGN(LENGTH(T2.CUST_LGL_ENG_NM) - 40), 1, SUBSTR(T2.CUST_LGL_ENG_NM, 0, 37)||'...', T2.CUST_LGL_ENG_NM) CUST_NM
     --, T2.CUST_LGL_ENG_NM CUST_NM
     , T1.INV_REF_NO INV_REF_NO
     , V1.AR_IF_NO AR_IF_NO
     , V1.BL_SRC_NO BL_SRC_NO 
     , (SELECT NVL(LOCL_PO_NO,'') FROM INV_AR_ISS WHERE INV_NO = V1.INV_NO AND INV_SEQ = V1.INV_SEQ) LOCL_PO_NO
#if (${issue_gubn} == 'I') 
     , NVL(DECODE(T1.IO_BND_CD, 'O', DECODE(T1.AR_OFC_CD,'VLCSC', NVL(T33.AUTO_INV_EML, T55.CNTC_PSON_EML) --2010-06-22
														,'HAMSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
														,'ANRSO', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
														,'RTMSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
														,'PRGSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                            							,'WRPSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                            							,'FXTSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                            							,'LEHSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                                     ,DECODE(T5.CNTC_PSON_EML, '', DECODE(T55.CNTC_PSON_EML, '', T33.AUTO_INV_EML, T55.CNTC_PSON_EML), T5.CNTC_PSON_EML))
                              , 'I', T33.AUTO_INV_EML), '') CUST_EML 
     , NVL(DECODE(T1.IO_BND_CD, 'O', DECODE(T1.AR_OFC_CD,'VLCSC', NVL(T3.OB_FAX_NO, T55.CNTC_PSON_FAX_NO) --2010-06-22
														,'HAMSC', T3.OB_FAX_NO
														,'ANRSO', T3.OB_FAX_NO
														,'RTMSC', T3.OB_FAX_NO
														,'PRGSC', T3.OB_FAX_NO
                                     ,DECODE(T5.CNTC_PSON_FAX_NO, '', DECODE(T55.CNTC_PSON_FAX_NO, '', T3.OB_FAX_NO, T55.CNTC_PSON_FAX_NO), T5.CNTC_PSON_FAX_NO))
                              , 'I', T3.IB_FAX_NO), '') CUST_FAX_NO
#else
     , NVL(DECODE(T1.IO_BND_CD, 'O', DECODE(T1.AR_OFC_CD,'VLCSC', NVL(NVL(T33.AUTO_INV_EML, T55.CNTC_PSON_EML), V2.INV_SND_CUST_NO)
														,'HAMSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,NVL(T55.CNTC_PSON_EML,V2.INV_SND_CUST_NO)))
														,'ANRSO', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,NVL(T55.CNTC_PSON_EML,V2.INV_SND_CUST_NO)))
														,'RTMSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,NVL(T55.CNTC_PSON_EML,V2.INV_SND_CUST_NO)))
														,'PRGSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,NVL(T55.CNTC_PSON_EML,V2.INV_SND_CUST_NO)))
                            							,'WRPSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,NVL(T55.CNTC_PSON_EML,V2.INV_SND_CUST_NO)))
                            							,'FXTSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,NVL(T55.CNTC_PSON_EML,V2.INV_SND_CUST_NO)))
                            							,'LEHSC', NVL(T33.AUTO_INV_EML, NVL(T5.CNTC_PSON_EML,NVL(T55.CNTC_PSON_EML,V2.INV_SND_CUST_NO)))
 
                            ,DECODE(T5.CNTC_PSON_EML, '', DECODE(T55.CNTC_PSON_EML, '', DECODE(T33.AUTO_INV_EML, '', V2.INV_SND_CUST_NO, T33.AUTO_INV_EML), T55.CNTC_PSON_EML), T5.CNTC_PSON_EML))
                              , 'I', DECODE(T33.AUTO_INV_EML, '',  V2.INV_SND_CUST_NO,T33.AUTO_INV_EML)), '') CUST_EML
     , NVL(DECODE(T1.IO_BND_CD, 'O', DECODE(T1.AR_OFC_CD,'VLCSC', NVL(NVL(T3.OB_FAX_NO, T55.CNTC_PSON_FAX_NO), V3.INV_SND_CUST_NO)
														,'HAMSC', DECODE(T3.OB_FAX_NO, '',  V3.INV_SND_CUST_NO, T3.OB_FAX_NO)
														,'ANRSO', DECODE(T3.OB_FAX_NO, '',  V3.INV_SND_CUST_NO, T3.OB_FAX_NO)
														,'RTMSC', DECODE(T3.OB_FAX_NO, '',  V3.INV_SND_CUST_NO, T3.OB_FAX_NO)
														,'PRGSC', DECODE(T3.OB_FAX_NO, '',  V3.INV_SND_CUST_NO, T3.OB_FAX_NO)
                            ,DECODE(T5.CNTC_PSON_FAX_NO, '', DECODE(T55.CNTC_PSON_FAX_NO, '', DECODE(T3.OB_FAX_NO, '', V3.INV_SND_CUST_NO, T3.OB_FAX_NO), T55.CNTC_PSON_FAX_NO), T5.CNTC_PSON_FAX_NO))
                            , 'I', DECODE(T3.IB_FAX_NO, '',  V3.INV_SND_CUST_NO, T3.IB_FAX_NO)), '') CUST_FAX_NO
#end
     , DECODE(T1.IO_BND_CD, 'I', T1.POD_CD, 'O', T1.POL_CD) PORT_CD
     , 0 F_ADD
     , 1 F_DEL
     , 2 F_COPY
     , DECODE(T1.INV_RMK, '', ' ', 'YES') INV_RMK
     , T1.INV_RMK INV_ISS_RMK
     , T1.IO_BND_CD IO_BND_CD
     , DECODE(T7.AR_OFC_CD, '', ' ', 'YES') ATTACH_VIEW
     , DECODE(T8.AR_OFC_CD, '', ' ', 'YES') ATTACH_VIEW2
     , V1.INV_ISS_CMB_FLG INV_ISS_CMB_FLG
	 , DECODE(T10.INV_EML_SPLIT_FLG,'Y','Y','N','N', DECODE(T9.INV_EML_SPLIT_FLG, 'Y','Y','N')) INV_EML_SPLIT_FLG 
  FROM
(SELECT C.INV_NO INV_NO
      --, MAX(A.AR_IF_NO) AR_IF_NO
      , SUBSTR(MAX(DECODE(A.REV_TP_CD, 'M', '1', '2')||A.AR_IF_NO), 2, 11) AR_IF_NO
      , A.BL_SRC_NO BL_SRC_NO
      , MAX(B.INV_SEQ) INV_SEQ
      , B.INV_ISS_CMB_FLG INV_ISS_CMB_FLG
   FROM INV_AR_MN A
      , INV_AR_ISS B
      , INV_AR_ISS_DTL C
  WHERE 1 = 1
#if (${flag} == 'S') 
    AND C.INV_NO IN (${invs})
#elseif (${flag} == 'M') 
    AND C.INV_NO BETWEEN @[f_inv] AND @[t_inv]
#elseif (${flag} == 'IF') 
    AND A.AR_IF_NO IN (${invs})
    AND C.INV_NO IN (SELECT INV_NO FROM INV_AR_MN WHERE AR_IF_NO IN (${invs}))
#end
    AND A.AR_OFC_CD = @[iss_ofc_cd]
    AND C.AR_IF_NO = A.AR_IF_NO
    AND C.INV_NO = B.INV_NO
 GROUP BY C.INV_NO
        , A.BL_SRC_NO 
        , B.INV_ISS_CMB_FLG) V1
     , INV_AR_MN T1
     , MDM_CUSTOMER T2    
     , MDM_CR_CUST T3
     , MDM_CUST_REP T33
     , BKG_BOOKING T4
     , BKG_CNTC_PSON T5
     , BKG_CNTC_PSON T55
#if (${issue_gubn} == 'R') 
, (SELECT INV_NO
     , INV_SEQ
     , LTRIM(SYS_CONNECT_BY_PATH(INV_SND_CUST_NO,';'),';') INV_SND_CUST_NO
  FROM ( SELECT INV_NO
              , INV_SEQ
              , INV_SND_CUST_NO
              , ROW_NUMBER() OVER (PARTITION BY INV_NO, INV_SEQ ORDER BY INV_SND_CUST_NO) RN
              , COUNT(*) OVER (PARTITION BY INV_NO, INV_SEQ) CNT
           FROM INV_AR_ISS_SND 
          WHERE (INV_NO,INV_SEQ) IN (SELECT C.INV_NO,MAX(B.INV_SEQ)
                                        FROM INV_AR_MN A
                                        , INV_AR_ISS B
                                        , INV_AR_ISS_DTL C
                                        WHERE 1 = 1
                                        #if (${flag} == 'S') 
                                        	AND C.INV_NO IN (${invs})
                                        #elseif (${flag} == 'M') 
                                            AND C.INV_NO BETWEEN @[f_inv] AND @[t_inv]
                                        #elseif (${flag} == 'IF') 
                                            AND A.AR_IF_NO IN (${invs})
                                            AND C.INV_NO IN (SELECT INV_NO FROM INV_AR_MN WHERE AR_IF_NO IN (${invs}))
                                        #end
                                        AND A.AR_OFC_CD = @[iss_ofc_cd]
                                        AND C.AR_IF_NO = A.AR_IF_NO
                                        AND C.INV_NO = B.INV_NO
                                        GROUP BY C.INV_NO
                                        --, A.BL_SRC_NO 
										)                
            AND INV_ISS_SND_TP_CD = 'E'           
            )
 WHERE LEVEL = CNT
 START WITH RN = 1
CONNECT BY PRIOR INV_NO = INV_NO AND INV_SEQ = INV_SEQ AND PRIOR RN = RN-1 ) V2
, ( SELECT *
      FROM INV_AR_ISS_SND E
     WHERE (INV_NO,INV_SEQ) IN (SELECT C.INV_NO,MAX(B.INV_SEQ)
                                        FROM INV_AR_MN A
                                        , INV_AR_ISS B
                                        , INV_AR_ISS_DTL C
                                        WHERE 1 = 1
                                        #if (${flag} == 'S') 
                                        	AND C.INV_NO IN (${invs})
                                        #elseif (${flag} == 'M') 
                                            AND C.INV_NO BETWEEN @[f_inv] AND @[t_inv]
                                        #elseif (${flag} == 'IF') 
                                            AND A.AR_IF_NO IN (${invs})
                                            AND C.INV_NO IN (SELECT INV_NO FROM INV_AR_MN WHERE AR_IF_NO IN (${invs}))
                                        #end
                                        AND A.AR_OFC_CD = @[iss_ofc_cd]
                                        AND C.AR_IF_NO = A.AR_IF_NO
                                        AND C.INV_NO = B.INV_NO
                                        GROUP BY C.INV_NO
                                        --, A.BL_SRC_NO 
										) 
       AND INV_ISS_SND_TP_CD = 'F' ) V3
#end
     , INV_ISS_ATCH T7
     , INV_ISS_CUST_ATCH T8
     , INV_AR_STUP_OFC T9
     , INV_AR_EML_CUST_RGST T10
 WHERE V1.AR_IF_NO = T1.AR_IF_NO
   AND T1.ACT_CUST_CNT_CD = T2.CUST_CNT_CD
   AND T1.ACT_CUST_SEQ = T2.CUST_SEQ
   AND T1.ACT_CUST_CNT_CD = T3.CUST_CNT_CD(+)
   AND T1.ACT_CUST_SEQ = T3.CUST_SEQ(+)
    AND T1.ACT_CUST_CNT_CD = T33.CUST_CNT_CD(+)
   AND T1.ACT_CUST_SEQ = T33.CUST_SEQ(+)
   AND T1.IO_BND_CD = T33.IO_BND_CD(+)
   AND T1.AR_OFC_CD = T33.OFC_CD(+)
   AND T1.BL_SRC_NO = T4.BL_NO(+)
   AND T4.BKG_NO = T5.BKG_NO(+)
   AND T5.BKG_CNTC_PSON_TP_CD(+) = 'BK'
   AND T4.BKG_NO = T55.BKG_NO(+)
   AND T55.BKG_CNTC_PSON_TP_CD(+) = 'SI'
#if (${issue_gubn} == 'R') 
   AND V1.INV_NO = V2.INV_NO(+)
   AND V1.INV_SEQ = V2.INV_SEQ(+)
   AND V1.INV_NO = V3.INV_NO(+)
   AND V1.INV_SEQ = V3.INV_SEQ(+)
#end
   AND T1.AR_OFC_CD = T7.AR_OFC_CD(+)
   AND T1.VSL_CD = T7.VSL_CD(+)
   AND T1.SKD_VOY_NO = T7.SKD_VOY_NO(+)
   AND T1.SKD_DIR_CD = T7.SKD_DIR_CD(+)
   AND DECODE(T1.IO_BND_CD, 'I', T1.POD_CD, 'O', T1.POL_CD) = T7.PORT_CD(+)
   AND T7.TXT_NO(+) = 1
   AND T1.AR_OFC_CD = T8.AR_OFC_CD(+)
   AND T1.ACT_CUST_CNT_CD = T8.CUST_CNT_CD(+)
   AND T1.ACT_CUST_SEQ = T8.CUST_SEQ(+)
   AND T8.TXT_NO(+) = 1
   AND T1.AR_OFC_CD = T9.AR_OFC_CD(+)
   AND T1.ACT_CUST_CNT_CD = T10.CUST_CNT_CD(+)
   AND T1.ACT_CUST_SEQ = T10.CUST_SEQ(+)
 ORDER BY T1.VSL_CD||T1.SKD_VOY_NO||T1.SKD_DIR_CD, T1.ACT_CUST_CNT_CD||LPAD(T1.ACT_CUST_SEQ, 6, '0'), V1.INV_NO, V1.AR_IF_NO, V1.BL_SRC_NO ASC
#else
SELECT DISTINCT S1.INV_NO INV_NO
     , S1.INV_SEQ INV_SEQ
     , T1.VSL_CD||T1.SKD_VOY_NO||T1.SKD_DIR_CD VVD
     , S1.ACT_CUST_CNT_CD||LPAD(S1.ACT_CUST_SEQ, 6, '0') CUST_CD
     , DECODE(SIGN(LENGTH(T2.CUST_LGL_ENG_NM) - 40), 1, SUBSTR(T2.CUST_LGL_ENG_NM, 0, 37)||'...', T2.CUST_LGL_ENG_NM) CUST_NM
     , T1.INV_REF_NO INV_REF_NO
     , S1.AR_IF_NO AR_IF_NO
     , S1.BL_SRC_NO BL_SRC_NO
     , '' LOCL_PO_NO  --DXBSC에만 사용.
     , NVL(DECODE(T1.IO_BND_CD, 'O', DECODE(T1.AR_OFC_CD,'VLCSC', NVL(T3.OB_EML, T55.CNTC_PSON_EML) --2010-06-22
              											,'HAMSC', NVL(T3.OB_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
              											,'ANRSO', NVL(T3.OB_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
              											,'RTMSC', NVL(T3.OB_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
              											,'PRGSC', NVL(T3.OB_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                                    ,'WRPSC', NVL(T3.OB_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                                    ,'FXTSC', NVL(T3.OB_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                                    ,'LEHSC', NVL(T3.OB_EML, NVL(T5.CNTC_PSON_EML,T55.CNTC_PSON_EML))
                                     ,DECODE(T5.CNTC_PSON_EML, '', DECODE(T55.CNTC_PSON_EML, '', T3.OB_EML, T55.CNTC_PSON_EML), T5.CNTC_PSON_EML))
                              , 'I', T3.IB_EML), '') CUST_EML
     , NVL(DECODE(T1.IO_BND_CD, 'O', DECODE(T1.AR_OFC_CD,'VLCSC', NVL(T3.OB_FAX_NO, T55.CNTC_PSON_FAX_NO) --2010-06-22
              											,'HAMSC', T3.OB_FAX_NO
              											,'ANRSO', T3.OB_FAX_NO
              											,'RTMSC', T3.OB_FAX_NO
              											,'PRGSC', T3.OB_FAX_NO
                                     ,DECODE(T5.CNTC_PSON_FAX_NO, '', DECODE(T55.CNTC_PSON_FAX_NO, '', T3.OB_FAX_NO, T55.CNTC_PSON_FAX_NO), T5.CNTC_PSON_FAX_NO))
                              , 'I', T3.IB_FAX_NO), '') CUST_FAX_NO
     , DECODE(T1.IO_BND_CD, 'I', T1.POD_CD, 'O', T1.POL_CD) PORT_CD
     , 0 F_ADD
     , 1 F_DEL
     , 2 F_COPY
     , DECODE(T1.INV_RMK, '', ' ', 'YES') INV_RMK
     , T1.INV_RMK INV_ISS_RMK
     , T1.IO_BND_CD IO_BND_CD
     , DECODE(T7.AR_OFC_CD, '', ' ', 'YES') ATTACH_VIEW
     , DECODE(T8.AR_OFC_CD, '', ' ', 'YES') ATTACH_VIEW2
     , 'N' INV_ISS_CMB_FLG  -- 베트남만 사용. 
	 , DECODE(T10.INV_EML_SPLIT_FLG,'Y','Y','N','N', DECODE(T9.INV_EML_SPLIT_FLG, 'Y','Y','N')) INV_EML_SPLIT_FLG 
  FROM
           INV_AR_MN  T1
         , INV_AR_SPLIT_ISS  S1
         , INV_AR_SPLIT_ISS_CHG S2
         , MDM_CUSTOMER T2    
         , MDM_CR_CUST T3
         , BKG_BOOKING T4
         , BKG_CNTC_PSON T5
         , BKG_CNTC_PSON T55
         , INV_ISS_ATCH T7
         , INV_ISS_CUST_ATCH T8
     	 , INV_AR_STUP_OFC T9
     	 , INV_AR_EML_CUST_RGST T10
 WHERE 1=1
   AND S1.AR_IF_NO = T1.AR_IF_NO
   AND S1.BL_SRC_NO = T1.BL_SRC_NO
   AND S1.ACT_CUST_CNT_CD = T2.CUST_CNT_CD
   AND S1.ACT_CUST_SEQ = T2.CUST_SEQ
   AND S1.ACT_CUST_CNT_CD = T3.CUST_CNT_CD(+)
   AND S1.ACT_CUST_SEQ = T3.CUST_SEQ(+)
   AND T1.BL_SRC_NO = T4.BL_NO(+)
   AND T4.BKG_NO = T5.BKG_NO(+)
   AND T5.BKG_CNTC_PSON_TP_CD(+) = 'BK'
   AND T4.BKG_NO = T55.BKG_NO(+)
   AND T55.BKG_CNTC_PSON_TP_CD(+) = 'SI'
   AND T1.AR_OFC_CD = T7.AR_OFC_CD(+)
   AND T1.VSL_CD = T7.VSL_CD(+)
   AND T1.SKD_VOY_NO = T7.SKD_VOY_NO(+)
   AND T1.SKD_DIR_CD = T7.SKD_DIR_CD(+)
   AND DECODE(T1.IO_BND_CD, 'I', T1.POD_CD, 'O', T1.POL_CD) = T7.PORT_CD(+)
   AND T7.TXT_NO(+) = 1
   AND S1.AR_OFC_CD = T8.AR_OFC_CD(+)
   AND S1.ACT_CUST_CNT_CD = T8.CUST_CNT_CD(+)
   AND S1.ACT_CUST_SEQ = T8.CUST_SEQ(+)
   AND T8.TXT_NO(+) = 1
#if (${issue_gubn} == 'SS')
   AND S1.AR_IF_NO IN (SELECT AR_IF_NO FROM INV_AR_SPLIT_ISS WHERE INV_NO IN (${f_inv}) AND INV_SEQ = 1)
#elseif (${issue_gubn} == 'S') 
   AND S1.AR_IF_NO = @[if_no]
#end 
   AND S1.INV_NO IN (${f_inv})
   AND S1.INV_SEQ  = 1
   AND T1.AR_OFC_CD = T9.AR_OFC_CD(+)
   AND T1.ACT_CUST_CNT_CD = T10.CUST_CNT_CD(+)
   AND T1.ACT_CUST_SEQ = T10.CUST_SEQ(+)
 ORDER BY T1.VSL_CD||T1.SKD_VOY_NO||T1.SKD_DIR_CD, S1.ACT_CUST_CNT_CD||LPAD(S1.ACT_CUST_SEQ, 6, '0'), S1.INV_NO, S1.AR_IF_NO, S1.BL_SRC_NO ASC
#end			]]></sql>
			<params>
				<param name="f_inv" type="12" value="VL0238948" out="N"/>
				<param name="t_inv" type="12" value="VL0238950" out="N"/>
				<param name="iss_ofc_cd" type="12" value="VLCBB" out="N"/>
				<param name="if_no" type="12" value="BOM02026223" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
