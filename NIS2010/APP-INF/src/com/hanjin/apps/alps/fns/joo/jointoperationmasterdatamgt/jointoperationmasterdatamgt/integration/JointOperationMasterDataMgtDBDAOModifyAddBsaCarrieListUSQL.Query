<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationMasterDataMgtDBDAOModifyAddBsaCarrieListUSQL">
			<desc><![CDATA[Add Bsa Carrier에 저장된 Teu Qty값을 JOO_BSA_INFO_ENTR 테이블에 Update 한다]]></desc>
			<sql><![CDATA[
UPDATE JOO_BSA_AGMT
   SET JO_ADD_BSA_CRR_FLG   = 'Y'    
   , JO_BSA_ADD_TEU_QTY   = (SELECT SUM(JO_BSA_TEU_QTY)
                               FROM JOO_ADD_BSA_CRR   
                              WHERE VSL_CD     =  @[vsl_cd] 
                                AND SKD_VOY_NO = @[skd_voy_no]
                                AND SKD_DIR_CD = @[skd_dir_cd]
                                AND PORT_CD = @[port_cd]
                                AND PORT_SEQ = @[port_seq]
                            )
   , JO_OVR_BSA_TEU_QTY   = JO_BSA_TEU_QTY + (SELECT SUM(JO_BSA_TEU_QTY)
                                                FROM JOO_ADD_BSA_CRR   
                                               WHERE VSL_CD     =  @[vsl_cd] 
                                                 AND SKD_VOY_NO = @[skd_voy_no]
                                                 AND SKD_DIR_CD = @[skd_dir_cd]
                                                 AND PORT_CD = @[port_cd]
                                                 AND PORT_SEQ = @[port_seq]
                                              )
	,UPD_DT                 = sysdate
    ,UPD_USR_ID             = @[upd_usr_id]
 WHERE VSL_CD     =  @[vsl_cd] 
   AND SKD_VOY_NO = @[skd_voy_no]
   AND SKD_DIR_CD = @[skd_dir_cd]
   AND PORT_CD = @[port_cd]
   AND PORT_SEQ = @[port_seq]
   AND JO_CRR_CD = @[jo_crr_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="skd_voy_no" type="12" value="" out="N"/>
				<param name="skd_dir_cd" type="12" value="" out="N"/>
				<param name="port_cd" type="12" value="" out="N"/>
				<param name="port_seq" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="jo_crr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
