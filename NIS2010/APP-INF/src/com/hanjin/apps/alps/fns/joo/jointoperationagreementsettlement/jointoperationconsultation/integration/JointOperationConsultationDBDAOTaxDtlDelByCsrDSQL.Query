<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JointOperationConsultationDBDAOTaxDtlDelByCsrDSQL">
			<desc><![CDATA[CSR번호로 JOO_TAX_DTL을 삭제]]></desc>
			<sql><![CDATA[
DELETE JOO_TAX_DTL
WHERE  (TAX_INV_YRMON, OFC_CD, TAX_SER_NO) IN (
SELECT TAX_INV_YRMON, OFC_CD, TAX_SER_NO
FROM   JOO_TAX
WHERE  SLP_TP_CD   = SUBSTR(@[csr_no], 1,2)
AND    SLP_FUNC_CD = SUBSTR(@[csr_no], 3,1)
AND    SLP_OFC_CD  = DECODE(LENGTH(@[csr_no]),20,SUBSTR(@[csr_no],4,6),SUBSTR(@[csr_no],4,5))
AND    SLP_ISS_DT  = TO_CHAR(TO_DATE(DECODE(LENGTH(@[csr_no]),20,SUBSTR(@[csr_no],10,6),SUBSTR(@[csr_no],9,6)),'RRMMDD'),'YYYYMMDD')
AND    SLP_SER_NO  = DECODE(LENGTH(@[csr_no]),20,SUBSTR(@[csr_no],16),SUBSTR(@[csr_no],15))
)			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
