<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmTradeDBDAOModifyMdmTradeUSQL">
			<desc><![CDATA[JMS에서 받은 데이터 DB Logic 처리를 담당한다.(수정)]]></desc>
			<sql><![CDATA[
UPDATE mdm_trade
   SET trd_nm       = @[trd_nm]     ,
       vsl_tp_cd    = @[vsl_tp_cd]  ,
       fm_conti_cd  = @[fm_conti_cd],
       to_conti_cd  = @[to_conti_cd],
       cre_usr_id   = @[cre_usr_id] ,
       cre_dt       = TO_DATE(@[cre_dt], 'yyyymmddhh24miss'),
       upd_usr_id   = @[upd_usr_id] ,
       upd_dt       = TO_DATE(@[upd_dt], 'yyyymmddhh24miss'),
       modi_trd_cd  = @[modi_trd_cd],
       delt_flg     = @[delt_flg]   ,
       eai_evnt_dt  = TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss'),
       ofc_cd       = @[ofc_cd]     ,
       st_eff_dt    = TO_DATE(@[st_eff_dt], 'yyyymmddhh24miss'),
       end_eff_dt   = TO_DATE(@[end_eff_dt], 'yyyymmddhh24miss'),
       eai_if_id    = @[eai_if_id]
 WHERE trd_cd       = @[trd_cd]
   AND eai_evnt_dt <= TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="trd_nm" type="12" value="" out="N"/>
				<param name="vsl_tp_cd" type="12" value="" out="N"/>
				<param name="fm_conti_cd" type="12" value="" out="N"/>
				<param name="to_conti_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="modi_trd_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="st_eff_dt" type="12" value="" out="N"/>
				<param name="end_eff_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="trd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
