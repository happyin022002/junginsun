<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmSubcontinentDBDAOModifyMdmSubcontinentUSQL">
			<desc><![CDATA[ModifyMdmSubcontinent]]></desc>
			<sql><![CDATA[
UPDATE mdm_subcontinent SET                                        			
	 					conti_cd    = @[conti_cd] ,sconti_nm   =  @[sconti_nm] ,cre_usr_id  = @[cre_usr_id] ,			
	 	cre_dt      = to_date( @[cre_dt] ,'yyyymmddhh24miss'),upd_usr_id  = @[upd_usr_id]   , 			
	 	upd_dt      = to_date( @[upd_dt],'yyyymmddhh24miss'),delt_flg    = @[delt_flg],    			
	 	eai_evnt_dt = to_date( @[eai_evnt_dt],'yyyymmddhh24miss'),eai_if_id = @[eai_if_id]                     			
	  WHERE 	sconti_cd = @[sconti_cd]                         								
	  AND 	eai_evnt_dt <= to_date( @[eai_evnt_dt],'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="sconti_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="sconti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
