<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCustomerDBDAOSearchMdmCustCntcPntListRSQL">
			<desc><![CDATA[SearchMdmCustCntcPntList]]></desc>
			<sql><![CDATA[
SELECT cust_cnt_cd 												
  FROM mdm_cust_cntc_pnt										
 WHERE cust_cnt_cd       = @[cust_cnt_cd]     	               				
   AND cust_seq          = @[cust_seq]      	               				
   AND cust_cntc_pnt_seq = @[cust_cntc_pnt_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_cntc_pnt_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
