<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueApTaxDBDAORemoveApTaxUSQL">
			<desc><![CDATA[RemoveApTax]]></desc>
			<sql><![CDATA[
UPDATE ap_tax SET aval_flg = 'N'
WHERE 	ap_tax_nm  = HJSEAI_PKG.h_decode(@[ap_tax_nm], 'UTF8' ,'UTF8')
AND	tax_no    = @[tax_no]
AND 	eai_evnt_dt <= to_date(@[eai_evnt_dt],'yyyymmddhh24miss')
			]]></sql>
			<params>
				<param name="ap_tax_nm" type="12" value="" out="N"/>
				<param name="tax_no" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
