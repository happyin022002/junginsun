<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCntrSzDBDAOSearchMdmCntrSzListRSQL">
			<desc><![CDATA[MDM_CNTR_SZ 기존 데이타 유무 검색]]></desc>
			<sql><![CDATA[
SELECT 
    COUNT(1) CNTR_SZ_CNT 
FROM 
    MDM_CNTR_SZ
WHERE 
    cntr_sz_cd = @[cntr_sz_cd]			]]></sql>
			<params>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
