<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCommodityDBDAOModifyMdmCommodityUSQL">
			<desc><![CDATA[JMS에서 받은 데이터 DB Logic 처리를 담당한다.(수정)]]></desc>
			<sql><![CDATA[
UPDATE mdm_commodity
   SET cmdt_nm         = @[cmdt_nm],
       grp_cmdt_cd     = @[grp_cmdt_cd],
       rep_cmdt_cd     = @[rep_cmdt_cd],
       rep_imdg_lvl_cd = @[rep_imdg_lvl_cd],
       cre_usr_id      = @[cre_usr_id],
       cre_dt          = TO_DATE(@[cre_dt], 'yyyymmddhh24miss'),
       upd_usr_id      = @[upd_usr_id],
       upd_dt          = TO_DATE(@[upd_dt], 'yyyymmddhh24miss'),
       delt_flg        = @[delt_flg],
       eai_evnt_dt     = TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss'),
       fmc_exp_flg     = @[fmc_exp_flg],
       eai_if_id       = @[eai_if_id],
       chem_flg        = @[chem_flg]
 WHERE cmdt_cd      = @[cmdt_cd]
   AND eai_evnt_dt <= TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="cmdt_nm" type="12" value="" out="N"/>
				<param name="grp_cmdt_cd" type="12" value="" out="N"/>
				<param name="rep_cmdt_cd" type="12" value="" out="N"/>
				<param name="rep_imdg_lvl_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="fmc_exp_flg" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="chem_flg" type="12" value="" out="N"/>
				<param name="cmdt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
