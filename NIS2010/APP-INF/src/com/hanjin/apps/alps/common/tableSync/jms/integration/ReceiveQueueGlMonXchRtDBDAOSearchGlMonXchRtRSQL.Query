<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueGlMonXchRtDBDAOSearchGlMonXchRtRSQL">
			<desc><![CDATA[SearchGlMonXchRt]]></desc>
			<sql><![CDATA[
SELECT acct_xch_rt_yrmon
  FROM GL_MON_XCH_RT
 WHERE acct_xch_rt_yrmon    = @[acct_xch_rt_yrmon]
   AND acct_xch_rt_lvl      = @[acct_xch_rt_lvl]
   AND curr_cd 	            = @[curr_cd]			]]></sql>
			<params>
				<param name="s_acct_xch_rt_yrmon" type="12" value="" out="N"/>
				<param name="s_acct_xch_rt_lvl" type="12" value="" out="N"/>
				<param name="s_curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
