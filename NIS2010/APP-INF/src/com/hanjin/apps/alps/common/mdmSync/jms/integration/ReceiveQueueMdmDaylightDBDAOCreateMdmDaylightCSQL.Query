<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmDaylightDBDAOCreateMdmDaylightCSQL">
			<desc><![CDATA[MDM Daylight Saving Time Creation]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_DYLGT_SAV_TM(
    DST_ID,
    DST_CNT_CD,
    DST_NOT_APLY_STE_CD,
    DST_YR,
    DST_MNTS,
    ST_DST_RULE_DESC,
    END_DST_RULE_DESC,
	ST_DST_DT,
	ST_DST_HRMNT,
	END_DST_HRMNT,
	END_DST_DT,
	CRE_USR_ID,
	CRE_DT,
	UPD_USR_ID,
	UPD_DT,
	DELT_FLG,
	EAI_EVNT_DT)
VALUES(
    @[dst_id],
    @[dst_cnt_cd],
    @[dst_not_aply_ste_cd],
    @[dst_yr],
    @[dst_mnts],
    @[st_dst_rule_desc],
    @[end_dst_rule_desc],
    SUBSTR(@[st_dst_dt],1,8),
    @[st_dst_hrmnt],
    @[end_dst_hrmnt],
    SUBSTR(@[end_dst_dt],1,8),
    @[cre_usr_id],
    to_date(@[cre_dt],'yyyymmddhh24miss'),
    @[upd_usr_id],
    to_date(@[upd_dt],'yyyymmddhh24miss'),
	NVL(@[delt_flg],'N'),
	SYSDATE)			]]></sql>
			<params>
				<param name="dst_id" type="12" value="" out="N"/>
				<param name="dst_cnt_cd" type="12" value="" out="N"/>
				<param name="dst_not_aply_ste_cd" type="12" value="" out="N"/>
				<param name="dst_yr" type="12" value="" out="N"/>
				<param name="dst_mnts" type="2" value="" out="N"/>
				<param name="st_dst_rule_desc" type="12" value="" out="N"/>
				<param name="end_dst_rule_desc" type="12" value="" out="N"/>
				<param name="st_dst_dt" type="12" value="" out="N"/>
				<param name="st_dst_hrmnt" type="12" value="" out="N"/>
				<param name="end_dst_hrmnt" type="12" value="" out="N"/>
				<param name="end_dst_dt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
