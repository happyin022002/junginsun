<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmSvcScpDBDAORemoveMdmSvcScpUSQL">
			<desc><![CDATA[modify delete]]></desc>
			<sql><![CDATA[
UPDATE mdm_svc_scp  SET delt_flg = 'Y',
	upd_usr_id = @[upd_usr_id]         ,
	upd_dt = to_date(@[upd_dt],'yyyymmddhh24miss'),
	eai_evnt_dt = to_date(@[eai_evnt_dt],'yyyymmddhh24miss')
WHERE 	svc_scp_cd = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
