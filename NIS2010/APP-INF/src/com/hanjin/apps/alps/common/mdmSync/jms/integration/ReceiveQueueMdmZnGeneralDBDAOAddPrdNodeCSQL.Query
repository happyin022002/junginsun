<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmZnGeneralDBDAOAddPrdNodeCSQL">
			<desc><![CDATA[AddPrdNode]]></desc>
			<sql><![CDATA[
INSERT INTO prd_node (                                      					
			 	nod_cd 	       ,nod_nm 	       ,nod_tp_cd      ,loc_cd 	       ,			
			 	onf_hir_yd_flg ,delt_flg       ,cre_usr_id     ,cre_dt 	       ,			
			 	upd_usr_id     ,upd_dt 	       ,eai_evnt_dt
			  )                                                           					
			  VALUES  (													 					
			 	@[zn_cd] ,@[zn_nm] ,'Z' ,@[loc_cd] ,'N' ,@[delt_flg] ,@[cre_usr_id] ,to_date(@[cre_dt] ,'yyyymmddhh24miss'), @[upd_usr_id] ,to_date(@[upd_dt] ,'yyyymmddhh24miss') ,to_date(@[eai_evnt_dt] ,'yyyymmddhh24miss')	
			  )			]]></sql>
			<params>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="zn_nm" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
