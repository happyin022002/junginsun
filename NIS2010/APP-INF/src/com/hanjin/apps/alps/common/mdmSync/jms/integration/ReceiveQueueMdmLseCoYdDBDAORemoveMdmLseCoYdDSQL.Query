<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmLseCoYdDBDAORemoveMdmLseCoYdDSQL">
			<desc><![CDATA[MDM_LSE_CO_YD 삭제

<Change History>
1	2009.09.08	Lee ByoungHun	최초작성]]></desc>
			<sql><![CDATA[
/* ReceiveQueueMdmLseCoYdDBDAO - removeMDM_LSE_CO_YD() */
UPDATE MDM_LSE_CO_YD SET 
    DELT_FLG = 'Y'
    ,UPD_USR_ID = @[upd_usr_id]         
    ,UPD_DT = TO_DATE(@[upd_dt],'yyyymmddhh24miss')
    ,EAI_EVNT_DT = TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')
	,EAI_IF_ID = @[eai_if_id]
WHERE 	LSE_CO_YD_CD = @[lse_co_yd_cd]
AND 	EAI_EVNT_DT <= TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="lse_co_yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
