<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCntrTpDBDAOSearchMdmCntrTpListRSQL">
			<desc><![CDATA[MDM_CNTR_TP 기존데이터 유무확인]]></desc>
			<sql><![CDATA[
SELECT
	COUNT(1) CNTR_TP_CNT
FROM 
	MDM_CNTR_TP
WHERE 
	CNTR_TP_CD = @[cntr_tp_cd]			]]></sql>
			<params>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
