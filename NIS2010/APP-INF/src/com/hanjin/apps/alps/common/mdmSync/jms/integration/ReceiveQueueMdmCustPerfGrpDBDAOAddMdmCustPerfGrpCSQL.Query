            NEW_KEY_ACCT_FLG, RGN_ACCT_FLG,  RHQ_CD,       OFC_TEAM_CD	
			)	                                                                    						
   VALUES  (																						
			@[cust_grp_id], @[cust_grp_nm],  @[ofc_cd],       @[srep_cd],
			@[vbs_clss_cd], @[nbs_clss_cd1], @[nbs_clss_cd2], @[nbs_clss_cd3],
			@[cre_usr_id],  to_date(@[cre_dt],'yyyymmddhh24miss'), @[upd_usr_id], to_date(@[upd_dt],'yyyymmddhh24miss'),			
			@[delt_flg],    to_date(@[eai_evnt_dt],'yyyymmddhh24miss'),
			@[eai_if_id],
			@[new_key_acct_flg], @[rgn_acct_flg], @[rhq_cd], @[ofc_team_cd]	                          			
			  )			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="b" out="N"/>
				<param name="cust_grp_nm" type="12" value="b" out="N"/>
				<param name="ofc_cd" type="12" value="b" out="N"/>
				<param name="srep_cd" type="12" value="b" out="N"/>
				<param name="vbs_clss_cd" type="12" value="b" out="N"/>
				<param name="nbs_clss_cd1" type="12" value="b" out="N"/>
				<param name="nbs_clss_cd2" type="12" value="b" out="N"/>
				<param name="nbs_clss_cd3" type="12" value="b" out="N"/>
				<param name="cre_usr_id" type="12" value="b" out="N"/>
				<param name="cre_dt" type="12" value="20100706170714" out="N"/>
				<param name="upd_usr_id" type="12" value="b" out="N"/>
				<param name="upd_dt" type="12" value="20100706170714" out="N"/>
				<param name="delt_flg" type="12" value="b" out="N"/>
				<param name="eai_evnt_dt" type="12" value="20100706170714" out="N"/>
				<param name="eai_if_id" type="12" value="b" out="N"/>
