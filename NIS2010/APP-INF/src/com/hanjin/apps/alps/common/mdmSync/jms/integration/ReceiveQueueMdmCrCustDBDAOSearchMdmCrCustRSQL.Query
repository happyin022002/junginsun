<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCrCustDBDAOSearchMdmCrCustRSQL">
			<desc><![CDATA[SearchMdmCrCust]]></desc>
			<sql><![CDATA[
SELECT cust_cnt_cd 
  FROM MDM_CR_CUST	
 WHERE cust_cnt_cd  = @[cust_cnt_cd]
   AND cust_seq 	= @[cust_seq]			]]></sql>
			<params>
				<param name="s_cust_cnt_cd" type="12" value="" out="N"/>
				<param name="s_cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
