<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueGlMonXchRtDBDAOModifyGlMonXchRtRSQL">
			<desc><![CDATA[ModifyGlMonXchRt]]></desc>
			<sql><![CDATA[
UPDATE GL_MON_XCH_RT
   SET usd_locl_xch_rt      = @[usd_locl_xch_rt],
       locl_krw_xch_rt      = @[locl_krw_xch_rt],
       usd_krw_xch_rt       = @[usd_krw_xch_rt],
       upd_dt               = TO_DATE(@[upd_dt],'YYYYMMDDHH24MISS'),
       delt_flg             = @[delt_flg],
       cre_dt               = TO_DATE(@[cre_dt],'YYYYMMDDHH24MISS'),
       eai_evnt_dt          = TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')
 WHERE acct_xch_rt_yrmon    = @[acct_xch_rt_yrmon]
   AND acct_xch_rt_lvl      = @[acct_xch_rt_lvl]
   AND curr_cd              = @[curr_cd]
   AND eai_evnt_dt          <= TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="s_usd_locl_xch_rt" type="12" value="" out="N"/>
				<param name="s_locl_krw_xch_rt" type="12" value="" out="N"/>
				<param name="s_usd_krw_xch_rt" type="12" value="" out="N"/>
				<param name="s_upd_dt" type="12" value="" out="N"/>
				<param name="s_delt_flg" type="12" value="" out="N"/>
				<param name="s_cre_dt" type="12" value="" out="N"/>
				<param name="s_eai_evnt_dt" type="12" value="" out="N"/>
				<param name="s_acct_xch_rt_yrmon" type="12" value="" out="N"/>
				<param name="s_acct_xch_rt_lvl" type="12" value="" out="N"/>
				<param name="s_curr_cd" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
