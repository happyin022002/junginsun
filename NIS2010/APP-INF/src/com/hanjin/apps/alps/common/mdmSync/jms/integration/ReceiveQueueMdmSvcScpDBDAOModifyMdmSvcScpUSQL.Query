<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmSvcScpDBDAOModifyMdmSvcScpUSQL">
			<desc><![CDATA[modify svcscp]]></desc>
			<sql><![CDATA[
UPDATE mdm_svc_scp SET
	svc_scp_nm     = @[svc_scp_nm],
	fmc_file_flg   = @[fmc_file_flg],
	trf_pfx_cd     = @[trf_pfx_cd],
	trf_no         = @[trf_no],
	conf_flg       = @[conf_flg],
	svc_scp_bnd_cd = @[svc_scp_bnd_cd],
	cre_usr_id     = @[cre_usr_id],
	upd_usr_id     = @[upd_usr_id],
	cre_dt         = to_date(@[cre_dt],'yyyymmddhh24miss'),
	upd_dt         = to_date(@[upd_dt],'yyyymmddhh24miss'),
	delt_flg       = @[delt_flg],
	eai_evnt_dt    = to_date(@[eai_evnt_dt],'yyyymmddhh24miss'),
	eai_if_id	   = @[eai_if_id]
	WHERE 	svc_scp_cd = @[svc_scp_cd]			]]></sql>
			<params>
				<param name="svc_scp_nm" type="12" value="" out="N"/>
				<param name="fmc_file_flg" type="12" value="" out="N"/>
				<param name="trf_pfx_cd" type="12" value="" out="N"/>
				<param name="trf_no" type="12" value="" out="N"/>
				<param name="conf_flg" type="12" value="" out="N"/>
				<param name="svc_scp_bnd_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
