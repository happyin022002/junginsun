<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCustAddrDBDAOModifyMdmCustAddrUSQL">
			<desc><![CDATA[ModifyMdmCustAddr

2010-07-09 : [CHM-201004308]EAI_IF_ID 추가]]></desc>
			<sql><![CDATA[
UPDATE mdm_cust_addr                              				
			    SET prmry_chk_flg = @[prmry_chk_flg],                         					
			        bzet_addr     = @[bzet_addr],                         					
			        cty_nm        = @[cty_nm],                         					
			        ste_cd        = @[ste_cd],											
					zip_cd        = @[zip_cd],                         						
			        cntc_eml      = @[cntc_eml],                         					
			        cntc_pson_nm  = @[cntc_pson_nm],                      
			        bzet_rmk      = @[bzet_rmk],											
					cre_usr_id    = @[cre_usr_id],                         						
			        cre_dt        = to_date(@[cre_dt],'yyyymmddhh24miss'),      
					upd_usr_id    = @[upd_usr_id],                         						
			        upd_dt        = to_date(@[upd_dt],'yyyymmddhh24miss'),      
					delt_flg      = @[delt_flg],               
			        eai_evnt_dt   = to_date(@[eai_evnt_dt],'yyyymmddhh24miss'),      
			        crm_row_id    = @[crm_row_id],  
					cnt_cd		  = @[cnt_cd],
					eai_if_id 	  = @[eai_if_id],
                    addr_tp_cd    = @[addr_tp_cd],
                    addr_seq      = @[addr_seq]     							 			
			  WHERE cust_cnt_cd   = @[cust_cnt_cd]          	         							
			    AND cust_seq      = @[cust_seq]            	       							
                AND CRM_ROW_ID    = @[crm_row_id]			]]></sql>
			<params>
				<param name="prmry_chk_flg" type="12" value="" out="N"/>
				<param name="bzet_addr" type="12" value="" out="N"/>
				<param name="cty_nm" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="zip_cd" type="12" value="" out="N"/>
				<param name="cntc_eml" type="12" value="" out="N"/>
				<param name="cntc_pson_nm" type="12" value="" out="N"/>
				<param name="bzet_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="crm_row_id" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="addr_tp_cd" type="12" value="" out="N"/>
				<param name="addr_seq" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
