			<desc><![CDATA[ModifyMdmCustPerfGrp]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUST_PERF_GRP SET                        											 
	 	CUST_GRP_NM   = @[cust_grp_nm],
	 	OFC_CD        = @[ofc_cd],  			 
	 	SREP_CD       = @[srep_cd],
	 	VBS_CLSS_CD	  = @[vbs_clss_cd],
	 	NBS_CLSS_CD1  = @[nbs_clss_cd1],  			 
	 	NBS_CLSS_CD2  = @[nbs_clss_cd2],
	 	NBS_CLSS_CD3  = @[nbs_clss_cd3],
	 	CRE_USR_ID    = @[cre_usr_id],  			 
	 	CRE_DT        = to_date(@[cre_dt],'yyyymmddhh24miss'),
	 	UPD_USR_ID    = @[upd_usr_id],			 
	 	UPD_DT        = to_date(@[upd_dt],'yyyymmddhh24miss'),
	 	DELT_FLG      = @[delt_flg],			 
	 	EAI_EVNT_DT   = to_date(@[eai_evnt_dt],'yyyymmddhh24miss'),
	 	EAI_IF_ID     = @[eai_if_id],	
	 	NEW_KEY_ACCT_FLG = @[new_key_acct_flg],	
	 	RGN_ACCT_FLG     = @[rgn_acct_flg],	  
	 	RHQ_CD           = @[rhq_cd],	      
	 	OFC_TEAM_CD      = @[ofc_team_cd]																		 
WHERE CUST_GRP_ID  = @[cust_grp_id]                          												 
AND   EAI_EVNT_DT <= to_date(@[eai_evnt_dt],'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="cust_grp_nm" type="12" value="b" out="N"/>
				<param name="ofc_cd" type="12" value="y" out="N"/>
				<param name="srep_cd" type="12" value="y" out="N"/>
				<param name="vbs_clss_cd" type="12" value="y" out="N"/>
				<param name="nbs_clss_cd1" type="12" value="y" out="N"/>
				<param name="nbs_clss_cd2" type="12" value="y" out="N"/>
				<param name="nbs_clss_cd3" type="12" value="y" out="N"/>
				<param name="cre_usr_id" type="12" value="y" out="N"/>
				<param name="cre_dt" type="12" value="20100706170714" out="N"/>
				<param name="upd_usr_id" type="12" value="y" out="N"/>
				<param name="upd_dt" type="12" value="20100706170714" out="N"/>
				<param name="delt_flg" type="12" value="y" out="N"/>
				<param name="eai_evnt_dt" type="12" value="20100706170714" out="N"/>
				<param name="eai_if_id" type="12" value="y" out="N"/>
				<param name="new_key_acct_flg" type="12" value="" out="N"/>
				<param name="rgn_acct_flg" type="12" value="" out="N"/>
				<param name="rhq_cd" type="12" value="" out="N"/>
				<param name="ofc_team_cd" type="12" value="" out="N"/>
				<param name="cust_grp_id" type="12" value="yz" out="N"/>
			</params>
		</query>
