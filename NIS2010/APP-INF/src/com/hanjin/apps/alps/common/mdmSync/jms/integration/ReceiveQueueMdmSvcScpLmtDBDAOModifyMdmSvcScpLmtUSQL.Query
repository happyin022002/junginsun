<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmSvcScpLmtDBDAOModifyMdmSvcScpLmtUSQL">
			<desc><![CDATA[mdm_svc_scp_lmt 테이블 수정]]></desc>
			<sql><![CDATA[
UPDATE mdm_svc_scp_lmt SET
	cre_usr_id	= @[cre_usr_id],
	upd_usr_id	= @[upd_usr_id],
	cre_dt		= to_date(@[cre_dt],'yyyymmddhh24miss'),
	upd_dt		= to_date(@[upd_dt],'yyyymmddhh24miss'),
	delt_flg	= @[delt_flg],
	eai_evnt_dt	= to_date(@[eai_evnt_dt],'yyyymmddhh24miss'),
	svc_scp_ind_flg	= @[svc_scp_ind_flg],
    rgn_cd 		= @[rgn_cd],
	org_dest_cd = @[org_dest_cd],
	eai_if_id = @[eai_if_id]
WHERE 	svc_scp_cd 	= @[svc_scp_cd]
AND		rgn_cd = @[rgn_cd]
AND		org_dest_cd = @[org_dest_cd]
AND		eai_evnt_dt	<= to_date(@[eai_evnt_dt],'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="Y"/>
				<param name="upd_usr_id" type="12" value="" out="Y"/>
				<param name="cre_dt" type="12" value="" out="Y"/>
				<param name="upd_dt" type="12" value="" out="Y"/>
				<param name="delt_flg" type="12" value="" out="Y"/>
				<param name="eai_evnt_dt" type="12" value="" out="Y"/>
				<param name="svc_scp_ind_flg" type="12" value="" out="N"/>
				<param name="rgn_cd" type="12" value="" out="N"/>
				<param name="org_dest_cd" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="svc_scp_cd" type="12" value="" out="Y"/>
			</params>
		</query>
	</querys>
</sqls>
