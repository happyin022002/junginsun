<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmContinentDBDAORemoveMdmContinentUSQL">
			<desc><![CDATA[RemoveMdmContinent]]></desc>
			<sql><![CDATA[
UPDATE mdm_continent
	SET delt_flg = 'Y',
 		upd_usr_id 	= @[upd_usr_id], upd_dt = to_date(@[upd_dt],'yyyymmddhh24miss'),
 		eai_evnt_dt = SYSDATE,
		eai_if_id   = @[eai_if_id]
 WHERE 	conti_cd = @[conti_cd]
 AND 	eai_evnt_dt <= SYSDATE			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="conti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
