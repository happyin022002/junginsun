<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCustomerDBDAOModifyMdmCustomerRqstUSQL">
			<desc><![CDATA[Customer request update]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUSTOMER_RQST MCR
SET MCR.DELT_FLG = DECODE(@[delt_flg],NULL,'A','N','A','R')
    ,cust_cnt_cd = @[cust_cnt_cd]                          								
    ,cust_seq = to_number(@[cust_seq])
    ,UPD_DT = sysdate
WHERE MCR.MDM_CUSTOMER_RQST_SEQ = @[crm_row_id]			]]></sql>
			<params>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="crm_row_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
