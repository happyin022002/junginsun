<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmSlsRepDBDAOModifyMdmSlsRepUSQL">
			<desc><![CDATA[JMS에서 받은 데이터 DB Logic 처리를 담당한다.(수정)]]></desc>
			<sql><![CDATA[
UPDATE mdm_sls_rep
   SET ofc_cd       = @[ofc_cd]      ,
       srep_nm      = @[srep_nm]     ,
       ofc_team_cd  = SUBSTR(@[ofc_team_cd], 1, 3),
       sex_cd       = @[sex_cd]      ,
       srep_abbr_nm = @[srep_abbr_nm],
       ib_srep_flg  = @[ib_srep_flg] ,
       ob_srep_flg  = @[ob_srep_flg] ,
       empe_cd      = @[empe_cd]     ,
       srep_eml     = @[srep_eml]    ,
       intl_mphn_no = @[intl_mphn_no],
       mphn_no      = @[mphn_no]     ,
       cre_usr_id   = @[cre_usr_id]  ,
       cre_dt       = TO_DATE(@[cre_dt], 'yyyymmddhh24miss'),
       upd_usr_id   = @[upd_usr_id]  ,
       upd_dt       = TO_DATE(@[upd_dt], 'yyyymmddhh24miss'),
       srep_sts_cd  = @[srep_sts_cd] ,
       eai_evnt_dt  = TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss'),
       eai_if_id    = @[eai_if_id]
 WHERE srep_cd      = @[srep_cd]
   AND eai_evnt_dt <= TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="srep_nm" type="12" value="" out="N"/>
				<param name="ofc_team_cd" type="12" value="" out="N"/>
				<param name="sex_cd" type="12" value="" out="N"/>
				<param name="srep_abbr_nm" type="12" value="" out="N"/>
				<param name="ib_srep_flg" type="12" value="" out="N"/>
				<param name="ob_srep_flg" type="12" value="" out="N"/>
				<param name="empe_cd" type="12" value="" out="N"/>
				<param name="srep_eml" type="12" value="" out="N"/>
				<param name="intl_mphn_no" type="12" value="" out="N"/>
				<param name="mphn_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="srep_sts_cd" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
