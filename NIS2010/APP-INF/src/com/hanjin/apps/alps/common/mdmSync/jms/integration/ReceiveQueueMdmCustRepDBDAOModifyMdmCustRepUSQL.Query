<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCustRepDBDAOModifyMdmCustRepUSQL">
			<desc><![CDATA[ModifyMdmCustRep]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUST_REP 
   SET  auto_inv_flg			= @[auto_inv_flg],
		hjs_cust_svc_pic_tp_cd	= @[hjs_cust_svc_pic_tp_cd],
		hjs_ref_no				= @[hjs_ref_no],
		cust_ref_no_flg			= @[cust_ref_no_flg],
		locl_chg_flg			= @[locl_chg_flg],
		hjs_ref_eml				= @[hjs_ref_eml],
		delt_flg				= @[delt_flg],
		eai_evnt_dt             = TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS'),
		eai_if_id				= @[eai_if_id],
		cre_usr_id				= @[cre_usr_id],
		cre_dt                  = TO_DATE(@[cre_dt],'YYYYMMDDHH24MISS'),
		upd_usr_id				= @[upd_usr_id],
		upd_dt                  = TO_DATE(@[upd_dt],'YYYYMMDDHH24MISS'),
		auto_inv_eml			= @[auto_inv_eml]
 WHERE  cust_cnt_cd              = @[cust_cnt_cd]
   AND  cust_seq 	            = @[cust_seq]
   AND  ofc_cd					= @[ofc_cd]
   AND  io_bnd_cd				= @[io_bnd_cd]
   AND  eai_evnt_dt              <= TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="auto_inv_flg" type="12" value="" out="N"/>
				<param name="hjs_cust_svc_pic_tp_cd" type="12" value="" out="N"/>
				<param name="hjs_ref_no" type="12" value="" out="N"/>
				<param name="cust_ref_no_flg" type="12" value="" out="N"/>
				<param name="locl_chg_flg" type="12" value="" out="N"/>
				<param name="hjs_ref_eml" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="auto_inv_eml" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="io_bnd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
