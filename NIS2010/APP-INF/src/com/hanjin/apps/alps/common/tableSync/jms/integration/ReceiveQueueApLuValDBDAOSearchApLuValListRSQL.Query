<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueApLuValDBDAOSearchApLuValListRSQL">
			<desc><![CDATA[ap_lu_val select]]></desc>
			<sql><![CDATA[
SELECT lu_tp_ind_cd 												
  FROM ap_lu_val												
 WHERE lu_tp_ind_cd = HJSEAI_PKG.h_decode(@[lu_tp_ind_cd],'UTF8','UTF8')
   AND lu_cd 	   = HJSEAI_PKG.h_decode(@[lu_cd],'UTF8','UTF8') 
			]]></sql>
			<params>
				<param name="lu_tp_ind_cd" type="12" value="" out="N"/>
				<param name="lu_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
