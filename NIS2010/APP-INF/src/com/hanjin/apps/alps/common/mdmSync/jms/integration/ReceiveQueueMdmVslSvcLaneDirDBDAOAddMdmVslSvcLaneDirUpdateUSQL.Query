<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmVslSvcLaneDirDBDAOAddMdmVslSvcLaneDirUpdateUSQL">
			<desc><![CDATA[AddMdmVslSvcLaneDirUpdate]]></desc>
			<sql><![CDATA[
UPDATE mdm_vsl_svc_lane_dir
SET 
  vsl_slan_dir_cd   = @[vsl_slan_dir_cd],
  vsl_slan_dir_seq  = @[vsl_slan_dir_seq],
  cre_usr_id        = @[cre_usr_id],
  cre_dt            = to_date(@[cre_dt], 'yyyymmddhh24miss'),
  upd_usr_id        = @[upd_usr_id],
  upd_dt            = to_date(@[upd_dt], 'yyyymmddhh24miss'),
  delt_flg          = @[delt_flg],
  eai_evnt_dt       = to_date(@[eai_evnt_dt], 'yyyymmddhh24miss'),
  eai_if_id			= @[eai_if_id]
WHERE vsl_slan_cd = @[vsl_slan_cd]
  AND vsl_slan_dir_cd = @[vsl_slan_dir_cd]  
  AND eai_evnt_dt <= to_date(@[eai_evnt_dt], 'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="vsl_slan_dir_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_seq" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
