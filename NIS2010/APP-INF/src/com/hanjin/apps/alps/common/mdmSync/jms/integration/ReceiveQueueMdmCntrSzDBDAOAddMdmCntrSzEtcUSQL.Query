<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCntrSzDBDAOAddMdmCntrSzEtcUSQL">
			<desc><![CDATA[MDM_CNTR_SZ 수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_CNTR_SZ SET 
	CNTR_SZ_DESC = @[cntr_sz_desc]
,	CNTR_SZ_TEU_CAPA = @[cntr_sz_teu_capa]
,	MODI_CNTR_SZ_CD = @[modi_cntr_sz_cd]
,	CRE_USR_ID = @[cre_usr_id]
,	CRE_DT = TO_DATE(@[cre_dt],'YYYYMMDDHH24MISS')
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = TO_DATE(@[upd_dt],'YYYYMMDDHH24MISS')
,	DELT_FLG = @[delt_flg]
,	EAI_EVNT_DT = TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')
,	EAI_IF_ID = @[eai_if_id]
WHERE	
	CNTR_SZ_CD = @[cntr_sz_cd]
AND	EAI_EVNT_DT <= TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="cntr_sz_desc" type="12" value="" out="N"/>
				<param name="cntr_sz_teu_capa" type="6" value="" out="N"/>
				<param name="modi_cntr_sz_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="cntr_sz_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
