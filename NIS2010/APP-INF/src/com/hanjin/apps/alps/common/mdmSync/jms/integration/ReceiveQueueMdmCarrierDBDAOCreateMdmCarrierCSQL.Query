<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCarrierDBDAOCreateMdmCarrierCSQL">
			<desc><![CDATA[CreateMdmCarrier]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CARRIER(
    CRR_CD,
    CRR_NM,
    BKG_EDI_CD,
    MAPG_CRR_CD,
    DELT_FLG,
    CRE_USR_ID,
    CRE_DT,
	UPD_USR_ID,
	UPD_DT,
    EAI_EVNT_DT,
    EAI_IF_ID)
VALUES(
    @[crr_cd],
    @[crr_nm],
    @[bkg_edi_cd],
    @[mapg_crr_cd],
    NVL(@[delt_flg],'N'),
    @[cre_usr_id],
    SYSDATE,
	@[upd_usr_id],
    SYSDATE,
    SYSDATE,
    @[eai_if_id])			]]></sql>
			<params>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="crr_nm" type="12" value="" out="N"/>
				<param name="bkg_edi_cd" type="12" value="" out="N"/>
				<param name="mapg_crr_cd" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
