<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmVendorCustomerGeneralDBDAOSearchMdmVendorRSQL">
			<desc><![CDATA[mdm_vendor 에 해당 vndr_seq 가 존재하는 지 확인]]></desc>
			<sql><![CDATA[
select vndr_seq
from
mdm_vendor
where
vndr_seq = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
