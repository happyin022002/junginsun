<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCntrTpDBDAOAddMdmCntrTpCSQL">
			<desc><![CDATA[MDM_CNTR_TP 입력]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CNTR_TP (
	UPD_USR_ID
,	CNTR_TP_CD
,	CNTR_TP_DESC
,	MODI_CNTR_TP_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_DT
,	DELT_FLG
,	EAI_EVNT_DT
,	EAI_IF_ID
) VALUES( 
	@[upd_usr_id]
,	@[cntr_tp_cd]
,	@[cntr_tp_desc]
,	@[modi_cntr_tp_cd]
,	@[cre_usr_id]
,	TO_DATE(@[cre_dt],'YYYYMMDDHH24MISS')
,	TO_DATE(@[upd_dt],'YYYYMMDDHH24MISS')
,	@[delt_flg]
,	TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')
,	@[eai_if_id]
)			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cntr_tp_desc" type="12" value="" out="N"/>
				<param name="modi_cntr_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
