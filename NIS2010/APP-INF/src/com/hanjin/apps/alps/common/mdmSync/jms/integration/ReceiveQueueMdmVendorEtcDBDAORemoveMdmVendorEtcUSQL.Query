<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmVendorEtcDBDAORemoveMdmVendorEtcUSQL">
			<desc><![CDATA[RemoveMdmVendorEtc]]></desc>
			<sql><![CDATA[
update MDM_VENDOR
set lu_delt_flg = @[lu_delt_flg] ,
  upd_usr_id = @[upd_usr_id] ,
  upd_dt = to_date(@[upd_dt], 'yyyymmddhh24miss') ,
  eai_evnt_dt = to_date(@[eai_evnt_dt], 'yyyymmddhh24miss'),
  eai_if_id = @[eai_if_id]
where vndr_seq = @[vndr_seq]			]]></sql>
			<params>
				<param name="lu_delt_flg" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
