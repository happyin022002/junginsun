<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmActivityDBDAOSearchMdmActivityListRSQL">
			<desc><![CDATA[SearchMdmActivityList]]></desc>
			<sql><![CDATA[
SELECT act_cd 
FROM mdm_activity
where 1=1
#if(${act_cd} != '')
and act_cd = @[act_cd]
#end			]]></sql>
			<params>
				<param name="act_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
