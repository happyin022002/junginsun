<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmGrpCmdtDBDAOAddMdmGrpCmdtCSQL">
			<desc><![CDATA[JMS에서 받은 데이터 DB Logic 처리를 담당한다.(추가)]]></desc>
			<sql><![CDATA[
INSERT INTO mdm_grp_cmdt (
    grp_cmdt_cd    ,
    grp_cmdt_eng_nm,
    grp_cmdt_krn_nm,
    grp_cmdt_jpn_nm,
    grp_cmdt_chn_nm,
    min_rep_cmdt_cd,
    max_rep_cmdt_cd,
    cre_usr_id     ,
    cre_dt         ,
    upd_usr_id     ,
    upd_dt         ,
    delt_flg       ,
    eai_evnt_dt    ,
    eai_if_id
) VALUES (
    @[grp_cmdt_cd]    ,
    @[grp_cmdt_eng_nm],
    @[grp_cmdt_krn_nm],
    @[grp_cmdt_jpn_nm],
    @[grp_cmdt_chn_nm],
    @[min_rep_cmdt_cd],
    @[max_rep_cmdt_cd],
    @[cre_usr_id]     ,
    TO_DATE(@[cre_dt], 'yyyymmddhh24miss'),
    @[upd_usr_id]     ,
    TO_DATE(@[upd_dt], 'yyyymmddhh24miss'),
    @[delt_flg]       ,
    TO_DATE(@[eai_evnt_dt], 'yyyymmddhh24miss'),
    @[eai_if_id]
)			]]></sql>
			<params>
				<param name="grp_cmdt_cd" type="12" value="" out="N"/>
				<param name="grp_cmdt_eng_nm" type="12" value="" out="N"/>
				<param name="grp_cmdt_krn_nm" type="12" value="" out="N"/>
				<param name="grp_cmdt_jpn_nm" type="12" value="" out="N"/>
				<param name="grp_cmdt_chn_nm" type="12" value="" out="N"/>
				<param name="min_rep_cmdt_cd" type="12" value="" out="N"/>
				<param name="max_rep_cmdt_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
