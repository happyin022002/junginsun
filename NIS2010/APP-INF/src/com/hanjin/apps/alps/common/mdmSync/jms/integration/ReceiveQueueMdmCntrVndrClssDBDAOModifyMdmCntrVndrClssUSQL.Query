<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCntrVndrClssDBDAOModifyMdmCntrVndrClssUSQL">
			<desc><![CDATA[MDM_CNTR_VNDR_CLSS 의 정보를 update 한다.
(실질적으로 cre_usr_id 등 4개 정보 만 update 하는데 어떤 의미가 있는지는 미상)]]></desc>
			<sql><![CDATA[
UPDATE	MDM_CNTR_VNDR_CLSS SET 
		cre_usr_id  = @[cre_usr_id],											
		cre_dt      = TO_DATE(@[cre_dt],'yyyymmddhh24miss'),				
		upd_usr_id  = @[upd_usr_id],              							
		upd_dt      = TO_DATE(@[upd_dt],'yyyymmddhh24miss'),				
		delt_flg    = NVL(@[delt_flg],'N'),              							
		eai_evnt_dt = SYSDATE,
		eai_if_id	= @[eai_if_id]              
WHERE	vndr_seq = @[vndr_seq]                           					
AND		vndr_cost_cd = @[vndr_cost_cd]                       					
AND		cntr_vndr_svc_cd = @[cntr_vndr_svc_cd]			]]></sql>
			<params>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vndr_cost_cd" type="12" value="" out="N"/>
				<param name="cntr_vndr_svc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
