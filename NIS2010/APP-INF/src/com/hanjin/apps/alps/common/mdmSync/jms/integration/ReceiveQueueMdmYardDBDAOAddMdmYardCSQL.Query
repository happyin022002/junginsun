<sqls>
	<querys>
		<query name="ReceiveQueueMdmYardDBDAOAddMdmYardCSQL">
			<desc><![CDATA[AddMdmYard
2010.10.11 이상용 [CHM-201006405-01] MDM_YARD 데이터 생성시  RF_AVG_DWLL_HRS, RF_MIN_DWLL_HRS, DRY_AVG_DWLL_HRS, DRY_MIN_DWLL_HRS 컬럼의  Default 값 설정(6,3,6,3)]]></desc>
			<sql><![CDATA[
INSERT INTO mdm_yard (
				rf_yd_ft_hrs		   ,
				yd_cd                  ,yd_nm                  ,loc_cd                 ,	
			 	n1st_vndr_cnt_cd       ,n1st_vndr_seq          ,n2nd_vndr_cnt_cd       ,	
			 	n2nd_vndr_seq          ,n3rd_vndr_cnt_cd       ,n3rd_vndr_seq          ,	
			 	ofc_cd                 ,													
			 	yd_chr_cd              ,yd_fcty_tp_mrn_tml_flg ,yd_fcty_tp_cy_flg      ,	
			 	yd_fcty_tp_cfs_flg     ,yd_fcty_tp_rail_rmp_flg,yd_oshp_cd             ,	
			 	bd_yd_flg              ,onf_hir_yd_flg         ,	
			 	rep_zn_cd              ,yd_addr                ,zip_cd                 ,	
			 	intl_phn_no            ,phn_no                 ,fax_no                 ,	
			 	yd_pic_nm              ,yd_ceo_nm              ,gate_opn_hrmnt         ,	
			 	gate_clz_hrmnt         ,hol_gate_opn_hrmnt     ,hol_gate_clz_hrmnt     ,	
