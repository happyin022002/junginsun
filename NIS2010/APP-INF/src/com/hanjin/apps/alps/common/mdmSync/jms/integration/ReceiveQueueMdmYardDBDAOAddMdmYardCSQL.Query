			 	yd_brth_dpth_chnl_knt  ,yd_ttl_spc             ,yd_act_spc             ,	
			 	yd_hjs_spc             ,yd_hndl_capa           ,yd_rf_rcpt_440v_knt    ,	
			 	yd_rf_rcpt_220v_knt    ,yd_rf_rcpt_dul_knt     ,yd_op_sys_cd           ,	
			 	yd_inrl_flg            ,yd_cfs_spc             ,mnr_shop_flg           ,	
			 	yd_hndl_yr             ,yd_ttl_vol_teu_knt     ,yd_ttl_vol_bx_knt      ,	
			 	yd_hjs_vol_teu_knt     ,yd_hjs_vol_bx_knt      ,yd_pst_pgc_knt         ,	
			 	yd_pgc_knt             ,trstr_knt              ,frk_knt                ,	
			 	yd_strdl_crr_knt       ,yd_trct_knt            ,yd_top_lft_knt         ,	
