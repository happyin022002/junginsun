<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCustAddrDBDAOSearchMdmCustAddrListRSQL">
			<desc><![CDATA[SearchMdmCustAddrList]]></desc>
			<sql><![CDATA[
SELECT cust_cnt_cd 									
FROM mdm_cust_addr									
WHERE cust_cnt_cd = @[cust_cnt_cd]	  						
AND cust_seq    =   @[cust_seq]   			               
AND CRM_ROW_ID = @[crm_row_id]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="crm_row_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
