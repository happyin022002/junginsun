		<query name="ReceiveQueueMdmLseCoYdDBDAOModifyMdmLseCoYdUSQL">
			<desc><![CDATA[MDM_LSE_CO_YD 수정

<Change History>
1	2009.09.08	Lee ByoungHun	최초작성]]></desc>
			<sql><![CDATA[
/* ReceiveQueueMdmLseCoYdDBDAO - modifyMDM_LSE_CO_YD() */
UPDATE MDM_LSE_CO_YD SET
    LSE_CO_YD_NM	  = @[lse_co_yd_nm]
    ,ONF_HIR_YD_FLG    = @[onf_hir_yd_flg]
    ,YD_ADDR           = @[yd_addr]
    ,INTL_PHN_NO       = @[intl_phn_no]
    ,PHN_NO            = @[phn_no]
    ,FAX_NO            = @[fax_no]
    ,YD_PIC_NM         = @[yd_pic_nm]
    ,YD_EML            = @[yd_eml]
    ,LSE_CO_VNDR_SEQ1  = @[lse_co_vndr_seq1]
    ,LSE_CO_VNDR_SEQ2  = @[lse_co_vndr_seq2]
    ,LSE_CO_VNDR_SEQ3  = @[lse_co_vndr_seq3]
    ,LSE_CO_VNDR_SEQ4  = @[lse_co_vndr_seq4]
    ,LSE_CO_VNDR_SEQ5  = @[lse_co_vndr_seq5]
    ,LSE_CO_VNDR_SEQ6  = @[lse_co_vndr_seq6]
    ,LSE_CO_VNDR_SEQ7  = @[lse_co_vndr_seq7]
    ,LSE_CO_VNDR_SEQ8  = @[lse_co_vndr_seq8]
    ,LSE_CO_VNDR_SEQ9  = @[lse_co_vndr_seq9]
    ,LSE_CO_VNDR_SEQ10 = @[lse_co_vndr_seq10]
    ,CRE_USR_ID        = @[cre_usr_id]
    ,CRE_DT            = to_date(@[cre_dt],'yyyymmddhh24miss')
    ,UPD_USR_ID        = @[upd_usr_id]
    ,UPD_DT            = to_date(@[upd_dt],'yyyymmddhh24miss')
    ,DELT_FLG          = @[delt_flg]
    ,EAI_EVNT_DT	   = TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')
    ,LSE_CO_VNDR_SEQ11 = @[lse_co_vndr_seq11]
    ,LSE_CO_VNDR_SEQ12 = @[lse_co_vndr_seq12]
    ,LSE_CO_VNDR_SEQ13 = @[lse_co_vndr_seq13]
    ,LSE_CO_VNDR_SEQ14 = @[lse_co_vndr_seq14]
    ,LSE_CO_VNDR_SEQ15 = @[lse_co_vndr_seq15]
    ,LSE_CO_VNDR_SEQ16 = @[lse_co_vndr_seq16]
    ,LSE_CO_VNDR_SEQ17 = @[lse_co_vndr_seq17]
    ,LSE_CO_VNDR_SEQ18 = @[lse_co_vndr_seq18]
    ,LSE_CO_VNDR_SEQ19 = @[lse_co_vndr_seq19]
    ,LSE_CO_VNDR_SEQ20 = @[lse_co_vndr_seq20]
	,EAI_IF_ID = @[eai_if_id]
WHERE 	LSE_CO_YD_CD = @[lse_co_yd_cd]
AND 	EAI_EVNT_DT  <= TO_DATE(@[eai_evnt_dt],'YYYYMMDDHH24MISS')			]]></sql>
			<params>
				<param name="lse_co_yd_nm" type="12" value="" out="N"/>
