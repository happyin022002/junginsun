				<param name="yd_eml" type="12" value="" out="N"/>
				<param name="lse_co_vndr_seq1" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq2" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq3" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq4" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq5" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq6" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq7" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq8" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq9" type="2" value="" out="N"/>
				<param name="lse_co_vndr_seq10" type="2" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
