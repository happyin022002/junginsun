<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmRejectCustDBDAORejectCustUSQL">
			<desc><![CDATA[Reject customer]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUSTOMER_RQST MCR
SET MCR.DELT_FLG = 'R'
	,MCR.EAI_IF_ID = @[eai_if_id]
    ,MCR.UPD_USR_ID = @[usr_id]
    ,UPD_DT = sysdate
WHERE MCR.CRM_ROW_ID = @[cust_rqst_id]			]]></sql>
			<params>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="cust_rqst_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
