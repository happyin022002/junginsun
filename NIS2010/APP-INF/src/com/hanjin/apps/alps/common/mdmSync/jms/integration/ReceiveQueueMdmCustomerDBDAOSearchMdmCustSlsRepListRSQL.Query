<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmCustomerDBDAOSearchMdmCustSlsRepListRSQL">
			<desc><![CDATA[Customer sales rep check]]></desc>
			<sql><![CDATA[
SELECT cust_cnt_cd 	   	               							
  FROM bkg_cust_sls_rep												
 WHERE cust_cnt_cd = @[cust_cnt_cd]     	               						
   AND cust_seq    = @[cust_seq]
   AND srep_cd     = @[srep_cd]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="srep_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
