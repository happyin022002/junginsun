<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmOrganizationDBDAOSearchMdmOrganizationRSQL">
			<desc><![CDATA[for selection MDM_ORGANIZATION]]></desc>
			<sql><![CDATA[
SELECT ofc_cd FROM mdm_organization
where 1=1
#if(${ofc_cd} != '')
and ofc_cd = @[ofc_cd]
#end			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
