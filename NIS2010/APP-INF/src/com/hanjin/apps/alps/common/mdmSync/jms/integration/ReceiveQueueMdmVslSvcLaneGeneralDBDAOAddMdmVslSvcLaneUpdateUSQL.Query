<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReceiveQueueMdmVslSvcLaneGeneralDBDAOAddMdmVslSvcLaneUpdateUSQL">
			<desc><![CDATA[AddMdmVslSvcLaneUpdate]]></desc>
			<sql><![CDATA[
UPDATE mdm_vsl_svc_lane
SET 
  vsl_slan_nm           = @[vsl_slan_nm],
  vsl_svc_tp_cd         = @[vsl_svc_tp_cd],
  st_eff_dt             = SUBSTR(@[st_eff_dt], 1, 8),
  end_eff_dt            = SUBSTR(@[end_eff_dt], 1, 8),
  vsl_slan_skd_tp_cd    = @[vsl_slan_skd_tp_cd],
  ofc_cd                = @[ofc_cd],
  co_cd                 = @[co_cd],
  vsl_tp_cd             = @[vsl_tp_cd],
  fdr_div_cd            = @[fdr_div_cd],
  cre_usr_id            = @[cre_usr_id],
  cre_dt                = to_date(@[cre_dt], 'yyyymmddhh24miss'),
  upd_usr_id            = @[upd_usr_id],
  upd_dt                = to_date(@[upd_dt], 'yyyymmddhh24miss'),
  delt_flg              = @[delt_flg],
  eai_evnt_dt           = to_date(@[eai_evnt_dt], 'yyyymmddhh24miss'),
  eai_if_id				= @[eai_if_id]
WHERE vsl_slan_cd = @[vsl_slan_cd]
  AND eai_evnt_dt <= to_date(@[eai_evnt_dt], 'yyyymmddhh24miss')			]]></sql>
			<params>
				<param name="vsl_slan_nm" type="12" value="" out="N"/>
				<param name="vsl_svc_tp_cd" type="12" value="" out="N"/>
				<param name="st_eff_dt" type="12" value="" out="N"/>
				<param name="end_eff_dt" type="12" value="" out="N"/>
				<param name="vsl_slan_skd_tp_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
				<param name="vsl_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_div_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="upd_dt" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
