<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOSearchMaxClaimNoRSQL">
			<desc><![CDATA[Claim No 생성시 최대값 가져오기]]></desc>
			<sql><![CDATA[
SELECT 
      'CC' || TO_CHAR(SYSDATE,'YYYY') || LTRIM(TO_CHAR(TO_NUMBER((NVL(SUBSTR(MAX(CGO_CLM_NO),7,4),0))) + 1,'0009')) 
      AS CGO_CLM_NO
FROM  CNI_CGO_CLM
WHERE CGO_CLM_NO LIKE 'CC' || TO_CHAR(SYSDATE,'YYYY') || '%'			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
