<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMConsultationSlipDBDAOSearchLocalDateRSQL">
			<desc><![CDATA[OFFICE 별 LOCAL DATE를 구한다.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[usr_ofc_cd]), 'YYYYMMDD') LCL_DT
FROM DUAL			]]></sql>
			<params>
				<param name="usr_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
