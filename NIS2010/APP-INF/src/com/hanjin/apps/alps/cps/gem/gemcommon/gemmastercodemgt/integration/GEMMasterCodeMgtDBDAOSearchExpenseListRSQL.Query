<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOSearchExpenseListRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT   GEN_EXPN_CD
        ,ENG_ABBR_NM
		,KRN_ABBR_NM
FROM     GEM_EXPENSE
WHERE    DELT_FLG = 'N'
AND      GEN_EXPN_CD IN (SELECT L_4 CODE
                         FROM   GEM_EXPN_LEVEL_V)
ORDER BY 1			]]></sql>
			<params>
			</params>
		</query>
	</querys>
</sqls>
