<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchToOfficeL2ListRSQL">
			<desc><![CDATA[From에 선택된 OFFICE별 TO OFFICE의 레벨2(L2)의 리스트를 취득한다.]]></desc>
			<sql><![CDATA[
SELECT DISTINCT L_3 CODE
  FROM GEM_OFC_LEVEL_V
 WHERE 1 = 1
   AND RGN_OFC_FLG = @[to_sls_ofc_div_cd]
   AND L_2 =  @[to_ofc_lvl1]
   AND L_4 IN ( 
               SELECT OFC_CD
                 FROM GEM_OFFICE
                WHERE RQST_AUTH_FLG = 'Y'				
#if (${ofc_co_div_cd} == 'S') 
                  AND PRNT_OFC_CD   = @[fm_ofc_lvl2]
                  AND OFC_CO_DIV_CD =  @[ofc_co_div_cd]
#end
                  AND DELT_FLG = 'N'
              )
ORDER BY CODE			]]></sql>
			<params>
				<param name="to_sls_ofc_div_cd" type="12" value="" out="N"/>
				<param name="to_ofc_lvl1" type="12" value="" out="N"/>
				<param name="fm_ofc_lvl2" type="12" value="" out="N"/>
				<param name="ofc_co_div_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
