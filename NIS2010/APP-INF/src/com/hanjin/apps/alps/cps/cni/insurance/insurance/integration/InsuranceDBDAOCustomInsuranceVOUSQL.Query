		<query name="InsuranceDBDAOCustomInsuranceVOUSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
UPDATE CNI_INSUR_CTRT SET 
	INSUR_CTNT = @[insur_ctnt]
,	RINS_CLM_PTY_NO = @[rins_clm_pty_no]
,	RINS_CTNT = @[rins_ctnt]
,	INS_CLM_PTY_NO = @[ins_clm_pty_no]
,	INS_CTNT = @[ins_ctnt]
,	CINS_CLM_PTY_NO = @[cins_clm_pty_no]
,	CINS_CTNT = @[cins_ctnt]
,	BRO_CLM_PTY_NO = @[bro_clm_pty_no]
,	INT_DESC = @[int_desc]
,	INSUR_CTRT_EFF_DT = REPLACE(@[insur_ctrt_eff_dt],'-')
,	INSUR_CTRT_EXP_DT = REPLACE(@[insur_ctrt_exp_dt],'-')
,	SUBJ_MAT_INS_DESC = @[subj_mat_ins_desc]
,	INS_CURR_CD = @[ins_curr_cd]
,	INS_LOCL_AMT = REPLACE(@[ins_locl_amt],',')
,	INS_XCH_RT = REPLACE(@[ins_xch_rt],',')
,	INS_USD_AMT = REPLACE(@[ins_usd_amt],',')
,	LMT_INS_CURR_CD = @[lmt_ins_curr_cd]
,	LMT_INS_LOCL_AMT = REPLACE(@[lmt_ins_locl_amt],',')
,	LMT_INS_XCH_RT = REPLACE(@[lmt_ins_xch_rt],',')
,	LMT_INS_USD_AMT = REPLACE(@[lmt_ins_usd_amt],',')
,	INSUR_RMK = @[insur_rmk]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = CNI_GET_GMT_FNC(@[upd_usr_id])
WHERE	INSUR_TP_CD = @[insur_tp_cd]
AND	INSUR_PLCY_YR = @[insur_plcy_yr]
AND	INSUR_CLM_PTY_NO = @[insur_clm_pty_no]
			]]></sql>
			<params>
				<param name="insur_ctnt" type="12" value="" out="N"/>
				<param name="rins_clm_pty_no" type="2" value="" out="N"/>
				<param name="rins_ctnt" type="12" value="" out="N"/>
