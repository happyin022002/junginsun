<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOsearchExistTicAuthOfficeRSQL">
			<desc><![CDATA[TIC Auth Office 존재하는지 조회]]></desc>
			<sql><![CDATA[
SELECT MAX(APRO_OFC_CD) AS APRO_OFC_CD
  FROM GEM_APRO_EXPT_OFC
 WHERE APRO_OFC_CD = @[ofc_cd]
   AND DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
