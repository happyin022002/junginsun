<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InsuranceDBDAOCustomInsuranceVOCSQL">
			<desc><![CDATA[   ]]></desc>
			<sql><![CDATA[
INSERT INTO CNI_INSUR_CTRT (
	INSUR_TP_CD
,	INSUR_PLCY_YR
,	INSUR_CLM_PTY_NO
,	INSUR_CTNT
,	RINS_CLM_PTY_NO
,	RINS_CTNT
,	INS_CLM_PTY_NO
,	INS_CTNT
,	CINS_CLM_PTY_NO
,	CINS_CTNT
,	BRO_CLM_PTY_NO
,	INT_DESC
,	INSUR_CTRT_EFF_DT
,	INSUR_CTRT_EXP_DT
,	SUBJ_MAT_INS_DESC
,	INS_CURR_CD
,	INS_LOCL_AMT
,	INS_XCH_RT
,	INS_USD_AMT
,	LMT_INS_CURR_CD
,	LMT_INS_LOCL_AMT
,	LMT_INS_XCH_RT
,	LMT_INS_USD_AMT
,	INSUR_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[insur_tp_cd]
,	@[insur_plcy_yr]
,	@[insur_clm_pty_no]
,	@[insur_ctnt]
,	@[rins_clm_pty_no]
,	@[rins_ctnt]
,	@[ins_clm_pty_no]
,	@[ins_ctnt]
,	@[cins_clm_pty_no]
,	@[cins_ctnt]
,	@[bro_clm_pty_no]
,	@[int_desc]
,	REPLACE(@[insur_ctrt_eff_dt],'-')
,	REPLACE(@[insur_ctrt_exp_dt],'-')
,	@[subj_mat_ins_desc]
,	@[ins_curr_cd]
,	REPLACE(@[ins_locl_amt],',')
,	REPLACE(@[ins_xch_rt],',')
,	REPLACE(@[ins_usd_amt],',')
,	@[lmt_ins_curr_cd]
,	REPLACE(@[lmt_ins_locl_amt],',')
,	REPLACE(@[lmt_ins_xch_rt],',')
,	REPLACE(@[lmt_ins_usd_amt],',')
,	@[insur_rmk]
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
)			]]></sql>
			<params>
				<param name="insur_tp_cd" type="12" value="" out="N"/>
				<param name="insur_plcy_yr" type="12" value="" out="N"/>
				<param name="insur_clm_pty_no" type="2" value="" out="N"/>
				<param name="insur_ctnt" type="12" value="" out="N"/>
				<param name="rins_clm_pty_no" type="2" value="" out="N"/>
				<param name="rins_ctnt" type="12" value="" out="N"/>
				<param name="ins_clm_pty_no" type="2" value="" out="N"/>
				<param name="ins_ctnt" type="12" value="" out="N"/>
				<param name="cins_clm_pty_no" type="2" value="" out="N"/>
				<param name="cins_ctnt" type="12" value="" out="N"/>
				<param name="bro_clm_pty_no" type="2" value="" out="N"/>
				<param name="int_desc" type="12" value="" out="N"/>
				<param name="insur_ctrt_eff_dt" type="12" value="" out="N"/>
				<param name="insur_ctrt_exp_dt" type="12" value="" out="N"/>
				<param name="subj_mat_ins_desc" type="12" value="" out="N"/>
				<param name="ins_curr_cd" type="12" value="" out="N"/>
				<param name="ins_locl_amt" type="12" value="" out="N"/>
				<param name="ins_xch_rt" type="12" value="" out="N"/>
				<param name="ins_usd_amt" type="12" value="" out="N"/>
				<param name="lmt_ins_curr_cd" type="12" value="" out="N"/>
				<param name="lmt_ins_locl_amt" type="12" value="" out="N"/>
				<param name="lmt_ins_xch_rt" type="12" value="" out="N"/>
				<param name="lmt_ins_usd_amt" type="12" value="" out="N"/>
				<param name="insur_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
