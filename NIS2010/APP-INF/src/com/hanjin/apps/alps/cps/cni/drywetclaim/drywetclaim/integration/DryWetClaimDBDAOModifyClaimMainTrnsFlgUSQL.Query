<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOModifyClaimMainTrnsFlgUSQL">
			<desc><![CDATA[Transfer Accept 할 경우 Main 에 Trns_flg 값 변경]]></desc>
			<sql><![CDATA[
UPDATE CNI_DW_CLM SET
       TRNS_FLG            = 'N'
     , UPD_USR_ID          = @[upd_usr_id]
     , UPD_DT              = CNI_GET_GMT_FNC(@[upd_usr_id])
WHERE DW_CLM_NO = @[dw_clm_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="dw_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
