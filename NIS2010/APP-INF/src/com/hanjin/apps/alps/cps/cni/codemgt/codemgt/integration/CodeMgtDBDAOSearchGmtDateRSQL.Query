<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeMgtDBDAOSearchGmtDateRSQL">
			<desc><![CDATA[로그인 사용자의 Gmt 일을 취득 YYYY-MM-DD]]></desc>
			<sql><![CDATA[
SELECT
    TO_CHAR (CNI_GET_GMT_FNC (@[user_id]), 'YYYY-MM-DD') GMT
FROM
    DUAL			]]></sql>
			<params>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
