<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOAddChangeApprovalRequestOfficeItemCSQL">
			<desc><![CDATA[2011.04.18 [CHM-201108838-01] 이준범
Title : OFC code Change 설정 시 Assigned Expense Data 변경 요청
DESC :  Item의 기존 OFC_CD 를 변경될 OFC_CD 로 생성 한다.]]></desc>
			<sql><![CDATA[
INSERT INTO GEM_ITEM
( GEN_EXPN_RQST_NO
 ,OFC_CD
 ,GEN_EXPN_CD
 ,GEN_EXPN_ITM_NO
 ,GEN_EXPN_TRNS_DIV_CD
 ,GEN_EXPN_RQST_SEQ
 ,CRNT_GEN_EXPN_APRO_STEP_CD
 ,CRNT_GEN_EXPN_APSTS_CD
 ,GEN_EXPN_APRO_AUTH_OFC_CD
 ,GEN_EXPN_ITM_DESC
 ,GEN_EXPN_CALC_BSS_DESC
 ,RQST_OPIN_RMK
 ,JAN_AMT
 ,FEB_AMT
 ,MAR_AMT
 ,APR_AMT
 ,MAY_AMT
 ,JUN_AMT
 ,JUL_AMT
 ,AUG_AMT
 ,SEP_AMT
 ,OCT_AMT
 ,NOV_AMT
 ,DEC_AMT
 ,CRE_USR_ID
 ,CRE_DT
 ,UPD_USR_ID
 ,UPD_DT
)
SELECT GEN_EXPN_RQST_NO
      ,@[bfr_ofc_cd]
      ,GEN_EXPN_CD
      ,GEN_EXPN_ITM_NO
      ,GEN_EXPN_TRNS_DIV_CD
      ,GEN_EXPN_RQST_SEQ
      ,CRNT_GEN_EXPN_APRO_STEP_CD
      ,CRNT_GEN_EXPN_APSTS_CD
      ,GEN_EXPN_APRO_AUTH_OFC_CD
      ,GEN_EXPN_ITM_DESC
      ,GEN_EXPN_CALC_BSS_DESC
      ,RQST_OPIN_RMK
      ,JAN_AMT
      ,FEB_AMT
      ,MAR_AMT
      ,APR_AMT
      ,MAY_AMT
      ,JUN_AMT
      ,JUL_AMT
      ,AUG_AMT
      ,SEP_AMT
      ,OCT_AMT
      ,NOV_AMT
      ,DEC_AMT
      ,@[usr_id]
      ,SYSDATE
      ,@[usr_id]
      ,SYSDATE
  FROM GEM_ITEM
 WHERE GEN_EXPN_RQST_NO IN ( 
                            SELECT GEN_EXPN_RQST_NO
                              FROM GEM_REQUEST
                             WHERE PLN_YRMON LIKE SUBSTR(@[stnd_dt], 1, 4)||'%'
                           )
   AND OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="bfr_ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="stnd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
