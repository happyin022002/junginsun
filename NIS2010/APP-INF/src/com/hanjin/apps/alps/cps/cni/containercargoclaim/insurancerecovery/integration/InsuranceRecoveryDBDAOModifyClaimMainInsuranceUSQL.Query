<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="InsuranceRecoveryDBDAOModifyClaimMainInsuranceUSQL">
			<desc><![CDATA[Claim Main Insurance 수정]]></desc>
			<sql><![CDATA[
UPDATE CNI_CGO_CLM_INSUR SET                
   INSUR_CLM_PTY_NO  = @[insur_clm_pty_no] 
 , CLM_PTY_NO        = @[clm_pty_no] 
 , INSUR_REF_NO      = @[insur_ref_no] 
 , AGN_CRSPN_TP_CD   = @[agn_crspn_tp_cd] 
 , AGN_CRSPN_APNT_DT = @[agn_crspn_apnt_dt] 
 , AGN_CRSPN_REF_NO  = @[agn_crspn_ref_no] 
 , UPD_USR_ID        = @[upd_usr_id] 
 , UPD_DT            = CNI_GET_GMT_FNC(@[upd_usr_id]) 
WHERE CGO_CLM_NO     = @[cgo_clm_no]			]]></sql>
			<params>
				<param name="insur_clm_pty_no" type="12" value="" out="N"/>
				<param name="clm_pty_no" type="12" value="" out="N"/>
				<param name="insur_ref_no" type="12" value="" out="N"/>
				<param name="agn_crspn_tp_cd" type="12" value="" out="N"/>
				<param name="agn_crspn_apnt_dt" type="12" value="" out="N"/>
				<param name="agn_crspn_ref_no" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
