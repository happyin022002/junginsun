<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOModifySubsidiaryActualResultsUSQL">
			<desc><![CDATA[현지 법인 월별 실적 수정]]></desc>
			<sql><![CDATA[
UPDATE GEM_SUBS_PERF
SET PERF_AMT = @[perf_amt],
UPD_USR_ID = @[upd_usr_id],
UPD_DT = SYSDATE
WHERE RSLT_YRMON = @[rslt_yrmon]
AND OFC_CD = @[ofc_cd]
AND GEN_EXPN_CD = @[gen_expn_cd]			]]></sql>
			<params>
				<param name="perf_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rslt_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="gen_expn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
