<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOCustomDryWetClaimHisCSQL">
			<desc><![CDATA[Dry Wet Claim Status history 입력]]></desc>
			<sql><![CDATA[
INSERT INTO CNI_DW_CLM_HIS (
	DW_CLM_NO
,	DW_CLM_HIS_SEQ
,	DW_CLM_STS_CD
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) SELECT 
	@[dw_clm_no]
,	(SELECT NVL(MAX(DW_CLM_HIS_SEQ),0)+1 FROM CNI_DW_CLM_HIS WHERE DW_CLM_NO = @[dw_clm_no])
,	@[dw_clm_sts_cd]
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
FROM DUAL			]]></sql>
			<params>
				<param name="dw_clm_no" type="12" value="" out="N"/>
				<param name="dw_clm_sts_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
