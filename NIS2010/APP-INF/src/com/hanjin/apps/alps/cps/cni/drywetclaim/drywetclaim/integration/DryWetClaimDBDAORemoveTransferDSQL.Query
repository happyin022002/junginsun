<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAORemoveTransferDSQL">
			<desc><![CDATA[Remove Transfer]]></desc>
			<sql><![CDATA[
DELETE FROM CNI_DW_TRNS    
WHERE DW_CLM_NO = @[dw_clm_no] 
  AND TRNS_SEQ   = @[trns_seq]			]]></sql>
			<params>
				<param name="dw_clm_no" type="12" value="" out="N"/>
				<param name="trns_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
