<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOAddTransferCSQL">
			<desc><![CDATA[Add Transfer]]></desc>
			<sql><![CDATA[
INSERT INTO CNI_DW_TRNS (
   DW_CLM_NO
,  TRNS_SEQ
,  TRNS_OFC_CD
,  TRNS_USR_ID
,  TRNS_FM_OFC_CD
,  TRNS_FM_USR_ID
,  TRNS_FM_DT
,  CLM_TRNS_AUTH_CD
,  TRNS_TO_OFC_CD
,  TRNS_TO_USR_ID
,  TRNS_TO_DT
,  TRNS_RMK
,  TRNS_KNT
,  CRE_USR_ID
,  CRE_DT
,  UPD_USR_ID
,  UPD_DT
  ) VALUES (
   @[dw_clm_no]
 , CNI_DWC_TRNS_SEQ.NEXTVAL
 , @[trns_ofc_cd]
 , @[cre_usr_id]
 , @[trns_fm_ofc_cd]
 , @[trns_fm_usr_id]
 , TO_CHAR(CNI_GET_GMT_FNC(@[cre_usr_id]),'YYYYMMDD')
 , 'W'
 , @[trns_to_ofc_cd]
 , @[trns_to_usr_id]
#if (${trns_to_dt} != '')
 , @[trns_to_dt]
#else
 , NULL
#end
 , @[trns_rmk]
 , (SELECT COUNT(DW_CLM_NO) FROM CNI_DW_TRNS WHERE DW_CLM_NO = @[dw_clm_no])
 , @[cre_usr_id]
 , CNI_GET_GMT_FNC(@[cre_usr_id])
 , @[upd_usr_id]
 , CNI_GET_GMT_FNC(@[upd_usr_id])
 )			]]></sql>
			<params>
				<param name="dw_clm_no" type="12" value="" out="N"/>
				<param name="trns_ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="trns_fm_ofc_cd" type="12" value="" out="N"/>
				<param name="trns_fm_usr_id" type="12" value="" out="N"/>
				<param name="trns_to_ofc_cd" type="12" value="" out="N"/>
				<param name="trns_to_usr_id" type="12" value="" out="N"/>
				<param name="trns_to_dt" type="12" value="" out="N"/>
				<param name="trns_rmk" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
