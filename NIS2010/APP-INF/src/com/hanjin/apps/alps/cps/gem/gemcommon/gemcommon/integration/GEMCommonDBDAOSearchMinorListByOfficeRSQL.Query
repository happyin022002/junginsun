<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMCommonDBDAOSearchMinorListByOfficeRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT DISTINCT L_4 CODE
FROM            GEM_OFC_LEVEL_V
WHERE           1 = 1
#if (${rgn_ofc_flg} != '')
AND             RGN_OFC_FLG = @[rgn_ofc_flg]
AND             L_3 = #if (${ofc_cd} == '') '' #else @[ofc_cd] #end
#end
AND             L_4 IN (SELECT OFC_CD
                        FROM   GEM_OFFICE
                        WHERE  RQST_AUTH_FLG = 'Y'
                        AND    DELT_FLG = 'N')
ORDER BY 		CODE			]]></sql>
			<params>
				<param name="rgn_ofc_flg" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
