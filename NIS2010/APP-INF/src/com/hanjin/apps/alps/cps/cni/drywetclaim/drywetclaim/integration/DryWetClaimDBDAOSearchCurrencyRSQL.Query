<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOSearchCurrencyRSQL">
			<desc><![CDATA[통화를 검사한다]]></desc>
			<sql><![CDATA[
SELECT CURR_CD FROM MDM_CURRENCY
 WHERE CURR_CD = @[curr_cd]
   AND ROWNUM =1			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
