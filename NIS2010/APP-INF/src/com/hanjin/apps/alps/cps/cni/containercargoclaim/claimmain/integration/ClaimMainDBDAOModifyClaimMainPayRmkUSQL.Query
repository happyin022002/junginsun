<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOModifyClaimMainPayRmkUSQL">
			<desc><![CDATA[Payment 팝업에서 Remark Update]]></desc>
			<sql><![CDATA[
UPDATE
    CNI_CGO_CLM
SET
    PAY_RMK = @[pay_rmk]
WHERE
    CGO_CLM_NO = @[cgo_clm_no]			]]></sql>
			<params>
				<param name="pay_rmk" type="12" value="" out="N"/>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
