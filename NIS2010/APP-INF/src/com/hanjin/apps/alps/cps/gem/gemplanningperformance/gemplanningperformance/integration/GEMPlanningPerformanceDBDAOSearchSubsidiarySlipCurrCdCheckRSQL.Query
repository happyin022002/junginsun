<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchSubsidiarySlipCurrCdCheckRSQL">
			<desc><![CDATA[현지법인 전표 데이터 currency code 유효성 체크]]></desc>
			<sql><![CDATA[
SELECT CURR_CD
  FROM GL_MON_XCH_RT
 WHERE ACCT_XCH_RT_YRMON     = SUBSTR(@[gl_eff_dt],1,6)
   AND CURR_CD = @[slp_curr_cd]
   AND ACCT_XCH_RT_LVL = '1'
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="gl_eff_dt" type="12" value="" out="N"/>
				<param name="slp_curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
