<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOModifyGemSlpIfApproUsrIdUSQL">
			<desc><![CDATA[FNS061-0001 연동관련한 GEM_SLP_IF 테이블에 승인자정보를 수정한다.]]></desc>
			<sql><![CDATA[
UPDATE GEM_SLP_IF
   SET PRPR_USR_ID = @[prpr_usr_id]
      ,APRO_USR_ID = @[apro_usr_id]
	  ,UPD_USR_ID = @[upd_usr_id]
	  ,UPD_DT = sysdate
 WHERE SLP_TJ_NO = @[slp_tj_no]			]]></sql>
			<params>
				<param name="prpr_usr_id" type="12" value="" out="N"/>
				<param name="apro_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="slp_tj_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
