,	N3RD_PRE_TS_LOC_CD		= @[n3rd_pre_ts_loc_cd]
,	N3RD_PRE_TS_DT			= @[n3rd_pre_ts_dt]
,	N1ST_PST_REF_VVD_NO		= @[n1st_pst_ref_vvd_no]
,	N2ND_PST_REF_VVD_NO		= @[n2nd_pst_ref_vvd_no]
