<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncidentSurveyDBDAOModifySalvageUSQL">
			<desc><![CDATA[Salvage 저장]]></desc>
			<sql><![CDATA[
UPDATE
    CNI_CGO_CLM_SLV
SET
    CLM_PTY_NO			= @[clm_pty_no]
  , REF_SLV_NO			= @[ref_slv_no]
  , SLV_DT				= @[slv_dt]
  , SLV_USD_AMT			= @[slv_usd_amt]
  , SLV_LOCL_AMT		= @[slv_locl_amt]
  , SLV_LOCL_CURR_CD    = @[slv_locl_curr_cd]
  , SLV_XCH_RT			= @[slv_xch_rt]
  , SLV_DESC			= @[slv_desc]
  , UPD_USR_ID			= @[upd_usr_id]
  , UPD_DT				= CNI_GET_GMT_FNC(@[upd_usr_id])
WHERE
    CGO_CLM_NO = @[cgo_clm_no]			]]></sql>
			<params>
				<param name="clm_pty_no" type="12" value="" out="N"/>
				<param name="ref_slv_no" type="12" value="" out="N"/>
				<param name="slv_dt" type="12" value="" out="N"/>
				<param name="slv_usd_amt" type="6" value="" out="N"/>
				<param name="slv_locl_amt" type="6" value="" out="N"/>
				<param name="slv_locl_curr_cd" type="12" value="" out="N"/>
				<param name="slv_xch_rt" type="6" value="" out="N"/>
				<param name="slv_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
