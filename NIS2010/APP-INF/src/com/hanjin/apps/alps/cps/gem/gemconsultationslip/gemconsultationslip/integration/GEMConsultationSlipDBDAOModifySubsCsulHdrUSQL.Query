<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMConsultationSlipDBDAOModifySubsCsulHdrUSQL">
			<desc><![CDATA[hdr 테이블 정보를 변경을 한다.]]></desc>
			<sql><![CDATA[
UPDATE GEM_SUBS_CSUL_HDR
SET 
   SUBS_OFC_CD        = @[subs_ofc_cd]
  ,APRO_DT            = SYSDATE       
  ,INP_DT             = REPLACE(@[inp_dt],'-','')    
  ,PAY_VNDR_NM        = @[pay_vndr_nm]     
  ,INV_CURR_CD        = @[inv_curr_cd]     
  ,INV_LOCL_TTL_AMT   = REPLACE(@[inv_locl_ttl_amt],',','')
  ,INV_USD_TTL_AMT    = REPLACE(@[inv_usd_ttl_amt],',','') 
  ,EXPN_DIV_CD        = @[expn_div_cd]     
  ,UPD_USR_ID         = @[upd_usr_id]      
  ,UPD_DT             = SYSDATE          
WHERE SUBS_CSR_NO = @[subs_csr_no]			]]></sql>
			<params>
				<param name="subs_ofc_cd" type="12" value="" out="N"/>
				<param name="inp_dt" type="12" value="" out="N"/>
				<param name="pay_vndr_nm" type="12" value="" out="N"/>
				<param name="inv_curr_cd" type="12" value="" out="N"/>
				<param name="inv_locl_ttl_amt" type="12" value="" out="N"/>
				<param name="inv_usd_ttl_amt" type="12" value="" out="N"/>
				<param name="expn_div_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="subs_csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
