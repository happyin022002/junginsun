<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOCustomHandlingCostVODSQL">
			<desc><![CDATA[해당 Case 관련 발생된 제 처리비용을 삭제한다]]></desc>
			<sql><![CDATA[
DELETE FROM CNI_DW_CLM_COST
WHERE	DW_CLM_NO = @[dw_clm_no]
AND	COST_SEQ = @[cost_seq]
			]]></sql>
			<params>
				<param name="dw_clm_no" type="12" value="" out="N"/>
