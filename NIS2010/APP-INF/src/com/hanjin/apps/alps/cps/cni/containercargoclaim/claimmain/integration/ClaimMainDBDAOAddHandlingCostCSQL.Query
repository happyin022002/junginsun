<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOAddHandlingCostCSQL">
			<desc><![CDATA[HandlingCost 입력]]></desc>
			<sql><![CDATA[
INSERT
INTO
    CNI_CGO_CLM_COST
    (
        CGO_CLM_PAY_NO
      , CGO_CLM_NO
      , CLM_COST_TP_CD
      , CLM_PTY_NO
      , COST_DESC
      , INV_NO
      , INV_DT
      , INV_AMT
      , LOCL_CURR_CD
      , INV_XCH_RT
      , INV_USD_AMT
      , PAY_DT
      , INV_RMK
      , INV_RGST_NO
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , UPD_DT
    )
    VALUES
    (
      CNI_CLM_PTY_NO_SEQ.NEXTVAL
      , @[cgo_clm_no]
      , @[clm_cost_tp_cd]
      , @[clm_pty_no]
      , @[cost_desc]
      , @[inv_no]
      , @[inv_dt]
      , @[inv_amt]
      , @[locl_curr_cd]
      , @[inv_xch_rt]
      , @[inv_usd_amt]
      , TO_DATE(@[pay_dt],'YYYYMMDD')
      , @[inv_rmk]
      , @[inv_rgst_no]
      , @[cre_usr_id]
      , cni_get_gmt_fnc (@[upd_usr_id])
      , @[upd_usr_id]
      , cni_get_gmt_fnc (@[upd_usr_id])
    )			]]></sql>
			<params>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
				<param name="clm_cost_tp_cd" type="12" value="" out="N"/>
				<param name="clm_pty_no" type="12" value="" out="N"/>
				<param name="cost_desc" type="12" value="" out="N"/>
				<param name="inv_no" type="12" value="" out="N"/>
				<param name="inv_dt" type="12" value="" out="N"/>
				<param name="inv_amt" type="6" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="inv_xch_rt" type="6" value="" out="N"/>
				<param name="inv_usd_amt" type="6" value="" out="N"/>
				<param name="pay_dt" type="12" value="" out="N"/>
				<param name="inv_rmk" type="12" value="" out="N"/>
				<param name="inv_rgst_no" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
