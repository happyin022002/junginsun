,	GRS_RGST_TONG_WGT
,	DWT_WGT
,	INSUR_VSL_OSHP_CD
,	VSL_OSHP_EFF_DT
,	VSL_OSHP_EXP_DT
,	INSUR_CVRG_CD
,	DDCT_CGO_AMT
,	DDCT_CRW_AMT
,	DDCT_DMG_HL_AMT
,	DDCT_OTR_AMT
,	INSUR_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) VALUES( 
	@[insur_tp_cd]
,	@[insur_plcy_yr]
,	@[insur_clm_pty_no]
,	@[vsl_cd]
,	@[insur_eff_dt]
