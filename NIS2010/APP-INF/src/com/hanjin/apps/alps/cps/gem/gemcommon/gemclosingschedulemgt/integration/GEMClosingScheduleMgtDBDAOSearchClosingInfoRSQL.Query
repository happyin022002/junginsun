<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMClosingScheduleMgtDBDAOSearchClosingInfoRSQL">
			<desc><![CDATA[select]]></desc>
			<sql><![CDATA[
SELECT   CLZ_YRMON
        ,CLZ_DIV_CD
        ,CLZ_DT
        ,DECODE (CLZ_FLG, 'Y', '1', '0') CLZ_FLG
        ,DECODE (GL_IF_FLG, 'Y', '1', '0') GL_IF_FLG
        ,CRE_USR_ID
        ,TO_CHAR (CRE_DT, 'YYYY-MM-DD') CRE_DT
        ,UPD_USR_ID
        ,TO_CHAR (UPD_DT, 'YYYY-MM-DD') UPD_DT
FROM     GEM_MON_CLZ
WHERE    CLZ_DIV_CD = @[clz_div_cd]
AND      CLZ_YRMON LIKE @[clz_yrmon]|| '%'
ORDER BY CLZ_YRMON			]]></sql>
			<params>
				<param name="clz_yrmon" type="12" value="2009" out="N"/>
				<param name="clz_div_cd" type="12" value="IN" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
