<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMConsultationSlipDBDAOSearchSubsCsulDtlCountRSQL">
			<desc><![CDATA[GEM_SUBS_CSUL_DTL 테이블의 해당 csr_no가 존재를 하는지 확인을 한다.]]></desc>
			<sql><![CDATA[
SELECT COUNT(SUBS_CSR_NO)  FROM GEM_SUBS_CSUL_DTL
WHERE SUBS_CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
