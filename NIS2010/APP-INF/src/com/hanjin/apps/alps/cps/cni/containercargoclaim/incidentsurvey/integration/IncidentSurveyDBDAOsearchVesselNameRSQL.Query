<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncidentSurveyDBDAOsearchVesselNameRSQL">
			<desc><![CDATA[VVD validation]]></desc>
			<sql><![CDATA[
SELECT
    A.VSL_CD
FROM
    MDM_COUNTRY MC
  , AR_MST_REV_VVD B
  , MDM_VSL_BLK A
WHERE
    MC.CNT_CD(+)                             = A.VSL_RGST_CNT_CD
    AND B.VSL_CD(+)                          = A.VSL_CD
    AND A.VSL_CD||B.SKD_VOY_NO||B.SKD_DIR_CD = @[vsl_cd]
    AND A.DELT_FLG                           = 'N'
UNION ALL
SELECT
    A.VSL_CD
FROM
    MDM_COUNTRY MC
  , VSK_VSL_SKD B
  , MDM_VSL_CNTR A
WHERE
    MC.CNT_CD                                = A.VSL_RGST_CNT_CD
    AND B.VSL_CD(+)                          = A.VSL_CD
    AND A.VSL_CD||B.SKD_VOY_NO||B.SKD_DIR_CD = @[vsl_cd]
    AND A.DELT_FLG                           = 'N'
    AND ROWNUM = 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
