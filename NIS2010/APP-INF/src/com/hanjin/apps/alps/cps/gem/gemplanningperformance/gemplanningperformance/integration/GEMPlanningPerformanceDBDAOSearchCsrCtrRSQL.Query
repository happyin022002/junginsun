<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchCsrCtrRSQL">
			<desc><![CDATA[A/P 품의서를 작성한 Center 의 Office 를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CTR_CD code
FROM   GEM_OFFICE
WHERE  DELT_FLG = 'N'
AND    OFC_CD = @[code]			]]></sql>
			<params>
				<param name="code" type="12" value="213006" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
