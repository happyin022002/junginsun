<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOSearchCheckPayeeRSQL">
			<desc><![CDATA[Payee를 검사한다]]></desc>
			<sql><![CDATA[
SELECT 
	CLM_PTY_NO
FROM CNI_PARTY
WHERE	CLM_PTY_NO = @[clm_pty_no]
			]]></sql>
			<params>
				<param name="clm_pty_no" type="2" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
