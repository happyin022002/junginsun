<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOSearchOfficeHistoryInfoByCtrCodeRSQL">
			<desc><![CDATA[Office Code History의 Ofc_Code에 해당하는 Ctr_Code를 조회]]></desc>
			<sql><![CDATA[
SELECT CTR_CD CODE
FROM   GEM_OFFICE
WHERE  OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
