<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOCustomHandlingCostVOCSQL">
			<desc><![CDATA[해당 Case 관련 발생된 제 처리비용을 생성한다]]></desc>
			<sql><![CDATA[
INSERT INTO CNI_DW_CLM_COST (
	DW_CLM_NO
,	COST_SEQ
,	CLM_COST_TP_CD
,	CLM_PTY_NO
,	COST_DESC
,	INV_DT
,	PAY_DT
,	LOCL_CURR_CD
,	INV_AMT
,	INV_USD_AMT
,	INV_XCH_RT
,	INV_RMK
,	CRE_USR_ID
,	CRE_DT
,	UPD_USR_ID
,	UPD_DT
) SELECT
	@[dw_clm_no]
,	(SELECT NVL(MAX(COST_SEQ),0)+1 FROM CNI_DW_CLM_COST WHERE DW_CLM_NO = @[dw_clm_no])
,	@[clm_cost_tp_cd]
,	@[clm_pty_no]
,	@[cost_desc]
,	@[inv_dt]
,	@[pay_dt]
,	@[locl_curr_cd]
,	@[inv_amt]
,	@[inv_usd_amt]
,	@[inv_xch_rt]
,	@[inv_rmk]
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
,	@[cre_usr_id]
,	CNI_GET_GMT_FNC(@[cre_usr_id])
FROM DUAL			]]></sql>
			<params>
				<param name="dw_clm_no" type="12" value="" out="N"/>
				<param name="clm_cost_tp_cd" type="12" value="" out="N"/>
				<param name="clm_pty_no" type="2" value="" out="N"/>
				<param name="cost_desc" type="12" value="" out="N"/>
				<param name="inv_dt" type="12" value="" out="N"/>
				<param name="pay_dt" type="12" value="" out="N"/>
				<param name="locl_curr_cd" type="12" value="" out="N"/>
				<param name="inv_amt" type="6" value="" out="N"/>
				<param name="inv_usd_amt" type="6" value="" out="N"/>
				<param name="inv_xch_rt" type="6" value="" out="N"/>
				<param name="inv_rmk" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
