<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="VesselStatusDBDAOSearchInsuranceFlagRSQL">
			<desc><![CDATA[국가명을 조회한다]]></desc>
			<sql><![CDATA[
SELECT 
	CNT_NM
FROM MDM_COUNTRY
WHERE	CNT_CD = @[cnt_cd]
AND     DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
