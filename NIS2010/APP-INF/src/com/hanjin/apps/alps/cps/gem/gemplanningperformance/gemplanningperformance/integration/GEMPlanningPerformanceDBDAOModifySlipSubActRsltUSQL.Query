<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOModifySlipSubActRsltUSQL">
			<desc><![CDATA[현지 법인 월별 실적 수정]]></desc>
			<sql><![CDATA[
UPDATE GEM_SUBS_PERF A
   SET A.PERF_AMT    = A.PERF_AMT + @[perf_amt]/(SELECT B.RQST_UT_VAL FROM GEM_OFFICE B WHERE B.OFC_CD = A.OFC_CD AND B.DELT_FLG = 'N' AND ROWNUM = 1),
       A.UPD_USR_ID  = @[upd_usr_id],
       A.UPD_DT      = SYSDATE
 WHERE A.RSLT_YRMON  = @[rslt_yrmon]
   AND A.OFC_CD      = @[ofc_cd]
   AND A.GEN_EXPN_CD = @[gen_expn_cd]			]]></sql>
			<params>
				<param name="perf_amt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rslt_yrmon" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="gen_expn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
