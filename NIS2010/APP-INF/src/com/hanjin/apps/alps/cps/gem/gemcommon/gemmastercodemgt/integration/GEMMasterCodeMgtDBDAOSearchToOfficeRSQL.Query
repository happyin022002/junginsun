<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOSearchToOfficeRSQL">
			<desc><![CDATA[모든 본사 조직 목록 조회]]></desc>
			<sql><![CDATA[
SELECT '0' SORT1,' ' CODE FROM   DUAL UNION ALL
SELECT '1' SORT1,'ALL' CODE FROM   DUAL UNION ALL
SELECT '2' SORT1, OFC_CD CODE
FROM   GEM_OFFICE
WHERE  OFC_CO_DIV_CD = 'O'
#if(${rgn_ofc_flg} != '')
AND    RGN_OFC_FLG = @[rgn_ofc_flg]
#end
#if(${delt_flg} != '')
AND    DELT_FLG = @[delt_flg]
#end
#if(${delt_flg} == '')
AND    DELT_FLG = 'N'
#end
ORDER BY 1, 2			]]></sql>
			<params>
				<param name="rgn_ofc_flg" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
