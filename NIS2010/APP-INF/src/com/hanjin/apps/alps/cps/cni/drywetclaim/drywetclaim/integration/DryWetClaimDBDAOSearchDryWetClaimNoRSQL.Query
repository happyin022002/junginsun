<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOSearchDryWetClaimNoRSQL">
			<desc><![CDATA[Dry Wet Claim No 조회]]></desc>
			<sql><![CDATA[
SELECT @[dw_clm_tp_cd]||TO_CHAR(SYSDATE, 'YYYYMM')||LPAD(TO_NUMBER(NVL(MAX(SUBSTR(DW_CLM_NO,10)),0))+1,2,'0') DW_CLM_NO
FROM   CNI_DW_CLM
WHERE  DW_CLM_NO LIKE @[dw_clm_tp_cd]||TO_CHAR(SYSDATE, 'YYYYMM')||'%'
AND    DW_CLM_TP_CD = @[dw_clm_tp_cd]
			]]></sql>
			<params>
				<param name="dw_clm_tp_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
