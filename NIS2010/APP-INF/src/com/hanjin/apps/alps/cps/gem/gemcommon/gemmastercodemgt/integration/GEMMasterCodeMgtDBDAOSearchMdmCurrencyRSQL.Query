<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOSearchMdmCurrencyRSQL">
			<desc><![CDATA[입력된 Currency Code가 유효한 Code 인지 체크]]></desc>
			<sql><![CDATA[
select 
	curr_cd
from mdm_currency
where	curr_cd = @[curr_cd]
and DELT_FLG = 'N'			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="n"/>
			</params>
		</query>
	</querys>
</sqls>
