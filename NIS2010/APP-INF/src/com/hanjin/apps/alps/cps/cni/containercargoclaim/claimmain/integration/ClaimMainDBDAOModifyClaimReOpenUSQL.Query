<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOModifyClaimReOpenUSQL">
			<desc><![CDATA[Claim ReOpen]]></desc>
			<sql><![CDATA[
UPDATE CNI_CGO_CLM SET
       CS_CLZ_ROPN_FLG     = 'Y'
     , CS_CLZ_ROPN_DT      = TO_CHAR(CNI_GET_GMT_FNC(@[upd_usr_id]),'YYYYMMDD')
     , CS_CLZ_ROPN_OFC_CD  = (SELECT OFC_CD FROM COM_USER WHERE USR_ID = @[upd_usr_id])
     , CS_CLZ_ROPN_USR_ID  = @[upd_usr_id]
     , UPD_USR_ID          = @[upd_usr_id]
     , UPD_DT              = CNI_GET_GMT_FNC(@[upd_usr_id])
#if (${cgo_clm_stl_tp_cd} == 'RP' || ${cgo_clm_stl_tp_cd} == 'TD' || ${cgo_clm_stl_tp_cd} == 'TB' || ${cgo_clm_stl_tp_cd} == 'WD' || ${cgo_clm_stl_tp_cd} == 'DS') 
	 , CLM_STL_APPL_DT     = ''
	 , CLM_STL_APPL_USR_ID = ''
	 , CLM_STL_APPL_OFC_CD = ''
	 , CLM_STL_AUTH_DT     = ''
	 , CLM_STL_AUTH_USR_ID = ''
	 , CLM_STL_AUTH_OFC_CD = ''
	 , CLM_STL_AUTH_CD     = ''
#end
WHERE CGO_CLM_NO = @[cgo_clm_no]			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cgo_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
