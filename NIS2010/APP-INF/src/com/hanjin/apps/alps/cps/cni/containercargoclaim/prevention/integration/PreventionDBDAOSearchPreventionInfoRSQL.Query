<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PreventionDBDAOSearchPreventionInfoRSQL">
			<desc><![CDATA[Prevention Key로 검색]]></desc>
			<sql><![CDATA[
SELECT
    CLM_PRVE_NO
  , CLM_PRVE_DIV_CD
  , CNI_GET_CLM_MISC_NM_FNC ('37', CLM_PRVE_DIV_CD, '2') CLM_PRVE_DIV_NM
  , EFF_DT
  , EXP_DT
  , CRE_OFC_CD
  , (
        SELECT
            CLM_AREA_CD
        FROM
            CNI_AREA_OFC
        WHERE
            OFC_CD = CRE_OFC_CD
    )
    CLM_AREA_CD
  , CLM_PRVE_SUBJ_NM
  , CLM_PRVE_DESC
  , CLM_PRVE_READ_KNT
  , CRE_USR_ID
  , CRE_DT
FROM
    CNI_CLM_PRVE
WHERE
    1               = 1
    AND CLM_PRVE_NO = @[clm_prve_no]			]]></sql>
			<params>
				<param name="clm_prve_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
