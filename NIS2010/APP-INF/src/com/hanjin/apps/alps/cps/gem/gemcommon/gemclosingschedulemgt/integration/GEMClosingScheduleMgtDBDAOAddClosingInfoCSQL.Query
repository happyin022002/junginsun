<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMClosingScheduleMgtDBDAOAddClosingInfoCSQL">
			<desc><![CDATA[insert]]></desc>
			<sql><![CDATA[
INSERT INTO GEM_MON_CLZ
            (CLZ_YRMON, CLZ_DIV_CD, CLZ_DT, CLZ_FLG, GL_IF_FLG, CRE_USR_ID, CRE_DT
            ,UPD_USR_ID, UPD_DT
            )
     VALUES (@[clz_yrmon], @[clz_div_cd], @[clz_dt], @[clz_flg], @[gl_if_flg], @[cre_usr_id], TO_DATE(@[cre_dt],'YYYY-MM-DD HH24:MI:SS')
            ,@[upd_usr_id], SYSDATE
            )			]]></sql>
			<params>
				<param name="clz_yrmon" type="12" value="" out="N"/>
				<param name="clz_div_cd" type="12" value="" out="N"/>
				<param name="clz_dt" type="12" value="" out="N"/>
				<param name="clz_flg" type="12" value="" out="N"/>
				<param name="gl_if_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="cre_dt" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
