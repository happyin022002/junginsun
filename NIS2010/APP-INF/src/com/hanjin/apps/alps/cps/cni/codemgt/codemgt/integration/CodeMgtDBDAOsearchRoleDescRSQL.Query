<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeMgtDBDAOsearchRoleDescRSQL">
			<desc><![CDATA[Role Code에 해당하는 Description을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT INTG_CD_VAL_DESC FROM COM_INTG_CD_DTL
WHERE INTG_CD_ID = 'CD03246'
AND INTG_CD_VAL_CTNT = @[role_code]			]]></sql>
			<params>
				<param name="role_code" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
