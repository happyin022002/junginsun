<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CodeMgtDBDAOSearchMdmOrganizationRSQL">
			<desc><![CDATA[Office Code조회]]></desc>
			<sql><![CDATA[
SELECT
    A.OFC_CD
  , B.INTG_CD_VAL_DP_DESC
  , A.OFC_ENG_NM
FROM
    MDM_ORGANIZATION A
  , COM_INTG_CD_DTL B
WHERE
    A.OFC_KND_CD = B.INTG_CD_VAL_CTNT
    AND A.OFC_CD LIKE @[ofc_cd]||'%'
    AND B.INTG_CD_ID = 'CD00675'
    AND A.DELT_FLG   = 'N'
ORDER BY A.OFC_CD ASC			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
