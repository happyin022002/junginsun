<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="IncidentSurveyDBDAOModifyIncidentUSQL">
			<desc><![CDATA[Modify 저장]]></desc>
			<sql><![CDATA[
UPDATE
    CNI_CGO_CLM_INCI
SET
    INCI_PLC_TP_CD  = @[inci_plc_tp_cd]
  , INCI_OCCR_DT    = @[inci_occr_dt]
  , INCI_LOC_CD     = @[inci_loc_cd]
  , INCI_SUBJ_NM    = @[inci_subj_nm]
  , INCI_REF_VVD_NO = @[inci_ref_vvd_no]
  , INCI_DEV_DESC   = @[inci_dev_desc]
  , UPD_USR_ID      = @[upd_usr_id]
  , UPD_DT          = CNI_GET_GMT_FNC(@[upd_usr_id])
WHERE
    CGO_CLM_INCI_NO = @[cgo_clm_inci_no]			]]></sql>
			<params>
				<param name="inci_plc_tp_cd" type="12" value="" out="N"/>
				<param name="inci_occr_dt" type="12" value="" out="N"/>
				<param name="inci_loc_cd" type="12" value="" out="N"/>
				<param name="inci_subj_nm" type="12" value="" out="N"/>
				<param name="inci_ref_vvd_no" type="12" value="" out="N"/>
				<param name="inci_dev_desc" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="cgo_clm_inci_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
