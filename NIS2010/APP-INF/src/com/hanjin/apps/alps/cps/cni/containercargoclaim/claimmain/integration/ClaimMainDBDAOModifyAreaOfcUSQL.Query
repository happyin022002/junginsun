<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ClaimMainDBDAOModifyAreaOfcUSQL">
			<desc><![CDATA[Area office Update]]></desc>
			<sql><![CDATA[
UPDATE CNI_AREA_OFC SET 
	OFC_CD = @[ofc_cd]
,	CLM_AREA_CD = @[clm_area_cd]
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = sysdate
WHERE	OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="clm_area_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
