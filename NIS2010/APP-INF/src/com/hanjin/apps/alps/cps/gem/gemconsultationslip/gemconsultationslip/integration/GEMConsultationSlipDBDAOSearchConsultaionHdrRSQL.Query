<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMConsultationSlipDBDAOSearchConsultaionHdrRSQL">
			<desc><![CDATA[HDR 정보를 조회를 한다.]]></desc>
			<sql><![CDATA[
SELECT  
 SUBS_CSR_NO 
,SUBS_OFC_CD
,TO_CHAR(TO_DATE(INP_DT,'YYYY-MM-DD'),'YYYY-MM-DD') INP_DT
,TO_CHAR(TO_DATE(INV_DT,'YYYY-MM-DD'),'YYYY-MM-DD') INV_DT
,PAY_VNDR_NM
,TO_CHAR(APRO_DT ,'YYYY-MM-DD') APRO_DT
,EXPN_DIV_CD
,DECODE(APRO_RSLT_CD,'X','Saved','C','Deleted','P','Submitted','N','Rejected','Y','Approved','E','Error') APRO_RSLT_CD
,INV_CURR_CD
,INV_LOCL_TTL_AMT 
,INV_USD_TTL_AMT 
,CRE_USR_ID
FROM GEM_SUBS_CSUL_HDR
WHERE 1=1
AND SUBS_CSR_NO = @[subs_csr_no]			]]></sql>
			<params>
				<param name="subs_csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
