<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOSearchVesselListVORSQL">
			<desc><![CDATA[선박명 조회 팝업]]></desc>
			<sql><![CDATA[
SELECT A.VSL_CD, A.VSL_ENG_NM, 'OTHR' TOV, A.VSL_OWN_IND_CD, TO_CHAR(A.LNCH_DT,'YYYYMMDD') LNCH_DT,
	   A.VSL_RGST_CNT_CD, MC.CNT_NM, A.GRS_RGST_TONG_WGT, B.CRR_NM
FROM   MDM_COUNTRY MC, MDM_CARRIER B, MDM_VSL_BLK A
WHERE  MC.CNT_CD(+)		= A.VSL_RGST_CNT_CD
AND    B.CRR_CD(+)	 	= A.BLK_CRR_TP_CD
AND    A.VSL_CD   		LIKE '%'||@[vsl_cd]||'%'
AND    A.VSL_ENG_NM   	LIKE '%'||@[vsl_eng_nm]||'%'
AND    A.DELT_FLG 		= 'N'
UNION ALL
SELECT A.VSL_CD, A.VSL_ENG_NM, 'CNTR' TOV, A.VSL_OWN_IND_CD, TO_CHAR(A.VSL_LNCH_DT,'YYYYMMDD'),
	   A.VSL_RGST_CNT_CD, MC.CNT_NM, A.GRS_RGST_TONG_WGT, B.CRR_NM
FROM   MDM_COUNTRY MC, MDM_CARRIER B, MDM_VSL_CNTR A
WHERE  MC.CNT_CD		= A.VSL_RGST_CNT_CD
AND    B.CRR_CD		 	= A.CRR_CD
AND	   A.VSL_CD   		LIKE '%'||@[vsl_cd]||'%'
AND    A.VSL_ENG_NM   	LIKE '%'||@[vsl_eng_nm]||'%'
AND    A.DELT_FLG 		= 'N'
ORDER BY 1			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vsl_eng_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
