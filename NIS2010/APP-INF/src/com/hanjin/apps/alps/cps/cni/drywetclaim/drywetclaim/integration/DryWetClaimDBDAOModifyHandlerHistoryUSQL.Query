<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOModifyHandlerHistoryUSQL">
			<desc><![CDATA[ModifyHandlerHistory]]></desc>
			<sql><![CDATA[
UPDATE CNI_DW_CLM_HIS SET 
    EXP_DT = TO_CHAR(CNI_GET_GMT_FNC(@[upd_usr_id]), 'YYYYMMDD')
,   CRNT_HDLR_FLG = 'N'
,	UPD_USR_ID = @[upd_usr_id]
,	UPD_DT = CNI_GET_GMT_FNC(@[upd_usr_id])
WHERE	DW_CLM_NO = @[dw_clm_no]
AND     EXP_DT IS NULL
AND     MGR_HDLR_DIV_CD = 'H'			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="dw_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
