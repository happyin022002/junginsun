<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMConsultationSlipDBDAOPrintGemCsrHeaderInfoRSQL">
			<desc><![CDATA[G/W xml header 정보 수집]]></desc>
			<sql><![CDATA[
SELECT  
 SUBS_CSR_NO  CSR_NO
,SUBS_OFC_CD
,SUBSTR(INP_DT,1,4)||'/'||SUBSTR(INP_DT,5,2)||'/'||SUBSTR(INP_DT,7,2)  CSRDATE
,SUBSTR(INV_DT,1,4)||'/'||SUBSTR(INV_DT,5,2)||'/'||SUBSTR(INV_DT,7,2)  INVOICE_DATE
,APRO_DT
,APRO_RSLT_CD
,GW_APRO_URL_CTNT
,GW_CSR_RQST_ID
,GW_CSR_RQSTR_ID
,GW_APROR_ID
,PAY_VNDR_NM    PAY_TO
,INV_CURR_CD
,INV_LOCL_TTL_AMT  INV_LCL_AMT
,INV_USD_TTL_AMT   INV_USD_AMT
,DECODE(EXPN_DIV_CD,'R','Salary&Allowance','W','Welfare','E','Enterainment','Management&Other') EXPN_TYPE 
,GEM_GET_FNL_JB_CD_FNC(@[csr_no]) APR_LINE
FROM GEM_SUBS_CSUL_HDR
WHERE SUBS_CSR_NO = @[csr_no]			]]></sql>
			<params>
				<param name="csr_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
