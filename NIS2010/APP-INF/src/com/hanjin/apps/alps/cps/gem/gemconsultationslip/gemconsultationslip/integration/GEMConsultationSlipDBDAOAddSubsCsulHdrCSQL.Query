<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMConsultationSlipDBDAOAddSubsCsulHdrCSQL">
			<desc><![CDATA[Add hdr 테이블에 insert]]></desc>
			<sql><![CDATA[
INSERT INTO GEM_SUBS_CSUL_HDR (
  SUBS_CSR_NO
  ,SUBS_OFC_CD
  ,INP_DT
  ,INV_DT
  ,APRO_DT
  ,APRO_RSLT_CD
  ,GW_APRO_URL_CTNT
  ,GW_CSR_RQST_ID
  ,GW_CSR_RQSTR_ID
  ,GW_APROR_ID
  ,PAY_VNDR_NM
  ,INV_CURR_CD
  ,INV_LOCL_TTL_AMT
  ,INV_USD_TTL_AMT
  ,EXPN_DIV_CD
  ,CRE_USR_ID
  ,CRE_DT
  ,UPD_USR_ID
  ,UPD_DT
  ,DELT_FLG
) VALUES( 
   @[subs_csr_no]
  ,@[subs_ofc_cd]
  ,REPLACE(@[inp_dt],'-','')
  ,REPLACE(@[inv_dt],'-','')
  ,SYSDATE
  ,@[apro_rslt_cd]
  ,@[gw_apro_url_ctnt]
  ,@[gw_csr_rqst_id]
  ,@[gw_csr_rqstr_id]
  ,@[gw_apror_id]
  ,@[pay_vndr_nm]
  ,@[inv_curr_cd]
  ,REPLACE(@[inv_locl_ttl_amt],',','')
  ,REPLACE(@[inv_usd_ttl_amt],',','')
  ,@[expn_div_cd]
  ,@[cre_usr_id]
  ,SYSDATE
  ,@[cre_usr_id]
  ,SYSDATE
  ,'N'
  )			]]></sql>
			<params>
				<param name="subs_csr_no" type="12" value="" out="N"/>
				<param name="subs_ofc_cd" type="12" value="" out="N"/>
				<param name="inp_dt" type="12" value="" out="N"/>
				<param name="inv_dt" type="12" value="" out="N"/>
				<param name="apro_rslt_cd" type="12" value="" out="N"/>
				<param name="gw_apro_url_ctnt" type="12" value="" out="N"/>
				<param name="gw_csr_rqst_id" type="12" value="" out="N"/>
				<param name="gw_csr_rqstr_id" type="12" value="" out="N"/>
				<param name="gw_apror_id" type="12" value="" out="N"/>
				<param name="pay_vndr_nm" type="12" value="" out="N"/>
				<param name="inv_curr_cd" type="12" value="" out="N"/>
				<param name="inv_locl_ttl_amt" type="12" value="" out="N"/>
				<param name="inv_usd_ttl_amt" type="12" value="" out="N"/>
				<param name="expn_div_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
