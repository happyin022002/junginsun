<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOSearchRqstExpenseRSQL">
			<desc><![CDATA[조직별 비용계획 수립 가능한 비용코드를 체크하고, 비용 코드의 한글약어명,영문약어명,비용주관팀 정보를 조회한다]]></desc>
			<sql><![CDATA[
SELECT A.GEN_EXPN_CD
      ,A.ENG_ABBR_NM
      ,A.KRN_ABBR_NM
      ,A.TIC_CD
      ,A.GEM_EXPN_GRP_CD1 GEN_EXPN_GROUP_CD
FROM   GEM_EXPN_GRP_V A, GEM_OFC_MTX B
WHERE  A.GEN_EXPN_CD = B.GEN_EXPN_CD
AND    B.DELT_FLG = 'N'
AND    B.OFC_CD = @[ofc_cd]
AND    B.GEN_EXPN_CD = @[gen_expn_cd]
#if (${tic_cd} != '') 	   
	   AND A.TIC_CD = @[tic_cd]
#end
#if (${gen_expn_group_cd} != '') 	   
       AND A.GEM_EXPN_GRP_CD1 =  @[gen_expn_group_cd]
#end			]]></sql>
			<params>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="gen_expn_cd" type="12" value="" out="N"/>
				<param name="tic_cd" type="12" value="" out="N"/>
				<param name="gen_expn_group_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
