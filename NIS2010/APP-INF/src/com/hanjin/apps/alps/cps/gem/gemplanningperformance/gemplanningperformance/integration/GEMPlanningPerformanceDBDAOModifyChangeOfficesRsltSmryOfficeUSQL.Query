<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMPlanningPerformanceDBDAOModifyChangeOfficesRsltSmryOfficeUSQL">
			<desc><![CDATA[2011.04.18 [CHM-201108838-01] 이준범
Title : OFC code Change 설정 시 Assigned Expense Data 변경 요청
DESC :  Summary의 OFC_CD 를 변경한다.]]></desc>
			<sql><![CDATA[
UPDATE GEM_RSLT_SMRY
   SET OFC_CD = @[bfr_ofc_cd]
      ,OFC_CO_DIV_CD = ( 
                        SELECT OFC_CO_DIV_CD
                          FROM GEM_OFFICE
                         WHERE OFC_CD = @[bfr_ofc_cd]
                           AND DELT_FLG = 'N'
                       )
      ,UPD_USR_ID = @[usr_id]
      ,UPD_DT     = SYSDATE
 WHERE RSLT_YRMON LIKE SUBSTR(@[stnd_dt], 1, 4)||'%'
   AND OFC_CD = @[ofc_cd]			]]></sql>
			<params>
				<param name="bfr_ofc_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="stnd_dt" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
