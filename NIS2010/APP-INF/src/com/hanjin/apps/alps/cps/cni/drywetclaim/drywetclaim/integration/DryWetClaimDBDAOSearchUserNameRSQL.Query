<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOSearchUserNameRSQL">
			<desc><![CDATA[2011.08.08 이준범[CHM-201112635-01]
Dry Wet Claim 등록 시 알림 Mail Auto 발송 요청
]]></desc>
			<sql><![CDATA[
SELECT USR_NM
  FROM COM_USER
 WHERE USR_ID = @[usr_id]
   AND ROWNUM = 1			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
