<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="GEMMasterCodeMgtDBDAOModifySubsidiaryAccountMatrixInfoUSQL">
			<desc><![CDATA[2012.04.09 [CHM-201217079-01] 이준범
1.Expense Code Maintenance for subsidiary  신규 기능 추가
  : 엡데이트 SQL]]></desc>
			<sql><![CDATA[
UPDATE GEM_SUBS_ACCT_MTX
   SET SUBS_ACCT_ENG_NM   = @[subs_acct_eng_nm]
      ,SUBS_ACCT_KRN_DESC = @[subs_acct_krn_desc]      
      ,GEN_EXPN_CD        = @[gen_expn_cd]
      ,UPD_USR_ID         = @[upd_usr_id]
      ,UPD_DT             = SYSDATE
 WHERE OFC_CD       = @[ofc_cd]
   AND SUBS_ACCT_CD = @[subs_acct_cd]			]]></sql>
			<params>
				<param name="subs_acct_eng_nm" type="12" value="" out="N"/>
				<param name="subs_acct_krn_desc" type="12" value="" out="N"/>
				<param name="gen_expn_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="subs_acct_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
