<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="DryWetClaimDBDAOSearchDryWetClaimVORSQL">
			<desc><![CDATA[Dry & Wet Clamin를 조회한다]]></desc>
			<sql><![CDATA[
SELECT 
	 A.DW_CLM_NO
	,A.DW_CLM_TP_CD
	,A.DW_CO_CD
	,A.DW_CLM_REF_VVD_NO
	,A.VSL_ENG_NM
	,A.INCI_PLC_TP_CD
	,TO_CHAR(TO_DATE(A.INCI_OCCR_DT, 'YYYYMMDD'),'YYYY-MM-DD') INCI_OCCR_DT
    ,A.CRE_OFC_CD
    ,A.HDLR_OFC_CD
	,A.HDLR_USR_ID
	,TO_CHAR(TO_DATE(A.TM_BAR_DT, 'YYYYMMDD'),'YYYY-MM-DD') TM_BAR_DT
	,TO_CHAR(TO_DATE(A.LIT_DT, 'YYYYMMDD'),'YYYY-MM-DD') LIT_DT
	,A.DW_CLM_STS_CD
	,A.DW_CLM_ATT_DEF_TP_CD
	,TO_CHAR(TO_DATE(A.PRLM_CLM_NTFY_DT, 'YYYYMMDD'),'YYYY-MM-DD') PRLM_CLM_NTFY_DT
	,TO_CHAR(TO_DATE(A.CS_CLZ_DT, 'YYYYMMDD'),'YYYY-MM-DD') CS_CLZ_DT
	,TO_CHAR(TO_DATE(A.ARBT_DT, 'YYYYMMDD'),'YYYY-MM-DD') ARBT_DT
	,A.CLMT_CLM_PTY_NO
  ,(SELECT PTY_NM FROM CNI_PARTY WHERE CLM_PTY_NO = A.CLMT_CLM_PTY_NO) CLMT_CLM_PTY_NM
	,A.CLMT_CTNT
	,A.INSUR_CLM_PTY_NO
  ,(SELECT PTY_NM FROM CNI_PARTY WHERE CLM_PTY_NO = A.INSUR_CLM_PTY_NO) INSUR_CLM_PTY_NM
	,TO_CHAR(A.DDCT_USD_AMT,'FM999,999,999,990.00') DDCT_USD_AMT
	,A.DEFT_CLM_PTY_NO
  ,(SELECT PTY_NM FROM CNI_PARTY WHERE CLM_PTY_NO = A.DEFT_CLM_PTY_NO) DEFT_CLM_PTY_NM
	,A.DEFT_CTNT
	,A.LABL_PTY_CLM_PTY_NO
  ,(SELECT PTY_NM FROM CNI_PARTY WHERE CLM_PTY_NO = A.LABL_PTY_CLM_PTY_NO) LABL_PTY_CLM_PTY_NM
	,A.LABL_PTY_CTNT
	,TO_CHAR(TO_DATE(A.LABL_PTY_TM_BAR_DT, 'YYYYMMDD'),'YYYY-MM-DD') LABL_PTY_TM_BAR_DT
	,A.CLM_LOCL_CURR_CD
	,TO_CHAR(A.CLM_LOCL_AMT,'FM999,999,999,990.00') CLM_LOCL_AMT
	,TO_CHAR(TO_DATE(A.FMAL_CLM_RCV_DT, 'YYYYMMDD'),'YYYY-MM-DD') FMAL_CLM_RCV_DT
	,TO_CHAR(A.CLM_XCH_RT,'FM999,990.00000') CLM_XCH_RT
	,TO_CHAR(A.CLM_USD_AMT,'FM999,999,999,990.00') CLM_USD_AMT
	,A.CLM_STL_LOCL_CURR_CD
	,TO_CHAR(A.CLM_STL_LOCL_AMT,'FM999,999,999,990.00') CLM_STL_LOCL_AMT
	,TO_CHAR(TO_DATE(A.CLM_STL_DT, 'YYYYMMDD'),'YYYY-MM-DD') CLM_STL_DT
	,TO_CHAR(A.CLM_STL_XCH_RT,'FM999,990.00000') CLM_STL_XCH_RT
	,TO_CHAR(A.CLM_STL_USD_AMT,'FM999,999,999,990.00') CLM_STL_USD_AMT
	,A.LABL_PTY_FILE_LOCL_CURR_CD
	,TO_CHAR(A.LABL_PTY_FILE_LOCL_AMT,'FM999,999,999,990.00') LABL_PTY_FILE_LOCL_AMT
	,TO_CHAR(TO_DATE(A.LABL_PTY_FILE_DT, 'YYYYMMDD'),'YYYY-MM-DD') LABL_PTY_FILE_DT
	,TO_CHAR(A.LABL_PTY_FILE_XCH_RT,'FM999,990.00000') LABL_PTY_FILE_XCH_RT
	,TO_CHAR(A.LABL_PTY_FILE_USD_AMT,'FM999,999,999,990.00') LABL_PTY_FILE_USD_AMT
	,A.LABL_PTY_RCVR_LOCL_CURR_CD
	,TO_CHAR(A.LABL_PTY_RCVR_LOCL_AMT,'FM999,999,999,990.00') LABL_PTY_RCVR_LOCL_AMT
	,TO_CHAR(TO_DATE(A.LABL_PTY_RCVR_DT, 'YYYYMMDD'),'YYYY-MM-DD') LABL_PTY_RCVR_DT
	,TO_CHAR(A.LABL_PTY_RCVR_XCH_RT,'FM999,990.00000') LABL_PTY_RCVR_XCH_RT
	,TO_CHAR(A.LABL_PTY_RCVR_USD_AMT,'FM999,999,999,990.00') LABL_PTY_RCVR_USD_AMT
	,A.INSUR_FILE_LOCL_CURR_CD
	,TO_CHAR(A.INSUR_FILE_LOCL_AMT,'FM999,999,999,990.00') INSUR_FILE_LOCL_AMT
	,TO_CHAR(TO_DATE(A.INSUR_FILE_DT, 'YYYYMMDD'),'YYYY-MM-DD') INSUR_FILE_DT
	,TO_CHAR(A.INSUR_FILE_XCH_RT,'FM999,990.00000') INSUR_FILE_XCH_RT
	,TO_CHAR(A.INSUR_FILE_USD_AMT,'FM999,999,999,990.00') INSUR_FILE_USD_AMT
	,A.INSUR_RCVR_LOCL_CURR_CD
	,TO_CHAR(A.INSUR_RCVR_LOCL_AMT,'FM999,999,999,990.00') INSUR_RCVR_LOCL_AMT
	,TO_CHAR(TO_DATE(A.INSUR_RCVR_DT, 'YYYYMMDD'),'YYYY-MM-DD') INSUR_RCVR_DT
	,TO_CHAR(A.INSUR_RCVR_XCH_RT,'FM999,990.00000') INSUR_RCVR_XCH_RT
	,TO_CHAR(A.INSUR_RCVR_USD_AMT,'FM999,999,999,990.00') INSUR_RCVR_USD_AMT
	,A.DW_CLM_CS_DESC
	,A.INCI_DEV_DESC
	,A.HDLR_STL_OPIN_DESC
	,A.CLMT_AGN_CLM_PTY_NO
    ,(SELECT PTY_NM FROM CNI_PARTY WHERE CLM_PTY_NO = A.CLMT_AGN_CLM_PTY_NO) CLMT_AGN_CLM_PTY_NM
    ,(SELECT NVL(INTL_PHN_NO,'')||'-'||NVL(PHN_NO,'') FROM CNI_PARTY WHERE CLM_PTY_NO = A.CLMT_AGN_CLM_PTY_NO) CLMT_AGN_TEL_NO
    ,(SELECT PTY_EML FROM CNI_PARTY WHERE CLM_PTY_NO = A.CLMT_AGN_CLM_PTY_NO) CLMT_AGN_EMAIL
	,A.CLMT_AGN_TP_CD
	,TO_CHAR(TO_DATE(A.CLMT_AGN_APNT_DT, 'YYYYMMDD'),'YYYY-MM-DD') CLMT_AGN_APNT_DT
	,A.CLMT_AGN_REF_NO
	,A.DEFT_AGN_CLM_PTY_NO
    ,(SELECT PTY_NM FROM CNI_PARTY WHERE CLM_PTY_NO = A.DEFT_AGN_CLM_PTY_NO) DEFT_AGN_CLM_PTY_NM
    ,(SELECT NVL(INTL_PHN_NO,'')||DECODE(INTL_PHN_NO,NULL,'','-')||NVL(PHN_NO,'') FROM CNI_PARTY WHERE CLM_PTY_NO = A.DEFT_AGN_CLM_PTY_NO) DEFT_AGN_TEL_NO
    ,(SELECT PTY_EML FROM CNI_PARTY WHERE CLM_PTY_NO = A.DEFT_AGN_CLM_PTY_NO) DEFT_AGN_EMAIL
	,A.DEFT_AGN_TP_CD
	,TO_CHAR(TO_DATE(A.DEFT_AGN_APNT_DT, 'YYYYMMDD'),'YYYY-MM-DD') DEFT_AGN_APNT_DT
	,A.DEFT_AGN_REF_NO
    ,A.TRNS_FLG
    ,I.CLM_TRNS_AUTH_CD
    ,I.TRNS_SEQ
	,A.CRE_USR_ID
	,A.CRE_DT
	,A.UPD_USR_ID
	,TO_CHAR(A.UPD_DT, 'YYYY-MM-DD') UPD_DT
    , (SELECT USR_NM FROM COM_USER WHERE USR_ID = A.CRE_USR_ID) R_HANDLER
FROM    CNI_DW_CLM A
    ,   ( SELECT DW_CLM_NO
            , TRNS_SEQ
            , CLM_TRNS_AUTH_CD 
         FROM CNI_DW_TRNS
        WHERE DW_CLM_NO = @[dw_clm_no]
          AND CLM_TRNS_AUTH_CD IN('W','R')
          AND TRNS_SEQ = (SELECT MAX(TRNS_SEQ)
                            FROM CNI_DW_TRNS
                           WHERE DW_CLM_NO = @[dw_clm_no])
      ) I
WHERE	A.DW_CLM_NO = @[dw_clm_no]
  AND   A.DW_CLM_NO = I.DW_CLM_NO(+)			]]></sql>
			<params>
				<param name="dw_clm_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
