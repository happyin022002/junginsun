<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOCheckSccCdMdmYardRSQL">
			<desc><![CDATA[MDM_YARD Check]]></desc>
			<sql><![CDATA[
SELECT  EQ_SCC_CD
  FROM  MDM_YARD
 WHERE  1 = 1
   AND  EQ_SCC_CD = @[scc_cd]
   AND  DELT_FLG <> 'Y' 			]]></sql>
			<params>
				<param name="scc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
