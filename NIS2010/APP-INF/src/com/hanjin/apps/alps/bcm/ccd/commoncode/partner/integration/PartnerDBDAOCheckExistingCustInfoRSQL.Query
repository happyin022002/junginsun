<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOCheckExistingCustInfoRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT DISTINCT A.CNT AS CUST_SEQ 
  FROM (
    SELECT 'SC' CNT
      FROM PRI_SP_CTRT_PTY 
     WHERE CUST_CNT_CD = @[cust_cnt_cd]
       AND CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'RFA' CNT
      FROM PRI_RP_MN
     WHERE CTRT_CUST_CNT_CD = @[cust_cnt_cd]
       AND CTRT_CUST_CNT_CD = @[cust_seq]
    UNION ALL
    SELECT 'SC' CNT
      FROM PRI_SP_AFIL
     WHERE CUST_CNT_CD = @[cust_cnt_cd]
       AND CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'RFA' CNT
      FROM PRI_RP_AFIL
     WHERE CUST_CNT_CD = @[cust_cnt_cd]
       AND CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'Invoice'
      FROM INV_AR_MN
     WHERE ACT_CUST_CNT_CD = @[cust_cnt_cd]
       AND ACT_CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'AR'
      FROM SAR_OTS_HDR
     WHERE BIL_TO_CUST_CNT_CD = @[cust_cnt_cd]
       AND BIL_TO_CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'AR'
      FROM SAR_OTS_HIS
     WHERE BIL_TO_CUST_CNT_CD = @[cust_cnt_cd]
       AND BIL_TO_CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'AR'
      FROM SAR_RECEIPT
     WHERE RCT_CUST_CNT_CD = @[cust_cnt_cd]
       AND RCT_CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'Booking/BL' CNT
      FROM BKG_BOOKING
     WHERE BKG_PTY_CUST_CNT_CD = @[cust_cnt_cd]
       AND BKG_PTY_CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'Booking/BL' CNT
      FROM BKG_BOOKING
     WHERE BKG_CTRL_PTY_CUST_CNT_CD = @[cust_cnt_cd]
       AND BKG_CTRL_PTY_CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'Booking/BL' CNT
      FROM BKG_BOOKING
     WHERE AGMT_ACT_CNT_CD = @[cust_cnt_cd]
       AND AGMT_ACT_CUST_SEQ = @[cust_seq]
    UNION ALL
    SELECT 'Booking/BL' CNT
      FROM BKG_CUSTOMER
     WHERE CUST_CNT_CD = @[cust_cnt_cd]
       AND CUST_SEQ = @[cust_seq]
  ) A			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
