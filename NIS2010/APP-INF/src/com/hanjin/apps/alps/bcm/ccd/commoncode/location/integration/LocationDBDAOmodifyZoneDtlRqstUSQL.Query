<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOmodifyZoneDtlRqstUSQL">
			<desc><![CDATA[Request Zone 정보를 수정합니다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_ZN_DTL_RQST
SET ZIP_CD = @[zip_cd],
    DSTR_NM = @[dstr_nm],
    UPD_USR_ID = @[usr_id],
    UPD_DT = sysdate,
    DELT_FLG = @[delt_flg]
WHERE RQST_NO = @[rqst_no]
AND ZN_SEQ = @[zn_seq]  			]]></sql>
			<params>
				<param name="zip_cd" type="12" value="" out="N"/>
				<param name="dstr_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
				<param name="zn_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
