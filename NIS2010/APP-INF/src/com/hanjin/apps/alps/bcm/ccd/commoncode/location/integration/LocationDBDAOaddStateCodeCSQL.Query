<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOaddStateCodeCSQL">
			<desc><![CDATA[신규 State 정보를 저장합니다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_STATE (
	STE_CD
	, STE_NM
	, CNT_CD
	, CRE_USR_ID
	, CRE_DT
	, UPD_USR_ID
	, UPD_DT
	, DELT_FLG
) VALUES (
	@[ste_cd]
	, @[ste_nm]
	, @[cnt_cd]
	, @[usr_id]
	, sysdate
	, @[usr_id]
	, sysdate
	, @[delt_flg]
)			]]></sql>
			<params>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="ste_nm" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
