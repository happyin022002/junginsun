<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOCheckSccCdMdmLocationRSQL">
			<desc><![CDATA[MDM_LOCATION Check]]></desc>
			<sql><![CDATA[
SELECT  LOC_CD
       ,SCC_CD
  FROM  MDM_LOCATION
 WHERE  1 = 1
   AND  LOC_CD <> @[scc_cd]
   AND  SCC_CD = @[scc_cd]
   AND  DELT_FLG <> 'Y' 			]]></sql>
			<params>
				<param name="scc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
