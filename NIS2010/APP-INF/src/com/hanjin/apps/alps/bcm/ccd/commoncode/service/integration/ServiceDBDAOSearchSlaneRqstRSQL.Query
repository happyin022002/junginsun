<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchSlaneRqstRSQL">
			<desc><![CDATA[Request VSL Code 상세 내용 조회]]></desc>
			<sql><![CDATA[
SELECT VSL.VSL_SLAN_CD 
	  ,VSL.VSL_SLAN_NM
      ,VSL.VSL_SVC_TP_CD
      ,VSL.VSL_TP_CD
      ,VSL.FDR_DIV_CD
      ,TO_CHAR(TO_DATE(VSL.ST_EFF_DT, 'YYYYMMDD'), 'YYYY-MM-DD') ST_EFF_DT
      ,TO_CHAR(TO_DATE(VSL.END_EFF_DT, 'YYYYMMDD'), 'YYYY-MM-DD') END_EFF_DT
      ,VSL.CO_CD
      ,VSL.DELT_FLG
      ,DIR.VSL_SLAN_DIR_CD
      ,DIR.VSL_SLAN_DIR_SEQ
      ,DIR.DELT_FLG DIR_DELT_FLG
      ,VSL.MODI_VSL_SLAN_CD
      ,VSL.MODI_VSL_SLAN_CD2
      ,VSL.MODI_VIP_TEAM_CD
      ,DIR.MODI_VSL_SLAN_DIR_CD
      ,VSL.MODI_COST_CTR_CD
  FROM MDM_VSL_SVC_LANE_RQST VSL
      ,MDM_VSL_SVC_LANE_DIR_RQST DIR
 WHERE VSL.RQST_NO = DIR.RQST_NO(+)
   AND VSL.RQST_NO = @[rqst_no]
 ORDER BY DIR_DELT_FLG, DIR.VSL_SLAN_DIR_SEQ			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
