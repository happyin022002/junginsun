<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchTotVndrListRSQL">
			<desc><![CDATA[Vendor 테이블의 Total Cnt 조회]]></desc>
			<sql><![CDATA[
SELECT COUNT(*) 
  FROM MDM_VENDOR A
 WHERE 1=1
#if (${vndr_cnt_cd} != '')
   AND A.VNDR_CNT_CD = @[vndr_cnt_cd]
#end
#if (${vndr_seq} != '')
   AND A.VNDR_SEQ = @[vndr_seq]
#end
#if (${loc_cd} != '')
   AND A.LOC_CD LIKE @[loc_cd] || '%'
#end
#if (${ofc_cd} != '')
   AND A.OFC_CD LIKE @[ofc_cd] || '%'
#end
#if (${tax_id} != '')
   AND A.TAX_ID LIKE '%' || @[tax_id] || '%'
#end
#if (${vndr_nm} != '')
   AND A.VNDR_NM LIKE '%' || @[vndr_nm] || '%'
#end
#if (${eng_addr} != '')
   AND A.ENG_ADDR LIKE '%' || @[eng_addr] || '%'
#end			]]></sql>
			<params>
				<param name="vndr_cnt_cd" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="loc_cd" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="tax_id" type="12" value="" out="N"/>
				<param name="vndr_nm" type="12" value="" out="N"/>
				<param name="eng_addr" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
