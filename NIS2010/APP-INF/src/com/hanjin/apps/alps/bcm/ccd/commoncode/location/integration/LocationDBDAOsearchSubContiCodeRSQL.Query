<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchSubContiCodeRSQL">
			<desc><![CDATA[Sub Continent 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT SCONTI_CD
	  , CONTI_CD
	  , SCONTI_NM
	  , DELT_FLG
      , CRE_USR_ID
      , CRE_DT
      , UPD_USR_ID
      , UPD_DT
FROM MDM_SUBCONTINENT
WHERE  SCONTI_CD = @[sconti_cd] 			]]></sql>
			<params>
				<param name="sconti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
