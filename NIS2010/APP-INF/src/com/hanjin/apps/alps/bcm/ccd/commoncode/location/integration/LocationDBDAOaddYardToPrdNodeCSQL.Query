<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOaddYardToPrdNodeCSQL">
			<desc><![CDATA[Prd Node에 생성된 Yard 정보를 추가]]></desc>
			<sql><![CDATA[
INSERT INTO prd_node (                                      		
	nod_cd 	       
  , nod_nm 	       
  , nod_tp_cd      
  , loc_cd 	       
  , onf_hir_yd_flg 
  , delt_flg       
  , cre_usr_id     
  , cre_dt 	       
  , upd_usr_id     
  , upd_dt 	       
) VALUES (													 		
    @[yd_cd] 
  , @[yd_nm] 
  , #if(${yd_fcty_tp_psdo_yd_flg} =='Y') 
        'Y'
    #elseif(${yd_fcty_tp_mrn_tml_flg} =='Y')
	    'M'
    #elseif(${yd_fcty_tp_brg_rmp_flg} =='Y')
		'B'
    #elseif(${yd_fcty_tp_rail_rmp_flg} =='Y')
		'R'
    #elseif(${yd_fcty_tp_cy_flg} =='Y')
		'Y'
    #elseif(${yd_fcty_tp_cfs_flg} =='Y')
		'S'
    #elseif(${yd_fcty_tp_psdo_yd_flg} !='Y' && ${yd_fcty_tp_mrn_tml_flg} !='Y' 
            && ${yd_fcty_tp_brg_rmp_flg} !='Y' && ${yd_fcty_tp_rail_rmp_flg} !='Y' 
            && ${yd_fcty_tp_cy_flg} !='Y' && ${yd_fcty_tp_cfs_flg} !='Y' )
		'Y'
    #end
  , SUBSTR(@[yd_cd], 1, 5)
  ,	'N' 
  , @[delt_flg] 
  , @[usr_id] 
  , sysdate
  ,	@[usr_id] 
  , sysdate
)			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
				<param name="yd_nm" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
