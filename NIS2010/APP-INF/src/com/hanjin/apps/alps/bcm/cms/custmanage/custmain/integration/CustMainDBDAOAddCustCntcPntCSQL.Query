<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOAddCustCntcPntCSQL">
			<desc><![CDATA[Add Customer contact point]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CUST_CNTC_PNT(
   CUST_CNT_CD
,  CUST_SEQ
,  CUST_CNTC_PNT_SEQ
,  CUST_EML
,  CUST_IP
,  CUST_URL
,  INTL_PHN_NO
,  PHN_NO
,  INTL_FAX_NO
,  FAX_NO
)VALUES (
	@[cust_cnt_cd]
, @[cust_seq]
,	1
,	@[cust_eml]
,	@[cust_ip]
,	@[cust_url]
,	@[intl_phn_no]
,	@[phn_no]
,	@[intl_fax_no]
,	@[fax_no]
)			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
				<param name="cust_eml" type="12" value="" out="N"/>
				<param name="cust_ip" type="12" value="" out="N"/>
				<param name="cust_url" type="12" value="" out="N"/>
				<param name="intl_phn_no" type="12" value="" out="N"/>
				<param name="phn_no" type="12" value="" out="N"/>
				<param name="intl_fax_no" type="12" value="" out="N"/>
				<param name="fax_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
