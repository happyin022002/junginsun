<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchCustCntcCodeRSQL">
			<desc><![CDATA[Customer Contact Point Code Creation 정보 조회]]></desc>
			<sql><![CDATA[
SELECT  
	   CUST_CNT_CD
	,  TRIM(TO_CHAR(CUST_SEQ,'000000')) CUST_SEQ 
	,  CUST_CNTC_PNT_SEQ
	,  CUST_EML
	,  CUST_IP
	,  CUST_URL
	,  INTL_PHN_NO
	,  PHN_NO
	,  INTL_FAX_NO
	,  FAX_NO
	,  EAI_EVNT_DT
	,  EAI_IF_ID
	,  PRMRY_CHK_FLG
	,  NVL(PAY_RQST_LTR_FLG, 'N') PAY_RQST_LTR_FLG
    ,  CRE_USR_ID
    ,  CRE_DT
    ,  UPD_USR_ID
    ,  UPD_DT
FROM MDM_CUST_CNTC_PNT
WHERE  CUST_CNT_CD =@[cust_cnt_cd]
AND CUST_SEQ = @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
