<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchZoneCodeRSQL">
			<desc><![CDATA[Zone master 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT ZN_CD,
	   ZN_NM,
	   CGO_HNDL_TM_HRS,
	   TZTM_HRS,
	   REP_YD_CD,
	   LNK_DIST,
	   DIST_UT_CD,
	   REP_ZN_FLG,
	   DELT_FLG,
       CRE_USR_ID,
       CRE_DT,
       UPD_USR_ID,
       UPD_DT
FROM MDM_ZONE
WHERE ZN_CD = @[zn_cd]			]]></sql>
			<params>
				<param name="zn_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
