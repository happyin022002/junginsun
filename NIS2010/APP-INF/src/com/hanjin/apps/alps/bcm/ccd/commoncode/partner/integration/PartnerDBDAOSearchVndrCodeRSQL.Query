<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchVndrCodeRSQL">
			<desc><![CDATA[입력된 Vendor 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT TO_CHAR(VNDR_SEQ,'000000') VNDR_SEQ
        ,VNDR_CNT_CD
        ,VNDR_LGL_ENG_NM
        ,VNDR_LOCL_LANG_NM
        ,VNDR_ABBR_NM
        ,LGS_FLG
		,BLK_FLG
        ,PROCU_FLG
        ,FINC_FLG
        ,TEAM_FLG
        ,INTER_CO_FLG
        ,OTR_FLG
        ,LOC_CD
        ,OFC_CD
        ,RGST_NO
        ,TAX_ID
        ,GEN_PAY_TERM_CD
        ,PAY_TERM_TP_CD
        ,PAY_MZD_CD
        ,PRNT_CNT_CD
        ,TRIM(TO_CHAR(PRNT_VNDR_SEQ,'000000')) PRNT_VNDR_SEQ
        ,INV_CURR_CD
        ,CNTC_PSON_NM
        ,CEO_NM
        ,BZCT_NM
        ,BZTP_NM
        ,USA_EDI_CD
        ,SVC_SCP_CD_NM 
        ,SVC_PRD_TP_NM
        ,SVC_PRD_RMK
        ,DCGO_HNDL_FLG
        ,MTY_RRO_EDI_USE_FLG
        ,WO_ATCH_FILE_FLG
        ,WO_EDI_USE_FLG
        ,INV_EDI_USE_FLG
        ,RFND_PSDO_CUST_CD
        ,VNDR_OFC_CD
        ,SUBS_CO_CD
        ,ENG_ADDR
        ,ZIP_CD
        ,LOCL_LANG_ADDR
        ,CHK_DE_ADDR1
        ,CHK_DE_ADDR2
        ,CHK_DE_ADDR3
        ,CHK_DE_CTY_NM
        ,CHK_DE_STE_CD
        ,CHK_DE_ZIP_CD
        ,CHK_DE_CNT_CD
        ,LU_DELT_FLG
        ,DELT_FLG
		, 'Approved' AS MST_RQST_STS_CD
--		,MODI_VNDR_SEQ AS MODI_VNDR_CD
        ,CRE_USR_ID
        ,TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT
        ,UPD_USR_ID
        ,TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT
		,BLK_VNDR_SVC_CD
-- 2018.01.12 삭제       ,SAP_ID
-- 2018.01.12 삭제        ,BANK_ACCT_FLG
FROM MDM_VENDOR
WHERE 1=1
AND VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="11" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
