<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOCheckLocValidationRSQL">
			<desc><![CDATA[Location Validation]]></desc>
			<sql><![CDATA[
SELECT  COUNT(1)
  FROM  MDM_EQ_ORZ_CHT
 WHERE  1 = 1
   AND  DELT_FLG <> 'Y'
   AND  SCC_CD <> @[scc_cd]
#if(${div} == 'RL')
   AND  LCC_CD = @[lcc_cd]
   AND  RCC_CD <> @[rcc_cd]
#elseif(${div} == 'LE')
   AND  ECC_CD = @[ecc_cd]
   AND  LCC_CD <> @[lcc_cd]
#elseif(${div} == 'RE')
   AND  ECC_CD = @[ecc_cd]
   AND  RCC_CD <> @[rcc_cd]
#end 			]]></sql>
			<params>
				<param name="scc_cd" type="12" value="" out="N"/>
				<param name="lcc_cd" type="12" value="" out="N"/>
				<param name="rcc_cd" type="12" value="" out="N"/>
				<param name="ecc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
