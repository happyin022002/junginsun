<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAOAddCustMaxSeqCSQL">
			<desc><![CDATA[새로운 CNT_CD 별 CUST_MAX_SEQ 정보를 추가한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CUST_SEQ_MGMT
( 
	CNT_CD, 
	CUST_MAX_SEQ, 
	CRE_USR_ID, 
	CRE_DT, 
	UPD_USR_ID, 
	UPD_DT
) VALUES ( 
	@[cnt_cd],
	1,
	@[usr_id],
	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]), SYSDATE),
	@[usr_id],
	NVL(GLOBALDATE_PKG.TIME_LOCAL_OFC_FNC(@[ofc_cd]), SYSDATE) 
) 			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
