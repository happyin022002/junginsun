<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOmodifyStateCodeUSQL">
			<desc><![CDATA[State 정보를 수정합니다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_STATE
SET
	STE_NM = @[ste_nm]
	, UPD_USR_ID = @[usr_id]
	, UPD_DT = SYSDATE
	, DELT_FLG = @[delt_flg]
WHERE 1 = 1
AND STE_CD = @[ste_cd]
AND CNT_CD = @[cnt_cd]			]]></sql>
			<params>
				<param name="ste_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="ste_cd" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
