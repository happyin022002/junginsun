<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchZoneDtlRqstRSQL">
			<desc><![CDATA[Request Zone detail 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT ZN_CD,
	   ZN_SEQ,
	   ZIP_CD,
	   DSTR_NM,
  	   DELT_FLG
FROM MDM_ZN_DTL_RQST
WHERE RQST_NO = @[rqst_no]  			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
