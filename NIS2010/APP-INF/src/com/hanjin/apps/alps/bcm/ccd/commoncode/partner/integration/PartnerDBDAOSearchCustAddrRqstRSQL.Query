<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchCustAddrRqstRSQL">
			<desc><![CDATA[MDM_CUST_ADDR_RQST 테이블정보를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT  CUST_CNT_CD 
       ,CUST_SEQ
       ,ADDR_TP_CD
       ,ADDR_SEQ
       ,PRMRY_CHK_FLG
       ,BZET_NM
       ,BZET_ADDR
       ,CTY_NM
       ,STE_CD
       ,ZIP_CD
       ,CNTC_EML
       ,CNTC_PSON_NM
       ,BZET_RMK
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
       ,DELT_FLG
       ,EAI_EVNT_DT
       ,CRM_ROW_ID
       ,LOCL_ADDR1
       ,LOCL_ADDR2
       ,LOCL_ADDR3
       ,LOCL_ADDR4
       ,CNT_CD
       ,EAI_IF_ID
FROM   MDM_CUST_ADDR_RQST
WHERE  RQST_NO = @[rqst_no]			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
