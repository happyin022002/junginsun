<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOModifyRqstVndrRjctUSQL">
			<desc><![CDATA[Vendor Create Request에 대한 Reject 정보를 저장한다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_VNDR_RQST 
   SET MST_RQST_STS_CD = 'R',
	   RJCT_RSN_RMK = @[rjct_rsn_rmk],
       RJCT_RSN_CD = @[rjct_rsn_cd],
	   VNDR_SEQ = null
 WHERE 1=1
   AND MDM_VNDR_RQST_SEQ = @[rqst_no]			]]></sql>
			<params>
				<param name="rjct_rsn_rmk" type="12" value="" out="N"/>
				<param name="rjct_rsn_cd" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
