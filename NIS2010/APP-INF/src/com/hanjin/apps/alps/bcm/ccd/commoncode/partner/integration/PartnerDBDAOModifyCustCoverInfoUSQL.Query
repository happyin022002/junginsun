<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOModifyCustCoverInfoUSQL">
			<desc><![CDATA[ModifyCustCoverInfo]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUSTOMER 
SET SREP_CD = @[srep_cd],
    UPD_USR_ID = @[user_id],
    UPD_DT = SYSDATE
WHERE CUST_CNT_CD = @[cust_cnt_cd]
AND CUST_SEQ = TO_NUMBER(@[cust_seq])			]]></sql>
			<params>
				<param name="srep_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
