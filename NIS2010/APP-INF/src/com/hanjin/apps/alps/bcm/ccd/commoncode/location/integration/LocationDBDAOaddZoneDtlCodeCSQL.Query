<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOaddZoneDtlCodeCSQL">
			<desc><![CDATA[신규 Zone Detail 정보를 저장합니다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_ZN_DTL ( 
  ZN_CD,
  ZN_SEQ,
  ZIP_CD,
  DSTR_NM,
  CRE_USR_ID,
  CRE_DT,
  UPD_USR_ID,
  UPD_DT,
  DELT_FLG
) VALUES (
  @[zn_cd],
  (SELECT DECODE (MAX(ZN_SEQ), NULL, 1, MAX(ZN_SEQ)+1) FROM MDM_ZN_DTL WHERE ZN_CD = @[zn_cd]),
  @[zip_cd],
  @[dstr_nm],
  @[usr_id],
  sysdate,
  @[usr_id],
  sysdate,
  @[delt_flg]
) 			]]></sql>
			<params>
				<param name="zn_cd" type="12" value="" out="N"/>
				<param name="zip_cd" type="12" value="" out="N"/>
				<param name="dstr_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
