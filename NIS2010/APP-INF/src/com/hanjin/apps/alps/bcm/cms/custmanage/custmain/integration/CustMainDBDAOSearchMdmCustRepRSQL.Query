<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOSearchMdmCustRepRSQL">
			<desc><![CDATA[MDM Customer Rep search]]></desc>
			<sql><![CDATA[
SELECT MCR.CUST_CNT_CD
       ,MCR.CUST_SEQ
       ,MCR.OFC_CD
       ,MCR.IO_BND_CD
       ,MCR.AUTO_INV_FLG
       ,MCR.HJS_CUST_SVC_PIC_TP_CD
       ,MCR.HJS_REF_NO
       ,MCR.DELT_FLG
       ,MCR.CUST_REF_NO_FLG
       ,MCR.LOCL_CHG_FLG
       ,MCR.AUTO_INV_EML
       ,MCR.CRE_USR_ID
       ,MCR.CRE_DT
       ,MCR.UPD_USR_ID
       ,MCR.UPD_DT
  FROM MDM_CUST_REP	MCR
 WHERE cust_cnt_cd  = @[cust_cnt_cd]
   AND cust_seq 	= @[cust_seq]			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
