<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOCreateMdmCustRepCSQL">
			<desc><![CDATA[Create MDM Customer rep]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CUST_REP (
	cust_cnt_cd
	,cust_seq
	,ofc_cd
	,io_bnd_cd
	,auto_inv_flg
	,hjs_cust_svc_pic_tp_cd
