<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchCntrVndrClssToEaiRSQL">
			<desc><![CDATA[EAI 전송을 위한 Vendor Classification 정보 조회]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ
    ,VNDR_COST_CD
    ,CNTR_VNDR_SVC_CD
    ,DELT_FLG
    ,CRE_USR_ID
    ,TO_CHAR(CRE_DT, 'YYYYMMDDHH24MISS') AS CRE_DT
    ,UPD_USR_ID
    ,TO_CHAR(UPD_DT, 'YYYYMMDDHH24MISS') AS UPD_DT
FROM MDM_CNTR_VNDR_CLSS
WHERE 1=1
AND VNDR_SEQ = @[vndr_seq]
AND VNDR_COST_CD = @[vndr_cost_cd]
AND CNTR_VNDR_SVC_CD = @[cntr_vndr_svc_cd]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
				<param name="vndr_cost_cd" type="12" value="" out="N"/>
				<param name="cntr_vndr_svc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
