<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOAddSlaneCodeCSQL">
			<desc><![CDATA[VSL cd를 생성]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_VSL_SVC_LANE
           (VSL_SLAN_CD
           ,VSL_SLAN_NM
           ,VSL_SVC_TP_CD
           ,VSL_TP_CD
           ,FDR_DIV_CD
		#if (${st_eff_dt} != '')
		   ,ST_EFF_DT
		#end
		#if (${end_eff_dt} != '')		
		   ,END_EFF_DT
		#end
		   ,CO_CD
		   ,MODI_COST_CTR_CD
           ,CRE_USR_ID
           ,CRE_DT
           ,UPD_USR_ID
           ,UPD_DT
           ,DELT_FLG
           ,MODI_VSL_SLAN_CD
		   ,MODI_VSL_SLAN_CD2
           ,MODI_VIP_TEAM_CD
           )
VALUES(@[vsl_slan_cd]
      ,@[vsl_slan_nm]
      ,@[vsl_svc_tp_cd]
      ,@[vsl_tp_cd]
      ,@[fdr_div_cd]
	#if (${st_eff_dt} != '')
      ,TO_CHAR(TO_DATE(@[st_eff_dt], 'YYYY-MM-DD'), 'YYYYMMDD')
	#end
	#if (${end_eff_dt} != '')
      ,TO_CHAR(TO_DATE(@[end_eff_dt], 'YYYY-MM-DD'), 'YYYYMMDD')
	#end
      ,@[co_cd]
	  ,@[modi_cost_ctr_cd]
      ,@[user_id]
      ,SYSDATE
      ,@[user_id]
      ,SYSDATE
      ,@[delt_flg]
      ,@[modi_vsl_slan_cd]
      ,@[modi_vsl_slan_cd2]
      ,@[modi_vip_team_cd]
      )			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_nm" type="12" value="" out="N"/>
				<param name="vsl_svc_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_div_cd" type="12" value="" out="N"/>
				<param name="st_eff_dt" type="12" value="" out="N"/>
				<param name="end_eff_dt" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
				<param name="modi_cost_ctr_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="modi_vsl_slan_cd" type="12" value="" out="N"/>
				<param name="modi_vsl_slan_cd2" type="12" value="" out="N"/>
				<param name="modi_vip_team_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
