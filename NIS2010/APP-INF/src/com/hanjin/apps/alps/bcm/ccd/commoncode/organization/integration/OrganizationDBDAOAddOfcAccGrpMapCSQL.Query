<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="OrganizationDBDAOAddOfcAccGrpMapCSQL">
			<desc><![CDATA[입력한 Office Access Group Mapping 정보를 등록한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_OFC_GRP_MAPG(
            SUB_SYS_CD
           ,OFC_GRP_ID 
           ,OFC_CD
           ,CRE_USR_ID
           ,CRE_DT
           ,UPD_USR_ID
           ,UPD_DT
           )
    VALUES(@[sub_sys_cd]
          ,@[ofc_grp_id]
          ,@[ofc_cd]
          ,@[cre_usr_id]
          ,SYSDATE
          ,@[upd_usr_id]
          ,SYSDATE
          ) 			]]></sql>
			<params>
				<param name="sub_sys_cd" type="12" value="" out="N"/>
				<param name="ofc_grp_id" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
