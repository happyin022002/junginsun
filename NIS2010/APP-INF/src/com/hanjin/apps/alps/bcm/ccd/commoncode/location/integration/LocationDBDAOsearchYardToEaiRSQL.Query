<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchYardToEaiRSQL">
			<desc><![CDATA[EAI 전송을 위한 Yard Main정보 조회]]></desc>
			<sql><![CDATA[
SELECT YD_CD
	 , YD_NM
	 , LOC_CD
     , N1ST_VNDR_CNT_CD
     , N1ST_VNDR_SEQ
     , N2ND_VNDR_CNT_CD
	 , N2ND_VNDR_SEQ
     , N3RD_VNDR_CNT_CD
     , N3RD_VNDR_SEQ
	 , OFC_CD
     , HUB_YD_FLG 
     , YD_CHR_CD
	 , YD_FCTY_TP_MRN_TML_FLG
     , YD_FCTY_TP_CY_FLG
     , YD_FCTY_TP_CFS_FLG
     , YD_FCTY_TP_RAIL_RMP_FLG
	 , YD_OSHP_CD       
     , BD_YD_FLG
     , YD_ADDR
     , ZIP_CD
     , INTL_PHN_NO
     , PHN_NO
     , FAX_NO
     , YD_PIC_NM
     , YD_CEO_NM
     , GATE_OPN_HRMNT
     , GATE_CLZ_HRMNT
     , HOL_GATE_OPN_HRMNT
     , HOL_GATE_CLZ_HRMNT
     , SUN_GATE_OPN_HRMNT
     , SUN_GATE_CLZ_HRMNT
     , SAT_GATE_OPN_HRMNT
     , SAT_GATE_CLZ_HRMNT
     , YD_RMK
     , BRTH_NO
     , YD_BRTH_LEN
     , YD_BRTH_ALNG_SD_DESC
     , YD_BRTH_DPTH_CHNL_KNT
     , YD_TTL_SPC
     , YD_ACT_SPC
	 , YD_HJS_SPC
     , YD_HNDL_CAPA
     , YD_RF_RCPT_440V_KNT
     , YD_RF_RCPT_220V_KNT
     , YD_RF_RCPT_DUL_KNT
     , YD_OP_SYS_CD
	 , YD_CFS_SPC
     , MNR_SHOP_FLG
     , YD_HNDL_YR
     , YD_TTL_VOL_TEU_KNT
     , YD_TTL_VOL_BX_KNT
	 , YD_HJS_VOL_TEU_KNT
	 , YD_HJS_VOL_BX_KNT
     , TRSTR_KNT
     , FRK_KNT
     , YD_STRDL_CRR_KNT
     , YD_TRCT_KNT
     , YD_TOP_LFT_KNT
     , TML_CHSS_KNT
     , EIR_SVC_FLG
     , TML_PROD_KNT
     , YD_CSTMS_NO
     , YD_FCTY_TP_PSDO_YD_FLG
     , YD_EML
     , DEM_IB_CLT_FLG
     , DEM_OB_CLT_FLG
	 , BKG_POD_YD_FLG
	 , YD_FCTY_TP_BRG_RMP_FLG
	 , BFR_OFC_CD
	 , TO_CHAR(BFR_OFC_CNG_DT, 'YYYYMMDDHH24MISS') AS BFR_OFC_CNG_DT
     , MODI_YD_CD
     , DMDT_OFC_CD
     , CRE_USR_ID
     , TO_CHAR(CRE_DT, 'YYYYMMDDHH24MISS') AS CRE_DT
     , UPD_USR_ID
     , TO_CHAR(UPD_DT, 'YYYYMMDDHH24MISS') AS UPD_DT
     , DELT_FLG
	 , REP_YD_TP_CD
	 , DRY_AVG_DWLL_HRS
	 , DRY_MIN_DWLL_HRS
	 , DRY_YD_FT_HRS
     , YD_LOCL_LANG_NM
     , YD_LOCL_LANG_ADDR
	 , YD_CGO_CLZ_HRMNT_MSG   
	 , YD_INRL_FLG            
	 , ONF_HIR_YD_FLG         
	 , YD_PST_PGC_KNT         
	 , YD_PGC_KNT             
	 , REP_ZN_CD              
	 , RF_AVG_DWLL_HRS        
	 , RF_MIN_DWLL_HRS        
	 , RF_YD_FT_HRS           
	 , TO_CHAR(EAI_EVNT_DT, 'YYYYMMDDHH24MISS') AS EAI_EVNT_DT            
	 , EAI_IF_ID              
FROM MDM_YARD
WHERE YD_CD = @[yd_cd] 			]]></sql>
			<params>
				<param name="yd_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
