<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOModifySlaneDirCodeUSQL">
			<desc><![CDATA[VSL DIR cd 내용 수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_VSL_SVC_LANE_DIR
   SET DELT_FLG = @[dir_delt_flg]
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
      ,VSL_SLAN_DIR_CD = @[vsl_slan_dir_cd]
      --,MODI_VSL_SLAN_DIR_CD = [modi_vsl_slan_dir_cd]
 WHERE VSL_SLAN_CD = @[vsl_slan_cd]
   AND VSL_SLAN_DIR_SEQ = @[vsl_slan_dir_seq]			]]></sql>
			<params>
				<param name="dir_delt_flg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
