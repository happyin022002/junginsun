<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountDBDAOAddRepChgCodeCSQL">
			<desc><![CDATA[2011.02.21 조인영 Rep Charge Code 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_REP_CHG (
 		REP_CHG_CD
       ,REP_CHG_NM
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
       ,DELT_FLG
       ,EAI_EVNT_DT
       ,EAI_IF_ID
) VALUES (
 		@[rep_chg_cd]
       ,@[rep_chg_nm]
       ,@[cre_usr_id]
       ,sysdate
       ,@[cre_usr_id]
       ,sysdate
       ,@[delt_flg]
       ,@[eai_evnt_dt]
       ,@[eai_if_id]

)			]]></sql>
			<params>
				<param name="rep_chg_cd" type="12" value="" out="N"/>
				<param name="rep_chg_nm" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
