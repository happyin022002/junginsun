<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchSubTrdCodeRSQL">
			<desc><![CDATA[sub trd code 조회]]></desc>
			<sql><![CDATA[
SELECT SUB_TRD_NM
      ,TRD_CD
      ,DELT_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
  FROM MDM_SUB_TRD
 WHERE SUB_TRD_CD = @[sub_trd_cd]			]]></sql>
			<params>
				<param name="sub_trd_cd" type="12" value="EM" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
