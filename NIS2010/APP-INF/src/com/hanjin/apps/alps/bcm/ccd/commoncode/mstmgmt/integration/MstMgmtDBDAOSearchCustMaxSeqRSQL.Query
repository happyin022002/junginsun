<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAOSearchCustMaxSeqRSQL">
			<desc><![CDATA[MDM_CUST_SEQ_MGMT 테이블에서 CNT_CD별 CUST_MAX_SEQ 값을 조회한다.]]></desc>
			<sql><![CDATA[
SELECT LPAD(NVL(MAX(CUST_MAX_SEQ), 0) + 1, 6, '0') CUST_MAX_SEQ
  FROM MDM_CUST_SEQ_MGMT
 WHERE CNT_CD = @[cnt_cd] 			]]></sql>
			<params>
				<param name="cnt_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
