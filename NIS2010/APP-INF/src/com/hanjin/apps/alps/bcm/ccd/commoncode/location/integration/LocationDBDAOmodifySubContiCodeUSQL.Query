<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOmodifySubContiCodeUSQL">
			<desc><![CDATA[Sub Continent 정보를 수정합니다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_SUBCONTINENT
SET
	CONTI_CD = @[conti_cd]
	, SCONTI_NM = @[sconti_nm]
	, UPD_USR_ID = @[usr_id]
	, UPD_DT = SYSDATE
	, DELT_FLG = @[delt_flg]
WHERE
	SCONTI_CD = @[sconti_cd]  			]]></sql>
			<params>
				<param name="conti_cd" type="12" value="" out="N"/>
				<param name="sconti_nm" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="sconti_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
