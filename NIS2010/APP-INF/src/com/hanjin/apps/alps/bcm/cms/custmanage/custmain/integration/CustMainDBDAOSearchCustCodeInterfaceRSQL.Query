<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOSearchCustCodeInterfaceRSQL">
			<desc><![CDATA[Interface]]></desc>
			<sql><![CDATA[
SELECT MC.Cust_Cnt_Cd         
      ,MC.Cust_Seq           
      ,Cntr_Div_Flg        
      ,Blk_Div_Flg         
      ,CUST_GRP_ID Cust_Grp_Cd         
      ,Cust_Lgl_Eng_Nm      
      ,Cust_Locl_Lang_Nm    
      ,Cust_Abbr_Nm        
      ,Rvis_Cntr_Cust_Tp_Cd  
      ,Blk_Cust_Tp_Cd       
      ,'' Dmst_Frgn_Div_Cd     
      ,Indiv_Corp_Div_Cd
      ,Ofc_Cd Mst_Ofc_Id          
      ,Ofc_Cd             
      ,'' Blk_Cnt_Cd          
      ,Cust_Ip            
      ,Cust_Eml           
      ,Cust_Url           
      ,MCC.Ownr_Nm            
      ,MCC.Bzct_Nm            
      ,MCC.BZTP_NM Bztp_Desc          
      ,Fndt_Dt            
      ,Cust_Rgst_No        
      ,'' Cust_Clss_Lvl_Cd     
      ,Finc_Sts_Lvl_Cd      
      ,MC.Loc_Cd             
      ,Capi_Curr_Cd        
      ,Capi_Amt           
      ,'' Sell_Curr_Cd        
      ,'' Sell_Amt           
      ,Lstk_Flg           
      ,Empe_Knt           
      ,'' N1_St_Bkg_Dt         
      ,rfnd_psdo_vndr_seq N1_St_Bkg_No         
      ,'' N1_St_Bkg_No_Split    
      ,Vndr_Seq           
      ,Cust_Rmk           
      ,VBS_CLSS_CD Val_Bse_Segm_Clss_Cd  
      ,NBS_CLSS_CD1 Nds_Bse_Segm_Clss_Cd1 
      ,NBS_CLSS_CD2 Nds_Bse_Segm_Clss_Cd2 
      ,NBS_CLSS_CD3 Nds_Bse_Segm_Clss_Cd3 
      ,MCCP.INTL_PHN_NO Mn_Intl_Phn_No       
      ,'' Mn_Arct_Pnn_No       
      ,'' Mn_Phn_No           
      ,MCCP.PHN_NO Mn_Full_Phn_No       
      ,MCCP.INTL_FAX_NO Mn_Intl_Fax_No       
      ,'' Mn_Arct_Fax_No       
      ,'' Mn_Fax_No           
      ,MCCP.FAX_NO Mn_Full_Fax_No       
      ,Cust_Sts_Cd         
      ,MC.Crm_Row_Id          
      ,Nvocc_Hjs_Scac_Cd    
      ,Nvocc_Bd_No         
      ,Nvocc_Lic_No        
      ,Nvocc_Bd_Amt        
      ,TO_CHAR(NVOCC_BD_ST_EFF_DT,'YYYYMMDDHH24MISS') Nvocc_Bd_Strt_Eff_Dt  
      ,TO_CHAR(Nvocc_Bd_End_Eff_Dt,'YYYYMMDDHH24MISS')   Nvocc_Bd_End_Eff_Dt
      ,Frt_Fwrd_Fmc_No      
      ,'' Frt_Fwrd_Eff_Dt      
      ,'' Fmc_No_Cd           
      ,Indus_Desc         
      ,Crnt_Vol_Knt        
      ,CMPT_DESC Cpeti_Desc         
      ,Spcl_Req_Desc       
      ,Prf_Svc_Desc        
      ,Prf_Svc_Dtl_Desc     
      ,Prf_Grp_Cmdt_Cd      
      ,PRF_REP_CMDT_CD Prf_Repre_Cmdt_Cd    
      ,Prf_Cntr_Tpsz_Cd     
      ,Srep_Cd            
      ,Cts_No             
      ,'' Bfr_Ofc_Id          
      ,'' Bfr_Ofc_Cd          
      ,'' Bfr_Ofc_Chng_Dt      
      ,MC.Cre_Usr_Id          
      ,TO_CHAR(MC.Cre_Dt,'YYYYMMDDHH24MISS')   Cre_Dt           
      ,MC.Upd_Usr_Id          
      ,TO_CHAR(MC.Upd_Dt,'YYYYMMDDHH24MISS')   Upd_Dt          
      ,DECODE(SLS_DELT_EFF_DT, NULL, 'N', 'Y') Delt_Flg           
      ,Modi_Cust_Cnt_Cd     
      ,Modi_Cust_Seq       
      ,'' O_Mst_Ofc_Id         
      ,'' O_Ofc_Cd            
      ,'' Rtn_Eml            
      ,MCA.CRM_ROW_ID Cust_Addr_Row_Id     
      ,DECODE(MCA.DELT_FLG, NULL, 'N', 'N','N', 'Y') Cust_Addr_Sts_Cd     
      ,MCA.ADDR_TP_CD Addr_Tp_Cd          
      ,MCA.PRMRY_CHK_FLG Prmry_Chk_Flg       
      ,MCA.BZET_ADDR Bzet_Addr          
      ,MCA.CTY_NM Cty_Nm             
      ,MCA.STE_CD Ste_Cd             
      ,MCA.ZIP_CD Zip_Cd             
      ,MCA.CNT_CD Cnt_Cd             
      ,'' Ssidi_Co_Cd         
      ,Key_Acct_Flg        
      ,TO_CHAR(KEY_ACCT_ST_EFF_DT, 'YYYYMMDD') Key_Acct_Strt_Eff_Dt  
      ,TO_CHAR(KEY_ACCT_END_EFF_DT, 'YYYYMMDD') Key_Acct_End_Eff_Dt   
      ,'' Delt_Eff_Dt         
      ,'' Sls_Delt_Flg        
      ,'' Key_Acct_Mgr_Glo_Usr_Id
      ,'' Key_Acct_Mgr_Glo_Usr_Nm
      ,Addr_Seq           
      ,'' Sub_S_Co_Fm_Dt        
      ,'' Sub_S_Co_To_Dt       
      ,Bkg_Alt_Rsn         
      ,TO_CHAR(Bkg_Alt_Fm_Dt,'YYYYMMDDHH24MISS')  Bkg_Alt_Fm_Dt      
      ,TO_CHAR(Bkg_Alt_To_Dt,'YYYYMMDDHH24MISS')  Bkg_Alt_To_Dt      
      ,Bkg_Alt_Msg         
      ,Bkg_Alt_Cre_Usr_Id    
      ,TO_CHAR(Bkg_Alt_Cre_Dt,'YYYYMMDDHH24MISS') Bkg_Alt_Cre_Dt
      ,Nmd_Cust_Flg        
      ,Mlt_Trd_Acct_Flg     
      ,Oti_Orz_No          
      ,Rf_Acct_Flg         
      ,Sls_Delt_Eff_Rsn_Cd   
      ,New_Key_Acct_Flg    
      ,Glo_Acct_Flg        
      ,Rgn_Acct_Flg               
FROM MDM_CUSTOMER MC
     , MDM_LOCATION ML
     , MDM_CUST_CNTC_PNT MCCP
     , (SELECT * FROM MDM_CUST_ADDR MCA WHERE MCA.CUST_CNT_CD = @[cust_cnt_cd] AND MCA.CUST_SEQ = @[cust_seq] AND MCA.ADDR_TP_CD='1' AND MCA.PRMRY_CHK_FLG='Y' AND ROWNUM = 1) MCA
     , MDM_CR_CUST MCC
WHERE MC.CUST_CNT_CD = @[cust_cnt_cd]
AND MC.CUST_SEQ = @[cust_seq]
AND MC.LOC_CD = ML.LOC_CD
AND MC.CUST_CNT_CD = MCCP.CUST_CNT_CD(+)
AND MC.CUST_SEQ = MCCP.CUST_SEQ(+)
AND MCCP.CUST_CNTC_PNT_SEQ(+) = 1
AND MC.CUST_CNT_CD = MCA.CUST_CNT_CD(+)
AND MC.CUST_SEQ = MCA.CUST_SEQ(+)
AND MC.CUST_CNT_CD <> 'TB'
AND MC.CUST_CNT_CD = MCC.CUST_CNT_CD(+)
AND MC.CUST_SEQ = MCC.CUST_SEQ(+)			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
