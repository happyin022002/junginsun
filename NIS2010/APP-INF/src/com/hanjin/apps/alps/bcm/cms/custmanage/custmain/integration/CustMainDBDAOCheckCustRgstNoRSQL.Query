<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOCheckCustRgstNoRSQL">
			<desc><![CDATA[Customer register no check]]></desc>
			<sql><![CDATA[
SELECT MC.CUST_RGST_NO
FROM MDM_CUSTOMER MC
WHERE MC.CUST_CNT_CD = 'KR'
AND MC.CUST_RGST_NO = @[cust_rgst_no]

			]]></sql>
			<params>
				<param name="cust_rgst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
