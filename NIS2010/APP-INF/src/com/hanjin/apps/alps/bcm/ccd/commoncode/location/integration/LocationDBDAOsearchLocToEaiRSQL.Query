<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="LocationDBDAOsearchLocToEaiRSQL">
			<desc><![CDATA[EAI Inteface를 위한 Location Info 조회]]></desc>
			<sql><![CDATA[
SELECT LOC_CD
  	 , SCC_CD
	 , LOC_NM
	 , RGN_CD
 	 , CNT_CD
	 , STE_CD
	 , CONTI_CD
	 , SCONTI_CD
	 , PORT_INLND_CD
	 , LOC_CHR_CD
	 , BLK_PORT_FLG
	 , HUB_LOC_CD
	 , SLS_OFC_CD
	 , LOC_GRD_NO
	 , GMT_HRS
	 , BKG_BL_PFX_CD
	 , CALL_PORT_FLG
	 , LOC_AMS_PORT_CD
	 , FINC_CTRL_OFC_CD
	 , EQ_CTRL_OFC_CD
	 , MTY_PKUP_YD_CD
	 , SEN_EQ_OFC_CD
	 , EQ_RTN_YD_CD
	 , UN_LOC_IND_CD
	 , UN_LOC_CD
	 , CML_ZN_FLG
	 , CSTMS_CD
	 , LOC_TP_CD
	 , BFR_FINC_CTRL_OFC_CD
	 , BFR_EQ_CTRL_OFC_CD
	 , BFR_SLS_OFC_CD
	 , TO_CHAR(BFR_OFC_CNG_DT, 'YYYYMMDDHH24MISS') AS BFR_OFC_CNG_DT
     , ZIP_CD
     , CRE_USR_ID
     , TO_CHAR(CRE_DT, 'YYYYMMDDHH24MISS') AS CRE_DT
     , UPD_USR_ID
     , TO_CHAR(UPD_DT, 'YYYYMMDDHH24MISS') AS UPD_DT
	 , DELT_FLG
	 , REP_ZN_CD
	 , LOC_LOCL_LANG_NM
	 , LOC_LAT
	 , LAT_UT_CD
	 , LOC_LON
	 , LON_UT_CD
	 , BFR_SLS_OFC_CD
	 , TO_CHAR(EAI_EVNT_DT, 'YYYYMMDDHH24MISS') AS EAI_EVNT_DT
	 , EAI_IF_ID
FROM MDM_LOCATION
WHERE LOC_CD = @[loc_cd] 			]]></sql>
			<params>
				<param name="loc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
