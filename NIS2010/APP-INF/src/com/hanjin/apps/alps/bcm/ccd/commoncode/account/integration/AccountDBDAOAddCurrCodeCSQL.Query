<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountDBDAOAddCurrCodeCSQL">
			<desc><![CDATA[2011.02.21 조인영 Currency Code 정보를 생성한다.]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CURRENCY (
		CURR_CD
       ,CURR_NM
       ,CURR_DESC
       ,CNT_CD
       ,FM_EFF_DT
       ,TO_EFF_DT
       ,DP_PRCS_KNT
       ,XTD_PRCS_KNT
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
       ,DELT_FLG
       ,EAI_EVNT_DT
       ,EAI_IF_ID
) VALUES (
		@[curr_cd]
       ,@[curr_nm]
       ,@[curr_desc]
       ,@[cnt_cd]
       ,TO_DATE(TRIM(REPLACE(@[fm_eff_dt], '-', '')),'YYYYMMDD')   
       ,TO_DATE(TRIM(REPLACE(@[to_eff_dt], '-', '')),'YYYYMMDD')
       ,@[dp_prcs_knt]
       ,@[xtd_prcs_knt]
       ,@[cre_usr_id]
       ,sysdate
       ,@[cre_usr_id]
       ,sysdate
       ,@[delt_flg]
       ,@[eai_evnt_dt]
       ,@[eai_if_id]

)			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
				<param name="curr_nm" type="12" value="" out="N"/>
				<param name="curr_desc" type="12" value="" out="N"/>
				<param name="cnt_cd" type="12" value="" out="N"/>
				<param name="fm_eff_dt" type="12" value="" out="N"/>
				<param name="to_eff_dt" type="12" value="" out="N"/>
				<param name="dp_prcs_knt" type="12" value="" out="N"/>
				<param name="xtd_prcs_knt" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="eai_evnt_dt" type="12" value="" out="N"/>
				<param name="eai_if_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
