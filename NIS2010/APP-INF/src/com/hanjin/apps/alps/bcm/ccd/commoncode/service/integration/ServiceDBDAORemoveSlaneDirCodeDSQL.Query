<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAORemoveSlaneDirCodeDSQL">
			<desc><![CDATA[VSL DIR cd 완전 삭제]]></desc>
			<sql><![CDATA[
DELETE FROM MDM_VSL_SVC_LANE_DIR
 WHERE VSL_SLAN_CD = @[vsl_slan_cd]
   AND VSL_SLAN_DIR_CD = @[vsl_slan_dir_cd]			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
