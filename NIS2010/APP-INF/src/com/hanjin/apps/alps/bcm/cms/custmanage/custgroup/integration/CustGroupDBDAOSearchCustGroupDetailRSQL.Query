<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustGroupDBDAOSearchCustGroupDetailRSQL">
			<desc><![CDATA[Group Customer Detail]]></desc>
			<sql><![CDATA[
SELECT MC.CUST_CNT_CD
     , MC.CUST_SEQ
     , MC.CUST_CNT_CD||TRIM(TO_CHAR(MC.CUST_SEQ,'000000')) CUST_CD
     , MC.CUST_LGL_ENG_NM
     , MC.LOC_CD
     , MC.SREP_CD
     , MC.OFC_CD
     , DECODE(MC.CUST_CNT_CD||MC.CUST_SEQ,MCPG.PRMRY_CUST_CNT_CD||MCPG.PRMRY_CUST_SEQ,'Y','N') PRMRY_CHK_FLG
FROM MDM_CUSTOMER MC
	 ,MDM_CUST_PERF_GRP MCPG
WHERE 1=1
AND MC.CUST_GRP_ID = @[cust_grp_id]
AND MC.CUST_GRP_ID = MCPG.CUST_GRP_ID			]]></sql>
			<params>
				<param name="cust_grp_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
