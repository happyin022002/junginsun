<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CommodityDBDAOSearchPackageTypeRSQL">
			<desc><![CDATA[2011.03.08 조인영 Package Type 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT PCK_CD
      ,PCK_NM
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
      ,DELT_FLG
FROM   MDM_PCK_TP
WHERE  PCK_CD = @[pck_cd]			]]></sql>
			<params>
				<param name="pck_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
