<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchScpLimitCodeRSQL">
			<desc><![CDATA[ScpLimitCode 조회]]></desc>
			<sql><![CDATA[
SELECT RGN_CD
      ,ORG_DEST_CD
      ,SVC_SCP_IND_FLG
      ,DELT_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
	  ,SVC_SCP_SCNT
  FROM MDM_SVC_SCP_LMT
 WHERE SVC_SCP_CD = @[svc_scp_cd]
 ORDER BY DELT_FLG, RGN_CD, ORG_DEST_CD, SVC_SCP_IND_FLG			]]></sql>
			<params>
				<param name="svc_scp_cd" type="12" value="ACE" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
