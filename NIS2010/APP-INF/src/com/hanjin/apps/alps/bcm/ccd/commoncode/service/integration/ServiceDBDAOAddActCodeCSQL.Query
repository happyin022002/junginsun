<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOAddActCodeCSQL">
			<desc><![CDATA[새로운 ACTIVITY CODE를 생성하는 쿼리]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_ACTIVITY
           (ACT_CD
           ,ACT_NM
           ,ACT_DESC
		#if (${act_tp_cd} != '')
           ,ACT_TP_CD
		#end
		#if (${full_mty_cd} != '')
           ,FULL_MTY_CD
		#end
		#if (${bnd_vskd_seq_cd} != '')
           ,BND_VSKD_SEQ_CD
		#end
		#if (${nod_tp_cd} != '')
           ,NOD_TP_CD
		#end
		#if (${act_op_tp_cd} != '')
           ,ACT_OP_TP_CD
		#end
		#if (${trsp_mod_cd} != '')
           ,TRSP_MOD_CD
		#end
		#if (${org_dest_cd} != '')
           ,ORG_DEST_CD
		#end
		#if (${act_flg} != '')
           ,ACT_FLG
		#end
		#if (${act_stnd_edi_sts_cd} != '')
           ,ACT_STND_EDI_STS_CD
		#end
		#if (${cop_skd_lgc_no} != '')
           ,COP_SKD_LGC_NO
		#end
           ,DELT_FLG
           ,CRE_USR_ID
           ,CRE_DT
           ,UPD_USR_ID
           ,UPD_DT)
VALUES(@[act_cd]
      ,@[act_nm]
      ,@[act_desc]
	#if (${act_tp_cd} != '')
      ,@[act_tp_cd]
	#end
	#if (${full_mty_cd} != '')
      ,@[full_mty_cd]
	#end
	#if (${bnd_vskd_seq_cd} != '')
      ,@[bnd_vskd_seq_cd]
	#end
	#if (${nod_tp_cd} != '')
      ,@[nod_tp_cd]
	#end
	#if (${act_op_tp_cd} != '')
      ,@[act_op_tp_cd]
	#end
	#if (${trsp_mod_cd} != '')
      ,@[trsp_mod_cd]
	#end
	#if (${org_dest_cd} != '')
      ,@[org_dest_cd]
	#end
	#if (${act_flg} != '')
      ,@[act_flg]
	#end
	#if (${act_stnd_edi_sts_cd} != '')
      ,@[act_stnd_edi_sts_cd]
	#end
	#if (${cop_skd_lgc_no} != '')
      ,@[cop_skd_lgc_no]
	#end
	#if (${delt_flg} != '')
      ,@[delt_flg]
	#else
	  ,'N'
	#end
      ,@[user_id]
      ,SYSDATE
      ,@[user_id]
      ,SYSDATE) 			]]></sql>
			<params>
				<param name="act_cd" type="12" value="" out="N"/>
				<param name="act_nm" type="12" value="" out="N"/>
				<param name="act_desc" type="12" value="" out="N"/>
				<param name="act_tp_cd" type="12" value="" out="N"/>
				<param name="full_mty_cd" type="12" value="" out="N"/>
				<param name="bnd_vskd_seq_cd" type="12" value="" out="N"/>
				<param name="nod_tp_cd" type="12" value="" out="N"/>
				<param name="act_op_tp_cd" type="12" value="" out="N"/>
				<param name="trsp_mod_cd" type="12" value="" out="N"/>
				<param name="org_dest_cd" type="12" value="" out="N"/>
				<param name="act_flg" type="12" value="" out="N"/>
				<param name="act_stnd_edi_sts_cd" type="12" value="" out="N"/>
				<param name="cop_skd_lgc_no" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
