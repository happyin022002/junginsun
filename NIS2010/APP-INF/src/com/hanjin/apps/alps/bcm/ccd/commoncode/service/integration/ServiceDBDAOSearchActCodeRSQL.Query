<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOSearchActCodeRSQL">
			<desc><![CDATA[ACTIVITY CODE의 상세 내용 조회]]></desc>
			<sql><![CDATA[
SELECT ACT_NM
      ,ACT_DESC
      ,ACT_TP_CD
      ,FULL_MTY_CD
      ,BND_VSKD_SEQ_CD
      ,NOD_TP_CD
      ,ACT_OP_TP_CD
      ,TRSP_MOD_CD
      ,ORG_DEST_CD
      ,ACT_FLG
      ,ACT_STND_EDI_STS_CD
      ,COP_SKD_LGC_NO
      ,DELT_FLG
      ,CRE_USR_ID
      ,CRE_DT
      ,UPD_USR_ID
      ,UPD_DT
  FROM MDM_ACTIVITY
 WHERE ACT_CD = @[act_cd]			]]></sql>
			<params>
				<param name="act_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
