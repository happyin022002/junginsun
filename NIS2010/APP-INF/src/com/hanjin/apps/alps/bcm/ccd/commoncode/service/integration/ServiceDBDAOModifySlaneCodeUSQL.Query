<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOModifySlaneCodeUSQL">
			<desc><![CDATA[VSL CD 상세 정보 수정]]></desc>
			<sql><![CDATA[
UPDATE MDM_VSL_SVC_LANE
   SET VSL_SLAN_NM = @[vsl_slan_nm]
      ,VSL_SVC_TP_CD = @[vsl_svc_tp_cd]
      ,VSL_TP_CD = @[vsl_tp_cd]
      ,FDR_DIV_CD = @[fdr_div_cd]
      ,ST_EFF_DT = TO_CHAR(TO_DATE(@[st_eff_dt], 'YYYY-MM-DD'), 'YYYYMMDD')
      ,END_EFF_DT = TO_CHAR(TO_DATE(@[end_eff_dt], 'YYYY-MM-DD'), 'YYYYMMDD')
      ,CO_CD = @[co_cd]
	  --,MODI_COST_CTR_CD = [modi_cost_ctr_cd]
      ,UPD_USR_ID = @[user_id]
      ,UPD_DT = SYSDATE
      ,DELT_FLG = @[delt_flg]
      --,MODI_VSL_SLAN_CD = [modi_vsl_slan_cd]
	  --,MODI_VSL_SLAN_CD2 = [modi_vsl_slan_cd2]
      --,MODI_VIP_TEAM_CD = [modi_vip_team_cd]
 WHERE VSL_SLAN_CD = @[vsl_slan_cd]			]]></sql>
			<params>
				<param name="vsl_slan_nm" type="12" value="" out="N"/>
				<param name="vsl_svc_tp_cd" type="12" value="" out="N"/>
				<param name="vsl_tp_cd" type="12" value="" out="N"/>
				<param name="fdr_div_cd" type="12" value="" out="N"/>
				<param name="st_eff_dt" type="12" value="" out="N"/>
				<param name="end_eff_dt" type="12" value="" out="N"/>
				<param name="co_cd" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="delt_flg" type="12" value="" out="N"/>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
