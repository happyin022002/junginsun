<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReportDBDAOSearchVesselReportListRSQL">
			<desc><![CDATA[Vessel Report]]></desc>
			<sql><![CDATA[
SELECT  
VSL_CD,
VSL_OWN_IND_CD,
VSL_ENG_NM,
VSL_LOCL_NM,
CRR_CD,
VSL_BLD_CD,
VSL_BLDR_NM,
CO_CD,
VSL_BLD_AREA_NM,
CALL_SGN_NO,
VSL_RGST_CNT_CD,
RGST_PORT_CD,
CLSS_NO_RGST_AREA_NM,
VSL_CLSS_NO,
LLOYD_NO,
VSL_HL_NO,
CRW_KNT,
PICLB_DESC,
VSL_EDI_NM,
PHN_NO,
FAX_NO,
TLX_NO,
VSL_EML,
RGST_NO,
FDR_DIV_CD,
VSL_CLSS_FLG,	
VSL_KEL_LY_DT,	
VSL_LNCH_DT,	
VSL_DE_DT,	
RGST_DT,	
VSL_CLZ_DT,	
VSL_RMK,	
CNTR_DZN_CAPA,	
CNTR_OP_CAPA,	
CNTR_PNM_CAPA,	
RF_RCPT_KNT,	
RF_RCPT_MAX_KNT,	
CNTR_VSL_CLSS_CAPA,	
TTL_TEU_KNT,	
VSL_HTCH_KNT,	
VSL_HLD_KNT,	
LOA_LEN,	
LBP_LEN,	
VSL_WDT,	
VSL_DPTH,	
VSL_HGT,	
SMR_DRFT_HGT,	
FBD_CAPA,	
ECN_SPD,	
VSL_SVC_SPD,	
MAX_SPD,	
DPL_CAPA,	
DWT_WGT,	
LGT_SHP_TONG_WGT,	
GRS_RGST_TONG_WGT,	
PNM_GT_WGT,	
SUZ_GT_WGT,	
INTL_TONG_CERTI_FLG,	
NET_RGST_TONG_WGT,	
PNM_NET_TONG_WGT,	
SUZ_NET_TONG_WGT,	
MADN_VOY_SUZ_NET_TONG_WGT,	
FOIL_CAPA,	
DOIL_CAPA,	
FRSH_WTR_CAPA,
BLST_TNK_CAPA,	
FOIL_CSM,	
DOIL_CSM,	
FRSH_WTR_CSM,	
MN_ENG_MKR_NM,	
MN_ENG_TP_DESC,	
MN_ENG_BHP_PWR,	
MN_ENG_RPM_PWR,	
BWTHST_MKR_NM,	
BWTHST_TP_DESC,	
BWTHST_BHP_PWR,	
BWTHST_RPM_PWR,	
GNR_MKR_NM,
GNR_TP_DESC,	
GNR_BHP_PWR,
GNR_RPM_PWR,
DELT_FLG,
CRE_USR_ID,
TO_CHAR(CRE_DT, 'YYYY-MM-DD HH24:MI:SS') CRE_DT,
UPD_USR_ID,
TO_CHAR(UPD_DT, 'YYYY-MM-DD HH24:MI:SS') UPD_DT,
MODI_VSL_CD
FROM MDM_VSL_CNTR
WHERE  1=1
#if (${vsl_cd} != '')
AND VSL_CD  LIKE @[vsl_cd]||'%'
#end
#if (${vsl_eng_nm} != '')
AND UPPER(VSL_ENG_NM) LIKE '%'||UPPER(@[vsl_eng_nm])||'%'
#end
#if (${crr_cd} != '')
AND CRR_CD LIKE @[crr_cd]||'%'
#end
#if (${fdr_div_cd} != ''&& ${fdr_div_cd} != ' ')
AND FDR_DIV_CD = @[fdr_div_cd]
#end
#if (${status} == 'N') 
AND DELT_FLG <> 'Y'
#elseif (${status} == 'Y') 
AND DELT_FLG = @[status]
#end			]]></sql>
			<params>
				<param name="vsl_cd" type="12" value="" out="N"/>
				<param name="vsl_eng_nm" type="12" value="" out="N"/>
				<param name="crr_cd" type="12" value="" out="N"/>
				<param name="fdr_div_cd" type="12" value="" out="N"/>
				<param name="status" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
