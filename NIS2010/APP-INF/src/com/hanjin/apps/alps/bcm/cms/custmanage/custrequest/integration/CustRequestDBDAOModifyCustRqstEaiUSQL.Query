<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustRequestDBDAOModifyCustRqstEaiUSQL">
			<desc><![CDATA[EAI Update]]></desc>
			<sql><![CDATA[
UPDATE MDM_CUSTOMER_RQST
SET MST_RQST_STS_CD = @[mst_rqst_sts_cd]
#if (${mst_rqst_sts_cd} == 'P')
	,RQST_DT = sysdate
#elseif (${mst_rqst_sts_cd} == 'A')
	,APRO_DT = SYSDATE
    ,APRO_USR_ID = @[upd_usr_id]
#elseif (${mst_rqst_sts_cd} == 'R')
    ,RJCT_RSN_RMK = @[rjct_rsn_rmk]
    ,RJCT_RSN_CD = @[rjct_rsn_cd]
#end
	,UPD_USR_ID = @[upd_usr_id]
    ,UPD_DT = sysdate
WHERE MDM_CUSTOMER_RQST_SEQ = @[rqst_no]			]]></sql>
			<params>
				<param name="mst_rqst_sts_cd" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rjct_rsn_rmk" type="12" value="" out="N"/>
				<param name="rjct_rsn_cd" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
