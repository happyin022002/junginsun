<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CcdCommonDBDAOCheckCtrCodeRSQL">
			<desc><![CDATA[MDM_VENDOR 테이블에 vndrCd 에 
해당하는 정보가 있는지 유무를 리턴한다.]]></desc>
			<sql><![CDATA[
SELECT CTR_ERP_NO
     , CTR_ERP_CD || CTR_ENG_NM AS CTR_DESC
FROM   GL_CTR_ERP  
WHERE  NVL(DELT_FLG,'N') != 'Y'
AND    UPPER(CTR_ERP_NO) LIKE UPPER(@[f_center])||'%'			]]></sql>
			<params>
				<param name="f_center" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
