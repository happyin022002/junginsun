<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchRgstNoRSQL">
			<desc><![CDATA[Register No의 중복 여부 체크]]></desc>
			<sql><![CDATA[
SELECT RGST_NO
  FROM MDM_VENDOR
 WHERE RGST_NO = @[rgst_no]
#if (${vndr_seq} != '')
   AND VNDR_SEQ != @[vndr_seq]
#end 			]]></sql>
			<params>
				<param name="rgst_no" type="12" value="" out="N"/>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
