<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ServiceDBDAOAddSlaneDirCodeCSQL">
			<desc><![CDATA[VSL DIR 정보를 생성]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_VSL_SVC_LANE_DIR
           (VSL_SLAN_CD
           ,VSL_SLAN_DIR_CD
		#if (${vsl_slan_dir_seq} != '')
           ,VSL_SLAN_DIR_SEQ
		#end
           ,CRE_USR_ID
           ,CRE_DT
           ,UPD_USR_ID
           ,UPD_DT
           ,DELT_FLG
           --,MODI_VSL_SLAN_DIR_CD
           )
VALUES(@[vsl_slan_cd]
      ,@[vsl_slan_dir_cd]
	#if (${vsl_slan_dir_seq} != '')
      ,@[vsl_slan_dir_seq]
	#end
      ,@[user_id]
      ,SYSDATE
      ,@[user_id]
      ,SYSDATE
      ,@[dir_delt_flg]
      --,[modi_vsl_slan_dir_cd]
      )			]]></sql>
			<params>
				<param name="vsl_slan_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_cd" type="12" value="" out="N"/>
				<param name="vsl_slan_dir_seq" type="12" value="" out="N"/>
				<param name="user_id" type="12" value="" out="N"/>
				<param name="dir_delt_flg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
