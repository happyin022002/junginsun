<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAORemoveMdmDatProcVODSQL">
			<desc><![CDATA[MdmDatProcVO 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
UPDATE MDM_DAT_PROC
   SET DELT_FLG = 'Y' ,
	   UPD_USR_ID = @[upd_usr_id] ,
       UPD_DT = SYSDATE
 WHERE RQST_NO = @[rqst_no] 			]]></sql>
			<params>
				<param name="upd_usr_id" type="12" value="" out="N"/>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
