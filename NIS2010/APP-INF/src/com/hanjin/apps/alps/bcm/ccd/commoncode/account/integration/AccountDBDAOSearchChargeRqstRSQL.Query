<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountDBDAOSearchChargeRqstRSQL">
			<desc><![CDATA[Request Charge Code 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CHG_CD
 FROM MDM_CHARGE
 WHERE  CHG_CD = @[chg_cd]			]]></sql>
			<params>
				<param name="chg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
