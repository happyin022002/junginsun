<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CcdCommonDBDAOSearchDyLgtSavTmIfRSQL">
			<desc><![CDATA[DayLight Saving Time Interface 정보를 조회합니다.]]></desc>
			<sql><![CDATA[
SELECT CRE_DT
, CRE_USR_ID
, DELT_FLG
, DST_CNT_CD
, DST_ID
, DST_MNTS
, DST_NOT_APLY_STE_CD
, DST_YR
, EAI_EVNT_DT
, EAI_IF_ID
, END_DST_DT
, ST_DST_DT
, ST_DST_HRMNT
, ST_DST_RULE_DESC
, UPD_DT
, UPD_USR_ID
FROM MDM_DYLGT_SAV_TM
WHERE DST_ID = @[dst_id] 			]]></sql>
			<params>
				<param name="dst_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
