<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="AccountDBDAOSearchCurrCodeRSQL">
			<desc><![CDATA[2011.02.21 조인영 Currency Code 정보를 조회한다.]]></desc>
			<sql><![CDATA[
SELECT CURR_CD
       ,CURR_NM
       ,CURR_DESC
       ,CNT_CD
       ,TO_CHAR(FM_EFF_DT,'YYYY-MM-DD') AS FM_EFF_DT
       ,TO_CHAR(TO_EFF_DT,'YYYY-MM-DD') AS TO_EFF_DT
       ,DP_PRCS_KNT
       ,XTD_PRCS_KNT
       ,CRE_USR_ID
       ,CRE_DT
       ,UPD_USR_ID
       ,UPD_DT
       ,DELT_FLG
       ,EAI_EVNT_DT   
       ,EAI_IF_ID
FROM MDM_CURRENCY
WHERE  CURR_CD = @[curr_cd]			]]></sql>
			<params>
				<param name="curr_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
