<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="PartnerDBDAOSearchMaxVndrCntcPnfSeqRSQL">
			<desc><![CDATA[MDM_VNDR_CNTC_PNT 테이블의 MAX VNDR_CNTC_PNT_SEQ 를 가져온다.]]></desc>
			<sql><![CDATA[
SELECT NVL(Max(VNDR_CNTC_PNT_SEQ),0)
FROM   MDM_VNDR_CNTC_PNT
WHERE  VNDR_SEQ = @[vndr_seq] 			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
