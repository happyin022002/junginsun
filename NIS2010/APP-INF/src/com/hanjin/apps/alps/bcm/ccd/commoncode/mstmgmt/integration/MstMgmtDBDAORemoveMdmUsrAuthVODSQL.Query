<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MstMgmtDBDAORemoveMdmUsrAuthVODSQL">
			<desc><![CDATA[MdmUsrAuthVO 정보를 삭제한다.]]></desc>
			<sql><![CDATA[
DELETE FROM MDM_USR_AUTH
WHERE USR_ID = @[usr_id]
  AND MST_DAT_SUBJ_CD = @[mst_dat_subj_cd] 			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="mst_dat_subj_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
