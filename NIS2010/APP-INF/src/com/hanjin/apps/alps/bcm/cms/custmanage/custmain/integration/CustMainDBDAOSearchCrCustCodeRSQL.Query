<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustMainDBDAOSearchCrCustCodeRSQL">
			<desc><![CDATA[Credit Customer Search]]></desc>
			<sql><![CDATA[
SELECT   
   ACT_CUST_CNT_CD
,  ACT_CUST_SEQ
,  ACT_CUST_CNT_CD||TRIM(TO_CHAR(ACT_CUST_SEQ,'000000')) act_cust_cd
,  CUST_RLSE_CTRL_FLG
,  CR_FLG
,  CR_CURR_CD
,  CR_AMT
,  CR_CLT_OFC_CD
,  CR_CUST_RMK
,  IB_CR_TERM_DYS
,  OB_CR_TERM_DYS
,  PAY_DIV_CD
,  TO_CHAR(TO_DATE(CR_ST_DT,'YYYYMMDD'),'YYYY-MM-DD') CR_ST_DT
,  TO_CHAR(TO_DATE(CR_END_DT,'YYYYMMDD'),'YYYY-MM-DD') CR_END_DT
,  CR_CUST_TP_CD
,  OB_EML
,  IB_EML
,  OB_PHN_NO
,  IB_PHN_NO
,  OB_FAX_NO
,  IB_FAX_NO
,  XCH_RT_DIV_CD
,  CNG_INDIV_CD
,  TO_CHAR(TO_DATE(DY_XCH_APLY_ST_DT,'YYYYMMDD'),'YYYY-MM-DD') DY_XCH_APLY_ST_DT
,  ISS_DIV_CD
,  BANK_ACCT_NO
,  CNTC_PSON_NM
,  CUST_CR_DUE_DT_DIV_CD
,  OWNR_NM
,  BZCT_NM
,  BZTP_NM
,  PAY_DT_DY1
,  PAY_DT_DY2
,  PAY_DT_DY3
,  PAY_DT_DY4
,  LOCL_NM
,  LOCL_ADDR1
,  LOCL_ADDR2
,  LOCL_ADDR3
,  LOCL_ADDR4
,  LOCL_ZIP_CD
,  BFR_CR_CLT_OFC_CD
,  MCC.BFR_OFC_CNG_DT
,  INV_DUE_DT_DP_FLG
,  RISS_INV_FLG
,  MCC.INV_ISS_CURR_TP_CD
,  MCC.CRE_USR_ID
,  TO_CHAR(MCC.CRE_DT,'YYYY-MM-DD HH24:MI') CRE_DT
,  MCC.UPD_USR_ID
,  TO_CHAR(MCC.UPD_DT,'YYYY-MM-DD HH24:MI') UPD_DT
,  MCC.DELT_FLG
,  MC.CUST_CNT_CD
,  TRIM(TO_CHAR(MC.CUST_SEQ,'000000')) CUST_SEQ
,  MC.CUST_LGL_ENG_NM
,  MCC.KR_IB_OFC_CD
,  AUTO_INV_IB_FLG
,  AUTO_INV_IB_HJS_REF_NO
,  AUTO_INV_IB_HJS_REF_PHN_NO
,  AUTO_INV_IB_CUST_REF_NO_FLG
,  AUTO_INV_IB_LOCL_CHG_FLG
,  AUTO_INV_IB_EML
,  AUTO_INV_OB_FLG
,  AUTO_INV_OB_HJS_REF_NO
,  AUTO_INV_OB_HJS_REF_PHN_NO
,  AUTO_INV_OB_CUST_REF_NO_FLG
,  AUTO_INV_OB_LOCL_CHG_FLG
,  AUTO_INV_OB_EML
,  MC.RFND_PSDO_VNDR_SEQ
,  DECODE(MC.RFND_PSDO_VNDR_SEQ,NULL,'N','Y') RFND_PSDO_VNDR_FLG
FROM  MDM_CUSTOMER MC
      ,MDM_CR_CUST MCC
WHERE MC.CUST_CNT_CD = @[cust_cnt_cd]
AND   MC.CUST_SEQ    = @[cust_seq]
AND   MC.CUST_CNT_CD = MCC.CUST_CNT_CD(+)
AND   MC.CUST_SEQ = MCC.CUST_SEQ(+)			]]></sql>
			<params>
				<param name="cust_cnt_cd" type="12" value="" out="N"/>
				<param name="cust_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
