<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustGroupDBDAOAddCustPerfCodeCSQL">
			<desc><![CDATA[Create Group Customer code]]></desc>
			<sql><![CDATA[
INSERT INTO MDM_CUST_PERF_GRP(
	 CUST_GRP_ID
,	 CUST_GRP_NM
,    OFC_CD
,    SREP_CD
,    CRE_USR_ID
,    CRE_DT
,    UPD_USR_ID
,    UPD_DT
,    DELT_FLG
)
SELECT 
   'G-'||@[rqst_no]
,  CUST_LGL_ENG_NM
,  OFC_CD
,  SREP_CD
,  UPD_USR_ID
,  UPD_DT
,  UPD_USR_ID
,  UPD_DT
,  'N'
FROM MDM_CUSTOMER_RQST MCR
WHERE MCR.MDM_CUSTOMER_RQST_SEQ = @[rqst_no]
AND MCR.MST_RQST_STS_CD = 'P'
AND MCR.GRP_INDIV_DIV = 'G'			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
