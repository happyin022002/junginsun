<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CustRequestDBDAOApproveCutomerCodeRSQL">
			<desc><![CDATA[Approve Customer Code]]></desc>
			<sql><![CDATA[
SELECT MC.CUST_CNT_CD||TRIM(TO_CHAR(MC.CUST_SEQ,'000000'))
FROM MDM_CUSTOMER MC
WHERE MC.CRM_ROW_ID = 'I-'||@[rqst_no]
AND ROWNUM = 1			]]></sql>
			<params>
				<param name="rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
