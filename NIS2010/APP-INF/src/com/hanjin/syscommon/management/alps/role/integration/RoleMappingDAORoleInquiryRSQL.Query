<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RoleMappingDAORoleInquiryRSQL">
			<desc><![CDATA[Role Inquiry]]></desc>
			<sql><![CDATA[
SELECT 	USR_ROLE_CD, 
		USR_ROLE_NM, 
		USR_ROLE_DESC
			
FROM 	COM_USR_ROLE 
WHERE 1=1		
		#if (${usr_role_cd} != '')
			AND USR_ROLE_CD LIKE @[usr_role_cd]||'%'
		#end

		#if (${usr_role_nm} != '')
			AND upper(USR_ROLE_NM) LIKE '%'||upper(@[usr_role_nm])||'%'
		#end
ORDER BY USR_ROLE_CD			]]></sql>
			<params>
				<param name="usr_role_cd" type="12" value="" out="N"/>
				<param name="usr_role_nm" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
