<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserDAOComAppBaseMenuViewUSQL">
			<desc><![CDATA[Insert com_usr_pgm_mtch]]></desc>
			<sql><![CDATA[
MERGE INTO COM_APP_BASE_MENU_VIEW TT USING
(SELECT @[usr_id] AS USR_ID FROM DUAL) SO ON (TT.USR_ID = SO.USR_ID)
WHEN MATCHED THEN
  UPDATE SET USE_FLG = @[view_flg], UPD_DT = SYSDATE, UPD_USR_ID = @[cre_usr_id]
WHEN NOT MATCHED THEN
  INSERT
    ( USR_ID, USE_FLG, CRE_USR_ID, UPD_USR_ID
    ) VALUES
    ( @[usr_id], @[view_flg], @[cre_usr_id], @[cre_usr_id]
    )			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="n"/>
				<param name="view_flg" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="n"/>
			</params>
		</query>
	</querys>
</sqls>
