<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JobCodeManagementDBDAOSearchRequestDetailRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DTL.USR_ROLE_CD,
       (SELECT USR_ROLE_NM FROM COM_USR_ROLE WHERE USR_ROLE_CD = DTL.USR_ROLE_CD) AS USR_ROLE_NM,
       (SELECT INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID = 'CD03202'
           AND INTG_CD_VAL_CTNT = DTL.USR_ROLE_RQST_TP_CD) AS USR_ROLE_RQST_TP_NM,
       DTL.USR_ROLE_RQST_TP_CD,
       DECODE((SELECT COUNT(PGM_NO) FROM COM_PGM_ROLE WHERE USR_ROLE_CD = DTL.USR_ROLE_CD AND ROWNUM=1),0,'X','O') AS PGM_ASGN
  FROM COM_APRO_ROLE_DTL DTL
 WHERE 1 = 1
   AND APRO_RQST_NO = @[apro_rqst_no]			]]></sql>
			<params>
				<param name="apro_rqst_no" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
