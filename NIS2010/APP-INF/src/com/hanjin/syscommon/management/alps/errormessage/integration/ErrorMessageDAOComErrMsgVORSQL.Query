<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ErrorMessageDAOComErrMsgVORSQL">
			<desc><![CDATA[select error message   ]]></desc>
			<sql><![CDATA[
SELECT                                                                
   	err_msg_cd,  
 	err_lvl_cd,                      
   	err_tp_cd,                                               
   	err_msg,                                               
   	err_desc                                                            
FROM com_err_msg                                                      
WHERE lang_tp_cd = 'ENG'
#if (${err_msg_cd} != '') 
AND   upper(err_msg_cd) LIKE upper('%'||@[err_msg_cd]||'%')
#end
#if (${err_msg} != '') 
and	upper(err_msg like upper('%'||@[err_msg]|| '%')
#end			]]></sql>
			<params>
				<param name="err_msg_cd" type="12" value="" out="N"/>
				<param name="err_msg" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
