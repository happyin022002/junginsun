<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="ReportSearchDAOComRDRSQL">
			<desc><![CDATA[모듈별 RD search하는 sql]]></desc>
			<sql><![CDATA[
select *from
com_rpt_dsgn_snd_appl
where
rd_sub_sys_cd = @[rd_sub_sys_cd]
order by RD_APPL_CD asc
			]]></sql>
			<params>
				<param name="rd_sub_sys_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
