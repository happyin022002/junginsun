<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JobCodeManagementDBDAOSearchJobCodeInquiryRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT USR.USR_ID,
       USR.USR_NM,
       USR.OFC_CD,
       ROL.USR_ROLE_CD,
       ROL.USR_ROLE_NM,
       DECODE((SELECT COUNT(USR_ROLE_CD) FROM COM_PGM_ROLE WHERE USR_ROLE_CD = ROL.USR_ROLE_CD AND ROWNUM = 1), 0, 'X', 'O') AS PGM_ASGN,
       ROL.USR_ROLE_DESC
  FROM COM_USER USR,
       COM_USR_ROLE_MTCH MTCH,
       COM_USR_ROLE ROL
 WHERE 1 = 1
#if (${usr_id} != '')
   AND USR.USR_ID LIKE @[usr_id]||'%'
#end
   AND USR.USR_ID = MTCH.USR_ID
#if (${usr_nm} != '')
   AND USR.USR_NM LIKE @[usr_nm]||'%'
#end
#if (${ofc_cd} != '')
   AND USR.OFC_CD LIKE @[ofc_cd]||'%'
#end
   AND MTCH.USR_ROLE_CD = ROL.USR_ROLE_CD
   AND ROL.USR_ROLE_TP_CD = 'J'			]]></sql>
			<params>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="usr_nm" type="12" value="" out="N"/>
				<param name="ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
