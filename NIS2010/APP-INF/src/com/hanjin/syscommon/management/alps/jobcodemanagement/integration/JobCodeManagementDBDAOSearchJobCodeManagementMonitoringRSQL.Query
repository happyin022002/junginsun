<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JobCodeManagementDBDAOSearchJobCodeManagementMonitoringRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT HDR.RQST_USR_ID,
       HDR.RQST_USR_NM,
       HDR.RQST_OFC_CD,
       (SELECT INTG_CD_VAL_DP_DESC
          FROM MDM_ORGANIZATION MDM , COM_INTG_CD_DTL DTL
         WHERE MDM.OFC_TP_CD = DTL.INTG_CD_VAL_CTNT 
	   AND DTL.INTG_CD_ID = 'CD00818' 
	   AND MDM.OFC_CD = HDR.RQST_OFC_CD) AS OFC_TP_CD,
       (SELECT INTG_CD_VAL_DP_DESC
          FROM COM_INTG_CD_DTL
         WHERE INTG_CD_ID = 'CD03205'
           AND INTG_CD_VAL_CTNT = ROUT.APSTS_CD) AS APSTS_NM,
       TO_CHAR(HDR.RQST_ST_DT, 'YYYY-MM-DD') AS RQST_ST_DT,
       TO_CHAR(ROUT.APRO_DT, 'YYYY-MM-DD') AS APRO_DT,
       ROUT.APRO_USR_NM,
       HDR.USR_ROLE_CD,
       (SELECT USR_ROLE_NM
          FROM COM_USR_ROLE
         WHERE USR_ROLE_CD = HDR.USR_ROLE_CD) AS USR_ROLE_NM,
       ROUT.APRO_RMK,
       HDR.RQST_RMK,
       ROUT.APRO_RQST_NO,
	 (SELECT INTG_CD_VAL_DP_DESC
		FROM COM_INTG_CD_DTL
		WHERE INTG_CD_ID = 'CD03202'
			AND INTG_CD_VAL_CTNT = USR_ROLE_RQST_TP_CD) AS USR_ROLE_RQST_TP_NM
FROM 
  (
   SELECT HDR.APRO_RQST_NO, 
	HDR.RQST_USR_ID, 
	HDR.RQST_USR_NM, 
	HDR.RQST_OFC_CD, 
	HDR.RQST_ST_DT, 
	HDR.RQST_RMK, 
	DTL.USR_ROLE_CD,
	DTL.USR_ROLE_RQST_TP_CD 
    FROM COM_APRO_ROLE_RQST_HDR HDR, COM_APRO_ROLE_DTL DTL 
    WHERE HDR.APRO_RQST_NO = DTL.APRO_RQST_NO
#if (${date_fm} != '' && ${date_to} != '')
   AND HDR.RQST_ST_DT BETWEEN TO_DATE(@[date_fm], 'YYYY-MM-DD') AND TO_DATE(@[date_to], 'YYYY-MM-DD') + 0.99999
#end
#if (${apsts_cd} != '')
   AND HDR.APSTS_CD = @[apsts_cd]
#end  
#if (${rqst_ofc_cd} != '')
   AND HDR.RQST_OFC_CD LIKE @[rqst_ofc_cd]||'%'
#end
  ) HDR, 
  COM_APRO_ROLE_RQST_ROUT ROUT
WHERE 1 = 1
   AND HDR.APRO_RQST_NO = ROUT.APRO_RQST_NO
ORDER BY ROUT.APRO_RQST_NO DESC			]]></sql>
			<params>
				<param name="date_fm" type="12" value="" out="N"/>
				<param name="date_to" type="12" value="" out="N"/>
				<param name="apsts_cd" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
