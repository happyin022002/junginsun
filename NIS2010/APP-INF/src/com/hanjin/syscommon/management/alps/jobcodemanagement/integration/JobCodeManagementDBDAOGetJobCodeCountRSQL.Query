<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JobCodeManagementDBDAOGetJobCodeCountRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT COUNT(USR_ROLE_CD) AS JOB_CODE_KNT
  FROM COM_USR_ROLE
 WHERE 1 = 1
   AND USR_ROLE_CD = @[usr_role_cd]
			]]></sql>
			<params>
				<param name="usr_role_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
