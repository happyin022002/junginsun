<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JobCodeManagementDBDAOSearchJobCodeRequestRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT DECODE((SELECT COUNT(USR_ROLE_CD)
                 FROM COM_USR_ROLE_MTCH
                WHERE USR_ID = @[rqst_usr_id]
                  AND USR_ROLE_CD = ROL.USR_ROLE_CD), 0, '0', '1') AS CHECK_VAL,
       ROL.USR_ROLE_CD,
       ROL.USR_ROLE_NM,
       DECODE((SELECT COUNT(USR_ROLE_CD)
                 FROM COM_PGM_ROLE
                WHERE USR_ROLE_CD = ROL.USR_ROLE_CD
                  AND ROWNUM = 1), 0, 'X', 'O') AS PGM_ASGN
  FROM COM_USR_ROLE ROL,
       (SELECT *
          FROM COM_USR_ROLE_MTCH
         WHERE USR_ID = @[rqst_usr_id]) MTCH
 WHERE ROL.USR_ROLE_CD = MTCH.USR_ROLE_CD(+)
   AND ROL.USR_ROLE_CD IN (SELECT USR_ROLE_CD
                             FROM COM_OFC_ROLE_MTCH
                            WHERE OFC_CD = @[rqst_ofc_cd])			]]></sql>
			<params>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="rqst_ofc_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
