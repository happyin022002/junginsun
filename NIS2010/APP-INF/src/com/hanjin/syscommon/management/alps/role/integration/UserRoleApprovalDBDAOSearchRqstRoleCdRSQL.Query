<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserRoleApprovalDBDAOSearchRqstRoleCdRSQL">
			<desc><![CDATA[Request Role Cd의 Validation check 한다.]]></desc>
			<sql><![CDATA[
select usr_role_cd
from com_usr_role_mtch 
where usr_id = @[rqst_usr_id]
and usr_role_cd = @[rqst_role_cd]			]]></sql>
			<params>
				<param name="rqst_usr_id" type="12" value="" out="N"/>
				<param name="rqst_role_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
