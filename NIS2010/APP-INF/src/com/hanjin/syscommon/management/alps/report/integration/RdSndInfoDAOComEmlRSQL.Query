<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="RdSndInfoDAOComEmlRSQL">
			<desc><![CDATA[Email RD 전송 정보]]></desc>
			<sql><![CDATA[
select 
*
from
com_eml_snd_info
where
EML_SND_NO like @[EML_SND_NO]
order by UPD_DT desc			]]></sql>
			<params>
				<param name="EML_SND_NO" type="12" value="20080401" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
