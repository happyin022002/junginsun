<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="UserRoleApprovalDBDAOAddAproRoleDtlCSQL">
			<desc><![CDATA[PROGRAM ROLE(JOB)의 신규/삭제 Request 건에 대하여 상세한 정보를 관리 한다]]></desc>
			<sql><![CDATA[
INSERT INTO COM_APRO_ROLE_DTL
       (APRO_RQST_NO,
        USR_ROLE_CD,
        USR_ROLE_RQST_TP_CD,
        APSTS_CD,
        CRE_USR_ID,
        CRE_DT,
        UPD_USR_ID,
        UPD_DT)

VALUES (@[apro_rqst_no],
        @[usr_role_cd],
        @[usr_role_rqst_tp_cd],
        'P',
        @[cre_usr_id],
        SYSDATE,
        @[upd_usr_id],
        SYSDATE)			]]></sql>
			<params>
				<param name="apro_rqst_no" type="12" value="" out="N"/>
				<param name="usr_role_cd" type="12" value="" out="N"/>
				<param name="usr_role_rqst_tp_cd" type="12" value="" out="N"/>
				<param name="cre_usr_id" type="12" value="" out="N"/>
				<param name="upd_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
