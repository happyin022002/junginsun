<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="JobCodeManagementDBDAOGetApproverUsrIdRSQL">
			<desc><![CDATA[DESC Enter..]]></desc>
			<sql><![CDATA[
SELECT DECODE(APRO.USR_ID,NULL,'N','Y') AS APRO_USR_ID,
    DECODE(USR.EP_ID,NULL,'N','Y') AS USR_CHK
FROM COM_OFC_ROLE_APRO_USR APRO, COM_USER USR
WHERE APRO.USR_ID(+) = USR.USR_ID
    AND USR.EP_ID = @[apro_usr_id]			]]></sql>
			<params>
				<param name="apro_usr_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
