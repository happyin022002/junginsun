				<param name="hdr_desc" type="12" value="" out="N"/>
				<param name="hdr_len_ctnt" type="12" value="" out="N"/>
				<param name="usr_id" type="12" value="" out="N"/>
				<param name="scrn_id" type="12" value="" out="N"/>
				<param name="sh_id" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
