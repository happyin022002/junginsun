<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MenuDBDAONavigationRSQL">
			<desc><![CDATA[Navigation �젙蹂� 議고쉶]]></desc>
			<sql><![CDATA[
-- UI NAVIGATION
SELECT SYS_CONNECT_BY_PATH(B.PGM_NM, '&nbsp;&gt;&nbsp;') UI_NAVIGATION    
FROM  COM_MNU_CFG A, COM_PROGRAM B    
WHERE A.CHD_PGM_NO = B.PGM_NO    
AND A.CHD_PGM_NO = @[pgm_no]  
START WITH A.PRNT_PGM_NO = @[rootPgmCd]
CONNECT BY PRIOR A.CHD_PGM_NO = A.PRNT_PGM_NO			]]></sql>
			<params>
				<param name="pgm_no" type="12" value="" out="N"/>
				<param name="rootPgmCd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
