<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="MenuDAO99RollViewRSQL">
			<desc><![CDATA[]]></desc>
			<sql><![CDATA[
SELECT NVL((SELECT USE_FLG FROM COM_APP_BASE_MENU_VIEW WHERE USR_ID = @[usr_id]),'Y') AS VIEW_FLG FROM DUAL			]]></sql>
			<params>
				<param name="usr_id" type="12" value="NIS2010" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
